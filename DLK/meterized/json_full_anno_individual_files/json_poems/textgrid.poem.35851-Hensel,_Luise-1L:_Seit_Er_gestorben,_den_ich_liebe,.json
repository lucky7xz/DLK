{"textgrid.poem.35851": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit Er gestorben, den ich liebe,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit Er gestorben, den ich liebe,", "tokens": ["Seit", "Er", "ge\u00b7stor\u00b7ben", ",", "den", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist mir so fremd und leer die Welt,", "tokens": ["Ist", "mir", "so", "fremd", "und", "leer", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all mein Sinnen, meine Triebe,", "tokens": ["Und", "all", "mein", "Sin\u00b7nen", ",", "mei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ziehn zum blauen Himmelszelt.", "tokens": ["Sie", "ziehn", "zum", "blau\u00b7en", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dorthin ist Er vorangegangen,", "tokens": ["Dor\u00b7thin", "ist", "Er", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um Den mein tr\u00fcbes Auge thr\u00e4nt.", "tokens": ["Um", "Den", "mein", "tr\u00fc\u00b7bes", "Au\u00b7ge", "thr\u00e4nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort ist's, wo meine Kronen hangen,", "tokens": ["Dort", "ist's", ",", "wo", "mei\u00b7ne", "Kro\u00b7nen", "han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort ist, wonach mein Herz sich sehnt.", "tokens": ["Dort", "ist", ",", "wo\u00b7nach", "mein", "Herz", "sich", "sehnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich kann nicht mehr hienieden wohnen.", "tokens": ["Ich", "kann", "nicht", "mehr", "hien\u00b7ie\u00b7den", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier hab' ich keine Liebe mehr.", "tokens": ["Hier", "hab'", "ich", "kei\u00b7ne", "Lie\u00b7be", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Erde Kr\u00e4nz' und eitle Kronen", "tokens": ["Der", "Er\u00b7de", "Kr\u00e4nz'", "und", "eit\u00b7le", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind meiner hei\u00dfen Stirn zu schwer.", "tokens": ["Sind", "mei\u00b7ner", "hei\u00b7\u00dfen", "Stirn", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O, warum ist aus Seinen Wunden", "tokens": ["O", ",", "wa\u00b7rum", "ist", "aus", "Sei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Leben mir nicht auch entflohn?", "tokens": ["Das", "Le\u00b7ben", "mir", "nicht", "auch", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00e4tte Ruh' in Ihm gefunden", "tokens": ["Ich", "h\u00e4t\u00b7te", "Ruh'", "in", "Ihm", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kniete schon an Seinem Thron. \u2013", "tokens": ["Und", "knie\u00b7te", "schon", "an", "Sei\u00b7nem", "Thron", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun mu\u00df ich mit gebroch'nem Herzen", "tokens": ["Nun", "mu\u00df", "ich", "mit", "ge\u00b7bro\u00b7ch'\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu Spiel und Mahl und Festen gehn", "tokens": ["Zu", "Spiel", "und", "Mahl", "und", "Fes\u00b7ten", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mu\u00df mit stillverhehlten Schmerzen", "tokens": ["Und", "mu\u00df", "mit", "still\u00b7ver\u00b7hehl\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Menschen eitles Treiben sehn.", "tokens": ["Der", "Men\u00b7schen", "eit\u00b7les", "Trei\u00b7ben", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und pflege doch dies bange Beben,", "tokens": ["Und", "pfle\u00b7ge", "doch", "dies", "ban\u00b7ge", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDS", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das tiefe Weh im Herzen gern;", "tokens": ["Das", "tie\u00b7fe", "Weh", "im", "Her\u00b7zen", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcrd' ich um die Welt es geben,", "tokens": ["Und", "w\u00fcrd'", "ich", "um", "die", "Welt", "es", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erst dann w\u00e4r' Er mir todt und fern.", "tokens": ["Erst", "dann", "w\u00e4r'", "Er", "mir", "todt", "und", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Jetzt ist Er bei mir in den Schmerzen,", "tokens": ["Jetzt", "ist", "Er", "bei", "mir", "in", "den", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur wenn ich Sein nicht denke, todt.", "tokens": ["Nur", "wenn", "ich", "Sein", "nicht", "den\u00b7ke", ",", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "PTKNEG", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch d\u00e4mmert leise mir im Herzen", "tokens": ["Noch", "d\u00e4m\u00b7mert", "lei\u00b7se", "mir", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6nsten Tage Abendroth.", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Ta\u00b7ge", "A\u00b7ben\u00b7droth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Oft wird die Seele mir so muthig,", "tokens": ["Oft", "wird", "die", "See\u00b7le", "mir", "so", "mut\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schau' ich der sanften R\u00f6the nach.", "tokens": ["Schau'", "ich", "der", "sanf\u00b7ten", "R\u00f6\u00b7the", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und denke, wie f\u00fcr mich so blutig", "tokens": ["Und", "den\u00b7ke", ",", "wie", "f\u00fcr", "mich", "so", "blu\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPR", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein treues Herz in Liebe brach. \u2013", "tokens": ["Sein", "treu\u00b7es", "Herz", "in", "Lie\u00b7be", "brach", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Zwar meine Nacht ist kalt und tr\u00fcbe,", "tokens": ["Zwar", "mei\u00b7ne", "Nacht", "ist", "kalt", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was mich freute \u2013 es ist todt.", "tokens": ["Und", "was", "mich", "freu\u00b7te", "\u2013", "es", "ist", "todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, Heil! bald schimmert meiner Liebe", "tokens": ["Doch", ",", "Heil", "!", "bald", "schim\u00b7mert", "mei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Hochzeittages Morgenroth.", "tokens": ["Des", "Hoch\u00b7zeit\u00b7ta\u00b7ges", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da zieht mit treuen Liebesarmen", "tokens": ["Da", "zieht", "mit", "treu\u00b7en", "Lie\u00b7be\u00b7sar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich der Geliebte himmelw\u00e4rts,", "tokens": ["Mich", "der", "Ge\u00b7lieb\u00b7te", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "An Seiner Brust werd' ich erwarmen", "tokens": ["An", "Sei\u00b7ner", "Brust", "werd'", "ich", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und selig l\u00e4cheln wird der Schmerz.", "tokens": ["Und", "se\u00b7lig", "l\u00e4\u00b7cheln", "wird", "der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O selig, selig, Den zu k\u00fcssen,", "tokens": ["O", "se\u00b7lig", ",", "se\u00b7lig", ",", "Den", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "$,", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der aller Liebe Urquell ist!", "tokens": ["Der", "al\u00b7ler", "Lie\u00b7be", "Ur\u00b7quell", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O schrecklich, schrecklich, Ihn zu missen,", "tokens": ["O", "schreck\u00b7lich", ",", "schreck\u00b7lich", ",", "Ihn", "zu", "mis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der alle Wonnen in sich schlie\u00dft! \u2013", "tokens": ["Der", "al\u00b7le", "Won\u00b7nen", "in", "sich", "schlie\u00dft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Seit Er gestorben, den ich liebe,", "tokens": ["Seit", "Er", "ge\u00b7stor\u00b7ben", ",", "den", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist mir so fremd und leer die Welt,", "tokens": ["Ist", "mir", "so", "fremd", "und", "leer", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all mein Sinnen, meine Triebe,", "tokens": ["Und", "all", "mein", "Sin\u00b7nen", ",", "mei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word"