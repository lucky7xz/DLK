{"textgrid.poem.36380": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9. Weihnachtslied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Weihnachtsabend in der Still'", "tokens": ["Am", "Weih\u00b7nachts\u00b7a\u00b7bend", "in", "der", "Still'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein tiefer Schlaf mich \u00fcberfiel", "tokens": ["Ein", "tie\u00b7fer", "Schlaf", "mich", "\u00fc\u00b7berf\u00b7iel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Freuden ganz umflossen;", "tokens": ["Mit", "Freu\u00b7den", "ganz", "um\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein' Seel' empfing viel S\u00fc\u00dfigkeit,", "tokens": ["Mein'", "Seel'", "emp\u00b7fing", "viel", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Trost ich schier zerflossen.", "tokens": ["Vor", "Trost", "ich", "schier", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich tr\u00e4umte wie ein Engel k\u00e4m'", "tokens": ["Ich", "tr\u00e4um\u00b7te", "wie", "ein", "En\u00b7gel", "k\u00e4m'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchrte mich gen Bethlehem,", "tokens": ["Und", "f\u00fchr\u00b7te", "mich", "gen", "Beth\u00b7le\u00b7hem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins j\u00fcd'sche Land gar ferne:", "tokens": ["Ins", "j\u00fcd'\u00b7sche", "Land", "gar", "fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Wunderding sich hier begab,", "tokens": ["Ein", "Wun\u00b7der\u00b7ding", "sich", "hier", "be\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6r' zu, dies von mir lerne.", "tokens": ["H\u00f6r'", "zu", ",", "dies", "von", "mir", "ler\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PDS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "In einen Stall ging ich hinein,", "tokens": ["In", "ei\u00b7nen", "Stall", "ging", "ich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darin ein Ochs und Eselein", "tokens": ["Da\u00b7rin", "ein", "Ochs", "und", "E\u00b7sel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Heu beim Kripplein a\u00dfen:", "tokens": ["Ihr", "Heu", "beim", "Krip\u00b7plein", "a\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein frommer Mann, ein' Jungfrau zart", "tokens": ["Ein", "from\u00b7mer", "Mann", ",", "ein'", "Jung\u00b7frau", "zart"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei ihnen kl\u00e4glich sa\u00dfen.", "tokens": ["Bei", "ih\u00b7nen", "kl\u00e4g\u00b7lich", "sa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Jungfrau hat auf ihrem Scho\u00df", "tokens": ["Die", "Jung\u00b7frau", "hat", "auf", "ih\u00b7rem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kindelein ganz nackt und blo\u00df,", "tokens": ["Ein", "Kin\u00b7de\u00b7lein", "ganz", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schien es als die Sonne:", "tokens": ["Doch", "schien", "es", "als", "die", "Son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein' \u00c4uglein str\u00f6mten Glanz umher", "tokens": ["Sein'", "\u00c4ug\u00b7lein", "str\u00f6m\u00b7ten", "Glanz", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleichwie ein lichter Bronne.", "tokens": ["Gleich\u00b7wie", "ein", "lich\u00b7ter", "Bron\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Dies Kindlein ward der gro\u00dfe Gott,", "tokens": ["Dies", "Kin\u00b7dlein", "ward", "der", "gro\u00b7\u00dfe", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns Bedr\u00e4ngten hilft aus Not,", "tokens": ["Der", "uns", "Be\u00b7dr\u00e4ng\u00b7ten", "hilft", "aus", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der alle Dinge machte;", "tokens": ["Der", "al\u00b7le", "Din\u00b7ge", "mach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt erkennt den Sch\u00f6pfer nicht,", "tokens": ["Die", "Welt", "er\u00b7kennt", "den", "Sch\u00f6p\u00b7fer", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So gar sie ihn verachte.", "tokens": ["So", "gar", "sie", "ihn", "ver\u00b7ach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In arme schlechte Windelein", "tokens": ["In", "ar\u00b7me", "schlech\u00b7te", "Win\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Kind die Jungfrau wickelt ein,", "tokens": ["Ihr", "Kind", "die", "Jung\u00b7frau", "wi\u00b7ckelt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Legt's in die Kripp' mit Neigen;", "tokens": ["Legt's", "in", "die", "Kripp'", "mit", "Nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies ist der Thron, wo Gottes Sohn", "tokens": ["Dies", "ist", "der", "Thron", ",", "wo", "Got\u00b7tes", "Sohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein' Liebe wollt' bezeigen.", "tokens": ["Sein'", "Lie\u00b7be", "wollt'", "be\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "H\u00f6rt weiter an, was ich euch sag':", "tokens": ["H\u00f6rt", "wei\u00b7ter", "an", ",", "was", "ich", "euch", "sag'", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht ward licht, als w\u00e4r' es Tag,", "tokens": ["Die", "Nacht", "ward", "licht", ",", "als", "w\u00e4r'", "es", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel Engel h\u00f6rt man singen;", "tokens": ["Viel", "En\u00b7gel", "h\u00f6rt", "man", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Harfen und mit Lautenklang", "tokens": ["Mit", "Har\u00b7fen", "und", "mit", "Lau\u00b7ten\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In hoher Luft erklingen.", "tokens": ["In", "ho\u00b7her", "Luft", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Engel sprach zur Hirtenschar:", "tokens": ["Ein", "En\u00b7gel", "sprach", "zur", "Hir\u00b7ten\u00b7schar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entsetzt euch nicht, nur Freud' f\u00fcrwahr", "tokens": ["Ent\u00b7setzt", "euch", "nicht", ",", "nur", "Freud'", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ADV", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt ihr aus meinem Munde;", "tokens": ["H\u00f6rt", "ihr", "aus", "mei\u00b7nem", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eu'r Heiland jetzt geboren ist,", "tokens": ["Eu'r", "Hei\u00b7land", "jetzt", "ge\u00b7bo\u00b7ren", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frisch auf zu dieser Stunde!", "tokens": ["Frisch", "auf", "zu", "die\u00b7ser", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Alsbald die Hirten dies geh\u00f6rt,", "tokens": ["Als\u00b7bald", "die", "Hir\u00b7ten", "dies", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschlossen sie auch ungest\u00f6rt", "tokens": ["Be\u00b7schlos\u00b7sen", "sie", "auch", "un\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gen Bethlehem zu reisen;", "tokens": ["Gen", "Beth\u00b7le\u00b7hem", "zu", "rei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Kindlein dorten anzuschaun,", "tokens": ["Das", "Kin\u00b7dlein", "dor\u00b7ten", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm Liebe zu beweisen.", "tokens": ["Ihm", "Lie\u00b7be", "zu", "be\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie zogen hin mit schneller Eil',", "tokens": ["Sie", "zo\u00b7gen", "hin", "mit", "schnel\u00b7ler", "Eil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weg war eine halbe Meil',", "tokens": ["Der", "Weg", "war", "ei\u00b7ne", "hal\u00b7be", "Meil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis sie zum Kripplein kamen;", "tokens": ["Bis", "sie", "zum", "Krip\u00b7plein", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieh da Maria fanden sie,", "tokens": ["Sieh", "da", "Ma\u00b7ria", "fan\u00b7den", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Joseph, das Kind zusammen.", "tokens": ["Jo\u00b7se\u00b7ph", ",", "das", "Kind", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Als sie daselbst gegangen ein,", "tokens": ["Als", "sie", "da\u00b7selbst", "ge\u00b7gan\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Joseph hie\u00df sie willkommen sein,", "tokens": ["Jo\u00b7se\u00b7ph", "hie\u00df", "sie", "will\u00b7kom\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dem sie erzeigten Ehre;", "tokens": ["Dem", "sie", "er\u00b7zeig\u00b7ten", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da zeigten sie die Wunder an,", "tokens": ["Da", "zeig\u00b7ten", "sie", "die", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies freut die Mutter sehre.", "tokens": ["Dies", "freut", "die", "Mut\u00b7ter", "seh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie fielen nieder hin zur Erd',", "tokens": ["Sie", "fie\u00b7len", "nie\u00b7der", "hin", "zur", "Erd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anbeteten den Heiland wert,", "tokens": ["An\u00b7be\u00b7te\u00b7ten", "den", "Hei\u00b7land", "wert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Freud' sie mu\u00dften weinen;", "tokens": ["F\u00fcr", "Freud'", "sie", "mu\u00df\u00b7ten", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann opfern sie ihm Gaben auf,", "tokens": ["Dann", "op\u00b7fern", "sie", "ihm", "Ga\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein L\u00e4mmlein von den Kleinen.", "tokens": ["Ein", "L\u00e4mm\u00b7lein", "von", "den", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Alsbald kehrten sie wieder um,", "tokens": ["Als\u00b7bald", "kehr\u00b7ten", "sie", "wie\u00b7der", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es ward das Evangelium", "tokens": ["Es", "ward", "das", "E\u00b7van\u00b7ge\u00b7li\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch sie bekannt im Lande;", "tokens": ["Durch", "sie", "be\u00b7kannt", "im", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ihnen niemand glauben wollt',", "tokens": ["Es", "ih\u00b7nen", "nie\u00b7mand", "glau\u00b7ben", "wollt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "We\u00df' Orts er war und Stande.", "tokens": ["We\u00df'", "Orts", "er", "war", "und", "Stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dies ist's, was ich im Traum gesehn,", "tokens": ["Dies", "ist's", ",", "was", "ich", "im", "Traum", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ist's kein Traum, es ist geschehn,", "tokens": ["Doch", "ist's", "kein", "Traum", ",", "es", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich als Traum erz\u00e4hlet;", "tokens": ["Was", "ich", "als", "Traum", "er\u00b7z\u00e4h\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wahr und wahrhaftig dieses ist,", "tokens": ["Wahr", "und", "wahr\u00b7haf\u00b7tig", "die\u00b7ses", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PDS", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Nichts ist daran gefehlet.", "tokens": ["Nichts", "ist", "da\u00b7ran", "ge\u00b7feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Am Weihnachtsabend in der Still'", "tokens": ["Am", "Weih\u00b7nachts\u00b7a\u00b7bend", "in", "der", "Still'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein tiefer Schlaf mich \u00fcberfiel", "tokens": ["Ein", "tie\u00b7fer", "Schlaf", "mich", "\u00fc\u00b7berf\u00b7iel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Freuden ganz umflossen;", "tokens": ["Mit", "Freu\u00b7den", "ganz", "um\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein' Seel' empfing viel S\u00fc\u00dfigkeit,", "tokens": ["Mein'", "Seel'", "emp\u00b7fing", "viel", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Trost ich schier zerflossen.", "tokens": ["Vor", "Trost", "ich", "schier", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich tr\u00e4umte wie ein Engel k\u00e4m'", "tokens": ["Ich", "tr\u00e4um\u00b7te", "wie", "ein", "En\u00b7gel", "k\u00e4m'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchrte mich gen Bethlehem,", "tokens": ["Und", "f\u00fchr\u00b7te", "mich", "gen", "Beth\u00b7le\u00b7hem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins j\u00fcd'sche Land gar ferne:", "tokens": ["Ins", "j\u00fcd'\u00b7sche", "Land", "gar", "fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Wunderding sich hier begab,", "tokens": ["Ein", "Wun\u00b7der\u00b7ding", "sich", "hier", "be\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6r' zu, dies von mir lerne.", "tokens": ["H\u00f6r'", "zu", ",", "dies", "von", "mir", "ler\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PDS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.17": {"line.1": {"text": "In einen Stall ging ich hinein,", "tokens": ["In", "ei\u00b7nen", "Stall", "ging", "ich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darin ein Ochs und Eselein", "tokens": ["Da\u00b7rin", "ein", "Ochs", "und", "E\u00b7sel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Heu beim Kripplein a\u00dfen:", "tokens": ["Ihr", "Heu", "beim", "Krip\u00b7plein", "a\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein frommer Mann, ein' Jungfrau zart", "tokens": ["Ein", "from\u00b7mer", "Mann", ",", "ein'", "Jung\u00b7frau", "zart"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei ihnen kl\u00e4glich sa\u00dfen.", "tokens": ["Bei", "ih\u00b7nen", "kl\u00e4g\u00b7lich", "sa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Die Jungfrau hat auf ihrem Scho\u00df", "tokens": ["Die", "Jung\u00b7frau", "hat", "auf", "ih\u00b7rem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kindelein ganz nackt und blo\u00df,", "tokens": ["Ein", "Kin\u00b7de\u00b7lein", "ganz", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schien es als die Sonne:", "tokens": ["Doch", "schien", "es", "als", "die", "Son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein' \u00c4uglein str\u00f6mten Glanz umher", "tokens": ["Sein'", "\u00c4ug\u00b7lein", "str\u00f6m\u00b7ten", "Glanz", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleichwie ein lichter Bronne.", "tokens": ["Gleich\u00b7wie", "ein", "lich\u00b7ter", "Bron\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.19": {"line.1": {"text": "Dies Kindlein ward der gro\u00dfe Gott,", "tokens": ["Dies", "Kin\u00b7dlein", "ward", "der", "gro\u00b7\u00dfe", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der uns Bedr\u00e4ngten hilft aus Not,", "tokens": ["Der", "uns", "Be\u00b7dr\u00e4ng\u00b7ten", "hilft", "aus", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der alle Dinge machte;", "tokens": ["Der", "al\u00b7le", "Din\u00b7ge", "mach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt erkennt den Sch\u00f6pfer nicht,", "tokens": ["Die", "Welt", "er\u00b7kennt", "den", "Sch\u00f6p\u00b7fer", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So gar sie ihn verachte.", "tokens": ["So", "gar", "sie", "ihn", "ver\u00b7ach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "In arme schlechte Windelein", "tokens": ["In", "ar\u00b7me", "schlech\u00b7te", "Win\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Kind die Jungfrau wickelt ein,", "tokens": ["Ihr", "Kind", "die", "Jung\u00b7frau", "wi\u00b7ckelt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Legt's in die Kripp' mit Neigen;", "tokens": ["Legt's", "in", "die", "Kripp'", "mit", "Nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies ist der Thron, wo Gottes Sohn", "tokens": ["Dies", "ist", "der", "Thron", ",", "wo", "Got\u00b7tes", "Sohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein' Liebe wollt' bezeigen.", "tokens": ["Sein'", "Lie\u00b7be", "wollt'", "be\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "H\u00f6rt weiter an, was ich euch sag':", "tokens": ["H\u00f6rt", "wei\u00b7ter", "an", ",", "was", "ich", "euch", "sag'", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht ward licht, als w\u00e4r' es Tag,", "tokens": ["Die", "Nacht", "ward", "licht", ",", "als", "w\u00e4r'", "es", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel Engel h\u00f6rt man singen;", "tokens": ["Viel", "En\u00b7gel", "h\u00f6rt", "man", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Harfen und mit Lautenklang", "tokens": ["Mit", "Har\u00b7fen", "und", "mit", "Lau\u00b7ten\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In hoher Luft erklingen.", "tokens": ["In", "ho\u00b7her", "Luft", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Ein Engel sprach zur Hirtenschar:", "tokens": ["Ein", "En\u00b7gel", "sprach", "zur", "Hir\u00b7ten\u00b7schar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entsetzt euch nicht, nur Freud' f\u00fcrwahr", "tokens": ["Ent\u00b7setzt", "euch", "nicht", ",", "nur", "Freud'", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ADV", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt ihr aus meinem Munde;", "tokens": ["H\u00f6rt", "ihr", "aus", "mei\u00b7nem", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eu'r Heiland jetzt geboren ist,", "tokens": ["Eu'r", "Hei\u00b7land", "jetzt", "ge\u00b7bo\u00b7ren", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frisch auf zu dieser Stunde!", "tokens": ["Frisch", "auf", "zu", "die\u00b7ser", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Alsbald die Hirten dies geh\u00f6rt,", "tokens": ["Als\u00b7bald", "die", "Hir\u00b7ten", "dies", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschlossen sie auch ungest\u00f6rt", "tokens": ["Be\u00b7schlos\u00b7sen", "sie", "auch", "un\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gen Bethlehem zu reisen;", "tokens": ["Gen", "Beth\u00b7le\u00b7hem", "zu", "rei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Kindlein dorten anzuschaun,", "tokens": ["Das", "Kin\u00b7dlein", "dor\u00b7ten", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm Liebe zu beweisen.", "tokens": ["Ihm", "Lie\u00b7be", "zu", "be\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Sie zogen hin mit schneller Eil',", "tokens": ["Sie", "zo\u00b7gen", "hin", "mit", "schnel\u00b7ler", "Eil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weg war eine halbe Meil',", "tokens": ["Der", "Weg", "war", "ei\u00b7ne", "hal\u00b7be", "Meil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis sie zum Kripplein kamen;", "tokens": ["Bis", "sie", "zum", "Krip\u00b7plein", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieh da Maria fanden sie,", "tokens": ["Sieh", "da", "Ma\u00b7ria", "fan\u00b7den", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Joseph, das Kind zusammen.", "tokens": ["Jo\u00b7se\u00b7ph", ",", "das", "Kind", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Als sie daselbst gegangen ein,", "tokens": ["Als", "sie", "da\u00b7selbst", "ge\u00b7gan\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Joseph hie\u00df sie willkommen sein,", "tokens": ["Jo\u00b7se\u00b7ph", "hie\u00df", "sie", "will\u00b7kom\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dem sie erzeigten Ehre;", "tokens": ["Dem", "sie", "er\u00b7zeig\u00b7ten", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da zeigten sie die Wunder an,", "tokens": ["Da", "zeig\u00b7ten", "sie", "die", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies freut die Mutter sehre.", "tokens": ["Dies", "freut", "die", "Mut\u00b7ter", "seh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Sie fielen nieder hin zur Erd',", "tokens": ["Sie", "fie\u00b7len", "nie\u00b7der", "hin", "zur", "Erd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anbeteten den Heiland wert,", "tokens": ["An\u00b7be\u00b7te\u00b7ten", "den", "Hei\u00b7land", "wert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Freud' sie mu\u00dften weinen;", "tokens": ["F\u00fcr", "Freud'", "sie", "mu\u00df\u00b7ten", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann opfern sie ihm Gaben auf,", "tokens": ["Dann", "op\u00b7fern", "sie", "ihm", "Ga\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein L\u00e4mmlein von den Kleinen.", "tokens": ["Ein", "L\u00e4mm\u00b7lein", "von", "den", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Alsbald kehrten sie wieder um,", "tokens": ["Als\u00b7bald", "kehr\u00b7ten", "sie", "wie\u00b7der", "um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es ward das Evangelium", "tokens": ["Es", "ward", "das", "E\u00b7van\u00b7ge\u00b7li\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch sie bekannt im Lande;", "tokens": ["Durch", "sie", "be\u00b7kannt", "im", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ihnen niemand glauben wollt',", "tokens": ["Es", "ih\u00b7nen", "nie\u00b7mand", "glau\u00b7ben", "wollt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "We\u00df' Orts er war und Stande.", "tokens": ["We\u00df'", "Orts", "er", "war", "und", "Stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Dies ist's, was ich im Traum gesehn,", "tokens": ["Dies", "ist's", ",", "was", "ich", "im", "Traum", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ist's kein Traum, es ist geschehn,", "tokens": ["Doch", "ist's", "kein", "Traum", ",", "es", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich als Traum erz\u00e4hlet;", "tokens": ["Was", "ich", "als", "Traum", "er\u00b7z\u00e4h\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wahr und wahrhaftig dieses ist,", "tokens": ["Wahr", "und", "wahr\u00b7haf\u00b7tig", "die\u00b7ses", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PDS", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Nichts ist daran gefehlet.", "tokens": ["Nichts", "ist", "da\u00b7ran", "ge\u00b7feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}