{"textgrid.poem.44259": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "Motto", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Unruh wird noch mehr, wenn Wieg- und Nahmenfest", "tokens": ["Der", "Un\u00b7ruh", "wird", "noch", "mehr", ",", "wenn", "Wieg", "und", "Nah\u00b7men\u00b7fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "KOUS", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Braut- und Meisterschmaus mein Pferd besprechen l\u00e4st.", "tokens": ["Und", "Braut", "und", "Meis\u00b7ter\u00b7schmaus", "mein", "Pferd", "be\u00b7spre\u00b7chen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da mu\u00df der m\u00fcde Gaul, o fr\u00e4\u00dfen ihn die Raben,", "tokens": ["Da", "mu\u00df", "der", "m\u00fc\u00b7de", "Gaul", ",", "o", "fr\u00e4\u00b7\u00dfen", "ihn", "die", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,", "FM", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor sechzehn Groschen flugs von hier nach Moscau draben.", "tokens": ["Vor", "sech\u00b7zehn", "Gro\u00b7schen", "flugs", "von", "hier", "nach", "Mo\u00b7scau", "dra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "APPR", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und gleichwohl k\u00f6nt er leicht der Welt zu Dienste stehn,", "tokens": ["Und", "gleich\u00b7wohl", "k\u00f6nt", "er", "leicht", "der", "Welt", "zu", "Diens\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erlaubt ihm nur das Volck, auf eigner Bahn zu gehn.", "tokens": ["Er\u00b7laubt", "ihm", "nur", "das", "Volck", ",", "auf", "eig\u00b7ner", "Bahn", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So aber sucht man ihm die Wege vorzuschreiben ...", "tokens": ["So", "a\u00b7ber", "sucht", "man", "ihm", "die", "We\u00b7ge", "vor\u00b7zu\u00b7schrei\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Unruh wird noch mehr, wenn Wieg- und Nahmenfest", "tokens": ["Der", "Un\u00b7ruh", "wird", "noch", "mehr", ",", "wenn", "Wieg", "und", "Nah\u00b7men\u00b7fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "KOUS", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Braut- und Meisterschmaus mein Pferd besprechen l\u00e4st.", "tokens": ["Und", 