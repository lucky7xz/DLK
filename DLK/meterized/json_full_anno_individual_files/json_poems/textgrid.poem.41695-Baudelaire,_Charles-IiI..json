{"textgrid.poem.41695": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr edlen Reisenden! Welch seltne Wunder k\u00f6nnen", "tokens": ["Ihr", "ed\u00b7len", "Rei\u00b7sen\u00b7den", "!", "Welch", "selt\u00b7ne", "Wun\u00b7der", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PIAT", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In euren Augen wir, die tief wie Meere, schaun!", "tokens": ["In", "eu\u00b7ren", "Au\u00b7gen", "wir", ",", "die", "tief", "wie", "Mee\u00b7re", ",", "schaun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$,", "PRELS", "ADJD", "KOKOM", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wollt des Gedenkens Schrein, den reichen, ihr uns g\u00f6nnen,", "tokens": ["Wollt", "des", "Ge\u00b7den\u00b7kens", "Schrein", ",", "den", "rei\u00b7chen", ",", "ihr", "uns", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kleinodien, die ihr schuft aus Licht und \u00c4therblaun!", "tokens": ["Klein\u00b7o\u00b7di\u00b7en", ",", "die", "ihr", "schuft", "aus", "Licht", "und", "\u00c4\u00b7ther\u00b7blaun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Dann reisen ohne Dampf und Segel wir von dannen!", "tokens": ["Dann", "rei\u00b7sen", "oh\u00b7ne", "Dampf", "und", "Se\u00b7gel", "wir", "von", "dan\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit ein Lichtstrahl uns des Kerkers Nacht besonnt,", "tokens": ["Da\u00b7mit", "ein", "Licht\u00b7strahl", "uns", "des", "Ker\u00b7kers", "Nacht", "be\u00b7sonnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00dft \u00fcber unsren Geist, den leinwandgleich wir spannen,", "tokens": ["La\u00dft", "\u00fc\u00b7ber", "un\u00b7sren", "Geist", ",", "den", "lein\u00b7wand\u00b7gleich", "wir", "span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erinnrungsbilder ziehn, umrahmt vom Horizont.", "tokens": ["E\u00b7rinn\u00b7rungs\u00b7bil\u00b7der", "ziehn", ",", "um\u00b7rahmt", "vom", "Ho\u00b7ri\u00b7zont", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was saht ihr? Sprecht!", "tokens": ["Was", "saht", "ihr", "?", "Sprecht", "!"], "token_info": ["word", "word", "word", "punct", "wo