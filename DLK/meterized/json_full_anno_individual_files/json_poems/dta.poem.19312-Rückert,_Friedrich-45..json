{"dta.poem.19312": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Gott nicht f\u00fchlt in sich und allen Lebenskreisen,", "tokens": ["Wer", "Gott", "nicht", "f\u00fchlt", "in", "sich", "und", "al\u00b7len", "Le\u00b7bens\u00b7krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKNEG", "VVFIN", "APPR", "PRF", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem werdet ihr ihn nicht beweisen mit Beweisen.", "tokens": ["Dem", "wer\u00b7det", "ihr", "ihn", "nicht", "be\u00b7wei\u00b7sen", "mit", "Be\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer \u00fcberall ihn sieht, was wollt ihr dem ihn zeigen?", "tokens": ["Wer", "\u00fc\u00b7be\u00b7rall", "ihn", "sieht", ",", "was", "wollt", "ihr", "dem", "ihn", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "ART", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum wollt mit euern Gottbeweisen endlich schweigen!", "tokens": ["Drum", "wollt", "mit", "eu\u00b7ern", "Gott\u00b7be\u00b7wei\u00b7sen", "end\u00b7lich", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wollt ihr mir auch vielleicht beweisen, da\u00df ich bin?", "tokens": ["Wollt", "ihr", "mir", "auch", "viel\u00b7leicht", "be\u00b7wei\u00b7sen", ",", "da\u00df", "ich", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich glaubt' es schwerlich euch, glaubt' ichs nicht meinem Sinn.", "tokens": ["Ich", "glaubt'", "es", "schwer\u00b7lich", "euch", ",", "glaubt'", "ichs", "nicht", "mei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPER", "$,", "VVFIN", "PIS", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}