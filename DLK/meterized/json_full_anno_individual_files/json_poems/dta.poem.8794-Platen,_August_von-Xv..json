{"dta.poem.8794": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Haus dich aufgenommen?", "tokens": ["Haus", "dich", "auf\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bist du je des Zweifels", "tokens": ["Bist", "du", "je", "des", "Zwei\u00b7fels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ungeth\u00fcm entkommen?", "tokens": ["Un\u00b7ge\u00b7th\u00fcm", "ent\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bist du je des Sehnens", "tokens": ["Bist", "du", "je", "des", "Seh\u00b7nens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meere durchgeschwommen?", "tokens": ["Mee\u00b7re", "durch\u00b7ge\u00b7schwom\u00b7men", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hat dir je den Busen", "tokens": ["Hat", "dir", "je", "den", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Liebesschmerz beklommen?", "tokens": ["Lie\u00b7bes\u00b7schmerz", "be\u00b7klom\u00b7men", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Hast du je des Todes", "tokens": ["Hast", "du", "je", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Tiefen Sinn vernommen?", "tokens": ["Tie\u00b7fen", "Sinn", "ver\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Bist du, hinzuopfern", "tokens": ["Bist", "du", ",", "hin\u00b7zu\u00b7o\u00b7pfern"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Irdisches, entglommen?", "tokens": ["Ir\u00b7di\u00b7sches", ",", "ent\u00b7glom\u00b7men", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Offen stehn die Thore,", "tokens": ["Of\u00b7fen", "stehn", "die", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Bist du's, magst du kommen!", "tokens": ["Bist", "du's", ",", "magst", "du", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}