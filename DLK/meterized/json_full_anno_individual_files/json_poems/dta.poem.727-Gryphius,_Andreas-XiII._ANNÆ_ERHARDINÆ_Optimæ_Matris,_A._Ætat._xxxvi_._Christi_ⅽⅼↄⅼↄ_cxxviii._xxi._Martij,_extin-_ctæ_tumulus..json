{"dta.poem.727": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiII. ANN\u00c6 ERHARDIN\u00c6  \n Optim\u00e6 Matris, A. \u00c6tat.  xxxvi . Christi  \n \u217d\u217c\u2184\u217c\u2184  cxxviii. xxi.  Martij, extin-  \n ct\u00e6 tumulus.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Edle tugendt Blum/ an welcher recht zu schawen ", "tokens": ["Ach", "Ed\u00b7le", "tu\u00b7gendt", "Blum", "/", "an", "wel\u00b7cher", "recht", "zu", "scha\u00b7wen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "NE", "$(", "APPR", "PRELS", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Gott recht f\u00fcrcht\u0113 hie\u00df! was trew vnd heilig seyn/", "tokens": ["Was", "Gott", "recht", "f\u00fcrcht\u0113", "hie\u00df", "!", "was", "trew", "vnd", "hei\u00b7lig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "ADJD", "VVFIN", "$.", "PWS", "ADJD", "KON", "ADJD", "VAINF", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "O Spiegel der gedult/ O Schawplatz h\u00f6chster pein", "tokens": ["O", "Spie\u00b7gel", "der", "ge\u00b7dult", "/", "O", "Schaw\u00b7platz", "h\u00f6chs\u00b7ter", "pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$(", "NE", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Andachts-volle Roos/ O Richtschnur kenscher Frawen/ ", "tokens": ["O", "An\u00b7dachts\u00b7vol\u00b7le", "Roos", "/", "O", "Richt\u00b7schnur", "ken\u00b7scher", "Fra\u00b7wen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$(", "NE", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat euch die sch\u00e4rfe Seens! de\u00df todes abgehawen/ ", "tokens": ["Hat", "euch", "die", "sch\u00e4r\u00b7fe", "Seens", "!", "de\u00df", "to\u00b7des", "ab\u00b7ge\u00b7ha\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Jm mittag ewrer zeit deckt dieser Marmorstein", "tokens": ["Jm", "mit\u00b7tag", "ew\u00b7rer", "zeit", "deckt", "die\u00b7ser", "Mar\u00b7mor\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die/ durch leid/ schwindtsucht angst/ vnd schmertz verzehr-", "tokens": ["Die", "/", "durch", "leid", "/", "schwindt\u00b7sucht", "angst", "/", "vnd", "schmertz", "ver\u00b7zehr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$(", "APPR", "ADJD", "$(", "ADJD", "VVPP", "$(", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ten Bein/", "tokens": ["ten", "Bein", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Nach dem jhr ewren Geist dem H\u00f6chsten woll\u2019n vertrawen.", "tokens": ["Nach", "dem", "jhr", "ew\u00b7ren", "Geist", "dem", "H\u00f6chs\u00b7ten", "woll'n", "ver\u00b7tra\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott rie\u00df euch v\u014d vns weg gleich als sein grim entbrandt.", "tokens": ["Gott", "rie\u00df", "euch", "v\u014d", "vns", "weg", "gleich", "als", "sein", "grim", "ent\u00b7brandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Seelen noth vnd Krig verheerten Kirch vnd Landt.", "tokens": ["Als", "See\u00b7len", "noth", "vnd", "Krig", "ver\u00b7heer\u00b7ten", "Kirch", "vnd", "Landt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt seht jhr Christum selbst mit s\u00fcsser Frewd vmbfangen! ", "tokens": ["Jtzt", "seht", "jhr", "Chris\u00b7tum", "selbst", "mit", "s\u00fcs\u00b7ser", "Frewd", "vmb\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wir schawen glutt vnd mordt vnd Pest/ vnd sturm vnd ", "tokens": ["Wir", "scha\u00b7wen", "glutt", "vnd", "mordt", "vnd", "Pest", "/", "vnd", "sturm", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NE", "KON", "NN", "$(", "KON", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schwerd", "tokens": ["schwerd"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "O Mutter/ jhr seid ench gar eben von der Erdt!", "tokens": ["O", "Mut\u00b7ter", "/", "jhr", "seid", "ench", "gar", "e\u00b7ben", "von", "der", "Erdt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir aber gar zu fr\u00fch/ Ach gar zu fr\u00fch entgangen.", "tokens": ["Mir", "a\u00b7ber", "gar", "zu", "fr\u00fch", "/", "Ach", "gar", "zu", "fr\u00fch", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKA", "ADJD", "$(", "ITJ", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}