{"textgrid.poem.60578": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begeistre mich, o Muse! die vor Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begeistre mich, o Muse! die vor Zeiten", "tokens": ["Be\u00b7geist\u00b7re", "mich", ",", "o", "Mu\u00b7se", "!", "die", "vor", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$.", "ART", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Flaccus Brust ein himmlisch Feuer blies,", "tokens": ["In", "Flac\u00b7cus", "Brust", "ein", "himm\u00b7lisch", "Feu\u00b7er", "blies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann, ganz entz\u00fcckt, er in Alk\u00e4ens Saiten", "tokens": ["Wann", ",", "ganz", "ent\u00b7z\u00fcckt", ",", "er", "in", "Al\u00b7k\u00e4\u00b7ens", "Sai\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "ADV", "VVPP", "$,", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Weisheit sang und ihre Sch\u00f6nheit pries.", "tokens": ["Von", "Weis\u00b7heit", "sang", "und", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "pries", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie? f\u00fchl ich schon dich, g\u00f6ttlichste der Musen?", "tokens": ["Wie", "?", "f\u00fchl", "ich", "schon", "dich", ",", "g\u00f6tt\u00b7lichs\u00b7te", "der", "Mu\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "PPER", "$,", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Welch hoher Trieb erf\u00fcllet meinen Busen!", "tokens": ["Welch", "ho\u00b7her", "Trieb", "er\u00b7f\u00fcl\u00b7let", "mei\u00b7nen", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo bin ich ietzt? o welche G\u00f6tteraue!", "tokens": ["Wo", "bin", "ich", "ietzt", "?", "o", "wel\u00b7che", "G\u00f6t\u00b7te\u00b7rau\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$.", "FM", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein sanfter West belebet Laub und Klee.", "tokens": ["Ein", "sanf\u00b7ter", "West", "be\u00b7le\u00b7bet", "Laub", "und", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier braust kein Busch, beperlt vom Silbertaue,", "tokens": ["Hier", "braust", "kein", "Busch", ",", "be\u00b7perlt", "vom", "Sil\u00b7ber\u00b7tau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch dich, o S\u00fcd, Tyrann der schwarzen See,", "tokens": ["Durch", "dich", ",", "o", "S\u00fcd", ",", "Ty\u00b7rann", "der", "schwar\u00b7zen", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der voller Wuth das schwache Schiff ersch\u00fcttert,", "tokens": ["Der", "vol\u00b7ler", "Wuth", "das", "schwa\u00b7che", "Schiff", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An Klippen wirft und an dem Fels zersplittert.", "tokens": ["An", "Klip\u00b7pen", "wirft", "und", "an", "dem", "Fels", "zer\u00b7split\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Herze wallt, mein reges Blut entbrennet:", "tokens": ["Mein", "Her\u00b7ze", "wallt", ",", "mein", "re\u00b7ges", "Blut", "ent\u00b7bren\u00b7net", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wollust ist dis Lustrevier geweiht,", "tokens": ["Der", "Wol\u00b7lust", "ist", "dis", "Lus\u00b7tre\u00b7vier", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wollust, ja! nicht die der P\u00f6bel kennet,", "tokens": ["Der", "Wol\u00b7lust", ",", "ja", "!", "nicht", "die", "der", "P\u00f6\u00b7bel", "ken\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "$.", "PTKNEG", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die, stets voll Weins, rast, wann sie sich erfreut:", "tokens": ["Die", ",", "stets", "voll", "Weins", ",", "rast", ",", "wann", "sie", "sich", "er\u00b7freut", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein! die vereint Natur und Weisheit preisen,", "tokens": ["Nein", "!", "die", "ver\u00b7eint", "Na\u00b7tur", "und", "Weis\u00b7heit", "prei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Weisheit Kind und K\u00f6niginn der Weisen.", "tokens": ["Der", "Weis\u00b7heit", "Kind", "und", "K\u00f6\u00b7ni\u00b7ginn", "der", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sehe sie, die junge Rosen kr\u00f6nen,", "tokens": ["Ich", "se\u00b7he", "sie", ",", "die", "jun\u00b7ge", "Ro\u00b7sen", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Rosen ruhn, mit Ruhe ganz umringt.", "tokens": ["Auf", "Ro\u00b7sen", "ruhn", ",", "mit", "Ru\u00b7he", "ganz", "um\u00b7ringt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie l\u00e4chelt s\u00fc\u00df: die Freude mu\u00df ihr fr\u00f6hnen,", "tokens": ["Sie", "l\u00e4\u00b7chelt", "s\u00fc\u00df", ":", "die", "Freu\u00b7de", "mu\u00df", "ihr", "fr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "NN", "VMFIN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die stets um sie die g\u00fcldnen Fl\u00fcgel schwingt:", "tokens": ["Die", "stets", "um", "sie", "die", "g\u00fcld\u00b7nen", "Fl\u00fc\u00b7gel", "schwingt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und, wo sie ruht, seh ich den Schoo\u00df der Erden,", "tokens": ["Und", ",", "wo", "sie", "ruht", ",", "seh", "ich", "den", "Schoo\u00df", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So rauh er war, mit Blumen tr\u00e4chtig werden.", "tokens": ["So", "rauh", "er", "war", ",", "mit", "Blu\u00b7men", "tr\u00e4ch\u00b7tig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VAFIN", "$,", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie trotzet hier dem Zufall und dem Gl\u00fccke,", "tokens": ["Sie", "trot\u00b7zet", "hier", "dem", "Zu\u00b7fall", "und", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr nahet sich kein Qval erf\u00fcllter Schmerz.", "tokens": ["Ihr", "na\u00b7het", "sich", "kein", "Qval", "er\u00b7f\u00fcll\u00b7ter", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergn\u00fcgen herrscht in ihrem heitern Blicke;", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "herrscht", "in", "ih\u00b7rem", "hei\u00b7tern", "Bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch edler Ernst verscheuchet wilden Scherz.", "tokens": ["Doch", "ed\u00b7ler", "Ernst", "ver\u00b7scheu\u00b7chet", "wil\u00b7den", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch sie ward selbst Ly\u00e4us zahm gemachet,", "tokens": ["Durch", "sie", "ward", "selbst", "Ly\u00b7\u00e4us", "zahm", "ge\u00b7ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "NE", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der hinter ihr mit einer Muse lachet.", "tokens": ["Der", "hin\u00b7ter", "ihr", "mit", "ei\u00b7ner", "Mu\u00b7se", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie sollte dir nicht alles dienen m\u00fcssen,", "tokens": ["Wie", "soll\u00b7te", "dir", "nicht", "al\u00b7les", "die\u00b7nen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du, die allein die Sterblichen begl\u00fcckt?", "tokens": ["Du", ",", "die", "al\u00b7lein", "die", "Sterb\u00b7li\u00b7chen", "be\u00b7gl\u00fcckt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gefesselt liegt, o G\u00f6ttinn! dir zu F\u00fcssen", "tokens": ["Ge\u00b7fes\u00b7selt", "liegt", ",", "o", "G\u00f6t\u00b7tinn", "!", "dir", "zu", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "FM", "NN", "$.", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der bleiche Gram, der schwache Seelen dr\u00fcckt,", "tokens": ["Der", "blei\u00b7che", "Gram", ",", "der", "schwa\u00b7che", "See\u00b7len", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und noch ein Schwarm hei\u00dfhungriger Begierden,", "tokens": ["Und", "noch", "ein", "Schwarm", "hei\u00df\u00b7hun\u00b7gri\u00b7ger", "Be\u00b7gier\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die ohne dich tyrannisch herrschen w\u00fcrden.", "tokens": ["Die", "oh\u00b7ne", "dich", "ty\u00b7ran\u00b7nisch", "herr\u00b7schen", "w\u00fcr\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Dich finden wir auf Blumen vollem Wege:", "tokens": ["Dich", "fin\u00b7den", "wir", "auf", "Blu\u00b7men", "vol\u00b7lem", "We\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du legest Reiz auch strengen Pflichten bey.", "tokens": ["Du", "le\u00b7gest", "Reiz", "auch", "stren\u00b7gen", "Pflich\u00b7ten", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur du erh\u00e4ltst, durch deine kluge Pflege,", "tokens": ["Nur", "du", "er\u00b7h\u00e4ltst", ",", "durch", "dei\u00b7ne", "klu\u00b7ge", "Pfle\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Leib gesund, den Geist vergn\u00fcgt und frey.", "tokens": ["Den", "Leib", "ge\u00b7sund", ",", "den", "Geist", "ver\u00b7gn\u00fcgt", "und", "frey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kann deine Huld, o Wollust! die\u00df gew\u00e4hren;", "tokens": ["Kann", "dei\u00b7ne", "Huld", ",", "o", "Wol\u00b7lust", "!", "die\u00df", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was will denn mehr ein Sterblicher begehren?", "tokens": ["Was", "will", "denn", "mehr", "ein", "Sterb\u00b7li\u00b7cher", "be\u00b7geh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wie th\u00f6rigt ist, sich vieles n\u00f6thig machen,", "tokens": ["Wie", "th\u00f6\u00b7rigt", "ist", ",", "sich", "vie\u00b7les", "n\u00f6\u00b7thig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "PRF", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da die Natur nur weniges verlangt?", "tokens": ["Da", "die", "Na\u00b7tur", "nur", "we\u00b7ni\u00b7ges", "ver\u00b7langt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie? oder sind verlangens werthe Sachen,", "tokens": ["Wie", "?", "o\u00b7der", "sind", "ver\u00b7lan\u00b7gens", "wert\u00b7he", "Sa\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VAFIN", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"