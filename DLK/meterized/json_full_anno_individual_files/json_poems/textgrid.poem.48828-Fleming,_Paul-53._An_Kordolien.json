{"textgrid.poem.48828": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "53. An Kordolien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hab' ich anders doch, ", "tokens": ["Was", "hab'", "ich", "an\u00b7ders", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "als Leid, als Herzensangst, als ganz ein totes Leben,", "tokens": ["als", "Leid", ",", "als", "Her\u00b7zen\u00b7sangst", ",", "als", "ganz", "ein", "to\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du, gro\u00dfe Z\u00e4uberin, hast mir die Liebe geben,", "tokens": ["du", ",", "gro\u00b7\u00dfe", "Z\u00e4u\u00b7be\u00b7rin", ",", "hast", "mir", "die", "Lie\u00b7be", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die einen s\u00fc\u00dfen Ha\u00df erwecket stets in mir.", "tokens": ["die", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Ha\u00df", "er\u00b7we\u00b7cket", "stets", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du schl\u00e4gst und heilest mich mit deiner starken Zier;", "tokens": ["Du", "schl\u00e4gst", "und", "hei\u00b7lest", "mich", "mit", "dei\u00b7ner", "star\u00b7ken", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da heilst und schl\u00e4gest mich; ach, schone doch beineben:", "tokens": ["da", "heilst", "und", "schl\u00e4\u00b7gest", "mich", ";", "ach", ",", "scho\u00b7ne", "doch", "bei\u00b7ne\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "$.", "XY", "$,", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "dein Trost auch macht mir Not. O friedsams Widerstreben,", "tokens": ["dein", "Trost", "auch", "macht", "mir", "Not", ".", "O", "fried\u00b7sams", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "NN", "$.", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die Unlust auch von dir macht mir nach dir Begier.", "tokens": ["die", "Un\u00b7lust", "auch", "von", "dir", "macht", "mir", "nach", "dir", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "PPER", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wolan, ", "tokens": ["Wo\u00b7lan", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "und auch getroste Furcht, ich kans nicht Umgang haben,", "tokens": ["und", "auch", "ge\u00b7tros\u00b7te", "Furcht", ",", "ich", "kans", "nicht", "Um\u00b7gang", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ich mu\u00df mich \u00fcber dir an meinem Ekel laben.", "tokens": ["ich", "mu\u00df", "mich", "\u00fc\u00b7ber", "dir", "an", "mei\u00b7nem", "E\u00b7kel", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es scheint, Kupido hat so \u00fcber uns gelost.", "tokens": ["Es", "scheint", ",", "Ku\u00b7pi\u00b7do", "hat", "so", "\u00fc\u00b7ber", "uns", "ge\u00b7lost", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bleibst ", "tokens": ["Du", "bleibst"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "und ich mu\u00df stets mit dir auf meinen Schaden handeln.", "tokens": ["und", "ich", "mu\u00df", "stets", "mit", "dir", "auf", "mei\u00b7nen", "Scha\u00b7den", "han\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {