{"textgrid.poem.37915": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das r\u00f6mische Glas", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stand ich auf einem hohen Berg,", "tokens": ["Stand", "ich", "auf", "ei\u00b7nem", "ho\u00b7hen", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah wohl den tiefen, tiefen Rhein,", "tokens": ["Sah", "wohl", "den", "tie\u00b7fen", ",", "tie\u00b7fen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah ich ein Schifflein schweben,", "tokens": ["Sah", "ich", "ein", "Schif\u00b7flein", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel Ritter tranken drein.", "tokens": ["Viel", "Rit\u00b7ter", "tran\u00b7ken", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der j\u00fcngste, der darunter war,", "tokens": ["Der", "j\u00fcngs\u00b7te", ",", "der", "da\u00b7run\u00b7ter", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PAV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hob auf sein r\u00f6misches Glas,", "tokens": ["Hob", "auf", "sein", "r\u00f6\u00b7mi\u00b7sches", "Glas", ","], "