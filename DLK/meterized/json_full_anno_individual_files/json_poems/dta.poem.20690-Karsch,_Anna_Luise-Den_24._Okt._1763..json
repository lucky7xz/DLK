{"dta.poem.20690": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Den 24. Okt. 1763.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu lange miedest Du, o Sappho! dieses Zimmer;                 ", "tokens": ["Zu", "lan\u00b7ge", "mie\u00b7dest", "Du", ",", "o", "Sap\u00b7pho", "!", "die\u00b7ses", "Zim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$,", "FM", "NE", "$.", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verw\u00f6hnt an Sanssouci, verblend\u2019t von K\u00f6nigs", "tokens": ["Ver\u00b7w\u00f6hnt", "an", "Sans\u00b7sou\u00b7ci", ",", "ver\u00b7blend't", "von", "K\u00f6\u00b7nigs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "NE", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schimmer!", "tokens": ["Schim\u00b7mer", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Monarch klingt zwar sehr sch\u00f6n; doch nicht so sch\u00f6n", "tokens": ["Mon\u00b7arch", "klingt", "zwar", "sehr", "sch\u00f6n", ";", "doch", "nicht", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ADV", "ADJD", "$.", "ADV", "PTKNEG", "ADV", "ADJD"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "als: Freund;", "tokens": ["als", ":", "Freund", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["KOUS", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Dein warten Blatt und Kiel; schreib, wie\u2019s Dein Herze", "tokens": ["Dein", "war\u00b7ten", "Blatt", "und", "Kiel", ";", "schreib", ",", "wie's", "Dein", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NE", "$.", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "meynt!", "tokens": ["meynt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}