{"dta.poem.20718": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Lied einer alten reichen Wittwe,  \n die gern Dame werden will.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.71", "sv:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum sollt ich mich denn h\u00e4rmen,                 ", "tokens": ["Wa\u00b7rum", "sollt", "ich", "mich", "denn", "h\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich doch Reichthum noch,", "tokens": ["Hab", "ich", "doch", "Reicht\u00b7hum", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Junges Muths zu schw\u00e4rmen!", "tokens": ["Jun\u00b7ges", "Muths", "zu", "schw\u00e4r\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Reichthum, Reichthum soll mir geben", "tokens": ["Reicht\u00b7hum", ",", "Reicht\u00b7hum", "soll", "mir", "ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Mann, der mich kann", "tokens": ["Ei\u00b7nen", "Mann", ",", "der", "mich", "kann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VMFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Uebern P\u00f6bel heben \u2014", "tokens": ["Ue\u00b7bern", "P\u00f6\u00b7bel", "he\u00b7ben"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dann bin ich ", "tokens": ["Dann", "bin", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "F\u00fcr mein Geld, alle Welt", "tokens": ["F\u00fcr", "mein", "Geld", ",", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PIAT", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Staunt und spitzt die Ohren.", "tokens": ["Staunt", "und", "spitzt", "die", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Freude wird mich \u00fcberladen,", "tokens": ["Freu\u00b7de", "wird", "mich", "\u00fc\u00b7berl\u00b7a\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Magd sch\u00fcchtern fragt;", "tokens": ["Wenn", "die", "Magd", "sch\u00fcch\u00b7tern", "fragt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "\u201ewas befehl\u2019n Ihr\u2019 Gnaden?\u201c", "tokens": ["\u201e", "was", "be\u00b7fehl'n", "Ih\u00b7r'", "Gna\u00b7den", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Wenn ich sie zum Dienstvergelten", "tokens": ["Wenn", "ich", "sie", "zum", "Dienst\u00b7ver\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ihrer M\u00fch tolles Vieh,", "tokens": ["Ih\u00b7rer", "M\u00fch", "tol\u00b7les", "Vieh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dumme Gans darf schelten;", "tokens": ["Dum\u00b7me", "Gans", "darf", "schel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn Sie mich wird bitten m\u00fcssen", "tokens": ["Wenn", "Sie", "mich", "wird", "bit\u00b7ten", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oben drein, um Verzeihn,", "tokens": ["O\u00b7ben", "drein", ",", "um", "Ver\u00b7zeihn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUI", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und den Rock mir k\u00fcssen \u2014", "tokens": ["Und", "den", "Rock", "mir", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hat sie mich nun angekleidet,", "tokens": ["Hat", "sie", "mich", "nun", "an\u00b7ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcck vor St\u00fcck, da\u00df mein Blick", "tokens": ["St\u00fcck", "vor", "St\u00fcck", ",", "da\u00df", "mein", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sich im Spiegel weidet:", "tokens": ["Sich", "im", "Spie\u00b7gel", "wei\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dann tr\u00e4gt ein Gespann von Rappen,", "tokens": ["Dann", "tr\u00e4gt", "ein", "Ge\u00b7spann", "von", "Rap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Im Gallop, hop, hop, hop!", "tokens": ["Im", "Gal\u00b7lop", ",", "hop", ",", "hop", ",", "hop", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ITJ", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mich und auch mein Wappen!", "tokens": ["Mich", "und", "auch", "mein", "Wap\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Welche Wollust, welch Entz\u00fccken!", "tokens": ["Wel\u00b7che", "Wol\u00b7lust", ",", "welch", "Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn im Saal mein Gemahl", "tokens": ["Wenn", "im", "Saal", "mein", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Links und rechts mit Blicken", "tokens": ["Links", "und", "rechts", "mit", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zu verstehn giebt, da\u00df sein Name", "tokens": ["Zu", "ver\u00b7stehn", "giebt", ",", "da\u00df", "sein", "Na\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Stolz gebeut: seyd gescheit", "tokens": ["Stolz", "ge\u00b7beut", ":", "seyd", "ge\u00b7scheit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$.", "VAFIN", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Kropzeug, weicht der ", "tokens": ["Krop\u00b7zeug", ",", "weicht", "der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}}}}