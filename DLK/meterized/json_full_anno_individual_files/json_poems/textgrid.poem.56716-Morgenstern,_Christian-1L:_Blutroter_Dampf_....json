{"textgrid.poem.56716": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Blutroter Dampf ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blutroter Dampf ...", "tokens": ["Blut\u00b7ro\u00b7ter", "Dampf", "..."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Rossegestampf ...", "tokens": ["Ros\u00b7se\u00b7ge\u00b7stampf", "..."], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "\u00bbkeine Szenen gemacht!", "tokens": ["\u00bb", "kei\u00b7ne", "Sze\u00b7nen", "ge\u00b7macht", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Es harren", "tokens": ["Es", "har\u00b7ren"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "und scharren", "tokens": ["und", "schar\u00b7ren"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "die Rosse der Nacht.\u00ab", "tokens": ["die", "Ros\u00b7se", "der", "Nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein lautloser Schatte,", "tokens": ["Ein", "laut\u00b7lo\u00b7ser", "Schat\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "\u00fcber Wiese und Matte", "tokens": ["\u00fc\u00b7ber", "Wie\u00b7se", "und", "Mat\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "empor durch den Tann,", "tokens": ["em\u00b7por", "durch", "den", "Tann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "das Geistergespann ...", "tokens": ["das", "Geis\u00b7ter\u00b7ge\u00b7spann", "..."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Auf hartem Granit", "tokens": ["Auf", "har\u00b7tem", "Gra\u00b7nit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "der fliegende Huf ...", "tokens": ["der", "flie\u00b7gen\u00b7de", "Huf", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Fallender Wasser", "tokens": ["Fal\u00b7len\u00b7der", "Was\u00b7ser"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "anhebender Ruf ...", "tokens": ["an\u00b7he\u00b7ben\u00b7der", "Ruf", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+---+", "measure": "dactylic.init"}, "line.9": {"text": "K\u00e4ltendes Hauchen ...", "tokens": ["K\u00e4l\u00b7ten\u00b7des", "Hau\u00b7chen", "..."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wir tauchen", "tokens": ["Wir", "tau\u00b7chen"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "in neblige D\u00e4mpfe ...", "tokens": ["in", "neb\u00b7li\u00b7ge", "D\u00e4mp\u00b7fe", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Donnernde K\u00e4mpfe", "tokens": ["Don\u00b7nern\u00b7de", "K\u00e4mp\u00b7fe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "st\u00fcrzender Wogen", "tokens": ["st\u00fcr\u00b7zen\u00b7der", "Wo\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "um uns.", "tokens": ["um", "uns", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Da hinauf", "tokens": ["Da", "hin\u00b7auf"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "--+", "measure": "anapaest.init"}, "line.2": {"text": "der Hufe Horn!", "tokens": ["der", "Hu\u00b7fe", "Horn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In die st\u00e4ubende Schwemme,", "tokens": ["In", "die", "st\u00e4u\u00b7ben\u00b7de", "Schwem\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "hoch \u00fcber den Zorn", "tokens": ["hoch", "\u00fc\u00b7ber", "den", "Zorn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "sich str\u00e4ubender K\u00e4mme", "tokens": ["sich", "str\u00e4u\u00b7ben\u00b7der", "K\u00e4m\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "empor, empor!", "tokens": ["em\u00b7por", ",", "em\u00b7por", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Aus klaffenden Wunden", "tokens": ["Aus", "klaf\u00b7fen\u00b7den", "Wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "speit der Berg", "tokens": ["speit", "der", "Berg"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "sein Blut gegen euch.", "tokens": ["sein", "Blut", "ge\u00b7gen", "euch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mit Wellenhunden", "tokens": ["Mit", "Wel\u00b7len\u00b7hun\u00b7den"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "f\u00e4llt euch an", "tokens": ["f\u00e4llt", "euch", "an"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "der Ha\u00df der H\u00f6he", "tokens": ["der", "Ha\u00df", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "wider das Tal.", "tokens": ["wi\u00b7der", "das", "Tal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Aber ihr fliegt,", "tokens": ["A\u00b7ber", "ihr", "fliegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "blutbespritzt,", "tokens": ["blut\u00b7be\u00b7spritzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "unbesiegt,", "tokens": ["un\u00b7be\u00b7siegt", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "empor, empor.", "tokens": ["em\u00b7por", ",", "em\u00b7por", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Vor euch noch Farben", "tokens": ["Vor", "euch", "noch", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "verzuckenden Lebens,", "tokens": ["ver\u00b7zu\u00b7cken\u00b7den", "Le\u00b7bens", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "auf gr\u00fcnlichem Grau", "tokens": ["auf", "gr\u00fcn\u00b7li\u00b7chem", "Grau"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "verr\u00f6tender Schaum;", "tokens": ["ver\u00b7r\u00f6\u00b7ten\u00b7der", "Schaum", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "hinter euch", "tokens": ["hin\u00b7ter", "euch"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Schwarz und Silber,", "tokens": ["Schwarz", "und", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "die Farben des Todes.", "tokens": ["die", "Far\u00b7ben", "des", "To\u00b7des", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ein Schleier,", "tokens": ["Ein", "Schlei\u00b7er", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "an eure M\u00e4hnen gekn\u00fcpft,", "tokens": ["an", "eu\u00b7re", "M\u00e4h\u00b7nen", "ge\u00b7kn\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "schleppt", "tokens": ["schleppt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "geisterhaft nach.", "tokens": ["geis\u00b7ter\u00b7haft", "nach", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Wie ein Busentuch", "tokens": ["Wie", "ein", "Bu\u00b7sen\u00b7tuch"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "zieht ihr hinauf ihn", "tokens": ["zieht", "ihr", "hin\u00b7auf", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00fcber des Bergs", "tokens": ["\u00fc\u00b7ber", "des", "Bergs"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "zerrissene Brust.", "tokens": ["zer\u00b7ris\u00b7se\u00b7ne", "Brust", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "M\u00fcde sprang sich", "tokens": ["M\u00fc\u00b7de", "sprang", "sich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PRF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "der Sturzbach.", "tokens": ["der", "Sturz\u00b7bach", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nur mit den Lippen", "tokens": ["Nur", "mit", "den", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "wehrt er sich noch.", "tokens": ["wehrt", "er", "sich", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und bald", "tokens": ["Und", "bald"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "wird er zum Kind", "tokens": ["wird", "er", "zum", "Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "und h\u00e4ngt sich selber", "tokens": ["und", "h\u00e4ngt", "sich", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "spielend an eure Schweife.", "tokens": ["spie\u00b7lend", "an", "eu\u00b7re", "Schwei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Weiter! weiter!", "tokens": ["Wei\u00b7ter", "!", "wei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Da!", "tokens": ["Da", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Winkende Gipfel", "tokens": ["Win\u00b7ken\u00b7de", "Gip\u00b7fel"], "token_info": ["