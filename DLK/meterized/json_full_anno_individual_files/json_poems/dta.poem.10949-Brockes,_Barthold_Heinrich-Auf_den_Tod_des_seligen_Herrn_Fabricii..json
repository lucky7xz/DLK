{"dta.poem.10949": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod  \n des seligen Herrn Fabricii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey Teutschlands allgemeinem Gram, worin es sich kaum\nweis zu fassen,", "tokens": ["Bey", "Teutschlands", "all\u00b7ge\u00b7mei\u00b7nem", "Gram", ",", "wo\u00b7rin", "es", "sich", "kaum", "weis", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,", "PWAV", "PPER", "PRF", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Weil sein Fabricius erbleichet, werd ich durch diesen Trost", "tokens": ["Weil", "sein", "Fab\u00b7ri\u00b7ci\u00b7us", "er\u00b7blei\u00b7chet", ",", "werd", "ich", "durch", "die\u00b7sen", "Trost"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NE", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "--+--+-+-+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da\u00df Gott, in ihm, die wahre Tugend der Welt,", "tokens": ["Da\u00df", "Gott", ",", "in", "ihm", ",", "die", "wah\u00b7re", "Tu\u00b7gend", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "wo Laster sonst regiert,\nZu ihrer Besserung gezeigt, und acht und sechzig\nJahr gelassen.", "tokens": ["wo", "Las\u00b7ter", "sonst", "re\u00b7giert", ",", "Zu", "ih\u00b7rer", "Bes\u00b7se\u00b7rung", "ge\u00b7zeigt", ",", "und", "acht", "und", "sech\u00b7zig", "Jahr", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,", "KON", "CARD", "KON", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}