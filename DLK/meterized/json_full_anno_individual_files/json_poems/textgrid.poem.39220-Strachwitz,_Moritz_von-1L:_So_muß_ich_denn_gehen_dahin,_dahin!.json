{"textgrid.poem.39220": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So mu\u00df ich denn gehen dahin, dahin!", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So mu\u00df ich denn gehen dahin, dahin!", "tokens": ["So", "mu\u00df", "ich", "denn", "ge\u00b7hen", "da\u00b7hin", ",", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "PAV", "$,", "PAV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und habe Dir nicht gesagt, geklagt,", "tokens": ["Und", "ha\u00b7be", "Dir", "nicht", "ge\u00b7sagt", ",", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie ich m\u00e4hlich Dein eigen worden bin,", "tokens": ["Wie", "ich", "m\u00e4h\u00b7lich", "Dein", "ei\u00b7gen", "wor\u00b7den", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VAPP", "VAFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bis das Herz mir in zehrender Liebe verzagt.", "tokens": ["Bis", "das", "Herz", "mir", "in", "zeh\u00b7ren\u00b7der", "Lie\u00b7be", "ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "So mu\u00df ich denn gehen hindann, hindann!", "tokens": ["So", "mu\u00df", "ich", "denn", "ge\u00b7hen", "hin\u00b7dann