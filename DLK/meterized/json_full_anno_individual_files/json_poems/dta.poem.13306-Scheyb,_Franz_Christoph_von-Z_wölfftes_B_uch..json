{"dta.poem.13306": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So war man endlich still der Ordnung nach gesessen,", "tokens": ["So", "war", "man", "end\u00b7lich", "still", "der", "Ord\u00b7nung", "nach", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJD", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit man dieses Streits Entscheidung k\u00f6nnt\u2019 ermessen.", "tokens": ["Da\u00b7mit", "man", "die\u00b7ses", "Streits", "Ent\u00b7schei\u00b7dung", "k\u00f6nnt'", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDAT", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wahrheit zeigte sich noch mit dem Schleyr bedeckt,", "tokens": ["Die", "Wahr\u00b7heit", "zeig\u00b7te", "sich", "noch", "mit", "dem", "Schleyr", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun aber ward gemach die Achtsamkeit erweckt;", "tokens": ["Nun", "a\u00b7ber", "ward", "ge\u00b7mach", "die", "Acht\u00b7sam\u00b7keit", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "275Sie wies das Angesicht in Reizungs-vollen Blicken,", "tokens": ["wies", "das", "An\u00b7ge\u00b7sicht", "in", "Rei\u00b7zungs\u00b7vol\u00b7len", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Da\u00df jedes Auge sich darinnen konnt erquicken.", "tokens": ["Da\u00df", "je\u00b7des", "Au\u00b7ge", "sich", "da\u00b7rin\u00b7nen", "konnt", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}