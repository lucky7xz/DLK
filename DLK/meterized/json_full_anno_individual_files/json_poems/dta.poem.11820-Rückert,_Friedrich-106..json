{"dta.poem.11820": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft geh' ich durch die Flur, mein Auge still zu weiden,", "tokens": ["Oft", "geh'", "ich", "durch", "die", "Flur", ",", "mein", "Au\u00b7ge", "still", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie ein Hirt sein Lamm auf \u00fcberbl\u00fcmten Heiden.", "tokens": ["Als\u00b7wie", "ein", "Hirt", "sein", "Lamm", "auf", "\u00fc\u00b7ber\u00b7bl\u00fcm\u00b7ten", "Hei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann frag' ich mich, was ich die Blumen sonst gefragt,", "tokens": ["Dann", "frag'", "ich", "mich", ",", "was", "ich", "die", "Blu\u00b7men", "sonst", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PWS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sage mir, was sonst die Blumen mir gesagt.", "tokens": ["Und", "sa\u00b7ge", "mir", ",", "was", "sonst", "die", "Blu\u00b7men", "mir", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von der ich einen Gru\u00df empfangen hab' im Winde,", "tokens": ["Von", "der", "ich", "ei\u00b7nen", "Gru\u00df", "emp\u00b7fan\u00b7gen", "hab'", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVPP", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Blumen saget mir, wo ich die Liebe finde.", "tokens": ["Ihr", "Blu\u00b7men", "sa\u00b7get", "mir", ",", "wo", "ich", "die", "Lie\u00b7be", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geh suche nur! sie ist wie Kindes Festbescherung", "tokens": ["Geh", "su\u00b7che", "nur", "!", "sie", "ist", "wie", "Kin\u00b7des", "Fest\u00b7be\u00b7sche\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "KOKOM", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Mutter auf der Flur versteckt in Blumumwehrung.", "tokens": ["Von", "Mut\u00b7ter", "auf", "der", "Flur", "ver\u00b7steckt", "in", "Blu\u00b7mum\u00b7weh\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Neugierig schaut' ich da in alle Blumenwiegen,", "tokens": ["Neu\u00b7gie\u00b7rig", "schaut'", "ich", "da", "in", "al\u00b7le", "Blu\u00b7men\u00b7wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und glaubte sie wie Thau in jedem Kelche liegen.", "tokens": ["Und", "glaub\u00b7te", "sie", "wie", "Thau", "in", "je\u00b7dem", "Kel\u00b7che", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und da wo ich sie fand, da stellten sich im Kreise", "tokens": ["Und", "da", "wo", "ich", "sie", "fand", ",", "da", "stell\u00b7ten", "sich", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PWAV", "PPER", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Blumench\u00f6re auf, mit mir zu beten leise.", "tokens": ["Die", "Blu\u00b7menc\u00b7h\u00f6\u00b7re", "auf", ",", "mit", "mir", "zu", "be\u00b7ten", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "PPER", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Blumen frag' ich nun: wo ist sie hingekommen?", "tokens": ["Die", "Blu\u00b7men", "frag'", "ich", "nun", ":", "wo", "ist", "sie", "hin\u00b7ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und leise sagen sie: den Strom hinabgeschwommen.", "tokens": ["Und", "lei\u00b7se", "sa\u00b7gen", "sie", ":", "den", "Strom", "hin\u00b7ab\u00b7ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "So schwimme nur den Strom auch du, o Thr\u00e4n', hinab,", "tokens": ["So", "schwim\u00b7me", "nur", "den", "Strom", "auch", "du", ",", "o", "Thr\u00e4n'", ",", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "PPER", "$,", "FM", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo du treibst ans Land, dort ist der Liebe Grab.", "tokens": ["Und", "wo", "du", "treibst", "ans", "Land", ",", "dort", "ist", "der", "Lie\u00b7be", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dort melde mich der Lieb' und sage: Bald wird kommen", "tokens": ["Dort", "mel\u00b7de", "mich", "der", "Lieb'", "und", "sa\u00b7ge", ":", "Bald", "wird", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "$.", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die m\u00fcde Sehnsucht auch, und sei hier aufgenommen.", "tokens": ["Die", "m\u00fc\u00b7de", "Sehn\u00b7sucht", "auch", ",", "und", "sei", "hier", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und wo die Sehnsucht ruht, da stellet euch im Kreise,", "tokens": ["Und", "wo", "die", "Sehn\u00b7sucht", "ruht", ",", "da", "stel\u00b7let", "euch", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Blumench\u00f6re, auf, und betet ob ihr leise.", "tokens": ["Ihr", "Blu\u00b7menc\u00b7h\u00f6\u00b7re", ",", "auf", ",", "und", "be\u00b7tet", "ob", "ihr", "lei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "$,", "KON", "VVFIN", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}