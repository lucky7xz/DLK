{"textgrid.poem.67469": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An den Storch", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keuscher, frommer Vogel, auf welchem friedlichen Hause", "tokens": ["Keu\u00b7scher", ",", "from\u00b7mer", "Vo\u00b7gel", ",", "auf", "wel\u00b7chem", "fried\u00b7li\u00b7chen", "Hau\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NE", "$,", "APPR", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wohnest Du jetzo und bringst seinen Einwohnenden Gl\u00fcck?", "tokens": ["Woh\u00b7nest", "Du", "jet\u00b7zo", "und", "bringst", "sei\u00b7nen", "Ein\u00b7woh\u00b7nen\u00b7den", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auf dem Tempel vielleicht der Muselmanen, und siehest", "tokens": ["Auf", "dem", "Tem\u00b7pel", "viel\u00b7leicht", "der", "Mu\u00b7sel\u00b7ma\u00b7nen", ",", "und", "sie\u00b7hest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihr and\u00e4chtig Gebet, ihre genie\u00dfende Ruh;", "tokens": ["Ihr", "an\u00b7d\u00e4ch\u00b7tig", "Ge\u00b7bet", ",", "ih\u00b7re", "ge\u00b7nie\u00b7\u00dfen\u00b7de", "Ruh", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und dann kehrest Du wieder zum alten freundlichen Neste,", "tokens": ["Und", "dann", "keh\u00b7rest", "Du", "wie\u00b7der", "zum", "al\u00b7ten", "freund\u00b7li\u00b7chen", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Ihm und dem Tage getreu, der mit dem Lenze Dich ruft.", "tokens": ["Ihm", "und", "dem", "Ta\u00b7ge", "ge\u00b7treu", ",", "der", "mit", "dem", "Len\u00b7ze", "Dich", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "ADJD", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Fr\u00fchling bringender Storch, o br\u00e4chtest Du auch Dein Gl\u00fcck mir,", "tokens": ["Fr\u00fch\u00b7ling", "brin\u00b7gen\u00b7der", "Storch", ",", "o", "br\u00e4ch\u00b7test", "Du", "auch", "Dein", "Gl\u00fcck", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Irgend ein ruhiges Haus und ein zufriedenes Herz!", "tokens": ["Ir\u00b7gend", "ein", "ru\u00b7hi\u00b7ges", "Haus", "und", "ein", "zu\u00b7frie\u00b7de\u00b7nes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.2": {"line.1": {"text": "Keuscher, frommer Vogel, auf welchem friedlichen Hause", "tokens": ["