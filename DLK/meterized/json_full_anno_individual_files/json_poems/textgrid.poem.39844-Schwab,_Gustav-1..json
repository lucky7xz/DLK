{"textgrid.poem.39844": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O m\u00f6chte mir dein traurig Bild erscheinen,", "tokens": ["O", "m\u00f6ch\u00b7te", "mir", "dein", "trau\u00b7rig", "Bild", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein bleiches Angesicht der letzten Stunde,", "tokens": ["Dein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht", "der", "letz\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der bange Schrei, das Blut der Todeswunde:", "tokens": ["Der", "ban\u00b7ge", "Schrei", ",", "das", "Blut", "der", "To\u00b7des\u00b7wun\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich w\u00fc\u00dfte, du bist hin, ich k\u00f6nnte weinen.", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", ",", "du", "bist", "hin", ",", "ich", "k\u00f6nn\u00b7te", "wei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKVZ", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt will's der Geist noch immer sich verneinen,", "tokens": ["Jetzt", "will's", "der", "Geist", "noch", "im\u00b7mer", "sich", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du schwebst vor ihm, der Freudige, Gesunde,", "tokens": ["Du", "schwebst", "vor", "ihm", ",", "der", "Freu\u00b7di\u00b7ge", ",", "Ge\u00b7sun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wangen Rot, das L\u00e4cheln in dem Munde,", "tokens": ["Der", "Wan\u00b7gen", "Rot", ",", "das", "L\u00e4\u00b7cheln", "in", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum holden Lebensbild will sich's vereinen.", "tokens": ["Zum", "hol\u00b7den", "Le\u00b7bens\u00b7bild", "will", "sich's", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So seh' ich dich an meiner Seite zechen;", "tokens": ["So", "seh'", "ich", "dich", "an", "mei\u00b7ner", "Sei\u00b7te", "ze\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mutwillig sch\u00fcttelnd mit den Lockenhaaren", "tokens": ["Mut\u00b7wil\u00b7lig", "sch\u00fct\u00b7telnd", "mit", "den", "Lo\u00b7cken\u00b7haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Wei\u00dft du von bunten M\u00e4rchen viel zu sagen.", "tokens": ["Wei\u00dft", "du", "von", "bun\u00b7ten", "M\u00e4r\u00b7chen", "viel", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch pl\u00f6tzlich mu\u00df das frohe Bild zerbrechen,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "mu\u00df", "das", "fro\u00b7he", "Bild", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Dunst und Nebel ist es hingefahren,", "tokens": ["In", "Dunst", "und", "Ne\u00b7bel", "ist", "es", "hin\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fern h\u00f6r' ich die Todesschwerter schlagen.", "tokens": ["Und", "fern", "h\u00f6r'", "ich", "die", "To\u00b7des\u00b7schwer\u00b7ter", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O m\u00f6chte mir dein traurig Bild erscheinen,", "tokens": ["O", "m\u00f6ch\u00b7te", "mir", "dein", "trau\u00b7rig", "Bild", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "