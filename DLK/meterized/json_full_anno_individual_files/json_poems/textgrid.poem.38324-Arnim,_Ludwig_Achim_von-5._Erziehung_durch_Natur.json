oken_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die reisen her und hin;", "tokens": ["Die", "rei\u00b7sen", "her", "und", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie sind voll M\u00fch f\u00fcr morgen,", "tokens": ["Sie", "sind", "voll", "M\u00fch", "f\u00fcr", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dies ist ihr Gewinn.", "tokens": ["Und", "dies", "ist", "ihr", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ach h\u00f6r das s\u00fcsse Lallen,", "tokens": ["Ach", "h\u00f6r", "das", "s\u00fcs\u00b7se", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den allersch\u00f6nsten Ton", "tokens": ["Den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Ton"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der kleinen Nachtigallen,", "tokens": ["Der", "klei\u00b7nen", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ihrem niedern Thron.", "tokens": ["Auf", "ih\u00b7rem", "nie\u00b7dern", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"],