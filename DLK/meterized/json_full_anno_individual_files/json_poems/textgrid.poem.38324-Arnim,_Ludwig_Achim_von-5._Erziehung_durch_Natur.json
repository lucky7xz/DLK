{"textgrid.poem.38324": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "5. Erziehung durch Natur", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach h\u00f6r das s\u00fcsse Lallen,", "tokens": ["Ach", "h\u00f6r", "das", "s\u00fcs\u00b7se", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den allersch\u00f6nsten Ton", "tokens": ["Den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Ton"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der kleinen Nachtigallen,", "tokens": ["Der", "klei\u00b7nen", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ihrem niedern Thron.", "tokens": ["Auf", "ih\u00b7rem", "nie\u00b7dern", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6r, was sie dir da singet,", "tokens": ["H\u00f6r", ",", "was", "sie", "dir", "da", "sin\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In ihrer gr\u00fcnen Claus;", "tokens": ["In", "ih\u00b7rer", "gr\u00fc\u00b7nen", "Claus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr schlechtes Wesen bringet", "tokens": ["Ihr", "schlech\u00b7tes", "We\u00b7sen", "brin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Viel weise Lehr heraus.", "tokens": ["Viel", "wei\u00b7se", "Lehr", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie spricht: ihr Menschen sehet,", "tokens": ["Sie", "spricht", ":", "ihr", "Men\u00b7schen", "se\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Nothdurft ist sehr klein;", "tokens": ["Mein", "Noth\u00b7durft", "ist", "sehr", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Wunsch nicht weiter gehet,", "tokens": ["Mein", "Wunsch", "nicht", "wei\u00b7ter", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Nachtigall zu seyn.", "tokens": ["Als", "Nach\u00b7ti\u00b7gall", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich la\u00df die hohen Nester,", "tokens": ["Ich", "la\u00df", "die", "ho\u00b7hen", "Nes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und liebe Niedrigkeit;", "tokens": ["Und", "lie\u