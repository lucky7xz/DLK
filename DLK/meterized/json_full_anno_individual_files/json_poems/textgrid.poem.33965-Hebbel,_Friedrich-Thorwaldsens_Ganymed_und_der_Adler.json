", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Weil er schnell, mich fester fassend,", "tokens": ["Weil", "er", "schnell", ",", "mich", "fes\u00b7ter", "fas\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Wolken mich entr\u00fcckt,", "tokens": ["In", "die", "Wol\u00b7ken", "mich", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich, schwindelnd und erblassend,", "tokens": ["Als", "ich", ",", "schwin\u00b7delnd", "und", "er\u00b7blas\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter mich hinabgeblickt;", "tokens": ["Un\u00b7ter", "mich", "hin\u00b7ab\u00b7ge\u00b7blickt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, weil er sogar die Klauen", "tokens": ["Ja", ",", "weil", "er", "so\u00b7gar", "die", "Klau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter'm Fittig-Paar verh\u00fcllt,", "tokens": ["Un\u00b7ter'm", "Fit\u00b7tig\u00b7Paar", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mich fast mit gr\u00f6\u00dferm Grauen,", "tokens": ["Die", "mich", "fast",