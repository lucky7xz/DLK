{"textgrid.poem.41577": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am sechsundzwanzigsten Sonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steht nicht der Greuel der Verw\u00fcstung da", "tokens": ["Steht", "nicht", "der", "Greu\u00b7el", "der", "Ver\u00b7w\u00fcs\u00b7tung", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An heil'ger St\u00e4tte?", "tokens": ["An", "heil'\u00b7ger", "St\u00e4t\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Was tr\u00e4umen wir von Dingen, die uns nah,", "tokens": ["Was", "tr\u00e4u\u00b7men", "wir", "von", "Din\u00b7gen", ",", "die", "uns", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als schliefen sie wie Feuerstoff im Bette", "tokens": ["Als", "schlie\u00b7fen", "sie", "wie", "Feu\u00b7er\u00b7stoff", "im", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "KOKOM", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Kohlenschachts? Blickt auf und schaut umher,", "tokens": ["Des", "Koh\u00b7len\u00b7schachts", "?", "Blickt", "auf", "und", "schaut", "um\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O, die Ver\u00f6dung, wie sie dumpf und schwer", "tokens": ["O", ",", "die", "Ve\u00b7r\u00f6\u00b7dung", ",", "wie", "sie", "dumpf", "und", "schwer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Traf Herz an Herz wie mit galvan'scher Kette!", "tokens": ["Traf", "Herz", "an", "Herz", "wie", "mit", "ga\u00b7lvan'\u00b7scher", "Ket\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gibt's eine St\u00e4tte denn, die heiliger", "tokens": ["Gibt's", "ei\u00b7ne", "St\u00e4t\u00b7te", "denn", ",", "die", "hei\u00b7li\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "ADV", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als Menschenherzen?", "tokens": ["Als", "Men\u00b7schen\u00b7her\u00b7zen", "?"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gibt es Verw\u00fcstung, die entsetzlicher,", "tokens": ["Gibt", "es", "Ver\u00b7w\u00fcs\u00b7tung", ",", "die", "ent\u00b7setz\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Als wenn das H\u00f6chste stirbt an matten Scherzen?", "tokens": ["Als", "wenn", "das", "H\u00f6chs\u00b7te", "stirbt", "an", "mat\u00b7ten", "Scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O Glaube, Glaube, wem du kalt und schwach,", "tokens": ["O", "Glau\u00b7be", ",", "Glau\u00b7be", ",", "wem", "du", "kalt", "und", "schwach", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "PWS", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der schleppt den Grabstein an der Ferse nach:", "tokens": ["Der", "schleppt", "den", "Grab\u00b7stein", "an", "der", "Fer\u00b7se", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dennoch Heil ihm, schleppt er ihn mit Schmerzen!", "tokens": ["Und", "den\u00b7noch", "Heil", "ihm", ",", "schleppt", "er", "ihn", "mit", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PPER", "$,", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wer sein Kleinod als ein Spielger\u00e4t", "tokens": ["Doch", "wer", "sein", "Klei\u00b7nod", "als", "ein", "Spiel\u00b7ge\u00b7r\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sieht l\u00e4chelnd brechen,", "tokens": ["Sieht", "l\u00e4\u00b7chelnd", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und wie aus Gnad' und milder Majest\u00e4t", "tokens": ["Und", "wie", "aus", "Gnad'", "und", "mil\u00b7der", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Mitleidswort will ob dem Toren sprechen,", "tokens": ["Ein", "Mit\u00b7leids\u00b7wort", "will", "ob", "dem", "To\u00b7ren", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Dem Toren, der beweint sein Steckenpferd:", "tokens": ["Dem", "To\u00b7ren", ",", "der", "be\u00b7weint", "sein", "Ste\u00b7cken\u00b7pferd", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ja, dem erlosch die Flamm' am heil'gen Herd", "tokens": ["Ja", ",", "dem", "er\u00b7losch", "die", "Flamm'", "am", "heil'\u00b7gen", "Herd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und seine Nahrung steht in Sumpf und B\u00e4chen.", "tokens": ["Und", "sei\u00b7ne", "Nah\u00b7rung", "steht", "in", "Sumpf", "und", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kannst du ertragen, da\u00df die Augen schaun,", "tokens": ["Kannst", "du", "er\u00b7tra\u00b7gen", ",", "da\u00df", "die", "Au\u00b7gen", "schaun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wem sie sich kehren:", "tokens": ["Wem", "sie", "sich", "keh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dorthin dann wende deinen Blick mit Graun,", "tokens": ["Dor\u00b7thin", "dann", "wen\u00b7de", "dei\u00b7nen", "Blick", "mit", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo wie im Moderschlamm die Massen g\u00e4ren!", "tokens": ["Wo", "wie", "im", "Mo\u00b7der\u00b7schlamm", "die", "Mas\u00b7sen", "g\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOKOM", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verla\u00df den kleinen gr\u00fcnen Fleck, der nur", "tokens": ["Ver\u00b7la\u00df", "den", "klei\u00b7nen", "gr\u00fc\u00b7nen", "Fleck", ",", "der", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch Gottes Huld ward zu des Lebens Flur,", "tokens": ["Durch", "Got\u00b7tes", "Huld", "ward", "zu", "des", "Le\u00b7bens", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sieh, wie sie von deinem Busen zehren!", "tokens": ["Und", "sieh", ",", "wie", "sie", "von", "dei\u00b7nem", "Bu\u00b7sen", "zeh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O h\u00e4tt' ich nimmer meinen Fu\u00df gewandt", "tokens": ["O", "h\u00e4tt'", "ich", "nim\u00b7mer", "mei\u00b7nen", "Fu\u00df", "ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von deiner Erde!", "tokens": ["Von", "dei\u00b7ner", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie segn' ich dich mein reiches kleines Land,", "tokens": ["Wie", "segn'", "ich", "dich", "mein", "rei\u00b7ches", "klei\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du frische Weide einer treuen Herde!", "tokens": ["Du", "fri\u00b7sche", "Wei\u00b7de", "ei\u00b7ner", "treu\u00b7en", "Her\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In dir sah ich die Schande nicht vergn\u00fcgt,", "tokens": ["In", "dir", "sah", "ich", "die", "Schan\u00b7de", "nicht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nicht hohen Geist an alle Schmach geschmiegt,", "tokens": ["Nicht", "ho\u00b7hen", "Geist", "an", "al\u00b7le", "Schmach", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch tiefsten Wahnsinns \u00fcppige Geb\u00e4rde.", "tokens": ["Noch", "tiefs\u00b7ten", "Wahn\u00b7sinns", "\u00fcp\u00b7pi\u00b7ge", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich bin entt\u00e4uscht, und manche Narbe trug", "tokens": ["Ich", "bin", "ent\u00b7t\u00e4uscht", ",", "und", "man\u00b7che", "Nar\u00b7be", "trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich aus dem Streite.", "tokens": ["Ich", "aus", "dem", "Strei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Als auch an meine Brust Verw\u00fcstung schlug", "tokens": ["Als", "auch", "an", "mei\u00b7ne", "Brust", "Ver\u00b7w\u00fcs\u00b7tung", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und forderte die halbverfallne Beute,", "tokens": ["Und", "for\u00b7der\u00b7te", "die", "halb\u00b7ver\u00b7fall\u00b7ne", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ward ich entrissen ihr durch Gottes Huld:", "tokens": ["Ward", "ich", "ent\u00b7ris\u00b7sen", "ihr", "durch", "Got\u00b7tes", "Huld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sein ist die Gnade, mein allein die Schuld;", "tokens": ["Sein", "ist", "die", "Gna\u00b7de", ",", "mein", "al\u00b7lein", "die", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PPOSAT", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dennoch eine Tr\u00fcmmer steh ich heute!", "tokens": ["Und", "den\u00b7noch", "ei\u00b7ne", "Tr\u00fcm\u00b7mer", "steh", "ich", "heu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ward ich nicht ganz der \u00f6den St\u00e4tte gleich,", "tokens": ["Ward", "ich", "nicht", "ganz", "der", "\u00f6\u00b7den", "St\u00e4t\u00b7te", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verfluchtem Grunde,", "tokens": ["Ver\u00b7fluch\u00b7tem", "Grun\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wo Salz gestreut auf Stein und Sch\u00e4del bleich,", "tokens": ["Wo", "Salz", "ge\u00b7streut", "auf", "Stein", "und", "Sch\u00e4\u00b7del", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "NN", "KON", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gibt hier und dort noch eine S\u00e4ule Kunde", "tokens": ["Gibt", "hier", "und", "dort", "noch", "ei\u00b7ne", "S\u00e4u\u00b7le", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vergangner Herrlichkeit: Dank dir mein Land!", "tokens": ["Ver\u00b7gang\u00b7ner", "Herr\u00b7lich\u00b7keit", ":", "Dank", "dir", "mein", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du hast zu fr\u00fch gelegt ein frommes Band", "tokens": ["Du", "hast", "zu", "fr\u00fch", "ge\u00b7legt", "ein", "from\u00b7mes", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Um meine Seele in der Kindheit Stunde.", "tokens": ["Um", "mei\u00b7ne", "See\u00b7le", "in", "der", "Kind\u00b7heit", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So will ich harren denn, und tiefbedr\u00e4ngt", "tokens": ["So", "will", "ich", "har\u00b7ren", "denn", ",", "und", "tief\u00b7be\