"line.4": {"text": "Du frische Weide einer treuen Herde!", "tokens": ["Du", "fri\u00b7sche", "Wei\u00b7de", "ei\u00b7ner", "treu\u00b7en", "Her\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In dir sah ich die Schande nicht vergn\u00fcgt,", "tokens": ["In", "dir", "sah", "ich", "die", "Schan\u00b7de", "nicht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nicht hohen Geist an alle Schmach geschmiegt,", "tokens": ["Nicht", "ho\u00b7hen", "Geist", "an", "al\u00b7le", "Schmach", "ge\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch tiefsten Wahnsinns \u00fcppige Geb\u00e4rde.", "tokens": ["Noch", "tiefs\u00b7ten", "Wahn\u00b7sinns", "\u00fcp\u00b7pi\u00b7ge", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Ich bin entt\u00e4uscht, und manche Narbe trug", "tokens": ["Ich", "bin", "ent\u00b7t\u00e4uscht", ",", "und", "man\u00b7che", "Nar\u00b7be", "trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich aus dem Streite.", "tokens": ["Ich", "aus", "dem", "Strei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Als auch an meine Brust Verw\u00fcstung schlug", "tokens": ["Als", "auch", "an", "mei\u00b7ne", "Brust", "Ver\u00b7w\u00fcs\u00b7tung", "schlug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und forderte die halbverfallne Beute,", "tokens": ["Und", "for\u00b7der\u00b7te", "die", "halb\u00b7ver\u00b7fall\u00b7ne", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ward ich entrissen ihr durch Gottes Huld:", "tokens": ["Ward", "ich", "ent\u00b7ris\u00b7sen", "ihr", "durch", "Got\u00b7tes", "Huld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sein ist die Gnade, mein allein die Schuld;", "tokens": ["Sein", "ist", "die", "Gna\u00b7de", ",", "mein", "al\u00b7lein", "die", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PPOSAT", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dennoch eine Tr\u00fcmmer steh ich heute!", "tokens": ["Und", "den\u00b7noch", "ei\u00b7ne", "Tr\u00fcm\u00b7mer", "steh", "ich", "heu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Ward ich nicht ganz der \u00f6den St\u00e4tte gleich,", "tokens": ["Ward", "ich", "nicht", "ganz", "der", "\u00f6\u00b7den", "St\u00e4t\u00b7te", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verfluchtem Grunde,", "tokens": ["Ver\u00b7fluch\u00b7tem", "Grun\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wo Salz gestreut auf Stein und Sch\u00e4del bleich,", "tokens": ["Wo", "Salz", "ge\u00b7streut", "auf", "Stein", "und", "Sch\u00e4\u00b7del", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "NN", "KON", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gibt hier und dort noch eine S\u00e4ule Kunde", "tokens": ["Gibt", "hier", "und", "dort", "noch", "ei\u00b7ne", "S\u00e4u\u00b7le", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vergangner Herrlichkeit: Dank dir mein Land!", "tokens": ["Ver\u00b7gang\u00b7ner", "Herr\u00b7lich\u00b7keit", ":", "Dank", "dir", "mein", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Du hast zu fr\u00fch gelegt ein frommes Band", "tokens": ["Du", "hast", "zu", "fr\u00fch", "ge\u00b7legt", "ein", "from\u00b7mes", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Um meine Seele in der Kindheit Stunde.", "tokens": ["Um", "mei\u00b7ne", "See\u00b7le", "in", "der", "Kind\u00b7heit", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "So will ich harren denn, und tiefbedr\u00e4ngt", "tokens": ["So", "will", "ich", "har\u00b7ren", "denn", ",", "und", "tief\u00b7be\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Will ich es tragen,", "tokens": ["Will", "ich", "es", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df immer wie zum Sturz die Mauer h\u00e4ngt:", "tokens": ["Da\u00df", "im\u00b7mer", "wie", "zum", "Sturz", "die", "Mau\u00b7er", "h\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KOKOM", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch m\u00f6gen einst erneut die Zinnen ragen.", "tokens": ["Noch", "m\u00f6\u00b7gen", "einst", "er\u00b7neut", "die", "Zin\u00b7nen", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es gibt ja eine stark und milde Hand,", "tokens": ["Es", "gibt", "ja", "ei\u00b7ne", "stark", "und", "mil\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So aus dem Nichts entflammt den Sonnenbrand;", "tokens": ["So", "aus", "dem", "Nichts", "ent\u00b7flammt", "den", "Son\u00b7nen\u00b7brand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie hat auch diesen morschen Bau getragen", "tokens": ["Sie", "hat", "auch", "die\u00b7sen", "mor\u00b7schen", "Bau", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PDAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Bis heute, wo aus dieser kranken Brust", "tokens": ["Bis", "heu\u00b7te", ",", "wo", "aus", "die\u00b7ser", "kran\u00b7ken", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "PWAV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Seufzer drangen.", "tokens": ["Die", "Seuf\u00b7zer", "dran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "O du, dem Wurmes Zucken selbst bewu\u00dft,", "tokens": ["O", "du", ",", "dem", "Wur\u00b7mes", "Zu\u00b7cken", "selbst", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hilf mir und jenen auch, die todumfangen!", "tokens": ["Hilf", "mir", "und", "je\u00b7nen", "auch", ",", "die", "to\u00b7dum\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "PDS", "ADV", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sei gn\u00e4dig, leg an ihr verknorpelt Herz", "tokens": ["Sei", "gn\u00e4\u00b7dig", ",", "leg", "an", "ihr", "ver\u00b7knor\u00b7pelt", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "NE", "APPR", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Leidens Moxa, da\u00df es lebt in Schmerz;", "tokens": ["Des", "Lei\u00b7dens", "Mo\u00b7xa", ",", "da\u00df", "es", "lebt", "in", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ach Herr, sie wu\u00dften nicht was sie begangen!", "tokens": ["Ach", "Herr", ",", "sie", "wu\u00df\u00b7ten", "nicht", "was", "sie", "be\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}