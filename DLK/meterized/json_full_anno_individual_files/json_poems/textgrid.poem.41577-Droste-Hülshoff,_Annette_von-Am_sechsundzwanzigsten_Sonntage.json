.12": {"line.1": {"text": "Doch wer sein Kleinod als ein Spielger\u00e4t", "tokens": ["Doch", "wer", "sein", "Klei\u00b7nod", "als", "ein", "Spiel\u00b7ge\u00b7r\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sieht l\u00e4chelnd brechen,", "tokens": ["Sieht", "l\u00e4\u00b7chelnd", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und wie aus Gnad' und milder Majest\u00e4t", "tokens": ["Und", "wie", "aus", "Gnad'", "und", "mil\u00b7der", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Mitleidswort will ob dem Toren sprechen,", "tokens": ["Ein", "Mit\u00b7leids\u00b7wort", "will", "ob", "dem", "To\u00b7ren", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Dem Toren, der beweint sein Steckenpferd:", "tokens": ["Dem", "To\u00b7ren", ",", "der", "be\u00b7weint", "sein", "Ste\u00b7cken\u00b7pferd", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ja, dem erlosch die Flamm' am heil'gen Herd", "tokens": ["Ja", ",", "dem", "er\u00b7losch", "die", "Flamm'", "am", "heil'\u00b7gen", "Herd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und seine Nahrung steht in Sumpf und B\u00e4chen.", "tokens": ["Und", "sei\u00b7ne", "Nah\u00b7rung", "steht", "in", "Sumpf", "und", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Kannst du ertragen, da\u00df die Augen schaun,", "tokens": ["Kannst", "du", "er\u00b7tra\u00b7gen", ",", "da\u00df", "die", "Au\u00b7gen", "schaun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wem sie sich kehren:", "tokens": ["Wem", "sie", "sich", "keh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dorthin dann wende deinen Blick mit Graun,", "tokens": ["Dor\u00b7thin", "dann", "wen\u00b7de", "dei\u00b7nen", "Blick", "mit", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo wie im Moderschlamm die Massen g\u00e4ren!", "tokens": ["Wo", "wie", "im", "Mo\u00b7der\u00b7schlamm", "die", "Mas\u00b7sen", "g\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOKOM", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verla\u00df den kleinen gr\u00fcnen Fleck, der nur", "tokens": ["Ver\u00b7la\u00df", "den", "klei\u00b7nen", "gr\