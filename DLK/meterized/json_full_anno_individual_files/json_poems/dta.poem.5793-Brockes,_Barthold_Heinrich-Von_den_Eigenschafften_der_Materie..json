{"dta.poem.5793": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um solch ein Druckwerck in den Kreysen,", "tokens": ["Um", "solch", "ein", "Druck\u00b7werck", "in", "den", "Krey\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wodurch sich alles f\u00fcgt, recht kl\u00e4rlich zu erweisen;", "tokens": ["Wo\u00b7durch", "sich", "al\u00b7les", "f\u00fcgt", ",", "recht", "kl\u00e4r\u00b7lich", "zu", "er\u00b7wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "VVFIN", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lasset uns vom Tantz ein leicht Exempel geben.", "tokens": ["So", "las\u00b7set", "uns", "vom", "Tantz", "ein", "leicht", "Ex\u00b7em\u00b7pel", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Man stell\u2019 sich einen Chor", "tokens": ["Man", "stell'", "sich", "ei\u00b7nen", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verschiedner Sch\u00e4fferinnen,", "tokens": ["Ver\u00b7schied\u00b7ner", "Sch\u00e4f\u00b7fe\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jm Schatten eines Waldes, vor;", "tokens": ["Jm", "Schat\u00b7ten", "ei\u00b7nes", "Wal\u00b7des", ",", "vor", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die nach der Stimme Ton den muntern Fu\u00df erheben,", "tokens": ["Die", "nach", "der", "Stim\u00b7me", "Ton", "den", "mun\u00b7tern", "Fu\u00df", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und einen runden Tantz beginnen,", "tokens": ["Und", "ei\u00b7nen", "run\u00b7den", "Tantz", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie h\u00f6ren kaum sobald den Ton erklingen;", "tokens": ["Sie", "h\u00f6\u00b7ren", "kaum", "so\u00b7bald", "den", "Ton", "er\u00b7klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So sieht man sie zugleich in solcher Ordnung springen,", "tokens": ["So", "sieht", "man", "sie", "zu\u00b7gleich", "in", "sol\u00b7cher", "Ord\u00b7nung", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}