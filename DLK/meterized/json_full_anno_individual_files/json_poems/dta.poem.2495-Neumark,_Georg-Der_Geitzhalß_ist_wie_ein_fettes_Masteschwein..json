{"dta.poem.2495": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Geitzhal\u00df ist wie ein fettes  \n Masteschwein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Man kan mit Fug und Recht den auffgeblasnen Reiche\u0303", "tokens": ["Man", "kan", "mit", "Fug", "und", "Recht", "den", "auff\u00b7ge\u00b7blas\u00b7nen", "Reich\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der karg und filtzigt ist mit einem Schweine gleiche\u0303:", "tokens": ["Der", "karg", "und", "filt\u00b7zigt", "ist", "mit", "ei\u00b7nem", "Schwei\u00b7ne", "gleich\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "VVPP", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Geitzhals weil er lebt thut keinem Menschen gut.", "tokens": ["Der", "Geitz\u00b7hals", "weil", "er", "lebt", "thut", "kei\u00b7nem", "Men\u00b7schen", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPER", "VVFIN", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja mit genauer Noht kaum seinem Fleisch und Blut.", "tokens": ["Ja", "mit", "ge\u00b7nau\u00b7er", "Noht", "kaum", "sei\u00b7nem", "Fleisch", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verl\u00e4st er aber nur den Reichthum und mu\u00df sterben", "tokens": ["Ver\u00b7l\u00e4st", "er", "a\u00b7ber", "nur", "den", "Reicht\u00b7hum", "und", "mu\u00df", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "KON", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kommen alsobald die geldbegierig\u2019 Erben", "tokens": ["So", "kom\u00b7men", "al\u00b7so\u00b7bald", "die", "geld\u00b7be\u00b7gie\u00b7rig'", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und theilen da sein Gut mit Freuden unter sich/", "tokens": ["Und", "thei\u00b7len", "da", "sein", "Gut", "mit", "Freu\u00b7den", "un\u00b7ter", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "APPR", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da wird der Mann gelobt/ da\u00df er so sparsamlich", "tokens": ["Da", "wird", "der", "Mann", "ge\u00b7lobt", "/", "da\u00df", "er", "so", "spar\u00b7sam\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$(", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vor sie gelebet hab\u2019; Ein Schwein wird gleich auch diesen", "tokens": ["Vor", "sie", "ge\u00b7le\u00b7bet", "hab'", ";", "Ein", "Schwein", "wird", "gleich", "auch", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenns abgeschlachtet ist \u00fcm W\u00fcrst und Spekk gepriesen/", "tokens": ["Wenns", "ab\u00b7ge\u00b7schlach\u00b7tet", "ist", "\u00fcm", "W\u00fcrst", "und", "Spekk", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "APPRART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das vor verachtet war. Derhalben kans wol seyn", "tokens": ["Das", "vor", "ver\u00b7ach\u00b7tet", "war", ".", "Der\u00b7hal\u00b7ben", "kans", "wol", "seyn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "VVPP", "VAFIN", "$.", "PDS", "VMFIN", "ADV", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df ieder targer Filtz ein grobes Masteschwein.", "tokens": ["Da\u00df", "ie\u00b7der", "tar\u00b7ger", "Filtz", "ein", "gro\u00b7bes", "Mas\u00b7te\u00b7schwein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}