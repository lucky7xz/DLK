{"dta.poem.13118": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.57", "en:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er redte weiter fort, da mir Thalia winckte", "tokens": ["Er", "red\u00b7te", "wei\u00b7ter", "fort", ",", "da", "mir", "Tha\u00b7lia", "win\u00b7ck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "NE", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mich fragte, was dann mich von diesen Reden d\u00fcnckte?", "tokens": ["Mich", "frag\u00b7te", ",", "was", "dann", "mich", "von", "die\u00b7sen", "Re\u00b7den", "d\u00fcnck\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eseynd diese nicht \u201e, sprach sie, auch einer Ehre werth?", "tokens": ["\u201e", "seynd", "die\u00b7se", "nicht", "\u201e", ",", "sprach", "sie", ",", "auch", "ei\u00b7ner", "Eh\u00b7re", "werth", "?"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDS", "PTKNEG", "$(", "$,", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "530\u201dHast du den Innbegriff des Vortrags wohl geh\u00f6rt?", "tokens": ["\"", "Hast", "du", "den", "Inn\u00b7be\u00b7griff", "des", "Vor\u00b7trags", "wohl", "ge\u00b7h\u00f6rt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sezte kurz darauf: mich schmerzt ihr armes Zimmer,", "tokens": ["Ich", "sez\u00b7te", "kurz", "da\u00b7rauf", ":", "mich", "schmerzt", "ihr", "ar\u00b7mes", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und finsters Ruh-Gemach; auf dieses denck\u2019 ich immer.", "tokens": ["Und", "fins\u00b7ters", "Ruh\u00b7Ge\u00b7mach", ";", "auf", "die\u00b7ses", "denck", "ich", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "APPR", "PDAT", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}