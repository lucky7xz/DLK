{"textgrid.poem.60973": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er ist begangen,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ist begangen,", "tokens": ["Er", "ist", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der V\u00f6lkermord!", "tokens": ["Der", "V\u00f6l\u00b7ker\u00b7mord", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nun schwingt die Schlangen,", "tokens": ["Nun", "schwingt", "die", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ihr Furien alle,", "tokens": ["Ihr", "Fu\u00b7ri\u00b7en", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zerst\u00f6rt dem W\u00fcrger", "tokens": ["Zer\u00b7st\u00f6rt", "dem", "W\u00fcr\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der besten B\u00fcrger", "tokens": ["Der", "bes\u00b7ten", "B\u00fcr\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Jedwede Lust,", "tokens": ["Jed\u00b7we\u00b7de", "Lust", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und setzt die Kralle", "tokens": ["Und", "setzt", "die", "Kral\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ihm auf die Brust!", "tokens": ["Ihm", "auf", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Er hat's erf\u00fcllet", "tokens": ["Er", "hat's", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Frevel Ma\u00df!", "tokens": ["Der", "Fre\u00b7vel", "Ma\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr Furien, br\u00fcllet", "tokens": ["Ihr", "Fu\u00b7ri\u00b7en", ",", "br\u00fcl\u00b7let"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Um ihn, den bangen,", "tokens": ["Um", "ihn", ",", "den", "ban\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und lockt den S\u00fcnder,", "tokens": ["Und", "lockt", "den", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den Todverk\u00fcnder,", "tokens": ["Den", "Tod\u00b7ver\u00b7k\u00fcn\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "In unsre Nacht.", "tokens": ["In", "uns\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Er hat's begangen,", "tokens": ["Er", "hat's", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Er hat's vollbracht!", "tokens": ["Er", "hat's", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihr m\u00f6gt ereilen", "tokens": ["Ihr", "m\u00f6gt", "er\u00b7ei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das Unget\u00fcm", "tokens": ["Das", "Un\u00b7ge\u00b7t\u00fcm"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit euren Pfeilen,", "tokens": ["Mit", "eu\u00b7ren", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ihr m\u00f6gt umspannen", "tokens": ["Ihr", "m\u00f6gt", "um\u00b7span\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Im Netz den Eber", "tokens": ["Im", "Netz", "den", "E\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den Kettenweber", "tokens": ["Den", "Ket\u00b7ten\u00b7we\u00b7ber"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Sklaverei!", "tokens": ["Der", "Skla\u00b7ve\u00b7rei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ihr wi\u00dft, Tyrannen", "tokens": ["Ihr", "wi\u00dft", ",", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Sind vogelfrei!", "tokens": ["Sind", "vo\u00b7gel\u00b7frei", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Den Gott zu spielen", "tokens": ["Den", "Gott", "zu", "spie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "War der imstand,", "tokens": ["War", "der", "ims\u00b7tand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der, vor so vielen", "tokens": ["Der", ",", "vor", "so", "vie\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ADV", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Geehrt und pr\u00e4chtig,", "tokens": ["Ge\u00b7ehrt", "und", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "So viel vermochte;", "tokens": ["So", "viel", "ver\u00b7moch\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Doch unterjochte", "tokens": ["Doch", "un\u00b7ter\u00b7joch\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Er jedes Recht:", "tokens": ["Er", "je\u00b7des", "Recht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Er war allm\u00e4chtig", "tokens": ["Er", "war", "all\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und war so schlecht!", "tokens": ["Und", "war", "so", "schlecht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Er baute Tempel", "tokens": ["Er", "bau\u00b7te", "Tem\u00b7pel"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Dem Teufel selbst!", "tokens": ["Dem", "Teu\u00b7fel", "selbst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nun soll den Stempel", "tokens": ["Nun", "soll", "den", "Stem\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Er auch empfangen,", "tokens": ["Er", "auch", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der gro\u00dfe Qu\u00e4ler:", "tokens": ["Der", "gro\u00b7\u00dfe", "Qu\u00e4\u00b7ler", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Es sein die Maler", "tokens": ["Es", "sein", "die", "Ma\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ihm aufgebrannt!", "tokens": ["Ihm", "auf\u00b7ge\u00b7brannt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Er hat's begangen,", "tokens": ["Er", "hat's", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Er ist erkannt!", "tokens": ["Er", "ist", "er\u00b7kannt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ihn schilt Vernichter", "tokens": ["Ihn", "schilt", "Ver\u00b7nich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein ganzes Volk;", "tokens": ["Ein", "gan\u00b7zes", "Volk", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nun schreibt der Richter", "tokens": ["Nun", "schreibt", "der", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ihm jede Tat an.", "tokens": ["Ihm", "je\u00b7de", "Tat", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Zu allen Fristen", "tokens": ["Zu", "al\u00b7len", "Fris\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Gewalt und Listen,", "tokens": ["Ge\u00b7walt", "und", "Lis\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Meineidig Spiel!", "tokens": ["Mei\u00b7nei\u00b7dig", "Spiel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Er ist ein Satan,", "tokens": ["Er", "ist", "ein", "Sa\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.9": {"text": "Die Maske fiel!", "tokens": ["Die", "Mas\u00b7ke", "fiel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Schlachtopfer schleichen", "tokens": ["Schlach\u00b7top\u00b7fer", "schlei\u00b7chen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In W\u00fcstenein;", "tokens": ["In", "W\u00fcs\u00b7ten\u00b7ein", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Voll sind von Leichen", "tokens": ["Voll", "sind", "von", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gefild und Schanzen;", "tokens": ["Ge\u00b7fild", "und", "Schan\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Vor seinem Heere", "tokens": ["Vor", "sei\u00b7nem", "Hee\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Von Meer zu Meere", "tokens": ["Von", "Meer", "zu", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ziehn Tod und Pest:", "tokens": ["Ziehn", "Tod", "und", "Pest", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Er kommt, wir tanzen", "tokens": ["Er", "kommt", ",", "wir", "tan\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Das sch\u00f6nste Fest!", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Fest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Den Volksbezwinger", "tokens": ["Den", "Volks\u00b7be\u00b7zwin\u00b7ger"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gr\u00fc\u00dft sein Geschlecht", "tokens": ["Gr\u00fc\u00dft", "sein", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit blutigem Finger,", "tokens": ["Mit", "blu\u00b7ti\u00b7gem", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Misset\u00e4ter", "tokens": ["Der", "Mis\u00b7se\u00b7t\u00e4\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Zahlreiche Schatten:", "tokens": ["Zahl\u00b7rei\u00b7che", "Schat\u00b7ten", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gefallne Gatten", "tokens": ["Ge\u00b7fall\u00b7ne", "Gat\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Von Fraun bestrickt,", "tokens": ["Von", "Fraun", "be\u00b7strickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Erw\u00fcrgte V\u00e4ter", "tokens": ["Er\u00b7w\u00fcrg\u00b7te", "V\u00e4\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Im Bett erstickt!", "tokens": ["Im", "Bett", "er\u00b7stickt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Von Schmach und Greuel", "tokens": ["Von", "Schmach", "und", "Greu\u00b7el"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Entwirrt sich ihm", "tokens": ["Ent\u00b7wirrt", "sich", "ihm"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein langer Kn\u00e4uel;", "tokens": ["Ein", "lan\u00b7ger", "Kn\u00e4u\u00b7el", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Doch kein Verbrecher", "tokens": ["Doch", "kein", "Ver\u00b7bre\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ist ihm vergleichbar,", "tokens": ["Ist", "ihm", "ver\u00b7gleich\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dem unerweichbar", "tokens": ["Dem", "un\u00b7er\u00b7weich\u00b7bar"], "token_info": ["word", "word"], "pos": ["ART", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der Busen schwoll:", "tokens": ["Der", "Bu\u00b7sen", "schwoll", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Geu\u00df ihm den Becher,", "tokens": ["Geu\u00df", "ihm", "den", "Be\u00b7cher", ","], "token_in