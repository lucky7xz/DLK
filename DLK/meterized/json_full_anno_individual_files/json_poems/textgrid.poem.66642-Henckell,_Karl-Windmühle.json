{"textgrid.poem.66642": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Windm\u00fchle", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst, als ich von Bergesh\u00f6hen gefahren", "tokens": ["J\u00fcngst", ",", "als", "ich", "von", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "NN", "VVPP"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In die flachen Lande nieder,", "tokens": ["In", "die", "fla\u00b7chen", "Lan\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Windm\u00fchle sah ich nach manchen Jahren", "tokens": ["Ei\u00b7ne", "Wind\u00b7m\u00fch\u00b7le", "sah", "ich", "nach", "man\u00b7chen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zum ersten Male wieder.", "tokens": ["Zum", "ers\u00b7ten", "Ma\u00b7le", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie ragt' auf einem kleinen H\u00fcgel \u2013", "tokens": ["Sie", "ragt'", "auf", "ei\u00b7nem", "klei\u00b7nen", "H\u00fc\u00b7gel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lange sie mein Auge sah! \u2013", "tokens": ["Wie", "lan\u00b7ge", "sie", "mein", "Au\u00b7ge", "sah", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie drehte langsam ihre Fl\u00fcgel", "tokens": ["Sie", "dreh\u00b7te", "lang\u00b7sam", "ih\u00b7re", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stand so philosophisch da.", "tokens": ["Und", "stand", "so", "phi\u00b7lo\u00b7so\u00b7phisch", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Da dacht' ich des ewigen Don Quichotte", "tokens": ["Da", "dacht'", "ich", "des", "e\u00b7wi\u00b7gen", "Don", "Qui\u00b7ch\u00b7ot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "NE"], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und meiner phantastischen N\u00f6te,", "tokens": ["Und", "mei\u00b7ner", "phan\u00b7tas\u00b7ti\u00b7schen", "N\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da lacht' ich meiner mit spielendem Spotte", "tokens": ["Da", "lacht'", "ich", "mei\u00b7ner", "mit", "spie\u00b7len\u00b7dem", "Spot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und stau