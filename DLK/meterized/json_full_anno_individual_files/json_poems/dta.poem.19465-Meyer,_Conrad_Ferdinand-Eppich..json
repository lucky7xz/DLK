{"dta.poem.19465": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Eppich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Eppich, mein alter Hausgesell,", "tokens": ["Ep\u00b7pich", ",", "mein", "al\u00b7ter", "Haus\u00b7ge\u00b7sell", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist von jungen Bl\u00e4ttern hell,", "tokens": ["Du", "bist", "von", "jun\u00b7gen", "Bl\u00e4t\u00b7tern", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Wintergr\u00fcn, so still und streng,", "tokens": ["Dein", "Win\u00b7ter\u00b7gr\u00fcn", ",", "so", "still", "und", "streng", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertr\u00e4gt sich's mit dem Lenzgedr\u00e4ng?", "tokens": ["Ver\u00b7tr\u00e4gt", "sich's", "mit", "dem", "Lenz\u00b7ge\u00b7dr\u00e4ng", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u2014 \u201eWarum denn nicht? Wie meines hat", "tokens": ["\u201e", "Wa\u00b7rum", "denn", "nicht", "?", "Wie", "mei\u00b7nes", "hat"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "$(", "PWAV", "ADV", "PTKNEG", "$.", "PWAV", "PIS", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein Leben alt und junges Blatt,", "tokens": ["Dein", "Le\u00b7ben", "alt", "und", "jun\u00b7ges", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eins streng und dunkel, eines licht", "tokens": ["Eins", "streng", "und", "dun\u00b7kel", ",", "ei\u00b7nes", "licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Lenz und Lust! Warum denn nicht?\u201c", "tokens": ["Von", "Lenz", "und", "Lust", "!", "Wa\u00b7rum", "denn", "nicht", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "PWAV", "ADV", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}