{"dta.poem.8478": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Elegie  \n bei dem Grabe meines Vaters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Selig, Vater, selig bist auch du!", "tokens": ["Se\u00b7lig", ",", "Va\u00b7ter", ",", "se\u00b7lig", "bist", "auch", "du", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADJD", "VAFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Engel brachten dir den Kranz, und riefen;", "tokens": ["En\u00b7gel", "brach\u00b7ten", "dir", "den", "Kranz", ",", "und", "rie\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und du gingst in Gottes Ruh;", "tokens": ["Und", "du", "gingst", "in", "Got\u00b7tes", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Wandelst \u00fcber Millionen Sternen,", "tokens": ["Wan\u00b7delst", "\u00fc\u00b7ber", "Mil\u00b7lion\u00b7en", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Siehst die Handvoll Staub, die Erde, nicht;", "tokens": ["Siehst", "die", "Hand\u00b7voll", "Staub", ",", "die", "Er\u00b7de", ",", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schwebst im Wink durch tausend Sonnenfernen,", "tokens": ["Schwebst", "im", "Wink", "durch", "tau\u00b7send", "Son\u00b7nen\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schauest Gottes Angesicht;", "tokens": ["Schau\u00b7est", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Siehst das Buch der Welten aufgeschlagen;", "tokens": ["Siehst", "das", "Buch", "der", "Wel\u00b7ten", "auf\u00b7ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trinkest durstig aus dem Lebensquell;", "tokens": ["Trin\u00b7kest", "durs\u00b7tig", "aus", "dem", "Le\u00b7bens\u00b7quell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "N\u00e4chte, voll von Labirinthen, tagen,", "tokens": ["N\u00e4ch\u00b7te", ",", "voll", "von", "La\u00b7bi\u00b7rin\u00b7then", ",", "ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dein Blick wird himmelhell.", "tokens": ["Und", "dein", "Blick", "wird", "him\u00b7mel\u00b7hell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch in deiner Ueberwinderkrone", "tokens": ["Doch", "in", "dei\u00b7ner", "Ue\u00b7ber\u00b7win\u00b7der\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Senkst du noch den Vaterblick auf mich;", "tokens": ["Senkst", "du", "noch", "den", "Va\u00b7ter\u00b7blick", "auf", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Betest f\u00fcr mich an Jehovas Throne,", "tokens": ["Be\u00b7test", "f\u00fcr", "mich", "an", "Je\u00b7ho\u00b7vas", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und Jehova h\u00f6ret dich.", "tokens": ["Und", "Je\u00b7ho\u00b7va", "h\u00f6\u00b7ret", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schwebe, wann der Tropfen Zeit verrinnet,", "tokens": ["Schwe\u00b7be", ",", "wann", "der", "Trop\u00b7fen", "Zeit", "ver\u00b7rin\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den mir Gott aus seiner Urne gab,", "tokens": ["Den", "mir", "Gott", "aus", "sei\u00b7ner", "Ur\u00b7ne", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schwebe, wann mein Todeskampf beginnet,", "tokens": ["Schwe\u00b7be", ",", "wann", "mein", "To\u00b7des\u00b7kampf", "be\u00b7gin\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf mein Sterbebett' herab:", "tokens": ["Auf", "mein", "Ster\u00b7be\u00b7bett'", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dass mir deine Palme K\u00fchlung wehe,", "tokens": ["Dass", "mir", "dei\u00b7ne", "Pal\u00b7me", "K\u00fch\u00b7lung", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "K\u00fchlung, wie von Lebensb\u00e4umen tr\u00e4uft;", "tokens": ["K\u00fch\u00b7lung", ",", "wie", "von", "Le\u00b7bens\u00b7b\u00e4u\u00b7men", "tr\u00e4uft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dass ich sonder Graun die Th\u00e4ler sehe,", "tokens": ["Dass", "ich", "son\u00b7der", "Graun", "die", "Th\u00e4\u00b7ler", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo die Auferstehung reift;", "tokens": ["Wo", "die", "Auf\u00b7er\u00b7ste\u00b7hung", "reift", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dass mit dir ich durch die Himmel schwebe,", "tokens": ["Dass", "mit", "dir", "ich", "durch", "die", "Him\u00b7mel", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wonnestralend und begl\u00fcckt, wie du;", "tokens": ["Won\u00b7nest\u00b7ra\u00b7lend", "und", "be\u00b7gl\u00fcckt", ",", "wie", "du", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit dir auf Einem Sterne lebe,", "tokens": ["Und", "mit", "dir", "auf", "Ei\u00b7nem", "Ster\u00b7ne", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und in Gottes Schoosse ruh.", "tokens": ["Und", "in", "Got\u00b7tes", "Schoos\u00b7se", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Gr\u00fcn' indessen, Strauch der Rosenblume,", "tokens": ["Gr\u00fcn'", "in\u00b7des\u00b7sen", ",", "Strauch", "der", "Ro\u00b7sen\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deinen Purpur auf sein Grab zu streun.", "tokens": ["Dei\u00b7nen", "Pur\u00b7pur", "auf", "sein", "Grab", "zu", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schlummre, wie im stillen Heiligthume,", "tokens": ["Schlumm\u00b7re", ",", "wie", "im", "stil\u00b7len", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hinges\u00e4etes Gebein.", "tokens": ["Hin\u00b7ge\u00b7s\u00e4e\u00b7tes", "Ge\u00b7bein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}