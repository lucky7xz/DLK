{"textgrid.poem.48467": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "41. Von sechs Schwestern auf deroselben Vettern Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bald der vierte Tag des Wintermonats kommen,", "tokens": ["So", "bald", "der", "vier\u00b7te", "Tag", "des", "Win\u00b7ter\u00b7mo\u00b7nats", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so hat man in der Welt auch andre Zeit vernommen.", "tokens": ["so", "hat", "man", "in", "der", "Welt", "auch", "and\u00b7re", "Zeit", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Sonnen g\u00fcldnes Liecht, das nunmehr ganz und gar", "tokens": ["Der", "Son\u00b7nen", "g\u00fcld\u00b7nes", "Liecht", ",", "das", "nun\u00b7mehr", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in so viel Wochen nicht gesehen worden war,", "tokens": ["in", "so", "viel", "Wo\u00b7chen", "nicht", "ge\u00b7se\u00b7hen", "wor\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "PTKNEG", "VVPP", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "go\u00df seine Strahlen aus. Die tr\u00fcben Wolken liefen,", "tokens": ["go\u00df", "sei\u00b7ne", "Strah\u00b7len", "aus", ".", "Die", "tr\u00fc\u00b7ben", "Wol\u00b7ken", "lie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "und lie\u00dfen eilends nach mit Regen so zu triefen.", "tokens": ["und", "lie\u00b7\u00dfen", "ei\u00b7lends", "nach", "mit", "Re\u00b7gen", "so", "zu", "trie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die L\u00fcfte wurden klar. Anstatt da\u00df Sturm und Wind", "tokens": ["Die", "L\u00fcf\u00b7te", "wur\u00b7den", "klar", ".", "An\u00b7statt", "da\u00df", "Sturm", "und", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "NN", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu Land und See erschreckt' so manches Mutterkind,", "tokens": ["zu", "Land", "und", "See", "er\u00b7schreckt'", "so", "man\u00b7ches", "Mut\u00b7ter\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "da trat der Westwind ein mit seinem sanften Sausen.", "tokens": ["da", "trat", "der", "West\u00b7wind", "ein", "mit", "sei\u00b7nem", "sanf\u00b7ten", "Sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Flut der gr\u00fcnen See lie\u00df nach sich so zu krausen.", "tokens": ["Die", "Flut", "der", "gr\u00fc\u00b7nen", "See", "lie\u00df", "nach", "sich", "so", "zu", "krau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "APPR", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es fiel ein sanfter Schnee, und ein gesunder Frost", "tokens": ["Es", "fiel", "ein", "sanf\u00b7ter", "Schnee", ",", "und", "ein", "ge\u00b7sun\u00b7der", "Frost"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "macht' aus die weiche Zeit, uns eine frische Lust.", "tokens": ["macht'", "aus", "die", "wei\u00b7che", "Zeit", ",", "uns", "ei\u00b7ne", "fri\u00b7sche", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Herr Vetter, diese Zier ist euch zur Zier geschehen,", "tokens": ["Herr", "Vet\u00b7ter", ",", "die\u00b7se", "Zier", "ist", "euch", "zur", "Zier", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PDAT", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "mit der ihr euren Tag gekr\u00f6net k\u00f6nnet sehen,", "tokens": ["mit", "der", "ihr", "eu\u00b7ren", "Tag", "ge\u00b7kr\u00f6\u00b7net", "k\u00f6n\u00b7net", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "den wir mit euch begehn aus herzlicher Begier.", "tokens": ["den", "wir", "mit", "euch", "be\u00b7gehn", "aus", "herz\u00b7li\u00b7cher", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sonst ist hier Traurigs nichts, als da\u00df ihr nicht seid hier.", "tokens": ["Sonst", "ist", "hier", "Trau\u00b7rigs", "nichts", ",", "als", "da\u00df", "ihr", "nicht", "seid", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "PIS", "$,", "KOKOM", "KOUS", "PPER", "PTKNEG", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der allerh\u00f6chste Gott, der woll' euch langes Leben", "tokens": ["Der", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Gott", ",", "der", "woll'", "euch", "lan\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "in Gl\u00fcck und Ungl\u00fcck Heil und alle Wolfahrt geben!", "tokens": ["in", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "Heil", "und", "al\u00b7le", "Wol\u00b7fahrt", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und da\u00df man r\u00fchmen mag, wie wol er euch getan,", "tokens": ["Und", "da\u00df", "man", "r\u00fch\u00b7men", "mag", ",", "wie", "wol", "er", "euch", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVINF", "VMFIN", "$,", "PWAV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "so bring' er euch allhier bald frisch und gl\u00fccklich an!", "tokens": ["so", "bring'", "er", "euch", "all\u00b7hier", "bald", "frisch", "und", "gl\u00fcck\u00b7lich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und da\u00df ihr gleichwol auch in Absein seid gebunden,", "tokens": ["Und", "da\u00df", "ihr", "gleich\u00b7wol", "auch", "in", "Ab\u00b7sein", "seid", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "so sei euch dieses Band zu Ehren aufgewunden,", "tokens": ["so", "sei", "euch", "die\u00b7ses", "Band", "zu", "Eh\u00b7ren", "auf\u00b7ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "das keiner zwar von uns euch itzt anlegen kan;", "tokens": ["das", "kei\u00b7ner", "zwar", "von", "uns", "euch", "itzt", "an\u00b7le\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "doch binden wir euch mehr mit unsern Herzen an.", "tokens": ["doch", "bin\u00b7den", "wir", "euch", "mehr", "mit", "un\u00b7sern", "Her\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Soll di\u00df dem H\u00f6chsten wol nicht gehn zu Herzen-Grunde?", "tokens": ["Soll", "di\u00df", "dem", "H\u00f6chs\u00b7ten", "wol", "nicht", "gehn", "zu", "Her\u00b7zen\u00b7Grun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "NN", "ADV", "PTKNEG", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sechs Schwestern sprechen es zugleich aus einem Munde.", "tokens": ["Sechs", "Schwes\u00b7tern", "spre\u00b7chen", "es", "zu\u00b7gleich", "aus", "ei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So bald der vierte Tag des Wintermonats kommen,", "tokens": ["So", "bald", "der", "vier\u00b7te", "Tag", "d