{"dta.poem.16043": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Uebel ist bestrebt sich selbst zu \u00fcberwinden,", "tokens": ["Das", "Ue\u00b7bel", "ist", "be\u00b7strebt", "sich", "selbst", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nur das Uebel lehrt den Menschen K\u00fcnst' erfinden;", "tokens": ["Denn", "nur", "das", "Ue\u00b7bel", "lehrt", "den", "Men\u00b7schen", "K\u00fcnst'", "er\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das aber ist der Zweck von Kunst und Wissenschaft,", "tokens": ["Das", "a\u00b7ber", "ist", "der", "Zweck", "von", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Uebel in der Welt zu brechen Spitz' und Kraft;", "tokens": ["Dem", "Ue\u00b7bel", "in", "der", "Welt", "zu", "bre\u00b7chen", "Spitz'", "und", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus der Nothwendigkeit und des Naturzwangs Ketten", "tokens": ["Aus", "der", "Noth\u00b7wen\u00b7dig\u00b7keit", "und", "des", "Na\u00b7tur\u00b7zwangs", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "--++-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Den Menschen ins Gebiet der Freiheit hinzuretten.", "tokens": ["Den", "Men\u00b7schen", "ins", "Ge\u00b7biet", "der", "Frei\u00b7heit", "hin\u00b7zu\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Durch Kunst und Wissenschaft ist er soweit entronnen,", "tokens": ["Durch", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", "ist", "er", "so\u00b7weit", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat durch sie der Natur soviel schon abgewonnen;", "tokens": ["Hat", "durch", "sie", "der", "Na\u00b7tur", "so\u00b7viel", "schon", "ab\u00b7ge\u00b7won\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Durch Uebung mehr und mehr wird er derselben Meister,", "tokens": ["Durch", "Ue\u00b7bung", "mehr", "und", "mehr", "wird", "er", "der\u00b7sel\u00b7ben", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bis endlich wird sein Geist beherrschen ihre Geister.", "tokens": ["Bis", "end\u00b7lich", "wird", "sein", "Geist", "be\u00b7herr\u00b7schen", "ih\u00b7re", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}