{"dta.poem.10283": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Weit fort, im s&#252;dlichen Italien war es. &#8212;             ", "tokens": ["Weit", "fort", ",", "im", "s", "&#252;", "dli\u00b7chen", "I\u00b7ta\u00b7li\u00b7en", "war", "es", ".", "&#8212;"], "token_info": ["word", "word", "punct", "word", "word", "XML_entity", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["ADJD", "PTKVZ", "$,", "APPRART", "ADJA", "$(", "ADJA", "NE", "VAFIN", "PPER", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du schautest vom Altane in den Garten", "tokens": ["Du", "schau\u00b7test", "vom", "Al\u00b7ta\u00b7ne", "in", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf weiterhellte, festbelebte Wege.", "tokens": ["Auf", "wei\u00b7ter\u00b7hell\u00b7te", ",", "fest\u00b7be\u00b7leb\u00b7te", "We\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann hob dein Auge sich, und deine Seele", "tokens": ["Dann", "hob", "dein", "Au\u00b7ge", "sich", ",", "und", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verlor sich in das Schweigen ferner Landschaft:", "tokens": ["Ver\u00b7lor", "sich", "in", "das", "Schwei\u00b7gen", "fer\u00b7ner", "Land\u00b7schaft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie lange willst du tr\u00e4umen, deutsche Frau,", "tokens": ["Wie", "lan\u00b7ge", "willst", "du", "tr\u00e4u\u00b7men", ",", "deut\u00b7sche", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von glutdurchtr\u00e4nkter Nacht des Romeo?", "tokens": ["Von", "glut\u00b7durch\u00b7tr\u00e4nk\u00b7ter", "Nacht", "des", "Ro\u00b7meo", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Weckt dir Erinnerung nicht liebe Bilder", "tokens": ["Weckt", "dir", "E\u00b7rin\u00b7ne\u00b7rung", "nicht", "lie\u00b7be", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Aus unbarmherzig strenger Winternacht,", "tokens": ["Aus", "un\u00b7barm\u00b7her\u00b7zig", "stren\u00b7ger", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die mit gesenktem Augenlid umd\u00e4mmert", "tokens": ["Die", "mit", "ge\u00b7senk\u00b7tem", "Au\u00b7gen\u00b7lid", "um\u00b7d\u00e4m\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die H\u00fcnengr\u00e4ber deines rauhen Strandes?", "tokens": ["Die", "H\u00fc\u00b7nen\u00b7gr\u00e4\u00b7ber", "dei\u00b7nes", "rau\u00b7hen", "Stran\u00b7des", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}