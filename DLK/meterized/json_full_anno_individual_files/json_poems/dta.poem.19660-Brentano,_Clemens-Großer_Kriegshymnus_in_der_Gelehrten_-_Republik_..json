{"dta.poem.19660": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfer Kriegshymnus in der Gelehrten -  \n  Republik .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sollt ich ein Feldherr seyn und Kriegesheere f\u00fchren,               ", "tokens": ["Sollt", "ich", "ein", "Feld\u00b7herr", "seyn", "und", "Krie\u00b7ges\u00b7hee\u00b7re", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wollt ich stracks auszieren", "tokens": ["So", "wollt", "ich", "stracks", "aus\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ganze Kriegesheer", "tokens": ["Das", "gan\u00b7ze", "Krie\u00b7ges\u00b7heer"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem solchen Volk, das hold den B\u00fcchern w\u00e4r,", "tokens": ["Mit", "ei\u00b7nem", "sol\u00b7chen", "Volk", ",", "das", "hold", "den", "B\u00fc\u00b7chern", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Studenten m\u00fcsten seyn", "tokens": ["Die", "Stu\u00b7den\u00b7ten", "m\u00fcs\u00b7ten", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VAINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Meine beste Bursch und F\u00fchrer,", "tokens": ["Mei\u00b7ne", "bes\u00b7te", "Bursch", "und", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Gelehrten Feindaussp\u00fcrer;", "tokens": ["Die", "Ge\u00b7lehr\u00b7ten", "Fein\u00b7daus\u00b7sp\u00fc\u00b7rer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00f6bus V\u00f6lker in gemein", "tokens": ["F\u00f6\u00b7bus", "V\u00f6l\u00b7ker", "in", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00fcsten die Feinde verjagen und d\u00e4mpfen,", "tokens": ["M\u00fcs\u00b7ten", "die", "Fein\u00b7de", "ver\u00b7ja\u00b7gen", "und", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "KON", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "M\u00fcsten uns helfen und ritterlich k\u00e4mpfen.", "tokens": ["M\u00fcs\u00b7ten", "uns", "hel\u00b7fen", "und", "rit\u00b7ter\u00b7lich", "k\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "B\u00fcchsmeister sollten seyn die s\u00fc\u00dfen Musikanten,", "tokens": ["B\u00fcchs\u00b7meis\u00b7ter", "soll\u00b7ten", "seyn", "die", "s\u00fc\u00b7\u00dfen", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Helikons Verwandten,", "tokens": ["Die", "He\u00b7li\u00b7kons", "Ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Orgeln Freudenschall,", "tokens": ["Der", "Or\u00b7geln", "Freu\u00b7den\u00b7schall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sollt an Trompetenstatt erklingen \u00fcberall,", "tokens": ["Sollt", "an", "Trom\u00b7pe\u00b7ten\u00b7statt", "er\u00b7klin\u00b7gen", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bachus und sein Kammerad,", "tokens": ["Ba\u00b7chus", "und", "sein", "Kam\u00b7me\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ceres sollten uns wohl geben", "tokens": ["Ce\u00b7res", "soll\u00b7ten", "uns", "wohl", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Brod und Speis und Wein zu leben;", "tokens": ["Brod", "und", "Speis", "und", "Wein", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frischen uns nach Krieges-Rath,", "tokens": ["Fri\u00b7schen", "uns", "nach", "Krie\u00b7ge\u00b7sRath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Musen und Grazien m\u00fcsten mitk\u00e4mpfen,", "tokens": ["Mu\u00b7sen", "und", "Gra\u00b7zi\u00b7en", "m\u00fcs\u00b7ten", "mit\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "M\u00fcsten die Feinde verjagen und d\u00e4mpfen.", "tokens": ["M\u00fcs\u00b7ten", "die", "Fein\u00b7de", "ver\u00b7ja\u00b7gen", "und", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Feder sollte mir anstatt der Schwerdter dienen,", "tokens": ["Die", "Fe\u00b7der", "soll\u00b7te", "mir", "an\u00b7statt", "der", "Schwerd\u00b7ter", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir wollten uns erk\u00fchnen", "tokens": ["Wir", "woll\u00b7ten", "uns", "er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In alle Welt zu gehn.", "tokens": ["In", "al\u00b7le", "Welt", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich deucht, ich wollte wohl mit diesem Volk bestehn,", "tokens": ["Mich", "deucht", ",", "ich", "woll\u00b7te", "wohl", "mit", "die\u00b7sem", "Volk", "be\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In Gefahr und Kriegesnoth;", "tokens": ["In", "Ge\u00b7fahr", "und", "Krie\u00b7ges\u00b7noth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schriftgelehrte und Juristen", "tokens": ["Schrift\u00b7ge\u00b7lehr\u00b7te", "und", "Ju\u00b7ris\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fcsten sich zu streiten r\u00fcsten,", "tokens": ["M\u00fcs\u00b7ten", "sich", "zu", "strei\u00b7ten", "r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die, ", "tokens": ["Die", ","], "token_info": ["word", "punct"], "pos": ["ART", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "M\u00fcsten uns helfen auch ritterlich k\u00e4mpfen,", "tokens": ["M\u00fcs\u00b7ten", "uns", "hel\u00b7fen", "auch", "rit\u00b7ter\u00b7lich", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "M\u00fcsten die Feinde verjagen und d\u00e4mpfen.", "tokens": ["M\u00fcs\u00b7ten", "die", "Fein\u00b7de", "ver\u00b7ja\u00b7gen", "und", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}