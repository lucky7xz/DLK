{"dta.poem.21027": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcI.  \n Einer alten Braut.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebster hie\u00df mich Braut/ und meynte nur", "tokens": ["Mein", "Liebs\u00b7ter", "hie\u00df", "mich", "Braut", "/", "und", "meyn\u00b7te", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$(", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mein Geld/", "tokens": ["mein", "Geld", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Pest rie\u00df mich von ihm/ und lie\u00df ihn in der Welt.", "tokens": ["Die", "Pest", "rie\u00df", "mich", "von", "ihm", "/", "und", "lie\u00df", "ihn", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPER", "$(", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Tod sa\u00df in der Scho\u00df der Winter in den Beine\u0303/", "tokens": ["Der", "Tod", "sa\u00df", "in", "der", "Scho\u00df", "der", "Win\u00b7ter", "in", "den", "Bein\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist Tod und Winter hier/ so mu\u00df ein ieder weinen.", "tokens": ["Ist", "Tod", "und", "Win\u00b7ter", "hier", "/", "so", "mu\u00df", "ein", "ie\u00b7der", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "$(", "ADV", "VMFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}