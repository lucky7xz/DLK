{"textgrid.poem.32535": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Tochter Zeusens von Dionen,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Tochter Zeusens von Dionen,", "tokens": ["Du", "Toch\u00b7ter", "Zeu\u00b7sens", "von", "Di\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was h\u00e4ltst du da die schlaugebogne Hand,", "tokens": ["Was", "h\u00e4ltst", "du", "da", "die", "schlau\u00b7ge\u00b7bog\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hier das neidische Gewand", "tokens": ["Und", "hier", "das", "nei\u00b7di\u00b7sche", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor die allm\u00e4chtge Sch\u00f6nheitszonen?", "tokens": ["Vor", "die", "all\u00b7m\u00e4cht\u00b7ge", "Sch\u00f6n\u00b7heits\u00b7zo\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Was ziehst du deinen Marmorleib", "tokens": ["Was", "ziehst", "du", "dei\u00b7nen", "Mar\u00b7mor\u00b7leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So furchtsam ein, und blickst besch\u00e4mt zur Erde?", "tokens": ["So", "furcht\u00b7sam", "ein", "