{"dta.poem.12131": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah am Abende des Mondes wachsend Horn,", "tokens": ["Ich", "sah", "am", "A\u00b7ben\u00b7de", "des", "Mon\u00b7des", "wach\u00b7send", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, seit ich wanderte, sich hatte neu geborn;", "tokens": ["Der", ",", "seit", "ich", "wan\u00b7der\u00b7te", ",", "sich", "hat\u00b7te", "neu", "ge\u00b7born", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRF", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sprach: die Sonne hat mir manchen Tag gelacht,", "tokens": ["Und", "sprach", ":", "die", "Son\u00b7ne", "hat", "mir", "man\u00b7chen", "Tag", "ge\u00b7lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun tr\u00f6stet auch der Mond den Wandrer in der Nacht.", "tokens": ["Nun", "tr\u00f6s\u00b7tet", "auch", "der", "Mond", "den", "Wand\u00b7rer", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "met