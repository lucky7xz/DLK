{"textgrid.poem.37050": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Knabenzeit", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00fccklich, wem das Knabenkleid", "tokens": ["Wie", "gl\u00fcck\u00b7lich", ",", "wem", "das", "Kna\u00b7ben\u00b7kleid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch um die Schultern fliegt,", "tokens": ["Noch", "um", "die", "Schul\u00b7tern", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wem l\u00e4chelnde Zufriedenheit", "tokens": ["Wem", "l\u00e4\u00b7cheln\u00b7de", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Den jungen Busen wiegt.", "tokens": ["Den", "jun\u00b7gen", "Bu\u00b7sen", "wiegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Kr\u00e4usel, und das Steckenpferd,", "tokens": ["Der", "Kr\u00e4u\u00b7sel", ",", "und", "das", "Ste\u00b7cken\u00b7pferd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dem er herrisch sitzt,", "tokens": ["Auf", "dem", "er", "her\u00b7risch", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "