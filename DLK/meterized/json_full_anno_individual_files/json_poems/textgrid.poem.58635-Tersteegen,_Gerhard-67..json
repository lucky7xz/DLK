{"textgrid.poem.58635": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gut ist doch der alte Weg, das innre Christenleben,", "tokens": ["Wie", "gut", "ist", "doch", "der", "al\u00b7te", "Weg", ",", "das", "inn\u00b7re", "Chris\u00b7ten\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Dem sich die Patriarchen schon von Anbeginn ergeben!", "tokens": ["Dem", "sich", "die", "Pat\u00b7ri\u00b7ar\u00b7chen", "schon", "von", "An\u00b7be\u00b7ginn", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Man wu\u00dft' nicht Schein noch Meinungsstreit, man starb der Kreatur,", "tokens": ["Man", "wu\u00dft'", "nicht", "Schein", "noch", "Mei\u00b7nungs\u00b7streit", ",", "man", "starb", "der", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$,", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Man lebte stets mit Gott gemein durch Glaub' und Liebe nur.", "tokens": ["Man", "leb\u00b7te", "stets", "mit", "Gott", "ge\u00b7mein", "durch", "Glaub'", "und", "Lie\u00b7be", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "ADJD", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Dies nennt man die geheime Lehr in diesen finstern Tagen,", "tokens": ["Dies", "nennt", "man", "die", "ge\u00b7hei\u00b7me", "Lehr", "in", "die\u00b7sen", "fins\u00b7tern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ART", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Man f\u00fcrchtet diesen guten Weg, man will darnach nicht fragen.", "tokens": ["Man", "f\u00fcrch\u00b7tet", "die\u00b7sen", "gu\u00b7ten", "Weg", ",", "man", "will", "dar\u00b7nach", "nicht", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDAT", "ADJA", "NN", "$,", "PIS", "VMFIN", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Wohl dem, der diesen Weg geschaut, der schlie\u00df' die Augen zu", "tokens": ["Wohl", "dem", ",", "der", "die\u00b7sen", "Weg", "ge\u00b7schaut", ",", "der", "schlie\u00df'", "die", "Au\u00b7gen", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PDAT", "NN", "VVPP", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Und wandle nur getrost drin fort zur wahren Seelenruh!", "tokens": ["Und", "wand\u00b7le", "nur", "ge\u00b7trost", "drin", "fort", "zur", "wah\u00b7ren", "See\u00b7len\u00b7ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "PTKVZ", "