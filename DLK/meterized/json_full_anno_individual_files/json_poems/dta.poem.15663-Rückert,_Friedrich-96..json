{"dta.poem.15663": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kr\u00e4fte Triebrad mu\u00df, das blinde, sich bequemen,", "tokens": ["Der", "Kr\u00e4f\u00b7te", "Trie\u00b7brad", "mu\u00df", ",", "das", "blin\u00b7de", ",", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "$,", "ART", "ADJA", "$,", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Menschen immer mehr die Arbeit abzunehmen;", "tokens": ["Dem", "Men\u00b7schen", "im\u00b7mer", "mehr", "die", "Ar\u00b7beit", "ab\u00b7zu\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df einst der freie Geist nichtmehr dem Stoffe diene,", "tokens": ["Da\u00df", "einst", "der", "frei\u00b7e", "Geist", "nicht\u00b7mehr", "dem", "Stof\u00b7fe", "die\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich nur als Denker f\u00fchl' und Lenker der Maschiene.", "tokens": ["Sich", "nur", "als", "Den\u00b7ker", "f\u00fchl'", "und", "Len\u00b7ker", "der", "Ma\u00b7schie\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KOUS", "NN", "VVFIN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur la\u00dft, wenn Alles soll Mechanik seyn auf Erden,", "tokens": ["Nur", "la\u00dft", ",", "wenn", "Al\u00b7les", "soll", "Me\u00b7cha\u00b7nik", "seyn", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PIS", "VMFIN", "NN", "PPOSAT", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Geistes Denkgesch\u00e4ft nicht auch mechanisch werden.", "tokens": ["Des", "Geis\u00b7tes", "Denk\u00b7ge\u00b7sch\u00e4ft", "nicht", "auch", "me\u00b7cha\u00b7nisch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}