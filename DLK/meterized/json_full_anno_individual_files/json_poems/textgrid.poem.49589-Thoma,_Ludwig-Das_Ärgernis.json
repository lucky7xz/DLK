{"textgrid.poem.49589": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das \u00c4rgernis", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist das doch in diesen Tagen", "tokens": ["Was", "ist", "das", "doch", "in", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Summen, Surren, Hasten, Jagen!", "tokens": ["Ein", "Sum\u00b7men", ",", "Sur\u00b7ren", ",", "Has\u00b7ten", ",", "Ja\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Boden welch ein froh Gew\u00fchl!", "tokens": ["Am", "Bo\u00b7den", "welch", "ein", "froh", "Ge\u00b7w\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PWAT", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder K\u00e4fer zeigt Gef\u00fchl", "tokens": ["Ein", "je\u00b7der", "K\u00e4\u00b7fer", "zeigt", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mu\u00df sein Weibchen wild umfassen.", "tokens": ["Und", "mu\u00df", "sein", "Weib\u00b7chen", "wild", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die ganze Welt ist ausgelassen,", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "aus\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und jedes Tier begreift sein Leben", "tokens": ["Und", "je\u00b7des", "Tier", "be\u00b7greift", "sein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In Liebe nehmen, Liebe geben.", "tokens": ["In", "Lie\u00b7be", "neh\u00b7men", ",", "Lie\u00b7be", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das ist ein Werben, Jubeln, Klagen", "tokens": ["Das", "ist", "ein", "Wer\u00b7ben", ",", "Ju\u00b7beln", ",", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In diesen sch\u00f6nen Fr\u00fchlingstagen!", "tokens": ["In", "die\u00b7sen", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Ochse steht am Wiesenrand,", "tokens": ["Ein", "O\u00b7chse", "steht", "am", "Wie\u00b7sen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sein kastrierter Viehverstand", "tokens": ["Und", "sein", "kast\u00b7rier\u00b7ter", "Vieh\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df unber\u00fchrt von diesem Treiben", "tokens": ["Mu\u00df", "un\u00b7be\u00b7r\u00fchrt", "von", "die\u00b7sem", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dieser Sinnenfreude bleiben.", "tokens": ["Und", "die\u00b7ser", "Sin\u00b7nen\u00b7freu\u00b7de", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er f\u00fchlt im Fressen sich gest\u00f6rt", "tokens": ["Er", "f\u00fchlt", "im", "Fres\u00b7sen", "sich", "ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von allem, was er sieht und h\u00f6rt.", "tokens": ["Von", "al\u00b7lem", ",", "was", "er", "sieht", "und", "h\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da wird gejagt und wird getanzt", "tokens": ["Da", "wird", "ge\u00b7jagt", "und", "wird", "ge\u00b7tanzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "KON", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sich ganz einfach fortgepflanzt!", "tokens": ["Und", "sich", "ganz", "ein\u00b7fach", "fort\u00b7ge\u00b7pflanzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+