{"dta.poem.688": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n Tibi sacrificabo hostiam laudis.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Avff meine Seel! auff! rei\u00df mit macht entzwey ", "tokens": ["Avff", "mei\u00b7ne", "Seel", "!", "auff", "!", "rei\u00df", "mit", "macht", "ent\u00b7zwey"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$.", "PTKVZ", "$.", "VVFIN", "APPR", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das feste netz/ mit dem dich grimme schmertzen", "tokens": ["Das", "fes\u00b7te", "netz", "/", "mit", "dem", "dich", "grim\u00b7me", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vnd harte noth/ vnd angst die pest der hertzen", "tokens": ["Vnd", "har\u00b7te", "noth", "/", "vnd", "angst", "die", "pest", "der", "hert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$(", "KON", "VVFIN", "ART", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bi\u00dfher verstrickt/ wir wollen sorgenfrey", "tokens": ["Bi\u00df\u00b7her", "ver\u00b7strickt", "/", "wir", "wol\u00b7len", "sor\u00b7gen\u00b7frey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$(", "PPER", "VMFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vnd kummer - lo\u00df des h\u00f6chsten g\u00fct erheben", "tokens": ["Vnd", "kum\u00b7mer", "lo\u00df", "des", "h\u00f6chs\u00b7ten", "g\u00fct", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der siechen heilt/ vnd leichen heisset leben.", "tokens": ["Der", "sie\u00b7chen", "heilt", "/", "vnd", "lei\u00b7chen", "heis\u00b7set", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$(", "KON", "VVINF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "2. Wo find\u2019 ich wort? ach GOtt! wo fang\u2019 ich an/", "tokens": ["Wo", "find'", "ich", "wort", "?", "ach", "Gott", "!", "wo", "fang'", "ich", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$.", "XY", "NN", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu zehlen was mir deine trew will g\u00f6nnen;", "tokens": ["Zu", "zeh\u00b7len", "was", "mir", "dei\u00b7ne", "trew", "will", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PWS", "PPER", "PPOSAT", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was kein verstand je hat begreiffen k\u00f6nnen", "tokens": ["Was", "kein", "ver\u00b7stand", "je", "hat", "be\u00b7greif\u00b7fen", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "VVFIN", "ADV", "VAFIN", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sprech\u2019 ich au\u00df/ was niemand zehlen kan/", "tokens": ["Wie", "sprech'", "ich", "au\u00df", "/", "was", "nie\u00b7mand", "zeh\u00b7len", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$(", "PWS", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Den weisen rath/ der grossen Armen st\u00e4rcke.", "tokens": ["Den", "wei\u00b7sen", "rath", "/", "der", "gros\u00b7sen", "Ar\u00b7men", "st\u00e4r\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Recht streichen au\u00dfeder klugen geister scharen", "tokens": ["Recht", "strei\u00b7chen", "au\u00b7\u00dfe\u00b7der", "klu\u00b7gen", "geis\u00b7ter", "scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die vmb dich her voll heisser andacht fahren ", "tokens": ["Die", "vmb", "dich", "her", "voll", "heis\u00b7ser", "an\u00b7dacht", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "APZR", "ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind hier zu schlecht die gunst wirdt st\u00fcndlich new/ ", "tokens": ["Sind", "hier", "zu", "schlecht", "die", "gunst", "wirdt", "st\u00fcnd\u00b7lich", "new", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "ART", "NN", "VAFIN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch die du dir den krei\u00df der Welt verbunden/", "tokens": ["Durch", "die", "du", "dir", "den", "krei\u00df", "der", "Welt", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der auff dein wort sich in den stand gefunden. ", "tokens": ["Der", "auff", "dein", "wort", "sich", "in", "den", "stand", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "APPR", "ART", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "4. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Da ich nicht war/ durch die du mich erkohren ", "tokens": ["Da", "ich", "nicht", "war", "/", "durch", "die", "du", "mich", "er\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "$(", "APPR", "PRELS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh\u2019 al\u00df ich wardt/ die al\u00df ich war verlohren", "tokens": ["Eh'", "al\u00df", "ich", "wardt", "/", "die", "al\u00df", "ich", "war", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "VAFIN", "$(", "ART", "KOUS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Adams schuldt/ mich dir hat wiederbracht;", "tokens": ["Durch", "A\u00b7dams", "schuldt", "/", "mich", "dir", "hat", "wie\u00b7der\u00b7bracht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$(", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die mich so wehrt/ da ich nichts wehrt/ gesch\u00e4tzet/", "tokens": ["Die", "mich", "so", "wehrt", "/", "da", "ich", "nichts", "wehrt", "/", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$(", "KOUS", "PPER", "PIS", "VVFIN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df sie dein Kindt f\u00fcr mich in todt versetzet.", "tokens": ["Da\u00df", "sie", "dein", "Kindt", "f\u00fcr", "mich", "in", "todt", "ver\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPER", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "5. Au\u00df wie viel weh/ au\u00df wie viel herbem leidt/", "tokens": ["Au\u00df", "wie", "viel", "weh", "/", "au\u00df", "wie", "viel", "her\u00b7bem", "leidt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "$(", "PTKVZ", "KOKOM", "PIAT", "ADJA", "NN", "$("], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hastu bi\u00dfher mich wunderlich gef\u00fchret?", "tokens": ["Has\u00b7tu", "bi\u00df\u00b7her", "mich", "wun\u00b7der\u00b7lich", "ge\u00b7f\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie offt hat mich der blasse todt ber\u00fchret?", "tokens": ["Wie", "offt", "hat", "mich", "der", "blas\u00b7se", "todt", "be\u00b7r\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie offt fiel ich in grundt der trawrigkeit?", "tokens": ["Wie", "offt", "fiel", "ich", "in", "grundt", "der", "traw\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie offt hat mich der rawe schmertz gefangen? ", "tokens": ["Wie", "offt", "hat", "mich", "der", "ra\u00b7we", "schmertz", "ge\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wie offt bin ich im elendt schier vergangen", "tokens": ["Wie", "offt", "bin", "ich", "im", "e\u00b7lendt", "schier", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPRART", "ADJD", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "6. Ich bin dur", "tokens": ["Ich", "bin", "dur"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Durch schweren raub/ durch schnelle pestilentzen/ ", "tokens": ["Durch", "schwe\u00b7ren", "raub", "/", "durch", "schnel\u00b7le", "pes\u00b7ti\u00b7lent\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch/ was noch jtzt so brennt in vnser grentzen/ ", "tokens": ["Durch", "/", "was", "noch", "jtzt", "so", "brennt", "in", "vn\u00b7ser", "grent\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "PWS", "ADV", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich bin durch sturm/ der S", "tokens": ["Ich", "bin", "durch", "sturm", "/", "der", "S"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$(", "ART", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch hohen neidt/ durch grim\u0303e ", "tokens": ["Durch", "ho\u00b7hen", "neidt", "/", "durch", "grim\u0303e"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch list ger\u00fcckt. Doch HErr/ mir ists gelungen!", "tokens": ["Durch", "list", "ge\u00b7r\u00fcckt", ".", "Doch", "Herr", "/", "mir", "ists", "ge\u00b7lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$.", "KON", "NN", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "7. Weil du mir stets gebotten deine Handt/", "tokens": ["Weil", "du", "mir", "stets", "ge\u00b7bot\u00b7ten", "dei\u00b7ne", "Handt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hab ich die offt mit schrecken sehn vergehen/", "tokens": ["Hab", "ich", "die", "offt", "mit", "schre\u00b7cken", "sehn", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADV", "APPR", "VVINF", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die mir vnd dir HERR wolten wiederstehen/", "tokens": ["Die", "mir", "vnd", "dir", "HeRR", "wol\u00b7ten", "wie\u00b7ders\u00b7te\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPER", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hast mein ach/ vnd jhren trotz gewandt:", "tokens": ["Du", "hast", "mein", "ach", "/", "vnd", "jhren", "trotz", "ge\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "XY", "$(", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Drumb will ich/ weil ich werd\u2019 ein ader r\u00fchren", "tokens": ["Drumb", "will", "ich", "/", "weil", "ich", "werd'", "ein", "a\u00b7der", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "$(", "KOUS", "PPER", "VAFIN", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein wehrtes lob in meinem munde f\u00fchren.", "tokens": ["Dein", "wehr\u00b7tes", "lob", "in", "mei\u00b7nem", "mun\u00b7de", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "8. Du hast mir mehr al\u00df jemals ich begehrt", "tokens": ["Du", "hast", "mir", "mehr", "al\u00df", "je\u00b7mals", "ich", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "KOKOM", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O guter ", "tokens": ["O", "gu\u00b7ter"], "token_info": ["word", "word"], "pos": ["NE", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Offt eh ich noch die H\u00e4nde wolt erheben;", "tokens": ["Offt", "eh", "ich", "noch", "die", "H\u00e4n\u00b7de", "wolt", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War diesem ", "tokens": ["War", "die\u00b7sem"], "token_info": ["word", "word"], "pos": ["VAFIN", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Drumb will ich/ weil ich werd ein Ader r\u00fchren:", "tokens": ["Drumb", "will", "ich", "/", "weil", "ich", "werd", "ein", "A\u00b7der", "r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$(", "KOUS", "PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein wehrtes lob in meinem munde f\u00fchren.", "tokens": ["Dein", "wehr\u00b7tes", "lob", "in", "mei\u00b7nem", "mun\u00b7de", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "9. HERR fahre fort zeuch doch die milde handt", "tokens": ["