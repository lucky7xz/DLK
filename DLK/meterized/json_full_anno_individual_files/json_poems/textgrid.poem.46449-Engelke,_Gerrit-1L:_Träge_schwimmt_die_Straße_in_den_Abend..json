, "pos": ["KOUI", "PRF", "APZR", "VVFIN", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Um dich st\u00fcrzt alles Schicksal hin!", "tokens": ["Um", "dich", "st\u00fcrzt", "al\u00b7les", "Schick\u00b7sal", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Klingelnd radelt der Bote stadthin,", "tokens": ["Klin\u00b7gelnd", "ra\u00b7delt", "der", "Bo\u00b7te", "stadt\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "In dem Stra\u00dfendampf \u2013", "tokens": ["In", "dem", "Stra\u00b7\u00dfen\u00b7dampf", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}