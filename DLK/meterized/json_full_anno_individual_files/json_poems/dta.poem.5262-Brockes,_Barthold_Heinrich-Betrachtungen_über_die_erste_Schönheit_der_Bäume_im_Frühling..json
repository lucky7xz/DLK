{"dta.poem.5262": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die erste Sch\u00f6nheit  \n der B\u00e4ume im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald die Knospen erst sich von einander geben,", "tokens": ["So", "bald", "die", "Knos\u00b7pen", "erst", "sich", "von", "ein\u00b7an\u00b7der", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "PRF", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wor\u00fcber sich ein Aug\u2019 in Hofnung schon erfreut,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "sich", "ein", "Aug'", "in", "Hof\u00b7nung", "schon", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So scheint der gantze Baum, mit zartem Gr\u00fcn bestreut,", "tokens": ["So", "scheint", "der", "gant\u00b7ze", "Baum", ",", "mit", "zar\u00b7tem", "Gr\u00fcn", "be\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wenn um jeden Ast viel gr\u00fcne Puncte schweben.", "tokens": ["Als", "wenn", "um", "je\u00b7den", "Ast", "viel", "gr\u00fc\u00b7ne", "Punc\u00b7te", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PIAT", "NN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Viel tausend kleine gr\u00fcne T\u00fcpfel", "tokens": ["Viel", "tau\u00b7send", "klei\u00b7ne", "gr\u00fc\u00b7ne", "T\u00fcp\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bedecken \u00fcberall den Wipfel.", "tokens": ["Be\u00b7de\u00b7cken", "\u00fc\u00b7be\u00b7rall", "den", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn nun der Sonnen heller Strahl", "tokens": ["Wenn", "nun", "der", "Son\u00b7nen", "hel\u00b7ler", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr z\u00e4rtliches Gespinste manches mahl", "tokens": ["Ihr", "z\u00e4rt\u00b7li\u00b7ches", "Ge\u00b7spins\u00b7te", "man\u00b7ches", "mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PIS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Bestrahlet und durchdringt; sollt einer \u00f6fters meinen,", "tokens": ["Be\u00b7strah\u00b7let", "und", "durch\u00b7dringt", ";", "sollt", "ei\u00b7ner", "\u00f6f\u00b7ters", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "VMFIN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df \u00fcberall im Baum gelb gr\u00fcne Funcken scheinen.", "tokens": ["Da\u00df", "\u00fc\u00b7be\u00b7rall", "im", "Baum", "gelb", "gr\u00fc\u00b7ne", "Fun\u00b7cken", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn sich die Bl\u00e4tter nun vergr\u00f6ssern und verbreiten,", "tokens": ["Wenn", "sich", "die", "Bl\u00e4t\u00b7ter", "nun", "ver\u00b7gr\u00f6s\u00b7sern", "und", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vermehren und vergr\u00f6ssern sich", "tokens": ["Ver\u00b7meh\u00b7ren", "und", "ver\u00b7gr\u00f6s\u00b7sern", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Derselben gr\u00fcne Lieblichkeiten!", "tokens": ["Der\u00b7sel\u00b7ben", "gr\u00fc\u00b7ne", "Lieb\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Indem ein gr\u00fcner Dunst, den man mit Anmuht sieht,", "tokens": ["In\u00b7dem", "ein", "gr\u00fc\u00b7ner", "Dunst", ",", "den", "man", "mit", "An\u00b7muht", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sich um den gantzen Baum so dann gem\u00e4hlig zieht.", "tokens": ["Sich", "um", "den", "gant\u00b7zen", "Baum", "so", "dann", "ge\u00b7m\u00e4h\u00b7lig", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch den nicht g\u00e4ntzlichen Zusammenhang der Bl\u00e4tter", "tokens": ["Durch", "den", "nicht", "g\u00e4ntz\u00b7li\u00b7chen", "Zu\u00b7sam\u00b7men\u00b7hang", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PTKNEG", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Kommt jeder Vorwurf uns so dann nicht anders vor", "tokens": ["Kommt", "je\u00b7der", "Vor\u00b7wurf", "uns", "so", "dann", "nicht", "an\u00b7ders", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als s\u00e4h man ihn durch einen gr\u00fcnen Flor;", "tokens": ["Als", "s\u00e4h", "man", "ihn", "durch", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Flor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIS", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Das uns denn, sonderlich bey heiterm Wetter,", "tokens": ["Das", "uns", "denn", ",", "son\u00b7der\u00b7lich", "bey", "hei\u00b7term", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Von unten auf den himmlischen Sapphier,", "tokens": ["Von", "un\u00b7ten", "auf", "den", "himm\u00b7li\u00b7schen", "Sap\u00b7phier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Von oben ab der Erden gr\u00fcne Zier", "tokens": ["Von", "o\u00b7ben", "ab", "der", "Er\u00b7den", "gr\u00fc\u00b7ne", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Annoch versch\u00f6nert weiset;", "tokens": ["An\u00b7noch", "ver\u00b7sch\u00f6\u00b7nert", "wei\u00b7set", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "So da\u00df wer es, mit achtsamen Gem\u00fcth", "tokens": ["So", "da\u00df", "wer", "es", ",", "mit", "acht\u00b7sa\u00b7men", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "PWS", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "Und einiger Erwegung, sieht,", "tokens": ["Und", "ei\u00b7ni\u00b7ger", "Er\u00b7we\u00b7gung", ",", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "In seiner Augen Lust, mit Recht, den Sch\u00f6pfer preiset,", "tokens": ["In", "sei\u00b7ner", "Au\u00b7gen", "Lust", ",", "mit", "Recht", ",", "den", "Sch\u00f6p\u00b7fer", "prei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der uns zu unsrer Lust, die Welt so lieblich schm\u00fcckt,", "tokens": ["Der", "uns", "zu", "uns\u00b7rer", "Lust", ",", "die", "Welt", "so", "lieb\u00b7lich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}