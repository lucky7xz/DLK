, "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.23": {"line.1": {"text": "Nicht mehr das Schwert handhaben und nicht den Pflug", "tokens": ["Nicht", "mehr", "das", "Schwert", "hand\u00b7ha\u00b7ben", "und", "nicht", "den", "Pflug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVINF", "KON", "PTKNEG", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Quiriten jetzt, kaum pflegt die entw\u00f6hnte Hand", "tokens": ["Qui\u00b7ri\u00b7ten", "jetzt", ",", "kaum", "pflegt", "die", "ent\u00b7w\u00f6hn\u00b7te", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den s\u00fc\u00dfen Weinstock, wurzelschlagend", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Wein\u00b7stock", ",", "wur\u00b7zel\u00b7schla\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber dem Schutte der alten Tugend.", "tokens": ["\u00dc\u00b7ber", "dem", "Schut\u00b7te", "der", "al\u00b7ten", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.24": {"line.1": {"text": "Im Flammenblick nur, oder im edlen Bau", "tokens": ["Im", "Flam\u00b7men\u00b7blick", "nur", ",", "o\u00b7der", "im", "ed\u00b7len", "Bau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+