{"textgrid.poem.60768": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "6. Acqua Paolina", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Quell, wie viel auch immer das sch\u00f6ne Rom", "tokens": ["Kein", "Quell", ",", "wie", "viel", "auch", "im\u00b7mer", "das", "sch\u00f6\u00b7ne", "Rom"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PWAV", "ADV", "ADV", "ADV", "ART", "ADJA", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Flutspendend ausgie\u00dft, ob ein Triton es spr\u00fctzt,", "tokens": ["Fluts\u00b7pen\u00b7dend", "aus\u00b7gie\u00dft", ",", "ob", "ein", "Tri\u00b7ton", "es", "spr\u00fctzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--++--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ob sanft es perlt aus Marmorbecken,", "tokens": ["Ob", "sanft", "es", "perlt", "aus", "Mar\u00b7mor\u00b7be\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder gigantischen, alten Schalen:", "tokens": ["O\u00b7der", "gi\u00b7gan\u00b7ti\u00b7schen", ",", "al\u00b7ten", "Scha\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Kein Quell, so weit einst herrschte der Sohn des Mars,", "tokens": ["Kein", "Quell", ",", "so", "weit", "einst", "herrschte", "der", "Sohn", "des", "Mars", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "ADJD", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sei dir vergleichbar, auf dem Janiculum", "tokens": ["Sei", "dir", "ver\u00b7gleich\u00b7bar", ",", "auf", "dem", "Ja\u00b7ni\u00b7cu\u00b7lum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "APPR", "ART", "NE"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Mit deinen f\u00fcnf stromreichen Armen", "tokens": ["Mit", "dei\u00b7nen", "f\u00fcnf", "strom\u00b7rei\u00b7chen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwischen granitene S\u00e4ulen pl\u00e4tschernd.", "tokens": ["Zwi\u00b7schen", "gra\u00b7ni\u00b7te\u00b7ne", "S\u00e4u\u00b7len", "pl\u00e4t\u00b7schernd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "---+--+---", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dort winkt mir Einsamkeit, die geliebte Braut,", "tokens": ["Dort", "winkt", "mir", "Ein\u00b7sam\u00b7keit", ",", "die", "ge\u00b7lieb\u00b7te", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von dort beschaut, vielf\u00e4ltig erg\u00f6tzt, der Blick", "tokens": ["Von", "dort", "be\u00b7schaut", ",", "viel\u00b7f\u00e4l\u00b7tig", "er\u00b7g\u00f6tzt", ",", "der", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "ADJD", "$,", "ADJD", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Rom des Knechts der Knechte Gottes", "tokens": ["Das", "Rom", "des", "Knechts", "der", "Knech\u00b7te", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Neben dem Rom der Triumphatoren.", "tokens": ["Ne\u00b7ben", "dem", "Rom", "der", "Tri\u00b7um\u00b7pha\u00b7to\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "K\u00fchn ragt, ein halbentbl\u00e4tterter Mauerkranz,", "tokens": ["K\u00fchn", "ragt", ",", "ein", "hal\u00b7bent\u00b7bl\u00e4t\u00b7ter\u00b7ter", "Mau\u00b7er\u00b7kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Kolosseum; aber auch dir, wie steigt", "tokens": ["Das", "Ko\u00b7los\u00b7se\u00b7um", ";", "a\u00b7ber", "auch", "dir", ",", "wie", "steigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "ADV", "PPER", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Trotz der Ewigkeit in jedem", "tokens": ["Der", "Trotz", "der", "E\u00b7wig\u00b7keit", "in", "je\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Pfeiler empor, o Palast Farnese!", "tokens": ["Pfei\u00b7ler", "em\u00b7por", ",", "o", "Pa\u00b7last", "Far\u00b7ne\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "FM", "FM", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Wo sonst des finsterlockigen Donnergotts", "tokens": ["Wo", "sonst", "des", "fins\u00b7ter\u00b7lo\u00b7cki\u00b7gen", "Don\u00b7ner\u00b7gotts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Siegreicher Aar ausbreitete scharfe Klaun,", "tokens": ["Sieg\u00b7rei\u00b7cher", "Aar", "aus\u00b7brei\u00b7te\u00b7te", "schar\u00b7fe", "Klaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da hob sich manch Jahrhundert \u00fcber", "tokens": ["Da", "hob", "sich", "manch", "Jahr\u00b7hun\u00b7dert", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Giebel und Zinne das Kreuz und herrschte.", "tokens": ["Gie\u00b7bel", "und", "Zin\u00b7ne", "das", "Kreuz", "und", "herrschte", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Bis j\u00fcngst, der Schicksalslaune gewaltig Spiel,", "tokens": ["Bis", "j\u00fcngst", ",", "der", "Schick\u00b7sals\u00b7lau\u00b7ne", "ge\u00b7wal\u00b7tig", "Spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein zweiter C\u00e4sar lenkte den Gang der Welt,", "tokens": ["Ein", "zwei\u00b7ter", "C\u00e4\u00b7sar", "lenk\u00b7te", "den", "Gang", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der pflanzte sein dreifarbig Banner", "tokens": ["Der", "pflanz\u00b7te", "sein", "drei\u00b7far\u00b7big", "Ban\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Neben den sch\u00f6nen Kolo\u00df des Phidias;", "tokens": ["Ne\u00b7ben", "den", "sch\u00f6\u00b7nen", "Ko\u00b7lo\u00df", "des", "Phi\u00b7di\u00b7as", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ein Sohn der Freiheit; aber uneingedenk", "tokens": ["Ein", "Sohn", "der", "Frei\u00b7heit", ";", "a\u00b7ber", "un\u00b7ein\u00b7ge\u00b7denk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Des edlen Ursprungs, einem Geschlechte sich", "tokens": ["Des", "ed\u00b7len", "Ur\u00b7sprungs", ",", "ei\u00b7nem", "Ge\u00b7schlech\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aufopfernd, das ihn wankelm\u00fctig", "tokens": ["Auf\u00b7op\u00b7fernd", ",", "das", "ihn", "wan\u00b7kel\u00b7m\u00fc\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heute verg\u00f6tterte, morgen preisgab.", "tokens": ["Heu\u00b7te", "ver\u00b7g\u00f6t\u00b7ter\u00b7te", ",", "mor\u00b7gen", "preis\u00b7gab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "O h\u00e4tte dein weitschallendes Kaiserwort", "tokens": ["O", "h\u00e4t\u00b7te", "dein", "weit\u00b7schal\u00b7len\u00b7des", "Kai\u00b7ser\u00b7wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Volk Europas, was es erfleht, geschenkt,", "tokens": ["Dem", "Volk", "Eu\u00b7ro\u00b7pas", ",", "was", "es", "er\u00b7fleht", ",", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohl w\u00e4rst du seines Lieds Harmodius,", "tokens": ["Wohl", "w\u00e4rst", "du", "sei\u00b7nes", "Lieds", "Har\u00b7mo\u00b7dius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seines Gesanges Aristogiton!", "tokens": ["Sei\u00b7nes", "Ge\u00b7san\u00b7ges", "A\u00b7ris\u00b7to\u00b7gi\u00b7ton", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Nun ist verp\u00f6nt dein Name, Musik erh\u00f6ht", "tokens": ["Nun", "ist", "ver\u00b7p\u00f6nt", "dein", "Na\u00b7me", ",", "Mu\u00b7sik", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "VVFIN", "PPOSAT", "NN", "$,", "NN", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihn nicht auf Wohllautsfittigen; nur sobald", "tokens": ["Ihn", "nicht", "auf", "Wohl\u00b7lauts\u00b7fit\u00b7ti\u00b7gen", ";", "nur", "so\u00b7bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "$.", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein Grab ein Schiff umsegelt, singen", "tokens": ["Dein", "Grab", "ein", "Schiff", "um\u00b7se\u00b7gelt", ",", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00fcde Matrosen von dir ein Chorlied.", "tokens": ["M\u00fc\u00b7de", "Mat\u00b7ro\u00b7sen", "von", "dir", "ein", "Chor\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.10": {"line.1": {"text": "Und Rom? Es fiel nochmaliger Nacht anheim,", "tokens": ["Und", "Rom", "?", "Es", "fiel", "noch\u00b7ma\u00b7li\u00b7ger", "Nacht", "an\u00b7heim", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "PPER", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch schweigt's, und lautlos neben der herrschenden,", "tokens": ["Doch", "schweigt's", ",", "und", "laut\u00b7los", "ne\u00b7ben", "der", "herr\u00b7schen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sechsrossig aufgez\u00e4umten Hoffart", "tokens": ["Sechs\u00b7ros\u00b7sig", "auf\u00b7ge\u00b7z\u00e4um\u00b7ten", "Hof\u00b7fart"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Schleicht der Beherrschten uns\u00e4glich Elend.", "tokens": ["Schleicht", "der", "Be\u00b7herrschten", "un\u00b7s\u00e4g\u00b7lich", "E\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Nicht mehr das Schwert handhaben und nicht den Pflug", "tokens": ["Nicht", "mehr", "das", "Schwert", "hand\u00b7ha\u00b7ben", "und", "nicht", "den", "Pflug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVINF", "KON", "PTKNEG", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Quiriten jetzt, kaum pflegt die entw\u00f6hnte Hand", "tokens": ["Qui\u00b7ri\u00b7ten", "jetzt", ",", "kaum", "pflegt", "die", "ent\u00b7w\u00f6hn\u00b7te", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den s\u00fc\u00dfen Weinstock, wurzelschlagend", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Wein\u00b7stock", ",", "wur\u00b7zel\u00b7schla\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber dem Schutte der alten Tugend.", "tokens": ["\u00dc\u00b7ber", "dem", "Schut\u00b7te", "der", "al\u00b7ten", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Im Flammenblick nur, oder im edlen Bau", "tokens": ["Im", "Flam\u00b7men\u00b7blick", "nur", ",", "o\u00b7der", "im", "ed\u00b7len", "Bau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des sch\u00f6nen, freiheitl\u00fcgenden Angesichts", "tokens": ["Des", "sch\u00f6\u00b7nen", ",", "frei\u00b7heit\u00b7l\u00fc\u00b7gen\u00b7den", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zeigt Rom sich noch, am Scheideweg noch,", "tokens": ["Zeigt", "Rom", "sich", "noch", ",", "am", "Schei\u00b7de\u00b7weg", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PRF", "ADV", "$,", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber es folgte dem Wink der Wollust!", "tokens": ["A\u00b7ber", "es", "folg\u00b7te", "dem", "Wink", "der", "Wol\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Kein Quell, wie viel auch immer das sch\u00f6ne Rom", "tokens": ["Kein", "Quell", ",", "wie", "viel", "auch", "im\u00b7mer", "das", "sch\u00f6\u00b7ne", "Rom"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["