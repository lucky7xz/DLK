{"textgrid.poem.45968": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[in dem Lichte wohnt das Heil]", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Lichte wohnt das Heil,", "tokens": ["In", "dem", "Lich\u00b7te", "wohnt", "das", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch der Pfad ist uns verloren", "tokens": ["Doch", "der", "Pfad", "ist", "uns", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder unerklimmbar steil,", "tokens": ["O\u00b7der", "un\u00b7er\u00b7klimm\u00b7bar", "steil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn wir au\u00dfer uns ihn steigen", "tokens": ["Wenn", "wir", "au\u00b7\u00dfer", "uns", "ihn", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden wir am Abgrund schwindeln", "tokens": ["Wer\u00b7den", "wir", "am", "Ab\u00b7grund", "schwin\u00b7deln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Aber in uns selbst, da zeigen", "tokens": ["A\u00b7ber", "in", "uns", "selbst", ",", "da", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPER",