{"textgrid.poem.49303": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "An eine sehr sch\u00f6ne Blume im Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df der Himmel dich sch\u00f6n geschm\u00fccket,", "tokens": ["Da\u00df", "der", "Him\u00b7mel", "dich", "sch\u00f6n", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df die Sonne dein Kleid gesticket,", "tokens": ["Da\u00df", "die", "Son\u00b7ne", "dein", "Kleid", "ge\u00b7sti\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da\u00df du prangest f\u00fcr Gold und Seiden,", "tokens": ["Da\u00df", "du", "pran\u00b7gest", "f\u00fcr", "Gold", "und", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mu\u00df mein' Adelwitz itz zwar leiden.", "tokens": ["Mu\u00df", "mein'", "A\u00b7del\u00b7witz", "itz", "zwar", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Da\u00df die Bienen dich oftmals k\u00fcssen,", "tokens": ["Da\u00df", "die", "Bie\u00b7nen", "dich", "oft\u00b7mals", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Da\u00df die Kranken dich preisen m\u00fcssen", "tokens": ["Da\u00df", "die", "Kran\u00b7ken", "dich", "prei\u00b7sen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "VMFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und ihr' Aerzte dich heilsam nennen,", "tokens": ["Und", "ih\u00b7r'", "A\u00b7erz\u00b7te", "dich", "heil\u00b7sam", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Mu\u00df mein' Adelwitz zwar bekennen.", "tokens": ["Mu\u00df", "mein'", "A\u00b7del\u00b7witz", "zwar", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Doch in allen denselben Sachen", "tokens": ["Doch", "in", "al\u00b7len", "den\u00b7sel\u00b7ben", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "PDAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Kan ihr' Herlichkeit dich verlachen,", "tokens": ["Kan", "ih\u00b7r'", "Her\u00b7lich\u00b7keit", "dich", "ver\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Denn man findet nicht ihres gleichen,", "tokens": ["Denn", "man", "fin\u00b7det", "nicht", "ih\u00b7res", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Was geschaffen ist, mu\u00df ihr weichen.", "tokens": ["Was", "ge\u00b7schaf\u00b7fen", "ist", ",", "mu\u00df", "ihr", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Deine Kleider vergehen schleunig,", "tokens": ["Dei\u00b7ne", "Klei\u00b7der", "ver\u00b7ge\u00b7hen", "schleu\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Deine Farben die n\u00fctzen weinig,", "tokens": ["Dei\u00b7ne", "Far\u00b7ben", "die", "n\u00fct\u00b7zen", "wei\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Deine Kr\u00e4fte sind zum Verderben,", "tokens": ["Dei\u00b7ne", "Kr\u00e4f\u00b7te", "sind", "zum", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Vielmals helfen sie gar zum Sterben.", "tokens": ["Viel\u00b7mals", "hel\u00b7fen", "sie", "gar", "zum", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Was hilft Lieblichkeit, wo kein Sprechen?", "tokens": ["Was", "hilft", "Lieb\u00b7lich\u00b7keit", ",", "wo", "kein", "Spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$,", "PWAV", "PIAT", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Was sind Blumen, die leicht zerbrechen?", "tokens": ["Was", "sind", "Blu\u00b7men", ",", "die", "leicht", "zer\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Was ist Kleiderpracht sonder Singen?", "tokens": ["Was", "ist", "Klei\u00b7der\u00b7pracht", "son\u00b7der", "Sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Die\u00df kan keinem das Herz bezwingen.", "tokens": ["Die\u00df", "kan", "kei\u00b7nem", "das", "Herz", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Nein, mein' Adelwitz kan bewegen,", "tokens": ["Nein", ",", "mein'", "A\u00b7del\u00b7witz", "kan", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Da\u00df sich grimmige L\u00f6wen legen,", "tokens": ["Da\u00df", "sich", "grim\u00b7mi\u00b7ge", "L\u00f6\u00b7wen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Anzuh\u00f6ren die s\u00fc\u00dfe Weisen,", "tokens": ["An\u00b7zu\u00b7h\u00f6\u00b7ren", "die", "s\u00fc\u00b7\u00dfe", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Da sie Daphnis mit pflegt zu preisen.", "tokens": ["Da", "sie", "Daph\u00b7nis", "mit", "pflegt", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "APPR", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "Was am Himmel ist sch\u00f6n zu finden,", "tokens": ["Was", "am", "Him\u00b7mel", "ist", "sch\u00f6n", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Was die Blumen kan \u00fcberwinden,", "tokens": ["Was", "die", "Blu\u00b7men", "kan", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Was der Nachtigal Kunst nicht weichet,", "tokens": ["Was", "der", "Nach\u00b7ti\u00b7gal", "Kunst", "nicht", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Was der Perlen Gestalt sich gleichet,", "tokens": ["Was", "der", "Per\u00b7len", "Ge\u00b7stalt", "sich", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "Was mit Freudigkeit ist begabet,", "tokens": ["Was", 