{"textgrid.poem.66379": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "J\u00f6rg", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wohl ist mir in meinem Sinn!", "tokens": ["Wie", "wohl", "ist", "mir", "in", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein M\u00e4del gibt's wie mein's;", "tokens": ["Kein", "M\u00e4\u00b7del", "gibt's", "wie", "mein's", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KOKOM", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Guck hin, guck her, guck her, guck hin,", "tokens": ["Guck", "hin", ",", "guck", "her", ",", "guck", "her", ",", "guck", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So findst in Schwaben keins.", "tokens": ["So", "findst", "in", "Schwa\u00b7ben", "keins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So jung und h\u00fcbsch, und doch so gut", "tokens": ["So", "jung", "und", "h\u00fcbsch", ",", "und", "doch", "so", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "KON", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Engel Gottes sind,", "tokens": ["Wie", "En\u00b7gel", "Got\u00b7tes", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mir so treu, bei meinem Blut", "tokens": ["Und", "mir", "so", "treu", ",", "bei", "mei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6cht' weinen, wie ein Kind.", "tokens": ["M\u00f6cht'", "wei\u00b7nen", ",", "wie", "ein", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hat Haar, kein Flachs ist traun so fein.", "tokens": ["Hat", "Haar", ",", "kein", "Flachs", "ist", "traun", "so", "fein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PIAT", "NN", "VAFIN", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie s\u00fc\u00df ihr M\u00fcndchen lacht!", "tokens": ["Wie", "s\u00fc\u00df", "ihr", "M\u00fcnd\u00b7chen", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es blinken ihre Aeugelein", "tokens": ["Es", "blin\u00b7ken", "ih\u00b7re", "A\u00b7e\u00b7u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Sternlein in der Nacht.", "tokens": ["Wie", "Stern\u00b7lein", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie schafft dir fr\u00fch, und schafft dir sp\u00e4t;", "tokens": ["Sie", "schafft", "dir", "fr\u00fch", ",", "und", "schafft", "dir", "sp\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das gibt einmal ein Weib.", "tokens": ["Das", "gibt", "ein\u00b7mal", "ein", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sie die runde Spindel dreht,", "tokens": ["Wenn", "sie", "die", "run\u00b7de", "Spin\u00b7del", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00fcpft mir's Herz im Leib.", "tokens": ["So", "h\u00fcpft", "mir's", "Herz", "im", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und ist dir doch so gut dabei,", "tokens": ["Und", "ist", "dir", "doch", "so", "gut", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fromm und tugendsam \u2013", "tokens": ["So", "fromm", "und", "tu\u00b7gend\u00b7sam", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch so heimlich, meiner Treu!", "tokens": ["Und", "doch", "so", "heim\u00b7lich", ",", "mei\u00b7ner", "Treu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So heimlich wie ein Lamm.", "tokens": ["So", "heim\u00b7lich", "wie", "ein", "Lamm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie wei\u00df dir nichts von Bauernstolz;", "tokens": ["Sie", "wei\u00df", "dir", "nichts", "von", "Bau\u00b7ern\u00b7stolz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rt sie Dudeldum,", "tokens": ["Und", "h\u00f6rt", "sie", "Du\u00b7del\u00b7dum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So dreht sie sich als wie ein Bolz", "tokens": ["So", "dreht", "sie", "sich", "als", "wie", "ein", "Bolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir im Ring herum.", "tokens": ["Mit", "mir", "im", "Ring", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wollt' gestern auf die Kirchweih gehn;", "tokens": ["Wollt'", "ge\u00b7stern", "auf", "die", "Kirch\u00b7weih", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Da blieb das M\u00e4del fein", "tokens": ["Da", "blieb", "das", "M\u00e4\u00b7del", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit mir vor einer H\u00fctte stehn", "tokens": ["Mit", "mir", "vor", "ei\u00b7ner", "H\u00fct\u00b7te", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach: J\u00f6rg, komm herein.", "tokens": ["Und", "sprach", ":", "J\u00f6rg", ",", "komm", "her\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da lag ein Armer auf der Streu", "tokens": ["Da", "lag", "ein", "Ar\u00b7mer", "auf", "der", "Streu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kaute schimmlich Brod,", "tokens": ["Und", "kau\u00b7te", "schimm\u00b7lich", "Brod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Kr\u00fcglein Wasser stand dabei;", "tokens": ["Ein", "Kr\u00fcg\u00b7lein", "Was\u00b7ser", "stand", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie f\u00fchlte seine Noth,", "tokens": ["Sie", "f\u00fchl\u00b7te", "sei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und sprach: O J\u00f6rg, gib ihm dein Geld,", "tokens": ["Und", "sprach", ":", "O", "J\u00f6rg", ",", "gib", "ihm", "dein", "Geld", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "NE", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hilf dem Armen nun;", "tokens": ["Und", "hilf", "dem", "Ar\u00b7men", "nun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nichts Liebers ist mir auf der Welt,", "tokens": ["Nichts", "Lie\u00b7bers", "ist", "mir", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Armen Gutes thun.", "tokens": ["Als", "Ar\u00b7men", "Gu\u00b7tes", "thun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da griff ich nach dem Beutel schnell,", "tokens": ["Da", "griff", "ich", "nach", "dem", "Beu\u00b7tel", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gab's dem Armen hin.", "tokens": ["Und", "gab's", "dem", "Ar\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des M\u00e4dels Augen wurden hell,", "tokens": ["Des", "M\u00e4\u00b7dels", "Au\u00b7gen", "wur\u00b7den", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir ward wohl im Sinn.", "tokens": ["Und", "mir", "ward", "wohl", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "An Tanz und Kirchweih dacht' ich nicht.", "tokens": ["An", "Tanz", "und", "Kirch\u00b7weih", "dacht'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der arme Kranke sprach", "tokens": ["Der", "ar\u00b7me", "Kran\u00b7ke", "sprach"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit hellen Thr\u00e4nen im Gesicht", "tokens": ["Mit", "hel\u00b7len", "Thr\u00e4\u00b7nen", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns Gottes Segen nach.", "tokens": ["Uns", "Got\u00b7tes", "Se\u00b7gen", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und als ich auf die Wiese kam,", "tokens": ["Und", "als", "ich", "auf", "die", "Wie\u00b7se", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War mir's im Herzen warm,", "tokens": ["War", "mir's", "im", "Her\u00b7zen", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, ach, mein liebes M\u00e4del nahm", "tokens": ["Und", ",", "ach", ",", "mein", "lie\u00b7bes", "M\u00e4\u00b7del", "nahm"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$,", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich weinend in den Arm.", "tokens": ["Mich", "wei\u00b7nend", "in", "den", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Seitdem denk' ich in meinem Sinn:", "tokens": ["Seit\u00b7dem", "denk'", "ich", "in", "mei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein M\u00e4del gibt's wie mein's.", "tokens": ["Kein", "M\u00e4\u00b7del", "gibt's", "wie", "mein'", "s."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PIAT", "NN", "VVFIN", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Guck hin, guck her, guck her, guck hin,", "tokens": ["Guck", "hin", ",", "guck", "her", ",", "guck", "her", ",", "guck", "hin", ","], "token_info": ["word", "word", "punct", "word"