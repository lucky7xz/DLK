{"textgrid.poem.57047": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Muhme Kunkel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Palma Kunkel ist mit Palm verwandt,", "tokens": ["Pal\u00b7ma", "Kun\u00b7kel", "ist", "mit", "Palm", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "doch im \u00fcbrigen sonst nicht bekannt.", "tokens": ["doch", "im", "\u00fcb\u00b7ri\u00b7gen", "sonst", "nicht", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und sie w\u00fcnscht auch nicht bekannt zu sein,", "tokens": ["Und", "sie", "w\u00fcnscht", "auch", "nicht", "be\u00b7kannt", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "lebt am liebsten ganz f\u00fcr sich allein.", "tokens": ["lebt", "am", "liebs\u00b7ten", "ganz", "f\u00fcr", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKA", "ADJD", "ADV", "APPR", "PRF", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00dcber Muhme Palma Kunkel drum", "tokens": ["\u00dc\u00b7ber", "Muh\u00b7me", "Pal\u00b7ma", "Kun\u00b7kel", "drum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "NN", "PAV"], "meter": "+-+-+-+-+", "m