{"textgrid.poem.57277": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie erscholl der Gang des lauten Heers", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie erscholl der Gang des lauten Heers", "tokens": ["Wie", "er\u00b7scholl", "der", "Gang", "des", "lau\u00b7ten", "Heers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem Gebirg in das Thal herab,", "tokens": ["Von", "dem", "Ge\u00b7birg", "in", "das", "Thal", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Da zu dem Angriff bey dem Waldstrom das Kriegslied", "tokens": ["Da", "zu", "dem", "An\u00b7griff", "bey", "dem", "Wald\u00b7strom", "das", "Kriegs\u00b7lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "---+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu der vertilgenden Schlacht und dem Siege den Befehl rief!", "tokens": ["Zu", "der", "ver\u00b7til\u00b7gen\u00b7den", "Schlacht", "und", "dem", "Sie\u00b7ge", "den", "Be\u00b7fehl", "rief", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit herab zu grosser Thaten Ernst!", "tokens": ["Mit", "her\u00b7ab", "zu", "gros\u00b7ser", "Tha\u00b7ten", "Ernst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zu der unsterblichen Rettung Ruhm!", "tokens": ["Zu", "der", "uns\u00b7terb\u00b7li\u00b7chen", "Ret\u00b7tung", "Ruhm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die am Gebirg uns bey dem Strom stolz erwarten,", "tokens": ["Die", "am", "Ge\u00b7birg", "uns", "bey", "dem", "Strom", "stolz", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und im Gefilde der Schlacht mit dem Donner in dem Arm stehn,", "tokens": ["Und", "im", "Ge\u00b7fil\u00b7de", "der", "Schlacht", "mit", "dem", "Don\u00b7ner", "in", "dem", "Arm", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}}, "stanza.3": {"line.1": {"text": "O Tyrannenknechte sind sie nur!", "tokens": ["O", "Ty\u00b7ran\u00b7nen\u00b7knech\u00b7te", "sind", "sie", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und vor dem Drohn des gesenkten Stahls,", "tokens": ["Und", "vor", "dem", "Drohn", "des", "ge\u00b7senk\u00b7ten", "Stahls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Vor dem Herannahn, und dem Ausspruch der Freyen,", "tokens": ["Vor", "dem", "Her\u00b7an\u00b7nahn", ",", "und", "dem", "Aus\u00b7spruch", "der", "Frey\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die sich dem Tode gelassener heiligen, entfliehn sie!", "tokens": ["Die", "sich", "dem", "To\u00b7de", "ge\u00b7las\u00b7se\u00b7ner", "hei\u00b7li\u00b7gen", ",", "ent\u00b7fliehn", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADJA", "ADJA", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie erscholl der Gang des lauten Heers", "tokens": ["Wie", "er\u00b7scholl", "der", "Gang", "des", "lau\u00b7ten", "Heers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem Gebirg in das Thal herab,", "tokens": ["Von", "dem", "Ge\u00b7birg", "in", "das", "Thal", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Da zu dem Angriff bey dem Waldstrom das Kriegslied", "tokens": ["Da", "zu", "dem", "An\u00b7griff", "bey", "dem", "Wald\u00b7strom", "das", "Kriegs\u00b7lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART",