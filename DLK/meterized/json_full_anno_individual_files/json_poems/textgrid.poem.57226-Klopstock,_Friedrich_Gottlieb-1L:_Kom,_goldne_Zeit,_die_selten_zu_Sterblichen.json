{"textgrid.poem.57226": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kom, goldne Zeit, die selten zu Sterblichen", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kom, goldne Zeit, die selten zu Sterblichen", "tokens": ["Kom", ",", "gold\u00b7ne", "Zeit", ",", "die", "sel\u00b7ten", "zu", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heruntersteiget, lass dich erflehn, und kom", "tokens": ["Her\u00b7un\u00b7ter\u00b7stei\u00b7get", ",", "lass", "dich", "er\u00b7flehn", ",", "und", "kom"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "VVINF", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu uns, wo dir es schon im Haine", "tokens": ["Zu", "uns", ",", "wo", "dir", "es", "schon", "im", "Hai\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PWAV", "PPER", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weht, und herab von dem Quell schon t\u00f6net!", "tokens": ["Weht", ",", "und", "her\u00b7ab", "von", "dem", "Quell", "schon", "t\u00f6\u00b7net", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Gedankenvoller, tief in Entz\u00fcckungen", "tokens": ["Ge\u00b7dan\u00b7ken\