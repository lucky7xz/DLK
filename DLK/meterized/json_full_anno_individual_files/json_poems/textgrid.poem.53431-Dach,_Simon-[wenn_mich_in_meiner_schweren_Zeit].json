{"textgrid.poem.53431": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[wenn mich in meiner schweren Zeit]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mich in meiner schweren Zeit", "tokens": ["Wenn", "mich", "in", "mei\u00b7ner", "schwe\u00b7ren", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betr\u00fcbni\u00df, Angst und Hertzeleid", "tokens": ["Be\u00b7tr\u00fcb\u00b7ni\u00df", ",", "Angst", "und", "Hert\u00b7ze\u00b7leid"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bi\u00df auff die Seele hat getroffen,", "tokens": ["Bi\u00df", "auff", "die", "See\u00b7le", "hat", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nirgends sich zeigt Trost und Heyl,", "tokens": ["Und", "nir\u00b7gends", "sich", "zeigt", "Trost", "und", "Heyl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist der wahre Gott mein Theil,", "tokens": ["So", "ist", "der", "wah\u00b7re", "Gott", "mein", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und darumb wil ich auff ihn hoffen.", "tokens": ["Und", "da\u00b7rumb", "wil", "ich", "auff", "ihn", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn Gott der Herr sehr freundlich ist", "tokens": ["Denn", "Gott", "der", "Herr", "sehr", "freund\u00b7lich", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem, der ihn ihm hat au\u00dferkiest", "tokens": ["Dem", ",", "der", "ihn", "ihm", "hat", "au\u00b7\u00dfer\u00b7kiest"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPER", "PPER", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auff ihn harrt in b\u00f6sen Tagen,", "tokens": ["Und", "auff", "ihn", "harrt", "in", "b\u00f6\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er wohnt mit v\u00e4terlicher Treu", "tokens": ["Er", "wohnt", "mit", "v\u00e4\u00b7ter\u00b7li\u00b7cher", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den hochbetr\u00fcbten Seelen bey,", "tokens": ["Den", "hoch\u00b7be\u00b7tr\u00fcb\u00b7ten", "See\u00b7len", "bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bald sie auch nur nach ihm fragen.", "tokens": ["So", "bald", "sie", "auch", "nur", "nach", "ihm", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist ein k\u00f6stlich Ding in Pein", "tokens": ["Es", "ist", "ein", "k\u00f6st\u00b7lich", "Ding", "in", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bescheiden und geduldig seyn,", "tokens": ["Be\u00b7schei\u00b7den", "und", "ge\u00b7dul\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sonderlich im Zuvertrauen", "tokens": ["Und", "son\u00b7der\u00b7lich", "im", "Zu\u00b7ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Hoffnung, welcher Grund besteht,", "tokens": ["Und", "Hoff\u00b7nung", ",", "wel\u00b7cher", "Grund", "be\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn gleich der Erden Reich vergeht,", "tokens": ["Wenn", "gleich", "der", "Er\u00b7den", "Reich", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allein auff Gott nach H\u00fclffe schauen.", "tokens": ["Al\u00b7lein", "auff", "Gott", "nach", "H\u00fclf\u00b7fe", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos"