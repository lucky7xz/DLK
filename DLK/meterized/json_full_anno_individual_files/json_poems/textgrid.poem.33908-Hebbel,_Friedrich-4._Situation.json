{"textgrid.poem.33908": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Situation", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In die k\u00fchle Felsengrotte", "tokens": ["In", "die", "k\u00fch\u00b7le", "Fel\u00b7sen\u00b7grot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt der junge J\u00e4ger ein.", "tokens": ["Tritt", "der", "jun\u00b7ge", "J\u00e4\u00b7ger", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00df ist's drau\u00dfen, um zu schlummern,", "tokens": ["Hei\u00df", "ist's", "drau\u00b7\u00dfen", ",", "um", "zu", "schlum\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Legt er still sich auf's Gestein.", "tokens": ["Legt", "er", "still", "sich", "auf's", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der Schlaf, der ewig milde,", "tokens": ["Und", "der", "Schlaf", ",", "der", "e\u00b7wig", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dft ihm bald die Augen dicht;", "tokens": ["Schlie\u00dft", "ihm", "bald", "die", "Au\u00b7gen", "dicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfer Tr\u00e4ume lichte Schatten", "tokens": ["S\u00fc\u00b7\u00dfer", "Tr\u00e4u\u00b7me", "lich\u00b7te", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegen \u00fcber sein Gesicht.", "tokens": ["Flie\u00b7gen", "\u00fc\u00b7ber", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In die k\u00fchle Felsengrotte", "tokens": ["In", "die", "k\u00fch\u00b7le", "Fel\u00b7sen\u00b7grot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt ein M\u00e4dchen, hoch und schlank,", "tokens": ["Tritt", "ein", "M\u00e4d\u00b7chen", ",", "hoch", "und", "schlank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht den Schl\u00e4fer, hold erschreckend,", "tokens": ["Sieht", "den", "Schl\u00e4\u00b7fer", ",", "hold", "er\u00b7schre\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht sich hastig seiner Bank.", "tokens": ["Naht", "sich", "has\u00b7tig", "sei\u00b7ner", "Bank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Will ihn wecken, h\u00f6ret Schritte,", "tokens": ["Will", "ihn", "we\u00b7cken", ",", "h\u00f6\u00b7ret", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft mit Angst: es ist zu sp\u00e4t!", "tokens": ["Ruft", "mit", "Angst", ":", "es", "ist", "zu", "sp\u00e4t", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht des Kreuzes schirmend Zeichen", "tokens": ["Macht", "des", "Kreu\u00b7zes", "schir\u00b7mend", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber ihn, wie im Gebet.", "tokens": ["Ue\u00b7ber", "ihn", ",", "wie", "im", "Ge\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "In die Grotte tritt der Wildsch\u00fctz,", "tokens": ["In", "die", "Grot\u00b7te", "tritt", "der", "Wild\u00b7sch\u00fctz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht den jungen J\u00e4gersmann,", "tokens": ["Sieht", "den", "jun\u00b7gen", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Greift erblassend nach der B\u00fcchse,", "tokens": ["Greift", "er\u00b7blas\u00b7send", "nach", "der", "B\u00fcch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spannt den Hahn, legt auf ihn an.", "tokens": ["Spannt", "den", "Hahn", ",", "legt", "auf", "ihn", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Vor den Bruder tritt das M\u00e4dchen,", "tokens": ["Vor", "den", "Bru\u00b7der", "tritt", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch er dr\u00e4ngt sie stumm zur\u00fcck;", "tokens": ["Doch", "er", "dr\u00e4ngt", "sie", "stumm", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der hat einst auf mich geschossen!", "tokens": ["Der", "hat", "einst", "auf", "mich", "ge\u00b7schos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sagt ihr ernst und streng sein Blick.", "tokens": ["Sagt", "ihr", "ernst", "und", "streng", "sein", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbsieh ihn schlafen \u2013 spricht sie leise \u2013", "tokens": ["\u00bb", "sieh", "ihn", "schla\u00b7fen", "\u2013", "spricht", "sie", "lei\u00b7se", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "VVINF", "$(", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er ist jetzt in Gottes Schutz,", "tokens": ["Er", "ist", "jetzt", "in", "Got\u00b7tes", "Schutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihm zur Seite steht ein Engel,", "tokens": ["Ihm", "zur", "Sei\u00b7te", "steht", "ein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlst du's nicht in deinem Trutz?\u00ab", "tokens": ["F\u00fchlst", "du's", "nicht", "in", "dei\u00b7nem", "Trutz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Als er auflacht, fleht sie innig:", "tokens": ["Als", "er", "auf\u00b7lacht", ",", "fleht", "sie", "in\u00b7nig", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbsieh, er schl\u00e4ft so ruhig fort!", "tokens": ["\u00bb", "sieh", ",", "er", "schl\u00e4ft", "so", "ru\u00b7hig", "fort", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df, bis er erwacht, ihn leben!\u00ab \u2013", "tokens": ["La\u00df", ",", "bis", "er", "er\u00b7wacht", ",", "ihn", "le\u00b7ben", "!", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "VVPP", "$,", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er gelobt's mit kurzem Wort.", "tokens": ["Er", "ge\u00b7lobt's", "mit", "kur\u00b7zem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Still am Flintensteine schraubend,", "tokens": ["Still", "am", "Flin\u00b7ten\u00b7stei\u00b7ne", "schrau\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt er auf den Feind so wild;", "tokens": ["Blickt", "er", "auf", "den", "Feind", "so", "wild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lautlos auf die Kniee sinkend,", "tokens": ["Laut\u00b7los", "auf", "die", "Kni\u00b7ee", "sin\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt sie bleich, ein Marmorbild.", "tokens": ["Liegt", "sie", "bleich", ",", "ein", "Mar\u00b7mor\u00b7bild", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbglaubst du nicht an seinen Engel,", "tokens": ["\u00bb", "glaubst", "du", "nicht", "an", "sei\u00b7nen", "En\u00b7gel", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder bist du's selbst zumeist?\u00ab", "tokens": ["O\u00b7der", "bist", "du's", "selbst", "zu\u00b7meist", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, ich bete \u2013 seufzt sie weinend \u2013", "tokens": ["Ach", ",", "ich", "be\u00b7te", "\u2013", "seufzt", "sie", "wei\u00b7nend", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$(", "VVFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du nie ein M\u00f6rder seist!", "tokens": ["Da\u00df", "du", "nie", "ein", "M\u00f6r\u00b7der", "seist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Pulver auf die Pfanne sch\u00fcttend,", "tokens": ["Pul\u00b7ver", "auf", "die", "Pfan\u00b7ne", "sch\u00fct\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht er finster, ungeirrt:", "tokens": ["Spricht", "er", "fins\u00b7ter", ",", "un\u00b7ge\u00b7irrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwenn ich auch ein M\u00f6rder werde,", "tokens": ["\u00bb", "wenn", "ich", "auch", "ein", "M\u00f6r\u00b7der", "wer\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es nur, da\u00df Der's nicht wird!\u00ab", "tokens": ["Ist", "es", "nur", ",", "da\u00df", "Der's", "nicht", "wird", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "PTKNEG", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ringsum Stille, durch das Summen", "tokens": ["Ring\u00b7sum", "Stil\u00b7le", ",", "durch", "das", "Sum\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines K\u00e4fers kaum gest\u00f6rt,", "tokens": ["Ei\u00b7nes", "K\u00e4\u00b7fers", "kaum", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief genug, da\u00df man des Schl\u00e4fers", "tokens": ["Tief", "ge\u00b7nug", ",", "da\u00df", "man", "des", "Schl\u00e4\u00b7fers"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise Athemz\u00fcge h\u00f6rt.", "tokens": ["Lei\u00b7se", "A\u00b7them\u00b7z\u00fc\u00b7ge", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "In die k\u00fchle Felsengrotte", "tokens": ["In", "die", "k\u00fch\u00b7le", "Fel\u00b7sen\u00b7grot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt der junge J\u00e4ger ein.", "tokens": ["Tritt", "der", "jun\u00b7ge", "J\u00e4\u00b7ger", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00df ist's drau\u00dfen, um zu schlummern,", "tokens": ["Hei\u00df", "ist's", "drau\u00b7\u00dfen", ",", "um", "zu", "schlum\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Legt er still sich auf's Gestein.", "tokens": ["Legt", "er", "still", "sich", "auf's", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und der Schlaf, der ewig milde,", "tokens": ["Und", "der", "Schlaf", ",", "der", "e\u00b7wig", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dft ihm bald die Augen dicht;", "tokens": ["Schlie\u00dft", "ihm", "bald", "die", "Au\u00b7gen", "dicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfer Tr\u00e4ume lichte Schatten", "tokens": ["S\u00fc\u00b7\u00dfer", "Tr\u00e4u\u00b7me", "lich\u00b7te", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegen \u00fcber sein Gesicht.", "tokens": ["Flie\u00b7gen", "\u00fc\u00b7ber", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "In die k\u00fchle Felsengrotte", "tokens": ["In", "die", "k\u00fch\u00b7le", "Fel\u00b7sen\u00b7grot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt ein M\u00e4dchen, hoch und schlank,", "tokens": ["Tritt", "ein", "M\u00e4d\u00b7chen", ",", "hoch", "und", "schlank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht den Schl\u00e4fer, hold erschreckend,", "tokens": ["Sieht", "den", "Schl\u00e4\u00b7fer", ",", "hold", "er\u00b7schre\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht sich hastig seiner Bank.", "tokens": ["Naht", "sich", "has\u00b7tig", "sei\u00b7ner", "Bank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Will ihn wecken, h\u00f6ret Schritte,", "tokens": ["Will", "ihn", "we\u00b7cken", ",", "h\u00f6\u00b7ret", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft mit Angst: es ist zu sp\u00e4t!", "tokens": ["Ruft", "mit", "Angst", ":", "es", "ist", "zu", "sp\u00e4t", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht des Kreuzes schirmend Zeichen", "tokens": ["Macht", "des", "Kreu\u00b7zes", "schir\u00b7mend", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber ihn, wie im Gebet.", "tokens": ["Ue\u00b7ber", "ihn", ",", "wie", "im", "Ge\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.17": {"line.1": {"text": "In die Grotte tritt der Wildsch\u00fctz,", "tokens": ["In", "die", "Grot\u00b7te", "tritt", "der", "Wild\u00b7sch\u00fctz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht den jungen J\u00e4gersmann,", "tokens": ["Sieht", "den", "jun\u00b7gen", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Greift erblassend nach der B\u00fcchse,", "tokens": ["Greift", "er\u00b7blas\u00b7send", "nach", "der", "B\u