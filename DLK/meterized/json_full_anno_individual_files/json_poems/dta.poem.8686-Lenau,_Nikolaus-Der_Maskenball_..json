{"dta.poem.8686": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Maskenball .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Wirres Durcheinanderwallen", "tokens": ["Wir\u00b7res", "Durch\u00b7ein\u00b7an\u00b7der\u00b7wal\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den lichten S\u00e4ulenhallen.", "tokens": ["In", "den", "lich\u00b7ten", "S\u00e4u\u00b7len\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Drommeten hell Gedr\u00f6hne,", "tokens": ["Der", "Drom\u00b7me\u00b7ten", "hell", "Ge\u00b7dr\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und der Geigen tolle Lieder", "tokens": ["Und", "der", "Gei\u00b7gen", "tol\u00b7le", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00fcrzen vom Ger\u00fcste nieder,", "tokens": ["St\u00fcr\u00b7zen", "vom", "Ge\u00b7r\u00fcs\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Wildbach froher T\u00f6ne;", "tokens": ["Als", "ein", "Wild\u00b7bach", "fro\u00b7her", "T\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem Strome leicht bezwungen", "tokens": ["Von", "dem", "Stro\u00b7me", "leicht", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird der G\u00e4ste bunte Menge,", "tokens": ["Wird", "der", "G\u00e4s\u00b7te", "bun\u00b7te", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wird vom seligen Gedr\u00e4nge", "tokens": ["Wird", "vom", "se\u00b7li\u00b7gen", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Rascher T\u00e4nze schnell verschlungen.", "tokens": ["Ra\u00b7scher", "T\u00e4n\u00b7ze", "schnell", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Blumen und Orangenb\u00e4ume", "tokens": ["Blu\u00b7men", "und", "O\u00b7ran\u00b7gen\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bl\u00fchen, duften rings im Saale,", "tokens": ["Bl\u00fc\u00b7hen", ",", "duf\u00b7ten", "rings", "im", "Saa\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mahnen, holde Fr\u00fchlingstr\u00e4ume,", "tokens": ["Mah\u00b7nen", ",", "hol\u00b7de", "Fr\u00fch\u00b7lings\u00b7tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mich an ferne Bl\u00fcthenthale,", "tokens": ["Mich", "an", "fer\u00b7ne", "Bl\u00fc\u00b7thent\u00b7ha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Wecken mit dem stillen Gru\u00df", "tokens": ["We\u00b7cken", "mit", "dem", "stil\u00b7len", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Mir ein banges Hinverlangen,", "tokens": ["Mir", "ein", "ban\u00b7ges", "Hin\u00b7ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Hauchen ihren leisen Ku\u00df", "tokens": ["Hau\u00b7chen", "ih\u00b7ren", "lei\u00b7sen", "Ku\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Sch\u00f6nen M\u00e4dchen an die Wangen;", "tokens": ["Sch\u00f6\u00b7nen", "M\u00e4d\u00b7chen", "an", "die", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Doch den Frohen, Ruhelosen,", "tokens": ["Doch", "den", "Fro\u00b7hen", ",", "Ru\u00b7he\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Weht nicht Sehnsucht in dem Hauche,", "tokens": ["Weht", "nicht", "Sehn\u00b7sucht", "in", "dem", "Hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Sind ja selber junge Rosen,", "tokens": ["Sind", "ja", "sel\u00b7ber", "jun\u00b7ge", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Die entflogen ihrem Strauche;", "tokens": ["Die", "ent\u00b7flo\u00b7gen", "ih\u00b7rem", "Strau\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Flatternd in geliebten T\u00e4nzen,", "tokens": ["Flat\u00b7ternd", "in", "ge\u00b7lieb\u00b7ten", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Dem Gewinde bald entbunden,", "tokens": ["Dem", "Ge\u00b7win\u00b7de", "bald", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Bald zu anmuthvollen Kr\u00e4nzen", "tokens": ["Bald", "zu", "an\u00b7muth\u00b7vol\u00b7len", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Von der Freude frisch gewunden;", "tokens": ["Von", "der", "Freu\u00b7de", "frisch", "ge\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "K\u00f6nnen sinnend nicht verweilen,", "tokens": ["K\u00f6n\u00b7nen", "sin\u00b7nend", "nicht", "ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "M\u00fcssen im Vergn\u00fcgen eilen,", "tokens": ["M\u00fcs\u00b7sen", "im", "Ver\u00b7gn\u00fc\u00b7gen", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Denn des Welkens Klage naht.", "tokens": ["Denn", "des", "Wel\u00b7kens", "Kla\u00b7ge", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Nie zu s\u00fchnender Verrath", "tokens": ["Nie", "zu", "s\u00fch\u00b7nen\u00b7der", "Ver\u00b7rath"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "An der Bl\u00fcthe Augenblicken", "tokens": ["An", "der", "Bl\u00fc\u00b7the", "Au\u00b7gen\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "W\u00e4re jede tr\u00fcbe S\u00e4umni\u00df.", "tokens": ["W\u00e4\u00b7re", "je\u00b7de", "tr\u00fc\u00b7be", "S\u00e4u\u00b7mni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Seht, da schwebt mit trautem Nicken,", "tokens": ["Seht", ",", "da", "schwebt", "mit", "trau\u00b7tem", "Ni\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Ein s\u00fc\u00df neckendes Geheimni\u00df,", "tokens": ["Ein", "s\u00fc\u00df", "ne\u00b7cken\u00b7des", "Ge\u00b7heim\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Eine holde Maske her.", "tokens": ["Ei\u00b7ne", "hol\u00b7de", "Mas\u00b7ke", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Ach, wer bist du? sage, wer? \u2014", "tokens": ["Ach", ",", "wer", "bist", "du", "?", "sa\u00b7ge", ",", "wer", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "PPER", "$.", "VVFIN", "$,", "PWS", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Lind und weich von heller Seide", "tokens": ["Lind", "und", "weich", "von", "hel\u00b7ler", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Ist dein schlanker Leib umfangen,", "tokens": ["Ist", "dein", "schlan\u00b7ker", "Leib", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Und vom amarantnen Kleide", "tokens": ["Und", "vom", "a\u00b7mar\u00b7ant\u00b7nen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Leicht und luftig \u00fcberhangen,", "tokens": ["Leicht", "und", "luf\u00b7tig", "\u00fc\u00b7berh\u00b7an\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Und du strahlst im Glanz des Goldes,", "tokens": ["Und", "du", "strahlst", "im", "Glanz", "des", "Gol\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Polenm\u00e4dchen! wunderholdes!", "tokens": ["Po\u00b7len\u00b7m\u00e4d\u00b7chen", "!", "wun\u00b7der\u00b7hol\u00b7des", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Schalkhaft k\u00fchn dein K\u00e4ppchen sizt,", "tokens": ["Schalk\u00b7haft", "k\u00fchn", "dein", "K\u00e4pp\u00b7chen", "sizt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Trotzend auf so sch\u00f6ne Stelle;", "tokens": ["Trot\u00b7zend", "auf", "so", "sch\u00f6\u00b7ne", "Stel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Wie der Demantstern dir blizt", "tokens": ["Wie", "der", "De\u00b7mants\u00b7tern", "dir", "blizt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.46": {"text": "Aus der Nacht der Lockenwelle!", "tokens": ["Aus", "der", "Nacht", "der", "Lo\u00b7cken\u00b7wel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Wie die Perlen dich umschmiegen,", "tokens": ["Wie", "die", "Per\u00b7len", "dich", "um\u00b7schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Die dir froh am Halse liegen!", "tokens": ["Die", "dir", "froh", "am", "Hal\u00b7se", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Deine Reize still zu ehren,", "tokens": ["Dei\u00b7ne", "Rei\u00b7ze", "still", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Haben sie sich dort vereinet.", "tokens": ["Ha\u00b7ben", "sie", "sich", "dort", "ver\u00b7ei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Hat ein Gott dir Freudez\u00e4hren", "tokens": ["Hat", "ein", "Gott", "dir", "Freu\u00b7de\u00b7z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "An den sch\u00f6nen Hals geweinet? \u2014", "tokens": ["An", "den", "sch\u00f6\u00b7nen", "Hals", "ge\u00b7wei\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Doch betracht' ich dich genauer,", "tokens": ["Doch", "be\u00b7tracht'", "ich", "dich", "ge\u00b7nau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Wei\u00df ich nicht, wie mir geschieht,", "tokens": ["Wei\u00df", "ich", "nicht", ",", "wie", "mir", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "R\u00fchrst du mir das Herz zur Trauer,", "tokens": ["R\u00fchrst", "du", "mir", "das", "Herz", "zur", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Und die heitre Deutung flieht.", "tokens": ["Und", "die", "heit\u00b7re", "Deu\u00b7tung", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.57": {"text": "M\u00e4dchen, willst du in Symbolen:", "tokens": ["M\u00e4d\u00b7chen", ",", "willst", "du", "in", "Sym\u00b7bo\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Wei\u00dfem Nacken, Perlenschn\u00fcren,", "tokens": ["Wei\u00b7\u00dfem", "Na\u00b7cken", ",", "Per\u00b7len\u00b7schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Uns das Trauerloos der Polen", "tokens": ["Uns", "das", "Trau\u00b7er\u00b7loos", "der", "Po\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Mahnend vor die Seele f\u00fchren?", "tokens": ["Mah\u00b7nend", "vor", "die", "See\u00b7le", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Zeigen uns im sch\u00f6nen Bilde", "tokens": ["Zei\u00b7gen", "uns", "im", "sch\u00f6\u00b7nen", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Thr\u00e4nenvolle Schneegefilde?", "tokens": ["Thr\u00e4\u00b7nen\u00b7vol\u00b7le", "Schnee\u00b7ge\u00b7fil\u00b7de", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Ja, du kamst in dieses Haus,", "tokens": ["Ja", ",", "du", "kamst", "in", "die\u00b7ses", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Leise strafend uns zu tragen", "tokens": ["Lei\u00b7se", "stra\u00b7fend", "uns", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "In den schmerzvergessnen Braus", "tokens": ["In", "den", "schmerz\u00b7ver\u00b7gess\u00b7nen", "Braus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.66": {"text": "Polens Gl\u00fcck aus alten Tagen,", "tokens": ["Po\u00b7lens", "Gl\u00fcck", "aus", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.67": {"text": "Da\u00df wir seinen Fall bedenken,", "tokens": ["Da\u00df", "wir", "sei\u00b7nen", "Fall", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Und in Wehmuth uns versenken. \u2014", "tokens": ["Und", "in", "Weh\u00b7muth", "uns", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.69": {"text": "Abgewendet nun mit Schweigen,", "tokens": ["Ab\u00b7ge\u00b7wen\u00b7det", "nun", "mit", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Schwindest du im dichten Reigen,", "tokens": ["Schwin\u00b7dest", "du", "im", "dich\u00b7ten", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.71": {"text": "Wie Polonia's Herrlichkeit", "tokens": ["Wie", "Po\u00b7lo\u00b7nia's", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.72": {"text": "Schwand im wilden Tanz der Zeit. \u2014", "tokens": ["Schwand", "im", "wil\u00b7den", "Tanz", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.73": {"text": "Masken kommen, immer neue:", "tokens": ["Mas\u00b7ken", "kom\u00b7men", ",", "im\u00b7mer", "neu\u00b7e", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Hier ein Ritter mit der Dame,", "tokens": ["Hier", "ein", "Rit\u00b7ter", "mit", "der", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.75": {"text": "Spricht von seinem Liebesgrame,", "tokens": ["Spricht", "von", "sei\u00b7nem", "Lie\u00b7bes\u00b7gra\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Und gelobt ihr seine Treue.", "tokens": ["Und", "ge\u00b7lobt", "ihr", "sei\u00b7ne", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.77": {"text": "Dort im h\u00e4renen Gewande,", "tokens": ["Dort", "im", "h\u00e4\u00b7re\u00b7nen", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.78": {"text": "Mit Sandal' und Muschelhut,", "tokens": ["Mit", "San\u00b7dal'", "und", "Mu\u00b7schel\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.79": {"text": "Wie entr\u00fcckt in ferne Lande,", "tokens": ["Wie", "ent\u00b7r\u00fcckt", "in", "fer\u00b7ne", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.80": {"text": "Ueber Berg' und Meeresfluth \u2014", "tokens": ["Ue\u00b7ber", "Ber\u00b7g'", "und", "Mee\u00b7res\u00b7fluth"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.81": {"text": "Steht ein Pilger; seine Tr\u00e4ume", "tokens": ["Steht", "ein", "Pil\u00b7ger", ";", "sei\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "S\u00e4useln ihm wie Palmenb\u00e4ume,", "tokens": ["S\u00e4u\u00b7seln", "ihm", "wie", "Pal\u00b7men\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.83": {"text": "Zaubern ihn zum heil'gen Grabe,", "tokens": ["Zau\u00b7bern", "ihn", "zum", "heil'\u00b7gen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.84": {"text": "Seines Glaubens liebster Habe. \u2014", "tokens": ["Sei\u00b7nes", "Glau\u00b7bens", "liebs\u00b7ter", "Ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.85": {"text": "Seyd willkommen mir, Matrosen!", "tokens": ["Seyd", "will\u00b7kom\u00b7men", "mir", ",", "Mat\u00b7ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Nehmt mich auf in eurem Schiffe!", "tokens": ["Nehmt", "mich", "auf", "in", "eu\u00b7rem", "Schif\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.87": {"text": "Frisch hinaus ins Meerestosen,", "tokens": ["Frisch", "hin\u00b7aus", "ins", "Mee\u00b7res\u00b7to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.88": {"text": "Durch die fluthbesch\u00e4umten Riffe!", "tokens": ["Durch", "die", "fluth\u00b7be\u00b7sch\u00e4um\u00b7ten", "Rif\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.89": {"text": "Ha! schon seh' ich M\u00f6wen ziehn,", "tokens": ["Ha", "!", "schon", "seh'", "ich", "M\u00f6\u00b7wen", "ziehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.90": {"text": "Wetterwolken seh' ich jagen,", "tokens": ["Wet\u00b7ter\u00b7wol\u00b7ken", "seh'", "ich", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.91": {"text": "Und die St\u00fcrme h\u00f6r' ich schlagen.", "tokens": ["Und", "die", "St\u00fcr\u00b7me", "h\u00f6r'", "ich", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.92": {"text": "S\u00fc\u00dfe Heimat, fahre hin!", "tokens": ["S\u00fc\u00b7\u00dfe", "Hei\u00b7mat", ",", "fah\u00b7re", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.93": {"text": "Nach der Freiheit Paradiesen", "tokens": ["Nach", "der", "Frei\u00b7heit", "Pa\u00b7ra\u00b7die\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.94": {"text": "Nehmen wir den raschen Zug,", "tokens": ["Neh\u00b7men", "wir", "den", "ra\u00b7schen", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.95": {"text": "Wo in heil'gen Waldverliesen", "tokens": ["Wo", "in", "heil'\u00b7gen", "Wald\u00b7ver\u00b7lie\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.96": {"text": "Kein Tyrann sich Throne schlug.", "tokens": ["Kein", "Ty\u00b7rann", "sich", "Thro\u00b7ne", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.97": {"text": "Weihend mich mit stillem Beten,", "tokens": ["Wei\u00b7hend", "mich", "mit", "stil\u00b7lem", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.98": {"text": "Will den Urwald ich betreten,", "tokens": ["Will", "den", "Ur\u00b7wald", "ich", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.99": {"text": "Wandeln will ich durch die Hallen,", "tokens": ["Wan\u00b7deln", "will", "ich", "durch", "die", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.100": {"text": "Wo die Schauer Gottes wallen;", "tokens": ["Wo", "die", "Schau\u00b7er", "Got\u00b7tes", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.101": {"text": "Wo in wunderbarer Pracht", "tokens": ["Wo", "in", "wun\u00b7der\u00b7ba\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.102": {"text": "Himmelw\u00e4rts die B\u00e4ume dringen,", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "die", "B\u00e4u\u00b7me", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.103": {"text": "Brausend um die keusche Nacht", "tokens": ["Brau\u00b7send", "um", "die", "keu\u00b7sche", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.104": {"text": "Ihre Riesenarme schlingen.", "tokens": ["Ih\u00b7re", "Rie\u00b7se\u00b7nar\u00b7me", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.105": {"text": "Wo Leuchtk\u00e4fer, Miriaden,", "tokens": ["Wo", "Leucht\u00b7k\u00e4\u00b7fer", ",", "Mi\u00b7ri\u00b7a\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.106": {"text": "Um die Schlingeblumen fliegen,", "tokens": ["Um", "die", "Schlin\u00b7ge\u00b7blu\u00b7men", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.107": {"text": "Die sich an die B\u00e4ume schmiegen,", "tokens": ["Die", "sich", "an", "die", "B\u00e4u\u00b7me", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.108": {"text": "Auf des Bl\u00fchens dunklen Pfaden", "tokens": ["Auf", "des", "Bl\u00fc\u00b7hens", "dunk\u00b7len", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.109": {"text": "Leuchten sie den Duftgewinden,", "tokens": ["Leuch\u00b7ten", "sie", "den", "Duft\u00b7ge\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.110": {"text": "Lehren sie den Wipfel finden. \u2014", "tokens": ["Leh\u00b7ren", "sie", "den", "Wip\u00b7fel", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.111": {"text": "Dort will ich f\u00fcr meinen Kummer", "tokens": ["Dort", "will", "ich", "f\u00fcr", "mei\u00b7nen", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.112": {"text": "Finden den ersehnten Schlummer,", "tokens": ["Fin\u00b7den", "den", "er\u00b7sehn\u00b7ten", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.113": {"text": "Will vom Schicksal Kunde werben,", 