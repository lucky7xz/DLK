{"textgrid.poem.57553": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "4. Stubbenkammer", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es klatscht die Woge \u00fcber Bord,", "tokens": ["Es", "klatscht", "die", "Wo\u00b7ge", "\u00fc\u00b7ber", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es spritzt der Schaum mir ins Gesicht,", "tokens": ["Es", "spritzt", "der", "Schaum", "mir", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es \u00e4chzt und pfeift im Takelwerk \u2013", "tokens": ["Es", "\u00e4chzt", "und", "pfeift", "im", "Ta\u00b7kel\u00b7werk", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich f\u00fcrchte nicht, ich hoffe nicht.", "tokens": ["Ich", "f\u00fcrch\u00b7te", "nicht", ",", "ich", "hof\u00b7fe", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Blitze zucken durchs Gew\u00f6lk,", "tokens": ["Die", "Blit\u00b7ze", "zu\u00b7cken", "durchs", "Ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Donner knattert durch den Sturm,", "tokens": ["Der", "Don\u00b7ner", "knat\u00b7tert", "durch", "den", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die M\u00f6we kreischt und ruft und schrillt,", "tokens": ["Die", "M\u00f6\u00b7we", "kreischt", "und", "ruft", "und", "schrillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Kielboot kr\u00fcmmt sich wie ein Wurm ...", "tokens": ["Das", "Kiel\u00b7boot", "kr\u00fcmmt", "sich", "wie", "ein", "Wurm", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Recht so, Kollege Himmel droben,", "tokens": ["Recht", "so", ",", "Kol\u00b7le\u00b7ge", "Him\u00b7mel", "dro\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "NN", "ADV", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Auch du bist ungl\u00fccklich verliebt,", "tokens": ["Auch", "du", "bist", "un\u00b7gl\u00fcck\u00b7lich", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und deine Lyrik mu\u00df ich loben,", "tokens": ["Und", "dei\u00b7ne", "Ly\u00b7rik", "mu\u00df", "ich", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hei, wie das br\u00fcllt und flammt und stiebt!", "tokens": ["Hei", ",", "wie", "das", "br\u00fcllt", "und", "flammt", "und", "stiebt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und doch, was hilft dein br\u00fcnstig Singen,", "tokens": ["Und", "doch", ",", "was", "hilft", "dein", "br\u00fcns\u00b7tig", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Herz bleibt kalt, dein Arm bleibt leer,", "tokens": ["Ihr", "Herz", "bleibt", "kalt", ",", "dein", "Arm", "bleibt", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du wirst dein Liebchen nie erringen,", "tokens": ["Du", "wirst", "dein", "Lieb\u00b7chen", "nie", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das schwarzgelockte, sch\u00f6ne Meer,", "tokens": ["Das", "schwarz\u00b7ge\u00b7lock\u00b7te", ",", "sch\u00f6\u00b7ne", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schau, in des Erdengotts Umarmung,", "tokens": ["Schau", ",", "in", "des", "Er\u00b7den\u00b7gotts", "Um\u00b7ar\u00b7mung", ","], "token_in