{"textgrid.poem.67426": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Uz", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df meinen schlichten Vers Dir sagen, was", "tokens": ["La\u00df", "mei\u00b7nen", "schlich\u00b7ten", "Vers", "Dir", "sa\u00b7gen", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein langes ", "tokens": ["Ein", "lan\u00b7ges"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nicht sagen mochte, v\u00e4terlicher Greis,", "tokens": ["Nicht", "sa\u00b7gen", "moch\u00b7te", ",", "v\u00e4\u00b7ter\u00b7li\u00b7cher", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sehr mein ganzes Herz, als ich Dich sah,", "tokens": ["Wie", "sehr", "mein", "gan\u00b7zes", "Herz", ",", "als", "ich", "Dich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An Deinem Anblick hing, an Deiner Brust,", "tokens": ["An", "Dei\u00b7nem", "An\u00b7blick", "hing", ",", "an", "Dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "