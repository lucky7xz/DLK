{"textgrid.poem.37475": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Dorfe wohnt ein Vetter,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Dorfe wohnt ein Vetter,", "tokens": ["Im", "Dor\u00b7fe", "wohnt", "ein", "Vet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gut versichert war", "tokens": ["Der", "gut", "ver\u00b7si\u00b7chert", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor Brand und Hagelwetter", "tokens": ["Vor", "Brand", "und", "Ha\u00b7gel\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun schon im zehnten Jahr.", "tokens": ["Nun", "schon", "im", "zehn\u00b7ten", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch nie seit dazumalen", "tokens": ["Doch", "nie", "seit", "da\u00b7zu\u00b7ma\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist ein Malheur passiert,", "tokens": ["Ist", "ein", "Mal\u00b7heur", "pas\u00b7siert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und so f\u00fcr nichts zu zahlen,", "tokens": ["Und", "so", "f\u00fcr", "nichts", "zu", "zah\u00b7len", "