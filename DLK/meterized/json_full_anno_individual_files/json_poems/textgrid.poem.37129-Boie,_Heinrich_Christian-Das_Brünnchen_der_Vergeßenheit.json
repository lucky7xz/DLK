{"textgrid.poem.37129": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Br\u00fcnnchen der Verge\u00dfenheit", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verzehrt von Schwermuth und von Liebe", "tokens": ["Ver\u00b7zehrt", "von", "Schwer\u00b7muth", "und", "von", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flo\u00df immer seufzend, immer tr\u00fcbe", "tokens": ["Flo\u00df", "im\u00b7mer", "seuf\u00b7zend", ",", "im\u00b7mer", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "VVPP", "$,", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Selindor in ein Br\u00fcnnchen hin;", "tokens": ["Se\u00b7lin\u00b7dor", "in", "ein", "Br\u00fcnn\u00b7chen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle, die zu diesem kamen,", "tokens": ["Und", "al\u00b7le", ",", "die", "zu", "die\u00b7sem", "ka\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verga\u00dfen trinkend selbst den Namen", "tokens": ["Ver\u00b7ga\u00b7\u00dfen", "trin\u00b7kend", "selbst", "den", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der ungetreuen Sch\u00e4ferin.", "tokens": ["Der", "un\u00b7ge\u00b7treu\u00b7en", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Philinden endlich zu verge\u00dfen,", "tokens": ["Phi\u00b7lin\u00b7den", "end\u00b7lich", "zu", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schon zu lang die\u00df Herz bese\u00dfen,", "tokens": ["Die", "schon", "zu", "lang", "die\u00df", "Herz", "be\u00b7se\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "ADJD", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kam ich auch j\u00fcngst hierher gerannt.", "tokens": ["Kam", "ich", "auch", "j\u00fcngst", "hier\u00b7her", "ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sie war mir zuvorgekommen", "tokens": ["Doch", "sie", "war", "mir", "zu\u00b7vor\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und hatte schon so viel genommen,", "tokens": ["Und", "hat\u00b7te", "schon", "so", "viel", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich f\u00fcr mich kein Tr\u00f6pfchen fand.", "tokens": ["Da\u00df", "ich", "f\u00fcr", "mich", "kein", "Tr\u00f6pf\u00b7chen", "fand", "."], "t