{"textgrid.poem.37986": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "6. Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Stauffenberg zur ersten Nacht,", "tokens": ["Auf", "Stauf\u00b7fen\u00b7berg", "zur", "ers\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur sch\u00f6nen Frau sein Herze dacht,", "tokens": ["Zur", "sch\u00f6\u00b7nen", "Frau", "sein", "Her\u00b7ze", "dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Alsbald an seinem Arme lag,", "tokens": ["Als\u00b7bald", "an", "sei\u00b7nem", "Ar\u00b7me", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sein mit steten Treuen pflag,", "tokens": ["Die", "sein", "mit", "ste\u00b7ten", "Treu\u00b7en", "pflag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie weinte, sprach: \u00bbNun wehe dir,", "tokens": ["Sie", "wein\u00b7te", ",", "sprach", ":", "\u00bb", "Nun", "we\u00b7he", "dir", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du folgtest gar zu wenig mir.", "tokens": ["Du", "folg\u00b7test", "gar", "zu", "we\u00b7nig", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "PIS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df du ein Weib nimmst zu der Eh,", "tokens": ["Da\u00df", "du", "ein", "Weib", "nimmst", "zu", "der", "Eh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am dritten Tag du lebst nicht mehr,", "tokens": ["Am", "drit\u00b7ten", "Tag", "du", "lebst", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sag dir was geschehen mu\u00df,", "tokens": ["Ich", "sag", "dir", "was", "ge\u00b7sche\u00b7hen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich lasse sehen meinen Fu\u00df,", "tokens": ["Ich", "las\u00b7se", "se\u00b7hen", "mei\u00b7nen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den sollen sehen Frau und Mann,", "tokens": ["Den", "sol\u00b7len", "se\u00b7hen", "Frau", "und", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sollen sich verwundern dran.", "tokens": ["Und", "sol\u00b7len", "sich", "ver\u00b7wun\u00b7dern", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So nun dein Aug den auch ersieht,", "tokens": ["So", "nun", "dein", "Aug", "den", "auch", "er\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sollst da l\u00e4nger s\u00e4umen nicht,", "tokens": ["So", "sollst", "da", "l\u00e4n\u00b7ger", "s\u00e4u\u00b7men", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADJD", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn es sich immer anders wendt,", "tokens": ["Denn", "es", "sich", "im\u00b7mer", "an\u00b7ders", "wendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Empfangt das heilge Sakrament,", "tokens": ["Emp\u00b7fangt", "das", "heil\u00b7ge", "Sak\u00b7ra\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du weist, da\u00df ich dir Glauben halten,", "tokens": ["Du", "weist", ",", "da\u00df", "ich", "dir", "Glau\u00b7ben", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf ewig sind wir nun zerspalten.\u00ab", "tokens": ["Auf", "e\u00b7wig", "sind", "wir", "nun", "zer\u00b7spal\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "VAFIN", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit nassem Aug sie zu ihm sprach:", "tokens": ["Mit", "nas\u00b7sem", "Aug", "sie", "zu", "ihm", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbherr denket flei\u00dfig nach der Sach,", "tokens": ["\u00bb", "herr", "den\u00b7ket", "flei\u00b7\u00dfig", "nach", "der", "Sach", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihr dauret mich im Herzen mein,", "tokens": ["Ihr", "dau\u00b7ret", "mich", "im", "Her\u00b7zen", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich nicht mehr kann bey euch seyn,", "tokens": ["Da\u00df", "ich", "nicht", "mehr", "kann", "bey", "euch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df mich nun nimmer sieht ein Mann,", "tokens": ["Da\u00df", "mich", "nun", "nim\u00b7mer", "sieht", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich fall in ewger Liebe Bann.\u00ab", "tokens": ["Ich", "fall", "in", "ew\u00b7ger", "Lie\u00b7be", "Bann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem Ritter liefen die Augen \u00fcber:", "tokens": ["Dem", "Rit\u00b7ter", "lie\u00b7fen", "die", "Au\u00b7gen", "\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbsoll ich denn nie dich sehen wieder,", "tokens": ["\u00bb", "soll", "ich", "denn", "nie", "dich", "se\u00b7hen", "wie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So seys geklagt dem h\u00f6chsten Gott,", "tokens": ["So", "seys", "ge\u00b7klagt", "dem", "h\u00f6chs\u00b7ten", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ende balde meine Noth,", "tokens": ["Der", "en\u00b7de", "bal\u00b7de", "mei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach da\u00df ich je zu Ruhm gekommen,", "tokens": ["Ach", "da\u00df", "ich", "je", "zu", "Ruhm", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df mich ein f\u00fcrstlich Weib genommen.", "tokens": ["Da\u00df", "mich", "ein", "f\u00fcrst\u00b7lich", "Weib", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie k\u00fc\u00dfte ihn auf seinen Mund,", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "ihn", "auf", "sei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie weinten beide zu der Stund,", "tokens": ["Sie", "wein\u00b7ten", "bei\u00b7de", "zu", "der", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umfingen einander noch mit Lieb,", "tokens": ["Um\u00b7fin\u00b7gen", "ein\u00b7an\u00b7der", "noch", "mit", "Lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie dr\u00fcckten zusammen beyde Br\u00fcst:", "tokens": ["Sie", "dr\u00fcck\u00b7ten", "zu\u00b7sam\u00b7men", "bey\u00b7de", "Br\u00fcst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00bbach sterben das ist jezt euer Gewinn,", "tokens": ["\u00bb", "ach", "ster\u00b7ben", "das", "ist", "jezt", "eu\u00b7er", "Ge\u00b7winn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN