{"textgrid.poem.24168": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die Ballade vom Tod und dem Zecher", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stell die Uhr ab, Freund Hein,", "tokens": ["Stell", "die", "Uhr", "ab", ",", "Freund", "Hein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "NN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schenk zum letzten Mal ein", "tokens": ["Schenk", "zum", "letz\u00b7ten", "Mal", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Meinen gl\u00e4senen Becher", "tokens": ["Mei\u00b7nen", "gl\u00e4\u00b7se\u00b7nen", "Be\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit tiefrotem Wein!", "tokens": ["Mit", "tief\u00b7ro\u00b7tem", "Wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df dein Sensengeschwank,", "tokens": ["La\u00df", "dein", "Sen\u00b7sen\u00b7ge\u00b7schwank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Setz dich her auf die Bank,", "tokens": ["Setz", "dich", "her", "auf", "die", "Bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin ein friedlicher Zecher", "tokens": ["Bin", "ein", "fried\u00b7li\u00b7cher", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und trinke nicht Zank.", "tokens": ["Und", "trin\u00b7ke", "nicht", "Zank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Gelt, der Wein da ist gut?", "tokens": ["Gelt", ",", "der", "Wein", "da", "ist", "gut", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Burgunderisch Blut!", "tokens": ["Bur\u00b7gun\u00b7de\u00b7risch", "Blut", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Molk oft mir im Keller", "tokens": ["Molk", "oft", "mir", "im", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Aus dem Fasse Mut.", "tokens": ["Aus", "dem", "Fas\u00b7se", "Mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Warum trinkst du denn nicht?", "tokens": ["Wa\u00b7rum", "trinkst", "du", "denn", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O du kalkig Gesicht!", "tokens": ["O", "du", "kal\u00b7kig", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Trink aus doch, trink schneller,", "tokens": ["Trink", "aus", "doch", ",", "trink", "schnel\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Langweiliger Wicht!", "tokens": ["Lang\u00b7wei\u00b7li\u00b7ger", "Wicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Herrgott, bist du fad!", "tokens": ["Herr\u00b7gott", ",", "bist", "du", "fad", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Es ist tief jammerschad,", "tokens": ["Es", "ist", "tief", "jam\u00b7mer\u00b7schad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df der Tod so'n langweiliger", "tokens": ["Da\u00df", "der", "Tod", "so'n", "lang\u00b7wei\u00b7li\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJA"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zechkamerad!", "tokens": ["Zech\u00b7ka\u00b7me\u00b7rad", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "H\u00e4tt es nimmer gedacht,", "tokens": ["H\u00e4tt", "es", "nim\u00b7mer", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da\u00df der Tod bei der Nacht", "tokens": ["Da\u00df", "der", "Tod", "bei", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ein Gesicht wie ein heiliger", "tokens": ["Ein", "Ge\u00b7sicht", "wie", "ein", "hei\u00b7li\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "ADJA"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Marabu macht.", "tokens": ["Ma\u00b7ra\u00b7bu", "macht", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "---+", "measure": "unknown.measure.single"}}, "stanza.7": {"line.1": {"text": "Gestorben mu\u00df sein,", "tokens": ["Ge\u00b7stor\u00b7ben", "mu\u00df", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VAINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Doch ich sehe nicht ein,", "tokens": ["Doch", "ich", "se\u00b7he", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Warum so steifleinene", "tokens": ["Wa\u00b7rum", "so", "stei\u00b7flei\u00b7ne\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Zeremonein.", "tokens": ["Ze\u00b7re\u00b7mo\u00b7nein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Nur n\u00e4her ger\u00fcckt!", "tokens": ["Nur", "n\u00e4\u00b7her", "ge\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Nur die Glatze geb\u00fcckt!", "tokens": ["Nur", "die", "Glat\u00b7ze", "ge\u00b7b\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sei die hell elfenbeinene", "tokens": ["Sei", "die", "hell", "el\u00b7fen\u00b7bei\u00b7ne\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJD", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Rosengeschm\u00fcckt!", "tokens": ["Ro\u00b7sen\u00b7ge\u00b7schm\u00fcckt", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Na, was fehlt noch? Vielleicht,", "tokens": ["Na", ",", "was", "fehlt", "noch", "?", "Viel\u00b7leicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ADV", "$.", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da\u00df ein Fiedelmann geigt?", "tokens": ["Da\u00df", "ein", "Fie\u00b7del\u00b7mann", "geigt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Los, L\u00e4ndler und Lieder!", "tokens": ["Los", ",", "L\u00e4nd\u00b7ler", "und", "Lie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Sensenmann schweigt.", "tokens": ["Der", "Sen\u00b7sen\u00b7mann", "schweigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Wie, noch immer verstimmt?", "tokens": ["Wie", ",", "noch", "im\u00b7mer", "ver\u00b7stimmt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Tief scheinst du ergrimmt!", "tokens": ["Tief", "scheinst", "du", "er\u00b7grimmt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch die Lust kommt dem wieder,", "tokens": ["Doch", "die", "Lust", "kommt", "dem", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADV", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der ein M\u00e4del sich nimmt.", "tokens": ["Der", "ein", "M\u00e4\u00b7del", "sich", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Komm herein, Leonor',", "tokens": ["Komm", "her\u00b7ein", ",", "Le\u00b7o\u00b7nor'", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Tanz dem Tode was vor,", "tokens": ["Tanz", "dem", "To\u00b7de", "was", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PWS", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Indessen Belinde", "tokens": ["In\u00b7des\u00b7sen", "Be\u00b7lin\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ihn kraue am Ohr.", "tokens": ["Ihn", "krau\u00b7e", "am", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Und es kommen zu Zwein", "tokens": ["Und", "es", "kom\u00b7men", "zu", "Zwein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die M\u00e4dchen herein,", "tokens": ["Die", "M\u00e4d\u00b7chen", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und es singen gelinde", "tokens": ["Und", "es", "sin\u00b7gen", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Geig und Schalmein.", "tokens": ["Geig", "und", "Schal\u00b7mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.13": {"line.1": {"text": "Ist ein lustiger Takt,", "tokens": ["Ist", "ein", "lus\u00b7ti\u00b7ger", "Takt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und die M\u00e4dchen sind nackt,", "tokens": ["Und", "die", "M\u00e4d\u00b7chen", "sind", "nackt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und den Tod hat der Zecher", "tokens": ["Und", "den", "Tod", "hat", "der", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Am Arme gepackt.", "tokens": ["Am", "Ar\u00b7me", "ge\u00b7packt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "Da eist ihm das Blut,", "tokens": ["Da", "eist", "ihm", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und es schrickt ihm der Mut,", "tokens": ["Und", "es", "schrickt", "ihm", "der", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und er greift nach dem Becher,", "tokens": ["Und", "er", "greift", "nach", "dem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Im Becher ist Blut.", "tokens": ["Im", "Be\u00b7cher", "ist", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.15": {"line.1": {"text": "Ist Blut, \u2013 aber bla\u00df,", "tokens": ["Ist", "Blut", ",", "\u2013", "a\u00b7ber", "bla\u00df", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "$(", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ein eisschaurig Na\u00df.", "tokens": ["Ein", "eis\u00b7schau\u00b7rig", "Na\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Trink, trink doch, du Frecher,", "tokens": ["Trink", ",", "trink", "doch", ",", "du", "Fre\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der Tod sch\u00e4nkt dir das!", "tokens": ["Der", "Tod", "sch\u00e4nkt", "dir", "das", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDS", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.16": {"line.1": {"text": "Will nit lumpen sich l\u00e2n,", "tokens": ["Will", "nit", "lum\u00b7pen", "sich", "l\u00e2n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "PRF", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auch zum Tanz tritt er an,", "tokens": ["Auch", "zum", "Tanz", "tritt", "er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hat auch Fr\u00e4ulein zweie", "tokens": ["Hat", "auch", "Fr\u00e4u\u00b7lein", "zwei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "CARD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Geladen zum Plan.", "tokens": ["Ge\u00b7la\u00b7den", "zum", "Plan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.17": {"line.1": {"text": "Sind auch splitternackt,", "tokens": ["Sind", "auch", "split\u00b7ter\u00b7nackt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Tanzen auch nach dem Takt,", "tokens": ["Tan\u00b7zen", "auch", "nach", "dem", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und des Todes Schalmeie", "tokens": ["Und", "des", "To\u00b7des", "Schal\u00b7mei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die fl\u00f6tet vertrackt.", "tokens": ["Die", "fl\u00f6\u00b7tet", "ver\u00b7trackt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.18": {"line.1": {"text": "Ist ein Menschengebein,", "tokens": ["Ist", "ein", "Men\u00b7schen\u00b7ge\u00b7bein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Gedrechselt fein,", "tokens": ["Ge\u00b7drech\u00b7selt", "fein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihre Tanzlieder klingen", "tokens": ["Ih\u00b7re", "Tanz\u00b7lie\u00b7der", "klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wie Fegfeuerschrein.", "tokens": ["Wie", "Feg\u00b7feu\u00b7er\u00b7schrein", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.19": {"line.1": {"text": "Und es schrillt die Schalmei,", "tokens": ["Und", "es", "schrillt", "die", "Schal\u00b7mei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und es packen die Zwei", "tokens": ["Und", "es", "pa\u00b7cken", "die", "Zwei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "CARD"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und drehen und schwingen", "tokens": ["Und", "dre\u00b7hen", "und", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im Tanze ihn frei.", "tokens": ["Im", "Tan\u00b7ze", "ihn", "frei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.20": {"line.1": {"text": "Leer\u00e4ugig und kalt", "tokens": ["Lee\u00b7r\u00e4u\u00b7gig", "und", "kalt"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und mi\u00dfgestalt", "tokens": ["Und", "mi\u00df\u00b7ge\u00b7stalt"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sind die T\u00e4nzerinnen", "tokens": ["Sind", "die", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und moderalt.", "tokens": ["Und", "mo\u00b7de\u00b7ralt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.21": {"line.1": {"text": "In grinsender Ruh,", "tokens": ["In", "grin\u00b7sen\u00b7der", "Ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Turulu, Turulu,", "tokens": ["Tu\u00b7ru\u00b7lu", ",", "Tu\u00b7ru\u00b7lu", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Spielt der Sensenmann selber", "tokens": ["Spielt", "der", "Sen\u00b7sen\u00b7mann", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Den Hopser dazu,", "tokens": 