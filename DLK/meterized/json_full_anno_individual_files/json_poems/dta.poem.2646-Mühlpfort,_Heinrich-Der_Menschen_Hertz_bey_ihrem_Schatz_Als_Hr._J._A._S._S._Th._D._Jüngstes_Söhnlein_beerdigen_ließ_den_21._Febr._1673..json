{"dta.poem.2646": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Menschen Hertz bey ihrem Schatz/  \n Als Hr.   J. A. S. S. Th. D.   J\u00fcngstes S\u00f6hnlein  \n beerdigen lie\u00df/ den 21. Febr. 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr schn\u00f6den Sterblichen/ die ihr nach Sch\u00e4tzen trachtet/", "tokens": ["Ihr", "schn\u00f6\u00b7den", "Sterb\u00b7li\u00b7chen", "/", "die", "ihr", "nach", "Sch\u00e4t\u00b7zen", "trach\u00b7tet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und oft die Seele gar f\u00fcr Geld und Gut verkaufft/", "tokens": ["Und", "oft", "die", "See\u00b7le", "gar", "f\u00fcr", "Geld", "und", "Gut", "ver\u00b7kaufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "APPR", "NN", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ihr das Reichthum mehr als euer Leben achtet/", "tokens": ["Die", "ihr", "das", "Reicht\u00b7hum", "mehr", "als", "eu\u00b7er", "Le\u00b7ben", "ach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PIAT", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und umb das eitle Geld nach Ost- und Westen laufft;", "tokens": ["Und", "umb", "das", "eit\u00b7le", "Geld", "nach", "Ost", "und", "Wes\u00b7ten", "laufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In Hoffnung durch gro\u00df Gut was herrlich zu erreichen/", "tokens": ["In", "Hoff\u00b7nung", "durch", "gro\u00df", "Gut", "was", "herr\u00b7lich", "zu", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJD", "NN", "PWS", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df eures Hauses Stamm von hohen Namen prangt/", "tokens": ["Da\u00df", "eu\u00b7res", "Hau\u00b7ses", "Stamm", "von", "ho\u00b7hen", "Na\u00b7men", "prangt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df das Verm\u00f6gen soll nicht von den Kindern weichen/", "tokens": ["Da\u00df", "das", "Ver\u00b7m\u00f6\u00b7gen", "soll", "nicht", "von", "den", "Kin\u00b7dern", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So mit viel tausend M\u00fch/ und noch mehr S\u00fcnd erlangt;", "tokens": ["So", "mit", "viel", "tau\u00b7send", "M\u00fch", "/", "und", "noch", "mehr", "S\u00fcnd", "er\u00b7langt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "CARD", "NN", "$(", "KON", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr die der Geld-Geitz auch noch in dem Sterben qu\u00e4let/", "tokens": ["Ihr", "die", "der", "Geld\u00b7Geitz", "auch", "noch", "in", "dem", "Ster\u00b7ben", "qu\u00e4\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und niemals nicht verl\u00e4\u00dft bi\u00df an denletzten Hauch;", "tokens": ["Und", "nie\u00b7mals", "nicht", "ver\u00b7l\u00e4\u00dft", "bi\u00df", "an", "den\u00b7letz\u00b7ten", "Hauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sagt/ wo ihr schliessen k\u00f6nnt/ was ihr euch habt erwehlet/", "tokens": ["Sagt", "/", "wo", "ihr", "schlies\u00b7sen", "k\u00f6nnt", "/", "was", "ihr", "euch", "habt", "er\u00b7weh\u00b7let", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAV", "PPER", "VVINF", "VVFIN", "$(", "PWS", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "F\u00fcr Diamante Gla\u00df/ f\u00fcr Rosen Heck\u2019 und Strauch.", "tokens": ["F\u00fcr", "Di\u00b7a\u00b7man\u00b7te", "Gla\u00df", "/", "f\u00fcr", "Ro\u00b7sen", "Heck'", "und", "Strauch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$(", "APPR", "NE", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gott der von Ewigkeit der Menschen Hertz ergr\u00fcndet/", "tokens": ["Gott", "der", "von", "E\u00b7wig\u00b7keit", "der", "Men\u00b7schen", "Hertz", "er\u00b7gr\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Und dessen Heimligkeit ihm nicht verborgen bleibt/", "tokens": ["Und", "des\u00b7sen", "Heim\u00b7lig\u00b7keit", "ihm", "nicht", "ver\u00b7bor\u00b7gen", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "PTKNEG", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Spricht: Sammlet einen Schatz den man im Himmel findet/", "tokens": ["Spricht", ":", "Samm\u00b7let", "ei\u00b7nen", "Schatz", "den", "man", "im", "Him\u00b7mel", "fin\u00b7det", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "ART", "NN", "ART", "PIS", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den auch kein Dieb nit stiehlt/ noch Mott\u2019 und Rost zerreibt.", "tokens": ["Den", "auch", "kein", "Dieb", "nit", "stiehlt", "/", "noch", "Mott'", "und", "Rost", "zer\u00b7reibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "PTKNEG", "VVFIN", "$(", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie reimen sich denn nun des Plutus schwartze Kammern/", "tokens": ["Wie", "rei\u00b7men", "sich", "denn", "nun", "des", "Plu\u00b7tus", "schwart\u00b7ze", "Kam\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und de\u00df Jerusalems bestrahlte Sonnen-Stadt?", "tokens": ["Und", "de\u00df", "Je\u00b7ru\u00b7sa\u00b7lems", "be\u00b7strahl\u00b7te", "Son\u00b7nen\u00b7Stadt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Schatz der wird bewacht mit Winseln/ Sorgen/ Jammern/", "tokens": ["Ein", "Schatz", "der", "wird", "be\u00b7wacht", "mit", "Win\u00b7seln", "/", "Sor\u00b7gen", "/", "Jam\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "VVPP", "APPR", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein Schatz der Fried und Ruhm in sich die F\u00fclle hat.", "tokens": ["Ein", "Schatz", "der", "Fried", "und", "Ruhm", "in", "sich", "die", "F\u00fcl\u00b7le", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "APPR", "PRF", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was sucht ihr Reichthum hier umb ewig dort zu darben/", "tokens": ["Was", "sucht", "ihr", "Reicht\u00b7hum", "hier", "umb", "e\u00b7wig", "dort", "zu", "dar\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Kriecht in der Erde Scho\u00df/ den Himmel nicht zusehn?", "tokens": ["Kriecht", "in", "der", "Er\u00b7de", "Scho\u00df", "/", "den", "Him\u00b7mel", "nicht", "zu\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$(", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Streut immer Saamen au\u00df und erndet niemals Garben/", "tokens": ["Streut", "im\u00b7mer", "Saa\u00b7men", "au\u00df", "und", "ern\u00b7det", "nie\u00b7mals", "Gar\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "PTKVZ", "KON", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Baut auf geh\u00e4ufften Sand/ den Winde leicht verwehn?", "tokens": ["Baut", "auf", "ge\u00b7h\u00e4uff\u00b7ten", "Sand", "/", "den", "Win\u00b7de", "leicht", "ver\u00b7wehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$(", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Erkennet nicht einmal den Geber alles Guten/", "tokens": ["Er\u00b7ken\u00b7net", "nicht", "ein\u00b7mal", "den", "Ge\u00b7ber", "al\u00b7les", "Gu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "H\u00e4ngt/ als wie angeleimt/ an einem klumpen Gold;", "tokens": ["H\u00e4ngt", "/", "als", "wie", "an\u00b7ge\u00b7leimt", "/", "an", "ei\u00b7nem", "klum\u00b7pen", "Gold", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "KOKOM", "VVPP", "$(", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Traut seinem Worte nicht/ mehr aber wilden Fluten/", "tokens": ["Traut", "sei\u00b7nem", "Wor\u00b7te", "nicht", "/", "mehr", "a\u00b7ber", "wil\u00b7den", "Flu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "$(", "ADV", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Verwechselt um Gewin des Sch\u00f6pffers Gnad und Huld.", "tokens": ["Ver\u00b7wech\u00b7selt", "um", "Ge\u00b7win", "des", "Sch\u00f6pf\u00b7fers", "Gnad", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sagt was sind eure Sch\u00e4tz? Abg\u00f6tter eurer Sinnen/", "tokens": ["Sagt", "was", "sind", "eu\u00b7re", "Sch\u00e4tz", "?", "Ab\u00b7g\u00f6t\u00b7ter", "eu\u00b7rer", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "VAFIN", "PPOSAT", "NN", "$.", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "An was h\u00e4ngt euer Hertz? Ach am verdammten Geld.", "tokens": ["An", "was", "h\u00e4ngt", "eu\u00b7er", "Hertz", "?", "Ach", "am", "ver\u00b7damm\u00b7ten", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "PPOSAT", "NN", "$.", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Was wird euch dieser S