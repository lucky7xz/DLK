{"textgrid.poem.55948": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Erblindende", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sa\u00df so wie die anderen beim Tee.", "tokens": ["Sie", "sa\u00df", "so", "wie", "die", "an\u00b7de\u00b7ren", "beim", "Tee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir war zuerst, als ob sie ihre Tasse", "tokens": ["Mir", "war", "zu\u00b7erst", ",", "als", "ob", "sie", "ih\u00b7re", "Tas\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOKOM", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ein wenig anders als die andern fasse.", "tokens": ["ein", "we\u00b7nig", "an\u00b7ders", "als", "die", "an\u00b7dern", "fas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie l\u00e4chelte einmal. Es tat fast weh.", "tokens": ["Sie", "l\u00e4\u00b7chel\u00b7te", "ein\u00b7mal", ".", "Es", "tat", "fast", "weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und als man schlie\u00dflich sich erhob und sprach", "tokens": ["Und", "als", "man", "schlie\u00df\u00b7lich", "sich", "er\u00b7hob", "und", "sprach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADJD", "PRF", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und langsam und wie es der Zufall brachte", "tokens": ["und", "lang\u00b7sam", "und", "wie", "es", "der", "Zu\u00b7fall", "brach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "PWAV", "PPER", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "durch viele Zimmer ging (man sprach und lachte),", "tokens": ["durch", "vie\u00b7le", "Zim\u00b7mer", "ging", "(", "man", "sprach", "und", "lach\u00b7te", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$(", "PIS", "VVFIN", "KON", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da sah ich sie. Sie ging den andern nach,", "tokens": ["da", "sah", "ich", "sie", ".", "Sie", "ging", "den", "an\u00b7dern", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "verhalten, so wie eine, welche gleich", "tokens": ["ver\u00b7hal\u00b7ten", ",", "so", "wie", "ei\u00b7ne", ",", "wel\u00b7che", "gleich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "KOKOM", "ART", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wird singen m\u00fcssen und vor vielen Leuten;", "tokens": ["wird", "sin\u00b7gen", "m\u00fcs\u00b7sen", "und", "vor", "vie\u00b7len", "Leu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "wo