{"dta.poem.12915": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre sch\u00f6nen augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus was vor ungemeinem zeuge", "tokens": ["Aus", "was", "vor", "un\u00b7ge\u00b7mei\u00b7nem", "zeu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind dieser sternen pracht,", "tokens": ["Sind", "die\u00b7ser", "ster\u00b7nen", "pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor welchen ich mein hertze beuge,", "tokens": ["Vor", "wel\u00b7chen", "ich", "mein", "hert\u00b7ze", "beu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Liebe! doch gemacht?", "tokens": ["O", "Lie\u00b7be", "!", "doch", "ge\u00b7macht", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In wahrheit, so viel glantz in enge circkel bringen;", "tokens": ["In", "wahr\u00b7heit", ",", "so", "viel", "glantz", "in", "en\u00b7ge", "cir\u00b7ckel", "brin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und eine solche glut in zartes fleisch zu zwingen,", "tokens": ["Und", "ei\u00b7ne", "sol\u00b7che", "glut", "in", "zar\u00b7tes", "fleisch", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist etwas, das kein witz, der irdisch ist, erdenckt;", "tokens": ["Ist", "et\u00b7was", ",", "das", "kein", "witz", ",", "der", "ir\u00b7disch", "ist", ",", "er\u00b7denckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PIAT", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jedoch die lieb\u2019 ist blind, so kan ich sicher schl\u00fc\u00dfen:", "tokens": ["Je\u00b7doch", "die", "lieb'", "ist", "blind", ",", "so", "kan", "ich", "si\u00b7cher", "schl\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "ADJD", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df du, o Amor! dir die augen ausgerissen,", "tokens": ["Da\u00df", "du", ",", "o", "A\u00b7mor", "!", "dir", "die", "au\u00b7gen", "aus\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "FM", "$.", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und sie der Sylvia geschenckt.", "tokens": ["Und", "sie", "der", "Syl\u00b7via", "ge\u00b7schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NE", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}