{"textgrid.poem.67356": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach Gott", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein n\u00fcchtern V\u00f6gelchen, das h\u00f6ret,", "tokens": ["Wie", "ein", "n\u00fcch\u00b7tern", "V\u00f6\u00b7gel\u00b7chen", ",", "das", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Siehet seiner Mutter Fl\u00fcgel schlagen", "tokens": ["Sie\u00b7het", "sei\u00b7ner", "Mut\u00b7ter", "Fl\u00fc\u00b7gel", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ueberm Neste, wenn sie Speis' ihm bringet", "tokens": ["Ue\u00b7berm", "Nes\u00b7te", ",", "wenn", "sie", "Speis'", "ihm", "brin\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und es neu belebt mit Blick und Speise;", "tokens": ["Und", "es", "neu", "be\u00b7lebt", "mit", "Blick", "und", "Spei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ungeduldig regt es seines Fittigs", "tokens": ["Un\u00b7ge\u00b7dul\u00b7dig", "regt", "es", "sei\u00b7nes", "Fit\u00b7tigs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spr\u00f6\u00dflinge, zu folgen ihr im Fluge,", "tokens": ["Spr\u00f6\u00df\u00b7lin\u00b7ge", ",", "zu", "fol\u00b7gen", "ihr", "im", "Flu\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Girret Dank ihr, da\u00df die schwache Zunge,", "tokens": ["Gir\u00b7ret", "Dank", "ihr", ",", "da\u00df", "die", "schwa\u00b7che", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ueber K\u00f6nnen girrend, los sich windet:", "tokens": ["Ue\u00b7ber", "K\u00f6n\u00b7nen", "gir\u00b7rend", ",", "los", "sich", "win\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Also ich, wenn warmer Strahl der Gottheit", "tokens": ["Al\u00b7so", "ich", ",", "wenn", "war\u00b7mer", "Strahl", "der", "Got\u00b7theit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "M\u00e4chtiger, lebendiger sich reget", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", ",", "le\u00b7ben\u00b7di\u00b7ger", "sich", "re\u00b7get"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "PRF", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mir im Herzen, da\u00df das Herz erquickt wird", "tokens": 