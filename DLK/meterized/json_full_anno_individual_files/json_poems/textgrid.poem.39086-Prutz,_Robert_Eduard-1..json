, "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADV", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "der N\u00e4chste war das seinem Thron.", "tokens": ["der", "N\u00e4chs\u00b7te", "war", "das", "sei\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun in der Bl\u00fcte seiner Jahre", "tokens": ["Nun", "in", "der", "Bl\u00fc\u00b7te", "sei\u00b7ner", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "hat ihn die Polizei umstrickt,", "tokens": ["hat", "ihn", "die", "Po\u00b7li\u00b7zei", "um\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "nun einsam liegt er auf der Bahre,", "tokens": ["nun", "ein\u00b7sam", "liegt", "er", "auf", "der", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von einem Bodelschwingh erdr\u00fcckt!", "tokens": ["von", "ei\u00b7nem", "Bo\u00b7del\u00b7schwingh", "er\u00b7dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir aber wolln die Gl\u00e4ser heben", "tokens": ["Wir", "a\u00b7ber", "wolln", "die", "Gl\u00e4\u00b7ser", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "V