{"textgrid.poem.34876": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutscher S\u00e4nger! sing und preise", "tokens": ["Deut\u00b7scher", "S\u00e4n\u00b7ger", "!", "sing", "und", "prei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutsche Freiheit, da\u00df dein Lied", "tokens": ["Deut\u00b7sche", "Frei\u00b7heit", ",", "da\u00df", "dein", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsrer Seelen sich bemeistre", "tokens": ["Uns\u00b7rer", "See\u00b7len", "sich", "be\u00b7meist\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zu Taten uns begeistre,", "tokens": ["Und", "zu", "Ta\u00b7ten", "uns", "be\u00b7geist\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In Marseillerhymnenweise.", "tokens": ["In", "Mar\u00b7seil\u00b7ler\u00b7hym\u00b7nen\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Girre nicht mehr wie ein Werther,", "tokens": ["Gir\u00b7re", "nicht", "mehr", "wie", "ein", "Wert\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher nur f\u00fcr Lotten gl\u00fcht \u2013", "tokens": ["Wel\u00b7cher", "nur", "f\u00fcr", "Lot\u00b7ten", "gl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Glocke hat geschlagen,", "tokens": ["Was", "die", "Glo\u00b7cke", "hat", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst du deinem Volke sagen,", "tokens": ["Sollst", "du", "dei\u00b7nem", "Vol\u00b7ke", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rede Dolche, rede Schwerter!", "tokens": ["Re\u00b7de", "Dol\u00b7che", ",", "re\u00b7de", "Schwer\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sei nicht mehr die weiche Fl\u00f6te,", "tokens": ["Sei", "nicht", "mehr", "die", "wei\u00b7che", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das idyllische Gem\u00fct \u2013", "tokens": ["Das", "i\u00b7dyl\u00b7li\u00b7sche", "Ge\u00b7m\u00fct", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei des Vaterlands Posaune,", "tokens": ["Sei", "des", "Va\u00b7ter\u00b7lan