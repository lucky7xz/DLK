{"textgrid.poem.42450": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stand an eines Gartens Rand", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand an eines Gartens Rand", "tokens": ["Ich", "stand", "an", "ei\u00b7nes", "Gar\u00b7tens", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaute in ein herrlich Land,", "tokens": ["Und", "schau\u00b7te", "in", "ein", "herr\u00b7lich", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das, weit gel\u00e4ndet, vor mir bl\u00fcht,", "tokens": ["Das", ",", "weit", "ge\u00b7l\u00e4n\u00b7det", ",", "vor", "mir", "bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "VVPP", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo hei\u00df die Erntesonne gl\u00fcht.", "tokens": ["Wo", "hei\u00df", "die", "Ern\u00b7te\u00b7son\u00b7ne", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Arm in Arm, es war kein Traum,", "tokens": ["Und", "Arm", "in", "Arm", ",", "es", "war", "kein", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Wirt und ich am Apfelbaum,", "tokens": ["Mein", "Wirt", "und", "ich", "am", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir lauschten einer Nachtigall,", "tokens": ["Wir", "lauschten", "ei\u00b7ner", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und Frieden, Frieden \u00fcberall.", "tokens": ["Und", "Frie\u00b7den", ",", "Frie\u00b7den", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Zug auf fernem Schienendamm", "tokens": ["Ein", "Zug", "auf", "fer\u00b7nem", "Schie\u00b7nen\u00b7damm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kam angebraust. Wie zaubersam,", "tokens": ["Kam", "an\u00b7ge\u00b7braust", ".", "Wie", "zau\u00b7ber\u00b7sam", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVPP", "$.", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er brachte frohe Menschen her", "tokens": ["Er", "brach\u00b7te", "fro\u00b7he", "Men\u00b7schen", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und G\u00fcterspende, segenschwer.", "tokens": ["Und", "G\u00fc\u00b7ter\u00b7spen\u00b7de", ",", "se\u00b7gen\u00b7schwer", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Einst sah ich den metallnen Strang", "tokens": ["Einst", "sah", "ich", "den", "me\u00b7tall\u00b7nen", "Strang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Zerst\u00f6rt, zerrissen meilenlang.", "tokens": ["Zer\u00b7st\u00f6rt", ",", "zer\u00b7ris\u00b7sen", "mei\u00b7len\u00b7lang", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und wo ich nun in Blumen stund,", "tokens": ["Und", "wo", "ich", "nun", "in", "Blu\u00b7men", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "War damals wildzerw\u00fchlter Grund.", "tokens": ["War", "da\u00b7mals", "wild\u00b7zer\u00b7w\u00fchl\u00b7ter", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sommermorgen gl\u00e4nzte sch\u00f6n", "tokens": ["Der", "Som\u00b7mer\u00b7mor\u00b7gen", "gl\u00e4nz\u00b7te", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie heute; glitzernd von den H\u00f6hn,", "tokens": ["Wie", "heu\u00b7te", ";", "glit\u00b7zernd", "von", "den", "H\u00f6hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbden ganzen Tag mit Sack und Pack\u00ab,", "tokens": ["\u00bb", "den", "gan\u00b7zen", "Tag", "mit", "Sack", "und", "Pack", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Str\u00f6mt nieder aus Verhau, Verhack", "tokens": ["Str\u00f6mt", "nie\u00b7der", "aus", "Ver\u00b7hau", ",", "Ver\u00b7hack"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum k\u00fchnsten Sturm, ein wei\u00dfes Meer,", "tokens": ["Zum", "k\u00fchns\u00b7ten", "Sturm", ",", "ein", "wei\u00b7\u00dfes", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Feindes wundervolles Heer.", "tokens": ["Des", "Fein\u00b7des", "wun\u00b7der\u00b7vol\u00b7les", "Heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich st\u00fctzte, wie aus Erz gezeugt,", "tokens": ["Ich", "st\u00fctz\u00b7te", ",", "wie", "aus", "Erz", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich auf den S\u00e4bel, vorgebeugt,", "tokens": ["Mich", "auf", "den", "S\u00e4\u00b7bel", ",", "vor\u00b7ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit weiten Augen, offnem Mund,", "tokens": ["Mit", "wei\u00b7ten", "Au\u00b7gen", ",", "off\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als starrt' ich in den H\u00f6llenschlund.", "tokens": ["Als", "starrt'", "ich", "in", "den", "H\u00f6l\u00b7len\u00b7schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nun sind sie da! \u00bbSchnellfeuer!\u00ab \u00bbSteht!\u00ab", "tokens": ["Nun", "sind", "sie", "da", "!", "\u00bb", "Schnell\u00b7feu\u00b7er", "!", "\u00ab", "\u00bb", "Steht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "$(", "NN", "$.", "$(", "$(", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wie hoch im Rauch die Fahne weht!", "tokens": ["Wie", "hoch", "im", "Rauch", "die", "Fah\u00b7ne", "weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und Mann an Mann, hinauf, hinab,", "tokens": ["Und", "Mann", "an", "Mann", ",", "hin\u00b7auf", ",", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und mancher sinkt in Graus und Grab.", "tokens": ["Und", "man\u00b7cher", "sinkt", "in", "Graus", "und", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zu Boden st\u00fcrz' ich, einer sticht", "tokens": ["Zu", "Bo\u00b7den", "st\u00fcrz'", "ich", ",", "ei\u00b7ner", "sticht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und zerrt mich, ich erraff' mich nicht,", "tokens": ["Und", "zerrt", "mich", ",", "ich", "er\u00b7raff'", "mich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und um mich, vor mir, unter mir", "tokens": ["Und", "um", "mich", ",", "vor", "mir", ",", "un\u00b7ter", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ein furchtbar Ringen, Gall' und Gier.", "tokens": ["Ein", "furcht\u00b7bar", "Rin\u00b7gen", ",", "Gall'", "und", "Gier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und \u00fcber unserm w\u00fcsten Knaul", "tokens": ["Und", "\u00fc\u00b7ber", "un\u00b7serm", "w\u00fcs\u00b7ten", "Knaul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "B\u00e4umt sich ein scheu gewordner Gaul.", "tokens": ["B\u00e4umt", "sich", "ein", "scheu", "ge\u00b7word\u00b7ner", "Gaul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Ich seh' der Vorderhufe Blitz,", "tokens": ["Ich", "seh'", "der", "Vor\u00b7der\u00b7hu\u00b7fe", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Blutfestgetrockneten Sporenritz,", "tokens": ["Blut\u00b7fest\u00b7ge\u00b7trock\u00b7ne\u00b7ten", "Spo\u00b7ren\u00b7ritz", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Den Gurt, den angespritzten Kot,", "tokens": ["Den", "Gurt", ",", "den", "an\u00b7ge\u00b7spritz\u00b7ten", "Kot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der aufgebl\u00e4hten N\u00fcstern Rot.", "tokens": ["Der", "auf\u00b7ge\u00b7bl\u00e4h\u00b7ten", "N\u00fcs\u00b7tern", "Rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und zwischen uns mit Klang und Kling", "tokens": ["Und", "zwi\u00b7schen", "uns", "mit", "Klang", "und", "Kling"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Platzt der Granate Eisenring:", "tokens": ["Platzt", "der", "Gra\u00b7na\u00b7te", "Ei\u00b7sen\u00b7ring", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.27": {"text": "Ein Drache br\u00fcllt, die Erde birst,", "tokens": ["Ein", "Dra\u00b7che", "br\u00fcllt", ",", "die", "Er\u00b7de", "birst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Einf\u00e4llt der Weltenhimmelfirst.", "tokens": ["Ein\u00b7f\u00e4llt", "der", "Wel\u00b7ten\u00b7him\u00b7mel\u00b7first", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Es \u00e4chzt, es st\u00f6hnt, und Schutt und Staub", "tokens": ["Es", "\u00e4chzt", ",", "es", "st\u00f6hnt", ",", "und", "Schutt", "und", "Staub"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Umh\u00fcllen Tod und Lorbeerlaub.", "tokens": ["Um\u00b7h\u00fcl\u00b7len", "Tod", "und", "Lor\u00b7beer\u00b7laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich stand an eines Gartens Rand", "tokens": ["Ich", "stand", "an", "ei\u00b7nes", "Gar\u00b7tens", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaute in ein herrlich Land,", "tokens": ["Und", "schau\u00b7te", "in", "ein", "herr\u00b7lich", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ausgebreitet vor mir liegt,", "to