{"textgrid.poem.54163": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Otto Reutter", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gutes Couplet ist nicht immer wirkungsvoll", "tokens": ["Ein", "gu\u00b7tes", "Cou\u00b7plet", "ist", "nicht", "im\u00b7mer", "wir\u00b7kungs\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": " und ein wirkungsvolles Couplet ist nicht immer gut.", "tokens": ["und", "ein", "wir\u00b7kungs\u00b7vol\u00b7les", "Cou\u00b7plet", "ist", "nicht", "im\u00b7mer", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Und sitzt auf der Bahn du ganz eingezw\u00e4ngt,", "tokens": ["Und", "sitzt", "auf", "der", "Bahn", "du", "ganz", "ein\u00b7ge\u00b7zw\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und dir wird noch ne Frau auf den Scho\u00df gedr\u00e4ngt,", "tokens": ["Und", "dir", "wird", "noch", "ne", "Frau", "auf", "den", "Scho\u00df", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und die hat noch ne Schachtel auf ihrem Scho\u00df,", "tokens": ["Und", "die", "hat", "noch", "ne", "Schach\u00b7tel", "auf", "ih\u00b7rem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und du wirst die beiden Schachteln nicht los,", "tokens": ["Und", "du", "wirst", "die", "bei\u00b7den", "Schach\u00b7teln", "nicht", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "PIAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und die F\u00fc\u00dfe werden dir schwer wie Blei:", "tokens": ["Und", "die", "F\u00fc\u00b7\u00dfe", "wer\u00b7den", "dir", "schwer", "wie", "Blei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "KOKOM", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In f\u00fcnfzig Jahren ist alles vorbei!", "tokens": ["In", "f\u00fcnf\u00b7zig", "Jah\u00b7ren", "ist", "al\u00b7les", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Oder bist du beim Zahnarzt \u2013 wenn er dich greift,", "tokens": ["O\u00b7der", "bist", "du", "beim", "Zahn\u00b7arzt", "\u2013", "wenn", "er", "dich", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "--+--+-++-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und dich mit dem Z