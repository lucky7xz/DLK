{"textgrid.poem.62813": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der eifers\u00fcchtige Pankraz", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der eifers\u00fcchtige Pankraz", "tokens": ["Der", "ei\u00b7fer\u00b7s\u00fcch\u00b7ti\u00b7ge", "Pan\u00b7kraz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag \u00e4chzend auf dem Sterbebette", "tokens": ["Lag", "\u00e4ch\u00b7zend", "auf", "dem", "Ster\u00b7be\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach zu seinem Weib, der z\u00e4rtlichen Finette,", "tokens": ["Und", "sprach", "zu", "sei\u00b7nem", "Weib", ",", "der", "z\u00e4rt\u00b7li\u00b7chen", "Fi\u00b7net\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die weinend vor ihm stand: Gehab dich wohl, mein Schatz!", "tokens": ["Die", "wei\u00b7nend", "vor", "ihm", "stand", ":", "Ge\u00b7hab", "dich", "wohl", ",", "mein", "Schatz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$.", "NN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und lieb als Wittwe mich im Grabe", "tokens": ["Und", "lieb", "als", "Witt\u00b7we", "mich", "im", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So treulich, wie du es bisher gethan.", "tokens": ["So", "treu\u00b7lich", ",", "wie", "du", "es", "bis\u00b7her", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Treue, schluchzte sie, verhindert mich daran,", "tokens": ["Die", "Treu\u00b7e", ",", "schluchz\u00b7te", "sie", ",", "ver\u00b7hin\u00b7dert", "mich", "da\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ich mich schon versprochen habe.", "tokens": ["Weil", "ich", "mich", "schon", "ver\u00b7spro\u00b7chen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der eifers\u00fcchtige Pankraz", "tokens": ["Der", "ei\u00b7fer\u00b7s\u00fcch\u00b7ti\u00b7ge", "Pan\u00b7kraz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag \u00e4chzend auf dem Sterbebette", "tokens": ["Lag", "\u00e4ch\u00b7zend", "auf", "dem", "Ster\u00b7be\u00b7bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach zu seinem Weib, der z\u00e4rtlichen Finette,", "tokens": ["Und", "sprach", "zu", "sei\u00b7nem", "Weib", ",", "der", "z