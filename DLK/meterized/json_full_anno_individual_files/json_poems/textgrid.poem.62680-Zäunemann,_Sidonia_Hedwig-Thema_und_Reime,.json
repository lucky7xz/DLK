{"textgrid.poem.62680": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Thema und Reime,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Gott der Arzeney verehrt ein Silber \u2013 \u2013 Haar;", "tokens": ["Den", "Gott", "der", "Ar\u00b7ze\u00b7ney", "ver\u00b7ehrt", "ein", "Sil\u00b7ber", "\u2013", "\u2013", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er widersteht durch Kunst der drohenden \u2013 \u2013 Gefahr,", "tokens": ["Er", "wi\u00b7der\u00b7steht", "durch", "Kunst", "der", "dro\u00b7hen\u00b7den", "\u2013", "\u2013", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die manches zartes Reis zur schwarzen Erde \u2013 \u2013 dr\u00fccken,", "tokens": ["Die", "man\u00b7ches", "zar\u00b7tes", "Reis", "zur", "schwar\u00b7zen", "Er\u00b7de", "\u2013", "\u2013", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der Verwesungs-Macht zum Opfer k\u00f6nte \u2013 \u2013 schicken.", "tokens": ["Und", "der", "Ver\u00b7we\u00b7sungs\u00b7Macht", "zum", "Op\u00b7fer", "k\u00f6n\u00b7te", "\u2013", "\u2013", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VMFIN", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch du alter Arzt, was blehet sich dein \u2013 \u2013 Bauch?", "tokens": ["Je\u00b7doch", "du", "al\u00b7ter", "Arzt", ",", "was", "ble\u00b7het", "sich", "dein", "\u2013", "\u2013", "Bauch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "NN", "$,", "PWS", "VVFIN", "PRF", "PPOSAT", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was runzelst du die Haut und machst die Stirne \u2013 \u2013 rauh?", "tokens": ["Was", "run\u00b7zelst", "du", "die", "Haut", "und", "machst", "die", "Stir\u00b7ne", "\u2013", "\u2013", "rauh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$(", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "La\u00df Zorn, la\u00df Neid und Zank, und kr\u00f6ne den mit \u2013 \u2013 Liebe,", "tokens": ["La\u00df", "Zorn", ",", "la\u00df", "Neid", "und", "Zank", ",", "und", "kr\u00f6\u00b7ne", "den", "mit", "\u2013", "\u2013", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVIMP", "NN", "KON", "NN", "$,", "KON", "VVFIN", "ART", "APPR", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der deine Wissenschaft nach Art der klugen \u2013 \u2013 Diebe", "tokens": ["Der", "dei\u00b7ne", "Wis\u00b7sen\u00b7schaft", "nach", "Art", "der", "klu\u00b7gen", "\u2013", "\u2013", "Die\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "ART", "ADJA", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erhascht, erlauscht und stiehlt; ergieb dich jetzt nur \u2013 \u2013 drein,", "tokens": ["Er\u00b7hascht", ",", "er\u00b7lauscht", "und", "stiehlt", ";", "er\u00b7gieb", "dich", "jetzt", "nur", "\u2013", "\u2013", "drein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "ADV", "$(", "$(", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der junge Mevius f\u00e4llt dir ins Handwerk \u2013 \u2013 ein,", "tokens": ["Der", "jun\u00b7ge", "Me\u00b7vius", "f\u00e4llt", "dir", "ins", "Hand\u00b7werk", "\u2013", "\u2013", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PPER", "APPRART", "NN", "$(", "$(", "PTKVZ", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Er will mit munterm Geist Gallenens Witz er \u2013 \u2013 greifen,", "tokens": ["Er", "will", "mit", "mun\u00b7term", "Geist", "Gal\u00b7le\u00b7nens", "Witz", "er", "\u2013", "\u2013", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "NE", "NN", "PPER", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und auch die kleine Welt mit seinem Sinn durch \u2013 \u2013 streifen.", "tokens": ["Und", "auch", "die", "klei\u00b7ne", "Welt", "mit", "sei\u00b7nem", "Sinn", "durch", "\u2013", "\u2013", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "APPR", "$(", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bricht die Gesundheit gleich und kriegt ein grosses \u2013 \u2013 Loch,", "tokens": ["Bricht", "die", "Ge\u00b7sund\u00b7heit", "gleich", "und", "kriegt", "ein", "gros\u00b7ses", "\u2013", "\u2013", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KON", "VVFIN", "ART", "ADJA", "$(", "$(", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "So lebt doch Mevius mit seiner Weisheit \u2013 \u2013 noch,", "tokens": ["So", "lebt", "doch", "Me\u00b7vius", "mit", "sei\u00b7ner", "Weis\u00b7heit", "\u2013", "\u2013", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "APPR", "PPOSAT", "NN", "$(", "$(", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Der h\u00e4lt ihr Sinken auf. Schreyt Lorgen: ich er \u2013 \u2013 warme,", "tokens": ["Der", "h\u00e4lt", "ihr", "Sin\u00b7ken", "auf", ".", "Schreyt", "Lor\u00b7gen", ":", "ich", "er", "\u2013", "\u2013", "war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "NE", "NN", "$.", "PPER", "PPER", "$(", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das hitzge Fieber w\u00e4chst, so fa\u00dft er sie beym \u2013 \u2013 Arme,", "tokens": ["Das", "hitz\u00b7ge", "Fie\u00b7ber", "w\u00e4chst", ",", "so", "fa\u00dft", "er", "sie", "beym", "\u2013", "\u2013", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und schickt nach ihrem Puls den sch\u00e4rfsten Augen \u2013 \u2013 Blick,", "tokens": ["Und", "schickt", "nach", "ih\u00b7rem", "Puls", "den", "sch\u00e4rfs\u00b7ten", "Au\u00b7gen", "\u2013", "\u2013", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er kennt, und merkt genau, wie er den Schmerzens \u2013 \u2013 Strick", "tokens": ["Er", "kennt", ",", "und", "merkt", "ge\u00b7nau", ",", "wie", "er", "den", "Schmer\u00b7zens", "\u2013", "\u2013", "Strick"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "ART", "NN", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch seinen Rath zerschneid. Was alte Aerzte \u2013 \u2013 naschen,", "tokens": ["Durch", "sei\u00b7nen", "Rath", "zer\u00b7schneid", ".", "Was", "al\u00b7te", "A\u00b7erz\u00b7te", "\u2013", "\u2013", "na\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PWS", "ADJA", "NN", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Kan auch ein junger Arzt erforschen und \u2013 \u2013 erhaschen.", "tokens": ["Kan", "auch", "ein", "jun\u00b7ger", "Arzt", "er\u00b7for\u00b7schen", "und", "\u2013", "\u2013", "er\u00b7ha\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "KON", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach Lorgen! Mevius weis wohl, was vor Ge \u2013 \u2013 stank", "tokens": ["Ach", "Lor\u00b7gen", "!", "Me\u00b7vius", "weis", "wohl", ",", "was", "vor", "Ge", "\u2013", "\u2013", "stank"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["ITJ", "NN", "$.", "NE", "PTKVZ", "ADV", "$,", "PRELS", "APPR", "NN", "$(", "$(", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und Krankheit in dir ist, drum ordnet er den \u2013 \u2013 Trank", "tokens": ["Und", "Krank\u00b7heit", "in", "dir", "ist", ",", "drum", "ord\u00b7net", "er", "den", "\u2013", "\u2013", "Trank"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "NN", "APPR", "PPER", "VAFIN", "$,", "PAV", "VVFIN", "PPER", "ART", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Davor, und hilft dir auf. Ich mu\u00df die Wahrheit \u2013 \u2013 geigen:", "tokens": ["Da\u00b7vor", ",", "und", "hilft", "dir", "auf", ".", "Ich", "mu\u00df", "die", "Wahr\u00b7heit", "\u2013", "\u2013", "gei\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PAV", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VMFIN", "ART", "NN", "$(", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es ist dem jungen Arzt des alten Klugheit \u2013 \u2013 eigen.", "tokens": ["Es", "ist", "dem", "jun\u00b7gen", "Arzt", "des", "al\u00b7ten", "Klug\u00b7heit", "\u2013", "\u2013", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$(", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Gott der Arzeney verehrt ein Silber \u2013 \u2013 Haar;", "tokens": ["Den", "Gott", "der", "Ar\u00b7ze\u00b7ney", "ver\u00b7ehrt", "ein", "Sil\u00b7ber", "\u2013", "\u2013", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er widersteht durch Kunst der drohenden \u2013 \u2013 Gefahr,", "tokens": ["Er", "wi\u00b7der\u00b7steht", "durch", "Kunst", "der", "dro\u00b7hen\u00b7den", "\u2013", "\u2013", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die manches zartes Reis zur schwarzen Erde \u2013 \u2013 dr\u00fccken,", "tokens": ["Die", "man\u00b7ches", "zar\u00b7tes", "Reis", "zur", "schwar\u00b7zen", "Er\u00b7de", "\u2013", "\u2013", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der Verwesungs-Macht zum Opfer k\u00f6nte \u2013 \u2013 schicken.", "tokens": ["Und", "der", "Ver\u00b7we\u00b7sungs\u00b7Macht", "zum", "Op\u00b7fer", "k\u00f6n\u00b7te", "\u2013", "\u2013", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VMFIN", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch du alter Arzt, was blehet sich dein \u2013 \u2013 Bauch?", "tokens": ["Je\u00b7doch", "du", "al\u00b7ter", "Arzt", ",", "was", "ble\u00b7het", "sich", "dein", "\u2013", "\u2013", "Bauch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "NN", "$,", "PWS", "VVFIN", "PRF", "PPOSAT", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was runzelst du die Haut und machst die Stirne \u2013 \u2013 rauh?", "tokens": ["Was", "run\u00b7zelst", "du", "die", "Haut", "und", "machst", "die", "Stir\u00b7ne", "\u2013", "\u2013", "rauh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$(", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "La\u00df Zorn, la\u00df Neid und Zank, und kr\u00f6ne den mit \u2013 \u2013 Liebe,", "tokens": ["La\u00df", "Zorn", ",", "la\u00df", "Neid", "und", "Zank", ",", "und", "kr\u00f6\u00b7ne", "den", "mit", "\u2013", "\u2013", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVIMP", "NN", "KON", "NN", "$,", "KON", "VVFIN", "ART", "APPR", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der deine Wissenschaft nach Art der klugen \u2013 \u2013 Diebe", "tokens": ["Der", "dei\u00b7ne", "Wis\u00b7sen\u00b7schaft", "nach", "Art", "der", "klu\u00b7gen", "\u2013", "\u2013", "Die\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "ART", "ADJA", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erhascht, erlauscht und stiehlt; ergieb dich jetzt nur \u2013 \u2013 drein,", "tokens": ["Er\u00b7hascht", ",", "er\u00b7lauscht", "und", "stiehlt", ";", "er\u00b7gieb", "dich", "jetzt", "nur", "\u2013", "\u2013", "drein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "ADV", "$(", "$(", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der junge Mevius f\u00e4llt dir ins Handwerk \u2013 \u2013 ein,", "tokens": ["Der", "jun\u00b7ge", "Me\u00b7vius", "f\u00e4llt", "dir", "ins", "Hand\u00b7werk", "\u2013", "\u2013", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PPER", "APPRART", "NN", "$(", "$(", "PTKVZ", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Er will mit munterm Geist Gallenens Witz er \u2013 \u2013 greifen,", "tokens": ["Er", "will", "mit", "mun\u00b7term", "Geist", "Gal\u00b7le\u00b7nens", "Witz", "er", "\u2013", "\u2013", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "NE", "NN", "PPER", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und auch die kleine Welt mit seinem Sinn durch \u2013 \u2013 streifen.", "tokens": ["Und", "auch", "die", "klei\u00b7ne", "Welt", "mit", "sei\u00b7nem", "Sinn", "durch", "\u2013", "\u2013", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "APPR", "$(", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bricht die Gesundheit gleich und kriegt ein grosses \u2013 \u2013 Loch,", "tokens": ["Bricht", "die", "Ge\u00b7sund\u00b7heit", "gleich", "und", "kriegt", "ein", "gros\u00b7ses", "\u2013", "\u2013", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KON", "VVFIN", "ART", "ADJA", "$(", "$(", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "So lebt doch Mevius mit seiner Weisheit \u2013 \u2013 noch,", "tokens": ["So", "lebt", "doch", "Me\u00b7vius", "mit", "sei\u00b7ner", "Weis\u00b7heit", "\u2013", "\u2013", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "APPR", "PPOSAT", "NN", "$(", "$(", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Der h\u00e4lt ihr Sinken auf. Schreyt Lorgen: ich er \u2013 \u2013 warme,", "tokens": ["Der", "h\u00e4lt", "ihr", "Sin\u00b7ken", "auf", ".", "Schreyt", "Lor\u00b7gen", ":", "ich", "er", "\u2013", "\u2013", "war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "NE", "NN", "$.", "PPER", "PPER", "$(", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das hitzge Fieber w\u00e4chst, so fa\u00dft er sie beym \u2013 \u2013 Arme,", "tokens": ["Das", "hitz\u00b7ge", "Fie\u00b7ber", "w\u00e4chst", ",", "so", "fa\u00dft", "er", "sie", "beym", "\u2013", "\u2013", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und schickt nach ihrem Puls den sch\u00e4rfsten Augen \u2013 \u2013 Blick,", "tokens": ["Und", "schickt", "nach", "ih\u00b7rem", "Puls", "den", "sch\u00e4rfs\u00b7ten", "Au\u00b7gen", "\u2013", "\u2013", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er kennt, und merkt genau, wie er den Schmerzens \u2013 \u2013 Strick", "tokens": ["Er", "kennt", ",", "und", "merkt", "ge\u00b7nau", ",", "wie", "er", "den", "Schmer\u00b7zens", "\u2013", "\u2013", "Strick"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "ART", "NN", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch seinen Rath zerschneid. Was alte Aerzte \u2013 \u2013 naschen,", "tokens": ["Durch", "sei\u00b7nen", "Rath", "zer\u00b7schneid", ".", "Was", "al\u00b7te", "A\u00b7erz\u00b7te", "\u2013", "\u2013", "na\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PWS", "ADJA", "NN", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Kan auch ein junger Arzt erforschen und \u2013 \u2013 erhaschen.", "tokens": ["Kan", "auch", "ein", "jun\u00b7ger", "Arzt", "er\u00b7for\u00b7schen", "und", "\u2013", "\u2013", "er\u00b7ha\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "KON", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach Lorgen! Mevius weis wohl, was vor Ge \u2013 \u2013 stank", "tokens": ["Ach", "Lor\u00b7gen", "!", "Me\u00b7vius", "weis", "wohl", ",", "was", "vor", "Ge", "\u2013", "\u2013", "stank"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["ITJ", "NN", "$.", "NE", "PTKVZ", "ADV", "$,", "PRELS", "APPR", "NN", "$(", "$(", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und Krankheit in dir ist, drum ordnet er den \u2013 \u2013 Trank", "tokens": ["Und", "Krank\u00b7heit", "in", "dir", "ist", ",", "drum", "ord\u00b7net", "er", "den", "\u2013", "\u2013", "Trank"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "NN", "APPR", "PPER", "VAFIN", "$,", "PAV", "VVFIN", "PPER", "ART", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Davor, und hilft dir auf. Ich mu\u00df die Wahrheit \u2013 \u2013 geigen:", "tokens": ["Da\u00b7vor", ",", "und", "hilft", "dir", "auf", ".", "Ich", "mu\u00df", "die", "Wahr\u00b7heit", "\u2013", "\u2013", "gei\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PAV", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VMFIN", "ART", "NN", "$(", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es ist dem jungen Arzt des alten Klugheit \u2013 \u2013 eigen.", "tokens": ["Es", "ist", "dem", "jun\u00b7gen", "Arzt", "des", "al\u00b7ten", "Klug\u00b7heit", "\u2013", "\u2013", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$(", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}