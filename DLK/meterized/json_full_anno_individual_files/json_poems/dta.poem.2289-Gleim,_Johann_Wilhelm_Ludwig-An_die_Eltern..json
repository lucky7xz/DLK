{"dta.poem.2289": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Eltern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "V\u00e4ter! n\u00f6tigt eure Kinder", "tokens": ["V\u00e4\u00b7ter", "!", "n\u00f6\u00b7tigt", "eu\u00b7re", "Kin\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie zum Lernen solcher K\u00fcnste,", "tokens": ["Nie", "zum", "Ler\u00b7nen", "sol\u00b7cher", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sie nicht erlernen wollen.", "tokens": ["Die", "sie", "nicht", "er\u00b7ler\u00b7nen", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft sie selber was erw\u00e4hlen,", "tokens": ["La\u00dft", "sie", "sel\u00b7ber", "was", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobt und billigt ihre Neigung;", "tokens": ["Lobt", "und", "bil\u00b7ligt", "ih\u00b7re", "Nei\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonst erlebt ihr, wie mein Vater,", "tokens": ["Sonst", "er\u00b7lebt", "ihr", ",", "wie", "mein", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ungl\u00fckk an den besten Kindern.", "tokens": ["Un\u00b7gl\u00fckk", "an", "den", "bes\u00b7ten", "Kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fragt ihn nur, itzt wird er sagen:", "tokens": ["Fragt", "ihn", "nur", ",", "itzt", "wird", "er", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "V\u00e4ter zwinget keine Kinder.", "tokens": ["V\u00e4\u00b7ter", "zwin\u00b7get", "kei\u00b7ne", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ich, sein Sohn, ward auch gezwungen,", "tokens": ["Ich", ",", "sein", "Sohn", ",", "ward", "auch", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber hat es was gefruchtet?", "tokens": ["A\u00b7ber", "hat", "es", "was", "ge\u00b7fruch\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Erst solt ich im schwarzen Kleide,", "tokens": ["Erst", "solt", "ich", "im", "schwar\u00b7zen", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sorgen f\u00fcr die Geister lernen,", "tokens": ["Sor\u00b7gen", "f\u00fcr", "die", "Geis\u00b7ter", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Weil es meine Mutter wolte;", "tokens": ["Weil", "es", "mei\u00b7ne", "Mut\u00b7ter", "wol\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch, es rettete mein Vater", "tokens": ["Doch", ",", "es", "ret\u00b7te\u00b7te", "mein", "Va\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Mich von solchen schweren Sorgen;", "tokens": ["Mich", "von", "sol\u00b7chen", "schwe\u00b7ren", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und da solt ich, wieder Willen,", "tokens": ["Und", "da", "solt", "ich", ",", "wie\u00b7der", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sorgen f\u00fcr die K\u00f6rper lernen,", "tokens": ["Sor\u00b7gen", "f\u00fcr", "die", "K\u00f6r\u00b7per", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Aber es erfuhr mein Vater,", "tokens": ["A\u00b7ber", "es", "er\u00b7fuhr", "mein", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Da\u00df ich lieber gar nichts lernte.", "tokens": ["Da\u00df", "ich", "lie\u00b7ber", "gar", "nichts", "lern\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Endlich nahm er mich beim Arme,", "tokens": ["End\u00b7lich", "nahm", "er", "mich", "beim", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "F\u00fchrte mich zum Advokaten,", "tokens": ["F\u00fchr\u00b7te", "mich", "zum", "Ad\u00b7vo\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und ermahnt ihn, da\u00df ichs h\u00f6rte:", "tokens": ["Und", "er\u00b7mahnt", "ihn", ",", "da\u00df", "ichs", "h\u00f6r\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Vetter, lehre diesen rechten,", "tokens": ["Vet\u00b7ter", ",", "leh\u00b7re", "die\u00b7sen", "rech\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PDAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Halt ihn scharf, und gieb ihm Arbeit.", "tokens": ["Halt", "ihn", "scharf", ",", "und", "gieb", "ihm", "Ar\u00b7beit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "KON", "VVIMP", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Hurtig gab sie mir der Vetter.", "tokens": ["Hur\u00b7tig", "gab", "sie", "mir", "der", "Vet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "K\u00f6pfen, Hangen, Peitschen, R\u00e4dern", "tokens": ["K\u00f6p\u00b7fen", ",", "Han\u00b7gen", ",", "Peit\u00b7schen", ",", "R\u00e4\u00b7dern"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Solt ich aus den Bl\u00e4ttern lernen.", "tokens": ["Solt", "ich", "aus", "den", "Bl\u00e4t\u00b7tern", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "O! wie ha\u00dft ich dieses Handwerk.", "tokens": ["O", "!", "wie", "ha\u00dft", "ich", "die\u00b7ses", "Hand\u00b7werk", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "O! wie w\u00fcnscht ich oft aus Unmut,", "tokens": ["O", "!", "wie", "w\u00fcnscht", "ich", "oft", "aus", "Un\u00b7mut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Meinen Lehrer an den Galgen,", "tokens": ["Mei\u00b7nen", "Leh\u00b7rer", "an", "den", "Gal\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Wenn er mich mit Schriften qu\u00e4lte,", "tokens": ["Wenn", "er", "mich", "mit", "Schrif\u00b7ten", "qu\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Welche Blut und Todt verlangten.", "tokens": ["Wel\u00b7che", "Blut", "und", "Todt", "ver\u00b7lang\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Aber gab er mir Prozesse", "tokens": ["A\u00b7ber", "gab", "er", "mir", "Pro\u00b7zes\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.35": {"text": "Von verlohrnen Liebesbriefen,", "tokens": ["Von", "ver\u00b7lohr\u00b7nen", "Lie\u00b7bes\u00b7brie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Von wilkommnen Nachtgespenstern,", "tokens": ["Von", "wil\u00b7komm\u00b7nen", "Nacht\u00b7ge\u00b7spens\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Von ertappten Anverwandten;", "tokens": ["Von", "er\u00b7tapp\u00b7ten", "An\u00b7ver\u00b7wand\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Oder solt ich, statt der Sch\u00f6nen,", "tokens": ["O\u00b7der", "solt", "ich", ",", "statt", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Uber bl\u00f6de M\u00e4nner klagen:", "tokens": ["U\u00b7ber", "bl\u00f6\u00b7de", "M\u00e4n\u00b7ner", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Gleich war Kopf und Feder flei\u00dfig;", "tokens": ["Gleich", "war", "Kopf", "und", "Fe\u00b7der", "flei\u00b7\u00dfig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Und mein Lehrer kont es merken,", "tokens": ["Und", "mein", "Leh\u00b7rer", "kont", "es", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Da\u00df ich nichts erlernen w\u00fcrde,", "tokens": ["Da\u00df", "ich", "nichts", "er\u00b7ler\u00b7nen", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Als die H\u00e4ndel der Verliebten;", "tokens": ["Als", "die", "H\u00e4n\u00b7del", "der", "Ver\u00b7lieb\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Drum verschaft er mir vom Richter", "tokens": ["Drum", "ver\u00b7schaft", "er", "mir", "vom", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Lauter H\u00e4ndel der Verliebten.", "tokens": ["Lau\u00b7ter", "H\u00e4n\u00b7del", "der", "Ver\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Itzo wei\u00df ich sie zu schlichten.", "tokens": ["It\u00b7zo", "wei\u00df", "ich", "sie", "zu", "schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Drum empfehl ich mich den Sch\u00f6nen,", "tokens": ["Drum", "emp\u00b7fehl", "ich", "mich", "den", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Die mich etwa brauchen m\u00f6chten.", "tokens": ["Die", "mich", "et\u00b7wa", "brau\u00b7chen", "m\u00f6ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}