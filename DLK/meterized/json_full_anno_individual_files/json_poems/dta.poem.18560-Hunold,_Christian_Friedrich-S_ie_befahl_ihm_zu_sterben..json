{"dta.poem.18560": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "S ie befahl ihm zu sterben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "So soll ich denn mein Kind/ in dieser Glut verbrennen/", "tokens": ["So", "soll", "ich", "denn", "mein", "Kind", "/", "in", "die\u00b7ser", "Glut", "ver\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$(", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die deiner Augen-Blitz in meiner Brust erregt?", "tokens": ["Die", "dei\u00b7ner", "Au\u00b7gen\u00b7Blitz", "in", "mei\u00b7ner", "Brust", "er\u00b7regt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohl denn/ ich bin bereit in meinen Tod zu rennen/", "tokens": ["Wohl", "denn", "/", "ich", "bin", "be\u00b7reit", "in", "mei\u00b7nen", "Tod", "zu", "ren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil mir dein sch\u00f6nster Mund es selbsten aufferlegt.", "tokens": ["Weil", "mir", "dein", "sch\u00f6ns\u00b7ter", "Mund", "es", "selbs\u00b7ten", "auf\u00b7fer\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja/ ja/ ich sterbe gern in diesen holden Flammen/", "tokens": ["Ja", "/", "ja", "/", "ich", "ster\u00b7be", "gern", "in", "die\u00b7sen", "hol\u00b7den", "Flam\u00b7men", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PTKANT", "$(", "PPER", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil sie vom Himmel nur und meiner G\u00f6ttin stammen.", "tokens": ["Weil", "sie", "vom", "Him\u00b7mel", "nur", "und", "mei\u00b7ner", "G\u00f6t\u00b7tin", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "La\u00df mich im Sterben doch nur deine Gnad' erlangen/", "tokens": ["La\u00df", "mich", "im", "Ster\u00b7ben", "doch", "nur", "dei\u00b7ne", "Gnad'", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stelle dich zur Grufft mit einen Seufftzer ein/", "tokens": ["Und", "stel\u00b7le", "dich", "zur", "Grufft", "mit", "ei\u00b7nen", "Seufft\u00b7zer", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "APPR", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So will ich meinen Tod mit h\u00f6chster Lust umfangen/", "tokens": ["So", "will", "ich", "mei\u00b7nen", "Tod", "mit", "h\u00f6chs\u00b7ter", "Lust", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und auch im Grabe dir annoch verbunden seyn/", "tokens": ["Und", "auch", "im", "Gra\u00b7be", "dir", "an\u00b7noch", "ver\u00b7bun\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PPER", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es soll mein reiner Geist stets um den deinen schweben", "tokens": ["Es", "soll", "mein", "rei\u00b7ner", "Geist", "stets", "um", "den", "dei\u00b7nen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und so werd' ich im Sarg erst recht verg\u00f6ttert leben.", "tokens": ["Und", "so", "werd'", "ich", "im", "Sarg", "erst", "recht", "ver\u00b7g\u00f6t\u00b7tert", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun/ sch\u00f6nste/ fahre wohl/ mein Geist will schon entweichen/", "tokens": ["Nun", "/", "sch\u00f6ns\u00b7te", "/", "fah\u00b7re", "wohl", "/", "mein", "Geist", "will", "schon", "ent\u00b7wei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "$(", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es lodert Seel' und Leib und steht in voller Glut/", "tokens": ["Es", "lo\u00b7dert", "Seel'", "und", "Leib", "und", "steht", "in", "vol\u00b7ler", "Glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Aetna Feuer ist der Brunst nicht zu vergleichen/", "tokens": ["Des", "A\u00b7et\u00b7na", "Feu\u00b7er", "ist", "der", "Brunst", "nicht", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VAFIN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn was der Blitz ger\u00fchrt/ lescht nichts als Milch und Blut.", "tokens": ["Denn", "was", "der", "Blitz", "ge\u00b7r\u00fchrt", "/", "lescht", "nichts", "als", "Milch", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "$(", "VVFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch soll ein Ph\u00f6nix einst aus meiner Asche lauffen/", "tokens": ["Doch", "soll", "ein", "Ph\u00f6\u00b7nix", "einst", "aus", "mei\u00b7ner", "A\u00b7sche", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So gib mir deinen Schoo\u00df zu meinen Scheiterhauffen.", "tokens": ["So", "gib", "mir", "dei\u00b7nen", "Schoo\u00df", "zu", "mei\u00b7nen", "Schei\u00b7ter\u00b7hauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}