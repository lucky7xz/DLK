{"textgrid.poem.24383": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein wahrer Christ", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer aber edel ists/ ein wahrer Christ zu seyn.", "tokens": ["Schwer", "a\u00b7ber", "e\u00b7del", "ists", "/", "ein", "wah\u00b7rer", "Christ", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "VAFIN", "$(", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verachtet und voll Ruhm/ gering/ doch ungemein.", "tokens": ["Ver\u00b7ach\u00b7tet", "und", "voll", "Ruhm", "/", "ge\u00b7ring", "/", "doch", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "NN", "$(", "ADJD", "$(", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Betr\u00fcbt und auch vergn\u00fcgt/ im Elend hoch begl\u00fccket/", "tokens": ["Be\u00b7tr\u00fcbt", "und", "auch", "ver\u00b7gn\u00fcgt", "/", "im", "E\u00b7lend", "hoch", "be\u00b7gl\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "VVPP", "$(", "APPRART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Gl\u00fcckes Rosen matt/ in Creutzes Dorn erqvicket.", "tokens": ["In", "Gl\u00fc\u00b7ckes", "Ro\u00b7sen", "matt", "/", "in", "Creut\u00b7zes", "Dorn", "er\u00b7qvic\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", 