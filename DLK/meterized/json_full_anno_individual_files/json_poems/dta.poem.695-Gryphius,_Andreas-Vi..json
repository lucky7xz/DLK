{"dta.poem.695": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "O Burg der sterbligkeit! ", "tokens": ["O", "Burg", "der", "ster\u00b7blig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O Kercker voll von Leidt! ", "tokens": ["O", "Ker\u00b7cker", "voll", "von", "Leidt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Erden Leichen-volle grufft! ", "tokens": ["O", "Er\u00b7den", "Lei\u00b7chen\u00b7vol\u00b7le", "grufft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Schlachtbanck/ Stock vnd See/ ", "tokens": ["O", "Schlacht\u00b7banck", "/", "Stock", "vnd", "See", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Abgrund-tieffes weh'! ", "tokens": ["O", "Ab\u00b7grun\u00b7dtief\u00b7fes", "weh'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Beschwert an deinem Joch? ", "tokens": ["Be\u00b7schwert", "an", "dei\u00b7nem", "Joch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der mich mit trost entsetzt/ ", "tokens": ["Der", "mich", "mit", "trost", "ent\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Ach! meiner Seelen licht! ", "tokens": ["Ach", "!", "mei\u00b7ner", "See\u00b7len", "licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ach meine zuversicht! ", "tokens": ["Ach", "mei\u00b7ne", "zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erquicke mich in diesem schmertz! ", "tokens": ["Er\u00b7qui\u00b7cke", "mich", "in", "die\u00b7sem", "schmertz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ernewre die Cedult! ", "tokens": ["Er\u00b7new\u00b7re", "die", "Ce\u00b7dult", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Vergi\u00df der alten Schuldt! ", "tokens": ["Ver\u00b7gi\u00df", "der", "al\u00b7ten", "Schuldt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Rei\u00df/ was mich bindt/ entzwey! ", "tokens": ["Rei\u00df", "/", "was", "mich", "bindt", "/", "ent\u00b7zwey", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "PWS", "PPER", "VVFIN", "$(", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Vnd mach' au\u00df n\u00f6then frey ", "tokens": ["Vnd", "mach'", "au\u00df", "n\u00f6\u00b7then", "frey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein dir so fest-verlobtes Hertz! ", "tokens": ["Ein", "dir", "so", "fest\u00b7ver\u00b7lob\u00b7tes", "Hertz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd f\u00fchr au\u00df dieser pein ", "tokens": ["Vnd", "f\u00fchr", "au\u00df", "die\u00b7ser", "pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich in dein Wonhau\u00df eyn. ", "tokens": ["Mich", "in", "dein", "Won\u00b7hau\u00df", "eyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}