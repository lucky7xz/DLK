{"textgrid.poem.35168": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wie das Meer", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei still in Gott, still wie das Meer!", "tokens": ["Sei", "still", "in", "Gott", ",", "still", "wie", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur seine Fl\u00e4che streift der Wind,", "tokens": ["Nur", "sei\u00b7ne", "Fl\u00e4\u00b7che", "streift", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tobt als Sturm er noch so sehr,", "tokens": ["Und", "tobt", "als", "Sturm", "er", "noch", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wi\u00df, da\u00df die Tiefen ruhig sind.", "tokens": ["Wi\u00df", ",", "da\u00df", "die", "Tie\u00b7fen", "ru\u00b7hig", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Sei weit in Gott, weit wie das Meer!", "tokens": ["Sei", "weit", "in", "Gott", ",", "weit", "wie", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "NN", "$,", "ADJD", "KOKO