{"textgrid.poem.45762": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich dein Album vermehren soll,", "tokens": ["Wenn", "ich", "dein", "Al\u00b7bum", "ver\u00b7meh\u00b7ren", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Begehre nur eilig die Gabe.", "tokens": ["Be\u00b7geh\u00b7re", "nur", "ei\u00b7lig", "die", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich sehe kaum mehr, ich h\u00f6re nicht", "tokens": ["Ich", "se\u00b7he", "kaum", "mehr", ",", "ich", "h\u00f6\u00b7re", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PPER", "VVFIN", "PTKNEG"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und gehe schon st\u00fcckweis zu Grabe.", "tokens": ["Und", "ge\u00b7he", "schon", "st\u00fcck\u00b7weis", "zu", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Alt wie ich bin, und blind und taub,", "tokens": ["Alt", "wie", "ich", "bin", ",", "und", "blind", "und", "taub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "VAFIN", "$,", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure"