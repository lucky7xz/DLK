{"textgrid.poem.59582": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Vaterlandslied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gott, der Eisen wachsen lie\u00df,", "tokens": ["Der", "Gott", ",", "der", "Ei\u00b7sen", "wach\u00b7sen", "lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wollte keine Knechte,", "tokens": ["Der", "woll\u00b7te", "kei\u00b7ne", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum gab er S\u00e4bel, Schwert und Spie\u00df", "tokens": ["Drum", "gab", "er", "S\u00e4\u00b7bel", ",", "Schwert", "und", "Spie\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Mann in seine Rechte,", "tokens": ["Dem", "Mann", "in", "sei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum gab er ihm den k\u00fchnen Mut,", "tokens": ["Drum", "gab", "er", "ihm", "den", "k\u00fch\u00b7nen", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Zorn der freien Rede,", "tokens": ["Den", "Zorn", "der", "frei\u00b7en", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df er best\u00e4nde bis aufs Blut,", "tokens": ["Da\u00df", "er", "be\u00b7st\u00e4n\u00b7de", "bis", "aufs", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis in den Tod die Fehde.", "tokens": ["Bis", "in", "den", "Tod", "die", "Feh\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So wollen wir, was Gott gewollt,", "tokens": ["So", "wol\u00b7len", "wir", ",", "was", "Gott", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PRELS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit rechter Treue halten", "tokens": ["Mit", "rech\u00b7ter", "Treu\u00b7e", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmer im Tyrannensold", "tokens": ["Und", "nim\u00b7mer", "im", "Ty\u00b7ran\u00b7nen\u00b7sold"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menschensch\u00e4del spalten,", "tokens": ["Die", "Men\u00b7schen\u00b7sch\u00e4\u00b7del", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch wer f\u00fcr Tand und Schande ficht,", "tokens": ["Doch", "wer", "f\u00fcr", "Tand", "und", "Schan\u00b7de", "ficht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den hauen wir zu Scherben,", "tokens": ["Den", "hau\u00b7en", "wir", "zu", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der soll im deutschen Lande nicht", "tokens": ["Der", "soll", "im", "deut\u00b7schen", "Lan\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPRART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit deutschen M\u00e4nnern erben.", "tokens": ["Mit", "deut\u00b7schen", "M\u00e4n\u00b7nern", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Deutschland, heil'ges Vaterland!", "tokens": ["O", "Deutschland", ",", "heil'\u00b7ges", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O deutsche Lieb' und Treue!", "tokens": ["O", "deut\u00b7sche", "Lieb'", "und", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hohes Land! Du sch\u00f6nes Land!", "tokens": ["Du", "ho\u00b7hes", "Land", "!", "Du", "sch\u00f6\u00b7nes", "Land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir schw\u00f6ren wir aufs neue:", "tokens": ["Dir", "schw\u00f6\u00b7ren", "wir", "aufs", "neu\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Buben und dem Knecht die Acht!", "tokens": ["Dem", "Bu\u00b7ben", "und", "dem", "Knecht", "die", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der f\u00fcttre Kr\u00e4hn und Raben!", "tokens": ["Der", "f\u00fctt\u00b7re", "Kr\u00e4hn", "und", "Ra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So ziehn wir aus zur Hermannsschlacht", "tokens": ["So", "ziehn", "wir", "aus", "zur", "Her\u00b7manns\u00b7schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wollen Rache haben.", "tokens": ["Und", "wol\u00b7len", "Ra\u00b7che", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00dft brausen, was nur brausen kann,", "tokens": ["La\u00dft", "brau\u00b7sen", ",", "was", "nur", "brau\u00b7sen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In hellen, lichten Flammen!", "tokens": ["In", "hel\u00b7len", ",", "lich\u00b7ten", "Flam\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Deutschen alle Mann f\u00fcr Mann", "tokens": ["Ihr", "Deut\u00b7schen", "al\u00b7le", "Mann", "f\u00fcr", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcrs Vaterland zusammen!", "tokens": ["F\u00fcrs", "Va\u00b7ter\u00b7land", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und hebt die Herzen himmelan!", "tokens": ["Und", "hebt", "die", "Her\u00b7zen", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und himmelan die H\u00e4nde!", "tokens": ["Und", "him\u00b7me\u00b7lan", "die", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und rufet alle Mann f\u00fcr Mann:", "tokens": ["Und", "ru\u00b7fet", "al\u00b7le", "Mann", "f\u00fcr", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Knechtschaft hat ein Ende!", "tokens": ["Die", "Knecht\u00b7schaft", "hat", "ein", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00dft klingen, was nur klingen kann,", "tokens": ["La\u00dft", "klin\u00b7gen", ",", "was", "nur", "klin\u00b7gen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Trommeln und die Fl\u00f6ten!", "tokens": ["Die", "Trom\u00b7meln", "und", "die", "Fl\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen heute Mann f\u00fcr Mann", "tokens": ["Wir", "wol\u00b7len", "heu\u00b7te", "Mann", "f\u00fcr", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Blut das Eisen r\u00f6ten,", "tokens": ["Mit", "Blut", "das", "Ei\u00b7sen", "r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Henkerblut, Franzosenblut \u2013", "tokens": ["Mit", "Hen\u00b7ker\u00b7blut", ",", "Fran\u00b7zo\u00b7sen\u00b7blut", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O s\u00fc\u00dfer Tag der Rache!", "tokens": ["O", "s\u00fc\u00b7\u00dfer", "Tag", "der", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das klinget allen Deutschen gut,", "tokens": ["Das", "klin\u00b7get", "al\u00b7len", "Deut\u00b7schen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ist die gro\u00dfe Sache.", "tokens": ["Das", "ist", "die", "gro\u00b7\u00dfe", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00dft wehen, was nur wehen kann,", "tokens": ["La\u00dft", "we\u00b7hen", ",", "was", "nur", "we\u00b7hen", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Standarten wehn und Fahnen!", "tokens": ["Stand\u00b7ar\u00b7ten", "wehn", "und", "Fah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen heut uns Mann f\u00fcr Mann", "tokens": ["Wir", "wol\u00b7len", "heut", "uns", "Mann", "f\u00fcr", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Heldentode mahnen:", "tokens": ["Zum", "Hel\u00b7den\u00b7to\u00b7de", "mah\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf! Fliege, stolzes Siegspanier", "tokens": ["Auf", "!", "Flie\u00b7ge", ",", "stol\u00b7zes", "Siegs\u00b7pa\u00b7nier"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "$.", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Voran dem k\u00fchnen Reihen!", "tokens": ["Vo\u00b7ran", "dem", "k\u00fch\u00b7nen", "Rei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir siegen oder sterben hier", "tokens": ["Wir", "sie\u00b7gen", "o\u00b7der", "ster\u00b7ben", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den s\u00fc\u00dfen Tod der Freien.", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Tod", "der", "Frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Gott, der Eisen wachsen lie\u00df,", "tokens": ["Der", "Gott", ",", "der", "Ei\u00b7sen", "wach\u00b7sen", "lie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wollte keine Knechte,", "tokens": ["Der", "woll\u00b7te", "kei\u00b7ne", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum gab er S\u00e4bel, Schwert und Spie\u00df", "tokens": ["Drum", "gab", "er", "S\u00e4\u00b7bel", ",", "Schwert", "und", "Spie\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Mann in seine Rechte,", "tokens": ["Dem", "Mann", "in", "sei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "