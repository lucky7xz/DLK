{"dta.poem.2143": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unterscheid zwischen einem Tr\u00e4umen-  \n den und Wachenden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es fragt sich, da wir schlafend tr\u00e4umen, ob wir auch,", "tokens": ["Es", "fragt", "sich", ",", "da", "wir", "schla\u00b7fend", "tr\u00e4u\u00b7men", ",", "ob", "wir", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wenn wir wachen, nicht", "tokens": ["wenn", "wir", "wa\u00b7chen", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "VVINF", "$,", "PTKNEG"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf gleiche Weise tr\u00e4umen w\u00fcrden, durch der Jdeen", "tokens": ["Auf", "glei\u00b7che", "Wei\u00b7se", "tr\u00e4u\u00b7men", "w\u00fcr\u00b7den", ",", "durch", "der", "Jdeen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VAFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "gro\u00dfes Heer,", "tokens": ["gro\u00b7\u00dfes", "Heer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und durch die ungez\u00e4hlte Vorw\u00fcrf, (worinn wir schwim-", "tokens": ["Und", "durch", "die", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Vor\u00b7w\u00fcrf", ",", "(", "wo\u00b7rinn", "wir", "schwim"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "$(", "PWAV", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "men wie im Meer)", "tokens": ["men", "wie", "im", "Meer", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sowohl als in der Nacht verwirrt? Da\u00df aber dieses", "tokens": ["So\u00b7wohl", "als", "in", "der", "Nacht", "ver\u00b7wirrt", "?", "Da\u00df", "a\u00b7ber", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ADJD", "$.", "KOUS", "ADV", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "nicht geschicht,", "tokens": ["nicht", "ge\u00b7schicht", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Scheint blo\u00df hiedurch allein behindert, da unsern Seelen", "tokens": ["Scheint", "blo\u00df", "hie\u00b7durch", "al\u00b7lein", "be\u00b7hin\u00b7dert", ",", "da", "un\u00b7sern", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PAV", "ADV", "VVPP", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "eine Kraft,", "tokens": ["ei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Das, was unordentlich, zu ordnen, ist zugetheilt, ein\u2019", "tokens": ["Das", ",", "was", "un\u00b7or\u00b7dent\u00b7lich", ",", "zu", "ord\u00b7nen", ",", "ist", "zu\u00b7ge\u00b7theilt", ",", "ein'"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "$,", "PRELS", "ADJD", "$,", "PTKZU", "VVINF", "$,", "VAFIN", "VVPP", "$,", "ART"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Eigenschaft,", "tokens": ["Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Das, was zu viel ist, zu entfernen, durch diese Richter-", "tokens": ["Das", ",", "was", "zu", "viel", "ist", ",", "zu", "ent\u00b7fer\u00b7nen", ",", "durch", "die\u00b7se", "Rich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "PIS", "VAFIN", "$,", "PTKZU", "VVINF", "$,", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "kraft allein", "tokens": ["kraft", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["ADJD", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Kann alles, was wir denken, recht und ordentlich gewir-", "tokens": ["Kann", "al\u00b7les", ",", "was", "wir", "den\u00b7ken", ",", "recht", "und", "or\u00b7dent\u00b7lich", "ge\u00b7wir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,", "ADJD", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "ket seyn.", "tokens": ["ket", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAINF", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.17": {"text": "Wir scheinen denn, so lange wir in dieser Welt sind, so", "tokens": ["Wir", "schei\u00b7nen", "denn", ",", "so", "lan\u00b7ge", "wir", "in", "die\u00b7ser", "Welt", "sind", ",", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "PPER", "APPR", "PDAT", "NN", "VAFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "geschaffen,", "tokens": ["ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Da\u00df, wenn wir in der Nuhe liegen, auch diese Richter-", "tokens": ["Da\u00df", ",", "wenn", "wir", "in", "der", "Nu\u00b7he", "lie\u00b7gen", ",", "auch", "die\u00b7se", "Rich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "PDAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "kr\u00e4fte schlafen.", "tokens": ["kr\u00e4f\u00b7te", "schla\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}