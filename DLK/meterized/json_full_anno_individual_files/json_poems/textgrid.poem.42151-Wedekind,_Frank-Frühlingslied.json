{"textgrid.poem.42151": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rtest du die Sonne frohlocken,", "tokens": ["H\u00f6r\u00b7test", "du", "die", "Son\u00b7ne", "froh\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Als du aus dem Fenster geschaut:", "tokens": ["Als", "du", "aus", "dem", "Fens\u00b7ter", "ge\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wald und Feld und Wiesen sind trocken,", "tokens": ["Wald", "und", "Feld", "und", "Wie\u00b7sen", "sind", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Warten auf Br\u00e4utigam und Braut.", "tokens": ["War\u00b7ten", "auf", "Br\u00e4u\u00b7ti\u00b7gam", "und", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Zieh dein wei\u00dfes Hemd an, mein Sch\u00e4tzchen,", "tokens": ["Zieh", "dein", "wei\u00b7\u00dfes", "Hemd", "an", ",", "mein", "Sch\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Das du keusch im Kasten verwahrt,", "tokens": ["Das", "du", "keusch", "im", "Kas\u00b7ten", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Komm hinaus zum lauschigen Pl\u00e4tzchen,", "tokens": ["Komm", "hin\u00b7aus", "zum", "lau\u00b7schi\u00b7gen", "Pl\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wo sich die V\u00f6glein l\u00e4ngst gepaart!", "tokens": ["Wo", "sich", "die", "V\u00f6\u00b7glein", "l\u00e4ngst", "ge\u00b7paart", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, das ist ja der holde Mai!", "tokens": ["Ja", ",", "das", "ist", "ja", "der", "hol\u00b7de", "Mai", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft uns wandeln zu zwei und zwei", "tokens": ["La\u00dft", "uns", "wan\u00b7deln", "zu", "zwei", "und", "zwei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "CARD", "KON", "CARD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Durch den Hochwald auf blumigen Pfaden!", "tokens": ["Durch", "den", "Hoch\u00b7wald", "auf", "blu\u00b7mi\u00b7gen", "Pfa\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wo das Auge des Himmels lacht,", "tokens": ["Wo", "das", "Au\u00b7ge", "des", "Him\u00b7mels", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "K\u00fcssen wir, da\u00df es man so kracht", "tokens": ["K\u00fcs\u00b7sen", "wir", ",", "da\u00df", "es", "man", "so", "kracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PIS", "ADV", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Vom Genick hinab in die Waden!", "tokens": ["Vom", "Ge\u00b7nick", "hin\u00b7ab", "in", "die", "Wa\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Horch, wie ohne Geld in den Zweigen", "tokens": ["Horch", ",", "wie", "oh\u00b7ne", "Geld", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sieben Treppen hoch unterm Dach", "tokens": ["Sie\u00b7ben", "Trep\u00b7pen", "hoch", "un\u00b7term", "Dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "All die K\u00fcnstler zwitschern und geigen!", "tokens": ["All", "die", "K\u00fcnst\u00b7ler", "zwit\u00b7schern", "und", "gei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "KON", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Keiner z\u00e4hlt seine Barschaft nach.", "tokens": ["Kei\u00b7ner", "z\u00e4hlt", "sei\u00b7ne", "Bar\u00b7schaft", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Und die Bl\u00fcmelein auf den Auen,", "tokens": ["Und", "die", "Bl\u00fc\u00b7me\u00b7lein", "auf", "den", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dran dein Auge sich innig erquickt,", "tokens": ["Dran", "dein", "Au\u00b7ge", "sich", "in\u00b7nig", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Brauchen sie auf den Pfennig zu schauen,", "tokens": ["Brau\u00b7chen", "sie", "auf", "den", "Pfen\u00b7nig", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Wo sie umsonst ihr Schneider schm\u00fcckt!", "tokens": ["Wo", "sie", "um\u00b7sonst", "ihr", "Schnei\u00b7der", "schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, das ist ja ... usw.", "tokens": ["Ja", ",", "das", "ist", "ja", "...", "us\u00b7w."], "token_info": ["word", "punct", "word", "word", "word", "punct", "abbreviation"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ADV", "$(", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ach wie billig ist doch das Leben,", "tokens": ["Ach", "wie", "bil\u00b7lig", "ist", "doch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wenn man keine Ausgaben hat.", "tokens": ["Wenn", "man", "kei\u00b7ne", "Aus\u00b7ga\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "H\u00f6chste Wonne liegt gleich daneben,", "tokens": ["H\u00f6chs\u00b7te", "Won\u00b7ne", "liegt", "gleich", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "PAV", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Bringst du sie mir doch mit aus der Stadt.", "tokens": ["Bringst", "du", "sie", "mir", "doch", "mit", "aus", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPER", "ADV", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sicherlich l\u00e4\u00dft du mich nicht darben,", "tokens": ["Si\u00b7cher\u00b7lich", "l\u00e4\u00dft", "du", "mich", "nicht", "dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "PAV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wenn auf meine Kosten du f\u00e4hrst.", "tokens": ["Wenn", "auf", "mei\u00b7ne", "Kos\u00b7ten", "du", "f\u00e4hrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Tr\u00e4gt der Wald doch der Hoffnung Farben,", "tokens": ["Tr\u00e4gt", "der", "Wald", "doch", "der", "Hoff\u00b7nung", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Hoff ich, da\u00df du mir drin geh\u00f6rst.", "tokens": ["Hoff", "ich", ",", "da\u00df", "du", "mir", "drin", "ge\u00b7h\u00f6rst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, das ist ja ... usw.", "tokens": ["Ja", ",", "das", "ist", "ja", "...", "us\u00b7w."], "token_info": ["word", "punct", "word", "word", "word", "punct", "abbreviation"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ADV", "$(", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Alsdann wolln zu Mittag wir speisen,", "tokens": ["Als\u00b7dann", "wolln", "zu", "Mit\u00b7tag", "wir", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wo des Rasens Tischtuch uns winkt.", "tokens": ["Wo", "des", "Ra\u00b7sens", "Tischtuch", "uns", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsere Mahlzeit soll euch beweisen,", "tokens": ["Un\u00b7se\u00b7re", "Mahl\u00b7zeit", "soll", "euch", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Wie man als K\u00fcnstler i\u00dft und trinkt.", "tokens": ["Wie", "man", "als", "K\u00fcnst\u00b7ler", "i\u00dft", "und", "trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KOKOM", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Deine Locken sind das Gem\u00fcse,", "tokens": ["Dei\u00b7ne", "Lo\u00b7cken", "sind", "das", "Ge\u00b7m\u00fc\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Deine Lippen spenden das Bier.", "tokens": ["Dei\u00b7ne", "Lip\u00b7pen", "spen\u00b7den", "das", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Von den Schultern bis auf die F\u00fc\u00dfe,", "tokens": ["Von", "den", "Schul\u00b7tern", "bis", "auf", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Alles ist G\u00e4nsebraten mir.", "tokens": ["Al\u00b7les", "ist", "G\u00e4n\u00b7se\u00b7bra\u00b7ten", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Ja, das ist ja ... usw.", "tokens": ["Ja", ",", "das", "ist", "ja", "...", "us\u00b7w."], "token_info": ["word", "punct", "word", "word", "word", "punct", "abbreviation"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ADV", "$(", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Gibst du mir den Laufpa\u00df, o Schrecken,", "tokens": ["Gibst", "du", "mir", "den", "Lauf\u00b7pa\u00df", ",", "o", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Kalt und ohne Herz wie Granit,", "tokens": ["Kalt", "und", "oh\u00b7ne", "Herz", "wie", "Gra\u00b7nit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "KOKOM", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Finger sollst du dir lecken,", "tokens": ["Dei\u00b7ne", "Fin\u00b7ger", "sollst", "du", "dir", "le\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ich singe einfach nicht mehr mit.", "tokens": ["Ich", "sin\u00b7ge", "ein\u00b7fach", "nicht", "mehr", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ein M\u00fchlstein werd ich verenden,", "tokens": ["Wie", "ein", "M\u00fchl\u00b7stein", "werd", "ich", "ver\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den man um den Hals dir geh\u00e4ngt,", "tokens": ["Den", "man", "um", "den", "Hals", "dir", "ge\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Denn das hei\u00dft sein Leben verschwenden,", "tokens": ["Denn", "das", "hei\u00dft", "sein", "Le\u00b7ben", "ver\u00b7schwen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wenn sich der Mensch allein ertr\u00e4nkt.", "tokens": ["Wenn", "sich", "der", "Mensch", "al\u00b7lein", "er\u00b7tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ja, das ist ja ... usw.", "tokens": ["Ja", ",", "das", "ist", "ja", "...", "us\u00b7w."], "token_info": ["word", "punct", "word", "word", "word", "punct", "abbreviation"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ADV", "$(", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "H\u00f6rtest du die Sonne frohlocken,", "tokens": ["H\u00f6r\u00b7test", "du", "die", "Son\u00b7ne", "froh\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Als du aus dem Fenster geschaut:", "tokens": ["Als", "du", "aus", "dem", "Fens\u00b7ter", "ge\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wald und Feld und Wiesen sind trocken,", "tokens": ["Wald", "und", "Feld", "und", "Wie\u00b7sen", "sind", "tro\u00b7cken", ","], "token_info