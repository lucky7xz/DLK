{"dta.poem.13139": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er zeigte mit der Hand, und wo er hin gedeutet,", "tokens": ["Er", "zeig\u00b7te", "mit", "der", "Hand", ",", "und", "wo", "er", "hin", "ge\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat ihm fast jedes Aug im Krei\u00df herum begleitet.", "tokens": ["Hat", "ihm", "fast", "je\u00b7des", "Aug", "im", "Krei\u00df", "he\u00b7rum", "be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "APPRART", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "105Er sah nach der Matron mit jenem Sonnen Schild,", "tokens": ["sah", "nach", "der", "Mat\u00b7ron", "mit", "je\u00b7nem", "Son\u00b7nen", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die mich beym ersten Blick der Stadt mit Lust erf\u00fcllt.", "tokens": ["Die", "mich", "beym", "ers\u00b7ten", "Blick", "der", "Stadt", "mit", "Lust", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er sagte: \u201eTritt hervor das Streiten abzustellen;", "tokens": ["Er", "sag\u00b7te", ":", "\u201e", "Tritt", "her\u00b7vor", "das", "Strei\u00b7ten", "ab\u00b7zu\u00b7stel\u00b7len", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PTKVZ", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edu, ", "tokens": ["\u201e", "du", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "\u201estimmt ihr nicht insgesamt mit diesem Vorschlag ein,", "tokens": ["\u201e", "stimmt", "ihr", "nicht", "ins\u00b7ge\u00b7samt", "mit", "die\u00b7sem", "Vor\u00b7schlag", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "110\u201dDa\u00df, was die Wahrheit schlie\u00dft, beschlossen solle seyn?", "tokens": ["\"", "Da\u00df", ",", "was", "die", "Wahr\u00b7heit", "schlie\u00dft", ",", "be\u00b7schlos\u00b7sen", "sol\u00b7le", "seyn", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVINF", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}