{"textgrid.poem.25771": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rt ihr einst, ich sey gestorben,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt ihr einst, ich sey gestorben,", "tokens": ["H\u00f6rt", "ihr", "einst", ",", "ich", "sey", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O! dann spielet auf Theorben", "tokens": ["O", "!", "dann", "spie\u00b7let", "auf", "The\u00b7or\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Trauermelodie.", "tokens": ["Kei\u00b7ne", "Trau\u00b7er\u00b7me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich, der euch im Leben nie", "tokens": ["Ich", ",", "der", "euch", "im", "Le\u00b7ben", "nie"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Eure Freude hat verdorben:", "tokens": ["Eu\u00b7re", "Freu\u00b7de", "hat", "ver\u00b7dor\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich verd\u00fcrb' im Sterben sie?", "tokens": ["Ich", "ver\u00b7d\u00fcrb'", "im", "Ster\u00b7ben", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein! Ihr sollt um mich nicht klagen!", "tokens": ["Nein", "!", "Ihr", "sollt", "um", "mich", "nicht", "kla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Freuen m\u00f6gt ihr euch, und sagen:", "tokens": ["Freu\u00b7en", "m\u00f6gt", "ihr", "euch", ",", "und", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbwohl! Geborgen ist auch der!", "tokens": ["\u00bb", "wohl", "!", "Ge\u00b7bor\u00b7gen", "ist", "auch", "der", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "NN", "VAFIN", "ADV", "ART", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Und wer gab die Hand wie er", "tokens": ["Und", "wer", "gab", "die", "Hand", "wie", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "ART", "NN", "KOKOM", "PPER"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Dem Befreier ohne Zagen?", "tokens": ["Dem", "Be\u00b7frei\u00b7er", "oh\u00b7ne", "Za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Darum klatschet hinterher!\u00ab", "tokens": ["Da\u00b7rum", "klat\u00b7schet", "hin\u00b7ter\u00b7her", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Dann so holt aus meinem Keller", "tokens": ["Dann", "so", "holt", "aus", "mei\u00b7nem", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die paar Flaschen Muskateller,", "tokens": ["Die", "paar", "Fla\u00b7schen", "Mus\u00b7ka\u00b7tel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$,"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.15": {"text": "Aufgespart f\u00fcr euch, heraus,", "tokens": ["Auf\u00b7ge\u00b7spart", "f\u00fcr", "euch", ",", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Trinkt, als w\u00e4r' ich noch zu Haus,", "tokens": ["Trinkt", ",", "als", "w\u00e4r'", "ich", "noch", "zu", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Sie auf meinem Lieblingss\u00f6ller", "tokens": ["Sie", "auf", "mei\u00b7nem", "Lieb\u00b7lings\u00b7s\u00f6l\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Mit einander fr\u00f6hlich aus.", "tokens": ["Mit", "ein\u00b7an\u00b7der", "fr\u00f6h\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Und erinnert euch der Zeiten,", "tokens": ["Und", "e\u00b7rin\u00b7nert", "euch", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Wo beim Klange s\u00fc\u00dfer Saiten", "tokens": ["Wo", "beim", "Klan\u00b7ge", "s\u00fc\u00b7\u00dfer", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Meine Laun', im Vogelflug',", "tokens": ["Mei\u00b7ne", "Laun'", ",", "im", "Vo\u00b7gel\u00b7flug'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Mich von Scherz zu Scherze trug.", "tokens": ["Mich", "von", "Scherz", "zu", "Scher\u00b7ze", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Stunden, die mich nicht gereuten,", "tokens": ["Stun\u00b7den", ",", "die", "mich", "nicht", "ge\u00b7reu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Als der Tod nun sprach: Genug!", "tokens": ["Als", "der", "Tod", "nun", "sprach", ":", "Ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$.", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "La\u00dft von mir die Leute sprechen,", "tokens": ["La\u00dft", "von", "mir", "die", "Leu\u00b7te", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.26": {"text": "Was sie wollen; von Verbrechen", "tokens": ["Was", "sie", "wol\u00b7len", ";", "von", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VMFIN", "$.", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "War mein ganzes Leben rein,", "tokens": ["War", "mein", "gan\u00b7zes", "Le\u00b7ben", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Und so darf man Mensch nur seyn,", "tokens": ["Und", "so", "darf", "man", "Mensch", "nur", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "NN", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Um auch mir so manche Schw\u00e4chen", "tokens": ["Um", "auch", "mir", "so", "man\u00b7che", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nach dem Tode zu verzeihn.", "tokens": ["Nach", "dem", "To\u00b7de", "zu", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Meine Lieder tadeln h\u00f6ren,", "tokens": ["Mei\u00b7ne", "Lie\u00b7der", "ta\u00b7deln", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Soll nicht eure Ruhe st\u00f6ren.", "tokens": ["Soll", "nicht", "eu\u00b7re", "Ru\u0