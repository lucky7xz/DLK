: ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Stunden, die mich nicht gereuten,", "tokens": ["Stun\u00b7den", ",", "die", "mich", "nicht", "ge\u00b7reu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Als der Tod nun sprach: Genug!", "tokens": ["Als", "der", "Tod", "nun", "sprach", ":", "Ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$.", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "La\u00dft von mir die Leute sprechen,", "tokens": ["La\u00dft", "von", "mir", "die", "Leu\u00b7te", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.26": {"text": "Was sie wollen; von Verbrechen", "tokens": ["Was", "sie", "wol\u00b7len", ";", "von", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VMFIN", "$.", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "War mein ganzes Leben rein,", "tokens": ["War", "mein", "gan\u00b7zes", "Le\u00b7ben", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Und so darf man Mensch nur seyn,", "tokens": ["Und", "so", "darf", "man", "Mensch", "nur", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "NN", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Um auch mir so manche Schw\u00e4chen", "tokens": ["Um", "auch", "mir", "so", "man\u00b7che", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nach dem Tode zu verzeihn.", "tokens": ["Nach", "dem", "To\u00b7de", "zu", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Meine Lieder tadeln h\u00f6ren,", "tokens": ["Mei\u00b7ne", "Lie\u00b7der", "ta\u00b7deln", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Soll nicht eure Ruhe st\u00f6ren.", "tokens": ["Soll", "nicht", "eu\u00b7re", "Ru\u00b7he", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Nie verschwand noch ein Gesang,", "tokens": ["Nie", "ver\u00b7schwand", "noch", "ein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Den ein reiner Geist durchdrang.", "tokens": ["Den", "ein", "rei\u00b7ner", "Geist", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Nichts, wenn meine den entbehren,", "tokens": ["Nichts", ",", "wenn", "mei\u00b7ne", "den", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PPOSAT", "ART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Rettet sie vom Untergang'.", "tokens": ["Ret\u00b7tet", "sie", "vom", "Un\u00b7ter\u00b7gang'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}