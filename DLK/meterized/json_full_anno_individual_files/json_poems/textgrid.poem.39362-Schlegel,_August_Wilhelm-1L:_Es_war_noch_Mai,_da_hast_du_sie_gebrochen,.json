{"textgrid.poem.39362": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war noch Mai, da hast du sie gebrochen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war noch Mai, da hast du sie gebrochen,", "tokens": ["Es", "war", "noch", "Mai", ",", "da", "hast", "du", "sie", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Blumen ausgesprochen,", "tokens": ["In", "Blu\u00b7men", "aus\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Selber Bl\u00fcthe,", "tokens": ["Sel\u00b7ber", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Was bl\u00fchend im Gem\u00fcthe", "tokens": ["Was", "bl\u00fc\u00b7hend", "im", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon sich regte", "tokens": ["Schon", "sich", "reg\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und heilig sich bewegte,", "tokens": ["Und", "hei\u00b7lig", "sich", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was kindlich, ach! der Freund so gerne hegte,", "tokens": ["Was", "kind\u00b7lich", ",", "ach", "!", "der", "Freund", "so", "ger\u00b7ne", "heg\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "ITJ", "$.", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn sie ihr Herzchen legte", "tokens": ["Wenn", "sie", "ihr", "Herz\u00b7chen", "leg\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "An das seine,", "tokens": ["An", "das", "sei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Wo ich nun ewig weine.", "tokens": ["Wo", "ich", "nun", "e\u00b7wig", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Veilchen sandte mir das Kind zum Zeichen,", "tokens": ["Die", "Veil\u00b7chen", "sand\u00b7te", "mir", "das", "Kind", "zum", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die so mein Herz erweichen,", "tokens": ["Die", "so", "mein", "Herz", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df die Augen", "tokens": ["Da\u00df", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Den Schmerz, den sie nun saugen,", "tokens": ["Den", "Schmerz", ",", "den", "sie", "nun", "sau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nie vollenden,", "tokens": ["Nie", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sich oft noch zu ihr wenden,", "tokens": ["Sich", "oft", "noch", "zu", "ihr", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und finden welk den Kranz dann in den H\u00e4nden,", "tokens": ["Und", "fin\u00b7den", "welk", "den", "Kranz", "dann", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie der, hat sie, zu enden", "tokens": ["Wie", "der", ",", "hat", "sie", ",", "zu", "en\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "$,", "VAFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Fr\u00fch erkoren,", "tokens": ["Fr\u00fch", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Sich unbewu\u00dft verloren.", "tokens": ["Sich", "un\u00b7be\u00b7wu\u00dft", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nimm hin die hohe, k\u00f6stlich liebe Gabe,", "tokens": ["Nimm", "hin", "die", "ho\u00b7he", ",", "k\u00f6st\u00b7lich", "lie\u00b7be", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Einz'ge, was ich habe", "tokens": ["Das", "Einz'\u00b7ge", ",", "was", "ich", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von der Theuern,", "tokens": ["Von", "der", "Theu\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Ihr Bild mir zu erneuern,", "tokens": ["Ihr", "Bild", "mir", "zu", "er\u00b7neu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn in Thr\u00e4nen", "tokens": ["Wenn", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Dem Tode zu das Sehnen", "tokens": ["Dem", "To\u00b7de", "zu", "das", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So gern entflieht des Daseins eitlem W\u00e4hnen.", "tokens": ["So", "gern", "ent\u00b7flieht", "des", "Da\u00b7seins", "eit\u00b7lem", "W\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch erst la\u00df mich in Thr\u00e4nen", "tokens": ["Doch", "erst", "la\u00df", "mich", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVIMP", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ganz versenken", "tokens": ["Ganz", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Das s\u00fc\u00dfe Angedenken!", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "An\u00b7ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Uns, die in Lust des Todes Leben fanden,", "tokens": ["Uns", ",", "die", "in", "Lust", "des", "To\u00b7des", "Le\u00b7ben", "fan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00fchn die Natur verstanden", "tokens": ["K\u00fchn", "die", "Na\u00b7tur", "ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "In den Flammen,", "tokens": ["In", "den", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wo Lieb' und Schmerz zusammen", "tokens": ["Wo", "Lieb'", "und", "Schmerz", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uns verbunden,", "tokens": ["Uns", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Uns sei die Stirn umwunden", "tokens": ["Uns", "sei", "die", "Stirn", "um\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vom Zeichen, dessen Sinn wir l\u00e4ngst gefunden.", "tokens": ["Vom", "Zei\u00b7chen", ",", "des\u00b7sen", "Sinn", "wir", "l\u00e4ngst", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn bl\u00fchten aus den Wunden", "tokens": ["Denn", "bl\u00fch\u00b7ten", "aus", "den", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Oft nicht Rosen,", "tokens": ["Oft", "nicht", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Uns schmerzlich liebzukosen?", "tokens": ["Uns", "schmerz\u00b7lich", "lieb\u00b7zu\u00b7ko\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df denn des M\u00e4dchens Schatten uns umschweben,", "tokens": ["La\u00df", "denn", "des", "M\u00e4d\u00b7chens", "Schat\u00b7ten", "uns", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wehmuth hingegeben,", "tokens": ["Der", "Weh\u00b7muth", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis wir im Tode Eins noch inn'ger leben,", "tokens": ["Bis", "wir", "im", "To\u00b7de", "Eins", "noch", "inn'\u00b7ger", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dann die\u00df tiefe Streben", "tokens": ["Und", "dann", "die\u00df", "tie\u00b7fe", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ganz vereinet,", "tokens": ["Ganz", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Das l\u00e4chelnd sich beweinet.", "tokens": ["Das", "l\u00e4\u00b7chelnd", "sich", "be\u00b7wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es war noch Mai, da hast du sie gebrochen,", "tokens": ["Es", "war", "noch", "Mai", ",", "da", "hast", "du", "sie", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Blumen ausgesprochen,", "tokens": ["In", "Blu\u00b7men", "aus\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "w