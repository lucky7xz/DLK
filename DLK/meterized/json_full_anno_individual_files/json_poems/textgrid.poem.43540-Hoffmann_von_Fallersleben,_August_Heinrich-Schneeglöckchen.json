{"textgrid.poem.43540": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schneegl\u00f6ckchen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schneegl\u00f6ckchen, ei, du bist schon da?", "tokens": ["Schnee\u00b7gl\u00f6ck\u00b7chen", ",", "ei", ",", "du", "bist", "schon", "da", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist denn der Fr\u00fchling schon so nah?", "tokens": ["Ist", "denn", "der", "Fr\u00fch\u00b7ling", "schon", "so", "nah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer lockte dich hervor ans Licht?", "tokens": ["Wer", "lock\u00b7te", "dich", "her\u00b7vor", "ans", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trau' doch dem Sonnenscheine nicht!", "tokens": ["Trau'", "doch", "dem", "Son\u00b7nen\u00b7schei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl gut er's eben heute meint,", "tokens": ["Wohl", "gut", "er's", "e\u00b7ben", "heu\u00b7te", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "lin