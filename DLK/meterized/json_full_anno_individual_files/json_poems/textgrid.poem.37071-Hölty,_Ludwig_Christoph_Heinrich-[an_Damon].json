os": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Na\u00dfgeweinte Veilchen, will mich ihres", "tokens": ["Na\u00df\u00b7ge\u00b7wein\u00b7te", "Veil\u00b7chen", ",", "will", "mich", "ih\u00b7res"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PRF", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Seelenl\u00e4chelns erinnern, und dann sterben.", "tokens": ["See\u00b7len\u00b7l\u00e4\u00b7chelns", "e\u00b7rin\u00b7nern", ",", "und", "dann", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}