{"dta.poem.21109": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Eusebia .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wem, o Freundin, als dir, die du den einsa-               ", "tokens": ["Wem", ",", "o", "Freun\u00b7din", ",", "als", "dir", ",", "die", "du", "den", "ein\u00b7sa"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "$,", "FM", "NN", "$,", "KOUS", "PPER", "$,", "PRELS", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "men Waller", "tokens": ["men", "Wal\u00b7ler"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Auf der Reis' in das Grab leitest mit freund-", "tokens": ["Auf", "der", "Reis'", "in", "das", "Grab", "lei\u00b7test", "mit", "freun\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "TRUNC"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "licher Hand,", "tokens": ["li\u00b7cher", "Hand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "In den einsamen Weg manch duftendes Bl\u00fcmchen", "tokens": ["In", "den", "ein\u00b7sa\u00b7men", "Weg", "manch", "duf\u00b7ten\u00b7des", "Bl\u00fcm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "ihm streutest,", "tokens": ["ihm", "streu\u00b7test", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Aus dem Antlitz ihm bogst manchen ver-", "tokens": ["Aus", "dem", "Ant\u00b7litz", "ihm", "bogst", "man\u00b7chen", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "PIAT", "TRUNC"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "wundenden Dorn;", "tokens": ["wun\u00b7den\u00b7den", "Dorn", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Die du, sorgsam den Frieden der eigenen Seele", "tokens": ["Die", "du", ",", "sorg\u00b7sam", "den", "Frie\u00b7den", "der", "ei\u00b7ge\u00b7nen", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "bewahrend,", "tokens": ["be\u00b7wah\u00b7rend", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "In des Gef\u00e4hrten Brust \u00f6fter die St\u00fcrme", "tokens": ["In", "des", "Ge\u00b7f\u00e4hr\u00b7ten", "Brust", "\u00f6f\u00b7ter", "die", "St\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "ART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "beschworst,", "tokens": ["be\u00b7schworst", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Jede Schickung ertrugst mit \u00fcberwindender Liebe,", "tokens": ["Je\u00b7de", "Schi\u00b7ckung", "er\u00b7trugst", "mit", "\u00fc\u00b7berw\u00b7in\u00b7den\u00b7der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Immer wahr und treu jegliche Pr\u00fcfung be-", "tokens": ["Im\u00b7mer", "wahr", "und", "treu", "jeg\u00b7li\u00b7che", "Pr\u00fc\u00b7fung", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PIAT", "NN", "TRUNC"], "meter": "+-+-+---+--", "measure": "unknown.measure.tetra"}, "line.15": {"text": "standst;", "tokens": ["standst", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Die du uns, Beste, umschlangst mit dieser Blu-", "tokens": ["Die", "du", "uns", ",", "Bes\u00b7te", ",", "um\u00b7schlangst", "mit", "die\u00b7ser", "Blu"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "$,", "NN", "$,", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "menguirlande", "tokens": ["men\u00b7gu\u00b7ir\u00b7lan\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Knospender Menschen, sie pflegt liebend", "tokens": ["Knos\u00b7pen\u00b7der", "Men\u00b7schen", ",", "sie", "pflegt", "lie\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "der Strahl und der Thau.", "tokens": ["der", "Strahl", "und", "der", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Horch, wie jauchzen die Frohen! wie st\u00fcrmen die", "tokens": ["Horch", ",", "wie", "jauch\u00b7zen", "die", "Fro\u00b7hen", "!", "wie", "st\u00fcr\u00b7men", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ART", "NN", "$.", "PWAV", "VVFIN", "ART"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Freudigen, siehe!", "tokens": ["Freu\u00b7di\u00b7gen", ",", "sie\u00b7he", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Diese mit Anmuth geschm\u00fcckt, jene ger\u00fcstet", "tokens": ["Die\u00b7se", "mit", "An\u00b7muth", "ge\u00b7schm\u00fcckt", ",", "je\u00b7ne", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "APPR", "NN", "VVPP", "$,", "PDS", "VVPP"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "mit Kraft!", "tokens": ["mit", "Kraft", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Zwischen den Lilien spielt, selbst Lilie, unsre", "tokens": ["Zwi\u00b7schen", "den", "Li\u00b7li\u00b7en", "spielt", ",", "selbst", "Li\u00b7lie", ",", "uns\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ADV", "NE", "$,", "PPOSAT"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Allwine,               ", "tokens": ["A\u00b7llwi\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Hinter dem Schmetterling jagt ", "tokens": ["Hin\u00b7ter", "dem", "Schmet\u00b7ter\u00b7ling", "jagt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.12": {"text": "der Schmetterer, her;", "tokens": ["der", "Schmet\u00b7te\u00b7rer", ",", "her", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "W\u00e4hrend am Saume der kosenden Mutter sich ", "tokens": ["W\u00e4h\u00b7rend", "am", "Sau\u00b7me", "der", "ko\u00b7sen\u00b7den", "Mut\u00b7ter", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.14": {"text": "anschmiegt,", "tokens": ["an\u00b7schmiegt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.15": {"text": "Und in dem Schooss, der ihn trug, l\u00e4chelnd", "tokens": ["Und", "in", "dem", "Scho\u00b7oss", ",", "der", "ihn", "trug", ",", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD"], "meter": "-+-++-+++-", "measure": "unknown.measure.hexa"}, "line.16": {"text": "Emilius ruht Diese bl&#252;hende Guirlande ist nun zerrissen. Unsre\nengelgleiche Julie entschlief drey Tage nach Abfassung dieser\nElegie; unser l&#228;chelnder Emilius drey Wochen sp&#228;ter. &#8212;               ", "tokens": ["E\u00b7mi\u00b7lius", "ruht", "Die\u00b7se", "bl", "&#252;", "hen\u00b7de", "Gu\u00b7ir\u00b7lan\u00b7de", "ist", "nun", "zer\u00b7ris\u00b7sen", ".", "Uns\u00b7re", "en\u00b7gel\u00b7glei\u00b7che", "Ju\u00b7lie", "ent\u00b7schlief", "drey", "Ta\u00b7ge", "nach", "Ab\u00b7fas\u00b7sung", "die\u00b7ser", "E\u00b7le\u00b7gie", ";", "un\u00b7ser", "l", "&#228;", "cheln\u00b7der", "E\u00b7mi\u00b7lius", "drey", "Wo\u00b7chen", "sp", "&#228;", "ter", ".", "&#8212;"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word", "punct", "XML_entity"], "pos": ["NE", "VVFIN", "PDAT", "NN", "$(", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$.", "PPOSAT", "ADJA", "NE", "VVFIN", "CARD", "NN", "APPR", "NN", "PDAT", "NN", "$.", "PPOSAT", "NN", "$(", "NE", "NE", "CARD", "NN", "FM", "FM", "FM", "$.", "$("], "meter": "+--+--+--+-+--+-+-+-+-+-+--+-+--+--+-+-+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Edele, Anspruchlose, Dem\u00fcthige, Friedliche, Stille,", "tokens": ["E\u00b7de\u00b7le", ",", "An\u00b7spruch\u00b7lo\u00b7se", ",", "De\u00b7m\u00fct\u00b7hi\u00b7ge", ",", "Fried\u00b7li\u00b7che", ",", "Stil\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PDS", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Wem, Geliebte, als dir, sollt' ich Eu-", "tokens": ["Wem", ",", "Ge\u00b7lieb\u00b7te", ",", "als", "dir", ",", "sollt'", "ich", "Eu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "$,", "NN", "$,", "KOUS", "PPER", "$,", "VMFIN", "PPER", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "sebien weihn!", "tokens": ["se\u00b7bi\u00b7en", "weihn", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hast du Eusebien wohl, die Tochter des Himmels,", "tokens": ["Hast", "du", "Eu\u00b7se\u00b7bi\u00b7en", "wohl", ",", "die", "Toch\u00b7ter", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "ADV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "gesehen?", "tokens": ["ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Zu den H\u00fctten des Grams stieg sie erbar-", "tokens": ["Zu", "den", "H\u00fct\u00b7ten", "des", "Grams", "stieg", "sie", "er\u00b7ba\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "TRUNC"], "meter": "--+--++-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "mend herab.", "tokens": ["mend", "her\u00b7ab", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "--+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Ihre Lippen umfliesst ein trostverk\u00fcndendes L\u00e4-", "tokens": ["Ih\u00b7re", "Lip\u00b7pen", "um\u00b7fliesst", "ein", "trost\u00b7ver\u00b7k\u00fcn\u00b7den\u00b7des", "L\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "cheln;", "tokens": ["cheln", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ihrer Augen Gewalt mildert der Mensch-", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "Ge\u00b7walt", "mil\u00b7dert", "der", "Men\u00b7sch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "lichkeit Thau.", "tokens": ["lich\u00b7keit", "Thau", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Freundlich leitet und sicher die H\u00f6he den tappenden", "tokens": ["Freund\u00b7lich", "lei\u00b7tet", "und", "si\u00b7cher", "die", "H\u00f6\u00b7he", "den", "tap\u00b7pen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wandrer", "tokens": ["Wand\u00b7rer"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Auf der Pflicht und des Rechts stickelen", "tokens": ["Auf", "der", "Pflicht", "und", "des", "Rechts", "sti\u00b7cke\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Pfaden hinan;", "tokens": ["Pfa\u00b7den", "hi\u00b7nan", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Leuchtet im Nebel des Meinens ihm vor mit der", "tokens": ["Leuch\u00b7tet", "im", "Ne\u00b7bel", "des", "Mei\u00b7nens", "ihm", "vor", "mit", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "APPR", "APPR", "ART"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Lampe des Glaubens;", "tokens": ["Lam\u00b7pe", "des", "Glau\u00b7bens", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Rettet auf schmalem Steg ihn durch die", "tokens": ["Ret\u00b7tet", "auf", "schma\u00b7lem", "Steg", "ihn", "durch", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "APPR", "ART"], "meter": "+--+-+-++", "measure": "iambic.penta.invert"}, "line.12": {"text": "S\u00fcmpfe des Wahns,", "tokens": ["S\u00fcmp\u00b7fe", "des", "Wahns", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Mahlt ihm in jegliche tr\u00e4ufelnde Wolke den Bogen", "tokens": ["Mahlt", "ihm", "in", "jeg\u00b7li\u00b7che", "tr\u00e4u\u00b7feln\u00b7de", "Wol\u00b7ke", "den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.14": {"text": "des Friedens,", "tokens": ["des", "Frie\u00b7dens", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "F\u00e4rbet mit Lasur und Gold jeden zerfliessen-", "tokens": ["F\u00e4r\u00b7bet", "mit", "La\u00b7sur", "und", "Gold", "je\u00b7den", "zer\u00b7flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PIAT", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "den Duft;", "tokens": ["den", "Duft", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Und wenn hinter die Gr\u00e4ber die Sonne des Lebens", "tokens": ["Und", "wenn", "hin\u00b7ter", "die", "Gr\u00e4\u00b7ber", "die", "Son\u00b7ne", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "hinabsinkt,", "tokens": ["hin\u00b7ab\u00b7sinkt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line