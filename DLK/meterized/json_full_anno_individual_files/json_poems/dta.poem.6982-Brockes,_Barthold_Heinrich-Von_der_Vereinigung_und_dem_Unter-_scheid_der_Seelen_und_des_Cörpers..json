{"dta.poem.6982": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sp\u00fcrt, da\u00df unser Trieb und Wollen streitig sey.", "tokens": ["Man", "sp\u00fcrt", ",", "da\u00df", "un\u00b7ser", "Trieb", "und", "Wol\u00b7len", "strei\u00b7tig", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Handlungen des Geists sind ohne Zwang und frey.", "tokens": ["Die", "Hand\u00b7lun\u00b7gen", "des", "Geists", "sind", "oh\u00b7ne", "Zwang", "und", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Leib nimmt andre Regeln an,", "tokens": ["Der", "Leib", "nimmt", "and\u00b7re", "Re\u00b7geln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird bewegt ohn Freyheit, sonder Wahl.", "tokens": ["Und", "wird", "be\u00b7wegt", "ohn", "Frey\u00b7heit", ",", "son\u00b7der", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVFIN", "APPR", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Seele, die der Sinn zu starck druckt, auf einmal,", "tokens": ["Die", "See\u00b7le", ",", "die", "der", "Sinn", "zu", "starck", "druckt", ",", "auf", "ein\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$,", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist in der Leidenschafft dem C\u00f6rper unterthan;", "tokens": ["Ist", "in", "der", "Lei\u00b7den\u00b7schafft", "dem", "C\u00f6r\u00b7per", "un\u00b7ter\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein, wenn sie recht will, ist sie von ihm entbunden", "tokens": ["Al\u00b7lein", ",", "wenn", "sie", "recht", "will", ",", "ist", "sie", "von", "ihm", "ent\u00b7bun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVINF"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und folgt dem edlen Trieb. Ja, von den Lastern rein,", "tokens": ["Und", "folgt", "dem", "ed\u00b7len", "Trieb", ".", "Ja", ",", "von", "den", "Las\u00b7tern", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$.", "PTKANT", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kan gar die Leidenschafft nicht str\u00e4fflich mehr befunden,", "tokens": ["Kan", "gar", "die", "Lei\u00b7den\u00b7schafft", "nicht", "str\u00e4ff\u00b7lich", "mehr", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Tugend selbst verkehrt, selbst eine Tugend seyn,", "tokens": ["In", "Tu\u00b7gend", "selbst", "ver\u00b7kehrt", ",", "selbst", "ei\u00b7ne", "Tu\u00b7gend", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da grosse Gegenw\u00fcrff\u2019 die wir offt vor uns finden,", "tokens": ["Da", "gros\u00b7se", "Ge\u00b7gen\u00b7w\u00fcrff'", "die", "wir", "offt", "vor", "uns", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Hertzen mit der reinsten Brunst entz\u00fcnden.", "tokens": ["Die", "Hert\u00b7zen", "mit", "der", "reins\u00b7ten", "Brunst", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Offt wird die Seele, wenn die Welt,", "tokens": ["Offt", "wird", "die", "See\u00b7le", ",", "wenn", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die wir vor unsern Augen haben,", "tokens": ["Die", "wir", "vor", "un\u00b7sern", "Au\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "In der Betrachtung ihr so wundervoll gef\u00e4llt,", "tokens": ["In", "der", "Be\u00b7trach\u00b7tung", "ihr", "so", "wun\u00b7der\u00b7voll", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zum SCHOEPFFER, der nicht sichtbar ist, erhaben:", "tokens": ["Zum", "ScHOE\u00b7PF\u00b7FER", ",", "der", "nicht", "sicht\u00b7bar", "ist", ",", "er\u00b7ha\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "VAFIN", "$,", "ADJD", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Sie setzet, ohn den Wust der Sinnen mehr zu achten,", "tokens": ["Sie", "set\u00b7zet", ",", "ohn", "den", "Wust", "der", "Sin\u00b7nen", "mehr", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die reine Lust darinn, die Wahrheit zu betrachten.", "tokens": ["Die", "rei\u00b7ne", "Lust", "da\u00b7rinn", ",", "die", "Wahr\u00b7heit", "zu", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}