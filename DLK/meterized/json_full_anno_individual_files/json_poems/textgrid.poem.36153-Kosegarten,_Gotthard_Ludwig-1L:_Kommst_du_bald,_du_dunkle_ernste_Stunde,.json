{"textgrid.poem.36153": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommst du bald, du dunkle ernste Stunde,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommst du bald, du dunkle ernste Stunde,", "tokens": ["Kommst", "du", "bald", ",", "du", "dunk\u00b7le", "erns\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die zu meinen V\u00e4tern mich hin\u00fcber ruft?", "tokens": ["Die", "zu", "mei\u00b7nen", "V\u00e4\u00b7tern", "mich", "hin\u00b7\u00fc\u00b7ber", "ruft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Hergesehnte, hergeweinte Stunde,", "tokens": ["Her\u00b7ge\u00b7sehn\u00b7te", ",", "her\u00b7ge\u00b7wein\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Braut des langen Schlummers, und der engen Gruft? \u2013", "tokens": ["Braut", "des", "lan\u00b7gen", "Schlum\u00b7mers", ",", "und", "der", "en\u00b7gen", "Gruft", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Kommst du einst zu mir, du grause Stunde,", "tokens": ["Kommst", "du", "einst", "zu", "mir", ",", "du", "grau\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trost des Jammervollen, und des Christen Lust;", "tokens": ["Trost", "des", "Jam\u00b7mer\u00b7vol\u00b7len", ",", "und", "des", "Chris\u00b7ten", "Lust", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Blutet mir die hei\u00dfe Todeswunde", "tokens": ["Blu\u00b7tet", "mir", "die", "hei\u00b7\u00dfe", "To\u00b7des\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einstens aus der fliegenden gepre\u00dften Brust \u2013", "tokens": ["Eins\u00b7tens", "aus", "der", "flie\u00b7gen\u00b7den", "ge\u00b7pre\u00df\u00b7ten", "Brust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "O wie wird sich dann der stolze J\u00fcngling schmiegen,", "tokens": ["O", "wie", "wird", "sich", "dann", "der", "stol\u00b7ze", "J\u00fcng\u00b7ling", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PRF", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Alles Erdenwesens m\u00fcd' und satt!", "tokens": ["Al\u00b7les", "Er\u00b7den\u00b7we\u00b7sens", "m\u00fcd'", "und", "satt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O wie ruhig wird der wilde Schw\u00e4rmer liegen,", "tokens": ["O", "wie", "ru\u00b7hig", "wird", "der", "wil\u00b7de", "Schw\u00e4r\u00b7mer", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie ein abeschlachtet Lamm so matt!", "tokens": ["Wie", "ein", "a\u00b7besc\u00b7hlach\u00b7tet", "Lamm", "so", "matt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVFIN", "NN", "ADV", "ADJD", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Seine Kraft wird sich zum Grabe neigen!", "tokens": ["Sei\u00b7ne", "Kraft", "wird", "sich", "zum", "Gra\u00b7be", "nei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00f6wengrimmig wird des M\u00f6rders Wuth", "tokens": ["L\u00f6\u00b7wen\u00b7grim\u00b7mig", "wird", "des", "M\u00f6r\u00b7ders", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sein Gebein zerbrechen, seinen Nacken beugen,", "tokens": ["Sein", "Ge\u00b7bein", "zer\u00b7bre\u00b7chen", ",", "sei\u00b7nen", "Na\u00b7cken", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Durstig saufen sein gegei\u00dfelt Blut!", "tokens": ["Durs\u00b7tig", "sau\u00b7fen", "sein", "ge\u00b7gei\u00b7\u00dfelt", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "VVPP", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Um sein letztes todumduftet Lager", "tokens": ["Um", "sein", "letz\u00b7tes", "to\u00b7dum\u00b7duf\u00b7tet", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Werden abgeh\u00e4rmt die Freunde stehn!", "tokens": ["Wer\u00b7den", "ab\u00b7ge\u00b7h\u00e4rmt", "die", "Freun\u00b7de", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Werden um ihn jammern: Bruder! Bruder!", "tokens": ["Wer\u00b7den", "um", "ihn", "jam\u00b7mern", ":", "Bru\u00b7der", "!", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVINF", "$.", "NN", "$.", "NN", "$."], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bald laut heulen, bald in stummen Harm vergehn!", "tokens": ["Bald", "laut", "heu\u00b7len", ",", "bald", "in", "stum\u00b7men", "Harm", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Leiser wird sein Odem werden!", "tokens": ["Lei\u00b7ser", "wird", "sein", "O\u00b7dem", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleicher sein entstellt Gesicht \u2013", "tokens": ["Blei\u00b7cher", "sein", "ent\u00b7stellt", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VVFIN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterben werd' ich, und im Schoo\u00df der Erden", "tokens": ["Ster\u00b7ben", "werd'", "ich", ",", "und", "im", "Schoo\u00df", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruhe finden, die mir hier gebricht! \u2013", "tokens": ["Ru\u00b7he", "fin\u00b7den", ",", "die", "mir", "hier", "ge\u00b7bricht", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVINF", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aber da\u00df in jener grausen Stunde", "tokens": ["A\u00b7ber", "da\u00df", "in", "je\u00b7ner", "grau\u00b7sen", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir nicht Kraft, im Kampf nicht Muth vergeh',", "tokens": ["Mir", "nicht", "Kraft", ",", "im", "Kampf", "nicht", "Muth", "ver\u00b7geh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "NN", "$,", "APPRART", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df mich nicht die letzte hei\u00dfe Stunde", "tokens": ["Da\u00df", "mich", "nicht", "die", "letz\u00b7te", "hei\u00b7\u00dfe", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Allzu grimmig niederschmettere;", "tokens": ["All\u00b7zu", "grim\u00b7mig", "nie\u00b7der\u00b7schmet\u00b7te\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Dazu hilf, Erbarmer! Hoch von oben", "tokens": ["Da\u00b7zu", "hilf", ",", "Er\u00b7bar\u00b7mer", "!", "Hoch", "von", "o\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$,", "NN", "$.", "ADJD", "APPR", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sende mir des Himmels Vorgef\u00fchl,", "tokens": ["Sen\u00b7de", "mir", "des", "Him\u00b7mels", "Vor\u00b7ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Vorgeschmack der Seligkeiten droben,", "tokens": ["Vor\u00b7ge\u00b7schmack", "der", "Se\u00b7lig\u00b7kei\u00b7ten", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Grabesruhe still und k\u00fchl!", "tokens": ["Und", "die", "Gra\u00b7bes\u00b7ru\u00b7he", "still", "und", "k\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Fege ab von meiner Menschenseele", "tokens": ["Fe\u00b7ge", "ab", "von", "mei\u00b7ner", "Men\u00b7schen\u00b7see\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Allen S\u00fcndenwust und S\u00fcndenweh,", "tokens": ["Al\u00b7len", "S\u00fcn\u00b7den\u00b7wust", "und", "S\u00fcn\u00b7den\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich frei von aller Erdenfehle", "tokens": ["Da\u00df", "ich", "frei", "von", "al\u00b7ler", "Er\u00b7den\u00b7feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Grad' hinauf zu meinem Vater geh'!", "tokens": ["Grad'", "hin\u00b7auf", "zu", "mei\u00b7nem", "Va\u00b7ter", "geh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "L\u00e4utre mich, wie Gold im Feuertiegel!", "tokens": ["L\u00e4ut\u00b7re", "mich", ",", "wie", "Gold", "im", "Feu\u00b7er\u00b7tie\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00fcrg' in mir die Schlange Sinnlichkeit!", "tokens": ["W\u00fcr\u00b7g'", "in", "mir", "die", "Schlan\u00b7ge", "Sinn\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ha! so flieg' ich mit Elias Fl\u00fcgel", "tokens": ["Ha", "!", "so", "flieg'", "ich", "mit", "E\u00b7lias", "Fl\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Durch den Orionendonner hoch hinauf zur Seligkeit!", "tokens": ["Durch", "den", "O\u00b7rio\u00b7nen\u00b7don\u00b7ner", "hoch", "hin\u00b7auf", "zur", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.11": {"line.1": {"text": "Kommst du bald, du dunkle ernste Stunde,", "tokens": ["Kommst", "du", "bald", ",", "du", "dunk\u00b7le", "erns\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die zu meinen V\u00e4tern mich hin\u00fcber ruft?", "tokens": ["Die", "zu", "mei\u00b7nen", "V\u00e4\u00b7tern", "mich", "hin\u00b7\u00fc\u00b7ber", "ruft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Hergesehnte, hergeweinte Stunde,", "tokens": ["Her\u00b7ge\u00b7sehn\u00b7te", ",", "her\u00b7ge\u00b7wein\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Braut des langen Schlummers, und der engen Gruft? \u2013", "tokens": ["Braut", "des", "lan\u00b7gen", "Schlum\u00b7mers", ",", "und", "der", "en\u00b7gen", "Gruft", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.12": {"line.1": {"text": "Kommst du einst zu mir, du grause Stunde,", "tokens": ["Kommst", "du", "einst", "zu", "mir", ",", "du", "grau\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trost des Jammervollen, und des Christen Lust;", "tokens": ["Trost", "des", "Jam\u00b7mer\u00b7vol\u00b7len", ",", "und", "des", "Chris\u00b7ten", "Lust", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Blutet mir die hei\u00dfe Todeswunde", "tokens": ["Blu\u00b7tet", "mir", "die", "hei\u00b7\u00dfe", "To\u00b7des\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einstens aus der fliegenden gepre\u00dften Brust \u2013", "tokens": ["Eins\u00b7tens", "aus", "der", "flie\u00b7gen\u00b7den", "ge\u00b7pre\u00df\u00b7ten", "Brust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.13": {"line.1": {"text": "O wie wird sich dann der stolze J\u00fcngling schmiegen,", "tokens": ["O", "wie", "wird", "sich", "dann", "der", "stol\u00b7ze", "J\u00fcng\u00b7ling", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN",