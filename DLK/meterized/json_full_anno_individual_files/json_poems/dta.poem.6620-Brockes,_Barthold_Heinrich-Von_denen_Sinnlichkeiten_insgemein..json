{"dta.poem.6620": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und dennoch ist sein Leib, der irdisch und gebrechlich,", "tokens": ["Und", "den\u00b7noch", "ist", "sein", "Leib", ",", "der", "ir\u00b7disch", "und", "ge\u00b7brech\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein wunderbahrer Bau, der uns verkleinet, weis\u2019t", "tokens": ["Ein", "wun\u00b7der\u00b7bah\u00b7rer", "Bau", ",", "der", "uns", "ver\u00b7klei\u00b7net", ",", "weis't"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Solch eine Kunst die unaussprechlich,", "tokens": ["Solch", "ei\u00b7ne", "Kunst", "die", "un\u00b7aus\u00b7sprech\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihren Grossen SCHOEPFFER preis\u2019t.", "tokens": ["Die", "ih\u00b7ren", "Gros\u00b7sen", "ScHOE\u00b7PF\u00b7FER", "preis'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PPOSAT", "NN", "XY", "XY", "XY"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}