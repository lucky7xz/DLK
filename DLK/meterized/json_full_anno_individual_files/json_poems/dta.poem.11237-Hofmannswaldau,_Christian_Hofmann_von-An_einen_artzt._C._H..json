{"dta.poem.11237": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen artzt.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du und dein weib curirt die krancken um die wette,", "tokens": ["Du", "und", "dein", "weib", "cu\u00b7rirt", "die", "kran\u00b7cken", "um", "die", "wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVPP", "ART", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie, in dem ihrigen, du in dem fremden bette;", "tokens": ["Sie", ",", "in", "dem", "ih\u00b7ri\u00b7gen", ",", "du", "in", "dem", "frem\u00b7den", "bet\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "PPOSS", "$,", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch hat sie ihr dadurch ein gr\u00f6sser lob erworben,", "tokens": ["Doch", "hat", "sie", "ihr", "da\u00b7durch", "ein", "gr\u00f6s\u00b7ser", "lob", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ihr noch keiner ist, dir aber viel gestorben.", "tokens": ["Weil", "ihr", "noch", "kei\u00b7ner", "ist", ",", "dir", "a\u00b7ber", "viel", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}