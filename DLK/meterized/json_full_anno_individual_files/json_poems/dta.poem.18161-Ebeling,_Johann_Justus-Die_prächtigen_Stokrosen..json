{"dta.poem.18161": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die pr\u00e4chtigen Stokrosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht unempfindlich ist, wird", "tokens": ["Wer", "nicht", "un\u00b7emp\u00b7find\u00b7lich", "ist", ",", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PTKNEG", "ADJD", "VAFIN", "$,", "VAFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "durch einen Blik ger\u00fchrt,", "tokens": ["durch", "ei\u00b7nen", "Blik", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man die Stokrosen sieht, de-", "tokens": ["Wenn", "man", "die", "Sto\u00b7kro\u00b7sen", "sieht", ",", "de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ren Pracht die G\u00e4rten ziert;", "tokens": ["ren", "Pracht", "die", "G\u00e4r\u00b7ten", "ziert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil ihr hocherhabnes Haupt sich", "tokens": ["Weil", "ihr", "ho\u00b7cher\u00b7hab\u00b7nes", "Haupt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "an derer Beeten Ekken", "tokens": ["an", "de\u00b7rer", "Bee\u00b7ten", "Ek\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDS", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die mit Blumen sind besetzt, pfleget weit hervor zu", "tokens": ["Die", "mit", "Blu\u00b7men", "sind", "be\u00b7setzt", ",", "pfle\u00b7get", "weit", "her\u00b7vor", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "ADJD", "PTKVZ", "PTKZU"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "strekken.", "tokens": ["strek\u00b7ken", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wie mir ihre schlanke Pracht j\u00fcngstens in die Au-", "tokens": ["Wie", "mir", "ih\u00b7re", "schlan\u00b7ke", "Pracht", "j\u00fcng\u00b7stens", "in", "die", "Au"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "TRUNC"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "gen fiel,", "tokens": ["gen", "fiel", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "So erwekte mir der Blik dabei dieses Sinnenspiel:", "tokens": ["So", "er\u00b7wek\u00b7te", "mir", "der", "Blik", "da\u00b7bei", "die\u00b7ses", "Sin\u00b7nen\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "PDAT", "NN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Ich sah sie als F\u00fcrsten an, die bei denen Blumen", "tokens": ["Ich", "sah", "sie", "als", "F\u00fcrs\u00b7ten", "an", ",", "die", "bei", "de\u00b7nen", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$,", "PRELS", "APPR", "PRELS", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Heeren,", "tokens": ["Hee\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Stunden vorne an der Spitz, als wenn sie die F\u00fch-", "tokens": ["Stun\u00b7den", "vor\u00b7ne", "an", "der", "Spitz", ",", "als", "wenn", "sie", "die", "F\u00fch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "ART", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "rer w\u00e4ren.", "tokens": ["rer", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Jhre Stang ist wunderbar und als ein gesteiftes Rohr,", "tokens": ["Ih\u00b7re", "Stang", "ist", "wun\u00b7der\u00b7bar", "und", "als", "ein", "ge\u00b7steif\u00b7tes", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Daraus gehn von unten an bis gantz oben Kn\u00f6pf", "tokens": ["Da\u00b7raus", "gehn", "von", "un\u00b7ten", "an", "bis", "gantz", "o\u00b7ben", "Kn\u00f6pf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ADV", "APPR", "KON", "ADV", "ADV", "NN"], "meter": "--+-+----+-+", "measure": "anapaest.init"}, "line.18": {"text": "hervor:", "tokens": ["her\u00b7vor", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Diese spriessen immersort ihre Blumen die, vor allen", "tokens": ["Die\u00b7se", "spries\u00b7sen", "im\u00b7mer\u00b7sort", "ih\u00b7re", "Blu\u00b7men", "die", ",", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "$,", "APPR", "PIAT"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Wegen ihres breiten Schmuks uns in das Gesichte", "tokens": ["We\u00b7gen", "ih\u00b7res", "brei\u00b7ten", "Schmuks", "uns", "in", "das", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.21": {"text": "fallen.", "tokens": ["fal\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Welch ein Anblik voller Lust! ist es wenn man eine", "tokens": ["Welch", "ein", "An\u00b7blik", "vol\u00b7ler", "Lust", "!", "ist", "es", "wenn", "man", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "ADJA", "NN", "$.", "VAFIN", "PPER", "KOUS", "PIS", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "sieht,", "tokens": ["sieht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Die an ihren ganzen Stok, bis zum h\u00f6chsten Gip-", "tokens": ["Die", "an", "ih\u00b7ren", "gan\u00b7zen", "Stok", ",", "bis", "zum", "h\u00f6chs\u00b7ten", "Gip"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "APPRART", "ADJA", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "fel bl\u00fcht,", "tokens": ["fel", "bl\u00fcht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Deren Blumen ausgef\u00fcllt und mit wollgemischten", "tokens": ["De\u00b7ren", "Blu\u00b7men", "aus\u00b7ge\u00b7f\u00fcllt", "und", "mit", "woll\u00b7ge\u00b7mischten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VVPP", "KON", "APPR", "NN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.27": {"text": "Kr\u00e4ntzen,", "tokens": ["Kr\u00e4nt\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Wenn die Sonne feurig strahlt, als wie kleine Son-", "tokens": ["Wenn", "die", "Son\u00b7ne", "feu\u00b7rig", "strahlt", ",", "als", "wie", "klei\u00b7ne", "Son"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,", "KOUS", "KOKOM", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "nen gl\u00e4nzen,", "tokens": ["nen", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Welch ein Anblik wenn man sieht! auf der einen Pur-", "tokens": ["Welch", "ein", "An\u00b7blik", "wenn", "man", "sieht", "!", "auf", "der", "ei\u00b7nen", "Pur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "KOUS", "PIS", "VVFIN", "$.", "APPR", "ART", "ART", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "purschein", "tokens": ["pur\u00b7schein"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Da die andern schw\u00e4rtzlich roth, und die wieder gelb-", "tokens": ["Da", "die", "an\u00b7dern", "schw\u00e4rtz\u00b7lich", "roth", ",", "und", "die", "wie\u00b7der", "gelb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJD", "ADJD", "$,", "KON", "ART", "ADV", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "licht sein,", "tokens": ["licht", "sein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.34": {"text": "Wie vergn\u00fcgt sich unser Hertz an den feuerreichen", "tokens": ["Wie", "ver\u00b7gn\u00fcgt", "sich", "un\u00b7ser", "Hertz", "an", "den", "feu\u00b7er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Strahlen,", "tokens": ["Strah\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Die uns unsers Sch\u00f6pfers Gr\u00f6\u00df herrlich vor die Au-", "tokens": ["Die", "uns", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Gr\u00f6\u00df", "herr\u00b7lich", "vor", "die", "Au"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "NN", "ADJD", "APPR", "ART", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "gen mahlen!", "tokens": ["gen", "mah\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "O! du Vater alles Lichts, der du ewig herrlich", "tokens": ["O", "!", "du", "Va\u00b7ter", "al\u00b7les", "Lichts", ",", "der", "du", "e\u00b7wig", "herr\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "NN", "PIAT", "NN", "$,", "PRELS", "PPER", "ADJD", "ADJD"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "bist,", "tokens": ["bist", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.40": {"text": "Wie man an der Kreatur mit ger\u00fchrten Sinnen liest,", "tokens": ["Wie", "man", "an", "der", "Kre\u00b7a\u00b7tur", "mit", "ge\u00b7r\u00fchr\u00b7ten", "Sin\u00b7nen", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.41": {"text": "Seh ich die Stokrosen an, die an ihren Stielen han-", "tokens": ["Seh", "ich", "die", "Sto\u00b7kro\u00b7sen", "an", ",", "die", "an", "ih\u00b7ren", "Stie\u00b7len", "han"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "+--++-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.42": {"text": "gen;", "tokens": ["gen", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.43": {"text": "So seh ich zu deiner Ehr diese sch\u00f6ne Blumen pran-", "tokens": ["So", "seh", "ich", "zu", "dei\u00b7ner", "Ehr", "die\u00b7se", "sch\u00f6\u00b7ne", "Blu\u00b7men", "pran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PDAT", "ADJA", "NN", "TRUNC"], "meter": "-+--+----+-+-+", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "gen", "tokens": ["gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.45": {"text": "Jhre hocherhabne Pracht, ihre wollgeorndte Zier", "tokens": ["Ih\u00b7re", "ho\u00b7cher\u00b7hab\u00b7ne", "Pracht", ",", "ih\u00b7re", "woll\u00b7ge\u00b7ornd\u00b7te", "Zier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.46": {"text": "K\u00f6mt mir nach dem Augenschein, als wie Pyrami-", "tokens": ["K\u00f6mt", "mir", "nach", "dem", "Au\u00b7gen\u00b7schein", ",", "als", "wie", "Py\u00b7ra\u00b7mi"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "KOKOM", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "den f\u00fcr,", "tokens": ["den", "f\u00fcr", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "APPR", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.48": {"text": "Die zu ihres Sch\u00f6pfers Ruhm aufgericht gepflan-", "tokens": ["Die", "zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Ruhm", "auf\u00b7ge\u00b7richt", "ge\u00b7pflan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN", "VVPP", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.49": {"text": "zet stehen,", "tokens": ["zet", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.50": {"text": "Da\u00df wir daran uns zur Lehr diese Ueberschriften se-", "tokens": ["Da\u00df", "wir", "da\u00b7ran", "uns", "zur", "Lehr", "die\u00b7se", "Ue\u00b7ber\u00b7schrif\u00b7ten", "se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "PPER", "APPRART", "NN", "PDAT", "ADJA", "TRUNC"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.51": {"text": "hen:", "tokens": ["hen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.52": {"text": "Menschen! seht wie die Natur auf des H\u00f6ch-", "tokens": ["Men\u00b7schen", "!", "seht", "wie", "die", "Na\u00b7tur", "auf", "des", "H\u00f6ch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.53": {"text": "sten Macht Geheis,", "tokens": ["sten", "Macht", "Ge\u00b7heis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.54": {"text": "Allenthalben gr\u00fcnnt und bl\u00fcht zu des Sch\u00f6p-\nfers Ruhm und Preis", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", "gr\u00fcnnt", "und", "bl\u00fcht", "zu", "des", "Sch\u00f6p", "fers", "Ruhm", "und", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "ART", "TRUNC", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.55": {"text": "Schauet uns Stokrosen an die wir Ehren-", "tokens": ["Schau\u00b7et", "uns", "Sto\u00b7kro\u00b7sen", "an", "die", "wir", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PRELS", "PPER", "TRUNC"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.56": {"text": "s\u00e4ulen gleichen,", "tokens": ["s\u00e4u\u00b7len", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.57": {"text": "Wolt ihr als vern\u00fcnftige unbelebten Blu-\nmen weichen.", "toke