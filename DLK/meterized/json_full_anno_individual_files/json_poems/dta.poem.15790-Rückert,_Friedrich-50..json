{"dta.poem.15790": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Worten malt man auch; mal' immer aus den Schalen", "tokens": ["Mit", "Wor\u00b7ten", "malt", "man", "auch", ";", "mal'", "im\u00b7mer", "aus", "den", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADV", "$.", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Fantasie, was sich nur l\u00e4\u00dft durch Worte malen!", "tokens": ["Der", "Fan\u00b7ta\u00b7sie", ",", "was", "sich", "nur", "l\u00e4\u00dft", "durch", "Wor\u00b7te", "ma\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sei es ein Herzgef\u00fchl, ein Sinnengegenstand;", "tokens": ["Sei", "es", "ein", "Herz\u00b7ge\u00b7f\u00fchl", ",", "ein", "Sin\u00b7nen\u00b7ge\u00b7gen\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je schwieriger, je mehr zeigt er die K\u00fcnstlerhand.", "tokens": ["Je", "schwie\u00b7ri\u00b7ger", ",", "je", "mehr", "zeigt", "er", "die", "K\u00fcnst\u00b7ler\u00b7hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch ganz unk\u00fcnstlerisch ist es, ein Wort an Sachen", "tokens": ["Doch", "ganz", "un\u00b7k\u00fcnst\u00b7le\u00b7risch", "ist", "es", ",", "ein", "Wort", "an", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlieren, die nicht kann das Wort anschaulich machen.", "tokens": ["Ver\u00b7lie\u00b7ren", ",", "die", "nicht", "kann", "das", "Wort", "an\u00b7schau\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}