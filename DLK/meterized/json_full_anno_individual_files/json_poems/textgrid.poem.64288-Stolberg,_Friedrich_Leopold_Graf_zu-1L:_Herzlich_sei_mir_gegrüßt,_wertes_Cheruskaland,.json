{"textgrid.poem.64288": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herzlich sei mir gegr\u00fc\u00dft, wertes Cheruskaland,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herzlich sei mir gegr\u00fc\u00dft, wertes Cheruskaland,", "tokens": ["Herz\u00b7lich", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "wer\u00b7tes", "Che\u00b7rus\u00b7ka\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Land des nervichten Arms, und der gef\u00fcrchteten", "tokens": ["Land", "des", "ner\u00b7vich\u00b7ten", "Arms", ",", "und", "der", "ge\u00b7f\u00fcrch\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "K\u00fchnheit, freieres Geistes,", "tokens": ["K\u00fchn\u00b7heit", ",", "frei\u00b7e\u00b7res", "Geis\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dann das blache Gefild' umher!", "tokens": ["Dann", "das", "bla\u00b7che", "Ge\u00b7fild'", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Dir gab Mutter Natur, aus der vergeudenden", "tokens": ["Dir", "gab", "Mut\u00b7ter", "Na\u00b7tur", ",", "aus", "der", "ver\u00b7geu\u00b7den\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Urne, m\u00e4nnlichen Schmuck! Einfalt und W\u00fcrde dir!", "tokens": ["Ur\u00b7ne", ",", "m\u00e4nn\u00b7li\u00b7chen", "Schmuck", "!", "Ein\u00b7falt", "und", "W\u00fcr\u00b7de", "dir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wolkenh\u00f6hnende Klippen,", "tokens": ["Wol\u00b7ken\u00b7h\u00f6h\u00b7nen\u00b7de", "Klip\u00b7pen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Donnerhallende Str\u00f6me dir!", "tokens": ["Don\u00b7ner\u00b7hal\u00b7len\u00b7de", "Str\u00f6\u00b7me", "dir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Im antwortenden Thal wallet die goldene", "tokens": ["Im", "ant\u00b7wor\u00b7ten\u00b7den", "Thal", "wal\u00b7let", "die", "gol\u00b7de\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "-++--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Flut des Segens, und str\u00f6mt in den gen\u00fcgsamen", "tokens": ["Flut", "des", "Se\u00b7gens", ",", "und", "str\u00f6mt", "in", "den", "ge\u00b7n\u00fcg\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Scho\u00df des l\u00e4chelnden Flei\u00dfes,", "tokens": ["Scho\u00df", "des", "l\u00e4\u00b7cheln\u00b7den", "Flei\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der nicht k\u00e4rglich die Garben z\u00e4hlt.", "tokens": ["Der", "nicht", "k\u00e4rg\u00b7lich", "die", "Gar\u00b7ben", "z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Schafe weiden die Trift; auf der gew\u00e4sserten", "tokens": ["Scha\u00b7fe", "wei\u00b7den", "die", "Trift", ";", "auf", "der", "ge\u00b7w\u00e4s\u00b7ser\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "APPR", "ART", "ADJA"], "meter": "+-+--+---+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Aue br\u00fcllet der Stier, stampft das ges\u00e4ttigte", "tokens": ["Au\u00b7e", "br\u00fcl\u00b7let", "der", "Stier", ",", "stampft", "das", "ge\u00b7s\u00e4t\u00b7tig\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ro\u00df; die b\u00e4rtige Ziege", "tokens": ["Ro\u00df", ";", "die", "b\u00e4r\u00b7ti\u00b7ge", "Zie\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Klimmt den zackigen Fels hinan.", "tokens": ["Klimmt", "den", "za\u00b7cki\u00b7gen", "Fels", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Wie der schirmende Forst deinen erhabenen", "tokens": ["Wie", "der", "schir\u00b7men\u00b7de", "Forst", "dei\u00b7nen", "er\u00b7ha\u00b7be\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPOSAT", "ADJA"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nacken schattet! Er n\u00e4hrt stolzes Geweihe dir!", "tokens": ["Na\u00b7cken", "schat\u00b7tet", "!", "Er", "n\u00e4hrt", "stol\u00b7zes", "Ge\u00b7wei\u00b7he", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dir den schnaubenden Keuler,", "tokens": ["Dir", "den", "schnau\u00b7ben\u00b7den", "Keu\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der entgegen der Wunde rennt!", "tokens": ["Der", "ent\u00b7ge\u00b7gen", "der", "Wun\u00b7de", "rennt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Dein wohlth\u00e4tiger Scho\u00df, selten mit goldenem", "tokens": ["Dein", "wohlt\u00b7h\u00e4\u00b7ti\u00b7ger", "Scho\u00df", ",", "sel\u00b7ten", "mit", "gol\u00b7de\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "APPR", "ADJA"], "meter": "--+--+---+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fluche schwanger, verleiht n\u00fctzendes Eisen uns,", "tokens": ["Flu\u00b7che", "schwan\u00b7ger", ",", "ver\u00b7leiht", "n\u00fct\u00b7zen\u00b7des", "Ei\u00b7sen", "uns", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ADJA", "NN", "PPER", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Das den Acker durchschneidet,", "tokens": ["Das", "den", "A\u00b7cker", "durch\u00b7schnei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und das Erbe der V\u00e4ter sch\u00fctzt!", "tokens": ["Und", "das", "Er\u00b7be", "der", "V\u00e4\u00b7ter", "sch\u00fctzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Dir giebt reinere Luft, und die teutonische", "tokens": ["Dir", "giebt", "rei\u00b7ne\u00b7re", "Luft", ",", "und", "die", "teu\u00b7to\u00b7ni\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "KON", "ART", "ADJA"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Keuschheit, Jugend von Stahl; moosigen Eichen gleich,", "tokens": ["Keuschheit", ",", "Ju\u00b7gend", "von", "Stahl", ";", "moo\u00b7si\u00b7gen", "Ei\u00b7chen", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "NN", "$.", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Achten silberne Greise", "tokens": ["Ach\u00b7ten", "sil\u00b7ber\u00b7ne", "Grei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nicht der eilenden Jahre Flucht!", "tokens": ["Nicht", "der", "ei\u00b7len\u00b7den", "Jah\u00b7re", "Flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Dort im wehenden Hain wohnt die Begeisterung!", "tokens": ["Dort", "im", "we\u00b7hen\u00b7den", "Hain", "wohnt", "die", "Be\u00b7geis\u00b7te\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Felsen jauchzten zur\u00fcck, wann des Bardiets Orkan", "tokens": ["Fel\u00b7sen", "jauchz\u00b7ten", "zu\u00b7r\u00fcck", ",", "wann", "des", "Bar\u00b7diets", "Or\u00b7kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "NN", "NE"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Edelthaten der Vorzeit", "tokens": ["E\u00b7delt\u00b7ha\u00b7ten", "der", "Vor\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und die himmlische Freiheit pries.", "tokens": ["Und", "die", "himm\u00b7li\u00b7sche", "Frei\u00b7heit", "pries", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Und dein Hermann vernahm's: Sturm war sein Arm, sein Schwert", "tokens": ["Und", "dein", "Her\u00b7mann", "ver\u00b7nahm's", ":", "Sturm", "war", "sein", "Arm", ",", "sein", "Schwert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NE", "$.", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wetterflamme! Bet\u00e4ubt st\u00fcrzten die trotzigen", "tokens": ["Wet\u00b7ter\u00b7flam\u00b7me", "!", "Be\u00b7t\u00e4ubt", "st\u00fcrz\u00b7ten", "die", "trot\u00b7zi\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--++-+++-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "R\u00f6meradler; und Freiheit", "tokens": ["R\u00f6\u00b7me\u00b7rad\u00b7ler", ";", "und", "Frei\u00b7heit"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Strahlte wieder im Lande Teuts!", "tokens": ["Strahl\u00b7te", "wie\u00b7der", "im", "Lan\u00b7de", "Teuts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Doch des Biedergeschlechts sklavische Brut verbarg", "tokens": ["Doch", "des", "Bie\u00b7der\u00b7ge\u00b7schlechts", "skla\u00b7vi\u00b7sche", "Brut", "ver\u00b7barg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hermanns Namen in Nacht; bis ihn (auch er dein Sohn!)", "tokens": ["Her\u00b7manns", "Na\u00b7men", "in", "Nacht", ";", "bis", "ihn", "(", "auch", "er", "dein", "Sohn", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$.", "KOUS", "PPER", "$(", "ADV", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Klopstocks m\u00e4chtige Harfe", "tokens": ["Klops\u00b7tocks", "m\u00e4ch\u00b7ti\u00b7ge", "Har\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], 