{"dta.poem.849": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.  Auff denheiligen Pfingst Tag/  \n Johan. 14.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wehn Christi Lieb entz\u00fcndt: pflegt Christi wortt zu trawen. ", "tokens": ["Wehn", "Chris\u00b7ti", "Lieb", "ent\u00b7z\u00fcndt", ":", "pflegt", "Chris\u00b7ti", "wortt", "zu", "tra\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVPP", "$.", "VVFIN", "NE", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer Christi wortten trawt: den schleust der grosse", "tokens": ["Wer", "Chris\u00b7ti", "wort\u00b7ten", "trawt", ":", "den", "schleust", "der", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NE", "NN", "VVFIN", "$.", "ART", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "GoTT ", "tokens": ["GoTT"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Der Vater fest ins Hertz: vnd ob in h\u00f6chster noth", "tokens": ["Der", "Va\u00b7ter", "fest", "ins", "Hertz", ":", "vnd", "ob", "in", "h\u00f6chs\u00b7ter", "noth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$.", "KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhm schon vor ach! vnd angst! vnd vntergang wil grawen:", "tokens": ["Jhm", "schon", "vor", "ach", "!", "vnd", "angst", "!", "vnd", "vn\u00b7ter\u00b7gang", "wil", "gra\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ITJ", "$.", "KON", "VVPP", "$.", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Wil Gott der Drey vnd Eins doch wohnung bey Jhm ba-", "tokens": ["Wil", "Gott", "der", "Drey", "vnd", "Eins", "doch", "woh\u00b7nung", "bey", "Jhm", "ba"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "ART", "CARD", "KON", "NN", "ADV", "NN", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "wen ", "tokens": ["wen"], "token_info": ["word"], "pos": ["PWS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der Geist der werthe Geist/ durch dessen trost der Todt", "tokens": ["Der", "Geist", "der", "wert\u00b7he", "Geist", "/", "durch", "des\u00b7sen", "trost", "der", "Todt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "APPR", "PDS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht l\u00e4nger t\u00f6dlich ist: der rechte Friedensbott", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "t\u00f6d\u00b7lich", "ist", ":", "der", "rech\u00b7te", "Frie\u00b7dens\u00b7bott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADJD", "VAFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wil Jhn die wahre Lust in vnlust lassen schawen.", "tokens": ["Wil", "Jhn", "die", "wah\u00b7re", "Lust", "in", "vn\u00b7lust", "las\u00b7sen", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.3": {"line.1": {"text": "Er wil was vnser Sinn durchau\u00df vor sich nicht fast", "tokens": ["Er", "wil", "was", "vn\u00b7ser", "Sinn", "durch\u00b7au\u00df", "vor", "sich", "nicht", "fast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "PPOSAT", "NN", "ADV", "APPR", "PRF", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erkl\u00e4ren ja Er wil/ wenn vns der Feind antast", "tokens": ["Er\u00b7kl\u00e4\u00b7ren", "ja", "Er", "wil", "/", "wenn", "vns", "der", "Feind", "an\u00b7tast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "VMFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Gottes ernsten fluch vnd eigner schuld entbinden.", "tokens": ["Von", "Got\u00b7tes", "erns\u00b7ten", "fluch", "vnd", "eig\u00b7ner", "schuld", "ent\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Erwil/ wenn Fleisch vnd Seel in sterbens schmertz\u1ebd kracht", "tokens": ["Er\u00b7wil", "/", "wenn", "Fleisch", "vnd", "Seel", "in", "ster\u00b7bens", "schmertz\u1ebd", "kracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "NN", "KON", "NN", "APPR", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Vnd wenn der matte Mensch aufs Teufels Siebe", "tokens": ["Vnd", "wenn", "der", "mat\u00b7te", "Mensch", "aufs", "Teu\u00b7fels", "Sie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schmacht/ ", "tokens": ["schmacht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Durch di\u00df/ was ", "tokens": ["Durch", "di\u00df", "/", "was"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "PDS", "$(", "PWS"], "meter": "+-+", "measure": "trochaic.di"}}}}}