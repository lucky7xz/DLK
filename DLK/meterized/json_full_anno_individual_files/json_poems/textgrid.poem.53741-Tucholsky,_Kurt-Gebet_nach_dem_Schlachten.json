{"textgrid.poem.53741": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Gebet nach dem Schlachten", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kopf ab zum Gebet!", "tokens": ["Kopf", "ab", "zum", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Herrgott! Wir alten vermoderten Knochen", "tokens": ["Herr\u00b7gott", "!", "Wir", "al\u00b7ten", "ver\u00b7mo\u00b7der\u00b7ten", "Kno\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "ADJA", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "sind aus den Kalkgr\u00e4bern noch einmal hervorgekrochen.", "tokens": ["sind", "aus", "den", "Kalk\u00b7gr\u00e4\u00b7bern", "noch", "ein\u00b7mal", "her\u00b7vor\u00b7ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir treten zum Beten vor dich und bleiben nicht stumm.", "tokens": ["Wir", "tre\u00b7ten", "zum", "Be\u00b7ten", "vor", "dich", "und", "blei\u00b7ben", "nicht", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPER", "KON", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und fragen dich, Gott:", "tokens": ["Und", "fra\u00b7gen", "dich", ",", "Gott", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Warum \u2013?", "tokens": ["Wa\u00b7rum", "\u2013", "?"], "token_info": ["word", "punct", "punct"], "pos": ["PWAV", "$(", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Warum haben wir unser rotes Herzblut dahingegeben?", "tokens": ["Wa\u00b7rum", "ha\u00b7ben", "wir", "un\u00b7ser", "ro\u00b7tes", "Herz\u00b7blut", "da\u00b7hin\u00b7ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bei unserm Kaiser blieben alle sechs am Leben.", "tokens": ["Bei", "un\u00b7serm", "Kai\u00b7ser", "blie\u00b7ben", "al\u00b7le", "sechs", "am", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "CARD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir haben einmal geglaubt . . . Wir waren sch\u00f6n dumm . . . !", "tokens": ["Wir", "ha\u00b7ben", "ein\u00b7mal", "ge\u00b7glaubt", ".", ".", ".", "Wir", "wa\u00b7ren", "sch\u00f6n", "dumm", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "$.", "$.", "PPER", "VAFIN", "ADJD", "ADJD", "$.", "$.", "$.", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Uns haben sie besoffen gemacht . . .", "tokens": ["Uns", "ha\u00b7ben", "sie", "be\u00b7sof\u00b7fen", "ge\u00b7macht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Warum \u2013?", "tokens": ["Wa\u00b7rum", "\u2013", "?"], "token_info": ["word", "punct", "punct"], "pos": ["PWAV", "$(", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Einer hat noch sechs Monate im Lazarett geschrien.", "tokens": ["Ei\u00b7ner", "hat", "noch", "sechs", "Mo\u00b7na\u00b7te", "im", "La\u00b7za\u00b7rett", "ge\u00b7schri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "CARD", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Erst das D\u00f6rrgem\u00fcse und zwei Stabs\u00e4rzte erledigten ihn.", "tokens": ["Erst", "das", "D\u00f6rr\u00b7ge\u00b7m\u00fc\u00b7se", "und", "zwei", "Stabs\u00b7\u00e4rz\u00b7te", "er\u00b7le\u00b7dig\u00b7ten", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "CARD", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Einer wurde blind und nahm heimlich Opium.", "tokens": ["Ei\u00b7ner", "wur\u00b7de", "blind", "und", "nahm", "heim\u00b7lich", "O\u00b7pium", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Drei von uns haben zusammen nur einen Arm . . .", "tokens": ["Drei", "von", "uns", "ha\u00b7ben", "zu\u00b7sam\u00b7men", "nur", "ei\u00b7nen", "Arm", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["CARD", "APPR", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$.", "$.", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Warum \u2013?", "tokens": ["Wa\u00b7rum", "\u2013", "?"], "token_info": ["word", "punct", "punct"], "pos": ["PWAV", "$(", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Wir haben Glauben, Krieg, Leben und alles verloren.", "tokens": ["Wir", "ha\u00b7ben", "Glau\u00b7ben", ",", "Krieg", ",", "Le\u00b7ben", "und", "al\u00b7les", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "$,", "NN", "KON", "PIS", "VVPP", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Uns trieben sie hinein wie im Kino die Gladiatoren.", "tokens": ["Uns", "trie\u00b7ben", "sie", "hin\u00b7ein", "wie", "im", "Ki\u00b7no", "die", "Gla\u00b7di\u00b7a\u00b7to\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KOKOM", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wir hatten das allerbeste Publikum.", "tokens": ["Wir", "hat\u00b7ten", "das", "al\u00b7ler\u00b7bes\u00b7te", "Pub\u00b7li\u00b7kum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das starb aber nicht mit . . .", "tokens": ["Das", "starb", "a\u00b7ber", "nicht", "mit", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$.", "$.", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Warum \u2013? Warum \u2013?", "tokens": ["Wa\u00b7rum", "\u2013", "?", "Wa\u00b7rum", "\u2013", "?"], "token_info": ["word", "punct", "punct", "word", "punct", "punct"], "pos": ["PWAV", "$(", "$.", "PWAV", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Herrgott!", "tokens": ["Herr\u00b7gott", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Wenn du wirklich der bist, als den wir dich lernten:", "tokens": ["Wenn", "du", "wirk\u00b7lich", "der", "bist", ",", "als", "den", "wir", "dich", "lern\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "VAFIN", "$,", "KOUS", "ART", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Steig herunter von deinem Himmel, dem besternten!", "tokens": ["Steig", "her\u00b7un\u00b7ter", "von", "dei\u00b7nem", "Him\u00b7mel", ",", "dem", "bes\u00b7tern\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Fahr hernieder oder schick deinen Sohn!", "tokens": ["Fahr", "her\u00b7nie\u00b7der", "o\u00b7der", "schick", "dei\u00b7nen", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVIMP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Rei\u00df ab die Fahnen, die Helme, die Ordensdekoration!", "tokens": ["Rei\u00df", "ab", "die", "Fah\u00b7nen", ",", "die", "Hel\u00b7me", ",", "die", "Or\u00b7dens\u00b7de\u00b7ko\u00b7ra\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Verk\u00fcnde den Staaten der Erde, wie wir gelitten,", "tokens": ["Ver\u00b7k\u00fcn\u00b7de", "den", "Staa\u00b7ten", "der", "Er\u00b7de", ",", "wie", "wir", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "wie uns Hunger, L\u00e4use, Schrapnells und L\u00fcgen den Leib zerschnitten!", "tokens": ["wie", "uns", "Hun\u00b7ger", ",", "L\u00e4u\u00b7se", ",", "Schrap\u00b7nells", "und", "L\u00fc\u00b7gen", "den", "Leib", "zer\u00b7schnit\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Feldprediger haben uns in deinem Namen zu Grabe getragen.", "tokens": ["Feld\u00b7pre\u00b7di\u00b7ger", "ha\u00b7ben", "uns", "in", "dei\u00b7nem", "Na\u00b7men", "zu", "Gra\u00b7be", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Erkl\u00e4re, da\u00df sie gelogen haben! L\u00e4\u00dft du dir das sagen?", "tokens": ["Er\u00b7kl\u00e4\u00b7re", ",", "da\u00df", "sie", "ge\u00b7lo\u00b7gen", "ha\u00b7ben", "!", "L\u00e4\u00dft", "du", "dir", "das", "sa\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVPP", "VAINF", "$.", "VVFIN", "PPER", "PPER", "PDS", "VVINF", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Jag uns zur\u00fcck in unsre Gr\u00e4ber, aber antworte zuvor!", "tokens": ["Jag", "uns", "zu\u00b7r\u00fcck", "in", "uns\u00b7re", "Gr\u00e4\u00b7ber", ",", "a\u00b7ber", "ant\u00b7wor\u00b7te", "zu\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Soweit wir das noch k\u00f6nnen, knien wir vor dir \u2013 aber leih uns dein Ohr!", "tokens": ["So\u00b7weit", "wir", "das", "noch", "k\u00f6n\u00b7nen", ",", "kni\u00b7en", "wir", "vor", "dir", "\u2013", "a\u00b7ber", "leih", "uns", "dein", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$(", "KON", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+----+-+--+", "measure": "iambic.septa.chol"}, "line.12": {"text": "Wenn unser Sterben nicht v\u00f6llig sinnlos war,", "tokens": ["Wenn", "un\u00b7ser", "Ster\u00b7ben", "nicht", "v\u00f6l\u00b7lig", "sinn\u00b7los", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "verh\u00fcte wie 1914 ein Jahr!", "tokens": ["ver\u00b7h\u00fc\u00b7te", "wie", "19\u00b714", "ein", "Jahr", "!"], "token_info": ["word", "word", "number", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "CARD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Sag es den Menschen! Treib sie zur Desertion!", "tokens": ["Sag", "es", "den", "Men\u00b7schen", "!", "Treib", "sie", "zur", "De\u00b