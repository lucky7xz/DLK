{"textgrid.poem.61094": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Verw\u00fcnschung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst dich nicht bei unsrem Feste zeigen,", "tokens": ["Du", "willst", "dich", "nicht", "bei", "uns\u00b7rem", "Fes\u00b7te", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo auf dem Rasen unter gr\u00fcnen B\u00e4umen", "tokens": ["Wo", "auf", "dem", "Ra\u00b7sen", "un\u00b7ter", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gitarren klingen und Pokale sch\u00e4umen", "tokens": ["Gi\u00b7tar\u00b7ren", "klin\u00b7gen", "und", "Po\u00b7ka\u00b7le", "sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Reb' und Rose sich zum Kranz verzweigen.", "tokens": ["Und", "Reb'", "und", "Ro\u00b7se", "sich", "zum", "Kranz", "ver\u00b7zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du fliehst den Scherz, den Becherklang, den Reigen,", "tokens": ["Du", "fliehst", "den", "Scherz", ",", "den", "Be\u00b7cher\u00b7klang", ",", "den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "