{"textgrid.poem.52709": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Apricari", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein J\u00fcngling lag am H\u00fcgelrand,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "lag", "am", "H\u00fc\u00b7gel\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00e4umte von besseren Tagen,", "tokens": ["Und", "tr\u00e4um\u00b7te", "von", "bes\u00b7se\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er tr\u00e4umte von dem Vaterland,", "tokens": ["Er", "tr\u00e4um\u00b7te", "von", "dem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom letzten Schlachtenschlagen;", "tokens": ["Vom", "letz\u00b7ten", "Schlach\u00b7ten\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Wimper zuckt, das Herz pocht laut,", "tokens": ["Die", "Wim\u00b7per", "zuckt", ",", "das", "Herz", "pocht", "laut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er hat auf menschlicher Erd erschaut", "tokens": ["Er", "hat", "auf", "menschli\u00b7cher", "Erd", "er\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Weinen mehr und Klagen.", "tokens": ["Kein", "Wei\u00b7nen", "mehr", "und", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die F\u00f6hren s\u00e4uselten in dem Wind,", "tokens": ["Die", "F\u00f6h\u00b7ren", "s\u00e4u\u00b7sel\u00b7ten", "in", "dem", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hoch kreiste der Weih in den L\u00fcften,", "tokens": ["Hoch", "kreis\u00b7te", "der", "Weih", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Morgenathem kam so lind", "tokens": ["Der", "Mor\u00b7ge\u00b7na\u00b7them", "kam", "so", "lind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Her\u00fcber die gr\u00fcnen Triften;", "tokens": ["Her\u00b7\u00fc\u00b7ber", "die", "gr\u00fc\u00b7nen", "Trif\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und in der jungen tr\u00e4umenden Brust", "tokens": ["Und", "in", "der", "jun\u00b7gen", "tr\u00e4u\u00b7men\u00b7den", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Eine Friedenswelt ward sich bewu\u00dft,", "tokens": ["Ei\u00b7ne", "Frie\u00b7dens\u00b7welt", "ward", "sich", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Berauscht von Freiheitsd\u00fcften.", "tokens": ["Be\u00b7rauscht", "von", "Frei\u00b7heits\u00b7d\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war der Liebe goldenes Reich", "tokens": ["Es", "war", "der", "Lie\u00b7be", "gol\u00b7de\u00b7nes", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In alle Th\u00e4ler gezogen,", "tokens": ["In", "al\u00b7le", "Th\u00e4\u00b7ler", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da machte die Sch\u00f6nheit die Menschen gleich,", "tokens": ["Da", "mach\u00b7te", "die", "Sch\u00f6n\u00b7heit", "die", "Men\u00b7schen", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da ward keiner Treue gelogen,", "tokens": ["Da", "ward", "kei\u00b7ner", "Treu\u00b7e", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da war keine S\u00fcnde, da war keine Schuld,", "tokens": ["Da", "war", "kei\u00b7ne", "S\u00fcn\u00b7de", ",", "da", "war", "kei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da schwelgete Alles in Liebeshuld", "tokens": ["Da", "schwel\u00b7ge\u00b7te", "Al\u00b7les", "in", "Lie\u00b7bes\u00b7huld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Was unter dem Himmelsbogen.", "tokens": ["Was", "un\u00b7ter", "dem", "Him\u00b7mels\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "So wacht er auf in gr\u00fcner Flur,", "tokens": ["So", "wacht", "er", "auf", "in", "gr\u00fc\u00b7ner", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gen S\u00fcden gl\u00e4nzte die Sonne,", "tokens": ["Gen", "S\u00fc\u00b7den", "gl\u00e4nz\u00b7te", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kein Menschenlaut! er h\u00f6rte nur", "tokens": ["Kein", "Men\u00b7schen\u00b7laut", "!", "er", "h\u00f6r\u00b7te", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der V\u00f6gel Liederwonne.", "tokens": ["Der", "V\u00f6\u00b7gel", "Lie\u00b7der\u00b7won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur Seite rieselt ihm leis ein Quell,", "tokens": ["Zur", "Sei\u00b7te", "rie\u00b7selt", "ihm", "leis", "ein", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So silberfrisch, so dunkel hell,", "tokens": ["So", "sil\u00b7ber\u00b7frisch", ",", "so", "dun\u00b7kel", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er trank vom klaren Bronne.", "tokens": ["Er", "trank", "vom", "kla\u00b7ren", "Bron\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df er getr\u00e4umt, er wu\u00dft es nicht,", "tokens": ["Da\u00df", "er", "ge\u00b7tr\u00e4umt", ",", "er", "wu\u00dft", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War von der Welt genesen,", "tokens": ["War", "von", "der", "Welt", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er glaubte wie im Traumgesicht", "tokens": ["Er", "glaub\u00b7te", "wie", "im", "Traum\u00b7ge\u00b7sicht"], 