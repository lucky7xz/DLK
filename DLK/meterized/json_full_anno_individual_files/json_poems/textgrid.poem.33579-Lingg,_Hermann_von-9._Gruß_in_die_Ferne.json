{"textgrid.poem.33579": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "9. Gru\u00df in die Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dunkelnd \u00fcber dem See d\u00e4mmert das Abendrot,", "tokens": ["Dun\u00b7kelnd", "\u00fc\u00b7ber", "dem", "See", "d\u00e4m\u00b7mert", "das", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nur die h\u00f6chsten Gebirge", "tokens": ["Nur", "die", "h\u00f6chs\u00b7ten", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Kr\u00f6nt noch Glut, doch es sinkt, d\u00fcstrer allm\u00e4hlich, nun", "tokens": ["Kr\u00f6nt", "noch", "Glut", ",", "doch", "es", "sinkt", ",", "d\u00fcst\u00b7rer", "all\u00b7m\u00e4h\u00b7lich", ",", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "NN", "$,", "KON", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "$,", "ADV"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Auch ihr Bild zu den Schatten.", "tokens": ["Auch", "ihr", "Bild", "zu", "den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Dort ach, fern in der Nacht, dort wo des Himmels Licht", "tokens": ["Dort", "ach", ",", "fern", "in", "der", "Nacht", ",", "dort", "wo", "des", "Him\u00b7mels", "Licht"], "token_info": ["word", "word"