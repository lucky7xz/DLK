: ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PIS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die Opferduft-umhauchten", "tokens": ["Als", "die", "Op\u00b7fer\u00b7duft\u00b7um\u00b7hauch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00e4ber der geliebten Lieben,", "tokens": ["Gr\u00e4\u00b7ber", "der", "ge\u00b7lieb\u00b7ten", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche wir zum Leben brauchten.", "tokens": ["Wel\u00b7che", "wir", "zum", "Le\u00b7ben", "brauch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O, wie wir das Sein so gerne,", "tokens": ["O", ",", "wie", "wir", "das", "Sein", "so", "ger\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie so freudig von uns streiften,", "tokens": ["Wie", "so", "freu\u00b7dig", "von", "uns", "streif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Flucht nach sch\u00f6n'rem Sterne", "tokens": ["Auf", "der", "Flucht", "nach", "sch\u00f6n'\u00b7rem", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"te