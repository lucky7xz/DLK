{"dta.poem.23731": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Zuschrift  \n  an ein paar Kinder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr fodert h\u00fcpfend eine Gabe", "tokens": ["Ihr", "fo\u00b7dert", "h\u00fcp\u00b7fend", "ei\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir, kleinen Schmeichler, ab?", "tokens": ["Mir", ",", "klei\u00b7nen", "Schmeich\u00b7ler", ",", "ab", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier habt Jhr alles, was ich habe,", "tokens": ["Hier", "habt", "Ihr", "al\u00b7les", ",", "was", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir die Muse gab.", "tokens": ["Und", "mir", "die", "Mu\u00b7se", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Muse &#8212; doch ich h\u00f6r Euch fragen,", "tokens": ["Die", "Mu\u00b7se", "&#8212;", "doch", "ich", "h\u00f6r", "Euch", "fra\u00b7gen", ","], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Welch Wunderding die\u00df ist?", "tokens": ["Welch", "Wun\u00b7der\u00b7ding", "die\u00df", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PDS", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will es im Vertraun Euch sagen:", "tokens": ["Ich", "will", "es", "im", "Ver\u00b7traun", "Euch", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bald ich Euch gek\u00fc\u00dft.", "tokens": ["So", "bald", "ich", "Euch", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist die v\u00e4terliche Liebe,", "tokens": ["Es", "ist", "die", "v\u00e4\u00b7ter\u00b7li\u00b7che", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der jede Liebe weicht,", "tokens": ["Der", "je\u00b7de", "Lie\u00b7be", "weicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und der bey mir nichts, als die Liebe", "tokens": ["Und", "der", "bey", "mir", "nichts", ",", "als", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "PPER", "PIS", "$,", "KOUS", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr Eure Mutter gleicht.", "tokens": ["F\u00fcr", "Eu\u00b7re", "Mut\u00b7ter", "gleicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00dft sie Euch diese Lehren geben,", "tokens": ["La\u00dft", "sie", "Euch", "die\u00b7se", "Leh\u00b7ren", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Harmonie vers\u00fc\u00dft:", "tokens": ["Durch", "Har\u00b7mo\u00b7nie", "ver\u00b7s\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weit kr\u00e4ftiger lehrt Euch ihr Leben,", "tokens": ["Weit", "kr\u00e4f\u00b7ti\u00b7ger", "lehrt", "Euch", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das lauter Wohllaut ist.", "tokens": ["Das", "lau\u00b7ter", "Wohl\u00b7laut", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}