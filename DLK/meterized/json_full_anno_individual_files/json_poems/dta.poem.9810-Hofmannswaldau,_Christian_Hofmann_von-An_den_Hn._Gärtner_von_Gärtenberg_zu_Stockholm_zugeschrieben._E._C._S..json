, "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "So bitt ich/ wann es m\u00f6cht in deinen garten dienen/", "tokens": ["So", "bitt", "ich", "/", "wann", "es", "m\u00f6cht", "in", "dei\u00b7nen", "gar\u00b7ten", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VMFIN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Verg\u00f6nn\u2019 ihm einen ort; wird es nicht untergehn/", "tokens": ["Ver\u00b7g\u00f6nn'", "ihm", "ei\u00b7nen", "ort", ";", "wird", "es", "nicht", "un\u00b7ter\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "VAFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "So soll es ewiglich zu deinen diensten stehn.", "tokens": ["So", "soll", "es", "e\u00b7wig\u00b7lich", "zu", "dei\u00b7nen", "diens\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Mein poley ist verwelckt/ der hyacinth verschwunden/", "tokens": ["Mein", "po\u00b7ley", "ist", "ver\u00b7welckt", "/", "der", "hya\u00b7cin\u00b7th", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": 