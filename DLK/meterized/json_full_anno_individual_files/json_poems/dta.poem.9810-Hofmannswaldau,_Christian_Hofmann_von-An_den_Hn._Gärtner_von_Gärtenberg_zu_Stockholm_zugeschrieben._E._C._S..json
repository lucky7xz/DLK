{"dta.poem.9810": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den Hn. G\u00e4rtner von G\u00e4rtenberg zu  \n Stockholm zugeschrieben.  \n E. C. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeih mir/ Mec\u00e4nat/ wenn diese schlechte zeilen", "tokens": ["Ver\u00b7zeih", "mir", "/", "Me\u00b7c\u00e4\u00b7nat", "/", "wenn", "die\u00b7se", "schlech\u00b7te", "zei\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$(", "NE", "$(", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu unbequemer zeit zu deinen f\u00fcssen eilen/", "tokens": ["Zu", "un\u00b7be\u00b7que\u00b7mer", "zeit", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "ei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn meiner feder mund nicht redet nach geb\u00fchr/", "tokens": ["Wenn", "mei\u00b7ner", "fe\u00b7der", "mund", "nicht", "re\u00b7det", "nach", "ge\u00b7b\u00fchr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bitt\u2019 ich/ sto\u00df mich nicht von deiner garten-th\u00fcr.", "tokens": ["So", "bitt'", "ich", "/", "sto\u00df", "mich", "nicht", "von", "dei\u00b7ner", "gar\u00b7ten", "th\u00fcr."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "TRUNC", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weil ich die g\u00e4rtner-kunst von jugend an geliebet", "tokens": ["Weil", "ich", "die", "g\u00e4rt\u00b7ner\u00b7kunst", "von", "ju\u00b7gend", "an", "ge\u00b7lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mich nach m\u00f6gligkeit in selbiger ge\u00fcbet/", "tokens": ["Und", "mich", "nach", "m\u00f6g\u00b7lig\u00b7keit", "in", "sel\u00b7bi\u00b7ger", "ge\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hat selbst Apollo mir/ aus sonderbarer gunst/", "tokens": ["Hat", "selbst", "A\u00b7pol\u00b7lo", "mir", "/", "aus", "son\u00b7der\u00b7ba\u00b7rer", "gunst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "PPER", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit eigner hand gelehrt ein theil von selber kunst.", "tokens": ["Mit", "eig\u00b7ner", "hand", "ge\u00b7lehrt", "ein", "theil", "von", "sel\u00b7ber", "kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "ART", "NN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er f\u00fchrte mich mit sich in einen sch\u00f6nen garten;", "tokens": ["Er", "f\u00fchr\u00b7te", "mich", "mit", "sich", "in", "ei\u00b7nen", "sch\u00f6\u00b7nen", "gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil ich nun willig war den blumen auffzuwarten/", "tokens": ["Weil", "ich", "nun", "wil\u00b7lig", "war", "den", "blu\u00b7men", "auff\u00b7zu\u00b7war\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hab ich mit stetem flei\u00df es letzt dahin gebracht/", "tokens": ["Hab", "ich", "mit", "ste\u00b7tem", "flei\u00df", "es", "letzt", "da\u00b7hin", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "PPER", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df er mich \u00fcber sie zum g\u00e4rtner hat gemacht.", "tokens": ["Da\u00df", "er", "mich", "\u00fc\u00b7ber", "sie", "zum", "g\u00e4rt\u00b7ner", "hat", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "APPRART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der garten/ welchen zwar sonst niemand noch geschauet/", "tokens": ["Der", "gar\u00b7ten", "/", "wel\u00b7chen", "zwar", "sonst", "nie\u00b7mand", "noch", "ge\u00b7schau\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PWAT", "ADV", "ADV", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als nur der dichter schaar/ war ziemlich ungebauet/", "tokens": ["Als", "nur", "der", "dich\u00b7ter", "schaar", "/", "war", "ziem\u00b7lich", "un\u00b7ge\u00b7bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$(", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zur rechten stund ein wald/ zur lincken Floren zelt/", "tokens": ["Zur", "rech\u00b7ten", "stund", "ein", "wald", "/", "zur", "lin\u00b7cken", "Flo\u00b7ren", "zelt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "NN", "$(", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hier ein erh\u00f6hter berg/ dort gr\u00fcn durchkleetes feld.", "tokens": ["Hier", "ein", "er\u00b7h\u00f6h\u00b7ter", "berg", "/", "dort", "gr\u00fcn", "durch\u00b7klee\u00b7tes", "feld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$(", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es gienge Castalis mit perlen-weissen f\u00fcssen", "tokens": ["Es", "gien\u00b7ge", "Cas\u00b7ta\u00b7lis", "mit", "per\u00b7len\u00b7weis\u00b7sen", "f\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jm garten hin und her die blumen zu begiessen/", "tokens": ["Jm", "gar\u00b7ten", "hin", "und", "her", "die", "blu\u00b7men", "zu", "be\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "PTKVZ", "KON", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bey meinem garten-hau\u00df an einer sch\u00f6nen stell", "tokens": ["Bey", "mei\u00b7nem", "gar\u00b7ten\u00b7hau\u00df", "an", "ei\u00b7ner", "sch\u00f6\u00b7nen", "stell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Entsprunge Hippocren/ die hell und schnelle quell/", "tokens": ["Ent\u00b7sprun\u00b7ge", "Hip\u00b7po\u00b7cren", "/", "die", "hell", "und", "schnel\u00b7le", "quell", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ART", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mit murmeindem ger\u00e4usch und f\u00fchrte gold im sande/", "tokens": ["Mit", "mur\u00b7mein\u00b7dem", "ge\u00b7r\u00e4usch", "und", "f\u00fchr\u00b7te", "gold", "im", "san\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "KON", "VVFIN", "NN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es stunden rosen-p\u00fcsch an ihrem gr\u00fcnen strande/", "tokens": ["Es", "stun\u00b7den", "ro\u00b7sen\u00b7p\u00fcsch", "an", "ih\u00b7rem", "gr\u00fc\u00b7nen", "stran\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In selber wohnete der helle wiederschall/", "tokens": ["In", "sel\u00b7ber", "woh\u00b7ne\u00b7te", "der", "hel\u00b7le", "wie\u00b7der\u00b7schall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit dem besprachte sich die laute nachtigal.", "tokens": ["Mit", "dem", "be\u00b7sprach\u00b7te", "sich", "die", "lau\u00b7te", "nach\u00b7ti\u00b7gal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der garten/ wie gedacht/ war \u00fcberall geschm\u00fccket/", "tokens": ["Der", "gar\u00b7ten", "/", "wie", "ge\u00b7dacht", "/", "war", "\u00fc\u00b7be\u00b7rall", "ge\u00b7schm\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PWAV", "VVPP", "$(", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit sch\u00f6nen blumen hier/ mit kr\u00e4utern dort gesticket/", "tokens": ["Mit", "sch\u00f6\u00b7nen", "blu\u00b7men", "hier", "/", "mit", "kr\u00e4u\u00b7tern", "dort", "ge\u00b7sti\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$(", "APPR", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein\u2019 augen haben stets/ was sie begehrt/ geschaut/", "tokens": ["Mein'", "au\u00b7gen", "ha\u00b7ben", "stets", "/", "was", "sie", "be\u00b7gehrt", "/", "ge\u00b7schaut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "PWS", "PPER", "VVPP", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nur keine m\u00fcntze nicht und tausend-g\u00fclden kraut.", "tokens": ["Nur", "kei\u00b7ne", "m\u00fcnt\u00b7ze", "nicht", "und", "tau\u00b7sen\u00b7dg\u00fcl\u00b7den", "kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Ph\u00f6bus hatte sich auf beyde nicht beflissen/", "tokens": ["Mein", "Ph\u00f6\u00b7bus", "hat\u00b7te", "sich", "auf", "bey\u00b7de", "nicht", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "APPR", "PIS", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Auch nicht auf l\u00f6ffel-kraut und nichtige narcissen/", "tokens": ["Auch", "nicht", "auf", "l\u00f6f\u00b7fel\u00b7kraut", "und", "nich\u00b7ti\u00b7ge", "nar\u00b7cis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NE", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Weil sie so fl\u00fcchtig seyn. Zu selber sch\u00f6nen zeit", "tokens": ["Weil", "sie", "so", "fl\u00fcch\u00b7tig", "seyn", ".", "Zu", "sel\u00b7ber", "sch\u00f6\u00b7nen", "zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAINF", "$.", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wust ich von keiner angst/ von keinem hertzen-leid.", "tokens": ["Wust", "ich", "von", "kei\u00b7ner", "angst", "/", "von", "kei\u00b7nem", "hert\u00b7zen\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "VVPP", "$(", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich sah\u2019 den ehren prei\u00df in seiner ehre sitzen/", "tokens": ["Ich", "sah'", "den", "eh\u00b7ren", "prei\u00df", "in", "sei\u00b7ner", "eh\u00b7re", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die hohe sonnen-blum als eine sonne blitzen/", "tokens": ["Die", "ho\u00b7he", "son\u00b7nen\u00b7blum", "als", "ei\u00b7ne", "son\u00b7ne", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die kecke lilje stund in ihrer gelben glut/", "tokens": ["Die", "ke\u00b7cke", "lil\u00b7je", "stund", "in", "ih\u00b7rer", "gel\u00b7ben", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die raut in gra\u00df-schmaragd/ die ros\u2019 in milch und blut.", "tokens": ["Die", "raut", "in", "gra\u00df\u00b7schma\u00b7ragd", "/", "die", "ros'", "in", "milch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$(", "ART", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich sah den nelcken-stock sein purpur-kleid empfangen/", "tokens": ["Ich", "sah", "den", "nel\u00b7cken\u00b7stock", "sein", "pur\u00b7pur\u00b7kleid", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mein sch\u00f6nes tausend-sch\u00f6n vor tausend blumen prangen/", "tokens": ["Mein", "sch\u00f6\u00b7nes", "tau\u00b7sen\u00b7dsch\u00f6n", "vor", "tau\u00b7send", "blu\u00b7men", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und meinte/ da\u00df mein hertz/ das damahls war im meer", "tokens": ["Und", "mein\u00b7te", "/", "da\u00df", "mein", "hertz", "/", "das", "da\u00b7mahls", "war", "im", "meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "$(", "PDS", "ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Voll unersch\u00f6pffter lust/ nicht zu betr\u00fcben w\u00e4r.", "tokens": ["Voll", "un\u00b7er\u00b7sch\u00f6pff\u00b7ter", "lust", "/", "nicht", "zu", "be\u00b7tr\u00fc\u00b7ben", "w\u00e4r", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$(", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nun aber hat die zeit/ mit der sich alles endet/", "tokens": ["Nun", "a\u00b7ber", "hat", "die", "zeit", "/", "mit", "der", "sich", "al\u00b7les", "en\u00b7det", "/"], "token_info": ["word", "word", "word", "word"