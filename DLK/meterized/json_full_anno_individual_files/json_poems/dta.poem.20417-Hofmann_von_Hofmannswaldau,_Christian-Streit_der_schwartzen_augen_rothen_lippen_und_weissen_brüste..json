{"dta.poem.20417": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr augen thut gemach/ kan euer plitz entz\u00fcnden/", "tokens": ["Ihr", "au\u00b7gen", "thut", "ge\u00b7mach", "/", "kan", "eu\u00b7er", "plitz", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$(", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So denckt/ da\u00df auch der mund voll glut und feuer steckt;", "tokens": ["So", "denckt", "/", "da\u00df", "auch", "der", "mund", "voll", "glut", "und", "feu\u00b7er", "steckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "ADV", "ART", "NN", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das rothe/ was sich will in diesen lippen finden/", "tokens": ["Das", "ro\u00b7the", "/", "was", "sich", "will", "in", "die\u00b7sen", "lip\u00b7pen", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PWS", "PRF", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist brand von reiner art mit rosen \u00fcberdeckt.", "tokens": ["Ist", "brand", "von", "rei\u00b7ner", "art", "mit", "ro\u00b7sen", "\u00fc\u00b7berd\u00b7eckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der athem/ so itzund aus diesem thale f\u00e4hret/", "tokens": ["Der", "at\u00b7hem", "/", "so", "it\u00b7zund", "aus", "die\u00b7sem", "tha\u00b7le", "f\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Laufft jagens halber aus/ und rennet durch die welt.", "tokens": ["Laufft", "ja\u00b7gens", "hal\u00b7ber", "aus", "/", "und", "ren\u00b7net", "durch", "die", "welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPO", "APPR", "$(", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich schwere/ da\u00df er nicht von dar zur\u00fccke kehret/", "tokens": ["Ich", "schwe\u00b7re", "/", "da\u00df", "er", "nicht", "von", "dar", "zu\u00b7r\u00fc\u00b7cke", "keh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "APPR", "PTKVZ", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bi\u00df da\u00df er einen geist hat in das garn gef\u00e4llt.", "tokens": ["Bi\u00df", "da\u00df", "er", "ei\u00b7nen", "geist", "hat", "in", "das", "garn", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}