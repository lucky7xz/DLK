{"dta.poem.11853": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gew\u00f6hnen will dich, was du siehest hier vom Sch\u00f6nen,", "tokens": ["Ge\u00b7w\u00f6h\u00b7nen", "will", "dich", ",", "was", "du", "sie\u00b7hest", "hier", "vom", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gew\u00f6hnen, was du h\u00f6rst vom Sch\u00f6nen hier in T\u00f6nen,", "tokens": ["Ge\u00b7w\u00f6h\u00b7nen", ",", "was", "du", "h\u00f6rst", "vom", "Sch\u00f6\u00b7nen", "hier", "in", "T\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "APPRART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gew\u00f6hnen deinen Sinn, stets h\u00f6her sich zu lenken,", "tokens": ["Ge\u00b7w\u00f6h\u00b7nen", "dei\u00b7nen", "Sinn", ",", "stets", "h\u00f6\u00b7her", "sich", "zu", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das h\u00f6chste Sch\u00f6ne selbst zu f\u00fchlen und zu denken,", "tokens": ["Das", "h\u00f6chs\u00b7te", "Sch\u00f6\u00b7ne", "selbst", "zu", "f\u00fch\u00b7len", "und", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das, ungesehn dem Aug', und ungeh\u00f6rt den Ohren,", "tokens": ["Das", ",", "un\u00b7ge\u00b7sehn", "dem", "Aug'", ",", "und", "un\u00b7ge\u00b7h\u00f6rt", "den", "Oh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist f\u00fcr den Weisen da, und nicht da f\u00fcr den Thoren.", "tokens": ["Ist", "f\u00fcr", "den", "Wei\u00b7sen", "da", ",", "und", "nicht", "da", "f\u00fcr", "den", "Tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}