{"textgrid.poem.34915": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche Fahrt", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wogte das Meer, aus dem dunklen Gew\u00f6lk", "tokens": ["Es", "wog\u00b7te", "das", "Meer", ",", "aus", "dem", "dunk\u00b7len", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Halbmond lugte scheu;", "tokens": ["Der", "Halb\u00b7mond", "lug\u00b7te", "scheu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und als wir stiegen in den Kahn,", "tokens": ["Und", "als", "wir", "stie\u00b7gen", "in", "den", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir waren unsrer drei.", "tokens": ["Wir", "wa\u00b7ren", "uns\u00b7rer", "drei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es pl\u00e4tschert' im Wasser des Ruderschlags", "tokens": ["Es", "pl\u00e4t\u00b7schert'", "im", "Was\u00b7ser", "des", "Ru\u00b7der\u00b7schlags"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Verdrossenes Einerlei;", "tokens": ["Ver\u00b7dros\u00b7se\u00b7nes", "Ei\u00b7ner\u00b7lei", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wei\u00dfsch\u00e4umende Wellen rauschten heran,", "tokens": ["Wei\u00df\u00b7sch\u00e4u\u00b7men\u00b7de", "Wel\u00b7len", "rauschten", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bespritzten uns alle drei.", "tokens": ["Be\u00b7spritz\u00b7ten", "uns", "al\u00b7le", "drei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "CARD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie stand im Kahn so bla\u00df, so schlank,", "tokens": ["Sie", "stand", "im", "Kahn", "so", "bla\u00df", ",", "so", "schlank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unbeweglich dabei,", "tokens": ["Und", "un\u00b7be\u00b7weg\u00b7lich", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als w\u00e4r sie ein welsches Marmorbild,", "tokens": ["Als", "w\u00e4r", "sie", "ein", "wel\u00b7sches", "Mar\u00b7mor\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dianens Konterfei.", "tokens": ["Di\u00b7a\u00b7nens", "Kon\u00b7ter\u00b7fei", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Mond verbirgt sich ganz. Es pfeift", "tokens": ["Der", "Mond", "ver\u00b7birgt", "sich", "ganz", ".", "Es", "pfeift"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nachtwind kalt vorbei;", "tokens": ["Der", "Nacht\u00b7wind", "kalt", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch \u00fcber unsern H\u00e4uptern ert\u00f6nt", "tokens": ["Hoch", "\u00fc\u00b7ber", "un\u00b7sern", "H\u00e4up\u00b7tern", "er\u00b7t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Pl\u00f6tzlich ein gellender Schrei.", "tokens": ["Pl\u00f6tz\u00b7lich", "ein", "gel\u00b7len\u00b7der", "Schrei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Die wei\u00dfe, gespenstische M\u00f6we war's,", "tokens": ["Die", "wei\u00b7\u00dfe", ",", "ge\u00b7spens\u00b7ti\u00b7sche", "M\u00f6\u00b7we", "wa\u00b7r's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ob dem b\u00f6sen Schrei,", "tokens": ["Und", "ob", "dem", "b\u00f6\u00b7sen", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der schauerlich klang wie Warnungsruf,", "tokens": ["Der", "schau\u00b7er\u00b7lich", "klang", "wie", "War\u00b7nungs\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erschraken wir alle drei.", "tokens": ["Er\u00b7schra\u00b7ken", "wir", "al\u00b7le", "drei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "CARD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Bin ich im Fieber? Ist das ein Spuk", "tokens": ["Bin", "ich", "im", "Fie\u00b7ber", "?", "Ist", "das", "ein", "Spuk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$.", "VAFIN", "PDS", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der n\u00e4chtlichen Phantasei?", "tokens": ["Der", "n\u00e4cht\u00b7li\u00b7chen", "Phan\u00b7ta\u00b7sei", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00c4fft mich ein Traum? Es tr\u00e4umet mir", "tokens": ["\u00c4fft", "mich", "ein", "Traum", "?", "Es", "tr\u00e4u\u00b7met", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Grausame Narretei.", "tokens": ["Grau\u00b7sa\u00b7me", "Nar\u00b7re\u00b7tei", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Grausame Narretei! Mir tr\u00e4umt,", "tokens": ["Grau\u00b7sa\u00b7me", "Nar\u00b7re\u00b7tei", "!", "Mir", "tr\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da\u00df ich ein Heiland sei,", "tokens": ["Da\u00df", "ich", "ein", "Hei\u00b7land", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df ich tr\u00fcge das gro\u00dfe Kreuz", "tokens": ["Und", "da\u00df", "ich", "tr\u00fc\u00b7ge", "das", "gro\u00b7\u00dfe", "Kreuz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Geduldig und getreu.", "tokens": ["Ge\u00b7dul\u00b7dig", "und", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die arme Sch\u00f6nheit ist schwer bedr\u00e4ngt,", "tokens": ["Die", "ar\u00b7me", "Sch\u00f6n\u00b7heit", "ist", "schwer", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich aber mache sie frei", "tokens": ["Ich", "a\u00b7ber", "ma\u00b7che", "sie", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Von Schmach und S\u00fcnde, von Qual und Not,", "tokens": ["Von", "Schmach", "und", "S\u00fcn\u00b7de", ",", "von", "Qual", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von der Welt Unfl\u00e4terei.", "tokens": ["Von", "der", "Welt", "Un\u00b7fl\u00e4\u00b7te\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Du arme Sch\u00f6nheit, schaudre nicht", "tokens": ["Du", "ar\u00b7me", "Sch\u00f6n\u00b7heit", ",", "schaud\u00b7re", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl ob der bittern Arznei;", "tokens": ["Wohl", "ob", "der", "bit\u00b7tern", "Arz\u00b7nei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich selber kredenze dir den Tod,", "tokens": ["Ich", "sel\u00b7ber", "kre\u00b7den\u00b7ze", "dir", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bricht auch mein Herz entzwei.", "tokens": ["Bricht", "auch", "mein", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O Narretei, grausamer Traum,", "tokens": ["O", "Nar\u00b7re\u00b7tei", ",", "grau\u00b7sa\u00b7mer", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wahnsinn und Raserei!", "tokens": ["Wahn\u00b7sinn", "und", "Ra\u00b7se\u00b7rei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es g\u00e4hnt die Nacht, es kreischt das Meer,", "tokens": ["Es", "g\u00e4hnt", "die", "Nacht", ",", "es", "kreischt", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Gott! o steh mir bei!", "tokens": ["O", "Gott", "!", "o", "steh", "mir", "bei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O steh mir bei, barmherziger Gott!", "tokens": ["O", "steh", "mir", "bei", ",", "barm\u00b7her\u00b7zi\u00b7ger", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Barmherziger Gott Schaddey!", "tokens": ["Barm\u00b7her\u00b7zi\u00b7ger", "Gott", "Schad\u00b7dey", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da schollert's hinab ins Meer \u2013 O Weh \u2013", "tokens": ["Da", "schol\u00b7lert's", "hin\u00b7ab", "ins", "Meer", "\u2013", "O", "Weh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$(", "NE", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schaddey! Schaddey! Adonay! \u2013", "tokens": ["Schad\u00b7dey", "!", "Schad\u00b7dey", "!", "A\u00b7do\u00b7nay", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$.", "NN", "$.", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Die Sonne ging auf, wir fuhren ans Land,", "tokens": ["Die", "Son\u00b7ne", "ging", "auf", ",", "wir", "fuh\u00b7ren", "ans", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da bl\u00fchte und gl\u00fchte der Mai!", "tokens": ["Da", "bl\u00fch\u00b7te", "und", "gl\u00fch\u00b7te", "der", "Mai", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und als wir stiegen aus dem Kahn,", "tokens": ["Und", "als", "wir", "stie\u00b7gen", "aus", "dem", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da waren wir unsrer ", "tokens": ["Da", "wa\u00b7ren", "wir", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Es wogte das Meer, aus dem dunklen Gew\u00f6lk", "tokens": ["Es", "wog\u00b7te", "das", "Meer", ",", "aus", "dem", "dunk\u00b7len", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Halbmond lugte scheu;", "tokens": ["Der", "Halb\u00b7mond", "lug\u00b7te", "scheu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und als wir stiegen in den Kahn,", "tokens": ["Und", "als", "wir", "stie\u00b7gen", "in", "den", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir waren unsrer drei.", "tokens": ["Wir", "wa\u00b7ren", "uns\u00b7rer", "drei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Es pl\u00e4tschert' im Wasser des Ruderschlags", "tokens": ["Es", "pl\u00e4t\u00b7schert'", "im", "Was\u00b7ser", "des", "Ru\u00b7der\u00b7schlags"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Verdrossenes Einerlei;", "tokens": ["Ver\u00b7dros\u00b7se\u00b7nes", "Ei\u00b7ner\u00b7lei", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wei\u00dfsch\u00e4umende Wellen rauschten heran,", "tokens": ["Wei\u00df\u00b7sch\u00e4u\u00b7men\u00b7de", "Wel\u00b7len", "rauschten", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bespritzten uns alle drei.", "tokens": ["Be\u00b7spritz\u00b7ten", "uns", "al\u00b7le", "drei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "CARD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Sie stand im Kahn so bla\u00df, so schlan