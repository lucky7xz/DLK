{"textgrid.poem.54292": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welt, behalte deine Freude,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welt, behalte deine Freude,", "tokens": ["Welt", ",", "be\u00b7hal\u00b7te", "dei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich erg\u00f6tzt des Creutzes Stamm,", "tokens": ["Mich", "er\u00b7g\u00f6tzt", "des", "Creut\u00b7zes", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da h\u00e4ngt meiner Augen Weyde,", "tokens": ["Da", "h\u00e4ngt", "mei\u00b7ner", "Au\u00b7gen", "Wey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das gedultge Gottes Lamm,", "tokens": ["Das", "ge\u00b7dult\u00b7ge", "Got\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das erb\u00e4rmlich zugericht,", "tokens": ["Das", "er\u00b7b\u00e4rm\u00b7lich", "zu\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil es S\u00fcnd und Schuld geschlicht.", "tokens": ["Weil", "es", "S\u00fcnd", "und", "Schuld", "ge\u00b7schlicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was die gantze Welt begangen,", "tokens": ["Was", "die", "gant\u00b7ze", "Welt", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird der Unschuld aufgehangen.", "tokens": ["Wird", "der", "Un\u00b7schuld", "auf\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaue, S\u00fcnder, was vor Schmertzen", "tokens": ["Schau\u00b7e", ",", "S\u00fcn\u00b7der", ",", "was", "vor", "Schmert\u00b7zen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ertr\u00e4gt es nicht vor Pein!", "tokens": ["Was", "er\u00b7tr\u00e4gt", "es", "nicht", "vor", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rinnt das Blut nicht aus den Hertzen,", "tokens": ["Rinnt", "das", "Blut", "nicht", "aus", "den", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie mu\u00df seiner Seele seyn?", "tokens": ["Wie", "mu\u00df", "sei\u00b7ner", "See\u00b7le", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O! wie \u00e4chtzt der Lebens-F\u00fcrst,", "tokens": ["O", "!", "wie", "\u00e4chtzt", "der", "Le\u00b7bens\u00b7F\u00fcrst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der, so hefftig ihn auch d\u00fcrst,", "tokens": ["Der", ",", "so", "heff\u00b7tig", "ihn", "auch", "d\u00fcrst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kaum kan, seinen Geist zu laben,", "tokens": ["Kaum", "kan", ",", "sei\u00b7nen", "Geist", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine Hand voll E\u00dfig haben.", "tokens": ["Ei\u00b7ne", "Hand", "voll", "E\u00b7\u00dfig", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df dich dieses doch bewegen,", "tokens": ["La\u00df", "dich", "die\u00b7ses", "doch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fleuch der Wollust Rosenthal,", "tokens": ["Fleuch", "der", "Wol\u00b7lust", "Ro\u00b7sen\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann sich Fleisch und Blut will regen,", "tokens": ["Wann", "sich", "Fleisch", "und", "Blut", "will", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "VMFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So bedencke seine Quaal.", "tokens": ["So", "be\u00b7den\u00b7cke", "sei\u00b7ne", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die N\u00e4gel scharff gespitzt", "tokens": ["Wie", "die", "N\u00e4\u00b7gel", "scharff", "ge\u00b7spitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm so Hand als Fu\u00df durchritzt,", "tokens": ["Ihm", "so", "Hand", "als", "Fu\u00df", "durc\u00b7hritzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die D\u00f6rner den zerstochen,", "tokens": ["Wie", "die", "D\u00f6r\u00b7ner", "den", "zer\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der nichts straffbares verbrochen.", "tokens": ["Der", "nichts", "straff\u00b7ba\u00b7res", "ver\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJA", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Schaue, was vor Striem und Strahlen", "tokens": ["Schau\u00b7e", ",", "was", "vor", "Striem", "und", "Strah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Findet man um Stirn und Haupt:", "tokens": ["Fin\u00b7det", "man", "um", "Stirn", "und", "Haupt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df der Heyland selbst bezahlen,", "tokens": ["Mu\u00df", "der", "Hey\u00b7land", "selbst", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was er niemahls hat geraubt;", "tokens": ["Was", "er", "nie\u00b7mahls", "hat", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O! wie wird es dir ergehn?", "tokens": ["O", "!", "wie", "wird", "es", "dir", "er\u00b7gehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was hast du nun auszustehn,", "tokens": ["Was", "hast", "du", "nun", "aus\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wenn die Rache dich l\u00e4st kommen,", "tokens": ["Wenn", "die", "Ra\u00b7che", "dich", "l\u00e4st", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die dein Schuld-Buch vorgenommen.", "tokens": ["Die", "dein", "Schuld\u00b7Buch", "vor\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch verbanne Furcht und Zweifel,", "tokens": ["Doch", "ver\u00b7ban\u00b7ne", "Furcht", "und", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn die Schuld ist abgethan,", "tokens": ["Denn", "die", "Schuld", "ist", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So da\u00df Rache, Tod, und Teufel", "tokens": ["So", "da\u00df", "Ra\u00b7che", ",", "Tod", ",", "und", "Teu\u00b7fel"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "KOUS", "NN", "$,", "NN", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich an dir nicht k\u00fchlen kan.", "tokens": ["Sich", "an", "dir", "nicht", "k\u00fch\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sein geheilgtes Blut und G\u00f6scht,", "tokens": ["Sein", "ge\u00b7heilg\u00b7tes", "Blut", "und", "G\u00f6scht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat die Handschrifft ausgel\u00f6scht,", "tokens": ["Hat", "die", "Hand\u00b7schrifft", "aus\u00b7ge\u00b7l\u00f6scht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die dereinst nach deinen Tode", "tokens": ["Die", "de\u00b7reinst", "nach", "dei\u00b7nen", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem ewgen Kercker drohte.", "tokens": ["Mit", "dem", "ew\u00b7gen", "Ker\u00b7cker", "droh\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Welt, behalte deine Freude,", "tokens": ["Welt", ",", "be\u00b7hal\u00b7te", "dei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich erg\u00f6tzt des Creutzes Stamm,", "tokens": ["Mich", "er\u00b7g\u00f6tzt", "des", "Creut\u00b7zes", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da h\u00e4ngt meiner Augen Weyde,", "tokens": ["Da", "h\u00e4ngt", "mei\u00b7ner", "Au\u00b7gen", "Wey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das gedultge Gottes Lamm,", "tokens": ["Das", "ge\u00b7dult\u00b7ge", "Got\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das erb\u00e4rmlich zugericht,", "tokens": ["Das", "er\u00b7b\u00