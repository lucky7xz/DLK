{"dta.poem.19322": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wahrheit ist durchaus ein mittleres Gebiet,", "tokens": ["Die", "Wahr\u00b7heit", "ist", "durc\u00b7haus", "ein", "mitt\u00b7le\u00b7res", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+++-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Das nicht nach hier und dort unendlich hin sich zieht.", "tokens": ["Das", "nicht", "nach", "hier", "und", "dort", "un\u00b7end\u00b7lich", "hin", "sich", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "ADV", "KON", "ADV", "ADJD", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr nachgehn kanst du meist gar wenig Schritte nur,", "tokens": ["Ihr", "nach\u00b7gehn", "kanst", "du", "meist", "gar", "we\u00b7nig", "Schrit\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ausgehn siehst du schon in Irrthum ihre Spur.", "tokens": ["Und", "aus\u00b7gehn", "siehst", "du", "schon", "in", "Irr\u00b7thum", "ih\u00b7re", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "VVFIN", "PPER", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wahrheiten h\u00e4ngen nicht zusammen wie Korallen,", "tokens": ["Wahr\u00b7hei\u00b7ten", "h\u00e4n\u00b7gen", "nicht", "zu\u00b7sam\u00b7men", "wie", "Ko\u00b7ral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die man kann an der Schnur herz\u00e4hlen nach Gefallen.", "tokens": ["Die", "man", "kann", "an", "der", "Schnur", "her\u00b7z\u00e4h\u00b7len", "nach", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Oft ist das Wahre gar vom Falschen nicht zu scheiden,", "tokens": ["Oft", "ist", "das", "Wah\u00b7re", "gar", "vom", "Fal\u00b7schen", "nicht", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADV", "APPRART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie F\u00e4den eines Zeugs, halb wollen und halb seiden.", "tokens": ["Wie", "F\u00e4\u00b7den", "ei\u00b7nes", "Zeugs", ",", "halb", "wol\u00b7len", "und", "halb", "sei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "$,", "ADJD", "VMFIN", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Von Wahrheit einen Kern schlie\u00dft jeder Irrthum ein,", "tokens": ["Von", "Wahr\u00b7heit", "ei\u00b7nen", "Kern", "schlie\u00dft", "je\u00b7der", "Irr\u00b7thum", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jede Wahrheit kann des Irrthums Saame seyn.", "tokens": ["Und", "je\u00b7de", "Wahr\u00b7heit", "kann", "des", "Irr\u00b7thums", "Saa\u00b7me", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vor allem h\u00fcte dich vor strengen Folgerungen,", "tokens": ["Vor", "al\u00b7lem", "h\u00fc\u00b7te", "dich", "vor", "stren\u00b7gen", "Fol\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn folgerichtig ist meist n\u00e4rrischstes entsprungen.", "tokens": ["Denn", "fol\u00b7ge\u00b7rich\u00b7tig", "ist", "meist", "n\u00e4r\u00b7rischs\u00b7tes", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Wahrheit, die du zuweit verfolgen willst und jagen,", "tokens": ["Wahr\u00b7heit", ",", "die", "du", "zu\u00b7weit", "ver\u00b7fol\u00b7gen", "willst", "und", "ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist, eh du dichs versiehst, in Irrthum umgeschlagen.", "tokens": ["Ist", ",", "eh", "du", "dichs", "ver\u00b7siehst", ",", "in", "Irr\u00b7thum", "um\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Viel lieber mag, anstatt die Jagd zu \u00fcbertreiben,", "tokens": ["Viel", "lie\u00b7ber", "mag", ",", "an\u00b7statt", "die", "Jagd", "zu", "\u00fc\u00b7bert\u00b7rei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ungewisses Wild im Grenzwald \u00fcberbleiben.", "tokens": ["Ein", "un\u00b7ge\u00b7wis\u00b7ses", "Wild", "im", "Grenz\u00b7wald", "\u00fc\u00b7berb\u00b7lei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der Sch\u00fctze l\u00e4\u00dft, was flieht, fliehn an der Grenz', und zieht", "tokens": ["Der", "Sch\u00fct\u00b7ze", "l\u00e4\u00dft", ",", "was", "flieht", ",", "fliehn", "an", "der", "Grenz'", ",", "und", "zieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit seiner Beute sich zur\u00fcck auf sein Gebiet.", "tokens": ["Mit", "sei\u00b7ner", "Beu\u00b7te", "sich", "zu\u00b7r\u00fcck", "auf", "sein", "Ge\u00b7biet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}