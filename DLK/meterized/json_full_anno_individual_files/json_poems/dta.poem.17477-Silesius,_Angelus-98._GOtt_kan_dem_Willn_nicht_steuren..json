{"dta.poem.17477": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "98.  GOtt kan dem Willn nicht steuren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts st\u00e4rkers ist als GOtt: doch kan er nicht verweh-", "tokens": ["Nichts", "st\u00e4r\u00b7kers", "ist", "als", "Gott", ":", "doch", "kan", "er", "nicht", "ver\u00b7weh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "VAFIN", "KOUS", "NN", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Da\u00df ich nicht was ich wil sol wollen und begehren.", "tokens": ["Da\u00df", "ich", "nicht", "was", "ich", "wil", "sol", "wol\u00b7len", "und", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PWS", "PPER", "VMFIN", "ADV", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}