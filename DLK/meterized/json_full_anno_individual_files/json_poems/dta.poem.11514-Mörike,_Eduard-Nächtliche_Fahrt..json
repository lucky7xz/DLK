{"dta.poem.11514": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche Fahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.85", "da:0.14"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber fremdes Heideland;", "tokens": ["Ue\u00b7ber", "frem\u00b7des", "Hei\u00b7de\u00b7land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor den halbverschloss'nen Wagen", "tokens": ["Vor", "den", "halb\u00b7ver\u00b7schloss'\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schien ein Trauerzug gespannt.", "tokens": ["Schien", "ein", "Trau\u00b7er\u00b7zug", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann durch mondbegl\u00e4nzte W\u00e4lder", "tokens": ["Dann", "durch", "mond\u00b7be\u00b7gl\u00e4nz\u00b7te", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ging die sonderbare Fahrt,", "tokens": ["Ging", "die", "son\u00b7der\u00b7ba\u00b7re", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der Anblick offner Felder", "tokens": ["Bis", "der", "An\u00b7blick", "off\u00b7ner", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich mir bekannter ward.", "tokens": ["End\u00b7lich", "mir", "be\u00b7kann\u00b7ter", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie im lustigen Gewimmel", "tokens": ["Wie", "im", "lus\u00b7ti\u00b7gen", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzt nun Busch und Baum vorbei!", "tokens": ["Tanzt", "nun", "Busch", "und", "Baum", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Dorf nun! Guter Himmel!", "tokens": ["Und", "ein", "Dorf", "nun", "!", "Gu\u00b7ter", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O mir ahnet, was es sey.", "tokens": ["O", "mir", "ah\u00b7net", ",", "was", "es", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sah ich doch vor Zeiten gerne", "tokens": ["Sah", "ich", "doch", "vor", "Zei\u00b7ten", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese H\u00e4user oft und viel,", "tokens": ["Die\u00b7se", "H\u00e4u\u00b7ser", "oft", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die am Wagen die Laterne", "tokens": ["Die", "am", "Wa\u00b7gen", "die", "La\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Streift im stummen Schattenspiel.", "tokens": ["Streift", "im", "stum\u00b7men", "Schat\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, dort unterm Giebeldache", "tokens": ["Ja", ",", "dort", "un\u00b7term", "Gie\u00b7bel\u00b7da\u00b7che"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlummerst du, verge\u00dflich Herz!", "tokens": ["Schlum\u00b7merst", "du", ",", "ver\u00b7ge\u00df\u00b7lich", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da\u00df dein Getreuer wache,", "tokens": ["Und", "da\u00df", "dein", "Ge\u00b7treu\u00b7er", "wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt dir kein geheimer Schmerz.", "tokens": ["Sagt", "dir", "kein", "ge\u00b7hei\u00b7mer", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u2014 Ferne waren schon die H\u00fctten;", "tokens": ["Fer\u00b7ne", "wa\u00b7ren", "schon", "die", "H\u00fct\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh', da flattert's durch den Wind!", "tokens": ["Sieh'", ",", "da", "flat\u00b7tert's", "durch", "den", "Wind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Gabe zu erbitten", "tokens": ["Ei\u00b7ne", "Ga\u00b7be", "zu", "er\u00b7bit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schien ein armes, holdes Kind.", "tokens": ["Schien", "ein", "ar\u00b7mes", ",", "hol\u00b7des", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie vom b\u00f6sen Geist getrieben,", "tokens": ["Wie", "vom", "b\u00f6\u00b7sen", "Geist", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werf' ich rasch der Bettlerin", "tokens": ["Wer\u00b7f'", "ich", "rasch", "der", "Bett\u00b7le\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Geschenk von meiner Lieben,", "tokens": ["Ein", "Ge\u00b7schenk", "von", "mei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene goldne Kette, hin.", "tokens": ["Je\u00b7ne", "gold\u00b7ne", "Ket\u00b7te", ",", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Pl\u00f6tzlich scheint ein Rad gebunden,", "tokens": ["Pl\u00f6tz\u00b7lich", "scheint", "ein", "Rad", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Wagen steht gebannt,", "tokens": ["Und", "der", "Wa\u00b7gen", "steht", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und das holde M\u00e4dchen unten", "tokens": ["Und", "das", "hol\u00b7de", "M\u00e4d\u00b7chen", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt mich schelmisch bei der Hand.", "tokens": ["H\u00e4lt", "mich", "schel\u00b7misch", "bei", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u201edenkt man so damit zu schalten?", "tokens": ["\u201e", "denkt", "man", "so", "da\u00b7mit", "zu", "schal\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So entdeck' ich den Betrug?", "tokens": ["So", "ent\u00b7deck'", "ich", "den", "Be\u00b7trug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, den Wagen festzuhalten,", "tokens": ["Doch", ",", "den", "Wa\u00b7gen", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "War die Kette stark genug.", "tokens": ["War", "die", "Ket\u00b7te", "stark", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Willst du, da\u00df ich dir verzeihe,", "tokens": ["Willst", "du", ",", "da\u00df", "ich", "dir", "ver\u00b7zei\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey erst selber wieder gut!", "tokens": ["Sey", "erst", "sel\u00b7ber", "wie\u00b7der", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder wo ist deine Treue,", "tokens": ["O\u00b7der", "wo", "ist", "dei\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Falsches Herze, falsches Blut?\u201c", "tokens": ["Fal\u00b7sches", "Her\u00b7ze", ",", "fal\u00b7sches", "Blut", "?", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "VVFIN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und sie streichelt mir die Wange,", "tokens": ["Und", "sie", "strei\u00b7chelt", "mir", "die", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fc\u00dft mir das erfrorne Kinn,", "tokens": ["K\u00fc\u00dft", "mir", "das", "er\u00b7fror\u00b7ne", "Kinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht und l\u00e4chelt, weinet lange", "tokens": ["Steht", "und", "l\u00e4\u00b7chelt", ",", "wei\u00b7net", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die sch\u00f6nste B\u00fc\u00dferin.", "tokens": ["Als", "die", "sch\u00f6ns\u00b7te", "B\u00fc\u00b7\u00dfe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Doch mir bleibt der Mund verschlossen,", "tokens": ["Doch", "mir", "bleibt", "der", "Mund", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und kaum wei\u00df ich, was geschehn;", "tokens": ["Und", "kaum", "wei\u00df", "ich", ",", "was", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PRELS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz in ihren Arm gegossen,", "tokens": ["Ganz", "in", "ih\u00b7ren", "Arm", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schien ich selig zu vergehn.", "tokens": ["Schien", "ich", "se\u00b7lig", "zu", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und nun fliegt mit uns, ihr Pferde,", "tokens": ["Und", "nun", "fliegt", "mit", "uns", ",", "ihr", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die graue Welt hinein!", "tokens": ["In", "die", "grau\u00b7e", "Welt", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter uns vergeh' die Erde", "tokens": ["Un\u00b7ter", "uns", "ver\u00b7geh'", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Morgen soll mehr seyn!", "tokens": ["Und", "kein", "Mor\u00b7gen", "soll", "mehr", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}