{"dta.poem.19193": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kropfgans schlingt den Fisch hinein auf Einen Schluck;", "tokens": ["Die", "Kropf\u00b7gans", "schlingt", "den", "Fisch", "hin\u00b7ein", "auf", "Ei\u00b7nen", "Schluck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es fehlt ihr der Geschmack, sie f\u00fchlet nur den Druck.", "tokens": ["Es", "fehlt", "ihr", "der", "Ge\u00b7schmack", ",", "sie", "f\u00fch\u00b7let", "nur", "den", "Druck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Schlemmer aber mag in kleinen Bissen kauen,", "tokens": ["Ein", "Schlem\u00b7mer", "a\u00b7ber", "mag", "in", "klei\u00b7nen", "Bis\u00b7sen", "kau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die W\u00e4rzchen des Geschmacks and\u00e4chtig zu erbauen.", "tokens": ["Die", "W\u00e4rz\u00b7chen", "des", "Ge\u00b7schmacks", "an\u00b7d\u00e4ch\u00b7tig", "zu", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So wenig jene mir, gef\u00e4llt mir diese Sitte;", "tokens": ["So", "we\u00b7nig", "je\u00b7ne", "mir", ",", "ge\u00b7f\u00e4llt", "mir", "die\u00b7se", "Sit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "PPER", "$,", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich rathe dir auch hier wie \u00fcberall die Mitte.", "tokens": ["Ich", "ra\u00b7the", "dir", "auch", "hier", "wie", "\u00fc\u00b7be\u00b7rall", "die", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}