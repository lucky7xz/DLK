{"textgrid.poem.49124": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "An die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonne, helles Aug der Welt,", "tokens": ["Son\u00b7ne", ",", "hel\u00b7les", "Aug", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6chster Schmuck vom Himmels-Zelt,", "tokens": ["H\u00f6chs\u00b7ter", "Schmuck", "vom", "Him\u00b7mels\u00b7Zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wenn Alles noch so sch\u00f6n,", "tokens": ["Denn", "wenn", "Al\u00b7les", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was w\u00e4rs, w\u00e4r' es nicht zu sehn?", "tokens": ["Was", "w\u00e4rs", ",", "w\u00e4r'", "es", "nicht", "zu", "sehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Keinen Gleichen hastu hier,", "tokens": ["Kei\u00b7nen", "Glei\u00b7chen", "has\u00b7tu", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Vorzug bleibet dir,", "tokens": ["Die\u00b7ser", "Vor\u00b7zug", "blei\u00b7bet", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ist noch ein' andre Sonn,", "tokens": ["Doch", "ist", "noch", "ein'", "and\u00b7re", "Sonn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.4": {"text": "Die von mehrer Zier und Wonn.", "tokens": ["Die", "von", "meh\u00b7rer", "Zier", "und", "Wonn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du gehst ab und wieder auff,", "tokens": ["Du", "gehst", "ab", "und", "wie\u00b7der", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so \u00e4ndert sich dein Lauff;", "tokens": ["Und", "so", "\u00e4n\u00b7dert", "sich", "dein", "Lauff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht so aber macht es die,", "tokens": ["Nicht", "so", "a\u00b7ber", "macht", "es", "die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVFIN", "PPER", "ART", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der es fr\u00fch nach Abend nie.", "tokens": ["Der", "es", "fr\u00fch", "nach", "A\u00b7bend", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Diesen auserwehlten Schein", "tokens": ["Die\u00b7sen", "au\u00b7ser\u00b7wehl\u00b7ten", "Schein"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bild' ich mir durch dich stets ein,", "tokens": ["Bild'", "ich", "mir", "durch", "dich", "stets", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wie du regierst die Zeit,", "tokens": ["Denn", "wie", "du", "re\u00b7gierst", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "F\u00fchret die die Ewigkeit.", "tokens": ["F\u00fch\u00b7ret", "die", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch dich wird der Tag gemacht,", "tokens": ["Durch", "dich", "wird", "der", "Tag", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trittstu ab, so wird es Nacht,", "tokens": ["Tritt\u00b7stu", "ab", ",", "so", "wird", "es", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du erweckst der Erden Pflicht", "tokens": ["Du", "er\u00b7weckst", "der", "Er\u00b7den", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bist Vater aller Fr\u00fccht.", "tokens": ["Und", "bist", "Va\u00b7ter", "al\u00b7ler", "Fr\u00fccht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jesus gibt ein besser Licht,", "tokens": ["Je\u00b7sus", "gibt", "ein", "bes\u00b7ser", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das nur mir die Nacht vernicht,", "tokens": ["Das", "nur", "mir", "die", "Nacht", "ver\u00b7nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, die schwere S\u00fcnden-Nacht,", "tokens": ["Ach", ",", "die", "schwe\u00b7re", "S\u00fcn\u00b7den\u00b7Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mich sonst gantz dunkel macht.", "tokens": ["Die", "mich", "sonst", "gantz", "dun\u00b7kel", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Durch ihn w\u00e4chset, gr\u00fcnt und bl\u00fcht,", "tokens": ["Durch", "ihn", "w\u00e4ch\u00b7set", ",", "gr\u00fcnt", "und", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mein Herz f\u00fcr Fr\u00fccht' erzieht,", "tokens": ["Was", "mein", "Herz", "f\u00fcr", "Fr\u00fccht'", "er\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichtes kan ich sonder ihn,", "tokens": ["Nich\u00b7tes", "kan", "ich", "son\u00b7der", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als nur mich zur H\u00f6llen ziehn.", "tokens": ["Als", "nur", "mich", "zur", "H\u00f6l\u00b7len", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dein beliebter Glantz erfreut,", "tokens": ["Dein", "be\u00b7lieb\u00b7ter", "Glantz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rmet, quickt und benedeyt,", "tokens": ["W\u00e4r\u00b7met", ",", "quickt", "und", "be\u00b7ne\u00b7deyt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So da\u00df du ohn Segen nicht,", "tokens": ["So", "da\u00df", "du", "ohn", "Se\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du gleich bew\u00f6lkst dein Licht.", "tokens": ["Wenn", "du", "gleich", "be\u00b7w\u00f6lkst", "dein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Er entz\u00fcndt in mir die Glut,", "tokens": ["Er", "ent\u00b7z\u00fcndt", "in", "mir", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mit Himmels vollem Muth", "tokens": ["Die", "mit", "Him\u00b7mels", "vol\u00b7lem", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des h\u00f6chsten Geistes Geist", "tokens": ["Und", "des", "h\u00f6chs\u00b7ten", "Geis\u00b7tes", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine schwache Seele speist.", "tokens": ["Mei\u00b7ne", "schwa\u00b7che", "See\u00b7le", "speist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ob er mich auch gleich betr\u00fcbt,", "tokens": ["Ob", "er", "mich", "auch", "gleich", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt er doch in mich verliebt,", "tokens": ["Bleibt", "er", "doch", "in", "mich", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn je h\u00e4rter er mich dr\u00fcckt,", "tokens": ["Denn", "je", "h\u00e4r\u00b7ter", "er", "mich", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Desto mehr er mich erquickt.", "tokens": ["Des\u00b7to", "mehr", "er", "mich", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ewger Auffgang vor der Zeit,", "tokens": ["Ew\u00b7ger", "Auff\u00b7gang", "vor", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonne der Gerechtigkeit,", "tokens": ["Son\u00b7ne", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gehe, Jesu, o mein Licht,", "tokens": ["Ge\u00b7he", ",", "Je\u00b7su", ",", "o", "mein", "Licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Nimmer in mir unter nicht.", "tokens": ["Nim\u00b7mer", "in", "mir", "un\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ohn dich bricht nur an der Tag,", "tokens": ["Ohn", "dich", "bricht", "nur", "an", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mein Kreutz anbrechen mag,", "tokens": ["Da\u00df", "mein", "Kreutz", "an\u00b7bre\u00b7chen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es macht der helle Schein", "tokens": ["Und", "es", "macht", "der", "hel\u00b7le", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir nur sichtbar meine Pein.", "tokens": ["Mir", "nur", "sicht\u00b7bar", "mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wirk' aus meiner H\u00e4nde Pflicht", "tokens": ["Wirk'", "aus", "mei\u00b7ner", "H\u00e4n\u00b7de", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele dir genehme Fr\u00fccht,", "tokens": ["Vie\u00b7le", "dir", "ge\u00b7neh\u00b7me", "Fr\u00fccht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leg' ich mich dann drauf zur Ruh,", "tokens": ["Leg'", "ich", "mich", "dann", "drauf", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "PAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lege du dich auch dazu.", "tokens": ["Le\u00b7ge", "du", "dich", "auch", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "W\u00e4rm mich, wenn ich liebe kalt", "tokens": ["W\u00e4rm", "mich", ",", "wenn", "ich", "lie\u00b7be", "kalt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit dir es laulicht halt,", "tokens": ["Und", "mit", "dir", "es", "lau\u00b7licht", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leucht mir, da\u00df ich zu dir geh',", "tokens": ["Leucht", "mir", ",", "da\u00df", "ich", "zu", "dir", "geh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erquick' mich, wenn mir weh.", "tokens": ["Und", "er\u00b7quick'", "mich", ",", "wenn", "mir", "weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Letzt la\u00df deines Todes Pein", "tokens": ["Letzt", "la\u00df", "dei\u00b7nes", "To\u00b7des", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Asche Leben seyn;", "tokens": ["Mei\u00b7ner", "A\u00b7sche", "Le\u00b7ben", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses bittet, ander ich,", "tokens": ["Die\u00b7ses", "bit\u00b7tet", ",", "an\u00b7der", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Sonnenwende dich.", "tokens": ["Dei\u00b7ne", "Son\u00b7nen\u00b7wen\u00b7de", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Sonne, helles Aug der Welt,", "tokens": ["Son\u00b7ne", ",", "hel\u00b7les", "Aug", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6chster Schmuck vom Himmels-Zelt,", "tokens": ["H\u00f6chs\u00b7ter", "Schmuck", "vom", "Him\u00b7mels\u00b7Zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wenn Alles noch so sch\u00f6n,", "tokens": ["Denn", "wenn", "Al\u00b7les", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was w\u00e4rs, w\u00e4r' es nicht zu sehn?", "tokens": ["Was", "w\u00e4rs", ",", "w\u00e4r'", "es", "nicht", "zu", "sehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Keinen Gleichen hastu hier,", "tokens": ["Kei\u00b7nen", "Glei\u00b7chen", "has\u00b7tu", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Vorzug bleibet dir,", "tokens": ["Die\u00b7ser", "Vor\u00b7zug", "blei\u00b7bet", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ist noch ein' andre Sonn,", "tokens": ["Doch", "ist", "noch", "ein'", "and\u00b7re", "Sonn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.4": {"text": "Die von mehrer Zier und Wonn.", "tokens": ["Die", "von", "meh\u00b7rer", "Zier", "und", "Wonn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "KON", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Du gehst ab und wieder auff,", "tokens": ["Du", "gehst", "ab", "und", "wie\u00b7der", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so \u00e4ndert sich dein Lauff;", "tokens": ["Und", "so", "\u00e4n\u00b7dert", "sich", "dein", "Lauff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht so aber macht es die,", "tokens": ["Nicht", "so", "a\u00b7ber", "macht", "es", "die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVFIN", "PPER", "ART", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der es fr\u00fch nach Abend nie.", "tokens": ["Der", "es", "fr\u00fch", "nach", "A\u00b7bend", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Diesen auserwehlten Schein", "tokens": ["Die\u00b7sen", "au\u00b7ser\u00b7wehl\u00b7ten", "Schein"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bild' ich mir durch dich stets ein,", "tokens": ["Bild'", "ich", "mir", "durch", "dich", "stets", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wie du regierst die Zeit,", "tokens": ["Denn", "wie", "du", "re\u00b7gierst", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "F\u00fchret die die Ewigkeit.", "tokens": ["F\u00fch\u00b7ret", "die", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Durch dich wird der Tag gemacht,", "tokens": ["Durch", "dich", "wird", "der", "Tag", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trittstu ab, so wird es Nacht,", "tokens": ["Tritt\u00b7stu", "ab", ",", "so", "wird", "es", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du erweckst der Erden Pflicht", "tokens": ["Du", "er\u00b7weckst", "der", "Er\u00b7den", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bist Vater aller Fr\u00fccht.", "tokens": ["Und", "bist", "Va\u00b7ter", "al\u00b7ler", "Fr\u00fccht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Jesus gibt ein besser Licht,", "tokens": ["Je\u00b7sus", "gibt", "ein", "bes\u00b7ser", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das nur mir die Nacht vernicht,", "tokens": ["Das", "nur", "mir", "die", "Nacht", "ver\u00b7nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, die schwere S\u00fcnden-Nacht,", "tokens": ["Ach", ",", "die", "schwe\u00b7re", "S\u00fcn\u00b7den\u00b7Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mich sonst gantz dunkel macht.", "tokens": ["Die", "mich", "sonst", "gantz", "dun\u00b7kel", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Durch ihn w\u00e4chset, gr\u00fcnt und bl\u00fcht,", "tokens": ["Durch", "ihn", "w\u00e4ch\u00b7set", ",", "gr\u00fcnt", "und", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mein Herz f\u00fcr Fr\u00fccht' erzieht,", "tokens": ["Was", "mein", "Herz", "f\u00fcr", "Fr\u00fccht'", "er\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichtes kan ich sonder ihn,", "tokens": ["Nich\u00b7tes", "kan", "ich", "son\u00b7der", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als nur mich zur H\u00f6llen ziehn.", "tokens": ["Als", "nur", "mich", "zur", "H\u00f6l\u00b7len", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Dein beliebter Glantz erfreut,", "tokens": ["Dein", "be\u00b7lieb\u00b7ter", "Glantz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rmet, quickt und benedeyt,", "tokens": ["W\u00e4r\u00b7met", ",", "quickt", "und", "be\u00b7ne\u00b7deyt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So da\u00df du ohn Segen nicht,", "tokens": ["So", "da\u00df", "du", "ohn", "Se\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du gleich bew\u00f6lkst dein Licht.", "tokens": ["Wenn", "du", "gleich", "be\u00b7w\u00f6lkst", "dein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Er entz\u00fcndt in mir die Glut,", "tokens": ["Er", "ent\u00b7z\u00fcndt", "in", "mir", "die", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mit Himmels vollem Muth", "tokens": ["Die", "mit", "Him\u00b7mels", "vol\u00b7lem", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des h\u00f6chsten Geistes Geist", "tokens": ["Und", "des", "h\u00f6chs\u00b7ten", "Geis\u00b7tes", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine schwache Seele speist.", "tokens": ["Mei\u00b7ne", "schwa\u00b7che", "See\u00b7le", "speist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Ob er mich auch gleich betr\u00fcbt,", "tokens": ["Ob", "er", "mich", "auch", "gleich", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt er doch in mich verliebt,", "tokens": ["Bleibt", "er", "doch", "in", "mich", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn je h\u00e4rter er mich dr\u00fcckt,", "tokens": ["Denn", "je", "h\u00e4r\u00b7ter", "er", "mich", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Desto mehr er mich erquickt.", "tokens": ["Des\u00b7to", "mehr", "er", "mich", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Ewger Auffgang vor der Zeit,", "tokens": ["Ew\u00b7ger", "Auff\u00b7gang", "vor", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonne der Gerechtigkeit,", "tokens": ["Son\u00b7ne", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gehe, Jesu, o mein Licht,", "tokens": ["Ge\u00b7he", ",", "Je\u00b7su", ",", "o", "mein", "Licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Nimmer in mir unter nicht.", "tokens": ["Nim\u00b7mer", "in", "mir", "un\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Ohn dich bricht nur an der Tag,", "tokens": ["Ohn", "dich", "bricht", "nur", "an", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mein Kreutz anbrechen mag,", "tokens": ["Da\u00df", "mein", "Kreutz", "an\u00b7bre\u00b7chen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es macht der helle Schein", "tokens": ["Und", "es", "macht", "der", "hel\u00b7le", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir nur sichtbar meine Pein.", "tokens": ["Mir", "nur", "sicht\u00b7bar", "mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Wirk' aus meiner H\u00e4nde Pflicht", "tokens": ["Wirk'", "aus", "mei\u00b7ner", "H\u00e4n\u00b7de", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele dir genehme Fr\u00fccht,", "tokens": ["Vie\u00b7le", "dir", "ge\u00b7neh\u00b7me", "Fr\u00fccht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leg' ich mich dann drauf zur Ruh,", "tokens": ["Leg'", "ich", "mich", "dann", "drauf", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "PAV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lege du dich auch dazu.", "tokens": ["Le\u00b7ge", "du", "dich", "auch", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "W\u00e4rm mich, wenn ich liebe kalt", "tokens": ["W\u00e4rm", "mich", ",", "wenn", "ich", "lie\u00b7be", "kalt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit dir es laulicht halt,", "tokens": ["Und", "mit", "dir", "es", "lau\u00b7licht", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leucht mir, da\u00df ich zu dir geh',", "tokens": ["Leucht", "mir", ",", "da\u00df", "ich", "zu", "dir", "geh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erquick' mich, wenn mir weh.", "tokens": ["Und", "er\u00b7quick'", "mich", ",", "wenn", "mir", "weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Letzt la\u00df deines Todes Pein", "tokens": ["Letzt", "la\u00df", "dei\u00b7nes", "To\u00b7des", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Asche Leben seyn;", "tokens": ["Mei\u00b7ner", "A\u00b7sche", "Le\u00b7ben", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses bittet, ander ich,", "tokens": ["Die\u00b7ses", "bit\u00b7tet", ",", "an\u00b7der", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Sonnenwende dich.", "tokens": ["Dei\u00b7ne", "Son\u00b7nen\u00b7wen\u00b7de", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}