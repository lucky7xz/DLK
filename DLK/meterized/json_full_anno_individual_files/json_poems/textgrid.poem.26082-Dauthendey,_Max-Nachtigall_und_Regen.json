{"textgrid.poem.26082": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Nachtigall und Regen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen durch die Fr\u00fchlingsnacht fiel ein Regen nackt und blo\u00df.", "tokens": ["Drau\u00b7\u00dfen", "durch", "die", "Fr\u00fch\u00b7lings\u00b7nacht", "fiel", "ein", "Re\u00b7gen", "nackt", "und", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Himmel hat sich aufgemacht, Segen zu den Steinen flo\u00df.", "tokens": ["Him\u00b7mel", "hat", "sich", "auf\u00b7ge\u00b7macht", ",", "Se\u00b7gen", "zu", "den", "Stei\u00b7nen", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "VVPP", "$,", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wie in einem Glashaus sa\u00df hinterm Regen Br\u00fcck' und Stra\u00df',", "tokens": ["Wie", "in", "ei\u00b7nem", "Glas\u00b7haus", "sa\u00df", "hin\u00b7term", "Re\u00b7gen", "Br\u00fcck", "und", "Stra\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "NE", "KON", "NN", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und es sang sich auf gut Gl\u00fcck, da\u00df das Dunkel sie verga\u00df,", "tokens": ["Und", "es", "sang", "sich", "auf", "gut", "Gl\u00fcck", ",", "da\u00df", "das", "Dun\u00b7kel", "sie", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ADJD", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Eine erste Nachtigall hell ein Liedlein, St\u00fcck um St\u00fcck.", "tokens": ["Ei\u00b7ne", "ers\u00b7te", "Nach\u00b7ti\u00b7gall", "hell", "ein", "Lied\u00b7lein", ",", "St\u00fcck", "um", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ART", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Wie aus einem K\u00e4fig klang hinterm Regen der Gesang.", "tokens": ["Wie", "aus", "ei\u00b7nem", "K\u00e4\u00b7fig", "klang", "hin\u00b7term", "Re\u00b7gen", "der", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Die Laternen, Stra\u00df' und Br\u00fcck', alle lauschten stundenlang", "tokens": ["Die", "La\u00b7ter\u00b7nen", ",", "Stra\u00df'", "und", "Br\u00fcck", ",", "al\u00b7le", "lauschten", "stun\u00b7den\u00b7lang"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+--+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Auf des kleinen Vogels Gl\u00fcck, und die Regenstr\u00e4nge rauschten.", "tokens": ["Auf", "des", "klei\u00b7nen", "Vo\u00b7gels", "Gl\u00fcck", ",", "und", "die", "Re\u00b7gen\u00b7str\u00e4n\u00b7ge", "rauschten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Nachtigall und Regen emsig ihrer Seele Lust austauschten.", "tokens": ["Nach\u00b7ti\u00b7gall", "und", "Re\u00b7gen", "em\u00b7sig", "ih\u00b7rer", "See\u00b7le", "Lust", "aus\u00b7tauschten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "PPOSAT", "NN", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Drau\u00dfen durch die Fr\u00fchlingsnacht fiel ein Regen nackt und blo\u00df.", "tokens": ["Drau\u00b7\u00dfen", "durch", "die", "Fr\u00fch\u00b7lings\u00b7nacht", "fiel", "ein", "Re\u00b7gen", "nackt", "und", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Himmel hat sich aufgemacht, Segen zu den Steinen flo\u00df.", "tokens": ["Him\u00b7mel", "hat", "sich", "auf\u00b7ge\u00b7macht", ",", "Se\u00b7gen", "zu", "den", "Stei\u00b7nen", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "VVPP", "$,", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wie in einem Glashaus sa\u00df hinterm Regen Br\u00fcck' und Stra\u00df',", "tokens": ["Wie", "in", "ei\u00b7nem", "Glas\u00b7haus", "sa\u00df", "hin\u00b7term", "Re\u00b7gen", "Br\u00fcck", "und", "Stra\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "wo