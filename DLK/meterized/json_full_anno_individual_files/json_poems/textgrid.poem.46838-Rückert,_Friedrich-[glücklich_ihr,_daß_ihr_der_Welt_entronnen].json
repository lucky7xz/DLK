{"textgrid.poem.46838": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gl\u00fccklich ihr, da\u00df ihr der Welt entronnen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich ihr, da\u00df ihr der Welt entronnen,", "tokens": ["Gl\u00fcck\u00b7lich", "ihr", ",", "da\u00df", "ihr", "der", "Welt", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eh das Netz der Wirrung euch umsponnen,", "tokens": ["Eh", "das", "Netz", "der", "Wir\u00b7rung", "euch", "um\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das um die da leben wirft das Leben,", "tokens": ["Das", "um", "die", "da", "le\u00b7ben", "wirft", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRELS", "ADV", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nicht Einsicht kann's, nur Tod, entweben.", "tokens": ["Und", "nicht", "Ein\u00b7sicht", "kann's", ",", "nur", "Tod", ",", "ent\u00b7we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "NN", "VMFIN", "$,", "ADV", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sich fremden, die sich lieben sollten,", "tokens": ["Wie", "sich", "frem\u00b7den", ",", "die", "sich", "lie\u00b7ben", "soll\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJA", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Selbst sich wehthun, die sich wohlthun wollten,", "tokens": ["Selbst", "sich", "weht\u00b7hun", ",", "die", "sich", "wohl\u00b7thun", "woll\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und so selten nur sich zwei verstehen,", "tokens": ["Und", "so", "sel\u00b7ten", "nur", "sich", "zwei", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "PRF", "CARD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die zusammen Eines Weges gehen.", "tokens": ["Die", "zu\u00b7sam\u00b7men", "Ei\u00b7nes", "We\u00b7ges", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dieses Streits, mit halberwachtem Sinne,", "tokens": ["Die\u00b7ses", "Streits", ",", "mit", "hal\u00b7ber\u00b7wach\u00b7tem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gl\u00fcckliche, seid ihr nicht worden inne,", "tokens": ["Gl\u00fcck\u00b7li\u00b7che", ",", "seid", "ihr", "nicht", "wor\u00b7den", "in\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAF