{"textgrid.poem.49882": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf einer Wanderung", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ein freundliches St\u00e4dtchen tret ich ein,", "tokens": ["In", "ein", "freund\u00b7li\u00b7ches", "St\u00e4dt\u00b7chen", "tret", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In den Stra\u00dfen liegt roter Abendschein.", "tokens": ["In", "den", "Stra\u00b7\u00dfen", "liegt", "ro\u00b7ter", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aus einem offnen Fenster eben,", "tokens": ["Aus", "ei\u00b7nem", "off\u00b7nen", "Fens\u00b7ter", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber den reichsten Blumenflor", "tokens": ["\u00dc\u00b7ber", "den", "reichs\u00b7ten", "Blu\u00b7men\u00b7flor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Hinweg, h\u00f6rt man Goldglockent\u00f6ne schweben,", "tokens": ["Hin\u00b7weg", ",", "h\u00f6rt", "man", "Gold\u00b7glo\u00b7cken\u00b7t\u00f6\u00b7ne", "schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Da\u00df die Bl\u00fcten beben,", "tokens": ["Da\u00df", "die", "Bl\u00fc\u00b7ten", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Da\u00df die L\u00fcfte leben,", "tokens": ["Da\u00df", "die", "L\u00fcf\u00b7te", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Da\u00df in h\u00f6herem Rot die Rosen leuchten vor.", "tokens": ["Da\u00df", "in", "h\u00f6\u00b7he\u00b7rem", "Rot", "die", "Ro\u00b7sen", "leuch\u00b7ten", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Lang hielt ich staunend, lustbeklommen.", "tokens": ["Lang", "hielt", "ich", "stau\u00b7nend", ",", "lust\u00b7be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie ich hinaus vors Tor gekommen,", "tokens": ["Wie", "ich", "hin\u00b7aus", "vors", "Tor", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APZR", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ich wei\u00df es wahrlich selber nicht.", "tokens": ["Ich", "wei\u00df", "es", "wahr\u00b7lich", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "p