{"dta.poem.19317": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arabisch hei\u00dfet Dien Religion von Dienen,", "tokens": ["A\u00b7ra\u00b7bisch", "hei\u00b7\u00dfet", "Di\u00b7en", "Re\u00b7li\u00b7gi\u00b7on", "von", "Die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Denn nicht zum Herrschen ist sie auf der Welt erschienen.", "tokens": ["Denn", "nicht", "zum", "Herr\u00b7schen", "ist", "sie", "auf", "der", "Welt", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Religion, solang sie dienstbar ist auf Erden,", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", ",", "so\u00b7lang", "sie", "dienst\u00b7bar", "ist", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Menschheit dienet sie zum Trost in den Beschwerden.", "tokens": ["Der", "Menschheit", "die\u00b7net", "sie", "zum", "Trost", "in", "den", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Da ist sie Gottesdienst ohn' \u00e4u\u00dfern Prunk und Braus;", "tokens": ["Da", "ist", "sie", "Got\u00b7tes\u00b7dienst", "ohn'", "\u00e4u\u00b7\u00dfern", "Prunk", "und", "Braus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald sie herrschend wird, wird eitler Weltdienst draus.", "tokens": ["So\u00b7bald", "sie", "herr\u00b7schend", "wird", ",", "wird", "eit\u00b7ler", "Welt\u00b7dienst", "draus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VAFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}