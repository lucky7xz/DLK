{"dta.poem.21609": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Uebermuth an allen Ecken \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Uebermuth an allen Ecken:", "tokens": ["Ue\u00b7ber\u00b7muth", "an", "al\u00b7len", "E\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohinaus noch, gute Welt!", "tokens": ["Wo\u00b7hi\u00b7naus", "noch", ",", "gu\u00b7te", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Rosen wachsen auf den Hecken,", "tokens": ["Ro\u00b7sen", "wach\u00b7sen", "auf", "den", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Golde starrt das Feld.", "tokens": ["Und", "im", "Gol\u00b7de", "starrt", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}