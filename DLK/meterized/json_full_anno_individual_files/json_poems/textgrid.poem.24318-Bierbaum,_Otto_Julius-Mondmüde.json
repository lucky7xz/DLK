{"textgrid.poem.24318": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Mondm\u00fcde", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond, die gro\u00dfe grelle Diebslaterne,", "tokens": ["Der", "Mond", ",", "die", "gro\u00b7\u00dfe", "grel\u00b7le", "Diebs\u00b7la\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der silberne Totensch\u00e4del der Nacht: Der Mond,", "tokens": ["Der", "sil\u00b7ber\u00b7ne", "To\u00b7ten\u00b7sch\u00e4\u00b7del", "der", "Nacht", ":", "Der", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$.", "ART", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein abgewetzter Knopf am schwarzen Schlafrock", "tokens": ["Ein", "ab\u00b7ge\u00b7wetz\u00b7ter", "Knopf", "am", "schwar\u00b7zen", "Schla\u00b7frock"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des lieben Gottes, dessen Kredit so sank,", "tokens": ["Des", "lie\u00b7ben", "Got\u00b7tes", ",", "des\u00b7sen", "Kre\u00b7dit", "so", "sank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da\u00df er sich keinen neuen leisten kann:", "tokens": ["Da\u00df", "er", "sich", "kei\u00b7nen", "neu\u00b7en", "leis\u00b7ten", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIAT", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Mond, das l\u00e4cherlichste aller Requisiten", "tokens": ["Der", "Mond", ",", "das", "l\u00e4\u00b7cher\u00b7lichs\u00b7te", "al\u00b7ler", "Re\u00b7qui\u00b7si\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Im lyrischen Kasperletheater, scheint", "tokens": ["Im", "ly\u00b7ri\u00b7schen", "Kas\u00b7per\u00b7le\u00b7the\u00b7a\u00b7ter", ",", "scheint"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So niedertr\u00e4chtig hell heut, naseweis,", "tokens": ["So", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "hell", "heut", ",", "na\u00b7se\u00b7weis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ADV", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aufdringlich und f\u00fcrwitzig, da\u00df ich ihm", "tokens": ["Auf\u00b7dring\u00b7lich", "und", "f\u00fcr\u00b7wit\u00b7zig", ",", "da\u00df", "ich", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Noch einen Schelmennamen geben mu\u00df:", "tokens": ["Noch", "ei\u00b7nen", "Schel\u00b7men\u00b7na\u00b7men", "ge\u00b7ben", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ohrfeigengesicht des Himmels. \u2013", "tokens": ["Ohr\u00b7fei\u00b7gen\u00b7ge\u00b7sicht", "des", "Him\u00b7mels", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Dies getan,", "tokens": ["Dies", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Schlie\u00df ich die L\u00e4den meines Fensters fest,", "tokens": ["Schlie\u00df", "ich", "die", "L\u00e4\u00b7den", "mei\u00b7nes", "Fens\u00b7ters", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So fest zu, da\u00df auch nicht der kleinste Spritzer", "tokens": ["So", "fest", "zu", ",", "da\u00df", "auch", "nicht", "der", "kleins\u00b7te", "Sprit\u00b7zer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "KOUS", "ADV", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Von seinem Katzensilber mich erreicht.", "tokens": ["Von", "sei\u00b7nem", "Kat\u00b7zen\u00b7sil\u00b7ber", "mich", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und samtenes Schwarz, die heilige Unfarbe", "tokens": ["Und", "sam\u00b7te\u00b7nes", "Schwarz", ",", "die", "hei\u00b7li\u00b7ge", "Un\u00b7far\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Der tiefsten Ruhe, senkt sich \u00fcber mich", "tokens": ["Der", "tiefs\u00b7ten", "Ru\u00b7he", ",", "senkt", "sich", "\u00fc\u00b7ber", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Gleich mohnduftdumpfem Staub von Schmetterlingen", "tokens": ["Gleich", "mohn\u00b7duft\u00b7dump\u00b7fem", "Staub", "von", "Schmet\u00b7ter\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der ewigen Nirwana. \u2013 Welt, schlaf wohl!", "tokens": ["Der", "e\u00b7wi\u00b7gen", "Nir\u00b7wa\u00b7na", ".", "\u2013", "Welt", ",", "schlaf", "wohl", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$.", "$(", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Bald schnarch ich wie ein alter Dudelsack", "tokens": ["Bald", "schnarch", "ich", "wie", "ein", "al\u00b7ter", "Du\u00b7del\u00b7sack"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und tr\u00e4um von meinen Feinden, wie sie schwitzend", "tokens": ["Und", "tr\u00e4um", "von", "mei\u00b7nen", "Fein\u00b7den", ",", "wie", "sie", "schwit\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Am Backherd stehn und Gallpasteten machen:", "tokens": ["Am", "Back\u00b7herd", "stehn", "und", "Gall\u00b7pas