{"dta.poem.7654": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie der Genesene ganz der Gesundheit Gl\u00fcck", "tokens": ["Wie", "der", "Ge\u00b7ne\u00b7se\u00b7ne", "ganz", "der", "Ge\u00b7sund\u00b7heit", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN", "NN"], "meter": "+-----+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Empfindet, wenn er an die Krankheit denkt zur\u00fcck;", "tokens": ["Emp\u00b7fin\u00b7det", ",", "wenn", "er", "an", "die", "Krank\u00b7heit", "denkt", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des ungehemmten Stroms der Lebensf\u00fclle froh,", "tokens": ["Des", "un\u00b7ge\u00b7hemm\u00b7ten", "Stroms", "der", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er der Hemmung nun, er hofft auf stets, entfloh:", "tokens": ["Wenn", "er", "der", "Hem\u00b7mung", "nun", ",", "er", "hofft", "auf", "stets", ",", "ent\u00b7floh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "PPER", "VVFIN", "APPR", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So auch, wer voriger Verirrungen gedenkt,", "tokens": ["So", "auch", ",", "wer", "vo\u00b7ri\u00b7ger", "Ver\u00b7ir\u00b7run\u00b7gen", "ge\u00b7denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+---+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Aus denen Gott ihn hat zur rechten Bahn gelenkt;", "tokens": ["Aus", "de\u00b7nen", "Gott", "ihn", "hat", "zur", "rech\u00b7ten", "Bahn", "ge\u00b7lenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er mag die rechte Bahn mit rechter Freude wallen,", "tokens": ["Er", "mag", "die", "rech\u00b7te", "Bahn", "mit", "rech\u00b7ter", "Freu\u00b7de", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kraft f\u00fchlend und Entschlu\u00df, nie mehr zur\u00fcck zu fallen.", "tokens": ["Kraft", "f\u00fch\u00b7lend", "und", "Ent\u00b7schlu\u00df", ",", "nie", "mehr", "zu\u00b7r\u00fcck", "zu", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "$,", "ADV", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wie ein Nachgef\u00fchl der Krankheit den Gesunden", "tokens": ["Doch", "wie", "ein", "Nach\u00b7ge\u00b7f\u00fchl", "der", "Krank\u00b7heit", "den", "Ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oft leise mahnt, und, kaum sich meldend, ist geschwunden;", "tokens": ["Oft", "lei\u00b7se", "mahnt", ",", "und", ",", "kaum", "sich", "mel\u00b7dend", ",", "ist", "ge\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "KON", "$,", "ADV", "PRF", "ADJD", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So den, der voriger Verirrung auch gedenkt,", "tokens": ["So", "den", ",", "der", "vo\u00b7ri\u00b7ger", "Ver\u00b7ir\u00b7rung", "auch", "ge\u00b7denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur da\u00df dis Nachgef\u00fchl von Krankheit st\u00e4rker kr\u00e4nkt.", "tokens": ["Nur", "da\u00df", "dis", "Nach\u00b7ge\u00b7f\u00fchl", "von", "Krank\u00b7heit", "st\u00e4r\u00b7ker", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PDS", "NN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Denn einen Unterschied in dem, was wir erduldet,", "tokens": ["Denn", "ei\u00b7nen", "Un\u00b7ter\u00b7schied", "in", "dem", ",", "was", "wir", "er\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht immer, ob es war ver- oder unverschuldet.", "tokens": ["Macht", "im\u00b7mer", ",", "ob", "es", "war", "ver", "o\u00b7der", "un\u00b7ver\u00b7schul\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "VAFIN", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}