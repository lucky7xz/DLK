{"dta.poem.8105": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dann hei\u00df\u2019 ichs kommen! St\u00e4dte von Mauern hoch", "tokens": ["Dann", "hei\u00df'", "ichs", "kom\u00b7men", "!", "St\u00e4d\u00b7te", "von", "Mau\u00b7ern", "hoch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVINF", "$.", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und H\u00fcgeln, fallen \u00f6de\u2019 zur Tr\u00fcmmer hin!", "tokens": ["Und", "H\u00fc\u00b7geln", ",", "fal\u00b7len", "\u00f6\u00b7de'", "zur", "Tr\u00fcm\u00b7mer", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schaam, und des Todes Furcht", "tokens": ["Schaam", ",", "und", "des", "To\u00b7des", "Furcht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "NN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Senkt zur Erde des Streitenden Arm!", "tokens": ["Senkt", "zur", "Er\u00b7de", "des", "Strei\u00b7ten\u00b7den", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}