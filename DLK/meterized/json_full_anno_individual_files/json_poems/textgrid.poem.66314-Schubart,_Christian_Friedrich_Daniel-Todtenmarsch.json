{"textgrid.poem.66314": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Todtenmarsch", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zieh hin, du braver Krieger, du!", "tokens": ["Zieh", "hin", ",", "du", "bra\u00b7ver", "Krie\u00b7ger", ",", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir leiten dich zur Grabesruh',", "tokens": ["Wir", "lei\u00b7ten", "dich", "zur", "Gra\u00b7bes\u00b7ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreiten mit gesunkner Wehr,", "tokens": ["Und", "schrei\u00b7ten", "mit", "ge\u00b7sun\u00b7kner", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Wehmuth schwer", "tokens": ["Von", "Weh\u00b7muth", "schwer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und stumm vor deinem Sarge her.", "tokens": ["Und", "stumm", "vor", "dei\u00b7nem", "Sar\u00b7ge", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du warst ein biedrer, deutscher Mann;", "tokens": ["Du", "warst", "ein", "bie\u00b7drer", ",", "deut\u00b7scher", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast immerhin so brav gethan.", "tokens": ["Hast", "im\u00b7mer\u00b7hin", "so", "brav", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Herz, voll edler Tapferkeit,", "tokens": ["Dein", "Herz", ",", "voll", "ed\u00b7ler", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat nie im Streit", "tokens": ["Hat", "nie", "im", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gescho\u00df und S\u00e4belhieb gescheut.", "tokens": ["Ge\u00b7scho\u00df", "und", "S\u00e4\u00b7bel\u00b7hieb", "ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Warst auch ein christlicher Soldat,", "tokens": ["Warst", "auch", "ein", "christ\u00b7li\u00b7cher", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wenig sprach und vieles that,", "tokens": ["Der", "we\u00b7nig", "sprach", "und", "vie\u00b7les", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem F\u00fcrsten und dem Lande treu,", "tokens": ["Dem", "F\u00fcrs\u00b7ten", "und", "dem", "Lan\u00b7de", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fromm dabei", "tokens": ["Und", "fromm", "da\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von Herzen, ohne Heuchelei.", "tokens": ["Von", "Her\u00b7zen", ",", "oh\u00b7ne", "Heu\u00b7che\u00b7lei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du standst in grauser Mitternacht,", "tokens": ["Du", "standst", "in", "grau\u00b7ser", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Frost und Hitze auf der Wacht;", "tokens": ["In", "Frost", "und", "Hit\u00b7ze", "auf", "der", "Wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ertrugst so standhaft manche Noth", "tokens": ["Er\u00b7trugst", "so", "stand\u00b7haft", "man\u00b7che", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und danktest Gott", "tokens": ["Und", "dank\u00b7test", "Gott"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "F\u00fcr Wasser und f\u00fcr's liebe Brod.", "tokens": ["F\u00fcr", "Was\u00b7ser", "und", "f\u00fcr's", "lie\u00b7be", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Wie du gelebt, so starbst auch du,", "tokens": ["Wie", "du", "ge\u00b7lebt", ",", "so", "starbst", "auch", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlo\u00df'st deine Augen freudig zu", "tokens": ["Schlo\u00df'st", "dei\u00b7ne", "Au\u00b7gen", "freu\u00b7dig", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dachtest: Aus ist nun der Streit", "tokens": ["Und", "dach\u00b7test", ":", "Aus", "ist", "nun", "der", "Streit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "APPR", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Kampf der Zeit.", "tokens": ["Und", "Kampf", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Jetzt kommt die ew'ge Seligkeit.", "tokens": ["Jetzt", "kommt", "die", "ew'\u00b7ge", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der liebe Herrgott kannte dich.", "tokens": ["Der", "lie\u00b7be", "Herr\u00b7gott", "kann\u00b7te", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gen Himmel kamst du sicherlich.", "tokens": ["Gen", "Him\u00b7mel", "kamst", "du", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, Wittwe, und ihr Kinderlein,", "tokens": ["Du", ",", "Witt\u00b7we", ",", "und", "ihr", "Kin\u00b7der\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Traut Gott allein:", "tokens": ["Traut", "Gott", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Er wird nun eure St\u00fctze sein.", "tokens": ["Er", "wird", "nun", "eu\u00b7re", "St\u00fct\u00b7ze", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Bahre poltert in die Gruft;", "tokens": ["Die", "Bah\u00b7re", "pol\u00b7tert", "in", "die", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir aber donnern in die Luft", "tokens": ["Wir", "a\u00b7ber", "don\u00b7nern", "in", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein letztes Lebewohl dreimal.", "tokens": ["Dein", "letz\u00b7tes", "Le\u00b7be\u00b7wohl", "drei\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Himmelssaal", "tokens": ["Im", "Him\u00b7mels\u00b7saal"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dort sehn wir dich ohn' alle Qual.", "tokens": ["Dort", "sehn", "wir", "dich", "ohn'", "al\u00b7le", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nehmt seinen S\u00e4bel von der Bahr',", "tokens": ["Nehmt", "sei\u00b7nen", "S\u00e4\u00b7bel", "von", "der", "Bahr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seid so brav, wie er es war;", "tokens": ["Und", "seid", "so", "brav", ",", "wie", "er", "es", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann \u00fcberwinden wir, wie er:", "tokens": ["Dann", "\u00fc\u00b7berw\u00b7in\u00b7den", "wir", ",", "wie", "er", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und hei\u00df und schwer", "tokens": ["Und", "hei\u00df", "und", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dr\u00fcckt uns des Lebens Joch nicht mehr.", "tokens": ["Dr\u00fcckt", "uns", "des", "Le\u00b7bens", "Joch", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Eilt, Kameraden, von der Gruft!", "tokens": ["Eilt", ",", "Ka\u00b7me\u00b7ra\u00b7den", ",", "von", "der", "Gruft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil uns die Trommel wieder ruft.", "tokens": ["Weil", "uns", "die", "Trom\u00b7mel", "wie\u00b7der", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er rastet nun im k\u00fchlen Sand:", "tokens": ["Er", "ras\u00b7tet", "nun", "im", "k\u00fch\u00b7len", "Sand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns fordert F\u00fcrst und Vaterland.", "tokens": ["Uns", "for\u00b7dert", "F\u00fcrst", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir bieten ihm", "tokens": ["Wir", "bie\u00b7ten", "ihm"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mit Ungest\u00fcm", "tokens": ["Mit", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die rauhe Kriegerhand.", "tokens": ["Die", "rau\u00b7he", "Krie\u00b7ger\u00b7hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Zwar ging' es leichter in dem Feld,", "tokens": ["Zwar", "ging'", "es", "leich\u00b7ter", "in", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als auf dem Bette, aus der Welt;", "tokens": ["Als", "auf", "dem", "Bet\u00b7te", ",", "aus", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch alles nur nach Gottes Rath,", "tokens": ["Doch", "al\u00b7les", "nur", "nach", "Got\u00b7tes", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So denkt ein redlicher Soldat.", "tokens": ["So", "denkt", "ein", "red\u00b7li\u00b7cher", "Sol\u00b7dat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm geht es gut;", "tokens": ["Ihm", "geht", "es", "gut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Er stirbt mit Muth,", "tokens": ["Er", "stirbt", "mit", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie unser Kamerad.", "tokens": ["Wie", "un\u00b7ser", "Ka\u00b7me\u00b7rad", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zieh hin, du braver Krieger, du!", "tokens": ["Zieh", "hin", ",", "du", "bra\u00b7ver", "Krie\u00b7ger", ",", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir leiten dich zur Grabesruh',", "tokens": ["Wir", "lei\u00b7ten", "dich", "zur", "Gra\u00b7bes\u00b7ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreiten mit gesunkner Wehr,", "tokens": ["Und", "schrei\u00b7ten", "mit", "ge\u00b7sun\u00b7kner", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Wehmuth schwer", "tokens": ["Von", "Weh\u00b7muth", "schwer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und stumm vor deinem Sarge her.", "tokens": ["Und", "stumm", "vor", "dei\u00b7nem", "Sar\u00b7ge", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du warst ein biedrer, deutscher Mann;", "tokens": ["Du", "warst", "ein", "bie\u00b7drer", ",", "deut\u00b7scher", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER",