], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Des Herzens Sprache gef\u00e4llt \u2013", "tokens": ["Des", "Her\u00b7zens", "Spra\u00b7che", "ge\u00b7f\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch einsam fand ich die Wohnung.", "tokens": ["Doch", "ein\u00b7sam", "fand", "ich", "die", "Woh\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich stand und dachte Verdru\u00df,", "tokens": ["Ich", "stand", "und", "dach\u00b7te", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Und gieng mit wankendem Schritt", "tokens": ["Und", "gieng", "mit", "wan\u00b7ken\u00b7dem", "Schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und warf die Augen herab", "tokens": ["Und", "warf", "die", "Au\u00b7gen", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nach der kleinstromigten Elbe,", "tokens": ["Nach", "der", "klein\u00b7stro\u00b7mig\u00b7ten", "El\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Itzt breit umufert von Eis.", "tokens