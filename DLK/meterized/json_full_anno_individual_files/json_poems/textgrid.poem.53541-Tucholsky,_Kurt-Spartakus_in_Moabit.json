{"textgrid.poem.53541": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Spartakus in Moabit", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh da: Justitia!", "tokens": ["Sieh", "da", ":", "Jus\u00b7ti\u00b7tia", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Voll mit Paragraphen", "tokens": ["Voll", "mit", "Pa\u00b7ra\u00b7gra\u00b7phen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "beh\u00e4ngt, so steht wie ehedem sie da.", "tokens": ["be\u00b7h\u00e4ngt", ",", "so", "steht", "wie", "e\u00b7he\u00b7dem", "sie", "da", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "KOKOM", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie hat nat\u00fcrlich alles ganz verschlafen", "tokens": ["Sie", "hat", "na\u00b7t\u00fcr\u00b7lich", "al\u00b7les", "ganz", "ver\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und wei\u00df nicht, was im Lande jetzt geschah.", "tokens": ["und", "wei\u00df", "nicht", ",", "was", "im", "Lan\u00b7de", "jetzt", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRELS", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbwas ist denn uns\u00ab, so spricht ein weiser Richter,", "tokens": ["\u00bb", "was", "ist", "denn", "uns", "\u00ab", ",", "so", "spricht", "ein", "wei\u00b7ser", "Rich\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ADV", "PPER", "$(", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbdie Politik und die Revolution!", "tokens": ["\u00bb", "die", "Po\u00b7li\u00b7tik", "und", "die", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die Kerle sind Banditen und Gelichter,", "tokens": ["Die", "Ker\u00b7le", "sind", "Ban\u00b7di\u00b7ten", "und", "Ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Pa\u00dft auf, und wir besorgens ihnen schon!\u00ab", "tokens": ["Pa\u00dft", "auf", ",", "und", "wir", "be\u00b7sor\u00b7gens", "ih\u00b7nen", "schon", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "PPER", "ADV", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr weisen und gerechten Richter fauchtet", "tokens": ["Ihr", "wei\u00b7sen", "und", "ge\u00b7rech\u00b7ten", "Rich\u00b7ter", "fauch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf die Emp\u00f6rer \u2013 nach mi\u00dflungener Tat.", "tokens": ["auf", "die", "Em\u00b7p\u00f6\u00b7rer", "\u2013", "nach", "mi\u00df\u00b7lun\u00b7ge\u00b7ner", "Tat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Wahlrecht aber, das ihr eben brauchtet,", "tokens": ["Das", "Wahl\u00b7recht", "a\u00b7ber", ",", "das", "ihr", "e\u00b7ben", "brauch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "verdanktet ihr dem gleichen Hochverrat.", "tokens": ["ver\u00b7dank\u00b7tet", "ihr", "dem", "glei\u00b7chen", "Hoch\u00b7ver\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Justitia, Trauerweib, du hast geschlafen,", "tokens": ["Jus\u00b7ti\u00b7tia", ",", "Trau\u00b7er\u00b7weib", ",", "du", "hast", "ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "wie stets, wenn wir vom Fleck gekommen sind.", "tokens": ["wie", "stets", ",", "wenn", "wir", "vom", "Fleck", "ge\u00b7kom\u00b7men", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir pfeifen auf den Spruch und auf die Strafen!", "tokens": ["Wir", "pfei\u00b7fen", "auf", "den", "Spruch", "und", "auf", "die", "Stra\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Rei\u00df deine Binde ab! Du bist ja blind!", "tokens": ["Rei\u00df", "dei\u00b7ne", "Bin\u00b7de", "ab", "!", "Du", "bist", "ja", "blind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du armes Hascherl kannst nicht unterscheiden,", "tokens": ["Du", "ar\u00b7mes", "Ha\u00b7scherl", "kannst", "nicht", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wer R\u00e4uber war und wer Idealist \u2013", "tokens": ["wer", "R\u00e4u\u00b7ber", "war", "und", "wer", "I\u00b7dea\u00b7list", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "KON", "PWS", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du knobelst ernst und strafst gleich hart die beiden,", "tokens": ["Du", "kno\u00b7belst", "ernst", "und", "strafst", "gleich", "hart", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "ART", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "weil du zu faul zum Pr\u00e4zisieren bist.", "tokens": ["weil", "du", "zu", "faul", "zum", "Pr\u00e4\u00b7zi\u00b7sie\u00b7ren", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbnoch gilt das Recht! Sie m\u00fcssen eben hangen!\u00ab", "tokens": ["\u00bb", "noch", "gilt", "das", "Recht", "!", "Sie", "m\u00fcs\u00b7sen", "e\u00b7ben", "han\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geschichte gilt \u2013 und nicht dein Tintenquark.", "tokens": ["Ge\u00b7schich\u00b7te", "gilt", "\u2013", "und", "nicht", "dein", "Tin\u00b7ten\u00b7quark", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "KON", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Willst du dir wegen Ruhest\u00f6rung langen", "tokens": ["Willst", "du", "dir", "we\u00b7gen", "Ru\u00b7hes\u00b7t\u00f6\u00b7rung", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die junge Mannschaft da von Langemarck?", "tokens": ["die", "jun\u00b7ge", "Mann\u00b7schaft", "da", "von", "Lan\u00b7ge\u00b7marck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das sei was andres?", "tokens": ["Das", "sei", "was", "and\u00b7res", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PWS", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ei, denkst du der Zeiten,", "tokens": ["Ei", ",", "denkst", "du", "der", "Zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "wo du die Adelsfrau im Schwesternkleid,", "tokens": ["wo", "du", "die", "A\u00b7dels\u00b7frau", "im", "Schwes\u00b7tern\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die stahl, wo du des Schutzmanns Grausamkeiten", "tokens": ["die", "stahl", ",", "wo", "du", "des", "Schutz\u00b7manns", "Grau\u00b7sam\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "fein legtest still bei Seiten \u2013", "tokens": ["fein", "leg\u00b7test", "still", "bei", "Sei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "sie w\u00fc\u00dften nichts von der Rechtswidrigkeit?", "tokens": ["sie", "w\u00fc\u00df\u00b7ten", "nichts", "von", "der", "Rechts\u00b7wid\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Straf du die Lumpe bei den Spartakisten.", "tokens": ["Straf", "du", "die", "Lum\u00b7pe", "bei", "den", "Spar\u00b7ta\u00b7kis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Steck die ins Zuchthaus, die beim Kampf geklaut", "tokens": ["Steck", "die", "ins", "Zucht\u00b7haus", ",", "die", "beim", "Kampf", "ge\u00b7klaut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Vergreif dich nicht an den Idealisten!", "tokens": ["Ver\u00b7greif", "dich", "nicht", "an", "den", "I\u00b7dea\u00b7lis\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wir kennen deine Paragraphenkisten!", "tokens": ["Wir", "ken\u00b7nen", "dei\u00b7ne", "Pa\u00b7ra\u00b7gra\u00b7phen\u00b7kis\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nimm dich in acht, du alte, falsche Haut!", "tokens": ["Nimm", "dich", "in", "acht", ",", "du", "al\u00b7te", ",", "fal\u00b7sche", "Haut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sieh da: Justitia!", "tokens": ["Sieh", "da", ":", "Jus\u00b7ti\u00b7tia", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Voll mit Paragraphen", "tokens": ["Voll", "mit", "Pa\u00b7ra\u00b7gra\u00b7phen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "beh\u00e4ngt, so steht wie ehedem sie da."