{"dta.poem.11483": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Herrn von Hoffmannswaldau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein kiel, ber\u00fchmter Mann! so nur von honig rinnt,", "tokens": ["Dein", "kiel", ",", "be\u00b7r\u00fchm\u00b7ter", "Mann", "!", "so", "nur", "von", "ho\u00b7nig", "rinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und amber-tropffen f\u00fchrt, hat nirgend seines gleichen:", "tokens": ["Und", "am\u00b7ber\u00b7tropf\u00b7fen", "f\u00fchrt", ",", "hat", "nir\u00b7gend", "sei\u00b7nes", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es mu\u00df Marini dir die sieges-crone reichen,", "tokens": ["Es", "mu\u00df", "Ma\u00b7ri\u00b7ni", "dir", "die", "sie\u00b7ges\u00b7cro\u00b7ne", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PPER", "ART", "ADJA", "ADJA", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Weil deine lieblichkeit der seinen abgewinnt.", "tokens": ["Weil", "dei\u00b7ne", "lieb\u00b7lich\u00b7keit", "der", "sei\u00b7nen", "ab\u00b7ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sagt, tichter! sagt es aus! was seine lieder sind?", "tokens": ["Sagt", ",", "tich\u00b7ter", "!", "sagt", "es", "aus", "!", "was", "sei\u00b7ne", "lie\u00b7der", "sind", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob ihre kr\u00e4ffte nicht ein st\u00e4hlern hertz erweichen?", "tokens": ["Ob", "ih\u00b7re", "kr\u00e4ff\u00b7te", "nicht", "ein", "st\u00e4h\u00b7lern", "hertz", "er\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es mag Amphion nur die stoltzen segel streichen,", "tokens": ["Es", "mag", "Am\u00b7phi\u00b7on", "nur", "die", "stolt\u00b7zen", "se\u00b7gel", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn seiner harffe krafft ist fabel, dunst und wind.", "tokens": ["Denn", "sei\u00b7ner", "harf\u00b7fe", "krafft", "ist", "fa\u00b7bel", ",", "dunst", "und", "wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADJD", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df itzt die lieblichkeit so wenig Musen n\u00e4hret,", "tokens": ["Da\u00df", "itzt", "die", "lieb\u00b7lich\u00b7keit", "so", "we\u00b7nig", "Mu\u00b7sen", "n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Macht deine poesie, die sie gantz ausgeleeret.", "tokens": ["Macht", "dei\u00b7ne", "po\u00b7e\u00b7sie", ",", "die", "sie", "gantz", "aus\u00b7ge\u00b7lee\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+--+--+-++-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Apollo sieht dich selbst mit neid und eyfer an.", "tokens": ["A\u00b7pol\u00b7lo", "sieht", "dich", "selbst", "mit", "neid", "und", "ey\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er h\u00e4tte dir zwar l\u00e4ngst den lorber-krantz geraubet;", "tokens": ["Er", "h\u00e4t\u00b7te", "dir", "zwar", "l\u00e4ngst", "den", "lor\u00b7ber\u00b7krantz", "ge\u00b7rau\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch er getraut sich nicht, dieweil er selbst nicht glaubet:", "tokens": ["Doch", "er", "ge\u00b7traut", "sich", "nicht", ",", "die\u00b7weil", "er", "selbst", "nicht", "glau\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df seine laute dir die wage halten kan.", "tokens": ["Da\u00df", "sei\u00b7ne", "lau\u00b7te", "dir", "die", "wa\u00b7ge", "hal\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}