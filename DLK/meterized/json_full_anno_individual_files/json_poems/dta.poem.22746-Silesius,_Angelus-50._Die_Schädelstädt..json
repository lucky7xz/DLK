{"dta.poem.22746": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "50. Die Sch\u00e4delst\u00e4dt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ist di\u00df die Sch\u00e4delftadt? wie kombt es dan\u0303 da\u00df hier", "tokens": ["Ist", "di\u00df", "die", "Sch\u00e4\u00b7delf\u00b7tadt", "?", "wie", "kombt", "es", "da\u00f1", "da\u00df", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "PAV", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und da der Lebensbaum? der Brunn mit den vier", "tokens": ["Und", "da", "der", "Le\u00b7bens\u00b7baum", "?", "der", "Brunn", "mit", "den", "vier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "$.", "ART", "NN", "APPR", "ART", "CARD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Fl&#252;ssen?", "tokens": ["Fl", "&#252;", "ssen", "?"], "token_info": ["word", "XML_entity", "word", "punct"], "pos": ["NE", "$(", "VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Es ist da\u00df Paradi\u00df: doch sey es was es wil:", "tokens": ["Es", "ist", "da\u00df", "Pa\u00b7ra\u00b7di\u00df", ":", "doch", "sey", "es", "was", "es", "wil", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "NN", "$.", "ADV", "VAFIN", "PPER", "PWS", "PPER", "V