{"textgrid.poem.24364": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Ihro Hoheit den Printzen von Sachsen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm Friederich August/ du junger Held der Sachsen/", "tokens": ["Komm", "Frie\u00b7de\u00b7rich", "Au\u00b7gust", "/", "du", "jun\u00b7ger", "Held", "der", "Sach\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$(", "PPER", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Vaters Tapferkeit wird dir in deiner Brust/", "tokens": ["Des", "Va\u00b7ters", "Tap\u00b7fer\u00b7keit", "wird", "dir", "in", "dei\u00b7ner", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein weiser Friederich in deinem Haupte wachsen/", "tokens": ["Ein", "wei\u00b7ser", "Frie\u00b7de\u00b7rich", "in", "dei\u00b7nem", "Haup\u00b7te", "wach\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Augusti Gottesfurcht ist dir bereits bewust.", "tokens": ["Au\u00b7gus\u00b7ti", "Got\u00b7tes\u00b7furcht", "ist", "dir", "be\u00b7reits", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Denn jener Friederi