{"textgrid.poem.55993": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Samuels Erscheinung vor Saul", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da schrie die Frau zu Endor auf: Ich sehe \u2013", "tokens": ["Da", "schrie", "die", "Frau", "zu", "En\u00b7dor", "auf", ":", "Ich", "se\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der K\u00f6nig packte sie am Arme: Wen?", "tokens": ["Der", "K\u00f6\u00b7nig", "pack\u00b7te", "sie", "am", "Ar\u00b7me", ":", "Wen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "PWS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da die Starrende beschrieb, noch ehe,", "tokens": ["Und", "da", "die", "Star\u00b7ren\u00b7de", "be\u00b7schrieb", ",", "noch", "e\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "KOUS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da war ihm schon, er h\u00e4tte selbst gesehn:", "tokens": ["da", "war", "ihm", "schon", ",", "er", "h\u00e4t\u00b7te", "selbst", "ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den, dessen Stimme ihn noch einmal traf:", "tokens": ["Den", ",", "des\u00b7sen", "Stim\u00b7me", "ihn", "noch", "ein\u00b7mal", "traf", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELAT", "NN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was st\u00f6rst du mich? Ich habe Schlaf.", "tokens": ["Was", "st\u00f6rst", "du", "mich", "?", "Ich", "ha\u00b7be", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst du, weil dir die Himmel fluchen", "tokens": ["Willst", "du", ",", "weil", "dir", "die", "Him\u00b7mel", "flu\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und weil der Herr sich vor dir schlo\u00df und schwieg,", "tokens": ["und", "weil", "der", "Herr", "sich", "vor", "dir", "schlo\u00df", "und", "schwieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "in meinem Mund nach einem Siege suchen?", "tokens": ["in", "mei\u00b7nem", "Mund", "nach", "ei\u00b7nem", "Sie\u00b7ge", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Soll ich dir meine Z\u00e4hne einzeln sagen?", "tokens": ["Soll", "ich", "dir", "mei\u00b7ne", "Z\u00e4h\u00b7ne", "ein\u00b7zeln", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich habe nichts als sie... Es schwand. Da schrie", "tokens": ["Ich", "ha\u00b7be", "nichts", "als", "sie", "...", "Es", "schwand", ".", "Da", "schrie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "PPER", "$(", "PPER", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "das Weib, die H\u00e4nde vors Gesicht geschlagen,", "tokens": ["das", "Weib", ",", "die", "H\u00e4n\u00b7de", "vors", "Ge\u00b7sicht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "als ob sie's sehen m\u00fc\u00dfte: Unterlieg \u2013", "tokens": ["als", "ob", "sie's", "se\u00b7hen", "m\u00fc\u00df\u00b7te", ":", "Un\u00b7ter\u00b7lieg", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "VVINF", "VMFIN", "$.", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und er, der in der Zeit, die ihm gelang,", "tokens": ["Und", "er", ",", "der", "in", "der", "Zeit", ",", "die", "ihm", "ge\u00b7lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das Volk wie ein Feldzeichen \u00fcberragte,", "tokens": ["das", "Volk", "wie", "ein", "Feld\u00b7zei\u00b7chen", "\u00fc\u00b7berr\u00b7ag\u0