{"textgrid.poem.64213": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Friedhof, wo die Weiden schwanken,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Friedhof, wo die Weiden schwanken,", "tokens": ["Im", "Fried\u00b7hof", ",", "wo", "die", "Wei\u00b7den", "schwan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schritt ich mit sinnenden Gedanken. \u2013", "tokens": ["Schritt", "ich", "mit", "sin\u00b7nen\u00b7den", "Ge\u00b7dan\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da sang, an eines Grabes Saum,", "tokens": ["Da", "sang", ",", "an", "ei\u00b7nes", "Gra\u00b7bes", "Saum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blaukehlchen hell von hohem Baum.", "tokens": ["Blau\u00b7kehl\u00b7chen", "hell", "von", "ho\u00b7hem", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Blaukehlchen f\u00fchrt, wie jeder wei\u00df,", "tokens": ["Blau\u00b7kehl\u00b7chen", "f\u00fchrt", ",", "wie", "je\u00b7der", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Zugleich ", "tokens": ["Zu\u00b7gleich"], "token_info": ["word"], "pos": ["ADV"], "meter": "++", "measure": "spondeus"}, "line.7": {"text": "Und Hart und Weich und Herb und Lind", "tokens": ["Und", "Hart", "und", "Weich", "und", "Herb", "und", "Lind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJD", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Rasch wechselnd ihm zu eigen sind:", "tokens": ["Rasch", "wech\u00b7selnd", "ihm", "zu", "ei\u00b7gen", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du schaust Ein V\u00f6glein auf dem Ast,", "tokens": ["Du", "schaust", "Ein", "V\u00f6\u00b7glein", "auf", "dem", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df zweie s\u00e4ngen, schw\u00f6rst du fast. \u2013", "tokens": ["Da\u00df", "zwei\u00b7e", "s\u00e4n\u00b7gen", ",", "schw\u00f6rst", "du", "fast", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Des gleichen Wunders wieder heute", "tokens": ["Des", "glei\u00b7chen", "Wun\u00b7ders", "wie\u00b7der", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ich mich im gr\u00fcnen Friedhof freute:", "tokens": ["Ich", "mich", "im", "gr\u00fc\u00b7nen", "Fried\u00b7hof", "freu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Denn, wechselnd, aus den Weidenzweigen,", "tokens": ["Denn", ",", "wech\u00b7selnd", ",", "aus", "den", "Wei\u00b7den\u00b7zwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Stolz f\u00e4chernd breiten Schweif mit Neigen,", "tokens": ["Stolz", "f\u00e4\u00b7chernd", "brei\u00b7ten", "Schweif", "mit", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zweistimmig sang das V\u00f6glein dort", "tokens": ["Zweis\u00b7tim\u00b7mig", "sang", "das", "V\u00f6\u00b7glein", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "An deinem Grab, Schalk Wunnebrord,", "tokens": ["An", "dei\u00b7nem", "Grab", ",", "Schalk", "Wun\u00b7ne\u00b7brord", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Den, widers Blut, noch ungeboren,", "tokens": ["Den", ",", "wi\u00b7ders", "Blut", ",", "noch", "un\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Gel\u00fcbde hat zum M\u00f6nch geschoren:", "tokens": ["Ge\u00b7l\u00fcb\u00b7de", "hat", "zum", "M\u00f6nch", "ge\u00b7scho\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die Mutter schwor's: \u2013 so ward's der Sohn.", "tokens": ["Die", "Mut\u00b7ter", "schwor's", ":", "\u2013", "so", "ward's", "der", "Sohn", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$.", "$(", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die Kirche trug kein Heil davon!", "tokens": ["Die", "Kir\u00b7che", "trug", "kein", "Heil", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Er, Kellrer in dem Kloster Fuld,", "tokens": ["Er", ",", "Kell\u00b7rer", "in", "dem", "Klos\u00b7ter", "Fuld", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Trug mehr dem Fa\u00df als Fasten Huld,", "tokens": ["Trug", "mehr", "dem", "Fa\u00df", "als", "Fas\u00b7ten", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "KOUS", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und unterwies er uns, die Jungen,", "tokens": ["Und", "un\u00b7ter\u00b7wies", "er", "uns", ",", "die", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sang er in zwei verschiednen Zungen:", "tokens": ["Sang", "er", "in", "zwei", "ver\u00b7schied\u00b7nen", "Zun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "\u00bbvom \u00dcbel ist der firne Wein!\u00ab", "tokens": ["\u00bb", "vom", "\u00dc\u00b7bel", "ist", "der", "fir\u00b7ne", "Wein", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "\u2013 (Doch trank ich nie genug noch sein!) \u2013", "tokens": ["\u2013", "(", "Doch", "trank", "ich", "nie", "ge\u00b7nug", "noch", "sein", "!", ")", "\u2013"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "VAINF", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Das Alter nur hat weise Tugend,", "tokens": ["Das", "Al\u00b7ter", "nur", "hat", "wei\u00b7se", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "\u2013 (Doch wahre Lust hat nur die Jugend!) \u2013", "tokens": ["\u2013", "(", "Doch", "wah\u00b7re", "Lust", "hat", "nur", "die", "Ju\u00b7gend", "!", ")", "\u2013"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "KON", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Man soll nur singen Mess' und Psalter,", "tokens": ["Man", "soll", "nur", "sin\u00b7gen", "Mess'", "und", "Psal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "\u2013 (Ein Taglied t\u00f6nt viel s\u00fc\u00dfer, Walther!) \u2013", "tokens": ["\u2013", "(", "Ein", "Ta\u00b7glied", "t\u00f6nt", "viel", "s\u00fc\u00b7\u00dfer", ",", "Walt\u00b7her", "!", ")", "\u2013"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "NE", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Zur H\u00f6lle f\u00fchret Weiberku\u00df,", "tokens": ["Zur", "H\u00f6l\u00b7le", "f\u00fch\u00b7ret", "Wei\u00b7ber\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "\u2013 (Ein Tropf, wer sein entraten mu\u00df!) \u2013", "tokens": ["\u2013", "(", "Ein", "Tropf", ",", "wer", "sein", "ent\u00b7ra\u00b7ten", "mu\u00df", "!", ")", "\u2013"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "ART", "NN", "$,", "PWS", "PPOSAT", "ADJA", "VMFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Dem Feind verzeihn, ist Christenpflicht,", "tokens": ["Dem", "Feind", "ver\u00b7zeihn", ",", "ist", "Chris\u00b7ten\u00b7pflicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "\u2013 (Heil, wer ihm sieben Rippen bricht!) \u2013", "tokens": ["\u2013", "(", "Heil", ",", "wer", "ihm", "sie\u00b7ben", "Rip\u00b7pen", "bricht", "!", ")", "\u2013"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "NN", "$,", "PWS", "PPER", "CARD", "NN", "VVFIN", "$.", "$(", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.35": {"text": "Wer trinkt, brennt einst im Schwefelloch,", "tokens": ["Wer", "trinkt", ",", "brennt", "einst", "im", "Schwe\u00b7fel\u00b7loch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "\u2013 (Doch brennt der Durst viel hei\u00dfer noch!)", "tokens": ["\u2013", "(", "Doch", "brennt", "der", "Durst", "viel", "hei\u00b7\u00dfer", "noch", "!", ")"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "KON", "VVFIN", "ART", "NN", "PIAT", "ADJA", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Heil, wer da stirbt in frommem Beten,", "tokens": ["Heil", ",", "wer", "da", "stirbt", "in", "from\u00b7mem", "Be\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "(doch sel'ger unter Kriegsdrommeten!) \u2013", "tokens": ["(", "doch", "sel'\u00b7ger", "un\u00b7ter", "Kriegs\u00b7drom\u00b7me\u00b7ten", "!", ")", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Jungfrau Maria preis' ich sehr,", "tokens": ["Jung\u00b7frau", "Ma\u00b7ria", "preis'", "ich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE",