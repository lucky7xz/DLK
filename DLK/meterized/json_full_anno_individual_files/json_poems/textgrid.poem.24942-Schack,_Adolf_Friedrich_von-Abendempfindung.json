{"textgrid.poem.24942": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Abendempfindung", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie s\u00fc\u00df im d\u00e4mmerhellen Walde,", "tokens": ["Wie", "s\u00fc\u00df", "im", "d\u00e4m\u00b7mer\u00b7hel\u00b7len", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Harzduft von den B\u00e4umen trieft,", "tokens": ["Wenn", "Harz\u00b7duft", "von", "den", "B\u00e4u\u00b7men", "trieft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu ruhen an der Bergeshalde,", "tokens": ["Zu", "ru\u00b7hen", "an", "der", "Ber\u00b7ges\u00b7hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In alter S\u00e4nger Lied vertieft!", "tokens": ["In", "al\u00b7ter", "S\u00e4n\u00b7ger", "Lied", "ver\u00b7tieft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Rings Stille, da\u00df vom L\u00e4rm der Erde", "tokens": ["Rings", "Stil\u00b7le", ",", "da\u00df", "vom", "L\u00e4rm", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum einen Ton dein Ohr vernimmt,", "tokens": ["Kaum", "ei\u00b7nen", "Ton", "dein", "Ohr", "ver\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als das Gel\u00e4ut der Ziegenherde,", "tokens": ["Als", "das", "Ge\u00b7l\u00e4ut", "der", "Zie\u00b7gen\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die einsam an der Halde klimmt.", "tokens": ["Die", "ein\u00b7sam", "an", "der", "Hal\u00b7de", "klimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und, wie dich aus den alten Rollen", "tokens": ["Und", ",", "wie", "dich", "aus", "den", "al\u00b7ten", "Rol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hauch vergangner Zeit umquillt,", "tokens": ["Der", "Hauch", "ver\u00b7gang\u00b7ner", "Zeit", "um\u00b7quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versinkt das Heut mit seinem Wollen", "tokens": ["Ver\u00b7sinkt", "das", "Heut", "mit", "sei\u00b7nem", "Wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Thun dir wie ein Schattenbild.", "tokens": ["Und", "Thun", "dir", "wie", "ein", "Schat\u00b7ten\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist diese Luft, die mir mit leisen", "tokens": ["Ist", "die\u00b7se", "Luft", ",", "die", "mir", "mit", "lei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "NN", "$,", "PRELS", "PPER", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Windhauchen um die Schl\u00e4fe spielt,", "tokens": ["Wind\u00b7hau\u00b7chen", "um", "die", "Schl\u00e4\u00b7fe", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht noch dieselbe, die den Weisen", "tokens": ["Nicht", "noch", "die\u00b7sel\u00b7be", ",", "die", "den", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PDAT", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Chald\u00e4as einst die Stirn gek\u00fchlt?", "tokens": ["Chald\u00b7\u00e4\u00b7as", "einst", "die", "Stirn", "ge\u00b7k\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Sah dem vergl\u00fch'nden Sonnengolde", "tokens": ["Sah", "dem", "ver\u00b7gl\u00fch'n\u00b7den", "Son\u00b7nen\u00b7gol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Im Westen dort nicht so wie du", "tokens": ["Im", "Wes\u00b7ten", "dort", "nicht", "so", "wie", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "PTKNEG", "ADV", "KOKOM", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An ihres Tristans Arm Isolde", "tokens": ["An", "ih\u00b7res", "Tris\u00b7tans", "Arm", "I\u00b7sol\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Waldesrande tr\u00e4umend zu?", "tokens": ["Vom", "Wal\u00b7des\u00b7ran\u00b7de", "tr\u00e4u\u00b7mend", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Unsterblich, wie vor tausend Jahren,", "tokens": ["U\u00b7nsterb\u00b7lich", ",", "wie", "vor", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPR", "CARD", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fchn noch die Fluren, gr\u00fcnt das Laub,", "tokens": ["Bl\u00fchn", "noch", "die", "Flu\u00b7ren", ",", "gr\u00fcnt", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Geschlechter, welche waren,", "tokens": ["Und", "die", "Ge\u00b7schlech\u00b7ter", ",", "wel\u00b7che", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie w\u00e4ren Asche nur und Staub?", "tokens": ["Sie", "w\u00e4\u00b7ren", "A\u00b7sche", "nur", "und", "Staub", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nein! in dem Werden und Entfalten", "tokens": ["Nein", "!", "in", "dem", "Wer\u00b7den", "und", "Ent\u00b7fal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zieht immer das Gewes'ne nur", "tokens": ["Zieht", "im\u00b7mer", "das", "Ge\u00b7wes'\u00b7ne", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch alle Formen und Gestalten", "tokens": ["Durch", "al\u00b7le", "For\u00b7men", "und", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der rastlos kreisenden Natur.", "tokens": ["Der", "rast\u00b7los", "krei\u00b7sen\u00b7den", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nicht anders lebst du selbst als jene,", "tokens": ["Nicht", "an\u00b7ders", "lebst", "du", "selbst", "als", "je\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "ADV", "KOUS", "PDS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die vor Jahrtausenden gelebt;", "tokens": ["Die", "vor", "Jahr\u00b7tau\u00b7sen\u00b7den", "ge\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Alt, wie die Erde, ist die Thr\u00e4ne,", "tokens": ["Alt", ",", "wie", "die", "Er\u00b7de", ",", "ist", "die", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die eben dir am Auge bebt.", "tokens": ["Die", "e\u00b7ben", "dir", "am", "Au\u00b7ge", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du denkst es; schon am Waldessaume", "tokens": ["Du", "denkst", "es", ";", "schon", "am", "Wal\u00b7des\u00b7sau\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erlosch die Glut des Abendscheins;", "tokens": ["Er\u00b7losch", "die", "Glut", "des", "A\u00b7bend\u00b7scheins", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es dunkelt, und du wirst im Traume,", "tokens": ["Es", "dun\u00b7kelt", ",", "und", "du", "wirst", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit allen, die gewesen, eins.", "tokens": ["Mit", "al\u00b7len", ",", "die", "ge\u00b7we\u00b7sen", ",", "eins", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "VAPP", "$,", "PIS", "$."], "meter": "-+-+-+-+", "measur