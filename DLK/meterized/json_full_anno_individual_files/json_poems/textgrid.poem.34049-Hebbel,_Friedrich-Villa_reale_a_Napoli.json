{"textgrid.poem.34049": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Villa reale a Napoli", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter duftigen B\u00e4umen, vom Hauch des Abends durchs\u00e4uselt,", "tokens": ["Un\u00b7ter", "duf\u00b7ti\u00b7gen", "B\u00e4u\u00b7men", ",", "vom", "Hauch", "des", "A\u00b7bends", "durch\u00b7s\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sammelt von reizenden Frau'n still sich ein gl\u00e4nzender Flor;", "tokens": ["Sam\u00b7melt", "von", "rei\u00b7zen\u00b7den", "Frau'n", "still", "sich", "ein", "gl\u00e4n\u00b7zen\u00b7der", "Flor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VMFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Leise ergie\u00dft sich der Strom melodischer Kl\u00e4nge und schaukelt", "tokens": ["Lei\u00b7se", "er\u00b7gie\u00dft", "sich", "der", "Strom", "me\u00b7lo\u00b7di\u00b7scher", "Kl\u00e4n\u00b7ge", "und", "schau\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "KON", "VVFIN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Zwischen Wonne und Weh jedes empf\u00e4ngliche Herz;", "tokens": ["Zwi\u00b7schen", "Won\u00b7ne", "und", "Weh", "je\u00b7des", "emp\u00b7f\u00e4ng\u00b7li\u00b7che", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Aber die Wogen des Meers, am nahen Gestade sich brechend", "tokens": ["A\u00b7ber", "die", "Wo\u00b7gen", "des", "Meers", ",", "am", "na\u00b7hen", "Ge\u00b7sta\u00b7de", "sich", "bre\u00b7chend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "PRF", "ADJD"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Und vom Winde geschwellt, donnern verhalten darein,", "tokens": ["Und", "vom", "Win\u00b7de", "ge\u00b7schwellt", ",", "don\u00b7nern", "ver\u00b7hal\u00b7ten", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$,", "KON", "VVFIN", "PAV", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "An die gewalt'gen Accorde der rollenden Sph\u00e4ren uns mahnend,", "tokens": ["An", "die", "ge\u00b7walt'\u00b7gen", "Ac\u00b7cor\u00b7de", "der", "rol\u00b7len\u00b7den", "Sph\u00e4\u00b7ren", "uns", "mah\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "---+--+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Welche f\u00fcr's menschliche Ohr sanft zur Musik sich ged\u00e4mpft.", "tokens": ["Wel\u00b7che", "f\u00fcr's", "menschli\u00b7che", "Ohr", "sanft", "zur", "Mu\u00b7sik", "sich", "ge\u00b7d\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "ADJA", "NN", "ADJD", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "+--+-++-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.2": {"line.1": {"text"