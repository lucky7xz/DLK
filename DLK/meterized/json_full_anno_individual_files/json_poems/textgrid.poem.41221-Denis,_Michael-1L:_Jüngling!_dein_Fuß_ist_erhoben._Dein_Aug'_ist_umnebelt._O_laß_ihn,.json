{"textgrid.poem.41221": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: J\u00fcngling! dein Fu\u00df ist erhoben. Dein Aug' ist umnebelt. O la\u00df ihn,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngling! dein Fu\u00df ist erhoben. Dein Aug' ist umnebelt. O la\u00df ihn,", "tokens": ["J\u00fcng\u00b7ling", "!", "dein", "Fu\u00df", "ist", "er\u00b7ho\u00b7ben", ".", "Dein", "Aug'", "ist", "um\u00b7ne\u00b7belt", ".", "O", "la\u00df", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "NE", "VVFIN", "PPER", "$,"], "meter": "+--+--+--+-+---+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "La\u00df in nicht nieder den Fu\u00df!", "tokens": ["La\u00df", "in", "nicht", "nie\u00b7der", "den", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PTKNEG", "PTKVZ", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "W\u00e4hnest du Blumen zu treten? O nein! Ein schrecklicher Abgrund", "tokens": ["W\u00e4h\u00b7nest", "du", "Blu\u00b7men", "zu", "tre\u00b7ten", "?", "O", "nein", "!", "Ein", "schreck\u00b7li\u00b7cher", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$.", "NE", "PTKANT", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Oeffnet den Rachen vor dir.", "tokens": ["Oeff\u00b7net", "den", "Ra\u00b7chen", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Aber du kannst ihn nicht sehen. Dein Aug' ist umnebelt. O Liebling", "tokens": ["A\u00b7ber", "du", "kannst", "ihn", "nicht", "se\u00b7hen", ".", "Dein", "Aug'", "ist", "um\u00b7ne\u00b7belt", ".", "O", "Lieb\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "NE", "NN"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Meiner Seele die Hand!", "tokens": ["Mei\u00b7ner", "See\u00b7le", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Reiche du treuen Gef\u00e4hrten die Hand! \u2013 Nun will ich dich halten,", "tokens": ["Rei\u00b7che", "du", "treu\u00b7en", "Ge\u00b7f\u00e4hr\u00b7ten", "die", "Hand", "!", "\u2013", "Nun", "will", "ich", "dich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$.", "$(", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Bis sich der Nebel zertheilt. \u2013", "tokens": ["Bis", "sich", "der", "Ne\u00b7bel", "zer\u00b7theilt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ach wie brennet die Hand, wie klopfet dein Busen, wie zweifelnd", "tokens": ["Ach", "wie", "bren\u00b7net", "die", "Hand", ",", "wie", "klop\u00b7fet", "dein", "Bu\u00b7sen", ",", "wie", "zwei\u00b7felnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KOKOM", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADJD"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Blicket dein sch\u00fcchternes Aug'!", "tokens": ["Bli\u00b7cket", "dein", "sch\u00fcch\u00b7ter\u00b7nes", "Aug'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "J\u00fcngling! denke den liebenden Vater, die z\u00e4rtliche Mutter!", "tokens": ["J\u00fcng\u00b7ling", "!", "den\u00b7ke", "den", "lie\u00b7ben\u00b7den", "Va\u00b7ter", ",", "die", "z\u00e4rt\u00b7li\u00b7che", "Mut\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "S\u00e4hen sie deine Gefahr,", "tokens": ["S\u00e4\u00b7hen", "sie", "dei\u00b7ne", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Ach sie riefen dir zu mit bebendem Rufen: O la\u00df ihn,", "tokens": ["Ach", "sie", "rie\u00b7fen", "dir", "zu", "mit", "be\u00b7ben\u00b7dem", "Ru\u00b7fen", ":", "O", "la\u00df", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$.", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.14": {"text": "La\u00df ihn nicht nieder den Fu\u00df!", "tokens": ["La\u00df", "ihn", "nicht", "nie\u00b7der", "den", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Denke die Reue! So wahr ich dich liebe, so wahr du mich liebest,", "tokens": ["Den\u00b7ke", "die", "Reu\u00b7e", "!", "So", "wahr", "ich", "dich", "lie\u00b7be", ",", "so", "wahr", "du", "mich", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$.", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+--+--+-+++-+-", "measure": "dactylic.tri.plus"}, "line.16": {"text": "Z\u00fcckt sie schon jetzo den Dolch,", "tokens": ["Z\u00fcckt", "sie", "schon", "jet\u00b7zo", "den", "Dolch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "St\u00f6\u00dft ihn, so bald du den Abgrund erreichest, dir tief in den Busen,", "tokens": ["St\u00f6\u00dft", "ihn", ",", "so", "bald", "du", "den", "Ab\u00b7grund", "er\u00b7rei\u00b7chest", ",", "dir", "tief", "in", "den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "ART", "NN", "VVFIN", "$,", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "O dann erwachet ein Weh,", "tokens": ["O", "dann", "er\u00b7wa\u00b7chet", "ein", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Lang, wie der Schlummer im Hause des Todes, nach fl\u00fcchtigen Tr\u00e4umen,", "tokens": ["Lang", ",", "wie", "der", "Schlum\u00b7mer", "im", "Hau\u00b7se", "des", "To\u00b7des", ",", "nach", "fl\u00fcch\u00b7ti\u00b7gen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Wilder, entehrender Lust.", "tokens": ["Wil\u00b7der", ",", "en\u00b7teh\u00b7ren\u00b7der", "Lust", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Und ich s\u00e4he dich leiden? O schone des treuen Gef\u00e4hrten,", "tokens": ["Und", "ich", "s\u00e4\u00b7he", "dich", "lei\u00b7den", "?", "O", "scho\u00b7ne", "des", "treu\u00b7en", "Ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF", "$.", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Dessen Seele dich liebt!", "tokens": ["Des\u00b7sen", "See\u00b7le", "dich", "liebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Halte dich fest am st\u00e4rkenden Arme des Barden, und la\u00df ihn,", "tokens": ["Hal\u00b7te", "dich", "fest", "am", "st\u00e4r\u00b7ken\u00b7den", "Ar\u00b7me", "des", "Bar\u00b7den", ",", "und", "la\u00df", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "KON", "VVIMP", "PPER", "$,"], "meter": "+--+-+--+--+-++-", "measure": "iambic.septa.invert"}, "line.24": {"text": "La\u00df ihn nicht nieder den Fu\u00df!", "tokens": ["La\u00df", "ihn", "nicht", "nie\u00b7der", "den", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Bald wird der Nebel verduften, dein Aug' den schrecklichen Abgrund", "tokens": ["Bald", "wird", "der", "Ne\u00b7bel", "ver\u00b7duf\u00b7ten", ",", "dein", "Aug'", "den", "schreck\u00b7li\u00b7chen", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Schwindelnd erblicken und fliehn,", "tokens": ["Schwin\u00b7delnd", "er\u00b7bli\u00b7cken", "und", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Du den treuen Gef\u00e4hrten umarmen, von reinerer Wollust", "tokens": ["Du", "den", "treu\u00b7en", "Ge\u00b7f\u00e4hr\u00b7ten", "um\u00b7ar\u00b7men", ",", "von", "rei\u00b7ne\u00b7rer", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Freudegef\u00fchlen ergl\u00fch'n.", "tokens": ["Freu\u00b7de\u00b7ge\u00b7f\u00fch\u00b7len", "er\u00b7gl\u00fch'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["NN", "VVFIN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "Aber, wann jetzo die Zahl von deinen Wintern heranw\u00e4chst,", "tokens": ["A\u00b7ber", ",", "wann", "jet\u00b7zo", "die", "Zahl", "von", "dei\u00b7nen", "Win\u00b7tern", "her\u00b7an\u00b7w\u00e4chst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Jetzo dein blumiges Kinn", "tokens": ["Jet\u00b7zo", "dein", "blu\u00b7mi\u00b7ges", "Kinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.31": {"text": "Mannheit beschattet, mit dir die B\u00fcrden und Wonnen des Lebens", "tokens": ["Mann\u00b7heit", "be\u00b7schat\u00b7tet", ",", "mit", "dir", "die", "B\u00fcr\u00b7den", "und", "Won\u00b7nen", "des", "Le\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "APPR", "PPER", "ART", "NN", "KON", "NN", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Theilet ein keusches Gemal,", "tokens": ["Thei\u00b7let", "ein", "keu\u00b7sches", "Ge\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.33": {"text": "Und dein ruhiges Aug' auf deiner geretteten Jugend", "tokens": ["Und", "dein", "ru\u00b7hi\u00b7ges", "Aug'", "auf", "dei\u00b7ner", "ge\u00b7ret\u00b7te\u00b7ten", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Heitere Tage nun blickt,", "tokens": ["Hei\u00b7te\u00b7re", "Ta\u00b7ge", "nun", "blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "O dann rufst du: Wo ist er, mein Retter, mein treuer Gef\u00e4hrte,", "tokens": ["O", "dann", "rufst", "du", ":", "Wo", "ist", "er", ",", "mein", "Ret\u00b7ter", ",", "mein", "treu\u00b7er", "Ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "$.", "PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Der mich liebte, mein Freund?", "tokens": ["Der", "mich", "lieb\u00b7te", ",", "mein", "Freund", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Nun erst kenn' ich ihn ganz den Werth von seinem Geleite,", "tokens": ["Nun", "erst", "kenn'", "ich", "ihn", "ganz", "den", "Werth", "von", "sei\u00b7nem", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Ganz sein z\u00e4rtliches Herz! \u2013", "tokens": ["Ganz", "sein", "z\u00e4rt\u00b7li\u00b7ches", "Herz", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.39": {"text": "J\u00fcngling! Sined ist todt. Von seiner verlassenen Halle", "tokens": ["J\u00fcng\u00b7ling", "!", "Si\u00b7ned", "ist", "todt", ".", "Von", "sei\u00b7ner", "ver\u00b7las\u00b7se\u00b7nen", "Hal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "VAFIN", "ADJD", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.40": {"text": "T\u00f6net kein freundlicher Laut,", "tokens": ["T\u00f6\u00b7net", "kein", "freund\u00b7li\u00b7cher", "Laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.41": {"text": "Leitet kein Fu\u00dftritt in Schatten. Ihm haben die S\u00f6hne der Lieder", "tokens": ["Lei\u00b7tet", "kein", "Fu\u00df\u00b7tritt", "in", "Schat\u00b7ten", ".", "Ihm", "ha\u00b7ben", "die", "S\u00f6h\u00b7ne", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.42": {"text": "Traurig sein Grabmal erh\u00f6ht,", "tokens": ["Trau\u00b7rig", "sein", "Grab\u00b7mal", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.43": {"text": "Haben ihm Namen gegeben; doch war ihm der liebste, dein Retter,", "tokens": ["Ha\u00b7ben", "ihm", "Na\u00b7men", "ge\u00b7ge\u00b7ben", ";", "doch", "war", "ihm", "der", "liebs\u00b7te", ",", "dein", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$.", "ADV", "VAFIN", "PPER", "ART", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.44": {"text": "J\u00fcngling! gewesen zu seyn.", "tokens": ["J\u00fcng\u00b7ling", "!", "ge\u00b7we\u00b7sen", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAPP", "PTKZU", "VAINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "J\u00fcngling! dein Fu\u00df ist erhoben. Dein Aug' ist umnebelt. O la\u00df ihn,", "tokens": ["J\u00fcng\u00b7ling", "!", "dein", "Fu\u00df", "ist", "er\u00b7ho\u00b7ben", ".", "Dein", "Aug'", "ist", "um\u00b7ne\u00b7belt", ".", "O", "la\u00df", "ihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "NE", "VVFIN", "PPER", "$,"], "meter": "+--+--+--+-+---+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "La\u00df in nicht nieder den Fu\u00df!", "tokens": ["La\u00df", "in", "nicht", "nie\u00b7der", "den", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PTKNEG", "PTKVZ", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "W\u00e4hnest du Blumen zu treten? O nein! Ein schrecklicher Abgrund", "tokens": ["W\u00e4h\u00b7nest", "du", "Blu\u00b7men", "zu", "tre\u00b7ten", "?", "O", "nein", "!", "Ein", "schreck\u00b7li\u00b7cher", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$.", "NE", "PTKANT", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Oeffnet den Rachen vor dir.", "tokens": ["Oeff\u00b7net", "den", "Ra\u00b7chen", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Aber du kannst ihn nicht sehen. Dein Aug' ist umnebelt. O Liebling", "tokens": ["A\u00b7ber", "du", "kannst", "ihn", "nicht", "se\u00b7hen", ".", "Dein", "Aug'", "ist", "um\u00b7ne\u00b7belt", ".", "O", "Lieb\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "NE", "NN"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Meiner Seele die Hand!", "tokens": ["Mei\u00b7ner", "See\u00b7le", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Reiche du treuen Gef\u00e4hrten die Hand! \u2013 Nun will ich dich halten,", "tokens": ["Rei\u00b7che", "du", "treu\u00b7en", "Ge\u00b7f\u00e4hr\u00b7ten", "die", "Hand", "!", "\u2013", "Nun", "will", "ich", "dich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$.", "$(", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Bis sich der Nebel zertheilt. \u2013", "tokens": ["Bis", "sich", "der", "Ne\u00b7bel", "zer\u00b7theilt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ach wie brennet die Hand, wie klopfet dein Busen, wie zweifelnd", "tokens": ["Ach", "wie", "bren\u00b7net", "die", "Hand", ",", "wie", "klop\u00b7fet", "dein", "Bu\u00b7sen", ",", "wie", "zwei\u00b7felnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KOKOM", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADJD"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Blicket dein sch\u00fcchternes Aug'!", "tokens": ["Bli\u00b7cket", "dein", "sch\u00fcch\u00b7ter\u00b7nes", "Aug'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "J\u00fcngling! denke den liebenden Vater, die z\u00e4rtliche Mutter!", "tokens": ["J\u00fcng\u00b7ling", "!", "den\u00b7ke", "den", "lie\u00b7ben\u00b7den", "Va\u00b7ter", ",", "die", "z\u00e4rt\u00b7li\u00b7che", "Mut\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "S\u00e4hen sie deine Gefahr,", "tokens": ["S\u00e4\u00b7hen", "sie", "dei\u00b7ne", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Ach sie riefen dir zu mit bebendem Rufen: O la\u00df ihn,", "tokens": ["Ach", "sie", "rie\u00b7fen", "dir", "zu", "mit", "be\u00b7ben\u00b7dem", "Ru\u00b7fen", ":", "O", "la\u00df", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$.", "NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.14": {"text": "La\u00df ihn nicht nieder den Fu\u00df!", "tokens": ["La\u00df", "ihn", "nicht", "nie\u00b7der", "den", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Denke die Reue! So wahr ich dich liebe, so wahr du mich liebest,", "tokens": ["Den\u00b7ke", "die", "Reu\u00b7e", "!", "So", "wahr", "ich", "dich", "lie\u00b7be", ",", "so", "wahr", "du", "mich", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$.", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,", "ADV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+--+--+-+++-+-", "measure": "dactylic.tri.plus"}, "line.16": {"text": "Z\u00fcckt sie schon jetzo den Dolch,", "tokens": ["Z\u00fcckt", "sie", "schon", "jet\u00b7zo", "den", "Dolch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "St\u00f6\u00dft ihn, so bald du den Abgrund erreichest, dir tief in den Busen,", "tokens": ["St\u00f6\u00dft", "ihn", ",", "so", "bald", "du", "den", "Ab\u00b7grund", "er\u00b7rei\u00b7chest", ",", "dir", "tief", "in", "den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "ART", "NN", "VVFIN", "$,", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "O dann erwachet ein Weh,", "tokens": ["O", "dann", "er\u00b7wa\u00b7chet", "ein", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Lang, wie der Schlummer im Hause des Todes, nach fl\u00fcchtigen Tr\u00e4umen,", "tokens": ["Lang", ",", "wie", "der", "Schlum\u00b7mer", "im", "Hau\u00b7se", "des", "To\u00b7des", ",", "nach", "fl\u00fcch\u00b7ti\u00b7gen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Wilder, entehrender Lust.", "tokens": ["Wil\u00b7der", ",", "en\u00b7teh\u00b7ren\u00b7der", "Lust", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Und ich s\u00e4he dich leiden? O schone des treuen Gef\u00e4hrten,", "tokens": ["Und", "ich", "s\u00e4\u00b7he", "dich", "lei\u00b7den", "?", "O", "scho\u00b7ne", "des", "treu\u00b7en", "Ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF", "$.", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Dessen Seele dich liebt!", "tokens": ["Des\u00b7sen", "See\u00b7le", "dich", "liebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Halte dich fest am st\u00e4rkenden Arme des Barden, und la\u00df ihn,", "tokens": ["Hal\u00b7te", "dich", "fest", "am", "st\u00e4r\u00b7ken\u00b7den", "Ar\u00b7me", "des", "Bar\u00b7den", ",", "und", "la\u00df", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "KON", "VVIMP", "PPER", "$,"], "meter": "+--+-+--+--+-++-", "measure": "iambic.septa.invert"}, "line.24": {"text": "La\u00df ihn nicht nieder den Fu\u00df!", "tokens": ["La\u00df", "ihn", "nicht", "nie\u00b7der", "den", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Bald wird der Nebel verduften, dein Aug' den schrecklichen Abgrund", "tokens": ["Bald", "wird", "der", "Ne\u00b7bel", "ver\u00b7duf\u00b7ten", ",", "dein", "Aug'", "den", "schreck\u00b7li\u00b7chen", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Schwindelnd erblicken und fliehn,", "tokens": ["Schwin\u00b7delnd", "er\u00b7bli\u00b7cken", "und", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Du den treuen Gef\u00e4hrten umarmen, von reinerer Wollust", "tokens": ["Du", "den", "treu\u00b7en", "Ge\u00b7f\u00e4hr\u00b7ten", "um\u00b7ar\u00b7men", ",", "von", "rei\u00b7ne\u00b7rer", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Freudegef\u00fchlen ergl\u00fch'n.", "tokens": ["Freu\u00b7de\u00b7ge\u00b7f\u00fch\u00b7len", "er\u00b7gl\u00fch'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["NN", "VVFIN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "Aber, wann jetzo die Zahl von deinen Wintern heranw\u00e4chst,", "tokens": ["A\u00b7ber", ",", "wann", "jet\u00b7zo", "die", "Zahl", "von", "dei\u00b7nen", "Win\u00b7tern", "her\u00b7an\u00b7w\u00e4chst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Jetzo dein blumiges Kinn", "tokens": ["Jet\u00b7zo", "dein", "blu\u00b7mi\u00b7ges", "Kinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.31": {"text": "Mannheit beschattet, mit dir die B\u00fcrden und Wonnen des Lebens", "tokens": ["Mann\u00b7heit", "be\u00b7schat\u00b7tet", ",", "mit", "dir", "die", "B\u00fcr\u00b7den", "und", "Won\u00b7nen", "des", "Le\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "APPR", "PPER", "ART", "NN", "KON", "NN", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Theilet ein keusches Gemal,", "tokens": ["Thei\u00b7let", "ein", "keu\u00b7sches", "Ge\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.33": {"text": "Und dein ruhiges Aug' auf deiner geretteten Jugend", "tokens": ["Und", "dein", "ru\u00b7hi\u00b7ges", "Aug'", "auf", "dei\u00b7ner", "ge\u00b7ret\u00b7te\u00b7ten", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Heitere Tage nun blickt,", "tokens": ["Hei\u00b7te\u00b7re", "Ta\u00b7ge", "nun", "blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "O dann rufst du: Wo ist er, mein Retter, mein treuer Gef\u00e4hrte,", "tokens": ["O", "dann", "rufst", "du", ":", "Wo", "ist", "er", ",", "mein", "Ret\u00b7ter", ",", "mein", "treu\u00b7er", "Ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "$.", "PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Der mich liebte, mein Freund?", "tokens": ["Der", "mich", "lieb\u00b7te", ",", "mein", "Freund", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Nun erst kenn' ich ihn ganz den Werth von seinem Geleite,", "tokens": ["Nun", "erst", "kenn'", "ich", "ihn", "ganz", "den", "Werth", "von", "sei\u00b7nem", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Ganz sein z\u00e4rtliches Herz! \u2013", "tokens": ["Ganz", "sein", "z\u00e4rt\u00b7li\u00b7ches", "Herz", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.39": {"text": "J\u00fcngling! Sined ist todt. Von seiner verlassenen Halle", "tokens": ["J\u00fcng\u00b7ling", "!", "Si\u00b7ned", "ist", "todt", ".", "Von", "sei\u00b7ner", "ver\u00b7las\u00b7se\u00b7nen", "Hal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "VAFIN", "ADJD", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.40": {"text": "T\u00f6net kein freundlicher Laut,", "tokens": ["T\u00f6\u00b7net", "kein", "freund\u00b7li\u00b7cher", "Laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.41": {"text": "Leitet kein Fu\u00dftritt in Schatten. Ihm haben die S\u00f6hne der Lieder", "tokens": ["Lei\u00b7tet", "kein", "Fu\u00df\u00b7tritt", "in", "Schat\u00b7ten", ".", "Ihm", "ha\u00b7ben", "die", "S\u00f6h\u00b7ne", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.42": {"text": "Traurig sein Grabmal erh\u00f6ht,", "tokens": ["Trau\u00b7rig", "sein", "Grab\u00b7mal", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.43": {"text": "Haben ihm Namen gegeben; doch war ihm der liebste, dein Retter,", "tokens": ["Ha\u00b7ben", "ihm", "Na\u00b7men", "ge\u00b7ge\u00b7ben", ";", "doch", "war", "ihm", "der", "liebs\u00b7te", ",", "dein", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$.", "ADV", "VAFIN", "PPER", "ART", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.44": {"text": "J\u00fcngling! gewesen zu seyn.", "tokens": ["J\u00fcng\u00b7ling", "!", "ge\u00b7we\u00b7sen", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAPP", "PTKZU", "VAINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}