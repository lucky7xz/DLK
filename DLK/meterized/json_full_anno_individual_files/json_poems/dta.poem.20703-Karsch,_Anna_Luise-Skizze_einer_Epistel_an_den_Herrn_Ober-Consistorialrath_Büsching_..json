{"dta.poem.20703": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Skizze einer Epistel                  \n  an                  \n  den Herrn Ober-Consistorialrath  B\u00fcsching .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ehrw\u00fcrdiger, und Liebenswerther,                 ", "tokens": ["Ehr\u00b7w\u00fcr\u00b7di\u00b7ger", ",", "und", "Lie\u00b7bens\u00b7wert\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer th\u00e4tiger, ber\u00fchmter, gro\u00dfer Mann!", "tokens": ["Und", "im\u00b7mer", "th\u00e4\u00b7ti\u00b7ger", ",", "be\u00b7r\u00fchm\u00b7ter", ",", "gro\u00b7\u00dfer", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Krankheit w\u00fcthet oft, wie Dolche, Pfeil und", "tokens": ["Die", "Krank\u00b7heit", "w\u00fct\u00b7het", "oft", ",", "wie", "Dol\u00b7che", ",", "Pfeil", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwerdter,", "tokens": ["Schwerd\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Greift aber nicht den Geist, greift nur den K\u00f6rper an,", "tokens": ["Greift", "a\u00b7ber", "nicht", "den", "Geist", ",", "greift", "nur", "den", "K\u00f6r\u00b7per", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In Menschen, die viel Geist besitzen \u2014", "tokens": ["In", "Men\u00b7schen", ",", "die", "viel", "Geist", "be\u00b7sit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drei Sommer fanden Dich schon auf der Kran-", "tokens": ["Drei", "Som\u00b7mer", "fan\u00b7den", "Dich", "schon", "auf", "der", "Kran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "kenbahn,", "tokens": ["ken\u00b7bahn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Und doch kannst Du der Welt noch n\u00fctzen,", "tokens": ["Und", "doch", "kannst", "Du", "der", "Welt", "noch", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und hast schon viel f\u00fcr sie gethan;", "tokens": ["Und", "hast", "schon", "viel", "f\u00fcr", "sie", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der junge Morgen k\u00f6mmt und siehet", "tokens": ["Der", "jun\u00b7ge", "Mor\u00b7gen", "k\u00f6mmt", "und", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dich heiter in Gesch\u00e4ftigkeit,", "tokens": ["Dich", "hei\u00b7ter", "in", "Ge\u00b7sch\u00e4f\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ob gleich der Schlaf vom Auge fliehet,", "tokens": ["Ob", "gleich", "der", "Schlaf", "vom", "Au\u00b7ge", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der sonst den Kranken Kraft verleiht;", "tokens": ["Der", "sonst", "den", "Kran\u00b7ken", "Kraft", "ver\u00b7leiht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ob Du gleich unter tausend B\u00fcchern", "tokens": ["Ob", "Du", "gleich", "un\u00b7ter", "tau\u00b7send", "B\u00fc\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die Nacht hindurch zu wandeln pflegst,", "tokens": ["Die", "Nacht", "hin\u00b7durch", "zu", "wan\u00b7deln", "pflegst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und die Geduld im glaubenssichern,", "tokens": ["Und", "die", "Ge\u00b7duld", "im", "glau\u00b7bens\u00b7si\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Gott unterworfnem Herzen tr\u00e4gst;", "tokens": ["Gott", "un\u00b7ter\u00b7worf\u00b7nem", "Her\u00b7zen", "tr\u00e4gst", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "So schonest Du Dich doch mit nichten,", "tokens": ["So", "scho\u00b7nest", "Du", "Dich", "doch", "mit", "nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PIS", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.20": {"text": "Du giebst ein Beyspiel rund herum,", "tokens": ["Du", "giebst", "ein", "Bey\u00b7spiel", "rund", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und bleibst nun bald ein Viertel-Sekulum", "tokens": ["Und", "bleibst", "nun", "bald", "ein", "Vier\u00b7tel\u00b7Se\u00b7ku\u00b7lum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Treu den Direktor-Pflichten", "tokens": ["Treu", "den", "Di\u00b7rek\u00b7tor\u00b7Pflich\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.23": {"text": "Im Kloster-Stift Gymnasium \u2014", "tokens": ["Im", "Klos\u00b7ter\u00b7Stift", "Gym\u00b7na\u00b7si\u00b7um"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Selbst, da Dirs nicht erlaubt gewesen", "tokens": ["Selbst", ",", "da", "Dirs", "nicht", "er\u00b7laubt", "ge\u00b7we\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "NN", "PTKNEG", "VVPP", "VAPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Hinab zu gehn im Klassensaal,", "tokens": ["Hin\u00b7ab", "zu", "gehn", "im", "Klas\u00b7sen\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Hast Du mit Worten auserlesen,", "tokens": ["Hast", "Du", "mit", "Wor\u00b7ten", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Was Vaterliebe Dir befahl,", "tokens": ["Was", "Va\u00b7ter\u00b7lie\u00b7be", "Dir", "be\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "In Deinem eignen Musensaal,", "tokens": ["In", "Dei\u00b7nem", "eig\u00b7nen", "Mu\u00b7sen\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Den J\u00fcnglingen gesagt, so munter,", "tokens": ["Den", "J\u00fcng\u00b7lin\u00b7gen", "ge\u00b7sagt", ",", "so", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.30": {"text": "Als w\u00e4rest Du nicht krank einmal;", "tokens": ["Als", "w\u00e4\u00b7rest", "Du", "nicht", "krank", "ein\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Mein Tochtersohn war mit darunter! \u2014 u. s. w.", "tokens": ["Mein", "Toch\u00b7ter\u00b7sohn", "war", "mit", "da\u00b7run\u00b7ter", "!", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PAV", "$.", "$(", "KON", "VVIMP", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}