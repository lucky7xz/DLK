{"textgrid.poem.24926": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das verschlossene Thor", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinw\u00e4lzt sich wild durch Ktesiphon", "tokens": ["Hin\u00b7w\u00e4lzt", "sich", "wild", "durch", "Kte\u00b7si\u00b7phon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das R\u00f6merheer mit Brand und Morde.", "tokens": ["Das", "R\u00f6\u00b7mer\u00b7heer", "mit", "Brand", "und", "Mor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verw\u00fcstet hat die R\u00e4uberhorde", "tokens": ["Ver\u00b7w\u00fcs\u00b7tet", "hat", "die", "R\u00e4u\u00b7ber\u00b7hor\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ormuzd gro\u00dfen Tempel schon;", "tokens": ["Des", "Or\u00b7muzd", "gro\u00b7\u00dfen", "Tem\u00b7pel", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Reihn gesunkener Pilaster", "tokens": ["Durch", "Reihn", "ge\u00b7sun\u00b7ke\u00b7ner", "Pi\u00b7las\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nun dringt sie in die Halle vor,", "tokens": ["Nun", "dringt", "sie", "in", "die", "Hal\u00b7le", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo um das Feu'r des Zoroaster", "tokens": ["Wo", "um", "das", "Feu'r", "des", "Zo\u00b7roas\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Anbetend kniet ein Magierchor", "tokens": ["An\u00b7be\u00b7tend", "kniet", "ein", "Ma\u00b7gier\u00b7chor"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Und Greife von Granit und Drachen", "tokens": ["Und", "Grei\u00b7fe", "von", "Gra\u00b7nit", "und", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NE", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Ein ries'ges Thor von Erz bewachen.", "tokens": ["Ein", "ries'\u00b7ges", "Thor", "von", "Erz", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In krausen Z\u00fcgen wunderbar", "tokens": ["In", "krau\u00b7sen", "Z\u00fc\u00b7gen", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flammt Spruch auf Spruch an jenem Thore,", "tokens": ["Flammt", "Spruch", "auf", "Spruch", "an", "je\u00b7nem", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dumpf schallt zu der R\u00f6mer Ohre", "tokens": ["Und", "dumpf", "schallt", "zu", "der", "R\u00f6\u00b7mer", "Oh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lied der Magier vom Altar:", "tokens": ["Das", "Lied", "der", "Ma\u00b7gier", "vom", "Al\u00b7tar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "\u00bbihr Priester, sch\u00fcrt das heil'ge Feuer,", "tokens": ["\u00bb", "ihr", "Pries\u00b7ter", ",", "sch\u00fcrt", "das", "heil'\u00b7ge", "Feu\u00b7er", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uebt an der Pforte treu die Wacht!", "tokens": ["Uebt", "an", "der", "Pfor\u00b7te", "treu", "die", "Wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Gebunden sind die Ungeheuer,", "tokens": ["Ge\u00b7bun\u00b7den", "sind", "die", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Schreckgeburt der alten Nacht,", "tokens": ["Die", "Schreck\u00b7ge\u00b7burt", "der", "al\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So lang der d\u00fcstre Schlund versiegelt;", "tokens": ["So", "lang", "der", "d\u00fcst\u00b7re", "Schlund", "ver\u00b7sie\u00b7gelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Weh, w\u00fcrde je das Thor entriegelt!\u00ab", "tokens": ["Weh", ",", "w\u00fcr\u00b7de", "je", "das", "Thor", "ent\u00b7rie\u00b7gelt", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch der betroffnen Krieger Herz", "tokens": ["Durch", "der", "be\u00b7troff\u00b7nen", "Krie\u00b7ger", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ziehn Schauer hin bei dem Gesange;", "tokens": ["Ziehn", "Schau\u00b7er", "hin", "bei", "dem", "Ge\u00b7san\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der eine weicht zur Seite bange;", "tokens": ["Der", "ei\u00b7ne", "weicht", "zur", "Sei\u00b7te", "ban\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheu blickt der andre bodenw\u00e4rts.", "tokens": ["Scheu", "blickt", "der", "and\u00b7re", "bo\u00b7den\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Feldherr aber: \u00bbSeid ihr Memmen,", "tokens": ["Der", "Feld\u00b7herr", "a\u00b7ber", ":", "\u00bb", "Seid", "ihr", "Mem\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "$(", "VAIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihr vor M\u00e4rchen zagen wollt?", "tokens": ["Da\u00df", "ihr", "vor", "M\u00e4r\u00b7chen", "za\u00b7gen", "wollt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschwind, die Eisen einzustemmen!", "tokens": ["Ge\u00b7schwind", ",", "die", "Ei\u00b7sen", "ein\u00b7zu\u00b7stem\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Riegel sprengt! Sie bergen Gold!", "tokens": ["Die", "Rie\u00b7gel", "sprengt", "!", "Sie", "ber\u00b7gen", "Gold", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Das ist der Sinn der Zauberworte!\u00ab", "tokens": ["Das", "ist", "der", "Sinn", "der", "Zau\u00b7ber\u00b7wor\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er ruft's und bricht sich Bahn zur Pforte.", "tokens": ["Er", "ruft's", "und", "bricht", "sich", "Bahn", "zur", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Magier nahen pflichtgetreu,", "tokens": ["Die", "Ma\u00b7gier", "na\u00b7hen", "pflicht\u00b7ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Weg dem R\u00e4uber zu vertreten!", "tokens": ["Den", "Weg", "dem", "R\u00e4u\u00b7ber", "zu", "ver\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Zoroaster, dem Propheten,", "tokens": ["Bei", "Zo\u00b7roas\u00b7ter", ",", "dem", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Verwegner, weich' von hinnen scheu!\u00ab", "tokens": ["Ver\u00b7weg\u00b7ner", ",", "weich'", "von", "hin\u00b7nen", "scheu", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch jener hebt das Beil; als Tote", "tokens": ["Doch", "je\u00b7ner", "hebt", "das", "Beil", ";", "als", "To\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "$.", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hinsinken sie von seinem Stahl;", "tokens": ["Hin\u00b7sin\u00b7ken", "sie", "von", "sei\u00b7nem", "Stahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das heil'ge Feuer schl\u00e4gt, das rote,", "tokens": ["Das", "heil'\u00b7ge", "Feu\u00b7er", "schl\u00e4gt", ",", "das", "ro\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch einmal auf mit hellem Strahl;", "tokens": ["Noch", "ein\u00b7mal", "auf", "mit", "hel\u00b7lem", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Altar sinkt in dumpfem Falle,", "tokens": ["Der", "Al\u00b7tar", "sinkt", "in", "dum\u00b7pfem", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und finster wird es in der Halle.", "tokens": ["Und", "fins\u00b7ter", "wird", "es", "in", "der", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Klammern bricht der Feldherr los;", "tokens": ["Die", "Klam\u00b7mern", "bricht", "der", "Feld\u00b7herr", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kracht das Thor; die Riegel springen;", "tokens": ["Es", "kracht", "das", "Thor", ";", "die", "Rie\u00b7gel", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Giftqualm und Schwefeld\u00fcnste dringen", "tokens": ["Gift\u00b7qualm", "und", "Schwe\u00b7fel\u00b7d\u00fcns\u00b7te", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gewitternd aus des Abgrunds Scho\u00df;", "tokens": ["Ge\u00b7wit\u00b7ternd", "aus", "des", "Ab\u00b7grunds", "Scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alle taumeln h\u00e4uptlings nieder \u2013", "tokens": ["Und", "al\u00b7le", "tau\u00b7meln", "h\u00e4upt\u00b7lings", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch siehe, wo die Tiefe klafft,", "tokens": ["Doch", "sie\u00b7he", ",", "wo", "die", "Tie\u00b7fe", "klafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da regt sich's, halb wie Menschenglieder,", "tokens": ["Da", "regt", "sich's", ",", "halb", "wie", "Men\u00b7schen\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Halb wie D\u00e4monen; grauenhaft,", "tokens": ["Halb", "wie", "D\u00e4\u00b7mo\u00b7nen", ";", "grau\u00b7en\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$.", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Vom Sturm emporgewirbelt, steigen", "tokens": ["Vom", "Sturm", "em\u00b7por\u00b7ge\u00b7wir\u00b7belt", ",", "stei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Drei Weiber auf in wildem Reigen.", "tokens": ["Drei", "Wei\u00b7ber", "auf", "in", "wil\u00b7dem", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die eine schwingt in rechter Hand", "tokens": ["Die", "ei\u00b7ne", "schwingt", "in", "rech\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gei\u00dfel, die zu Raub und Morden", "tokens": ["Die", "Gei\u00b7\u00dfel", ",", "die", "zu", "Raub", "und", "Mor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aufpeitscht die wilden V\u00f6lkerhorden,", "tokens": ["Auf\u00b7peitscht", "die", "wil\u00b7den", "V\u00f6l\u00b7ker\u00b7hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In linker einen Feuerbrand;", "tokens": ["In", "lin\u00b7ker", "ei\u00b7nen", "Feu\u00b7er\u00b7brand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weithin durch die beeisten Oeden", "tokens": ["Weit\u00b7hin", "durch", "die", "be\u00b7eis\u00