{"textgrid.poem.35701": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "25. Fr\u00fchlingsliebe", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche sang, die Sonne schien,", "tokens": ["Die", "Ler\u00b7che", "sang", ",", "die", "Son\u00b7ne", "schien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es f\u00e4rbte sich die Wiese gr\u00fcn,", "tokens": ["Es", "f\u00e4rb\u00b7te", "sich", "die", "Wie\u00b7se", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und braungeschwollne Keime", "tokens": ["Und", "braun\u00b7ge\u00b7schwoll\u00b7ne", "Kei\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versch\u00f6nten B\u00fcsch' und B\u00e4ume:", "tokens": ["Ver\u00b7sch\u00f6n\u00b7ten", "B\u00fcsch'", "und", "B\u00e4u\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da pfl\u00fcckt' ich am bedornten See", "tokens": ["Da", "pfl\u00fcckt'", "ich", "am", "be\u00b7dorn\u00b7ten", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Strau\u00df ihr, unter sp\u00e4tem Schnee,", "tokens": ["Zum", "Strau\u00df", "ihr", ",", "un\u00b7ter", "sp\u00e4\u00b7tem", "Schnee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Blau, rot und wei\u00dfen G\u00fcldenklee.", "tokens": ["Blau", ",", "rot", "und", "wei\u00b7\u00dfen", "G\u00fcl\u00b7den\u00b7klee", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das M\u00e4gdlein nahm des Busens Zier,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "nahm", "des", "Bu\u00b7sens", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und nickte freundlich Dank daf\u00fcr.", "tokens": ["Und", "nick\u00b7te", "freund\u00b7lich", "Dank", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur einzeln gr\u00fcnten noch im Hain", "tokens": ["Nur", "ein\u00b7zeln", "gr\u00fcn\u00b7ten", "noch", "im", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Buchen und die jungen Mai'n;", "tokens": ["Die", "Bu\u00b7chen", "und", "die", "jun\u00b7gen", "Mai'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Kresse wankt' in hellen", "tokens": ["Und", "Kres\u00b7se", "wankt'", "in", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umbl\u00fcmten Wiesenquellen:", "tokens": ["Um\u00b7bl\u00fcm\u00b7ten", "Wie\u00b7sen\u00b7quel\u00b7len", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf k\u00fchlem Moose, weich und prall,", "tokens": ["Auf", "k\u00fch\u00b7lem", "Moo\u00b7se", ",", "weich", "und", "prall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Buchbaum, horchten wir dem Schall", "tokens": ["Am", "Buch\u00b7baum", ",", "horch\u00b7ten", "wir", "dem", "Schall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Quelles und der Nachtigall.", "tokens": ["Des", "Quel\u00b7les", "und", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie pfl\u00fcckte Moos, wo wir geruht,", "tokens": ["Sie", "pfl\u00fcck\u00b7te", "Moos", ",", "wo", "wir", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und kr\u00e4nzte sich den Sch\u00e4ferhut.", "tokens": ["Und", "kr\u00e4nz\u00b7te", "sich", "den", "Sch\u00e4\u00b7fer\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir gingen atmend, Arm in Arm,", "tokens": ["Wir", "gin\u00b7gen", "at\u00b7mend", ",", "Arm", "in", "Arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Fr\u00fchlingsabend, still und warm,", "tokens": ["Am", "Fr\u00fch\u00b7lings\u00b7a\u00b7bend", ",", "still", "und", "warm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Schatten gr\u00fcner Schlehen", "tokens": ["Im", "Schat\u00b7ten", "gr\u00fc\u00b7ner", "Schle\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns Veilchen zu ersp\u00e4hen:", "tokens": ["Uns", "Veil\u00b7chen", "zu", "er\u00b7sp\u00e4\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Rot schien der Himmel und das Meer;", "tokens": ["Rot", "schien", "der", "Him\u00b7mel", "und", "das", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einmal strahlte, gro\u00df und hehr,", "tokens": ["Mit", "ein\u00b7mal", "strahl\u00b7te", ",", "gro\u00df", "und", "hehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der liebe volle Mond daher.", "tokens": ["Der", "lie\u00b7be", "vol\u00b7le", "Mond", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das M\u00e4gdlein stand und ging und stand,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "stand", "und", "ging", "und", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und dr\u00fcckte sprachlos mir die Hand.", "tokens": ["Und", "dr\u00fcck\u00b7te", "sprach\u00b7los", "mir", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Rotwangicht, leichtgekleidet sa\u00df", "tokens": ["Rot\u00b7wan\u00b7gicht", ",", "leicht\u00b7ge\u00b7klei\u00b7det", "sa\u00df"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sie neben mir auf Klee und Gras,", "tokens": ["Sie", "ne\u00b7ben", "mir", "auf", "Klee", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ringsum helle Bl\u00fcten", "tokens": ["Wo", "ring\u00b7sum", "hel\u00b7le", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Apfelb\u00e4ume gl\u00fchten:", "tokens": ["Der", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", "gl\u00fch\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "mea