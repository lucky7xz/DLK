{"dta.poem.11383": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der vergebens, obwohl treulich,  \n gewarnete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00e4tte nicht gedacht, wie ich erleben wolte,", "tokens": ["Ich", "h\u00e4t\u00b7te", "nicht", "ge\u00b7dacht", ",", "wie", "ich", "er\u00b7le\u00b7ben", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df einen alten fuchs man dennoch fangen solte:", "tokens": ["Da\u00df", "ei\u00b7nen", "al\u00b7ten", "fuchs", "man", "den\u00b7noch", "fan\u00b7gen", "sol\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn der wohl andere viel tausendmahl verlacht,", "tokens": ["Denn", "der", "wohl", "an\u00b7de\u00b7re", "viel", "tau\u00b7send\u00b7mahl", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "PIS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tapt itzo selber ein; das h\u00e4tt\u2019 ich nicht gedacht.", "tokens": ["Tapt", "it\u00b7zo", "sel\u00b7ber", "ein", ";", "das", "h\u00e4tt'", "ich", "nicht", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PTKVZ", "$.", "PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erinnerst du dich nicht der vormahls sch\u00f6nen zeiten,", "tokens": ["E\u00b7rin\u00b7nerst", "du", "dich", "nicht", "der", "vor\u00b7mahls", "sch\u00f6\u00b7nen", "zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da du nach deiner lust kontst fremde pferde reiten?", "tokens": ["Da", "du", "nach", "dei\u00b7ner", "lust", "kontst", "frem\u00b7de", "pfer\u00b7de", "rei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VMFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie du den besten schatz der freyheit itzt verlierst,", "tokens": ["Wie", "du", "den", "bes\u00b7ten", "schatz", "der", "frey\u00b7heit", "itzt", "ver\u00b7lierst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nun du ein eigen pferd an deine krippe f\u00fchrst.", "tokens": ["Nun", "du", "ein", "ei\u00b7gen", "pferd", "an", "dei\u00b7ne", "krip\u00b7pe", "f\u00fchrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und zwar, da schon bey dir der haber will veralten,", "tokens": ["Und", "zwar", ",", "da", "schon", "bey", "dir", "der", "ha\u00b7ber", "will", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ADV", "APPR", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie wirst du immerhin den strengen dienst verwalten?", "tokens": ["Wie", "wirst", "du", "im\u00b7mer\u00b7hin", "den", "stren\u00b7gen", "dienst", "ver\u00b7wal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn dieses glaube nur, da\u00df solch ein klepperlein", "tokens": ["Denn", "die\u00b7ses", "glau\u00b7be", "nur", ",", "da\u00df", "solch", "ein", "klep\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ADV", "$,", "KOUS", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Will teutsch gestriegelt und franz\u00f6sch gef\u00fcttert seyn.", "tokens": ["Will", "teutsch", "ge\u00b7strie\u00b7gelt", "und", "fran\u00b7z\u00f6sch", "ge\u00b7f\u00fct\u00b7tert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVPP", "KON", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wei\u00df noch wohl die zeit, da\u00df du dem frey\u2019n entsagtest,", "tokens": ["Ich", "wei\u00df", "noch", "wohl", "die", "zeit", ",", "da\u00df", "du", "dem", "frey'n", "ent\u00b7sag\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und mit verliebtem mund: Ist sie noch droben? fragtest.", "tokens": ["Und", "mit", "ver\u00b7lieb\u00b7tem", "mund", ":", "Ist", "sie", "noch", "dro\u00b7ben", "?", "frag\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADV", "ADV", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da stund es wohl um dich, da kontest du mit ruh", "tokens": ["Da", "stund", "es", "wohl", "um", "dich", ",", "da", "kon\u00b7test", "du", "mit", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KOUS", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mir, und ich wieder dir, ein halbes bringen zu.", "tokens": ["Mir", ",", "und", "ich", "wie\u00b7der", "dir", ",", "ein", "hal\u00b7bes", "brin\u00b7gen", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PPER", "ADV", "PPER", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da mocht ein pfeiffgen noch in deinen h\u00e4nden glimmen:", "tokens": ["Da", "mocht", "ein", "pfeiff\u00b7gen", "noch", "in", "dei\u00b7nen", "h\u00e4n\u00b7den", "glim\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Man dorfft uns keine zeit, zu bett zu gehn, bestimmen,", "tokens": ["Man", "dorfft", "uns", "kei\u00b7ne", "zeit", ",", "zu", "bett", "zu", "gehn", ",", "be\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "$,", "PTKA", "ADJD", "PTKZU", "VVINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie leyder! wird geschehn: Wo hast du hingedacht,", "tokens": ["Wie", "ley\u00b7der", "!", "wird", "ge\u00b7schehn", ":", "Wo", "hast", "du", "hin\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "VAFIN", "VVPP", "$.", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df du, ein freyer mann, zum sclaven dich gemacht?", "tokens": ["Da\u00df", "du", ",", "ein", "frey\u00b7er", "mann", ",", "zum", "scla\u00b7ven", "dich", "ge\u00b7macht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NN", "$,", "APPRART", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du kuntest franc und frey, wohin du woltest, wandern:", "tokens": ["Du", "kun\u00b7test", "franc", "und", "frey", ",", "wo\u00b7hin", "du", "wol\u00b7test", ",", "wan\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "KON", "ADJD", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mi\u00dffiel dir ort und dienst, so nahmst du einen andern;", "tokens": ["Mi\u00df\u00b7fiel", "dir", "ort", "und", "dienst", ",", "so", "nahmst", "du", "ei\u00b7nen", "an\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nun aber ists um dich, mein lieber freund! gethan:", "tokens": ["Nun", "a\u00b7ber", "ists", "um", "dich", ",", "mein", "lie\u00b7ber", "freund", "!", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Man h\u00e4ngt dir listiglich den ehstands-kleppel an,", "tokens": ["Man", "h\u00e4ngt", "dir", "lis\u00b7tig\u00b7lich", "den", "eh\u00b7stands\u00b7klep\u00b7pel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "An welchem du gewi\u00df mit nicht geringen plagen,", "tokens": ["An", "wel\u00b7chem", "du", "ge\u00b7wi\u00df", "mit", "nicht", "ge\u00b7rin\u00b7gen", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "PTKNEG", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit kummer und verdru\u00df dich wirst zu tode tragen;", "tokens": ["Mit", "kum\u00b7mer", "und", "ver\u00b7dru\u00df", "dich", "wirst", "zu", "to\u00b7de", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich rathe, wo es nicht zu sp\u00e4te m\u00f6chte seyn,", "tokens": ["Ich", "ra\u00b7the", ",", "wo", "es", "nicht", "zu", "sp\u00e4\u00b7te", "m\u00f6ch\u00b7te", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PTKNEG", "PTKZU", "VVFIN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn du mit ehren kanst, so bleib vor dich allein!", "tokens": ["Wenn", "du", "mit", "eh\u00b7ren", "kanst", ",", "so", "bleib", "vor", "dich", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VVINF", "VMFIN", "$,", "ADV", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Zwar wei\u00df ich, da\u00df du offt wirst bey dir selber fragen:", "tokens": ["Zwar", "wei\u00df", "ich", ",", "da\u00df", "du", "offt", "wirst", "bey", "dir", "sel\u00b7ber", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was mag doch dieser mann von meiner heyrath sagen,", "tokens": ["Was", "mag", "doch", "die\u00b7ser", "mann", "von", "mei\u00b7ner", "hey\u00b7rath", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der selber ist beweibt, und der so liederlich", "tokens": ["Der", "sel\u00b7ber", "ist", "be\u00b7weibt", ",", "und", "der", "so", "lie\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "$,", "KON", "ART", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vor diesem hat geliebt, da\u00df meister Michel sich", "tokens": ["Vor", "die\u00b7sem", "hat", "ge\u00b7liebt", ",", "da\u00df", "meis\u00b7ter", "Mi\u00b7chel", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Halb krumm und lahm curirt? Wie traurig kunt er sitzen,", "tokens": ["Halb", "krumm", "und", "lahm", "cu\u00b7rirt", "?", "Wie", "trau\u00b7rig", "kunt", "er", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "VVFIN", "VVPP", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wenn er ihn kommen sah mit pflastern, wicken, spritzen!", "tokens": ["Wenn", "er", "ihn", "kom\u00b7men", "sah", "mit", "pflas\u00b7tern", ",", "wi\u00b7cken", ",", "sprit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVFIN", "APPR", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie \u00f6ffters hat er doch dieselbe nacht verflucht,", "tokens": ["Wie", "\u00f6ff\u00b7ters", "hat", "er", "doch", "die\u00b7sel\u00b7be", "nacht", "ver\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da auf dem Donau-strom die Venus wurd besucht!", "tokens": ["Da", "auf", "dem", "Do\u00b7nau\u00b7strom", "die", "Ve\u00b7nus", "wurd", "be\u00b7sucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und was dergleichen mehr, wer wei\u00df sie all zu nennen?", "tokens": ["Und", "was", "derg\u00b7lei\u00b7chen", "mehr", ",", "wer", "wei\u00df", "sie", "all", "zu", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "$,", "PWS", "VVFIN", "PPER", "PIAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mein freund! die wahrheit ists, ich mu\u00df es selbst bekennen,", "tokens": ["Mein", "freund", "!", "die", "wahr\u00b7heit", "ists", ",", "ich", "mu\u00df", "es", "selbst", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "De\u00df ich so liederlich, wie du, vorhin gelebt:", "tokens": ["De\u00df", "ich", "so", "lie\u00b7der\u00b7lich", ",", "wie", "du", ",", "vor\u00b7hin", "ge\u00b7lebt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Viel terpentin gebraucht: Viel pflaster aufgeklebt:", "tokens": ["Viel", "ter\u00b7pen\u00b7tin", "ge\u00b7braucht", ":", "Viel", "pflas\u00b7ter", "auf\u00b7ge\u00b7klebt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$.", "PIAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Ja, da\u00df ich endlich, um den s\u00fcnden abzukommen,", "tokens": ["Ja", ",", "da\u00df", "ich", "end\u00b7lich", ",", "um", "den", "s\u00fcn\u00b7den", "ab\u00b7zu\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "$,", "KOUI", "ART", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mir hab ein eigen weib auf meinen leib genommen:", "tokens": ["Mir", "hab", "ein", "ei\u00b7gen", "weib", "auf", "mei\u00b7nen", "leib", "ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Mit welcher mirs gegl\u00fcckt, mit der ich leben kan,", "tokens": ["Mit", "wel\u00b7cher", "mirs", "ge\u00b7gl\u00fcckt", ",", "mit", "der", "ich", "le\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NE", "VVPP", "$,", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Nach meines hertzens wunsch, als ein vergn\u00fcgter mann;", "tokens": ["Nach", "mei\u00b7nes", "hert\u00b7zens", "wunsch", ",", "als", "ein", "ver\u00b7gn\u00fcg\u00b7ter", "mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und also hab\u2019 ichs zwar nach meinem theil getroffen,", "tokens": ["Und", "al\u00b7so", "hab'", "ichs", "zwar", "nach", "mei\u00b7nem", "theil", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Doch, was ich schon erlangt, das must du erstlich hoffen.", "tokens": ["Doch", ",", "was", "ich", "schon", "er\u00b7langt", ",", "das", "must", "du", "erst\u00b7lich", "hof\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wie? wenn dein engelgen dereinst, wie jene, spricht:", "tokens": ["Wie", "?", "wenn", "dein", "en\u00b7gel\u00b7gen", "de\u00b7reinst", ",", "wie", "je\u00b7ne", ",", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPOSAT", "ADJA", "ADV", "$,", "PWAV", "PDS", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Mein kind! was lernet man doch wohl bey hofe nicht?", "tokens": ["Mein", "kind", "!", "was", "ler\u00b7net", "man", "doch", "wohl", "bey", "ho\u00b7fe", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VVFIN", "PIS", "ADV", "ADV", "APPR", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Es ist gefahr dabey, wenn man in deinen jahren", "tokens": ["Es", "ist", "ge\u00b7fahr", "da\u00b7bey", ",", "wenn", "man", "in", "dei\u00b7nen", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PAV", "$,", "KOUS", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Erst an das freyen denckt: Der fr\u00fchling mu\u00df sich paaren.", "tokens": ["Erst", "an", "das", "frey\u00b7en", "denckt", ":", "Der", "fr\u00fch\u00b7ling", "mu\u00df", "sich", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "VVFIN", "$.", "ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wenn in dem winter schon die sonn in steinbock tritt,", "tokens": ["Wenn", "in", "dem", "win\u00b7ter", "schon", "die", "sonn", "in", "stein\u00b7bock", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Da bringt das zeichen auch gewi\u00df die h\u00f6rner mit.", "tokens": ["Da", "bringt", "das", "zei\u00b7chen", "auch", "ge\u00b7wi\u00df", "die", "h\u00f6r\u00b7ner", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ists aber nicht mehr zeit? So w\u00fcnsch ich dir von hertzen,", "tokens": ["Ists", "a\u00b7ber", "nicht", "mehr", "zeit", "?", "So", "w\u00fcnsch", "ich", "dir", "von", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "PIAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Da\u00df du nur mit geduld dein leiden m\u00f6gst verschmertzen,", "tokens": ["Da\u00df", "du", "nur", "mit", "ge\u00b7duld", "dein", "lei\u00b7den", "m\u00f6gst", "ver\u00b7schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "PPOSAT", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und wenn es endlich nicht ist ferner auszustehn,", "tokens": ["Und", "wenn", "es", "end\u00b7lich", "nicht", "ist", "fer\u00b7ner", "aus\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Gecr\u00f6nt, als m\u00e4rtyrer, dann in den himmel gehn!", "tokens": ["Ge\u00b7cr\u00f6nt", ",", "als", "m\u00e4r\u00b7ty\u00b7rer", ",", "dann", "in", "den", "him\u00b7mel", "gehn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADJA", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}