{"dta.poem.11157": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das goldne kalb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje reimt sich gold und gott? das gienge wohl noch hin;", "tokens": ["Wie", "reimt", "sich", "gold", "und", "gott", "?", "das", "gien\u00b7ge", "wohl", "noch", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "KON", "NN", "$.", "PDS", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein ein kalb darzu, das ist ein toller sinn.", "tokens": ["Al\u00b7lein", "ein", "kalb", "dar\u00b7zu", ",", "das", "ist", "ein", "tol\u00b7ler", "sinn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "PAV", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat man auch jemahls wohl dergleichen ding gelesen?", "tokens": ["Hat", "man", "auch", "je\u00b7mahls", "wohl", "derg\u00b7lei\u00b7chen", "ding", "ge\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "ADV", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die dieses kalb verehrt, sind ochsen gar gewesen.", "tokens": ["Die", "die\u00b7ses", "kalb", "ver\u00b7ehrt", ",", "sind", "och\u00b7sen", "gar", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJD", "VVPP", "$,", "VAFIN", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}