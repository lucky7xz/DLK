{"dta.poem.7529": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kr\u00e4nze, die du siehst, sind lauter Trauerzeichen", "tokens": ["Die", "Kr\u00e4n\u00b7ze", ",", "die", "du", "siehst", ",", "sind", "lau\u00b7ter", "Trau\u00b7er\u00b7zei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erblichner Freuden, die den Freuden nach erbleichen.", "tokens": ["Er\u00b7blich\u00b7ner", "Freu\u00b7den", ",", "die", "den", "Freu\u00b7den", "nach", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "F\u00fcr jede Lust, die starb, zum Denkmal einen Kranz", "tokens": ["F\u00fcr", "je\u00b7de", "Lust", ",", "die", "starb", ",", "zum", "Denk\u00b7mal", "ei\u00b7nen", "Kranz"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab' ich geflochten, und umkr\u00e4nzt bin ich nun ganz.", "tokens": ["Hab'", "ich", "ge\u00b7floch\u00b7ten", ",", "und", "um\u00b7kr\u00e4nzt", "bin", "ich", "nun", "ganz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "KON", "VVPP", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier h\u00e4ngt der Freundschaft Laub, und hier der Liebe Flitter,", "tokens": ["Hier", "h\u00e4ngt", "der", "Freund\u00b7schaft", "Laub", ",", "und", "hier", "der", "Lie\u00b7be", "Flit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "KON", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hier das Vatergl\u00fcck, gem\u00e4ht vom dunklen Schnitter.", "tokens": ["Und", "hier", "das", "Va\u00b7ter\u00b7gl\u00fcck", ",", "ge\u00b7m\u00e4ht", "vom", "dunk\u00b7len", "Schnit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hier welkt die Jugend, hier der Ruhm, und hier daneben", "tokens": ["Hier", "welkt", "die", "Ju\u00b7gend", ",", "hier", "der", "Ruhm", ",", "und", "hier", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,", "KON", "ADV", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist eine Stelle noch f\u00fcr diesen Rest von Leben.", "tokens": ["Ist", "ei\u00b7ne", "Stel\u00b7le", "noch", "f\u00fcr", "die\u00b7sen", "Rest", "von", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer nach mir \u00fcbrig bleibt, wann ich geschieden bin,", "tokens": ["Wer", "nach", "mir", "\u00fcb\u00b7rig", "bleibt", ",", "wann", "ich", "ge\u00b7schie\u00b7den", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADJD", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4ng' einen letzten Kranz aus dunkeln Blumen hin.", "tokens": ["H\u00e4ng'", "ei\u00b7nen", "letz\u00b7ten", "Kranz", "aus", "dun\u00b7keln", "Blu\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wenn ein Gast besucht die leere Siedelei,", "tokens": ["Und", "wenn", "ein", "Gast", "be\u00b7sucht", "die", "lee\u00b7re", "Sie\u00b7de\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr welken Kr\u00e4nze, sagt: So geht die Welt vorbei.", "tokens": ["Ihr", "wel\u00b7ken", "Kr\u00e4n\u00b7ze", ",", "sagt", ":", "So", "geht", "die", "Welt", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}