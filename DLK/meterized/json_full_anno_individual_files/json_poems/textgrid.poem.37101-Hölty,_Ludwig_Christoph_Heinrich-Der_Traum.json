{"textgrid.poem.37101": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steh mir immer am Haupt, wenn mich des Morgenschlafs", "tokens": ["Steh", "mir", "im\u00b7mer", "am", "Haupt", ",", "wenn", "mich", "des", "Mor\u00b7gen\u00b7schlafs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leiser Fittig umweht, l\u00e4chelnder Wonnetraum,", "tokens": ["Lei\u00b7ser", "Fit\u00b7tig", "um\u00b7weht", ",", "l\u00e4\u00b7cheln\u00b7der", "Won\u00b7ne\u00b7traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Der mich in die Gefilde,", "tokens": ["Der", "mich", "in", "die", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo die Seligen wohnen, rief.", "tokens": ["Wo", "die", "Se\u00b7li\u00b7gen", "woh\u00b7nen", ",", "rief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Eine Wolke von Gold, wo ein bepalmeter", "tokens": ["Ei\u00b7ne", "Wol\u00b7ke", "von", "Gold", ",", "wo", "ein", "be\u00b7pal\u00b7me\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Bote Gottes, im Klang schmelzender Lieder, stand,", "tokens": ["Bo\u00b7te", "Got\u00b7tes", ",", "im", "Klang", "schmel\u00b7zen\u00b7der", "Lie\u00b7der", ",", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Trug mich, schnell wie Gedanken,", "tokens": ["Trug", "mich", ",", "schnell", "wie", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In ein blumiges Eden hin.", "tokens": ["In", "ein", "blu\u00b7mi\u00b7ges", "E\u00b7den", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Rosenbl\u00fctengeruch wehte vom Ufer her,", "tokens": ["Ro\u00b7sen\u00b7bl\u00fc\u00b7ten\u00b7ge\u00b7ruch", "weh\u00b7te", "vom", "U\u00b7fer", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wo der Wechselgesang wirbelnder Harfen scholl,", "tokens": ["Wo", "der", "Wech\u00b7sel\u00b7ge\u00b7sang", "wir\u00b7beln\u00b7der", "Har\u00b7fen", "scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die Seelen und Engel", "tokens": ["Und", "die", "See\u00b7len", "und", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihre Jubel verschwisterten.", "tokens": ["Ih\u00b7re", "Ju\u00b7bel", "ver\u00b7schwis\u00b7ter\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Singer, Laura, Petrarch, sa\u00dfen im Kreis umher,", "tokens": ["Sin\u00b7ger", ",", "Lau\u00b7ra", ",", "Pe\u00b7trarch", ",", "sa\u00b7\u00dfen", "im", "Kreis", "um\u00b7her", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NE", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ihre Lauten am Arm, V\u00f6gelget\u00f6n erscholl", "tokens": ["Ih\u00b7re", "Lau\u00b7ten", "am", "Arm", ",", "V\u00f6\u00b7gel\u00b7ge\u00b7t\u00f6n", "er\u00b7scholl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "FM.la", "FM.la"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "In die Lauten, und Wohlklang", "tokens": ["In", "die", "Lau\u00b7ten", ",", "und", "Wohl\u00b7klang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "NN"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Flo\u00df vom Fl\u00fcgel der Abendluft.", "tokens": ["Flo\u00df", "vom", "Fl\u00fc\u00b7gel", "der", "A\u00b7ben\u00b7dluft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wonnethr\u00e4nen im Aug, Thr\u00e4nen der Seeligkeit,", "tokens": ["Won\u00b7net\u00b7hr\u00e4\u00b7nen", "im", "Aug", ",", "Thr\u00e4\u00b7nen", "der", "See\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Wallte Meta daher, Engel enttrockneten", "tokens": ["Wall\u00b7te", "Me\u00b7ta", "da\u00b7her", ",", "En\u00b7gel", "ent\u00b7trock\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "PAV", "$,", "NE", "VVFIN"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihr die Thr\u00e4nen, und meine", "tokens": ["Ihr", "die", "Thr\u00e4\u00b7nen", ",", "und", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "KON", "PPOSAT"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Minna folgte der Wallerin.", "tokens": ["Min\u00b7na", "folg\u00b7te", "der", "Wal\u00b7le\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Sie entwand sich dem Arm ihrer Gespielin, flog", "tokens": ["Sie", "ent\u00b7wand", "sich", "dem", "Arm", "ih\u00b7rer", "Ge\u00b7spie\u00b7lin", ",", "flog"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mir entgegen, und go\u00df, unter der gr\u00fcnen Nacht", "tokens": ["Mir", "ent\u00b7ge\u00b7gen", ",", "und", "go\u00df", ",", "un\u00b7ter", "der", "gr\u00fc\u00b7nen", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Einer fl\u00fcsternden Myrthe,", "tokens": ["Ei\u00b7ner", "fl\u00fcs\u00b7tern\u00b7den", "Myr\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sich urpl\u00f6tzlich an meine Brust.", "tokens": ["Sich", "ur\u00b7pl\u00f6tz\u00b7lich", "an", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Wand den liebenden Arm mir um die Brust herum,", "tokens": ["Wand", "den", "lie\u00b7ben\u00b7den", "Arm", "mir", "um", "die", "Brust", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Blickte z\u00e4rtlich mich an, k\u00fc\u00dfte mit Engelsku\u00df", "tokens": ["Blick\u00b7te", "z\u00e4rt\u00b7lich", "mich", "an", ",", "k\u00fc\u00df\u00b7te", "mit", "En\u00b7gels\u00b7ku\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Meine Lippen. Die Myrthe", "tokens": ["Mei\u00b7ne", "Lip\u00b7pen", ".", "Die", "Myr\u00b7the"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Rauschte Silbergelispel drein.", "tokens": ["Rauschte", "Sil\u00b7ber\u00b7ge\u00b7lis\u00b7pel", "drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Geister folgeten uns, Laurens verkl\u00e4rter Geist,", "tokens": ["Geis\u00b7ter", "fol\u00b7ge\u00b7ten", "uns", ",", "Lau\u00b7rens", "ver\u00b7kl\u00e4r\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "ADJA", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hand in Hand mit Petrarch, lagerten sich mit uns", "tokens": ["Hand", "in", "Hand", "mit", "Pe\u00b7trarch", ",", "la\u00b7ger\u00b7ten", "sich", "mit", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "NE", "$,", "VVFIN", "PRF", "APPR", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "In die K\u00fchle der Blumen,", "tokens": ["In", "die", "K\u00fch\u00b7le", "der", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und begannen mit uns Gespr\u00e4ch.", "tokens": ["Und", "be\u00b7gan\u00b7nen", "mit", "uns", "Ge\u00b7spr\u00e4ch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Pl\u00f6tzlich t\u00f6nte der Hahn dreymal den Feyerhall", "tokens": ["Pl\u00f6tz\u00b7lich", "t\u00f6n\u00b7te", "der", "Hahn", "drey\u00b7mal", "den", "Fey\u00b7er\u00b7hall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seines Morgengesang