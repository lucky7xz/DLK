{"dta.poem.2677": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Beerdigung Fr. H. F. g. J. den. 2.  \n Octobr. 1675.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entbrichst du dich der Erden H\u00f6le/", "tokens": ["Ent\u00b7brichst", "du", "dich", "der", "Er\u00b7den", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und suchst das unumschriebne Licht/", "tokens": ["Und", "suchst", "das", "un\u00b7um\u00b7schrieb\u00b7ne", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Tugend-Weib/ du edle Seele", "tokens": ["Du", "Tu\u00b7gen\u00b7dWeib", "/", "du", "ed\u00b7le", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$(", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00e4llt dir dieser Nothstall nicht?", "tokens": ["Ge\u00b7f\u00e4llt", "dir", "die\u00b7ser", "Noth\u00b7stall", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In dem dein Leib hier eingespannet?", "tokens": ["In", "dem", "dein", "Leib", "hier", "ein\u00b7ge\u00b7span\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eilst du des Himmels Freyheit zu/", "tokens": ["Eilst", "du", "des", "Him\u00b7mels", "Frey\u00b7heit", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und legst vom Sterben \u00fcbermannet/", "tokens": ["Und", "legst", "vom", "Ster\u00b7ben", "\u00fc\u00b7berm\u00b7an\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die m\u00fcrben Glieder zu der Ruh?", "tokens": ["Die", "m\u00fcr\u00b7ben", "Glie\u00b7der", "zu", "der", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach ja du weist da\u00df diese H\u00fctten", "tokens": ["Ach", "ja", "du", "weist", "da\u00df", "die\u00b7se", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "PPER", "VVFIN", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus schlechtem Leim seyn auffgebaut/", "tokens": ["Aus", "schlech\u00b7tem", "Leim", "seyn", "auff\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Zeit und Zufall sie zerr\u00fctten/", "tokens": ["Da\u00df", "Zeit", "und", "Zu\u00b7fall", "sie", "zer\u00b7r\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und man daran nichts ewigs schaut;", "tokens": ["Und", "man", "da\u00b7ran", "nichts", "e\u00b7wigs", "schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es reisset jeder Tag und Stunde", "tokens": ["Es", "reis\u00b7set", "je\u00b7der", "Tag", "und", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Bau des Lebens etwas ein/", "tokens": ["Vom", "Bau", "des", "Le\u00b7bens", "et\u00b7was", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da gehn wir \u00e4rmsten auch zu Grunde/", "tokens": ["Da", "gehn", "wir", "\u00e4rms\u00b7ten", "auch", "zu", "Grun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So bald wir nur gebohren seyn.", "tokens": ["So", "bald", "wir", "nur", "ge\u00b7boh\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Tod f\u00e4llt nicht nur graue Jahre/", "tokens": ["Der", "Tod", "f\u00e4llt", "nicht", "nur", "grau\u00b7e", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und die beschneiten H\u00e4upter an.", "tokens": ["Und", "die", "be\u00b7schnei\u00b7ten", "H\u00e4up\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Jugend krause locken Haare/", "tokens": ["Der", "Ju\u00b7gend", "krau\u00b7se", "lo\u00b7cken", "Haa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was beheglich heissen kan/", "tokens": ["Und", "was", "be\u00b7heg\u00b7lich", "heis\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Augen lichtes Sternen-prangen;", "tokens": ["Der", "Au\u00b7gen", "lich\u00b7tes", "Ster\u00b7nen\u00b7pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Glieder Anmuths-reicher May", "tokens": ["Der", "Glie\u00b7der", "An\u00b7muths\u00b7rei\u00b7cher", "May"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Lilg\u2019 und Rosen auff den Wangen/", "tokens": ["Die", "Lil\u00b7g'", "und", "Ro\u00b7sen", "auff", "den", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sind nicht von der Verwesung frey.", "tokens": ["Sind", "nicht", "von", "der", "Ver\u00b7we\u00b7sung", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vergebens/ da\u00df ihr Lust gestalten", "tokens": ["Ver\u00b7ge\u00b7bens", "/", "da\u00df", "ihr", "Lust", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00e4tzt eure Sch\u00f6nheit sonder gleich;", "tokens": ["Sch\u00e4tzt", "eu\u00b7re", "Sch\u00f6n\u00b7heit", "son\u00b7der", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die zarte Haut kan bald sich falten/", "tokens": ["Die", "zar\u00b7te", "Haut", "kan", "bald", "sich", "fal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein eintzig Fieber macht euch bleich;", "tokens": ["Ein", "eint\u00b7zig", "Fie\u00b7ber", "macht", "euch", "bleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn ihr bey dem h\u00f6chsten bl\u00fchen", "tokens": ["Und", "wenn", "ihr", "bey", "dem", "h\u00f6chs\u00b7ten", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stecht aller Blumen Zierath hin/", "tokens": ["Stecht", "al\u00b7ler", "Blu\u00b7men", "Zie\u00b7rath", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird euch der Tod ein Kleid anziehen/", "tokens": ["Wird", "euch", "der", "Tod", "ein", "Kleid", "an\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor dem ihr andern werdet fliehn.", "tokens": ["Vor", "dem", "ihr", "an\u00b7dern", "wer\u00b7det", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es sind doch ", "tokens": ["Es", "sind", "doch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Und G\u00fcter auff gar kurtze Zeit/", "tokens": ["Und", "G\u00fc\u00b7ter", "auff", "gar", "kurt\u00b7ze", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nicht/ wenn wir von hinnen fahren/", "tokens": ["Die", "nicht", "/", "wenn", "wir", "von", "hin\u00b7nen", "fah\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$(", "KOUS", "PPER", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wegweiser zu der Ewigkeit.", "tokens": ["Weg\u00b7wei\u00b7ser", "zu", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sey; das L\u00e4nder angestecket", "tokens": ["Es", "sey", ";", "das", "L\u00e4n\u00b7der", "an\u00b7ge\u0