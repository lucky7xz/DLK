{"dta.poem.13547": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Wunderland ist oben aufgeschlagen,", "tokens": ["Ein", "Wun\u00b7der\u00b7land", "ist", "o\u00b7ben", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo gold'ne Str\u00f6me geh'n und dunkel schallen,", "tokens": ["Wo", "gold'\u00b7ne", "Str\u00f6\u00b7me", "geh'n", "und", "dun\u00b7kel", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVPP", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ges\u00e4nge durch das Rauschen tief verhallen,", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "durch", "das", "Rau\u00b7schen", "tief", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die m\u00f6chten gern ein hohes Wort dir sagen.", "tokens": ["Die", "m\u00f6ch\u00b7ten", "gern", "ein", "ho\u00b7hes", "Wort", "dir", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}