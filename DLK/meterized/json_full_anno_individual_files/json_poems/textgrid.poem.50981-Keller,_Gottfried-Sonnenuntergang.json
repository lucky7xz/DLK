nfo": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als deines Glanzes schw\u00e4chste Spur!", "tokens": ["Als", "dei\u00b7nes", "Glan\u00b7zes", "schw\u00e4chs\u00b7te", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie geht, ich wende bang mich ab,", "tokens": ["Sie", "geht", ",", "ich", "wen\u00b7de", "bang", "mich", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es d\u00fcnkt die Welt mich eine Kohle;", "tokens": ["Es", "d\u00fcnkt", "die", "Welt", "mich", "ei\u00b7ne", "Koh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was j\u00fcngst nur Klarheit wiedergab,", "tokens": ["Was", "j\u00fcngst", "nur", "Klar\u00b7heit", "wie\u00b7der\u00b7gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00e4ubt, Asche, unter meiner Sohle. \u2013", "tokens": ["St\u00e4ubt", ",", "A\u00b7sche", ",", "un\u00b7ter", "mei\u00b7ner", "Soh\u00b7le", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch schau, wie ich gen Osten kehre,", "tokens": ["Doch", "schau", ",", "wie", "ich", "gen", "Os\u00b7ten", "keh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Taucht mir ein neues Wunder auf:", "tokens": ["Taucht", "mir", "ein", "neu\u00b7es", "Wun\u00b7der", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im rosig milden Nebelmeere", "tokens": ["Im", "ro\u00b7sig", "mil\u00b7den", "Ne\u00b7bel\u00b7mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beginnt der Silbermond den Lauf!", "tokens": ["Be\u00b7ginnt", "der", "Sil\u00b7ber\u00b7mond", "den", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der nach verlornen Strahlen jagt,", "tokens": ["Der", "nach", "ver\u00b7lor\u00b7nen", "Strah\u00b7len", "jagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist er der Sonne \u00c4hrenleser?", "tokens": ["Ist", "er", "der", "Son\u00b7ne", "\u00c4h\u00b7ren\u00b7le\u00b7ser", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist er, bis sie im Osten tagt,", "tokens": ["Ist", "er", ",", "bis", "sie", "im", "Os\u00b7ten", "tagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der goldnen Herrin Reichsverweser?", "tokens": ["Der", "gold\u00b7nen", "Her\u00b7rin", "Reichs\u00b7ver\u00b7we\u00b7ser", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, unsrer armen Mutter Erde", "tokens": ["Ach", ",", "uns\u00b7rer", "ar\u00b7men", "Mut\u00b7ter", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist er ja nur ein Lehenmann;", "tokens": ["Ist", "er", "ja", "nur", "ein", "Le\u00b7hen\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und seht, mit gl\u00e4nzender Geb\u00e4rde", "tokens": ["Und", "seht", ",", "mit", "gl\u00e4n\u00b7zen\u00b7der", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Tut er die Lehnspflicht, wie er kann!", "tokens": ["Tut", "er", "die", "Lehns\u00b7pflicht", ",", "wie", "er", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er tr\u00e4gt das Licht durch Nacht und Graun", "tokens": ["Er", "tr\u00e4gt", "das", "Licht", "durch", "Nacht", "und", "Graun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getreu auf sanft erhellten Wegen,", "tokens": ["Ge\u00b7treu", "auf", "sanft", "er\u00b7hell\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis wir den Morgen wieder schaun", "tokens": ["Bis", "wir", "den", "Mor\u00b7gen", "wie\u00b7der", "schaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und frisch die Erde taut im Segen.", "tokens": ["Und", "frisch", "die", "Er\u00b7de", "taut", "im", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe wird den Ruhm nicht mindern,", "tokens": ["Die", "Lie\u00b7be", "wird", "den", "Ruhm", "nicht", "min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Kleine mit den Kleinern gehn:", "tokens": ["Wenn", "Klei\u00b7ne", "mit", "den", "Klei\u00b7nern", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sonne selbst samt ihren Kindern", "tokens": ["Die", "Son\u00b7ne", "selbst", "samt", "ih\u00b7ren", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measu