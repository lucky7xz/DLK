{"dta.poem.12542": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LiI.  \n  Parzielle Auferstehung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zweifelt hinf\u00fcro mir nicht an der Auferstehung", "tokens": ["Zwei\u00b7felt", "hin\u00b7f\u00fc\u00b7ro", "mir", "nicht", "an", "der", "Auf\u00b7er\u00b7ste\u00b7hung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Todten:", "tokens": ["der", "Tod\u00b7ten", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hab' ich doch selbst in Berlin Hunderte neu-", "tokens": ["Hab'", "ich", "doch", "selbst", "in", "Ber\u00b7lin", "Hun\u00b7der\u00b7te", "neu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "NE", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "lich gesehn!", "tokens": ["lich", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Sind sie auch nicht mit Fleisch und Blut, gleich", "tokens": ["Sind", "sie", "auch", "nicht", "mit", "Fleisch", "und", "Blut", ",", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "KON", "NN", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Menschen, bekleidet:", "tokens": ["Men\u00b7schen", ",", "be\u00b7klei\u00b7det", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "H\u00f6rt man doch fernhin schon klappern das", "tokens": ["H\u00f6rt", "man", "doch", "fern\u00b7hin", "schon", "klap\u00b7pern", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ADV", "VVFIN", "ART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "d\u00fcrre Gebein.", "tokens": ["d\u00fcr\u00b7re", "Ge\u00b7bein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Zwar ", "tokens": ["Zwar"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "lehrten, dem Adel:", "tokens": ["lehr\u00b7ten", ",", "dem", "A\u00b7del", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Denn an den Lazarus hat nie noch ein K\u00f6nig", "tokens": ["Denn", "an", "den", "La\u00b7za\u00b7rus", "hat", "nie", "noch", "ein", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "gedacht.", "tokens": ["ge\u00b7dacht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}