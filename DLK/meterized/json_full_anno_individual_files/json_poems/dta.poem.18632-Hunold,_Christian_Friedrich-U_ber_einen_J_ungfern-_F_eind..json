{"dta.poem.18632": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber einen  J ungfern- F eind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Rosander spricht/ ich will ein Feind", "tokens": ["Ro\u00b7san\u00b7der", "spricht", "/", "ich", "will", "ein", "Feind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen Jungfern ewig bleiben/", "tokens": ["Von", "al\u00b7len", "Jung\u00b7fern", "e\u00b7wig", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch ist es gut gemeint/", "tokens": ["Und", "den\u00b7noch", "ist", "es", "gut", "ge\u00b7meint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich sie g\u00e4ntzlich will vertreiben:", "tokens": ["Wenn", "ich", "sie", "g\u00e4ntz\u00b7lich", "will", "ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So offt ihr meine Feindschaft stehlt/", "tokens": ["So", "offt", "ihr", "mei\u00b7ne", "Feind\u00b7schaft", "stehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So m\u00fcst ihr Jungfern dr\u00fcber lachen/", "tokens": ["So", "m\u00fcst", "ihr", "Jung\u00b7fern", "dr\u00fc\u00b7ber", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Warum? ihr wi\u00dft wohin es zielt/", "tokens": ["Wa\u00b7rum", "?", "ihr", "wi\u00dft", "wo\u00b7hin", "es", "zielt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich mag euch gern zu Weibern machen.", "tokens": ["Ich", "mag", "euch", "gern", "zu", "Wei\u00b7bern", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}