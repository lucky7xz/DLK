{"textgrid.poem.54505": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir die blinde Welt/ mit hei\u00df entbrandtem Neid/", "tokens": ["Wenn", "dir", "die", "blin\u00b7de", "Welt", "/", "mit", "hei\u00df", "ent\u00b7brand\u00b7tem", "Neid", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$(", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit \u00fcberh\u00e4ufftem Ha\u00df/ mit Mord vnd Schwerdt zusetzet.", "tokens": ["Mit", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7tem", "Ha\u00df", "/", "mit", "Mord", "vnd", "Schwerdt", "zu\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So dencke da\u00df sie hat den Herren gleich verletzet.", "tokens": ["So", "den\u00b7cke", "da\u00df", "sie", "hat", "den", "Her\u00b7ren", "gleich", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man schenckts den Knechten nicht/ wenn auch der Meister leidt.", "tokens": ["Man", "schenckts", "den", "Knech\u00b7ten", "nicht", "/", "wenn", "auch", "der", "Meis\u00b7ter", "leidt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["P