{"textgrid.poem.63256": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Prolog zu einem Schauspiel", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich neige mich vor aller B\u00fchnen Auditorien:", "tokens": ["Ich", "nei\u00b7ge", "mich", "vor", "al\u00b7ler", "B\u00fch\u00b7nen", "Au\u00b7di\u00b7to\u00b7ri\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Es ist so schwer, ein Mensch zu sein.", "tokens": ["Es", "ist", "so", "schwer", ",", "ein", "Mensch", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Selbst in der Heiligkeit ersehnter Glorien", "tokens": ["Selbst", "in", "der", "Hei\u00b7lig\u00b7keit", "er\u00b7sehn\u00b7ter", "Glo\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fchlt schmerzlich sich der Einzelne allein.", "tokens": ["F\u00fchlt", "schmerz\u00b7lich", "sich", "der", "Ein\u00b7zel\u00b7ne", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Die Einsamkeit beschattet seine Seele;", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "be\u00b7schat\u00b7tet", "sei\u00b7ne", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie l\u00e4sst erzittern seines Herzens Schlag.", "tokens": ["Sie", "l\u00e4sst", "er\u00b7zit\u00b7tern", "sei\u00b7nes", "Her\u00b7zens", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und selbst der Sang der s\u00fcssen Philomele", "tokens": ["Und", "selbst", "der", "Sang", "der", "s\u00fcs\u00b7sen", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verdunkelt nur den \u00fcberw\u00f6lkten Tag.", "tokens": ["Ver\u00b7dun\u00b7kelt", "nur", "den", "\u00fc\u00b7berw\u00b7\u00f6lk\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da hebt am Abend leicht vor einem jeden", "tokens": ["Da", "hebt", "am", "A\u00b7bend", "leicht", "vor", "ei\u00b7nem", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADJD", "APPR", "ART", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Vorhang sich zu einer innern Welt.", "tokens": ["Der", "Vor\u00b7hang", "sich", "zu", "ei\u00b7ner", "in\u00b7nern", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es gleitet puppenspielerisch an F\u00e4den", "tokens": ["Es", "glei\u00b7tet", "pup\u00b7pen\u00b7spie\u00b7le\u00b7risch", "an", "F\u00e4\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Hass, der Hohn, die Liebe und das Geld.", "tokens": ["Der", "Hass", ",", "der", "Hohn", ",", "die", "Lie\u00b7be", "und", "das", "Geld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gestaltung wird die l\u00e4chelnde Geb\u00e4rde,", "tokens": ["Ge\u00b7stal\u00b7tung", "wird", "die", "l\u00e4\u00b7cheln\u00b7de", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Zur Totenbahre neigt sich die Monstranz.", "tokens": ["Zur", "To\u00b7ten\u00b7bah\u00b7re", "neigt", "sich", "die", "Mons\u00b7tranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die gelobte, die geliebte Erde", "tokens": ["Und", "die", "ge\u00b7lob\u00b7te", ",", "die", "ge\u00b7lieb\u00b7te", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bev\u00f6lkert sich mit Rausch und Traum und Tanz.", "tokens": ["Be\u00b7v\u00f6l\u00b7kert", "sich", "mit", "Rausch", "und", "Traum", "und", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie dunkler Wein ist Wahrheit zu geniessen;", "tokens": ["Wie", "dunk\u00b7ler", "Wein", "ist", "Wahr\u00b7heit", "zu", "ge\u00b7nies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wirklichkeit ist leerer Winde Schall.", "tokens": ["Die", "Wirk\u00b7lich\u00b7keit", "ist", "lee\u00b7rer", "Win\u00b7de", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Tr\u00e4nen, die aus unsern Augen fliessen,", "tokens": ["Die", "Tr\u00e4\u00b7nen", ",", "die", "aus", "un\u00b7sern", "Au\u00b7gen", "flies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empfangt sie in des Herzens Blutkristall!", "tokens": ["Emp\u00b7fangt", "sie", "in", "des", "Her\u00b7zens", "Blut\u00b7kris\u00b7tall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das Lachen, das in eure Ohren t\u00f6ne,", "tokens": ["Das", "La\u00b7chen", ",", "das", "in", "eu\u00b7re", "Oh\u00b7ren", "t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es fiel vom Himmel; ein metallner Stern.", "tokens": ["Es", "fiel", "vom", "Him\u00b7mel", ";", "ein", "me\u00b7tall\u00b7ner", "Stern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und es verkl\u00e4re klingend, es versch\u00f6ne", "tokens": ["Und", "es", "ver\u00b7kl\u00e4\u00b7re", "klin\u00b7gend", ",", "es", "ver\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "VVPP", "$,", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die edlen Damen und die stolzen Herrn.", "tokens": ["Die", "ed\u00b7len", "Da\u00b7men", "und", "die", "stol\u00b7zen", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So klug ist keiner, dass ihn Liebe sch\u00e4nde.", "tokens": ["So", "klug", "ist", "kei\u00b7ner", ",", "dass", "ihn", "Lie\u00b7be", "sch\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIS", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sch\u00f6n ist niemand, dass ihn Schmerz entehrt.", "tokens": ["So", "sch\u00f6n", "ist", "nie\u00b7mand", ",", "dass", "ihn", "Schmerz", "ent\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIS", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es zeigt der B\u00fchne buntestes Gel\u00e4nde", "tokens": ["Es", "zeigt", "der", "B\u00fch\u00b7ne", "bun\u00b7tes\u00b7tes", "Ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den G\u00f6tterj\u00fcngling mit dem Rosenschwert.", "tokens": ["Den", "G\u00f6t\u00b7ter\u00b7j\u00fcng\u00b7ling", "mit", "dem", "Ro\u00b7sen\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Es hebe seinen Stab nun der Ephebe", "tokens": ["Es", "he\u00b7be", "sei\u00b7nen", "Stab", "nun", "der", "E\u00b7phe\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und r\u00fchre euer Herz zum frommen Schaun.", "tokens": ["Und", "r\u00fch\u00b7re", "eu\u00b7er", "Herz", "zum", "from\u00b7men", "Schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder ahne freundlich, dass er lebe,", "tokens": ["Ein", "je\u00b7der", "ah\u00b7ne", "freund\u00b7lich", ",", "dass", "er", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihn begl\u00fccke Nymphe, Gott und Faun.", "tokens": ["Und", "ihn", "be\u00b7gl\u00fc\u00b7cke", "Nym\u00b7phe", ",", "Gott", "und", "Faun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Es sinken eines tr\u00fcben Tages D\u00fcnste,", "tokens": ["Es", "sin\u00b7ken", "ei\u00b7nes", "tr\u00fc\u00b7ben", "Ta\u00b7ges", "D\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine Blume bl\u00fcht Gemeinsamkeit,", "tokens": ["Wie", "ei\u00b7ne", "Blu\u00b7me", "bl\u00fcht", "Ge\u00b7mein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umarmt euch angesichts der goldnen K\u00fcnste", "tokens": ["Um\u00b7armt", "euch", "an\u00b7ge\u00b7sichts", "der", "gold\u00b7nen", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fchlt beseligt, dass ihr Br\u00fcder seid.", "tokens": ["Und", "f\u00fchlt", "be\u00b7se\u00b7ligt", ",", "dass", "ihr", "Br\u00fc\u00b7der", "seid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich neige mich vor aller B\u00fchnen Auditorien:", "tokens": ["Ich", "nei\u00b7ge", "mich", "vor", "al\u00b7ler", "B\u00fch\u00b7nen", "Au\u00b7di\u00b7to\u00b7ri\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "i