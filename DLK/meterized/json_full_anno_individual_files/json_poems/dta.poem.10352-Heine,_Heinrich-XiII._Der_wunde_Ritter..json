{"dta.poem.10352": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiII.  \n  Der wunde Ritter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df eine alte Kunde,", "tokens": ["Ich", "wei\u00df", "ei\u00b7ne", "al\u00b7te", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die hallet dumpf und tr\u00fcb';", "tokens": ["Die", "hal\u00b7let", "dumpf", "und", "tr\u00fcb'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ritter lag liebeswunde,", "tokens": ["Ein", "Rit\u00b7ter", "lag", "lie\u00b7bes\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch treulos ist sein Lieb.", "tokens": ["Doch", "treu\u00b7los", "ist", "sein", "Lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als treulos mu\u00df er verachten", "tokens": ["Als", "treu\u00b7los", "mu\u00df", "er", "ver\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die eigne Herzliebste sein,", "tokens": ["Die", "eig\u00b7ne", "Herz\u00b7liebs\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als schimpflich mu\u00df er betrachten", "tokens": ["Als", "schimpf\u00b7lich", "mu\u00df", "er", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die eigne Liebespein.", "tokens": ["Die", "eig\u00b7ne", "Lie\u00b7be\u00b7spein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er m\u00f6cht' in die Schranken reiten,", "tokens": ["Er", "m\u00f6cht'", "in", "die", "Schran\u00b7ken", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und rufen die Ritter zum Streit:", "tokens": ["Und", "ru\u00b7fen", "die", "Rit\u00b7ter", "zum", "Streit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der mag sich zum Kampfe bereiten,", "tokens": ["Der", "mag", "sich", "zum", "Kamp\u00b7fe", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wer mein Lieb eines Makels zeih't!", "tokens": ["Wer", "mein", "Lieb", "ei\u00b7nes", "Ma\u00b7kels", "zeih't", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Da w\u00fcrden wohl Alle schweigen,", "tokens": ["Da", "w\u00fcr\u00b7den", "wohl", "Al\u00b7le", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nur nicht sein eigener Schmerz;", "tokens": ["Nur", "nicht", "sein", "ei\u00b7ge\u00b7ner", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da m\u00fc\u00dft' er die Lanze neigen", "tokens": ["Da", "m\u00fc\u00dft'", "er", "die", "Lan\u00b7ze", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wider's eigne klagende Herz.", "tokens": ["Wi\u00b7der's", "eig\u00b7ne", "kla\u00b7gen\u00b7de", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}