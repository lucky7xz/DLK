{"dta.poem.9846": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgungs-ode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol dem der schon den hohen berg erstiegen/", "tokens": ["Wol", "dem", "der", "schon", "den", "ho\u00b7hen", "berg", "er\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wann andre sich noch um die wurtzeln schmiegen/", "tokens": ["Wann", "and\u00b7re", "sich", "noch", "um", "die", "wurt\u00b7zeln", "schmie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der kan gewi\u00df wol recht vergn\u00fcget seyn:", "tokens": ["Der", "kan", "ge\u00b7wi\u00df", "wol", "recht", "ver\u00b7gn\u00fc\u00b7get", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn andre erst um reich und scepter streiten/", "tokens": ["Wenn", "and\u00b7re", "erst", "um", "reich", "und", "scep\u00b7ter", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So siht man ihn in cron und purpur schreiten/", "tokens": ["So", "siht", "man", "ihn", "in", "cron", "und", "pur\u00b7pur", "schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "APPR", "NN", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jhn h\u00fcllt der sieg in tausend freuden ein.", "tokens": ["Jhn", "h\u00fcllt", "der", "sieg", "in", "tau\u00b7send", "freu\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn jener bitt zu stehen f\u00fcr den f\u00fcssen/", "tokens": ["Wenn", "je\u00b7ner", "bitt", "zu", "ste\u00b7hen", "f\u00fcr", "den", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "PTKZU", "VVINF", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kan dieser schon die rosen-wangen k\u00fcssen/", "tokens": ["Kan", "die\u00b7ser", "schon", "die", "ro\u00b7sen\u00b7wan\u00b7gen", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und dr\u00fcckt den mund an ihre liljen-brust.", "tokens": ["Und", "dr\u00fcckt", "den", "mund", "an", "ih\u00b7re", "lil\u00b7jen\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wenn jenen noch die hofnung h\u00e4lt gefangen/", "tokens": ["Wenn", "je\u00b7nen", "noch", "die", "hof\u00b7nung", "h\u00e4lt", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kan dieser schon derselben frucht erlangen/", "tokens": ["Kan", "die\u00b7ser", "schon", "der\u00b7sel\u00b7ben", "frucht", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und sitzt allda in voller hertzens-lust.", "tokens": ["Und", "sitzt", "all\u00b7da", "in", "vol\u00b7ler", "hert\u00b7zens\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Was kan uns mehr als dieses wol vergn\u00fcgen!", "tokens": ["Was", "kan", "uns", "mehr", "als", "die\u00b7ses", "wol", "ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PIAT", "KOKOM", "PDS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wenn sich der stock will selbst zum pflocke biegen/", "tokens": ["Wenn", "sich", "der", "stock", "will", "selbst", "zum", "pflo\u00b7cke", "bie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und beut uns an das sch\u00f6ne rosen-haupt.", "tokens": ["Und", "beut", "uns", "an", "das", "sch\u00f6\u00b7ne", "ro\u00b7sen\u00b7haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wenn jener sich bem\u00fcht sie abzubrechen/", "tokens": ["Wenn", "je\u00b7ner", "sich", "be\u00b7m\u00fcht", "sie", "ab\u00b7zu\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PRF", "VVFIN", "PPER", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ist di\u00df sein raub/ da\u00df er sich pflegt zu stechen/", "tokens": ["Ist", "di\u00df", "sein", "raub", "/", "da\u00df", "er", "sich", "pflegt", "zu", "ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PRF", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sonst weiter ist ihm nichtes nicht erlaubt.", "tokens": ["Sonst", "wei\u00b7ter", "ist", "ihm", "nich\u00b7tes", "nicht", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der sitz umzirckt in seiner sch\u00f6nheit armen/", "tokens": ["Der", "sitz", "um\u00b7zirckt", "in", "sei\u00b7ner", "sch\u00f6n\u00b7heit", "ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und kan mit lust an ihrer brust erwarmen/", "tokens": ["Und", "kan", "mit", "lust", "an", "ih\u00b7rer", "brust", "er\u00b7war\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wenn er list auf den s\u00fcssen liebes-thau.", "tokens": ["Wenn", "er", "list", "auf", "den", "s\u00fcs\u00b7sen", "lie\u00b7bes\u00b7\u00b7thau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Da dieser sich mit tausend sorgen kr\u00e4ncket/", "tokens": ["Da", "die\u00b7ser", "sich", "mit", "tau\u00b7send", "sor\u00b7gen", "kr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PRF", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und manches mal mit leeren b\u00e4chen tr\u00e4ncket/", "tokens": ["Und", "man\u00b7ches", "mal", "mit", "lee\u00b7ren", "b\u00e4\u00b7chen", "tr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Fast halb verwirrt auf seiner liebes-au.", "tokens": ["Fast", "halb", "ver\u00b7wirrt", "auf", "sei\u00b7ner", "lie\u00b7bes\u00b7au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Wenn er sich plagt bi\u00df an den hellen morgen/", "tokens": ["Wenn", "er", "sich", "plagt", "bi\u00df", "an", "den", "hel\u00b7len", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "APPR", "ART", "ADJA", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Ja halb erstirbt in lauter liebes-sorgen/", "tokens": ["Ja", "halb", "er\u00b7stirbt", "in", "lau\u00b7ter", "lie\u00b7bes\u00b7sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und gleichsam wie aus einer nacht erwacht.", "tokens": ["Und", "gleich\u00b7sam", "wie", "aus", "ei\u00b7ner", "nacht", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Will dieser schon in lauter anmuth lachen/", "tokens": ["Will", "die\u00b7ser", "schon", "in", "lau\u00b7ter", "an\u00b7muth", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und einen schertz aus allen sorgen machen/", "tokens": ["Und", "ei\u00b7nen", "schertz", "aus", "al\u00b7len", "sor\u00b7gen", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Er ist auf nichts als nur auf lust bedacht.", "tokens": ["Er", "ist", "auf", "nichts", "als", "nur", "auf", "lust", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "KOKOM", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Wenn man den kahn will von dem hafen trennen/", "tokens": ["Wenn", "man", "den", "kahn", "will", "von", "dem", "ha\u00b7fen", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Siht er sein schiff mit vollem segel rennen/", "tokens": ["Siht", "er", "sein", "schiff", "mit", "vol\u00b7lem", "se\u00b7gel", "ren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAINF", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Es neigt zu ihm sich der erw\u00fcnschte port/", "tokens": ["Es", "neigt", "zu", "ihm", "sich", "der", "er\u00b7w\u00fcnschte", "port", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PRF", "ART", "ADJA", "NE", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Das ufer will als g\u00f6ttlich ihn selbst ehren/", "tokens": ["Das", "u\u00b7fer", "will", "als", "g\u00f6tt\u00b7lich", "ihn", "selbst", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KOKOM", "ADJD", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Da jenen doch ein fels dr\u00e4ut zu versehren/", "tokens": ["Da", "je\u00b7nen", "doch", "ein", "fels", "dr\u00e4ut", "zu", "ver\u00b7seh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und zeiget ihm oft die verf\u00e4lschte pfort.", "tokens": ["Und", "zei\u00b7get", "ihm", "oft", "die", "ver\u00b7f\u00e4lschte", "pfort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.37": {"text": "Gl\u00fcckseelig ist derselbe ja zu nennen/", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "ist", "der\u00b7sel\u00b7be", "ja", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Nach dessen blick der sch\u00f6nsten hertz wil brennen/", "tokens": ["Nach", "des\u00b7sen", "blick", "der", "sch\u00f6ns\u00b7ten", "hertz", "wil", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "So tag und nacht in voller wache steht/", "tokens": ["So", "tag", "und", "nacht", "in", "vol\u00b7ler", "wa\u00b7che", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Wenn er die glut sieht aus dem hertzen stammen/", "tokens": ["Wenn", "er", "die", "glut", "sieht", "aus", "dem", "hert\u00b7zen", "stam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Die ihm erweckt vergn\u00fcgungs-volle flammen/", "tokens": ["Die", "ihm", "er\u00b7weckt", "ver\u00b7gn\u00fc\u00b7gungs\u00b7vol\u00b7le", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Wenn durch den schnee der liebe westwind geht.", "tokens": ["Wenn", "durch", "den", "schnee", "der", "lie\u00b7be", "west\u00b7wind", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Da jener noch das feuer soll aufblasen/", "tokens": ["Da", "je\u00b7ner", "noch", "das", "feu\u00b7er", "soll", "auf\u00b7bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Da er sich oft verbrennen kan die nasen/", "tokens": ["Da", "er", "sich", "oft", "ver\u00b7bren\u00b7nen", "kan", "die", "na\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Wenn er nicht wohl mit umzugehen wei\u00df.", "tokens": ["Wenn", "er", "nicht", "wohl", "mit", "um\u00b7zu\u00b7ge\u00b7hen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Gesetzt auch/ da\u00df es endlich ihm gelinge/", "tokens": ["Ge\u00b7setzt", "auch", "/", "da\u00df", "es", "end\u00b7lich", "ihm", "ge\u00b7lin\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "