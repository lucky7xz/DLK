{"textgrid.poem.27572": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00fcnstler", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Buonarroti fing an, den Block zur B\u00fcste zu bilden,", "tokens": ["Buo\u00b7nar\u00b7ro\u00b7ti", "fing", "an", ",", "den", "Block", "zur", "B\u00fcs\u00b7te", "zu", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Sah, es wurde nichts draus, Freunde, da lie\u00df er ihn stehn.", "tokens": ["Sah", ",", "es", "wur\u00b7de", "nichts", "draus", ",", "Freun\u00b7de", ",", "da", "lie\u00df", "er", "ihn", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "PIS", "PAV", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Als ein wahrer Narzi\u00df besorgest du Karikaturen,", "tokens": ["Als", "ein", "wah\u00b7rer", "Nar\u00b7zi\u00df", "be\u00b7sor\u00b7gest", "du", "Ka\u00b7ri\u00b7ka\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Stehst und be\u00e4ugelst mit Lust immer aufs neue ", "tokens": ["Stehst", "und", "be\u00b7\u00e4u\u00b7gelst", "mit", "Lust", "im\u00b7mer", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN", "ADV", "APPRART", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Euch verk\u00fcmmerte man das Allgemeine des Titels;", "tokens": ["Euch", "ver\u00b7k\u00fcm\u00b7mer\u00b7te", "man", "das", "All\u00b7ge\u00b7mei\u00b7ne", "des", "Ti\u00b7tels", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Allgemeinen Gehalt, Freunde, gew\u00e4hret uns nun.", "tokens": ["All\u00b7ge\u00b7mei\u00b7nen", "Ge\u00b7halt", ",", "Freun\u00b7de", ",", "ge\u00b7w\u00e4h\u00b7ret", "uns", "nun", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Saget, wann n\u00fctzt mein Gedicht, o Musen? \u2013 Wenn es den Edlen", "tokens": ["Sa\u00b7get", ",", "wann", "n\u00fctzt", "mein", "Ge\u00b7dicht", ",", "o", "Mu\u00b7sen", "?", "\u2013", "Wenn", "es", "den", "Ed\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "$(", "KOUS", "PPER", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Weckt in dem Augenblick, wenn er sich selber vergi\u00dft.", "tokens": ["Weckt", "in", "dem", "Au\u00b7gen\u00b7blick", ",", "wenn", "er", "sich", "sel\u00b7ber", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "+--+-+---+--