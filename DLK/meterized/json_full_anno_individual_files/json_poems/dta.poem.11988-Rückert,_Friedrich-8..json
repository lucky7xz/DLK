{"dta.poem.11988": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enicht \u00e4ndern kannst du es, ergib dich in Geduld!\u201c", "tokens": ["\u201e", "nicht", "\u00e4n\u00b7dern", "kannst", "du", "es", ",", "er\u00b7gib", "dich", "in", "Ge\u00b7duld", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKNEG", "VVINF", "VMFIN", "PPER", "PPER", "$,", "VVIMP", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ehrst du Gottes Macht, nicht ehrst du seine Huld.", "tokens": ["So", "ehrst", "du", "Got\u00b7tes", "Macht", ",", "nicht", "ehrst", "du", "sei\u00b7ne", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NN", "$,", "PTKNEG", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sprich ob du dich, wenn du es k\u00f6nntest \u00e4ndern eben,", "tokens": ["Sprich", "ob", "du", "dich", ",", "wenn", "du", "es", "k\u00f6nn\u00b7test", "\u00e4n\u00b7dern", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "PRF", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergeben w\u00fcrdest auch? das w\u00e4r' ein recht Ergeben.", "tokens": ["Er\u00b7ge\u00b7ben", "w\u00fcr\u00b7dest", "auch", "?", "das", "w\u00e4r'", "ein", "recht", "Er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "$.", "PDS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nun ist halb das Wort um seinen Sinn gekommen;", "tokens": ["Doch", "nun", "ist", "halb", "das", "Wort", "um", "sei\u00b7nen", "Sinn", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn halb nur gabest du, halb ward es dir genommen.", "tokens": ["Denn", "halb", "nur", "ga\u00b7best", "du", ",", "halb", "ward", "es", "dir", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER", "$,", "ADJD", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}