{"textgrid.poem.67720": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf Willamow's Tod, des deutschen Dithyrambens\u00e4ngers", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vaterland, das seine besten S\u00f6hne,", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "das", "sei\u00b7ne", "bes\u00b7ten", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein rauher Fremdling, von sich stie\u00df,", "tokens": ["Ein", "rau\u00b7her", "Fremd\u00b7ling", ",", "von", "sich", "stie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie oder sich im Schoo\u00df, sie sonder Hippokrene", "tokens": ["Wie", "o\u00b7der", "sich", "im", "Schoo\u00df", ",", "sie", "son\u00b7der", "Hip\u00b7po\u00b7kre\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "KON", "PRF", "APPRART", "NN", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Brod verschmachten lie\u00df!", "tokens": ["Und", "Brod", "ver\u00b7schmach\u00b7ten", "lie\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch er ist hin, der einst auf Chelmo's Fluren,", "tokens": ["Auch", "er", "ist", "hin", ",", "der", "einst", "auf", "Chel\u00b7mo's", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$,", "PRELS", "ADV", "APPR", "NE", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bei Wasser und geringem Mahl,", "tokens": ["Bei", "Was\u00b7ser", "und", "ge\u00b7rin\u00b7gem", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir Dithyramben sang und k\u00fchn auf Pindar's Spuren", "tokens": ["Dir", "Dit\u00b7hy\u00b7ram\u00b7ben", "sang", "und", "k\u00fchn", "auf", "Pin\u00b7da\u00b7r's", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "KON", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aus seinem K\u00f6cher stahl", "tokens": ["Aus", "sei\u00b7nem", "K\u00f6\u00b7cher", "stahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Pfeile nicht geringsten, Nordens Helden,", "tokens": ["Der", "Pfei\u00b7le", "nicht", "ge\u00b7rings\u00b7ten", ",", "Nor\u00b7dens", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sobieski, Peter, Friederich,", "tokens": ["So\u00b7bies\u00b7ki", ",", "Pe\u00b7ter", ",", "Frie\u00b7de\u00b7rich", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein hohes Drei! mit Stolz der Ewigkeit zu melden.", "tokens": ["Ein", "ho\u00b7hes", "Drei", "!", "mit", "Stolz", "der", "E\u00b7wig\u00b7keit", "zu", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und w\u00fcnscht' und r\u00fchmte Dich,", "tokens": ["Und", "w\u00fcnscht'", "und", "r\u00fchm\u00b7te", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Berlin-Athene, das ihn ohn' Erbarmen", "tokens": ["Ber\u00b7lin\u00b7A\u00b7the\u00b7ne", ",", "das", "ihn", "ohn'", "Er\u00b7bar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf seinen Fluren schmachten lie\u00df,", "tokens": ["Auf", "sei\u00b7nen", "Flu\u00b7ren", "schmach\u00b7ten", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis, nicht mit Naso's Schuld, das Schicksal ihn, den Armen,", "tokens": ["Bis", ",", "nicht", "mit", "Na\u00b7so's", "Schuld", ",", "das", "Schick\u00b7sal", "ihn", ",", "den", "Ar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKNEG", "APPR", "NE", "NN", "$,", "ART", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur Newa hin verstie\u00df,", "tokens": ["Zur", "Ne\u00b7wa", "hin", "ver\u00b7stie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df er Enkomien dort s\u00e4nge, Namen,", "tokens": ["Da\u00df", "er", "En\u00b7ko\u00b7mi\u00b7en", "dort", "s\u00e4n\u00b7ge", ",", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$,", "NN", "$,"], "meter": "--++-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die weder Reim noch Sprache fa\u00dft,", "tokens": ["Die", "we\u00b7der", "Reim", "noch", "Spra\u00b7che", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Sieger Tschesme's, Chotschym's, Kayul's und zusammen", "tokens": ["Den", "Sie\u00b7ger", "Tsches\u00b7me's", ",", "Chot\u00b7schym's", ",", "Kayul's", "und", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "NE", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Eis- und Goldpalast", "tokens": ["Den", "Eis", "und", "Gold\u00b7pa\u00b7last"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sarsk-Szielo's, Petersburg's und Petershof's und Peter's,", "tokens": ["Sar\u00b7sk\u00b7\u00b7S\u00b7zie\u00b7lo's", ",", "Pe\u00b7ters\u00b7bur\u00b7g's", "und", "Pe\u00b7ters\u00b7hof's", "und", "Pe\u00b7ter's", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NE", "KON", "NE", "$,"], "meter": "+--+-+--+-+-+--+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Nach Lomonossow's Melodie,", "tokens": ["Nach", "Lo\u00b7mo\u00b7nos\u00b7sow's", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Sumorokow's Schwung, dem Raum des leeren Aethers", "tokens": ["In", "Su\u00b7mo\u00b7ro\u00b7kow's", "Schwung", ",", "dem", "Raum", "des", "lee\u00b7ren", "A\u00b7e\u00b7thers"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Griechisch und Deutsch verlieh;", "tokens": ["Grie\u00b7chisch", "und", "Deutsch", "ver\u00b7lieh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und Nonnen Me\u00dfkunst las und unverstanden", "tokens": ["Und", "Non\u00b7nen", "Me\u00df\u00b7kunst", "las", "und", "un\u00b7ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kriegslieder f\u00fcr Barbaren sang;", "tokens": ["Kriegs\u00b7lie\u00b7der", "f\u00fcr", "Bar\u00b7ba\u00b7ren", "sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Krim, dem Hellespont, dem Oby und den Landen,", "tokens": ["Der", "Krim", ",", "dem", "Hel\u00b7les\u00b7pont", ",", "dem", "O\u00b7by", "und", "den", "Lan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo kaum die Sonn' hindrang,", "tokens": ["Wo", "kaum", "die", "Sonn'", "hin\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Vaterland ert\u00f6nte, das nicht ihnen,", "tokens": ["Ein", "Va\u00b7ter\u00b7land", "er\u00b7t\u00f6n\u00b7te", ",", "das", "nicht", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht ihm ein Vaterland je war.", "tokens": ["Nicht", "ihm", "ein", "Va\u00b7ter\u00b7land", "je", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ah! Dir ein Kleid und Brod und Wasser zu verdienen", "tokens": ["Ah", "!", "Dir", "ein", "Kleid", "und", "Brod", "und", "Was\u00b7ser", "zu", "ver\u00b7die\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "ART", "NN", "KON", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und goldne Dose gar", "tokens": ["Und", "gold\u00b7ne", "Do\u00b7se", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Aus Kaiserh\u00e4nden und wie Deine Linde,", "tokens": ["Aus", "Kai\u00b7ser\u00b7h\u00e4n\u00b7den", "und", "wie", "Dei\u00b7ne", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PWAV", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Deiner Muse Blumenkranz)", "tokens": ["Wie", "Dei\u00b7ner", "Mu\u00b7se", "Blu\u00b7men\u00b7kranz", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu welken, zu verwehn in alle drei\u00dfig Winde:", "tokens": ["Zu", "wel\u00b7ken", ",", "zu", "ver\u00b7wehn", "in", "al\u00b7le", "drei\u00b7\u00dfig", "Win\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "APPR", "PIAT", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das, Bruder, war Dein Glanz,", "tokens": ["Das", ",", "Bru\u00b7der", ",", "war", "Dein", "Glanz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dein Lorbeer und Dein Leben! Ruh in Frieden,", "tokens": ["Dein", "Lor\u00b7beer", "und", "Dein", "Le\u00b7ben", "!", "Ruh", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$.", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwelkte Blume, liebliche", "tokens": ["Ver\u00b7welk\u00b7te", "Blu\u00b7me", ",", "lieb\u00b7li\u00b7che"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Verdorrte Linde! Nie sei's jener Flur beschieden,", "tokens": ["Ver\u00b7dorr\u00b7te", "Lin\u00b7de", "!", "Nie", "sei's", "je\u00b7ner", "Flur", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "ADV", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie ein Pindar seh'!", "tokens": ["Da\u00df", "sie", "ein", "Pin\u00b7dar", "seh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O Vaterland, das seine besten S\u00f6hne,", "tokens": ["O", "Va\u00b7ter\u00b7land", ",", "das", "sei\u00b7ne", "bes\u00b7ten", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein rauher Fremdling, von sich stie\u00df,", "tokens": ["Ein", "rau\u00b7her", "Fremd\u00b7ling", ",", "von", "sich", "stie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie oder sich im Schoo\u00df, sie sonder Hippokrene", "tokens": ["Wie", "o\u00b7der", "sich", "im", "Schoo\u00df", ",", "sie", "son\u00b7der", "Hip\u00b7po\u00b7kre\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "KON", "PRF", "APPRART", "NN", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Brod verschmachten lie\u00df!", "tokens": ["Und", "Brod", "ver\u00b7schmach\u00b7ten", "lie\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Auch er ist hin, der einst auf Chelmo's Fluren,", "tokens": ["Auch", "er", "ist", "hin", ",", "der", "einst", "auf", "Chel\u00b7mo's", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$,", "PRELS", "ADV", "APPR", "NE", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bei Wasser und geringem Mahl,", "tokens": ["Bei", "Was\u00b7ser", "und", "ge\u00b7rin\u00b7gem", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir Dithyramben sang und k\u00fchn auf Pindar's Spuren", "tokens": ["Dir", "Dit\u00b7hy\u00b7ram\u00b7ben", "sang", "und", "k\u00fchn", "auf", "Pin\u00b7da\u00b7r's", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "KON", "ADJD", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aus seinem K\u00f6cher stahl", "tokens": ["Aus", "sei\u00b7nem", "K\u00f6\u00b7cher", "stahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der Pfeile nicht geringsten, Nordens Helden,", "tokens": ["Der", "Pfei\u00b7le", "nicht", "ge\u00b7rings\u00b7ten", ",", "Nor\u00b7dens", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sobieski, Peter, Friederich,", "tokens": ["So\u00b7bies\u00b7ki", ",", "Pe\u00b7ter", ",", "Frie\u00b7de\u00b7rich", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+", 