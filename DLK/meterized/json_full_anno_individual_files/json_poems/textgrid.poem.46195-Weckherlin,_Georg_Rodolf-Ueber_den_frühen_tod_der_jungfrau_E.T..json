{"textgrid.poem.46195": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber den fr\u00fchen tod der jungfrau E.T.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der wahren tugend glanz, der klar in dem aufgang", "tokens": ["Der", "wah\u00b7ren", "tu\u00b7gend", "glanz", ",", "der", "klar", "in", "dem", "auf\u00b7gang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "PRELS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "dich mehr, dan der mittag in andern wolte zieren,", "tokens": ["dich", "mehr", ",", "dan", "der", "mit\u00b7tag", "in", "an\u00b7dern", "wol\u00b7te", "zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "ART", "NN", "APPR", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "bezeugte, da\u00df dein lauf ganz l\u00f6blich und nicht lang", "tokens": ["be\u00b7zeug\u00b7te", ",", "da\u00df", "dein", "lauf", "ganz", "l\u00f6b\u00b7lich", "und", "nicht", "lang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "KON", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "solt, wie ein sch\u00f6ner tag, schnellfl\u00fcchtig fort passieren.", "tokens": ["solt", ",", "wie", "ein", "sch\u00f6\u00b7ner", "tag", ",", "schnell\u00b7fl\u00fcch\u00b7tig", "fort", "pas\u00b7sie\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "ADJD", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die blumen, welche sich erzeigen reif zu fr\u00fch,", "tokens": ["Die", "blu\u00b7men", ",", "wel\u00b7che", "sich", "er\u00b7zei\u00b7gen", "reif", "zu", "fr\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "ADJD", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die werden von dem frost bald welk und weggenommen,", "tokens": ["die", "wer\u00b7den", "von", "dem", "frost", "bald", "welk", "und", "weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und kein zu fr\u00fche frucht kan, wan man schon mit m\u00fch", "tokens": ["und", "kein", "zu", "fr\u00fc\u00b7he", "frucht", "kan", ",", "wan", "man", "schon", "mit", "m\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "APPR", "ADJA", "NN", "VMFIN", "$,", "PWAV", "PIS", "ADV", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sie lang behalten wolt, den winter \u00fcberkommen.", "tokens": ["sie", "lang", "be\u00b7hal\u00b7ten", "wolt", ",", "den", "win\u00b7ter", "\u00fc\u00b7ber\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$,", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dan der natur gesatz, das der mensch halten mu\u00df,", "tokens": ["Dan", "der", "na\u00b7tur", "ge\u00b7satz", ",", "das", "der", "mensch", "hal\u00b7ten", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "gebeut, da\u00df nichts alhie gl\u00fcckselig lang soll wehren,", "tokens": ["ge\u00b7beut", ",", "da\u00df", "nichts", "al\u00b7hie", "gl\u00fcck\u00b7se\u00b7lig", "lang", "soll", "weh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIS", "ADV", "ADJD", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und da\u00df was herrlich ist, gleichsam zu einer bu\u00df", "tokens": ["und", "da\u00df", "was", "herr\u00b7lich", "ist", ",", "gleich\u00b7sam", "zu", "ei\u00b7ner", "bu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADJD", "VAFIN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "soll, die welt nicht zu lang zu ehren, bald aufh\u00f6ren.", "tokens": ["soll", ",", "die", "welt", "nicht", "zu", "lang", "zu", "eh\u00b7ren", ",", "bald", "auf\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "PTKZU", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So deine jugend zart mit arbeit, ehr und zucht,", "tokens": ["So", "dei\u00b7ne", "ju\u00b7gend", "zart", "mit", "ar\u00b7beit", ",", "ehr", "und", "zucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "APPR", "NN", "$,", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit weisheit und gotsforcht, wissenschaft viler zungen,", "tokens": ["mit", "weis\u00b7heit", "und", "gots\u00b7forcht", ",", "wis\u00b7sen\u00b7schaft", "vi\u00b7ler", "zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "mit deiner sch\u00f6nheit blust, mit deines verstands frucht,", "tokens": ["mit", "dei\u00b7ner", "sch\u00f6n\u00b7heit", "blust", ",", "mit", "dei\u00b7nes", "ver\u00b7stands", "frucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "zog billich dich herf\u00fcr bei alten und bei jungen.", "tokens": ["zog", "bil\u00b7lich", "dich", "her\u00b7f\u00fcr", "bei", "al\u00b7ten", "und", "bei", "jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ADV", "APPR", "ADJA", "KON", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Daher der freche tod, sehend, wie deine seel", "tokens": ["Da\u00b7her", "der", "fre\u00b7che", "tod", ",", "se\u00b7hend", ",", "wie", "dei\u00b7ne", "seel"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "$,", "VVPP", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "war mit vernunft und kunst des alters selbs geschm\u00fccket,", "tokens": ["war", "mit", "ver\u00b7nunft", "und", "kunst", "des", "al\u00b7ters", "selbs", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJD", "KON", "NN", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hat als ein reife frucht dich, frei von allem fehl,", "tokens": ["hat", "als", "ein", "rei\u00b7fe", "frucht", "dich", ",", "frei", "von", "al\u00b7lem", "fehl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "PPER", "$,", "ADJD", "APPR", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit ganz gnadloser hand, noch bl\u00fchend, abgezwicket.", "tokens": ["mit", "ganz", "gnad\u00b7lo\u00b7ser", "hand", ",", "noch", "bl\u00fc\u00b7hend", ",", "ab\u00b7ge\u00b7zwi\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "ADV", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ob aber wol der tod dir, unbefleckte blum", "tokens": ["Ob", "a\u00b7ber", "wol", "der", "tod", "dir", ",", "un\u00b7be\u00b7fleck\u00b7te", "blum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "durch seiner sichel streich den fall zu fr\u00fch gegeben:", "tokens": ["durch", "sei\u00b7ner", "si\u00b7chel", "streich", "den", "fall", "zu", "fr\u00fch", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so bl\u00fchet allzeit doch frisch deiner tugend ruhm,", "tokens": ["so", "bl\u00fc\u00b7het", "all\u00b7zeit", "doch", "frisch", "dei\u00b7ner", "tu\u00b7gend", "ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "weil die leben lang gnug, die recht und wol gnug leben.", "tokens": ["weil", "die", "le\u00b7ben", "lang", "gnug", ",", "die", "recht", "und", "wol", "gnug", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "ADJD", "ADV", "$,", "PRELS", "ADJD", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wan dan, o s\u00fc\u00dfe seel, dein leben und dein tod", "tokens": ["Wan", "dan", ",", "o", "s\u00fc\u00b7\u00dfe", "seel", ",", "dein", "le\u00b7ben", "und", "dein", "tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "FM", "FM", "FM", "$,", "PPOSAT", "VVINF", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und deines heils gewin und unsern verlust weisen,", "tokens": ["und", "dei\u00b7nes", "heils", "ge\u00b7win", "und", "un\u00b7sern", "ver\u00b7lust", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so m\u00f6gen billich wir got klagen unsre not,", "tokens": ["so", "m\u00f6\u00b7gen", "bil\u00b7lich", "wir", "got", "kla\u00b7gen", "uns\u00b7re", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "PPER", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "doch also, da\u00df wir ihn auch f\u00fcr dein leben preisen.", "tokens": ["doch", "al\u00b7so", ",", "da\u00df", "wir", "ihn", "auch", "f\u00fcr", "dein", "le\u00b7ben", "prei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der wahren tugend glanz, der klar in dem aufgang", "tokens": ["Der", "wah\u00b7ren", "tu\u00b7gend", "glanz", ",", "der", "klar", "in", "dem", "auf\u00b7gang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "PRELS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "dich mehr, dan der mittag in andern wolte zieren,", "tokens": ["dich", "mehr", ",", "dan", "der", "mit\u00b7tag", "in", "an\u00b7dern", "wol\u00b7te", "zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "ART", "NN", "APPR", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "bezeugte, da\u00df dein lauf ganz l\