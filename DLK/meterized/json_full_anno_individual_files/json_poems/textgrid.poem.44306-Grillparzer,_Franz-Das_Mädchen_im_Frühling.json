{"textgrid.poem.44306": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen im Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Weinstock sprosset, der Apfelbaum bl\u00fcht,", "tokens": ["Der", "Wein\u00b7stock", "spros\u00b7set", ",", "der", "Ap\u00b7fel\u00b7baum", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Veilchen duftet, die Rose gl\u00fcht,", "tokens": ["Das", "Veil\u00b7chen", "duf\u00b7tet", ",", "die", "Ro\u00b7se", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Mai senkt sich freundlich hernieder,", "tokens": ["Der", "Mai", "senkt", "sich", "freund\u00b7lich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Nachtigall fl\u00f6tet, die Wachtel ruft,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "fl\u00f6\u00b7tet", ",", "die", "Wach\u00b7tel", "ruft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die Lerche zwitschert in heiterer Luft,", "tokens": ["Die", "Ler\u00b7che", "zwit\u00b7schert", "in", "hei\u00b7te\u00b7rer", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das h\u00e4usliche Schw\u00e4lbchen kehrt wieder!", "tokens": ["Das", "h\u00e4us\u00b7li\u00b7che", "Schw\u00e4lb\u00b7chen", "kehrt", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Schon f\u00fcnfzehnmal sah ich den Apfelbaum bl\u00fchn,", "tokens": ["Schon", "f\u00fcnf\u00b7zehn\u00b7mal", "sah", "ich", "den", "Ap\u00b7fel\u00b7baum", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das Veilchen sprossen, die Rose gl\u00fchn,", "tokens": ["Das", "Veil\u00b7chen", "spros\u00b7sen", ",", "die", "Ro\u00b7se", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und feuriger strahlen die Sonne;", "tokens": ["Und", "feu\u00b7ri\u00b7ger", "strah\u00b7len", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da tanzt ich so heiter den Fr\u00fchlingsreihn,", "tokens": ["Da", "tanzt", "ich", "so", "hei\u00b7ter", "den", "Fr\u00fch\u00b7lings\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Da konnt ich der jungen Natur mich erfreun,", "tokens": ["Da", "konnt", "ich", "der", "jun\u00b7gen", "Na\u00b7tur", "mich", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da f\u00fchlt ich im Fr\u00fchling nur Wonne!", "tokens": ["Da", "f\u00fchlt", "ich", "im", "Fr\u00fch\u00b7ling", "nur", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Doch jetzt ists anders, doch nicht so gut,", "tokens": ["Doch", "jetzt", "ists", "an\u00b7ders", ",", "doch", "nicht", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "$,", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dahin, dahin ist mein fr\u00f6hlicher Mut,", "tokens": ["Da\u00b7hin", ",", "da\u00b7hin", "ist", "mein", "fr\u00f6h\u00b7li\u00b7cher", "Mut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich kann nicht mehr tanzen und scherzen!", "tokens": ["Ich", "kann", "nicht", "mehr", "tan\u00b7zen", "und", "scher\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Fr\u00fchling f\u00fcllt nicht mein Herz mehr mit Lust,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "f\u00fcllt", "nicht", "mein", "Herz", "mehr", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es atmet beklommen die junge Brust,", "tokens": ["Es", "at\u00b7met", "be\u00b7klom\u00b7men", "die", "jun\u00b7ge", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Es ist mir so \u00e4ngstlich im Herzen!", "tokens": ["Es", "ist", "mir", "so", "\u00e4ngst\u00b7lich", "im", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Im Busen wogts wie ein wallendes Meer,", "tokens": ["Im", "Bu\u00b7sen", "wogts", "wie", "ein", "wal\u00b7len\u00b7des", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So fr\u00f6hlich ist alles rings um mich her,", "tokens": ["So", "fr\u00f6h\u00b7lich", "ist", "al\u00b7les", "rings", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIS", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch ich f\u00fchl ein trauriges Sehnen!", "tokens": ["Doch", "ich", "f\u00fchl", "ein", "trau\u00b7ri\u00b7ges", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es freut sich so innig die ganze Natur,", "tokens": ["Es", "freut", "sich", "so", "in\u00b7nig", "die", "gan\u00b7ze", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die Freude belebt jede Kreatur,", "tokens": ["Die", "Freu\u00b7de", "be\u00b7lebt", "je\u00b7de", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich Arme vergie\u00dfe nur Tr\u00e4nen!", "tokens": ["Ich", "Ar\u00b7me", "ver\u00b7gie\u00b7\u00dfe", "nur", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Beklommenheit ists, was die Seele f\u00fchlt,", "tokens": ["Be\u00b7klom\u00b7men\u00b7heit", "ists", ",", "was", "die", "See\u00b7le", "f\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was nimmer ruhend im Busen w\u00fchlt,", "tokens": ["Was", "nim\u00b7mer", "ru\u00b7hend", "im", "Bu\u00b7sen", "w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie hat mir die Ruhe entrissen;", "tokens": ["Sie", "hat", "mir", "die", "Ru\u00b7he", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und dennoch m\u00f6cht ich das Gef\u00fchl, das mich qu\u00e4lt,", "tokens": ["Und", "den\u00b7noch", "m\u00f6cht", "ich", "das", "Ge\u00b7f\u00fchl", ",", "das", "mich", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Um alle Sch\u00e4tze der ganzen Welt", "tokens": ["Um", "al\u00b7le", "Sch\u00e4t\u00b7ze", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aus meinem Innern nicht missen!", "tokens": ["Aus", "mei\u00b7nem", "In\u00b7nern", "nicht", "mis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ach, darum bin ich wohl nur betr\u00fcbt,", "tokens": ["Ach", ",", "da\u00b7rum", "bin", "ich", "wohl", "nur", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PAV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Weil mich kein lebendes Wesen liebt? \u2013", "tokens": ["Weil", "mich", "kein", "le\u00b7ben\u00b7des", "We\u00b7sen", "liebt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das Nachtigallweibchen im Schatten", "tokens": ["Das", "Nach\u00b7ti\u00b7gall\u00b7weib\u00b7chen", "im", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der sparsambelaubten Linde baut", "tokens": ["Der", "spar\u00b7sam\u00b7be\u00b7laub\u00b7ten", "Lin\u00b7de", "baut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ihr Nest und locket mit freudigem Laut", "tokens": ["Ihr", "Nest", "und", "lo\u00b7cket", "mit", "freu\u00b7di\u00b7gem", "Laut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den sorgsam helfenden Gatten!", "tokens": ["Den", "sorg\u00b7sam", "hel\u00b7fen\u00b7den", "Gat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Tauben schn\u00e4beln im Rosengestr\u00e4uch", "tokens": ["Die", "Tau\u00b7ben", "schn\u00e4\u00b7beln", "im", "Ro\u00b7sen\u00b7ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und girren so z\u00e4rtlich, so innig, so weich,", "tokens": ["Und", "gir\u00b7ren", "so", "z\u00e4rt\u00b7lich", ",", "so", "in\u00b7nig", ",", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich seh sie mit sehnenden Blicken!", "tokens": ["Ich", "seh", "sie", "mit", "seh\u00b7nen\u00b7den", "Bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ach, w\u00e4r ich doch auch so gl\u00fccklich wie ihr!", "tokens": ["Ach", ",", "w\u00e4r", "ich", "doch", "auch", "so", "gl\u00fcck\u00b7lich", "wie", "ihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Ach, flattert ihr T\u00e4ubchen, ach flattert zu mir.", "tokens": ["Ach", ",", "flat\u00b7tert", "ihr", "T\u00e4ub\u00b7chen", ",", "ach", "flat\u00b7tert", "zu", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "$,", "ITJ", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "La\u00dft mich an den Busen euch dr\u00fccken!", "tokens": ["La\u00dft", "mich", "an", "den", "Bu\u00b7sen", "euch", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Kommt, liebt mich! \u2013 Dann w\u00e4r ich wohl wieder so", "tokens": ["Kommt", ",", "liebt", "mich", "!", "\u2013", "Dann", "w\u00e4r", "ich", "wohl", "wie\u00b7der", "so"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$.", "$(", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie ehmals, w\u00fcrde so heiter und froh", "tokens": ["Wie", "eh\u00b7mals", ",", "w\u00fcr\u00b7de", "so", "hei\u00b7ter", "und", "froh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie sonst die Tage verleben! \u2013", "tokens": ["Wie", "sonst", "die", "Ta\u00b7ge", "ver\u00b7le\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch nein, eine innere Stimme spricht:", "tokens": ["Doch", "nein", ",", "ei\u00b7ne", "in\u00b7ne\u00b7re", "Stim\u00b7me", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Was meinem f\u00fchlenden Herzen gebricht,", "tokens": ["Was", "mei\u00b7nem", "f\u00fch\u00b7len\u00b7den", "Her\u00b7zen", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kann ein f\u00fchlendes Herz mir nur geben!", "tokens": ["Kann", "ein", "f\u00fch\u00b7len\u00b7des", "Herz", "mir", "nur", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Der Weinstock sprosset, der Apfelbaum bl\u00fcht,", "tokens": ["Der", "Wein\u00b7stock", "spros\u00b7set", ",", "der", "Ap\u00b7fel\u00b7baum", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Veilchen duftet, die Rose gl\u00fcht,", "tokens": ["Das", "Veil\u00b7chen", "duf\u00b7tet", ",", "die", "Ro\u00b7se", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Mai senkt sich freundlich hernieder,", "tokens": ["Der", "Mai", "senkt", "sich", "freund\u00b7lich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Nachtigall fl\u00f6tet, die Wachtel ruft,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "fl\u00f6\u00b7tet", ",", "die", "Wach\u00b7tel", "ruft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die Lerche zwitschert in heiterer Luft,", "tokens": ["Die", "Ler\u00b7che", "zwit\u00b7schert", "in", "hei\u00b7te\u00b7rer", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das h\u00e4usliche Schw\u00e4lbchen kehrt wieder!", "tokens": ["Das", "h\u00e4us\u00b7li\u00b7che", "Schw\u00e4lb\u00b7chen", "kehrt", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Schon f\u00fcnfzehnmal sah ich den Apfelbaum bl\u00fchn,", "tokens": ["Schon", "f\u00fcnf\u00b7zehn\u00b7mal", "sah", "ich", "den", "Ap\u00b7fel\u00b7baum", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das Veilchen sprossen, die Rose gl\u00fchn,", "tokens": ["Das", "Veil\u00b7chen", "spros\u00b7sen", ",", "die", "Ro\u00b7se", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und feuriger strahlen die Sonne;", "tokens": ["Und", "feu\u00b7ri\u00b7ger", "strah\u00b7len", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da tanzt ich so heiter den Fr\u00fchlingsreihn,", "tokens": ["Da", "tanzt", "ich", "so", "hei\u00b7ter", "den", "Fr\u00fch\u00b7lings\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Da konnt ich der jungen Natur mich erfreun,", "tokens": ["Da", "konnt", "ich", "der", "jun\u00b7gen", "Na\u00b7tur", "mich", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da f