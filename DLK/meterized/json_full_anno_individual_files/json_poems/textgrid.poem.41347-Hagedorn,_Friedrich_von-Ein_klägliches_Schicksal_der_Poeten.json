{"textgrid.poem.41347": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ein kl\u00e4gliches Schicksal der Poeten", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sorglos schl\u00e4ft der sichre Musensohn,", "tokens": ["Wie", "sorg\u00b7los", "schl\u00e4ft", "der", "sich\u00b7re", "Mu\u00b7sen\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wann er, bei Kerz' und Nacht, in dichterischen Stunden,", "tokens": ["Wann", "er", ",", "bei", "Ker\u00b7z'", "und", "Nacht", ",", "in", "dich\u00b7te\u00b7ri\u00b7schen", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nun, wie er glaubt, den Einfall ausgefunden,", "tokens": ["Nun", ",", "wie", "er", "glaubt", ",", "den", "Ein\u00b7fall", "aus\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den er gesucht, der ihn zu sehr geflohn!", "tokens": ["Den", "er", "ge\u00b7sucht", ",", "der", "ihn", "zu", "sehr", "ge\u00b7flohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PRELS", "PPER", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie unruhvoll wird seine Lagerstatt,", "tokens": ["Wie", "un\u00b7ruh\u00b7voll", "wird", "sei\u00b7ne", "La\u00b7ger\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wann ihm der n\u00e4chste Tag, sobald er ganz erwachet,", "tokens": ["Wann", "ihm", "der", "n\u00e4chs\u00b7te", "Tag", ",", "so\u00b7bald", "er", "ganz", "er\u00b7wa\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Fundes Werth mit Recht verd\u00e4chtig machet!", "tokens": ["Des", "Fun\u00b7des", "Werth", "mit", "Recht", "ver\u00b7d\u00e4ch\u00b7tig", "ma\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Einfall welkt: die Worte flie\u00dfen matt.", "tokens": ["Der", "Ein\u00b7fall", "welkt", ":", "die", "Wor\u00b7te", "flie\u00b7\u00dfen", "matt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So schmeichelhaft war Jacobs Nacht und Stand,", "tokens": ["So", "schmei\u00b7chel\u00b7haft", "war", "Ja\u00b7cobs", "Nacht", "und", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als, wie er w\u00fcnscht' und hofft', ihn Rahels Reiz begl\u00fcckte:"