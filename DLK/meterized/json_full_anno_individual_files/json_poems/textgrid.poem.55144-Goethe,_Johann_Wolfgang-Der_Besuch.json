{"textgrid.poem.55144": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Besuch", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Liebste wollt ich heut beschleichen,", "tokens": ["Mei\u00b7ne", "Liebs\u00b7te", "wollt", "ich", "heut", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber ihre T\u00fcre war verschlossen.", "tokens": ["A\u00b7ber", "ih\u00b7re", "T\u00fc\u00b7re", "war", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hab ich doch den Schl\u00fcssel in der Tasche!", "tokens": ["Hab", "ich", "doch", "den", "Schl\u00fcs\u00b7sel", "in", "der", "Ta\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00d6ffn' ich leise die geliebte T\u00fcre!", "tokens": ["\u00d6ffn'", "ich", "lei\u00b7se", "die", "ge\u00b7lieb\u00b7te", "T\u00fc\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Auf dem Saale fand ich nicht das M\u00e4dchen,", "tokens": ["Auf", "dem", "Saa\u00b7le", "fand", "ich", "nicht", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fand das M\u00e4dchen nicht in ihrer Stube;", "tokens": ["Fand", "das", "M\u00e4d\u00b7chen", "nicht", "in", "ih\u00b7rer", "Stu\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Endlich, da ich leis die Kammer \u00f6ffne,", "tokens": ["End\u00b7lich", ",", "da", "ich", "leis", "die", "Kam\u00b7mer", "\u00f6ff\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Find ich sie, gar zierlich eingeschlafen,", "tokens": ["Find", "ich", "sie", ",", "gar", "zier\u00b7lich", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Angekleidet, auf dem Sofa liegen.", "tokens": ["An\u00b7ge\u00b7klei\u00b7det", ",", "auf", "dem", "So\u00b7fa", "lie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Bei der Arbeit war sie eingeschlafen;", "tokens": ["Bei", "der", "Ar\u00b7beit", "war", "sie", "ein\u00b7ge\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das Gestrickte mit den Nadeln ruhte", "tokens": ["Das", "Ge\u00b7strick\u00b7te", "mit", "den", "Na\u00b7deln", "ruh\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Zwischen den gefaltnen zarten H\u00e4nden;", "tokens": ["Zwi\u00b7schen", "den", "ge\u00b7falt\u00b7nen", "zar\u00b7ten", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich setzte mich an ihre Seite,", "tokens": ["Und", "ich", "setz\u00b7te", "mich", "an", "ih\u00b7re", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ging bei mir zu Rat, ob ich sie weckte.", "tokens": ["Ging", "bei", "mir", "zu", "Rat", ",", "ob", "ich", "sie", "weck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Da betrachtet ich den sch\u00f6nen Frieden,", "tokens": ["Da", "be\u00b7trach\u00b7tet", "ich", "den", "sch\u00f6\u00b7nen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der auf ihren Augenlidern ruhte:", "tokens": ["Der", "auf", "ih\u00b7ren", "Au\u00b7gen\u00b7li\u00b7dern", "ruh\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf den Lippen war die stille Treue,", "tokens": ["Auf", "den", "Lip\u00b7pen", "war", "die", "stil\u00b7le", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf den Wangen Lieblichkeit zu Hause,", "tokens": ["Auf", "den", "Wan\u00b7gen", "Lieb\u00b7lich\u00b7keit", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und die Unschuld eines guten Herzens", "tokens": ["Und", "die", "Un\u00b7schuld", "ei\u00b7nes", "gu\u00b7ten", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Regte sich im Busen hin und wider.", "tokens": ["Reg\u00b7te", "sich", "im", "Bu\u00b7sen", "hin", "und", "wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Jedes ihrer Glieder lag gef\u00e4llig", "tokens": ["Je\u00b7des", "ih\u00b7rer", "Glie\u00b7der", "lag", "ge\u00b7f\u00e4l\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aufgel\u00f6st vom s\u00fc\u00dfen G\u00f6tterbalsam.", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "vom", "s\u00fc\u00b7\u00dfen", "G\u00f6t\u00b7ter\u00b7bal\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Freudig sa\u00df ich da, und die Betrachtung", "tokens": ["Freu\u00b7dig", "sa\u00df", "ich", "da", ",", "und", "die", "Be\u00b7trach\u00b7tung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Hielte die Begierde, sie zu wecken,", "tokens": ["Hiel\u00b7te", "die", "Be\u00b7gier\u00b7de", ",", "sie", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Mit geheimen Banden fest und fester.", "tokens": ["Mit", "ge\u00b7hei\u00b7men", "Ban\u00b7den", "fest", "und", "fes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "O du Liebe, dacht ich, kann der Schlummer,", "tokens": ["O", "du", "Lie\u00b7be", ",", "dacht", "ich", ",", "kann", "der", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Verr\u00e4ter jedes falschen Zuges,", "tokens": ["Der", "Ver\u00b7r\u00e4\u00b7ter", "je\u00b7des", "fal\u00b7schen", "Zu\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kann er dir nicht schaden, nichts entdecken,", "tokens": ["Kann", "er", "dir", "nicht", "scha\u00b7den", ",", "nichts", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was des Freundes zarte Meinung st\u00f6rte?", "tokens": ["Was", "des", "Freun\u00b7des", "zar\u00b7te", "Mei\u00b7nung", "st\u00f6r\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Deine holden Augen sind geschlossen,", "tokens": ["Dei\u00b7ne", "hol\u00b7den", "Au\u00b7gen", "sind", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die mich offen schon allein bezaubern;", "tokens": ["Die", "mich", "of\u00b7fen", "schon", "al\u00b7lein", "be\u00b7zau\u00b7bern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Es bewegen deine s\u00fc\u00dfen Lippen", "tokens": ["Es", "be\u00b7we\u00b7gen", "dei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weder sich zur Rede noch zum Kusse;", "tokens": ["We\u00b7der", "sich", "zur", "Re\u00b7de", "noch", "zum", "Kus\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aufgel\u00f6st sind diese Zauberbande", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "sind", "die\u00b7se", "Zau\u00b7berb\u00b7an\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Deiner Arme, die mich sonst umschlingen,", "tokens": ["Dei\u00b7ner", "Ar\u00b7me", ",", "die", "mich", "sonst", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und die Hand, die reizende Gef\u00e4hrtin", "tokens": ["Und", "die", "Hand", ",", "die", "rei\u00b7zen\u00b7de", "Ge\u00b7f\u00e4hr\u00b7tin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "S\u00fc\u00dfer Schmeicheleien, unbeweglich.", "tokens": ["S\u00fc\u00b7\u00dfer", "Schmei\u00b7che\u00b7lei\u00b7en", ",", "un\u00b7be\u00b7weg\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "W\u00e4r's ein Irrtum, wie ich von dir denke,", "tokens": ["W\u00e4r's", "ein", "Irr\u00b7tum", ",", "wie", "ich", "von", "dir", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "W\u00e4r es Selbstbetrug, wie ich dich liebe,", "tokens": ["W\u00e4r", "es", "Selbst\u00b7be\u00b7trug", ",", "wie", "ich", "dich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "M\u00fc\u00dft ich's jetzt entdecken, da sich Amor", "tokens": ["M\u00fc\u00dft", "ich's", "jetzt", "ent\u00b7de\u00b7cken", ",", "da", "sich", "A\u00b7mor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "VVINF", "$,", "KOUS", "PRF", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ohne Binde neben mich gestellet.", "tokens": ["Oh\u00b7ne", "Bin\u00b7de", "ne\u00b7ben", "mich", "ge\u00b7stel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Lange sa\u00df ich so und freute herzlich", "tokens": ["Lan\u00b7ge", "sa\u00df", "ich", "so", "und", "freu\u00b7te", "herz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihres Wertes mich und meiner Liebe;", "tokens": ["Ih\u00b7res", "Wer\u00b7tes", "mich", "und", "mei\u00b7ner", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schlafend hatte sie mir so gefallen,", "tokens": ["Schla\u00b7fend", "hat\u00b7te", "sie", "mir", "so", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df ich mich nicht traute, sie zu wecken.", "tokens": ["Da\u00df", "ich", "mich", "nicht", "trau\u00b7te", ",", "sie", "zu", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Leise leg ich ihr zwei Pomeranzen", "tokens": ["Lei\u00b7se", "leg", "ich", "ihr", "zwei", "Po\u00b7me\u00b7ran\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "PPER", "PPOSAT", "CARD", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und zwei Rosen auf das Tischchen nieder;", "tokens": ["Und", "zwei", "Ro\u00b7sen", "auf", "das", "Tischchen", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sachte, sachte schleich ich meiner Wege.", "tokens": ["Sach\u00b7te", ",", "sach\u00b7te", "schleich", "ich", "mei\u00b7ner", "We\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "\u00d6ffnet sie die Augen, meine Gute,", "tokens": ["\u00d6ff\u00b7net", "sie", "die", "Au\u00b7gen", ",", "mei\u00b7ne", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Gleich erblickt sie diese bunte Gabe,", "tokens": ["Gleich", "er\u00b7blickt", "sie", "die\u00b7se", "bun\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", 