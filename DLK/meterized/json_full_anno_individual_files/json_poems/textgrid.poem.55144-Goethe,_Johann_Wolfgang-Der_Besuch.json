e": "trochaic.penta"}, "line.10": {"text": "Hielte die Begierde, sie zu wecken,", "tokens": ["Hiel\u00b7te", "die", "Be\u00b7gier\u00b7de", ",", "sie", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Mit geheimen Banden fest und fester.", "tokens": ["Mit", "ge\u00b7hei\u00b7men", "Ban\u00b7den", "fest", "und", "fes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "O du Liebe, dacht ich, kann der Schlummer,", "tokens": ["O", "du", "Lie\u00b7be", ",", "dacht", "ich", ",", "kann", "der", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Verr\u00e4ter jedes falschen Zuges,", "tokens": ["Der", "Ver\u00b7r\u00e4\u00b7ter", "je\u00b7des", "fal\u00b7schen", "Zu\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kann er dir nicht schaden, nichts entdecken,", "tokens": ["Kann", "er", "dir", "nicht", "scha\u00b7den", ",", "nichts", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was des Freundes zarte Meinung st\u00f6rte?", "tokens": ["Was", "des", "Freun\u00b7des", "zar\u00b7te", "Mei\u00b7nung", "st\u00f6r\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Deine holden Augen sind geschlossen,", "tokens": ["Dei\u00b7ne", "hol\u00b7den", "Au\u00b7gen", "sind", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die mich offen schon allein bezaubern;", "tokens": ["Die", "mich", "of\u00b7fen", "schon", "al\u00b7lein", "be\u00b7zau\u00b7bern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Es bewegen deine s\u00fc\u00dfen Lippen", "tokens": ["Es", "be\u00b7we\u00b7gen", "dei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weder sich zur Rede noch zum Kusse;", "tokens": ["We\u00b7der", "sich", "zur", "Re\u00b7de", "noch", "zum", "Kus\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aufgel\u00f6st sind diese Zauberbande", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "sind", "die\u00b7se", "Zau\u00b7berb\u00b7an\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Deiner Arme, die mich sonst umschlingen,", "tokens": ["Dei\u00b7ner", "Ar\u00b7me", ",", "die", "mich", "sonst", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und die Hand, die reizende Gef\u00e4hrtin", "tokens": ["Und", "die", "Hand", ",", "die", "rei\u00b7zen\u00b7de", "Ge\u00b7f\u00e4hr\u00b7tin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "S\u00fc\u00dfer Schmeicheleien, unbeweglich.", "tokens": ["S\u00fc\u00b7\u00dfer", "Schmei\u00b7che\u00b7lei\u00b7en", ",", "un\u00b7be\u00b7weg\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "W\u00e4r's ein Irrtum, wie ich von dir denke,", "tokens": ["W\u00e4r's", "ein", "Irr\u00b7tum", ",", "wie", "ich", "von", "dir", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "W\u00e4r es Selbstbetrug, wie ich dich liebe,", "tokens": ["W\u00e4r", "es", "Selbst\u00b7be\u00b7trug", ",", "wie", "ich", "dich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "M\u00fc\u00dft ich's jetzt entdecken, da sich Amor", "tokens": ["M\u00fc\u00dft", "ich's", "jetzt", "ent\u00b7de\u00b7cken", ",", "da", "sich", "A\u00b7mor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "VVINF", "$,", "KOUS", "PRF", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ohne Binde neben mich gestellet.", "tokens": ["Oh\u00b7ne", "Bin\u00b7de", "ne\u00b7ben", "mich", "ge\u00b7stel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Lange sa\u00df ich so und freute herzlich", "tokens": ["Lan\u00b7ge", "sa\u00df", "ich", "so", "und", "freu\u00b7te", "herz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihres Wertes mich und meiner Liebe;", "tokens": ["Ih\u00b7res", "Wer\u00b7tes", "mich", "und", "mei\u00b7ner", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schlafend hatte sie mir so gefallen,", "tokens": ["Schla\u00b7fend", "hat\u00b7te", "sie", "mir", "so", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df ich mich nicht traute, sie zu wecken.", "tokens": ["Da\u00df", "ich", "mich", "nicht", "trau\u00b7te", ",", "sie", "zu", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Leise leg ich ihr zwei Pomeranzen", "tokens": ["Lei\u00b7se", "leg", "ich", "ihr", "zwei", "Po\u00b7me\u00b7ran\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "PPER", "PPOSAT", "CARD", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und zwei Rosen auf das Tischchen nieder;", "tokens": ["Und", "zwei", "Ro\u00b7sen", "auf", "das", "Tischchen", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sachte, sachte schleich ich meiner Wege.", "tokens": ["Sach\u00b7te", ",", "sach\u00b7te", "schleich", "ich", "mei\u00b7ner", "We\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "\u00d6ffnet sie die Augen, meine Gute,", "tokens": ["\u00d6ff\u00b7net", "sie", "die", "Au\u00b7gen", ",", "mei\u00b7ne", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Gleich erblickt sie diese bunte Gabe,", "tokens": ["Gleich", "er\u00b7blickt", "sie", "die\u00b7se", "bun\u00b7te", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Staunt, wie immer bei verschlo\u00dfnen T\u00fcren", "tokens": ["Staunt", ",", "wie", "im\u00b7mer", "bei", "ver\u00b7schlo\u00df\u00b7nen", "T\u00fc\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Dieses freundliche Geschenk sich finde.", "tokens": ["Die\u00b7ses", "freund\u00b7li\u00b7che", "Ge\u00b7schenk", "sich", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Seh ich diese Nacht den Engel wieder,", "tokens": ["Seh", "ich", "die\u00b7se", "Nacht", "den", "En\u00b7gel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O wie freut sie sich, vergilt mir doppelt", "tokens": ["O", "wie", "freut", "sie", "sich", ",", "ver\u00b7gilt", "mir", "dop\u00b7pelt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dieses Opfer meiner zarten Liebe.", "tokens": ["Die\u00b7ses", "Op\u00b7fer", "mei\u00b7ner", "zar\u00b7ten", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}