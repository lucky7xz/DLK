{"textgrid.poem.38727": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Arion schifft auf Meereswogen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arion schifft auf Meereswogen", "tokens": ["A\u00b7rion", "schifft", "auf", "Mee\u00b7res\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach seiner theuren Heimath zu,", "tokens": ["Nach", "sei\u00b7ner", "theu\u00b7ren", "Hei\u00b7math", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird vom Winde fortgezogen", "tokens": ["Er", "wird", "vom", "Win\u00b7de", "fort\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die See in stiller, sanfter Ruh'.", "tokens": ["Die", "See", "in", "stil\u00b7ler", ",", "sanf\u00b7ter", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Schiffer stehn von fern und fl\u00fcstern,", "tokens": ["Die", "Schif\u00b7fer", "stehn", "von", "fern", "und", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Dichter sieht in's Morgenroth,", "tokens": ["Der", "Dich\u00b7ter", "sieht", "in's", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach seinen goldnen Sch\u00e4tzen l\u00fcstern", "tokens": ["Nach", "sei\u00b7nen", "gold\u00b7nen", "Sch\u00e4t\u00b7zen", "l\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beschlie\u00dfen sie des S\u00e4ngers Tod.", "tokens": ["Be\u00b7schlie\u00b7\u00dfen", "sie", "des", "S\u00e4n\u00b7gers", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Arion merkt die stille T\u00fccke,", "tokens": ["A\u00b7rion", "merkt", "die", "stil\u00b7le", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er bietet ihnen all' sein Gold,", "tokens": ["Er", "bie\u00b7tet", "ih\u00b7nen", "all'", "sein", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er klagt und seufzt, da\u00df seinem Gl\u00fccke", "tokens": ["Er", "klagt", "und", "seufzt", ",", "da\u00df", "sei\u00b7nem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schicksal nicht wie vordem hold. \u2013", "tokens": ["Das", "Schick\u00b7sal", "nicht", "wie", "vor\u00b7dem", "hold", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKNEG", "KOKOM", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie aber haben es beschlossen,", "tokens": ["Sie", "a\u00b7ber", "ha\u00b7ben", "es", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Tod giebt ihnen Sicherheit,", "tokens": ["Nur", "Tod", "giebt", "ih\u00b7nen", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinab in's Meer wird er gesto\u00dfen,", "tokens": ["Hin\u00b7ab", "in's", "Meer", "wird", "er", "ge\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon sind sie mit dem Schiffe weit.", "tokens": ["Schon", "sind", "sie", "mit", "dem", "Schif\u00b7fe", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er hat die Leyer nur gerettet,", "tokens": ["Er", "hat", "die", "Le\u00b7yer", "nur", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie schwebt in seiner sch\u00f6nen Hand,", "tokens": ["Sie", "schwebt", "in", "sei\u00b7ner", "sch\u00f6\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Meeresfluthen hingebettet", "tokens": ["In", "Mee\u00b7res\u00b7flut\u00b7hen", "hin\u00b7ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Freude von ihm abgewandt.", "tokens": ["Ist", "Freu\u00b7de", "von", "ihm", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch greift er in die goldnen Saiten", "tokens": ["Doch", "greift", "er", "in", "die", "gold\u00b7nen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df laut die W\u00f6lbung wiederklingt,", "tokens": ["Da\u00df", "laut", "die", "W\u00f6l\u00b7bung", "wie\u00b7der\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt mit den Wogen wild zu streiten", "tokens": ["Statt", "mit", "den", "Wo\u00b7gen", "wild", "zu", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er sanft die zarten T\u00f6ne singt:", "tokens": ["Er", "sanft", "die", "zar\u00b7ten", "T\u00f6\u00b7ne", "singt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Klinge Saitenspiel,", "tokens": ["Klin\u00b7ge", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "In der Fluth", "tokens": ["In", "der", "Fluth"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "W\u00e4chst mein Muth,", "tokens": ["W\u00e4chst", "mein", "Muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Sterb' ich gleich, verfehl' ich nicht mein Ziel.", "tokens": ["Sterb'", "ich", "gleich", ",", "ver\u00b7fehl'", "ich", "nicht", "mein", "Ziel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Unverdrossen", "tokens": ["Un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Komm' ich, Tod,", "tokens": ["Komm'", "ich", ",", "Tod", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dein Gebot", "tokens": ["Dein", "Ge\u00b7bot"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Schreckt' mich nicht, mein Leben ward genossen.", "tokens": ["Schreckt'", "mich", "nicht", ",", "mein", "Le\u00b7ben", "ward", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Welle hebt", "tokens": ["Wel\u00b7le", "hebt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Mich im Schimmer,", "tokens": ["Mich", "im", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bald den Schwimmer", "tokens": ["Bald", "den", "Schwim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sie in tiefer, nasser Fluth begr\u00e4bt.", "tokens": ["Sie", "in", "tie\u00b7fer", ",", "nas\u00b7ser", "Fluth", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJD", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "So klang das Lied durch alle Tiefen,", "tokens": ["So", "klang", "das", "Lied", "durch", "al\u00b7le", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wogen wurden sanft bewegt,", "tokens": ["Die", "Wo\u00b7gen", "wur\u00b7den", "sanft", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Abgrund's Schl\u00fcften, wo sie schliefen,", "tokens": ["In", "Ab\u00b7grun\u00b7d's", "Schl\u00fcf\u00b7ten", ",", "wo", "sie", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Seegethiere aufgeregt.", "tokens": ["Die", "See\u00b7ge\u00b7thie\u00b7re", "auf\u00b7ge\u00b7regt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Aus allen Tiefen blaue Wunder,", "tokens": ["Aus", "al\u00b7len", "Tie\u00b7fen", "blau\u00b7e", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die h\u00fcpfend um den S\u00e4nger ziehn,", "tokens": ["Die", "h\u00fcp\u00b7fend", "um", "den", "S\u00e4n\u00b7ger", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Meeresfl\u00e4che weit hinunter", "tokens": ["Die", "Mee\u00b7res\u00b7fl\u00e4\u00b7che", "weit", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beschwimmen die Tritonen gr\u00fcn.", "tokens": ["Be\u00b7schwim\u00b7men", "die", "Tri\u00b7to\u00b7nen", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die Wellen tanzen, Fische springen,", "tokens": ["Die", "Wel\u00b7len", "tan\u00b7zen", ",", "Fi\u00b7sche", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seit Venus aus den Fluthen kam,", "tokens": ["Seit", "Ve\u00b7nus", "aus", "den", "Flut\u00b7hen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man dieses Jauchzen, Wonneklingen", "tokens": ["Man", "die\u00b7ses", "Jauch\u00b7zen", ",", "Won\u00b7ne\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PIS", "PDAT", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Meeresvesten nicht vernahm.", "tokens": ["In", "Mee\u00b7res\u00b7ves\u00b7ten", "nicht", "ver\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Arion sieht mit trunknen Blicken", "tokens": ["A\u00b7rion", "sieht", "mit", "trunk\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lautsingend in das Seegew\u00fchl,", "tokens": ["Lauts\u00b7in\u00b7gend", "in", "das", "See\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er f\u00e4hrt auf eines Delphins R\u00fccken,", "tokens": ["Er", "f\u00e4hrt", "auf", "ei\u00b7nes", "Del\u00b7phins", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4gt l\u00e4chelnd in sein Saitenspiel.", "tokens": ["Schl\u00e4gt", "l\u00e4\u00b7chelnd", "in", "sein", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Der Fisch, zu Diensten ihm gezwungen,", "tokens": ["Der", "Fisch", ",", "zu", "Diens\u00b7ten", "ihm", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Naht schon mit ihm der Felsenbank,", "tokens": ["Naht", "schon", "mit", "ihm", "der", "Fel\u00b7sen\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Arion hat den Fels errungen", "tokens": ["A\u00b7rion", "hat", "den", "Fels", "er\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und singt dem F\u00e4hrmann seinen Dank.", "tokens": ["Und", "singt", "dem", "F\u00e4hr\u00b7mann", "sei\u00b7nen", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Am Ufer kniet er, dankt den G\u00f6ttern,", "tokens": ["Am", "U\u00b7fer", "kniet", "er", ",", "dankt", "den", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er entrann dem nassen Tod.", "tokens": ["Da\u00df", "er", "ent\u00b7rann", "dem", "nas\u00b7sen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der S\u00e4nger triumphirt in Wettern,", "tokens": ["Der", "S\u00e4n\u00b7ger", "tri\u00b7um\u00b7phirt", "in", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn r\u00fchrt Gefahr nicht an und Tod.", "tokens": ["Ihn", "r\u00fchrt", "Ge\u00b7fahr", "nicht", "an", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKNEG", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Arion schifft auf Meereswogen", "tokens": ["A\u00b7rion", "schifft", "auf", "Mee\u00b7res\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach seiner theuren Heimath zu,", "tokens": ["Nach", "sei\u00b7ner", "theu\u00b7ren", "Hei\u00b7math", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird vom Winde fortgezogen", "tokens": ["Er", "wird", "vom", "Win\u00b7de", "fort\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die See in stiller, sanfter Ruh'.", "tokens": ["Die", "See", "in", "stil\u00b7ler", ",", "sanf\u00b7ter", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Die Schiffer stehn von fern und fl\u00fcstern,", "tokens": ["Die", "Schif\u00b7fer", "stehn", "von", "fern", "und", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Dichter sieht in's Morgenroth,", "tokens": ["Der", "Dich\u00b7ter", "sieht", "in's", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach seinen goldnen Sch\u00e4tzen l\u00fcstern", "tokens": ["Nach", "sei\u00b7nen", "gold\u00b7nen", "Sch\u00e4t\u00b7zen", "l\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beschlie\u00dfen sie des S\u00e4ngers Tod.", "tokens": ["Be\u00b7schlie\u00b7\u00dfen", "sie", "des", "S\u00e4n\u00b7gers", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Arion merkt die stille T\u00fccke,", "tokens": ["A\u00b7rion", "merkt", "die", "stil\u00b7le", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er bietet ihnen all' sein Gold,", "tokens": ["Er", "bie\u00b7tet", "ih\u00b7nen", "all'", "sein", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er klagt und seufzt, da\u00df seinem Gl\u00fccke", "tokens": ["Er", "klagt", "und", "seufzt", ",", "da\u00df", "sei\u00b7nem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schicksal nicht wie vordem hold. \u2013", "tokens": ["Das", "Schick\u00b7sal", "nicht", "wie", "vor\u00b7dem", "hold", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKNEG", "KOKOM", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Sie aber haben es beschlossen,", "tokens": ["Sie", "a\u00b7ber", "ha\u00b7ben", "es", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Tod giebt ihnen Sicherheit,", "tokens": ["Nur", "Tod", "giebt", "ih\u00b7nen", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinab in's Meer wird er gesto\u00dfen,", "tokens": ["Hin\u00b7ab", "in's", "Meer", "wird", "er", "ge\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon sind sie mit dem Schiffe weit.", "tokens": ["Schon", "sind", "sie", "mit", "dem", "Schif\u00b7fe", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Er hat die Leyer nur gerettet,", "tokens": ["Er", "hat", "die", "Le\u00b7yer", "nur", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie schwebt in seiner sch\u00f6nen Hand,", "tokens": ["Sie", "schwebt", "in", "sei\u00b7ner", "sch\u00f6\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Meeresfluthen hingebettet", "tokens": ["In", "Mee\u00b7res\u00b7flut\u00b7hen", "hin\u00b7ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Freude von ihm abgewandt.", "tokens": ["Ist", "Freu\u00b7de", "von", "ihm", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Doch greift er in die goldnen Saiten", "tokens": ["Doch", "greift", "er", "in", "die", "gold\u00b7nen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df laut die W\u00f6lbung wiederklingt,", "tokens": ["Da\u00df", "laut", "die", "W\u00f6l\u00b7bung", "wie\u00b7der\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt mit den Wogen wild zu streiten", "tokens": ["Statt", "mit", "den", "Wo\u