{"dta.poem.7662": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Angedenken stellst du um dich her zusammen", "tokens": ["Viel", "An\u00b7ge\u00b7den\u00b7ken", "stellst", "du", "um", "dich", "her", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "PRF", "APZR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Ehren Theuerer, von denen sie dir stammen.", "tokens": ["Zu", "Eh\u00b7ren", "Theu\u00b7e\u00b7rer", ",", "von", "de\u00b7nen", "sie", "dir", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die theuern Namen nennt dir nun ihr stummer Mund,", "tokens": ["Die", "theu\u00b7ern", "Na\u00b7men", "nennt", "dir", "nun", "ihr", "stum\u00b7mer", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und machet dir das Herz nicht fr\u00f6hlich, sondern wund.", "tokens": ["Und", "ma\u00b7chet", "dir", "das", "Herz", "nicht", "fr\u00f6h\u00b7lich", ",", "son\u00b7dern", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Beim Angedenken denkst du, da\u00df vom Lebensmale", "tokens": ["Beim", "An\u00b7ge\u00b7den\u00b7ken", "denkst", "du", ",", "da\u00df", "vom", "Le\u00b7bens\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir nichts geblieben ist, als die geleerte Schale.", "tokens": ["Dir", "nichts", "ge\u00b7blie\u00b7ben", "ist", ",", "als", "die", "ge\u00b7leer\u00b7te", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVPP", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}