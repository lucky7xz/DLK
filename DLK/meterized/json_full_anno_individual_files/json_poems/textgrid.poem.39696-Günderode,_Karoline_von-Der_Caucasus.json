{"textgrid.poem.39696": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Der Caucasus", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir zu H\u00e4upten Wolken wandeln,", "tokens": ["Mir", "zu", "H\u00e4up\u00b7ten", "Wol\u00b7ken", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zur Seite Luft verwehet,", "tokens": ["Mir", "zur", "Sei\u00b7te", "Luft", "ver\u00b7we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wellen mir den Fu\u00df umspielen,", "tokens": ["Wel\u00b7len", "mir", "den", "Fu\u00df", "um\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Th\u00fcrmen sich und brausen, sinken. \u2013", "tokens": ["Th\u00fcr\u00b7men", "sich", "und", "brau\u00b7sen", ",", "sin\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "PRF", "KON", "VVFIN", "$,", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Schl\u00e4fe, Jahr' umgauklen,", "tokens": ["Mei\u00b7ne", "Schl\u00e4\u00b7fe", ",", "Jahr'", "um\u00b7gau\u00b7klen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sommer, Fr\u00fchling, Winter kamen,", "tokens": ["Som\u00b7mer", ",", "F