{"textgrid.poem.31253": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weil die sch\u00f6nen Sommer-Wochen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil die sch\u00f6nen Sommer-Wochen", "tokens": ["Weil", "die", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7Wo\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wihder\u00fcmb herf\u00fcr gekrochen/", "tokens": ["wih\u00b7de\u00b7r\u00fcmb", "her\u00b7f\u00fcr", "ge\u00b7kro\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "b\u00fcn ich gleichsahm wie entz\u00fckkt/", "tokens": ["b\u00fcn", "ich", "gleich\u00b7sahm", "wie", "ent\u00b7z\u00fckkt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "KOKOM", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn betaut von P\u00e4rlen-Kr\u00e4ntzen", "tokens": ["wenn", "be\u00b7taut", "von", "P\u00e4r\u00b7len\u00b7Kr\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "der saffirnen Felder Gl\u00e4ntzen", "tokens": ["der", "saf\u00b7fir\u00b7nen", "Fel\u00b7der", "Gl\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "fast mich au\u00df mir s\u00e4lbst entr\u00fckkt!", "tokens": ["fast", "mich", "au\u00df", "mir", "s\u00e4lbst", "ent\u00b7r\u00fckkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Morgends schon in aller Fr\u00fche", "tokens": ["Mor\u00b7gends", "schon", "in", "al\u00b7ler", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dreibt ", "tokens": ["dreibt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "und sie h\u00e4ngen ihre Eutter", "tokens": ["und", "sie", "h\u00e4n\u00b7gen", "ih\u00b7re", "Eut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bluhmen-k\u00e4uend in die Kreutter/", "tokens": ["Bluh\u00b7men\u00b7k\u00e4u\u00b7end", "in", "die", "Kreut\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "blanck vor\u00fcber rauscht der Bach.", "tokens": ["blanck", "vor\u00b7\u00fc\u00b7ber", "rauscht", "der", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwischen gr\u00fcnen Amaranthen/", "tokens": ["Zwi\u00b7schen", "gr\u00fc\u00b7nen", "A\u00b7ma\u00b7ran\u00b7then", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zwischen feisten Alakanten/", "tokens": ["zwi\u00b7schen", "feis\u00b7ten", "A\u00b7la\u00b7kan\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo die dikksten Spindeln stehn/", "tokens": ["wo", "die", "dikks\u00b7ten", "Spin\u00b7deln", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "unter Wolcken-hohen Eichen/", "tokens": ["un\u00b7ter", "Wol\u00b7cken\u00b7ho\u00b7hen", "Ei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die fast an die Sterne streichen/", "tokens": ["die", "fast", "an", "die", "Ster\u00b7ne", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "la\u00df uns itzt spazziren gehn!", "tokens": ["la\u00df", "uns", "itzt", "spaz\u00b7zi\u00b7ren", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo die weissen L\u00e4mmer grahsen/", "tokens": ["Wo", "die", "weis\u00b7sen", "L\u00e4m\u00b7mer", "grah\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "st\u00f6rt uns kein Trompeten-Blahsen/", "tokens": ["st\u00f6rt", "uns", "kein", "Trom\u00b7pe\u00b7ten\u00b7Blah\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Weil ihn ihre Br\u00fcste pressen/", "tokens": ["Weil", "ihn", "ih\u00b7re", "Br\u00fcs\u00b7te", "pres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hat er gantz und gar vergessen", "tokens": ["hat", "er", "gantz", "und", "gar", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "KON", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trummel-Schlag und Feld-Geschrey.", "tokens": ["Trum\u00b7mel\u00b7Schlag", "und", "Feld\u00b7Ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit Frau ", "tokens": ["Mit", "Frau"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "kr\u00e4ntzt er ihr die g\u00f6ldnen Hahre/", "tokens": ["kr\u00e4ntzt", "er", "ihr", "die", "g\u00f6ld\u00b7nen", "Hah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df das Hertz ihr lacht und springt/", "tokens": ["da\u00df", "das", "Hertz", "ihr", "lacht", "und", "springt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bi\u00df sie wie zwey Reben-Trauben", "tokens": ["bi\u00df", "sie", "wie", "zwey", "Re\u00b7ben\u00b7Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "zahrt sich durcheinander schrauben/", "tokens": ["zahrt", "sich", "durch\u00b7ein\u00b7an\u00b7der", "schrau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "worzu s\u00fc\u00df ein Vogel singt.", "tokens": ["wor\u00b7zu", "s\u00fc\u00df", "ein", "Vo\u00b7gel", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sih au\u00df spihlenden Opalen", "tokens": ["Sih", "au\u00df", "spih\u00b7len\u00b7den", "O\u00b7pa\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dort den Regen-Bogen strahlen \u2013", "tokens": ["dort", "den", "Re\u00b7gen\u00b7Bo\u00b7gen", "strah\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "fort die Grillen-F\u00e4ngerey!", "tokens": ["fort", "die", "Gril\u00b7len\u00b7F\u00e4n\u00b7ge\u00b7rey", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hindter unbeschnittnen Myrthen", "tokens": ["Hind\u00b7ter", "un\u00b7be\u00b7schnitt\u00b7nen", "Myr\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "blahsen durchau\u00df ferne Hirten/", "tokens": ["blah\u00b7sen", "durch\u00b7au\u00df", "fer\u00b7ne", "Hir\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "niemand/ der nicht fr\u00f6lig sey!", "tokens": ["nie\u00b7mand", "/", "der", "nicht", "fr\u00f6\u00b7lig", "sey", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "ART", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Itzt mit D\u00fcnnen/ wie mit Dikken/", "tokens": ["Itzt", "mit", "D\u00fcn\u00b7nen", "/", "wie", "mit", "Dik\u00b7ken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$(", "KOKOM", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schl\u00e4gt sich mancher in die Wikken/", "tokens": ["schl\u00e4gt", "sich", "man\u00b7cher", "in", "die", "Wik\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "oder auch ins Rokken-Feld;", "tokens": ["o\u00b7der", "auch", "ins", "Rok\u00b7ken\u00b7Feld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00fcberall auff allen Trifften", "tokens": ["\u00fc\u00b7be\u00b7rall", "auff", "al\u00b7len", "Triff\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "h\u00f6rt man nichts wie Heyrath stifften/", "tokens": ["h\u00f6rt", "man", "nichts", "wie", "Hey\u00b7rath", "stiff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "KOKOM", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "klar-blau blizzt das G\u00f6tter-Zelt.", "tokens": ["kla\u00b7rblau", "blizzt", "das", "G\u00f6t\u00b7ter\u00b7Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dort in jene k\u00fchle Grotte", "tokens": ["Dort", "in", "je\u00b7ne", "k\u00fch\u00b7le", "Grot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schlipffte eben Marmelotte/", "tokens": ["schlipff\u00b7te", "e\u00b7ben", "Mar\u00b7me\u00b7lot\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sollte drin nicht Strephon seyn?", "tokens": ["soll\u00b7te", "drin", "nicht", "Stre\u00b7phon", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df die Frevel-M\u00e4uler schwezzen!", "tokens": ["La\u00df", "die", "Fre\u00b7vel\u00b7M\u00e4u\u00b7ler", "schwez\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn sie sich nur r\u00e4cht ergezzen!", "tokens": ["Wenn", "sie", "sich", "nur", "r\u00e4cht", "er\u00b7gez\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer wird gleich Lamento schreyn?", "tokens": ["Wer", "wird", "gleich", "La\u00b7men\u00b7to", "schreyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NE", "VAINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Au\u00df verguldeten Narzissen/", "tokens": ["Au\u00df", "ver\u00b7gu