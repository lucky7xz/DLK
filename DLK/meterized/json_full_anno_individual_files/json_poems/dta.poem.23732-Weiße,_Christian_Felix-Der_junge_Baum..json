{"dta.poem.23732": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der junge Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Das liebe kleine B\u00e4umchen hier,", "tokens": ["Das", "lie\u00b7be", "klei\u00b7ne", "B\u00e4um\u00b7chen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das einst gepflanzet ward mit mir,", "tokens": ["Das", "einst", "ge\u00b7pflan\u00b7zet", "ward", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tr\u00e4gt schon so jung und zart", "tokens": ["Tr\u00e4gt", "schon", "so", "jung", "und", "zart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Fr\u00fcchte von der besten Art.", "tokens": ["Die", "Fr\u00fcch\u00b7te", "von", "der", "bes\u00b7ten", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es lohnt des G\u00e4rtners froher Hand,", "tokens": ["Es", "lohnt", "des", "G\u00e4rt\u00b7ners", "fro\u00b7her", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Flei\u00df, den er darauf verwandt:", "tokens": ["Den", "Flei\u00df", ",", "den", "er", "da\u00b7rauf", "ver\u00b7wandt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wird, ihn zu erfreun,", "tokens": ["Was", "wird", ",", "ihn", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es nicht erst einst erwachsen seyn!", "tokens": ["Es", "nicht", "erst", "einst", "er\u00b7wach\u00b7sen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O bin ich nicht dem B\u00e4umchen gleich?", "tokens": ["O", "bin", "ich", "nicht", "dem", "B\u00e4um\u00b7chen", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar itzt nur noch an Hoffnung reich.", "tokens": ["Zwar", "itzt", "nur", "noch", "an", "Hoff\u00b7nung", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch will ich nicht nur bl\u00fchn,", "tokens": ["Doch", "will", "ich", "nicht", "nur", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nein, einst von goldnen Fr\u00fcchten gl\u00fchn.", "tokens": ["Nein", ",", "einst", "von", "gold\u00b7nen", "Fr\u00fcch\u00b7ten", "gl\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}