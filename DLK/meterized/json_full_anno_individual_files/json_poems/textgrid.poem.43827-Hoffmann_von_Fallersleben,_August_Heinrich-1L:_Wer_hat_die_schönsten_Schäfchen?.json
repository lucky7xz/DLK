{"textgrid.poem.43827": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer hat die sch\u00f6nsten Sch\u00e4fchen?", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat die sch\u00f6nsten Sch\u00e4fchen?", "tokens": ["Wer", "hat", "die", "sch\u00f6ns\u00b7ten", "Sch\u00e4f\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hat der goldne Mond,", "tokens": ["Die", "hat", "der", "gold\u00b7ne", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hinter unsern B\u00e4umen", "tokens": ["Der", "hin\u00b7ter", "un\u00b7sern", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Himmel dr\u00fcben wohnt.", "tokens": ["Am", "Him\u00b7mel", "dr\u00fc\u00b7ben", "wohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er kommt am sp\u00e4ten Abend,", "tokens": ["Er", "kommt", "am", "sp\u00e4\u00b7ten", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann Alles schlafen will,", "tokens": ["Wann", "Al\u00b7les", "schla\u00b7fen", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hervor aus seinem Hause", "tokens": ["Her\u00b7vor", "aus", "sei\u00b7nem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Himmel leis' und still.", "tokens": ["Zum", "Him\u00b7mel", "leis'", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann weidet er die Sch\u00e4fchen", "tokens": ["Dann", "wei\u00b7det", "er", "die", "Sch\u00e4f\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf seiner blauen Flur;", "tokens": ["Auf", "sei\u00b7ner", "blau\u00b7en", "Flur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn all die wei\u00dfen Sterne", "tokens": ["Denn", "all", "die", "wei\u00b7\u00dfen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind seine Sch\u00e4fchen nur.", "tokens": ["Sind", "sei\u00b7ne", "Sch\u00e4f\u00b7chen", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie thun sich nichts zu Leide,", "tokens": ["Sie", "thun", "sich", "nichts", "zu", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat eins das andre gern,", "tokens": ["Hat", "eins", "das", "and\u00b7re", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Schwestern sind und Br\u00fcder", "tokens": ["Und", "Schwes\u00b7tern", "sind", "und", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da droben Stern an Stern.", "tokens": ["Da", "dro\u00b7ben", "Stern", "an", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und soll ich dir eins bringen,", "tokens": ["Und", "soll", "ich", "dir", "eins", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So darfst du niemals schrei'n,", "tokens": ["So", "darfst", "du", "nie\u00b7mals", "schrei'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00dft freundlich wie die Sch\u00e4fchen", "tokens": ["Mu\u00dft", "freund\u00b7lich", "wie", "die", "Sch\u00e4f\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wie ihr Sch\u00e4fer sein!", "tokens": ["Und", "wie", "ihr", "Sch\u00e4\u00b7fer", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NE", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"li