{"textgrid.poem.55116": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein s\u00fc\u00dfes Liebchen! Hier in Schachtelw\u00e4nden", "tokens": ["Mein", "s\u00fc\u00b7\u00dfes", "Lieb\u00b7chen", "!", "Hier", "in", "Schach\u00b7tel\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gar mannigfalt geformte S\u00fc\u00dfigkeiten.", "tokens": ["Gar", "man\u00b7nig\u00b7falt", "ge\u00b7form\u00b7te", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Fr\u00fcchte sind es heil'ger Weihnachtszeiten,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "sind", "es", "heil'\u00b7ger", "Weih\u00b7nachts\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gebackne nur, den Kindern auszuspenden!", "tokens": ["Ge\u00b7back\u00b7ne", "nur", ",", "den", "Kin\u00b7dern", "aus\u00b7zu\u00b7spen\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dir m\u00f6cht ich dann mit s\u00fc\u00dfem Redewenden", "tokens": ["Dir", "m\u00f6cht", "ich", "dann", "mit", "s\u00fc\u00b7\u00dfem", "Re\u00b7de\u00b7wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Poetisch Zuckerbrot zum Fest bereiten;", "tokens": ["Poe\u00b7tisch", "Zu\u00b7cker\u00b7brot", "zum", "Fest", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Allein was soll's mit solchen Eitelkeiten?", "tokens": ["Al\u00b7lein", "was", "soll's", "mit", "sol\u00b7chen", "Ei\u00b7tel\u00b7kei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PIS", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weg den Versuch, mit Schmeichelei zu blenden!", "tokens": ["Weg", "den", "Ver\u00b7such", ",", "mit", "Schmei\u00b7che\u00b7lei", "zu", "blen\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Doch gibt es noch ein S\u00fc\u00dfes, das vom Innern", "tokens": ["Doch", "gibt", "es", "noch", "ein", "S\u00fc\u00b7\u00dfes", ",", "das", "vom", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Innern spricht, genie\u00dfbar in der Ferne,", "tokens": ["Zum", "In\u00b7nern", "spricht", ",", "ge\u00b7nie\u00df\u00b7bar", "in", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das kann nur bis zu dir hin\u00fcberwehen.", "tokens": ["Das", "kann", "nur", "bis", "zu", "dir", "hin\u00b7\u00fc\u00b7berw\u00b7e\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und f\u00fchlst du dann ein freundliches Erinnern,", "tokens": ["Und", "f\u00fchlst", "du", "dann", "ein", "freund\u00b7li\u00b7ches", "E\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als blinkten froh dir wohlbekannte Sterne,", "tokens": ["Als", "blink\u00b7ten", "froh", "dir", "wohl\u00b7be\u00b7kann\u00b7te", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADJD", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wirst du die kleinste Gabe nicht verschm\u00e4hen.", "tokens": ["Wirst", "du", "die", "kleins\u00b7te", "Ga\u00b7be", "nicht", "ver\u00b7schm\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mein s\u00fc\u00dfes Liebchen! Hier in Schachtelw\u00e4nden", "tokens": ["Mein", "s\u00fc\u00b7\u00dfes", "Lieb\u00b7chen", "!", "Hier", "in", "Schach\u00b7tel\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text"