{"textgrid.poem.38833": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: In inn'ger Lieb' war ich mit diesem Kinde,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In inn'ger Lieb' war ich mit diesem Kinde,", "tokens": ["In", "inn'\u00b7ger", "Lieb'", "war", "ich", "mit", "die\u00b7sem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihm gelang, in s\u00fc\u00dfen Himmels-Weisen", "tokens": ["Und", "ihm", "ge\u00b7lang", ",", "in", "s\u00fc\u00b7\u00dfen", "Him\u00b7mels\u00b7Wei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Mutter Gottes wunderhold zu preisen,", "tokens": ["Die", "Mut\u00b7ter", "Got\u00b7tes", "wun\u00b7der\u00b7hold", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aller Herzen r\u00fchrt sein Geist gelinde.", "tokens": ["Und", "al\u00b7ler", "Her\u00b7zen", "r\u00fchrt", "sein", "Geist", "ge\u00b7lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da l\u00f6sten sie in Wehmuth ihre S\u00fcnde,", "tokens": ["Da", "l\u00f6s\u00b7ten", "sie", "in", "Weh\u00b7muth", "ih\u00b7re", "S\u00fcn\u00b7de", ","], "