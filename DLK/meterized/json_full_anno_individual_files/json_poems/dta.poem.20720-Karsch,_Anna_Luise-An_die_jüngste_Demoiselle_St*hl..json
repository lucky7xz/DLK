{"dta.poem.20720": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die j\u00fcngste Demoiselle St*hl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Du von eines Thronensitzers Mund gek\u00fc\u00dft,               ", "tokens": ["Die", "Du", "von", "ei\u00b7nes", "Thro\u00b7nen\u00b7sit\u00b7zers", "Mund", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch so bescheiden bliebst, so liebreich wie Du bist,", "tokens": ["Noch", "so", "be\u00b7schei\u00b7den", "bliebst", ",", "so", "lieb\u00b7reich", "wie", "Du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Freundin! die sich nicht ver\u00e4ndern,", "tokens": ["O", "Freun\u00b7din", "!", "die", "sich", "nicht", "ver\u00b7\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PRELS", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht stolzer machen lie\u00dfe, weil", "tokens": ["Nicht", "stol\u00b7zer", "ma\u00b7chen", "lie\u00b7\u00dfe", ",", "weil"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADJD", "VVINF", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein gro\u00dfer Herr von vielen L\u00e4ndern", "tokens": ["Ein", "gro\u00b7\u00dfer", "Herr", "von", "vie\u00b7len", "L\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich liebte, Dir von seinem Glanz den halben Theil", "tokens": ["Dich", "lieb\u00b7te", ",", "Dir", "von", "sei\u00b7nem", "Glanz", "den", "hal\u00b7ben", "Theil"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und seinen ganzen Namen g\u00e4be,", "tokens": ["Und", "sei\u00b7nen", "gan\u00b7zen", "Na\u00b7men", "g\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schw\u00fcre, da\u00df er Dir nur zu gefallen lebe,", "tokens": ["Und", "schw\u00fc\u00b7re", ",", "da\u00df", "er", "Dir", "nur", "zu", "ge\u00b7fal\u00b7len", "le\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gef\u00fchrt am starken Liebesseil,", "tokens": ["Ge\u00b7f\u00fchrt", "am", "star\u00b7ken", "Lie\u00b7bes\u00b7seil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sprich: ob das m\u00f6glich ist, was uns Dein Vater saget,", "tokens": ["Sprich", ":", "ob", "das", "m\u00f6g\u00b7lich", "ist", ",", "was", "uns", "Dein", "Va\u00b7ter", "sa\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "KOUS", "ART", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df einen sonst ganz sanft geschaffnen K\u00f6nigssohn", "tokens": ["Da\u00df", "ei\u00b7nen", "sonst", "ganz", "sanft", "ge\u00b7schaff\u00b7nen", "K\u00f6\u00b7nigs\u00b7sohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Herrschsucht und der Geiz nach weitern Gr\u00e4nzen", "tokens": ["Die", "Herrschsucht", "und", "der", "Geiz", "nach", "wei\u00b7tern", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "naget,", "tokens": ["na\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "So bald er auf dem goldnen Thron", "tokens": ["So", "bald", "er", "auf", "dem", "gold\u00b7nen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Schwindel bringende zu steile H\u00f6h bestiegen,", "tokens": ["Die", "Schwin\u00b7del", "brin\u00b7gen\u00b7de", "zu", "stei\u00b7le", "H\u00f6h", "be\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Alsdann vereinet sich die Ehre mit der Macht,", "tokens": ["Als\u00b7dann", "ver\u00b7ei\u00b7net", "sich", "die", "Eh\u00b7re", "mit", "der", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der junge Herrscher w\u00fcnscht den Nachbar zu", "tokens": ["Der", "jun\u00b7ge", "Herr\u00b7scher", "w\u00fcnscht", "den", "Nach\u00b7bar", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "bekriegen,", "tokens": ["be\u00b7krie\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Am Tage denket er den Plan zu Feldesz\u00fcgen", "tokens": ["Am", "Ta\u00b7ge", "den\u00b7ket", "er", "den", "Plan", "zu", "Fel\u00b7des\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und tr\u00e4umet vor dem Zug die Schlacht,", "tokens": ["Und", "tr\u00e4u\u00b7met", "vor", "dem", "Zug", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wirbt J\u00fcnglinge zu seinen Fahnen,", "tokens": ["Wirbt", "J\u00fcng\u00b7lin\u00b7ge", "zu", "sei\u00b7nen", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.22": {"text": "Legt Schatzung auf die Unterthanen,", "tokens": ["Legt", "Schat\u00b7zung", "auf", "die", "Un\u00b7ter\u00b7tha\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und glaubet keinen Gott als sich,", "tokens": ["Und", "glau\u00b7bet", "kei\u00b7nen", "Gott", "als", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "KOUS", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Macht durch Befehl und schnelle Strafen", "tokens": ["Macht", "durch", "Be\u00b7fehl", "und", "schnel\u00b7le", "Stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Sein Ansehn drohend f\u00fcrchterlich,", "tokens": ["Sein", "An\u00b7sehn", "dro\u00b7hend", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und wird doch niemals froh wie Einer bei den", "tokens": ["Und", "wird", "doch", "nie\u00b7mals", "froh", "wie", "Ei\u00b7ner", "bei", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "PIS", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Schafen", "tokens": ["Scha\u00b7fen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Und Ziegen seines Landes lebt,", "tokens": ["Und", "Zie\u00b7gen", "sei\u00b7nes", "Lan\u00b7des", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der nie nach gr\u00f6\u00dferm Raum des kleinen Ackers strebt,", "tokens": ["Der", "nie", "nach", "gr\u00f6\u00b7\u00dferm", "Raum", "des", "klei\u00b7nen", "A\u00b7ckers", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zufrieden ist mit seinem Garten", "tokens": ["Zu\u00b7frie\u00b7den", "ist", "mit", "sei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und von dem unverschnittnen hohen Apfelbaum", "tokens": ["Und", "von", "dem", "un\u00b7ver\u00b7schnitt\u00b7nen", "ho\u00b7hen", "Ap\u00b7fel\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die reifen Fr\u00fcchte bricht und nicht des Weines", "tokens": ["Die", "rei\u00b7fen", "Fr\u00fcch\u00b7te", "bricht", "und", "nicht", "des", "Wei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Arten", "tokens": ["Ar\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.34": {"text": "Zu unterscheiden wei\u00df wie F\u00fcrsten mit dem zarten", "tokens": ["Zu", "un\u00b7ter\u00b7schei\u00b7den", "wei\u00df", "wie", "F\u00fcrs\u00b7ten", "mit", "dem", "zar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "KOKOM", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Zur Kitzelung gew\u00f6hnten Gaum.", "tokens": ["Zur", "Kit\u00b7ze\u00b7lung", "ge\u00b7w\u00f6hn\u00b7ten", "Gaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "O Freundin, als die Welt noch keine F\u00fcrsten", "tokens": ["O", "Freun\u00b7din", ",", "als", "die", "Welt", "noch", "kei\u00b7ne", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "brauchte,", "tokens": ["brauch\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Als noch der Fr\u00fchlingswind in keine Blumen hauchte,", "tokens": ["Als", "noch", "der", "Fr\u00fch\u00b7lings\u00b7wind", "in", "kei\u00b7ne", "Blu\u00b7men", "hauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die ein gedingter Arm begossen und gepflanzt,", "tokens": ["Die", "ein", "ge\u00b7ding\u00b7ter", "Arm", "be\u00b7gos\u00b7sen", "und", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und noch die Menschen vor einander", "tokens": ["Und", "noch", "die", "Men\u00b7schen", "vor", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Sich nicht gef\u00fcrchtet und verschanzt,", "tokens": ["Sich", "nicht", "ge\u00b7f\u00fcrch\u00b7tet", "und", "ver\u00b7schanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Da machte noch kein Alexander", "tokens": ["Da", "mach\u00b7te", "noch", "kein", "A\u00b7lex\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Sich selbst zum Gott, und kein Geschrei", "tokens": ["Sich", "selbst", "zum", "Gott", ",", "und", "kein", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "APPRART", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Des Treffens ri\u00df die Wolken von einander,", "tokens": ["Des", "Tref\u00b7fens", "ri\u00df", "die", "Wol\u00b7ken", "von", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Da war ein Sterblicher dem andern noch getreu,", "tokens": ["Da", "war", "ein", "Sterb\u00b7li\u00b7cher", "dem", "an\u00b7dern", "noch", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die M\u00e4nner winkten sich am Pfluge Freundschaft zu,", "tokens": ["Die", "M\u00e4n\u00b7ner", "wink\u00b7ten", "sich", "am", "Pflu\u00b7ge", "Freund\u00b7schaft", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die M\u00e4dchen waren Sch\u00e4ferinnen,", "tokens": ["Die", "M\u00e4d\u00b7chen", "wa\u00b7ren", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Und sangen auf der Trift und wu\u00dften so wie Du", "tokens": ["Und", "san\u00b7gen", "auf", "der", "Trift", "und", "wu\u00df\u00b7ten", "so", "wie", "Du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ADV", "KOKOM", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Durch s\u00fc\u00dfen Blick die Herzen zu gewinnen.", "tokens": ["Durch", "s\u00fc\u00b7\u00dfen", "Blick", "die", "Her\u00b7zen", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}