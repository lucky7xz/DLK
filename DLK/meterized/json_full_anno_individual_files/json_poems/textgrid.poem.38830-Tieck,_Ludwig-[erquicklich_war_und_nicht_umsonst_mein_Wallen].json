{"textgrid.poem.38830": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[erquicklich war und nicht umsonst mein Wallen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erquicklich war und nicht umsonst mein Wallen,", "tokens": ["Er\u00b7quick\u00b7lich", "war", "und", "nicht", "um\u00b7sonst", "mein", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Maria, Mutter, Sohn und ewge Liebe,", "tokens": ["Ma\u00b7ria", ",", "Mut\u00b7ter", ",", "Sohn", "und", "ew\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich kann in T\u00f6nen sagen wie ich liebe,", "tokens": ["Ich", "kann", "in", "T\u00f6\u00b7nen", "sa\u00b7gen", "wie", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sch\u00f6nen Weisen soll mein Preisen schallen.", "tokens": ["Ich", "sch\u00f6\u00b7nen", "Wei\u00b7sen", "soll", "mein", "Prei\u00b7sen", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bist, Jesus, du vergessen denn von allen?", "tokens": ["Bist", ",", "Je\u00b7sus", ",", "du", "ver\u00b7ges\u00b