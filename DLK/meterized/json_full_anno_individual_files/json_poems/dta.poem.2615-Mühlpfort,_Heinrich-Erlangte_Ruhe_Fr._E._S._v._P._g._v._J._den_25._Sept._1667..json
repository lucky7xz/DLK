{"dta.poem.2615": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erlangte Ruhe  \n  Fr. E. S. v. P. g. v. J. den 25. Sept.  1667.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr die ihr auff der See des Kummer-reichen Lebens", "tokens": ["Ihr", "die", "ihr", "auff", "der", "See", "des", "Kum\u00b7mer\u00b7rei\u00b7chen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPOSAT", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit vollen Segeln laufft und neue Hafen sucht/", "tokens": ["Mit", "vol\u00b7len", "Se\u00b7geln", "laufft", "und", "neu\u00b7e", "Ha\u00b7fen", "sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ihr der Hoffnungs Haupt umbschweiffet gantz verge-", "tokens": ["Die", "ihr", "der", "Hoff\u00b7nungs", "Haupt", "umbsc\u00b7hweif\u00b7fet", "gantz", "ver\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und den Genaden-Port verliehrt bey solcher Flucht/", "tokens": ["Und", "den", "Ge\u00b7na\u00b7den\u00b7Port", "ver\u00b7liehrt", "bey", "sol\u00b7cher", "Flucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was meint ihr? Woll\u2019t ihr Ruh in dieser Unruh finden?", "tokens": ["Was", "meint", "ihr", "?", "Woll't", "ihr", "Ruh", "in", "die\u00b7ser", "Un\u00b7ruh", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NE", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da euer Fleisch und Blut stets neuen Auffstand macht/", "tokens": ["Da", "eu\u00b7er", "Fleisch", "und", "Blut", "stets", "neu\u00b7en", "Auffs\u00b7tand", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da die Begierden sich als wie ein Feur entz\u00fcnden/", "tokens": ["Da", "die", "Be\u00b7gier\u00b7den", "sich", "als", "wie", "ein", "Feur", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und mancher Donnerschlag\u2019 in dem Gem\u00fct\u2019 erkracht.", "tokens": ["Und", "man\u00b7cher", "Don\u00b7ner\u00b7schlag'", "in", "dem", "Ge\u00b7m\u00fct'", "er\u00b7kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Elende Sterblichen! was eure Ruh zerst\u00f6ret", "tokens": ["E\u00b7len\u00b7de", "Sterb\u00b7li\u00b7chen", "!", "was", "eu\u00b7re", "Ruh", "zer\u00b7st\u00f6\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PWS", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Und mas meineydig offt des Geistes Frieden bricht;", "tokens": ["Und", "mas", "mei\u00b7ney\u00b7dig", "offt", "des", "Geis\u00b7tes", "Frie\u00b7den", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das sind die Regungen/ die man liebkosend\u2019 h\u00f6ret/", "tokens": ["Das", "sind", "die", "Re\u00b7gun\u00b7gen", "/", "die", "man", "lieb\u00b7ko\u00b7send'", "h\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "PRELS", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ja die der Selbst-Betrug mit Biesam zugericht.", "tokens": ["Ja", "die", "der", "Selbst\u00b7Be\u00b7trug", "mit", "Bie\u00b7sam", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELS", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So bald nun der Geruch euch ins Gehirne steiget/", "tokens": ["So", "bald", "nun", "der", "Ge\u00b7ruch", "euch", "ins", "Ge\u00b7hir\u00b7ne", "stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nimt die verborgne Gifft Vernunfft und Sinnen ein/", "tokens": ["Nimt", "die", "ver\u00b7borg\u00b7ne", "Gifft", "Ver\u00b7nunfft", "und", "Sin\u00b7nen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn ists nicht wunderns werth da\u00df ihr euch rasend zeiget/", "tokens": ["Denn", "ists", "nicht", "wun\u00b7derns", "werth", "da\u00df", "ihr", "euch", "ra\u00b7send", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "ADJD", "KOUS", "PPER", "PPER", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Von Aberwitz verkehrt/ verblendt durch falschen Schein.", "tokens": ["Von", "A\u00b7ber\u00b7witz", "ver\u00b7kehrt", "/", "ver\u00b7blendt", "durch", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Man \u00fcberlege nur das Endziel unsrer Wercke/", "tokens": ["Man", "\u00fc\u00b7berl\u00b7e\u00b7ge", "nur", "das", "End\u00b7ziel", "uns\u00b7rer", "Wer\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der wenigste lebt so/ da\u00df er ihm selber lebt.", "tokens": ["Der", "we\u00b7nigs\u00b7te", "lebt", "so", "/", "da\u00df", "er", "ihm", "sel\u00b7ber", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$(", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Indessen f\u00e4llt dahin der Jahre Kern und St\u00e4rcke/", "tokens": ["In\u00b7des\u00b7sen", "f\u00e4llt", "da\u00b7hin", "der", "Jah\u00b7re", "Kern", "und", "St\u00e4r\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Indem der arme Mensch sich lebendig begr\u00e4bt.", "tokens": ["In\u00b7dem", "der", "ar\u00b7me", "Mensch", "sich", "le\u00b7ben\u00b7dig", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Wir w\u00e4ren wol geschickt auch Ruh\u2019 allhier zu haben/", "tokens": ["Wir", "w\u00e4\u00b7ren", "wol", "ge\u00b7schickt", "auch", "Ruh'", "all\u00b7hier", "zu", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "ADV", "NN", "ADV", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Allein der spr\u00f6de Geist der wil und mag nicht ruhn/", "tokens": ["Al\u00b7lein", "der", "spr\u00f6\u00b7de", "Geist", "der", "wil", "und", "mag", "nicht", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "VMFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bald heist ihn Neid und Geitz/ bald Zorn und Hochmuth traben/", "tokens": ["Bald", "heist", "ihn", "Neid", "und", "Geitz", "/", "bald", "Zorn", "und", "Hoch\u00b7muth", "tra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$(", "ADV", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was nur die Reitzung schafft das mu\u00df er willig thun.", "tokens": ["Was", "nur", "die", "Reit\u00b7zung", "schafft", "das", "mu\u00df", "er", "wil\u00b7lig", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denn zancken wir uns selbst in unsrem eignen Hertzen/", "tokens": ["Denn", "zan\u00b7cken", "wir", "uns", "selbst", "in", "uns\u00b7rem", "eig\u00b7nen", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verdammen alle Rast/ und lieben steten Streit/", "tokens": ["Ver\u00b7dam\u00b7men", "al\u00b7le", "Rast", "/", "und", "lie\u00b7ben", "ste\u00b7ten", "Streit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$(", "KON", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Gedencken nicht einmal welch Gut wir uns verschertzen/", "tokens": ["Ge\u00b7den\u00b7cken", "nicht", "ein\u00b7mal", "welch", "Gut", "wir", "uns", "ver\u00b7schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PWAT", "NN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie weit wir irre gehn vom Weg der Ewigkeit.", "tokens": ["Wie", "weit", "wir", "ir\u00b7re", "gehn", "vom", "Weg", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So ruht der innre Mensch. Ob schon des Gl\u00fcckes Sonne", "tokens": ["So", "ruht", "der", "inn\u00b7re", "Mensch", ".", "Ob", "schon", "des", "Gl\u00fc\u00b7ckes", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Von aussen nichts als Gold umb seine Scheitel flicht/", "tokens": ["Von", "aus\u00b7sen", "nichts", "als", "Gold", "umb", "sei\u00b7ne", "Schei\u00b7tel", "flicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIS", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So qu\u00e4lt ihn Angst und Furcht bey h\u00f6chster Lust und Wonne", "tokens": ["So", "qu\u00e4lt", "ihn", "Angst", "und", "Furcht", "bey", "h\u00f6chs\u00b7ter", "Lust", "und", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wird arm durch Uberflu\u00df und kennt sich selbsten nicht.", "tokens": ["Wird", "arm", "durch", "U\u00b7ber\u00b7flu\u00df", "und", "kennt", "sich", "selbs\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "KON", "VVFIN", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zu dem wohnt schlechte Ruh in pr\u00e4chtigen Pall\u00e4sten/", "tokens": ["Zu", "dem", "wohnt", "schlech\u00b7te", "Ruh", "in", "pr\u00e4ch\u00b7ti\u00b7gen", "Pal\u00b7l\u00e4s\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wer grosse Sorgen f\u00fchrt schl\u00e4fft selten allzuwol.", "tokens": ["Wer", "gros\u00b7se", "Sor\u00b7gen", "f\u00fchrt", "schl\u00e4fft", "sel\u00b7ten", "all\u00b7zu\u00b7wol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie offt f\u00e4llt heute der/ so gestern stund am festen/", "tokens": ["Wie", "offt", "f\u00e4llt", "heu\u00b7te", "der", "/", "so", "ge\u00b7stern", "stund", "am", "fes\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "$(", "ADV", "ADV", "ADJD", "APPRART", "ADJA", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Und mancher wird gedr\u00fcckteh\u2019 als er steigen sol.", "tokens": ["Und", "man\u00b7cher", "wird", "ge\u00b7dr\u00fcck\u00b7teh'", "als", "er", "stei\u00b7gen", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "NE", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Noch th\u00f6richter sind die/ so bey der Wollustrasten/", "tokens": ["Noch", "th\u00f6\u00b7rich\u00b7ter", "sind", "die", "/", "so", "bey", "der", "Wol\u00b7lust\u00b7ras\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "$(", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.38": {"text": "Und richten ihnen selbst ein Bett\u2019 von Dornen zu/", "tokens": ["Und", "rich\u00b7ten", "ih\u00b7nen", "selbst", "ein", "Bett'", "von", "Dor\u00b7nen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sind M\u00f6rder ihrer Zeit/ die sie so sehr nicht hasten/", "tokens": ["Sind", "M\u00f6r\u00b7der", "ih\u00b7rer", "Zeit", "/", "die", "sie", "so", "sehr", "nicht", "has\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn ihr Gem\u00fcthe d\u00e4cht\u2019 auff ungekr\u00e4nckte Ruh.", "tokens": ["Wenn", "ihr", "Ge\u00b7m\u00fc\u00b7the", "d\u00e4cht'", "auff", "un\u00b7ge\u00b7kr\u00e4nck\u00b7te", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Mit kurtzem/ was wir hier geniessen/ das betr\u00fcbet/", "tokens": ["Mit", "kurt\u00b7zem", "/", "was", "wir", "hier", "ge\u00b7nies