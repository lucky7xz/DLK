{"textgrid.poem.49536": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df es uns niemand recht macht, ist gemeiniglich", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df es uns niemand recht macht, ist gemeiniglich", "tokens": ["Da\u00df", "es", "uns", "nie\u00b7mand", "recht", "macht", ",", "ist", "ge\u00b7mei\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PIS", "ADJD", "VVFIN", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht andrer Leute, sondern unsre eigne Schuld.", "tokens": ["Nicht", "an\u00b7drer", "Leu\u00b7te", ",", "son\u00b7dern", "uns\u00b7re", "eig\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als einsmahls Luna ihre Mutter flehete,", "tokens": ["Als", "eins\u00b7mahls", "Lu\u00b7na", "ih\u00b7re", "Mut\u00b7ter", "fle\u00b7he\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr doch ein neues, und bequemlich pa\u00dfendes", "tokens": ["Ihr", "doch", "ein", "neu\u00b7es", ",", "und", "be\u00b7quem\u00b7lich", "pa\u00b7\u00dfen\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "ADJA", "$,", "KON", "ADJD", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "Gewand zu schaffen, und bewegli