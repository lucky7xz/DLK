{"dta.poem.19623": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der sch\u00f6ne Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In k\u00fchler Tiefe spiegelt sich", "tokens": ["In", "k\u00fch\u00b7ler", "Tie\u00b7fe", "spie\u00b7gelt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Juli-Himmels warmes Blau,", "tokens": ["Des", "Ju\u00b7li\u00b7Him\u00b7mels", "war\u00b7mes", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Libellen tanzen auf der Flut,", "tokens": ["Li\u00b7bel\u00b7len", "tan\u00b7zen", "auf", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nicht der kleinste Hauch bewegt.", "tokens": ["Die", "nicht", "der", "kleins\u00b7te", "Hauch", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwei Knaben und ein ledig Boot \u2014", "tokens": ["Zwei", "Kna\u00b7ben", "und", "ein", "le\u00b7dig", "Boot"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ART", "ADJD", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sie springen jauchzend in das Bad,", "tokens": ["Sie", "sprin\u00b7gen", "jauch\u00b7zend", "in", "das", "Bad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der eine taucht gek\u00fchlt empor,", "tokens": ["Der", "ei\u00b7ne", "taucht", "ge\u00b7k\u00fchlt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der andre steigt nicht wieder auf.", "tokens": ["Der", "and\u00b7re", "steigt", "nicht", "wie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein wilder Schrei: \u201eDer Bruder sank!\u201c", "tokens": ["Ein", "wil\u00b7der", "Schrei", ":", "\u201e", "Der", "Bru\u00b7der", "sank", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Booten wimmelt's schon. Man fischt.", "tokens": ["Von", "Boo\u00b7ten", "wim\u00b7melt's", "schon", ".", "Man", "fischt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "$.", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Einen rudern sie ans Land,", "tokens": ["Den", "Ei\u00b7nen", "ru\u00b7dern", "sie", "ans", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der fahl wie ein Verbrecher sitzt.", "tokens": ["Der", "fahl", "wie", "ein", "Ver\u00b7bre\u00b7cher", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der andre Knabe sinkt und sinkt", "tokens": ["Der", "and\u00b7re", "Kna\u00b7be", "sinkt", "und", "sinkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gemach hinab, ein Schlummernder,", "tokens": ["Ge\u00b7mach", "hin\u00b7ab", ",", "ein", "Schlum\u00b7mern\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geschmiegt das sanfte Lockenhaupt", "tokens": ["Ge\u00b7schmiegt", "das", "sanf\u00b7te", "Lo\u00b7cken\u00b7haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An einer Nymphe wei\u00dfe Brust.", "tokens": ["An", "ei\u00b7ner", "Nym\u00b7phe", "wei\u00b7\u00dfe", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}