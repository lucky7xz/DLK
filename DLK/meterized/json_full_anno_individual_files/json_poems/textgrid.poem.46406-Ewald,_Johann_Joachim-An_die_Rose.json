{"textgrid.poem.46406": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An die Rose", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe dich o Rose!", "tokens": ["Ich", "lie\u00b7be", "dich", "o", "Ro\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "FM", "FM", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du K\u00f6nigin der Blumen,", "tokens": ["Du", "K\u00f6\u00b7ni\u00b7gin", "der", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mag dich gerne pfl\u00fccken;", "tokens": ["Und", "mag", "dich", "ger\u00b7ne", "pfl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch, pfl\u00fcckt' ich dich vom Stamme,", "tokens": ["Doch", ",", "pfl\u00fcckt'", "ich", "dich", "vom", "Stam\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So st\u00e4chen mich die Dornen;", "tokens": ["So", "st\u00e4\u00b7chen", "mich", "die", "Dor\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darum pfl\u00fcck' ich dich lieber", "tokens": ["Da\u00b7rum", "pfl\u00fcck", "ich", "dich", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "A