{"dta.poem.5572": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der weise Mann, den Clasomen uns weist,", "tokens": ["Der", "wei\u00b7se", "Mann", ",", "den", "Cla\u00b7so\u00b7men", "uns", "weist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erkannt auch einen h\u00f6hern Geist,", "tokens": ["Er\u00b7kannt", "auch", "ei\u00b7nen", "h\u00f6\u00b7hern", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ward daher in Griechenland", "tokens": ["Und", "ward", "da\u00b7her", "in", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von jedermann die ", "tokens": ["Von", "je\u00b7der\u00b7mann", "die"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Er f\u00fchrt der Welt Gebuhrt, Form, Ordnung, Eigenschafft,", "tokens": ["Er", "f\u00fchrt", "der", "Welt", "Ge\u00b7buhrt", ",", "Form", ",", "Ord\u00b7nung", ",", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus einer H\u00f6chsten Macht und H\u00f6chsten Weisheit her;", "tokens": ["Aus", "ei\u00b7ner", "H\u00f6chs\u00b7ten", "Macht", "und", "H\u00f6chs\u00b7ten", "Weis\u00b7heit", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht aber aus der blinden Krafft,", "tokens": ["Nicht", "a\u00b7ber", "aus", "der", "blin\u00b7den", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vom Schicksal und vom Ungefehr.", "tokens": ["Vom", "Schick\u00b7sal", "und", "vom", "Un\u00b7ge\u00b7fehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}