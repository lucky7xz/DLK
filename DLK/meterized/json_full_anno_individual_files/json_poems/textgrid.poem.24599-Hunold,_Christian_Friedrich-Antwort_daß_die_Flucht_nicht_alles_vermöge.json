{"textgrid.poem.24599": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Antwort da\u00df die Flucht nicht alles verm\u00f6ge", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar Josephs Artzeney/ den starcken Feind zu d\u00e4mpfen/", "tokens": ["Zwar", "Jo\u00b7se\u00b7phs", "Art\u00b7ze\u00b7ney", "/", "den", "star\u00b7cken", "Feind", "zu", "d\u00e4mp\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der alle Kunst gebraucht/ in Fesseln ihn zu sehn/", "tokens": ["Der", "al\u00b7le", "Kunst", "ge\u00b7braucht", "/", "in", "Fes\u00b7seln", "ihn", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$(", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War eines Theils die Flucht. Frey von der Wahlstatt gehn/", "tokens": ["War", "ei\u00b7nes", "Theils", "die", "Flucht", ".", "Frey", "von", "der", "Wahl\u00b7statt", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$.", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ja wieder ihren Strahl in Gegenwart zu k\u00e4mpfen/", "tokens": ["Ja", "wie\u00b7der", "ih\u00b7ren", "Strahl", "in", "Ge\u00b7gen\u00b7wart", "zu", "k\u00e4mp\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df war der Keuschheit Sieg. Di\u00df war der rechte Streit.", "tokens": ["Di\u00df", "war", "der", "Keuschheit", "Sieg", ".", "Di\u00df", "war", "der", "rech\u00b7te", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Hast du ein Josephs Hertz frey von der Uppigkeit/", "tokens": ["Hast", "du", "ein", "Jo\u00b7se\u00b7phs", "Hertz", "frey", "von", "der", "Up\u00b7pig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NE", "NN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "So sey ein Feind so starck/ er wird zur\u00fccke prallen/", "tokens": ["So", "sey", "ein", "Feind", "so", "starck", "/", "er", "wird", "zu\u00b7r\u00fc\u00b7cke", "pral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$(", "PPER", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und vor ein himmlisch Hertz die gr\u00f6fte Sch\u00f6nheit fallen.", "tokens": ["Und", "vor", "ein", "himm\u00b7lisch", "Hertz", "die", "gr\u00f6f\u00b7te", "Sch\u00f6n\u00b7heit", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.