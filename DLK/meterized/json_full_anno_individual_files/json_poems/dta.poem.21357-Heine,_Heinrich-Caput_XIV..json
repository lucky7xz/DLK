{"dta.poem.21357": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie pochte mein Herz, wenn die alte Frau", "tokens": ["Wie", "poch\u00b7te", "mein", "Herz", ",", "wenn", "die", "al\u00b7te", "Frau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von der K\u00f6nigstochter erz\u00e4hlte,", "tokens": ["Von", "der", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "er\u00b7z\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die einsam auf der Heide sa\u00df", "tokens": ["Die", "ein\u00b7sam", "auf", "der", "Hei\u00b7de", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die goldnen Haare str\u00e4hlte.", "tokens": ["Und", "die", "gold\u00b7nen", "Haa\u00b7re", "str\u00e4hl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die G\u00e4nse mu\u00dfte sie h\u00fcten dort", "tokens": ["Die", "G\u00e4n\u00b7se", "mu\u00df\u00b7te", "sie", "h\u00fc\u00b7ten", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als G\u00e4nsemagd, und trieb sie", "tokens": ["Als", "G\u00e4n\u00b7se\u00b7magd", ",", "und", "trieb", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Abend die G\u00e4nse wieder durch\u2019s Thor,", "tokens": ["Am", "A\u00b7bend", "die", "G\u00e4n\u00b7se", "wie\u00b7der", "durch's", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gar traurig stehen blieb sie.", "tokens": ["Gar", "trau\u00b7rig", "ste\u00b7hen", "blieb", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn angenagelt \u00fcber dem Thor", "tokens": ["Denn", "an\u00b7ge\u00b7na\u00b7gelt", "\u00fc\u00b7ber", "dem", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sah sie ein Ro\u00dfhaupt ragen,", "tokens": ["Sah", "sie", "ein", "Ro\u00df\u00b7haupt", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das war der Kopf des armen Pferds,", "tokens": ["Das", "war", "der", "Kopf", "des", "ar\u00b7men", "Pferds", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sie in die Fremde getragen.", "tokens": ["Das", "sie", "in", "die", "Frem\u00b7de", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Die K\u00f6nigstochter seufzte tief:", "tokens": ["Die", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "seufz\u00b7te", "tief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Falada, da\u00df du hangest!", "tokens": ["O", "Fa\u00b7la\u00b7da", ",", "da\u00df", "du", "han\u00b7gest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Pferdekopf herunter rief:", "tokens": ["Der", "Pfer\u00b7de\u00b7kopf", "her\u00b7un\u00b7ter", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O wehe! da\u00df du gangest!", "tokens": ["O", "we\u00b7he", "!", "da\u00df", "du", "gan\u00b7gest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}