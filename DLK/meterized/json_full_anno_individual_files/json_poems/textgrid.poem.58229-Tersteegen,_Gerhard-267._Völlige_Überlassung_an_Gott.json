{"textgrid.poem.58229": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "267. V\u00f6llige \u00dcberlassung an Gott", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein W\u00e4hlen ist geschehn, mein Wollen sinkt dahin,", "tokens": ["\u00bb", "mein", "W\u00e4h\u00b7len", "ist", "ge\u00b7schehn", ",", "mein", "Wol\u00b7len", "sinkt", "da\u00b7hin", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie du es machst, mein Herr, ich wohl vergn\u00fcget bin\u00ab,", "tokens": ["Wie", "du", "es", "machst", ",", "mein", "Herr", ",", "ich", "wohl", "ver\u00b7gn\u00fc\u00b7get", "bin", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "ADV", "VVPP", "VAFIN", "$(", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So spricht mein Seelengrund, Vernunft mag immer klagen,", "tokens": ["So", "spricht", "mein", "See\u00b7len\u00b7grund", ",", "Ver\u00b7nunft", "mag", "im\u00b7mer", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00bbherr, nur dein Will' gescheh', du darfst mich selbst nicht fragen!\u00ab", "tokens": ["\u00bb", "herr", ",", "nur", "dein", "Will'", "ge\u00b7scheh'", ",", "du", "darfst", "mich", "selbs