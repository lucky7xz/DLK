{"dta.poem.3875": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "5) Als er ihm an seinem Nahmens-Tage  \n schrieb.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Frommer Vater! da dein Nahme itzt mit die-", "tokens": ["From\u00b7mer", "Va\u00b7ter", "!", "da", "dein", "Nah\u00b7me", "itzt", "mit", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "sem Tage steigt,", "tokens": ["sem", "Ta\u00b7ge", "steigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da sich dessen froher Schimmer deinem Sohn so", "tokens": ["Da", "sich", "des\u00b7sen", "fro\u00b7her", "Schim\u00b7mer", "dei\u00b7nem", "Sohn", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PDS", "ADJA", "NN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "herrlich zeigt,", "tokens": ["herr\u00b7lich", "zeigt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "La\u00df mich deinen Tag begehn bey so heitern Son-", "tokens": ["La\u00df", "mich", "dei\u00b7nen", "Tag", "be\u00b7gehn", "bey", "so", "hei\u00b7tern", "Son"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ADV", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "nen Blicken,", "tokens": ["nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "La\u00df dir itzt den stillsten Wunsch aus dem Plei\u00df-A-", "tokens": ["La\u00df", "dir", "itzt", "den", "stills\u00b7ten", "Wunsch", "aus", "dem", "Plei\u00df\u00b7A"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "thene schicken.", "tokens": ["the\u00b7ne", "schi\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Flieht ihr Grillen, flieht ihr Sorgen, weiche Un-", "tokens": ["Flieht", "ihr", "Gril\u00b7len", ",", "flieht", "ihr", "Sor\u00b7gen", ",", "wei\u00b7che", "Un"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "muth, falscher Feind!", "tokens": ["muth", ",", "fal\u00b7scher", "Feind", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Weil an diesem hellen Lichte, meines Vaters Licht", "tokens": ["Weil", "an", "die\u00b7sem", "hel\u00b7len", "Lich\u00b7te", ",", "mei\u00b7nes", "Va\u00b7ters", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "erscheint.", "tokens": ["er\u00b7scheint", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Vater! bl\u00fch in steter Lust, Freude, Gl\u00fcck und", "tokens": ["Va\u00b7ter", "!", "bl\u00fch", "in", "ste\u00b7ter", "Lust", ",", "Freu\u00b7de", ",", "Gl\u00fcck", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "VVFIN", "APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Wohlergehen,", "tokens": ["Woh\u00b7ler\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Da\u00df wir unsrer Seuffzer Frucht mit sehr viel Ver-", "tokens": ["Da\u00df", "wir", "uns\u00b7rer", "Seuff\u00b7zer", "Frucht", "mit", "sehr", "viel", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "APPR", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "gn\u00fcgen sehen.", "tokens": ["gn\u00fc\u00b7gen", "se\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Vater! meine gantze Regung soll zu deinem", "tokens": ["Va\u00b7ter", "!", "mei\u00b7ne", "gant\u00b7ze", "Re\u00b7gung", "soll", "zu", "dei\u00b7nem"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Dienste gl\u00fchn,", "tokens": ["Diens\u00b7te", "gl\u00fchn", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Bis mich einst das sch\u00f6ne Salem wird der Sterb-", "tokens": ["Bis", "mich", "einst", "das", "sch\u00f6\u00b7ne", "Sa\u00b7lem", "wird", "der", "Ster\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NE", "VAFIN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "lichkeit entziehn.", "tokens": ["lich\u00b7keit", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Nun verdopple Zeil und Schrifft, suche balde mir", "tokens": ["Nun", "ver\u00b7dopp\u00b7le", "Zeil", "und", "Schrifft", ",", "su\u00b7che", "bal\u00b7de", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,", "ADJA", "NN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "zu schreiben,", "tokens": ["zu", "schrei\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Aber ich will bis in Tod dein getreuster Sohn ver-", "tokens": ["A\u00b7ber", "ich", "will", "bis", "in", "Tod", "dein", "ge\u00b7treus\u00b7ter", "Sohn", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "NN", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.24": {"text": "bleiben.", "tokens": ["blei\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}