, "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch brauner ist das Gesicht,", "tokens": ["Noch", "brau\u00b7ner", "ist", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo kam er nur her, der m\u00fcde Gesell?", "tokens": ["Wo", "kam", "er", "nur", "her", ",", "der", "m\u00fc\u00b7de", "Ge\u00b7sell", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wahrhaftig, ich wei\u00df es nicht.", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "ich", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der m\u00fcde Wandrer sitzt am Steg,", "tokens": ["Der", "m\u00fc\u00b7de", "Wand\u00b7rer", "sitzt", "am", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor\u00fcber eilet der Flu\u00df,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ei\u00b7let", "der", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Am Ufer lehnend, die H\u00e4nde gekreuzt,", "tokens": ["Am", "U\u00b7fer", "leh\u00b7nend", ",", "die", "H\u00e4n\u00b7de", "ge\u00b7kreuzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ART", "NN", "VVPP"