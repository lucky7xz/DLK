{"dta.poem.11766": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Knabe sitzt am See, und taucht die Ruthe drein;", "tokens": ["Der", "Kna\u00b7be", "sitzt", "am", "See", ",", "und", "taucht", "die", "Ru\u00b7the", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die au\u00dfen grade war, scheint innen krumm zu seyn.", "tokens": ["Die", "au\u00b7\u00dfen", "gra\u00b7de", "war", ",", "scheint", "in\u00b7nen", "krumm", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "$,", "VVFIN", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er zieht die Ruth' hervor, da ist sie wieder grade,", "tokens": ["Er", "zieht", "die", "Ruth'", "her\u00b7vor", ",", "da", "ist", "sie", "wie\u00b7der", "gra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Taucht neu sie drein, und krumm ist sie im Wellenbade.", "tokens": ["Taucht", "neu", "sie", "drein", ",", "und", "krumm", "ist", "sie", "im", "Wel\u00b7len\u00b7ba\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So oft er ein sie taucht, ist sie auch wieder krumm,", "tokens": ["So", "oft", "er", "ein", "sie", "taucht", ",", "ist", "sie", "auch", "wie\u00b7der", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und grade, wenn er sie hervorholt wiederum.", "tokens": ["Und", "gra\u00b7de", ",", "wenn", "er", "sie", "her\u00b7vor\u00b7holt", "wie\u00b7de\u00b7rum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Knabe spricht: du scheinst so lauter, es ist Schade,", "tokens": ["Der", "Kna\u00b7be", "spricht", ":", "du", "scheinst", "so", "lau\u00b7ter", ",", "es", "ist", "Scha\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du so falsch doch bist, dein Sinn ist nicht gerade.", "tokens": ["Da\u00df", "du", "so", "falsch", "doch", "bist", ",", "dein", "Sinn", "ist", "nicht", "ge\u00b7ra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "VAFIN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Grade machst du krumm; geh weg, du bist ein Wicht.", "tokens": ["Das", "Gra\u00b7de", "machst", "du", "krumm", ";", "geh", "weg", ",", "du", "bist", "ein", "Wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da h\u00f6rt der Knabe, wie der See mit Rauschen spricht:", "tokens": ["Da", "h\u00f6rt", "der", "Kna\u00b7be", ",", "wie", "der", "See", "mit", "Rau\u00b7schen", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df ohne Falsch ich bin und lauter bis zum Grund,", "tokens": ["Da\u00df", "oh\u00b7ne", "Falsch", "ich", "bin", "und", "lau\u00b7ter", "bis", "zum", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "VAFIN", "KON", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thut dir dein eignes Bild und das der Sonne kund.", "tokens": ["Thut", "dir", "dein", "eig\u00b7nes", "Bild", "und", "das", "der", "Son\u00b7ne", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "KON", "PDS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Denk, eh du schlimmes denkst, dein Aug' ist nur nicht fein", "tokens": ["Denk", ",", "eh", "du", "schlim\u00b7mes", "denkst", ",", "dein", "Aug'", "ist", "nur", "nicht", "fein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJA", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genug, das Grade recht zu sehn im schiefen Schein.", "tokens": ["Ge\u00b7nug", ",", "das", "Gra\u00b7de", "recht", "zu", "sehn", "im", "schie\u00b7fen", "Schein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}