{"dta.poem.17200": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "50.  Die Sch\u00e4delst\u00e4dt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.85", "af:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ist di\u00df die Sch\u00e4delstadt? wie kombt es dann da\u00df hier", "tokens": ["Ist", "di\u00df", "die", "Sch\u00e4\u00b7del\u00b7stadt", "?", "wie", "kombt", "es", "dann", "da\u00df", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und da der Lebensbaum? der Brunn mit den vier Fl\u00fcssen?", "tokens": ["Und", "da", "der", "Le\u00b7bens\u00b7baum", "?", "der", "Brunn", "mit", "den", "vier", "Fl\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$.", "ART", "NN", "APPR", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist das Paradi\u00df: doch sey es was es wil:", "tokens": ["Es", "ist", "das", "Pa\u00b7ra\u00b7di\u00df", ":", "doch", "sey", "es", "was", "es", "wil", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bey mir gilt diese st\u00e4dt unds Paradi\u00df gleich viel.", "tokens": ["Bey", "mir", "gilt", "die\u00b7se", "st\u00e4dt", "unds", "Pa\u00b7ra\u00b7di\u00df", "gleich", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PDS", "VVFIN", "KON", "NN", "ADV", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}