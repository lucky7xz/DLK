{"dta.poem.19095": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXVII.   Auf seiner Gr\u00e4fin Nahmens-Tag,  \n Dorothe\u00e4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.85", "af:0.14"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer werffen wir uns vor dir nieder,", "tokens": ["Hjer", "werf\u00b7fen", "wir", "uns", "vor", "dir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und singen dir geringe Lieder,", "tokens": ["Und", "sin\u00b7gen", "dir", "ge\u00b7rin\u00b7ge", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der du, nach abgelegter Last.", "tokens": ["Der", "du", ",", "nach", "ab\u00b7ge\u00b7leg\u00b7ter", "Last", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Nahmen \u00fcber alle hast.", "tokens": ["Den", "Nah\u00b7men", "\u00fc\u00b7ber", "al\u00b7le", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die \u00fcbrigen vom Weibes-Saamen", "tokens": ["Die", "\u00fcb\u00b7ri\u00b7gen", "vom", "Wei\u00b7bes\u00b7Saa\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind Menschen unbekannte Nahmen,", "tokens": ["Sind", "Men\u00b7schen", "un\u00b7be\u00b7kann\u00b7te", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr hoher Stand ist Geist von Geist,", "tokens": ["Ihr", "ho\u00b7her", "Stand", "ist", "Geist", "von", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Fleischlicher wei\u00df, was das heist.", "tokens": ["Kein", "Fleischli\u00b7cher", "wei\u00df", ",", "was", "das", "heist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PWS", "PDS", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Der aber \u00fcber alle thronet,", "tokens": ["Der", "a\u00b7ber", "\u00fc\u00b7ber", "al\u00b7le", "thro\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in den stillen Seelen wohnet,", "tokens": ["Und", "in", "den", "stil\u00b7len", "See\u00b7len", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der wei\u00df um die Gelegenheit,", "tokens": ["Der", "wei\u00df", "um", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kennet sie von Ewigkeit.", "tokens": ["Und", "ken\u00b7net", "sie", "von", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er selber hatte sie gezogen,", "tokens": ["Er", "sel\u00b7ber", "hat\u00b7te", "sie", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sie die Welt noch \u00fcberwogen,,", "tokens": ["Da", "sie", "die", "Welt", "noch", "\u00fc\u00b7ber\u00b7wo\u00b7gen", ",", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "$,", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als seine Kraft, die unsichtbar", "tokens": ["Als", "sei\u00b7ne", "Kraft", ",", "die", "un\u00b7sicht\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Seelen noch zuwider war.", "tokens": ["Den", "See\u00b7len", "noch", "zu\u00b7wi\u00b7der", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch hatte seine Helden-St\u00e4rcke,", "tokens": ["Doch", "hat\u00b7te", "sei\u00b7ne", "Hel\u00b7den\u00b7St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhn nach durchbrochner S\u00fcnden-Nacht,", "tokens": ["Jhn", "nach", "durch\u00b7broch\u00b7ner", "S\u00fcn\u00b7den\u00b7Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Meister \u00fcber sie gemacht.", "tokens": ["Zum", "Meis\u00b7ter", "\u00fc\u00b7ber", "sie", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er hatte sie auf seinem Throne", "tokens": ["Er", "hat\u00b7te", "sie", "auf", "sei\u00b7nem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem bi\u00df zum Tod getreuen Sohne,", "tokens": ["Dem", "bi\u00df", "zum", "Tod", "ge\u00b7treu\u00b7en", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den aller Seelen Elend kr\u00e4nckt,", "tokens": ["Den", "al\u00b7ler", "See\u00b7len", "E\u00b7lend", "kr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem Eigenthum geschenckt.", "tokens": ["Zu", "ei\u00b7nem", "Ei\u00b7gen\u00b7thum", "ge\u00b7schenckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Sohn, der vor Erbarmen brennek,", "tokens": ["Der", "Sohn", ",", "der", "vor", "Er\u00b7bar\u00b7men", "bren\u00b7nek", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und hellig ", "tokens": ["Und", "hel\u00b7lig"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Fiel den Verlohrnen um den Hals.", "tokens": ["Fiel", "den", "Ver\u00b7lohr\u00b7nen", "um", "den", "Hals", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Er sprach: ", "tokens": ["Er", "sprach", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Doch bin ich Joseph, euer Bruder,", "tokens": ["Doch", "bin", "ich", "Jo\u00b7se\u00b7ph", ",", "eu\u00b7er", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Zu eurem Nutz ans Creutz verkauft,", "tokens": ["Zu", "eu\u00b7rem", "Nutz", "ans", "Creutz", "ver\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr euch mit GOttes Zorn getauft.", "tokens": ["F\u00fcr", "euch", "mit", "Got\u00b7tes", "Zorn", "ge\u00b7tauft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich habe euch bey GOtt vertreten,", "tokens": ["Ich", "ha\u00b7be", "euch", "bey", "Gott", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und von dem Teufel lo\u00df gebeten.", "tokens": ["Und", "von", "dem", "Teu\u00b7fel", "lo\u00df", "ge\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schuld ist di\u00dfmal gut gemacht,", "tokens": ["Die", "Schuld", "ist", "di\u00df\u00b7mal", "gut", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eure Freyheit wiederbracht.", "tokens": ["Und", "eu\u00b7re", "Frey\u00b7heit", "wie\u00b7der\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Des Starcken Wohnung ist zerbrochen,", "tokens": ["Des", "Star\u00b7cken", "Woh\u00b7nung", "ist", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Anspruch ist ihm abgesprochen,", "tokens": ["Sein", "An\u00b7spruch", "ist", "ihm", "ab\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er zeucht dahin; ich klopfe nun,", "tokens": ["Er", "zeucht", "da\u00b7hin", ";", "ich", "klop\u00b7fe", "nun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seyd ihr bereit, mir aufzuthun!", "tokens": ["Seyd", "ihr", "be\u00b7reit", ",", "mir", "auf\u00b7zu\u00b7thun", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADJD", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Feind durchwandelt d\u00fcrre St\u00e4tte,", "tokens": ["Der", "Feind", "durch\u00b7wan\u00b7delt", "d\u00fcr\u00b7re", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht Ruh und Raum vor sein Ger\u00e4the;", "tokens": ["Sucht", "Ruh", "und", "Raum", "vor", "sein", "Ge\u00b7r\u00e4\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er findet aber nirgend Ruh,", "tokens": ["Er", "fin\u00b7det", "a\u00b7ber", "nir\u00b7gend", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald spricht er wieder bey euch zu.", "tokens": ["Bald", "spricht", "er", "wie\u00b7der", "bey", "euch", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Da m\u00f6gt ihr euer Hau\u00df bewachen,", "tokens": ["Da", "m\u00f6gt", "ihr", "eu\u00b7er", "Hau\u00df", "be\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst wird er euch zu Sclaven machen,", "tokens": ["Sonst", "wird", "er", "euch", "zu", "Scla\u00b7ven", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Werft ihm, was sein ist, gar hinaus,", "tokens": ["Werft", "ihm", ",", "was", "sein", "ist", ",", "gar", "hin\u00b7aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "VAINF", "VAFIN", "$,", "ADV", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprecht: ", "tokens": ["Und", "sprecht", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.13": {"line.1": {"text": "Ach! rett uns von dem Widersacher,", "tokens": ["Ach", "!", "rett", "uns", "von", "dem", "Wi\u00b7der\u00b7sa\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sohn GOttes, unser Seligmacher,", "tokens": ["Sohn", "Got\u00b7tes", ",", "un\u00b7ser", "Se\u00b7lig\u00b7ma\u00b7cher", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So schreyn die Seelen Tag und Nacht", "tokens": ["So", "schreyn", "die", "See\u00b7len", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu JEsu, der sie lo\u00df gemacht.", "tokens": ["Zu", "Je\u00b7su", ",", "der", "sie", "lo\u00df", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da greift er zu, und in der K\u00fcrtze,", "tokens": ["Da", "greift", "er", "zu", ",", "und", "in", "der", "K\u00fcrt\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eh sie der Feind zu Grunde st\u00fcrtze,", "tokens": ["Eh", "sie", "der", "Feind", "zu", "Grun\u00b7de", "st\u00fcrt\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Nimmt JEsus ihm zu Hohn und Trutz", "tokens": ["Nimmt", "Je\u00b7sus", "ihm", "zu", "Hohn", "und", "Trutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seelen ein in seinen Schutz.", "tokens": ["Die", "See\u00b7len", "ein", "in", "sei\u00b7nen", "Schutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Er wandelt auch der Seelen Nahmen,", "tokens": ["Er", "wan\u00b7delt", "auch", "der", "See\u00b7len", "Nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter"