{"textgrid.poem.31490": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Nie-gesehne Sach! ein Jungfrau-Mutter wieget", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Nie-gesehne Sach! ein Jungfrau-Mutter wieget", "tokens": ["O", "Nie\u00b7ge\u00b7seh\u00b7ne", "Sach", "!", "ein", "Jung\u00b7frau\u00b7Mut\u00b7ter", "wie\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "denselben / der doch selbst die Haubtbewegung ist.", "tokens": ["den\u00b7sel\u00b7ben", "/", "der", "doch", "selbst", "die", "Haubt\u00b7be\u00b7we\u00b7gung", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$(", "ART", "ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hat zur lieg statt ihm ein spannbreit Ort erkiest.", "tokens": ["Er", "hat", "zur", "lieg", "statt", "ihm", "ein", "spann\u00b7breit", "Ort", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der / so die Erd' umspannt / jetzt in der Krippen lieget.", "tokens": ["Der", "/", "so", "die", "Erd'", "um\u00b7spannt", "/", "jetzt", "in", "der", "Krip\u00b7pen", "lie\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "ART", "NN", "VVFIN", "$(", "ADV", "APPR",