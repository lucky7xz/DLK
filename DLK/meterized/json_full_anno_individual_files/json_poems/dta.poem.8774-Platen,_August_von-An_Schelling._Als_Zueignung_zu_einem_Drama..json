{"dta.poem.8774": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "An Schelling.  \n Als Zueignung zu einem Drama.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Es kann zur\u00fcck sich nicht ergehn zum Kinde;", "tokens": ["Es", "kann", "zu\u00b7r\u00fcck", "sich", "nicht", "er\u00b7gehn", "zum", "Kin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PRF", "PTKNEG", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Dichtung erster, jugendlicher Reigen", "tokens": ["Der", "Dich\u00b7tung", "ers\u00b7ter", ",", "ju\u00b7gend\u00b7li\u00b7cher", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zog l\u00e4ngst vor\u00fcber, flog vorbey geschwinde:", "tokens": ["Zog", "l\u00e4ngst", "vor\u00b7\u00fc\u00b7ber", ",", "flog", "vor\u00b7bey", "ge\u00b7schwin\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sophisten kamen, sie begann zu schweigen,", "tokens": ["So\u00b7phis\u00b7ten", "ka\u00b7men", ",", "sie", "be\u00b7gann", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und l\u00f6ste nach und nach die goldne Binde.", "tokens": ["Und", "l\u00f6s\u00b7te", "nach", "und", "nach", "die", "gold\u00b7ne", "Bin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch jene N\u00fcchternen bezwang dein Streben,", "tokens": ["Doch", "je\u00b7ne", "N\u00fcch\u00b7ter\u00b7nen", "be\u00b7zwang", "dein", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Und so entflammtest du das neue Leben!", "tokens": ["Und", "so", "ent\u00b7flamm\u00b7test", "du", "das", "neu\u00b7e", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was deutsche Kraft in dieser Zeit erreichte,", "tokens": ["Was", "deut\u00b7sche", "Kraft", "in", "die\u00b7ser", "Zeit", "er\u00b7reich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geh\u00f6rt dir an, und neigt sich deinem Bilde,", "tokens": ["Ge\u00b7h\u00f6rt", "dir", "an", ",", "und", "neigt", "sich", "dei\u00b7nem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dein vor allen sey dies Lied, das leichte,", "tokens": ["Und", "dein", "vor", "al\u00b7len", "sey", "dies", "Lied", ",", "das", "leich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPR", "PIS", "VAFIN", "PDS", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das du zuerst empfingst mit edler Milde,", "tokens": ["Das", "du", "zu\u00b7erst", "emp\u00b7fingst", "mit", "ed\u00b7ler", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Versammelnd rings um dessen fr\u00fchste Beichte,", "tokens": ["Ver\u00b7sam\u00b7melnd", "rings", "um", "des\u00b7sen", "fr\u00fchs\u00b7te", "Beich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Frau'n und M\u00e4nnern eine sch\u00f6ne Gilde:", "tokens": ["Von", "Frau'n", "und", "M\u00e4n\u00b7nern", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Gil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sey's, da\u00df das Volk es nun mit Gunst bezahle,", "tokens": ["Sey's", ",", "da\u00df", "das", "Volk", "es", "nun", "mit", "Gunst", "be\u00b7zah\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du lie\u00dfest leben es zum ersten Male!", "tokens": ["Du", "lie\u00b7\u00dfest", "le\u00b7ben", "es", "zum", "ers\u00b7ten", "Ma\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun m\u00f6gen Lieder sich zum Liede reihen,", "tokens": ["Nun", "m\u00f6\u00b7gen", "Lie\u00b7der", "sich", "zum", "Lie\u00b7de", "rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschichte zu Geschichte, Sag' an Sage,", "tokens": ["Ge\u00b7schich\u00b7te", "zu", "Ge\u00b7schich\u00b7te", ",", "Sag'", "an", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sehne mich, sie alle dir zu weihen,", "tokens": ["Ich", "seh\u00b7ne", "mich", ",", "sie", "al\u00b7le", "dir", "zu", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "PIS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die noch als Keim ich in der Seele trage,", "tokens": ["Die", "noch", "als", "Keim", "ich", "in", "der", "See\u00b7le", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dir, der geh\u00f6rt mit g\u00fctigem Verzeihen", "tokens": ["Dir", ",", "der", "ge\u00b7h\u00f6rt", "mit", "g\u00fc\u00b7ti\u00b7gem", "Ver\u00b7zei\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die fr\u00fchsten Kl\u00e4nge meiner jungen Tage,", "tokens": ["Die", "fr\u00fchs\u00b7ten", "Kl\u00e4n\u00b7ge", "mei\u00b7ner", "jun\u00b7gen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da noch ich sang des Stolzes muth'ge Triebe,", "tokens": ["Da", "noch", "ich", "sang", "des", "Stol\u00b7zes", "muth'\u00b7ge", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und jenen brennenden nach Ruhm und Liebe.", "tokens": ["Und", "je\u00b7nen", "bren\u00b7nen\u00b7den", "nach", "Ruhm", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch hat das Herz sich nie zurecht gefunden", "tokens": ["Doch", "hat", "das", "Herz", "sich", "nie", "zu\u00b7recht", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PRF", "ADV", "PTKVZ", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dieses Lebens ird'schen Paradiesen:", "tokens": ["In", "die\u00b7ses", "Le\u00b7bens", "ird'\u00b7schen", "Pa\u00b7ra\u00b7die\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die freye Liebe, die es ungebunden", "tokens": ["Die", "frey\u00b7e", "Lie\u00b7be", ",", "die", "es", "un\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Menschen bot, sie ward verlacht von diesen,", "tokens": ["Den", "Men\u00b7schen", "bot", ",", "sie", "ward", "ver\u00b7lacht", "von", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und fr\u00fche f\u00fchlt' ich in verlass'nen Stunden", "tokens": ["Und", "fr\u00fc\u00b7he", "f\u00fchlt'", "ich", "in", "ver\u00b7lass'\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich auf mein eignes, dunkles Selbst verwiesen,", "tokens": ["Mich", "auf", "mein", "eig\u00b7nes", ",", "dunk\u00b7les", "Selbst", "ver\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und fr\u00fch begann ein unaussprechlich Sehnen", "tokens": ["Und", "fr\u00fch", "be\u00b7gann", "ein", "un\u00b7aus\u00b7sprech\u00b7lich", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Brust durch Seufzer m\u00e4chtig auszudehnen.", "tokens": ["Die", "Brust", "durch", "Seuf\u00b7zer", "m\u00e4ch\u00b7tig", "aus\u00b7zu\u00b7deh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das ist vorbey! Ich lernte viel verschmerzen,", "tokens": ["Das", "ist", "vor\u00b7bey", "!", "Ich", "lern\u00b7te", "viel", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchlte Kraft, mir Alles zu versagen,", "tokens": ["Ich", "f\u00fchl\u00b7te", "Kraft", ",", "mir", "Al\u00b7les", "zu", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eine Welt von Heiterkeit und Scherzen", "tokens": ["Und", "ei\u00b7ne", "Welt", "von", "Hei\u00b7ter\u00b7keit", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im leichtbeweglichen Gem\u00fcth zu tragen:", "tokens": ["Im", "leicht\u00b7be\u00b7weg\u00b7li\u00b7chen", "Ge\u00b7m\u00fcth", "zu", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur selten soll die tiefe Qual im Herzen", "tokens": ["Nur", "sel\u00b7ten", "soll", "die", "tie\u00b7fe", "Qual", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ergie\u00dfen sich in ungeheure Klagen,", "tokens": ["Er\u00b7gie\u00b7\u00dfen", "sich", "in", "un\u00b7ge\u00b7heu\u00b7re", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und jeder H\u00f6rer f\u00fchle dann mit Beben,", "tokens": ["Und", "je\u00b7der", "H\u00f6\u00b7rer", "f\u00fch\u00b7le", "dann", "mit", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was f\u00fcr ein trauriges Geschenk das Leben!", "tokens": ["Was", "f\u00fcr", "ein", "trau\u00b7ri\u00b7ges", "Ge\u00b7schenk", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So ward gest\u00e4hlt ich denn und ausgestattet", "tokens": ["So", "ward", "ge\u00b7st\u00e4hlt", "ich", "denn", "und", "aus\u00b7ge\u00b7stat\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Thaten, die ich l\u00e4nger nicht verschiebe:", "tokens": ["Zu", "Tha\u00b7ten", ",", "die", "ich", "l\u00e4n\u00b7ger", "nicht", "ver\u00b7schie\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Muth, in Qualen nach und nach ermattet,", "tokens": ["Mein", "Muth", ",", "in", "Qua\u00b7len", "nach", "und", "nach", "er\u00b7mat\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "APPR", "KON", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird nie mehr betteln gehn um weiche Liebe.", "tokens": ["Wird", "nie", "mehr", "bet\u00b7teln", "gehn", "um", "wei\u00b7che", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vielleicht, da Stunde sich zu Stunde gattet,", "tokens": ["Viel\u00b7leicht", ",", "da", "Stun\u00b7de", "sich", "zu", "Stun\u00b7de", "gat\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gelingt es meinem gl\u00fchenden Betriebe,", "tokens": ["Ge\u00b7lingt", "es", "mei\u00b7nem", "gl\u00fc\u00b7hen\u00b7den", "Be\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ich dereinst, wenn deutsches Wort ich meistre,", "tokens": ["Da\u00df", "ich", "de\u00b7reinst", ",", "wenn", "deut\u00b7sches", "Wort", "ich", "meist\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die edle Jugend dieses Volks begeistre.", "tokens": ["Die", "ed\u00b7le", "Ju\u00b7gend", "die\u00b7ses", "Volks", "be\u00b7geist\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}