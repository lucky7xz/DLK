{"dta.poem.13056": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egemach, gemach erblickt ein kleiner Sonnen-Strahl,", "tokens": ["\u201e", "ge\u00b7mach", ",", "ge\u00b7mach", "er\u00b7blickt", "ein", "klei\u00b7ner", "Son\u00b7nen\u00b7Strahl", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eder schon der falben Nacht den Abzug anbefahl,", "tokens": ["\u201e", "der", "schon", "der", "fal\u00b7ben", "Nacht", "den", "Ab\u00b7zug", "an\u00b7be\u00b7fahl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edie Krone des Gestirns. Was Freuden in den Sternen?", "tokens": ["\u201e", "die", "Kro\u00b7ne", "des", "Ge\u00b7stirns", ".", "Was", "Freu\u00b7den", "in", "den", "Ster\u00b7nen", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$.", "PWS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201e(Vernehmet, Wertheste! nun k\u00f6nnt ihr etwas lernen)", "tokens": ["\u201e", "(", "Ver\u00b7neh\u00b7met", ",", "Wert\u00b7hes\u00b7te", "!", "nun", "k\u00f6nnt", "ihr", "et\u00b7was", "ler\u00b7nen", ")"], "token_info": ["punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "VVFIN", "$,", "NN", "$.", "ADV", "VVFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "505\u201dDie Schatten wichen ab; die Nacht verschliche sich,", "tokens": ["\"", "Die", "Schat\u00b7ten", "wi\u00b7chen", "ab", ";", "die", "Nacht", "ver\u00b7schli\u00b7che", "sich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund Luft und Stern\u2019 und Mond ward g\u00e4h ver\u00e4nderlich;", "tokens": ["\u201e", "und", "Luft", "und", "Stern'", "und", "Mond", "ward", "g\u00e4h", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "KON", "NN", "KON", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eje mehr die Sonne stieg, je mehr der Krei\u00df erbla\u00dfte,", "tokens": ["\u201e", "je", "mehr", "die", "Son\u00b7ne", "stieg", ",", "je", "mehr", "der", "Krei\u00df", "er\u00b7bla\u00df\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "\u201eindem er sich zum Gru\u00df doch Kummer-voll verfa\u00dfte.", "tokens": ["\u201e", "in\u00b7dem", "er", "sich", "zum", "Gru\u00df", "doch", "Kum\u00b7mer\u00b7voll", "ver\u00b7fa\u00df\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "APPRART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edie sanfte Dunckelheit ward unvermerckt zu Licht.", "tokens": ["\u201e", "die", "sanf\u00b7te", "Dun\u00b7ckel\u00b7heit", "ward", "un\u00b7ver\u00b7merckt", "zu", "Licht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "510\u201dDie Strahlen fielen schon den Sternen ins Gesicht;", "tokens": ["\"", "Die", "Strah\u00b7len", "fie\u00b7len", "schon", "den", "Ster\u00b7nen", "ins", "Ge\u00b7sicht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edie Sonne nahte sich, fieng heimlich an zu lachen,", "tokens": ["\u201e", "die", "Son\u00b7ne", "nah\u00b7te", "sich", ",", "fi\u00b7eng", "heim\u00b7lich", "an", "zu", "la\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "$,", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "\u201eda sie die Reihe sah um ihren Wagen machen.", "tokens": ["\u201e", "da", "sie", "die", "Rei\u00b7he", "sah", "um", "ih\u00b7ren", "Wa\u00b7gen", "ma\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ees wich was Schatten hei\u00dft, von allen Seiten ab,", "tokens": ["\u201e", "es", "wich", "was", "Schat\u00b7ten", "hei\u00dft", ",", "von", "al\u00b7len", "Sei\u00b7ten", "ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "NN", "VVFIN", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eso da\u00df die Sonne sich ganz hell zu sehen gab.", "tokens": ["\u201e", "so", "da\u00df", "die", "Son\u00b7ne", "sich", "ganz", "hell", "zu", "se\u00b7hen", "gab", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "ART", "NN", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "515\u201dO du verblendter Mond! geh! weich mit deinen Sternen!", "tokens": ["\"", "O", "du", "ver\u00b7blend\u00b7ter", "Mond", "!", "geh", "!", "weich", "mit", "dei\u00b7nen", "Ster\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "ADJA", "NN", "$.", "VVFIN", "$.", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201esie mu\u00dften ihr Gesicht vom Sonnen-Feur entfernen;", "tokens": ["\u201e", "sie", "mu\u00df\u00b7ten", "ihr", "Ge\u00b7sicht", "vom", "Son\u00b7nen\u00b7Feur", "ent\u00b7fer\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eund als sie sich besahn, war Pracht und Schimmer hin:", "tokens": ["\u201e", "und", "als", "sie", "sich", "be\u00b7sahn", ",", "war", "Pracht", "und", "Schim\u00b7mer", "hin", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "PRF", "VVPP", "$,", "VAFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201esie merckten kaum, wohin die Sonne wurde ziehn;", "tokens": ["\u201e", "sie", "merck\u00b7ten", "kaum", ",", "wo\u00b7hin", "die", "Son\u00b7ne", "wur\u00b7de", "ziehn", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eso ward der Sterne Glanz vernebelt und verh\u00fcllet;", "tokens": ["\u201e", "so", "ward", "der", "Ster\u00b7ne", "Glanz", "ver\u00b7ne\u00b7belt", "und", "ver\u00b7h\u00fcl\u00b7let", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "520\u201dSo blieb ihr Wunsch, ihr Gru\u00df, ihr Vorsaz unerf\u00fcllet.", "tokens": ["\"", "So", "blieb", "ihr", "Wunsch", ",", "ihr", "Gru\u00df", ",", "ihr", "Vor\u00b7saz", "un\u00b7er\u00b7f\u00fcl\u00b7let", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}