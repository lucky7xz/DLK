{"textgrid.poem.42066": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson f\u00fcttert sein Lamm", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sind nicht St\u00e4dte mehr, die rufen: bau!", "tokens": ["Hier", "sind", "nicht", "St\u00e4d\u00b7te", "mehr", ",", "die", "ru\u00b7fen", ":", "bau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "ADV", "$,", "PRELS", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Wein auch, der bet\u00f6rte: komm und schl\u00fcrfe!", "tokens": ["Nicht", "Wein", "auch", ",", "der", "be\u00b7t\u00f6r\u00b7te", ":", "komm", "und", "schl\u00fcr\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "$,", "PRELS", "VVFIN", "$.", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Trug der Sehnsucht um die fremde Frau", "tokens": ["Kein", "Trug", "der", "Sehn\u00b7sucht", "um", "die", "frem\u00b7de", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und keiner Tat, da\u00df sie mein Tun bed\u00fcrfe.", "tokens": ["Und", "kei\u00b7ner", "Tat", ",", "da\u00df", "sie", "mein", "Tun", "be\u00b7d\u00fcr\u00b7fe", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier 