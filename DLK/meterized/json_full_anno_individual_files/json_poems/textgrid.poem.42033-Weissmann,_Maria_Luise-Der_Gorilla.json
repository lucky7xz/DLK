{"textgrid.poem.42033": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Gorilla", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er atmet ihre Schw\u00fcle l\u00e4ngst nicht mehr,", "tokens": ["Er", "at\u00b7met", "ih\u00b7re", "Schw\u00fc\u00b7le", "l\u00e4ngst", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch lastet seinem Nacken immer noch der Traum der gro\u00dfen Seen", "tokens": ["Doch", "las\u00b7tet", "sei\u00b7nem", "Na\u00b7cken", "im\u00b7mer", "noch", "der", "Traum", "der", "gro\u00b7\u00dfen", "Seen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und l\u00e4\u00dft ihn tief zum Sand geb\u00fcckt und schwer", "tokens": ["Und", "l\u00e4\u00dft", "ihn", "tief", "zum", "Sand", "ge\u00b7b\u00fcckt", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Takt zur Wiederkehr der Eisenst\u00e4be gehn.", "tokens": ["Im", "Takt", "zur", "Wie\u00b7der\u00b7kehr", "der", "Ei\u00b7sen\u00b7st\u00e4\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er m\u00f6chte wohl der Glanz der Papageien sein,", "tokens": ["Er", "m\u00f6ch\u00b7te", "wohl", "der", "Glanz", "der", "Pa\u00b7pa\u00b7gei\u00b7en", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Duften der Reseden und der Walzerklang,", "tokens": ["Das", "Duf\u00b7ten", "der", "Re\u00b7se\u00b7den", "und", "der", "Wal\u00b7zer\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch bricht kein Strahl den tr\u00fcben Spiegel seines Auges ein:", "tokens": ["Doch", "bricht", "kein", "Strahl", "den", "tr\u00fc\u00b7ben", "Spie\u00b7gel", "sei\u00b7nes", "Au\u00b7ges", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Die Hand tr\u00e4gt still gefaltet den betr\u00e4umten Gang", "tokens": ["Die", "Hand", "tr\u00e4gt", "still", "ge\u00b7fal\u00b7tet", "den", "be\u00b7tr\u00e4um\u00b7ten", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem fremden Leuchten still und fremd vorbei.", "tokens": ["Dem", "frem\u00b7den", "Leuch\u00b7ten", "still", "und", "fremd", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+",