{"textgrid.poem.54350": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "32. Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach mein Schicksal la\u00df mich wissen,", "tokens": ["Ach", "mein", "Schick\u00b7sal", "la\u00df", "mich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn erscheint der frohe Tag,", "tokens": ["Wenn", "er\u00b7scheint", "der", "fro\u00b7he", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mein Leiden sich soll schliessen,", "tokens": ["Da", "mein", "Lei\u00b7den", "sich", "soll", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ich nicht gestehen mag?", "tokens": ["Das", "ich", "nicht", "ge\u00b7ste\u00b7hen", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lach ich oftmahls mit dem Munde,", "tokens": ["Lach", "ich", "oft\u00b7mahls", "mit", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So entfernet sich das Herz;", "tokens": ["So", "ent\u00b7fer\u00b7net", "sich", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn da lieget in dem Grunde", "tokens": ["Denn", "da", "lie\u00b7get", "in", "dem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der so tief versteckte Schmerz.", "tokens": ["Der", "so", "tief", "ver\u00b7steck\u00b7te", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fragt ein Freund nach meinem Schmerzen,", "tokens": ["Fragt", "ein", "Freund", "nach", "mei\u00b7nem", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So verhel ich meine Pein.", "tokens": ["So", "ver\u00b7hel", "ich", "mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht ihm meine Noth zu Herzen,", "tokens": ["Geht", "ihm", "mei\u00b7ne", "Noth", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich doch verschwiegen seyn.", "tokens": ["Mu\u00df", "ich", "doch", "ver\u00b7schwie\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn er \u00e4ndert nicht die Sorgen,", "tokens": ["Denn", "er", "\u00e4n\u00b7dert", "nicht", "die", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmt er gleich mit Theil daran;", "tokens": ["Nimmt", "er", "gleich", "mit", "Theil", "da\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es bleibet ihm verborgen,", "tokens": ["Und", "es", "blei\u00b7bet", "ihm", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie, und wenn sichs \u00e4ndern kann.", "tokens": ["Wie", ",", "und", "wenn", "sichs", "\u00e4n\u00b7dern", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will die Hoffnung mich ergetzen,", "tokens": ["Will", "die", "Hoff\u00b7nung", "mich", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den frohen Tag bestimmt,", "tokens": ["Die", "den", "fro\u00b7hen", "Tag", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich werde w\u00fcrdig sch\u00e4tzen,", "tokens": ["Denn", "ich", "wer\u00b7de", "w\u00fcr\u00b7dig", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er gar kein Ende nimmt:", "tokens": ["Da\u00df", "er", "gar", "kein", "En\u00b7de", "nimmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So will ich auch da verhelen,", "tokens": ["So", "will", "ich", "auch", "da", "ver\u00b7he\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Was der Trost f\u00fcr Wirkung hat;", "tokens": ["Was", "der", "Trost", "f\u00fcr", "Wir\u00b7kung", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich bin nicht wie feige Seelen:", "tokens": ["Ich", "bin", "nicht", "wie", "fei\u00b7ge", "See\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich schreckt nicht ein rauschend Blat.", "tokens": ["Mich", "schreckt", "nicht", "ein", "rau\u00b7schend", "Blat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Vor den kleinsten Sturm erschrecken,", "tokens": ["Vor", "den", "kleins\u00b7ten", "Sturm", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nicht edel und gemein.", "tokens": ["Ist", "nicht", "e\u00b7del", "und", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man mu\u00df unter Dorn und Hecken", "tokens": ["Man", "mu\u00df", "un\u00b7ter", "Dorn", "und", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufger\u00e4umt und munter seyn.", "tokens": ["Auf\u00b7ge\u00b7r\u00e4umt", "und", "mun\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So kann man den Schmerz besiegen;", "tokens": ["So", "kann", "man", "den", "Schmerz", "be\u00b7sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So verf\u00e4hrt ein grosser Geist;", "tokens": ["So", "ver\u00b7f\u00e4hrt", "ein", "gros\u00b7ser", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er ist stille und verschwiegen,", "tokens": ["Er", "ist", "stil\u00b7le", "und", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn man ihn in Ketten schleu\u00dft.", "tokens": ["Wenn", "man", "ihn", "in", "Ket\u00b7ten", "schleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6rt er gleich den Neider lachen,", "tokens": ["H\u00f6rt", "er", "gleich", "den", "Nei\u00b7der", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er dies Gest\u00e4ndni\u00df thut,", "tokens": ["Wenn", "er", "dies", "Ge\u00b7st\u00e4nd\u00b7ni\u00df", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So wird er kein Wesen machen,", "tokens": ["So", "wird", "er", "kein", "We\u00b7sen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil die Ruh in ihm beruht.", "tokens": ["Weil", "die", "Ruh", "in", "ihm", "be\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese kann man ihm nicht kr\u00e4nken;", "tokens": ["Die\u00b7se", "kann", "man", "ihm", "nicht", "kr\u00e4n\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Man macht sich nur l\u00e4cherlich,", "tokens": ["Man", "macht", "sich", "nur", "l\u00e4\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Denn man irrt so gleich im Denken.", "tokens": ["Denn", "man", "irrt", "so", "gleich", "im", "Den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder sorge nur vor sich.", "tokens": ["Je\u00b7der", "sor\u00b7ge", "nur", "vor", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun so tobe tolles Gl\u00fccke!", "tokens": ["Nun", "so", "to\u00b7be", "tol\u00b7les", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du mir zuwider bist,", "tokens": ["Wenn", "du", "mir", "zu\u00b7wi\u00b7der", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehr ich dennoch dein Geschicke,", "tokens": ["Ehr", "ich", "den\u00b7noch", "dein", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mein Mund voll Ehrfurcht k\u00fc\u00dft.", "tokens": ["Das", "mein", "Mund", "voll", "Ehr\u00b7furcht", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich schl\u00e4gt die frohe Stunde,", "tokens": ["End\u00b7lich", "schl\u00e4gt", "die", "fro\u00b7he", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dich noch bewegen kann.", "tokens": ["Die", "dich", "noch", "be\u00b7we\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weicht der Schmerz den ich empfunde,", "tokens": ["Weicht", "der", "Schmerz", "den", "ich", "emp\u00b7fun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Beth ich dich gedoppelt an.", "tokens": ["Be\u00b7th", "ich", "dich", "ge\u00b7dop\u00b7pelt", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach mein Schicksal la\u00df mich wissen,", "tokens": ["Ach", "mein", "Schick\u00b7sal", "la\u00df", "mich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn erscheint der frohe Tag,", "tokens": ["Wenn", "er\u00b7scheint", "der", "fro\u00b7he", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mein Leiden sich soll schliessen,", "tokens": ["Da", "mein", "Lei\u00b7den", "sich", "soll", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ich nicht gestehen mag?", "tokens": ["Das", "ich", "nicht", "ge\u00b7ste\u00b7hen", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lach ich oftmahls mit dem Munde,", "tokens": ["Lach", "ich", "oft\u00b7mahls", "mit", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So entfernet sich das Herz;", "tokens": ["So", "ent\u00b7fer\u00b7net", "sich", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn da lieget in dem Grunde", "tokens": ["Denn", "da", "lie\u00b7get", "in", "dem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der so tief versteckte Schmerz.", "tokens": ["Der", "so", "tief", "ver\u00b7steck\u00b7te", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Fragt ein Freund nach meinem Schmerzen,", "tokens": ["Fragt", "ein", "Freund", "nach", "mei\u00b7nem", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So verhel ich meine Pein.", "tokens": ["So", "ver\u00b7hel", "ich", "mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht ihm meine Noth zu Herzen,", "tokens": ["Geht", "ihm", "mei\u00b7ne", "Noth", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df ich doch verschwiegen seyn.", "tokens": ["Mu\u00df", "ich", "doch", "ver\u00b7schwie\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn er \u00e4ndert nicht die Sorgen,", "tokens": ["Denn", "er", "\u00e4n\u00b7dert", "nicht", "die", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmt er gleich mit Theil daran;", "tokens": ["Nimmt", "er", "gleich", "mit", "Theil", "da\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es bleibet ihm verborgen,", "tokens": ["Und", "es", "blei\u00b7bet", "ihm", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie, und wenn sichs \u00e4ndern kann.", "tokens": ["Wie", ",", "und", "wenn", "sichs", "\u00e4n\u00b7dern", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Will die Hoffnung mich ergetzen,", "tokens": ["Will", "die", "Hoff\u00b7nung", "mich", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den frohen Tag bestimmt,", "tokens": ["Die", "den", "fro\u00b7hen", "Tag", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich werde w\u00fcrdig sch\u00e4tzen,", "tokens": ["Denn", "ich", "wer\u00b7de", "w\u00fcr\u00b7dig", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er gar kein Ende nimmt:", "tokens": ["Da\u00df", "er", "gar", "kein", "En\u00b7de", "nimmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So will ich auch da verhelen,", "tokens": ["So", "will", "ich", "auch", "da", "ver\u00b7he\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Was der Trost f\u00fcr Wirkung hat;", "tokens": ["Was", "der", "Trost", "f\u00fcr", "Wir\u00b7kung", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich bin nicht wie feige Seelen:", "tokens": ["Ich", "bin", "nicht", "wie", "fei\u00b7ge", "See\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich schreckt nicht ein rauschend Blat.", "tokens": ["Mich", "schreckt", "nicht", "ein", "rau\u00b7schend", "Blat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Vor den kleinsten Sturm erschrecken,", "tokens": ["Vor", "den", "kleins\u00b7ten", "Sturm", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nicht edel und gemein.", "tokens": ["Ist", "nicht", "e\u00b7del", "und", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man mu\u00df unter Dorn und Hecken", "tokens": ["Man", "mu\u00df", "un\u00b7ter", "Dorn", "und", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufger\u00e4umt und munter seyn.", "tokens": ["Auf\u00b7ge\u00b7r\u00e4umt", "und", "mun\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So kann man den Schmerz besiegen;", "tokens": ["So", "kann", "man", "den", "Schmerz", "be\u00b7sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "So verf\u00e4hrt ein grosser Geist;", "tokens": ["So", "ver\u00b7f\u00e4hrt", "ein", "gros\u00b7ser", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er ist stille und verschwiegen,", "tokens": ["Er", "ist", "stil\u00b7le", "und", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn man ihn in Ketten schleu\u00dft.", "tokens": ["Wenn", "man", "ihn", "in", "Ket\u00b7ten", "schleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "H\u00f6rt er gleich den Neider lachen,", "tokens": ["H\u00f6rt", "er", "gleich", "den", "Nei\u00b7der", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er dies Gest\u00e4ndni\u00df thut,", "tokens": ["Wenn", "er", "dies", "Ge\u00b7st\u00e4nd\u00b7ni\u00df", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So wird er kein Wesen machen,", "tokens": ["So", "wird", "er", "kein", "We\u00b7sen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil die Ruh in ihm beruht.", "tokens": ["Weil", "die", "Ruh", "in", "ihm", "be\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese kann man ihm nicht kr\u00e4nken;", "tokens": ["Die\u00b7se", "kann", "man", "ihm", "nicht", "kr\u00e4n\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Man macht sich nur l\u00e4cherlich,", "tokens": ["Man", "macht", "sich", "nur", "l\u00e4\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Denn man irrt so gleich im Denken.", "tokens": ["Denn", "man", "irrt", "so", "gleich", "im", "Den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeder sorge nur vor sich.", "tokens": ["Je\u00b7der", "sor\u00b7ge", "nur", "vor", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nun so tobe tolles Gl\u00fccke!", "tokens": ["Nun", "so", "to\u00b7be", "tol\u00b7les", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du mir zuwider bist,", "tokens": ["Wenn", "du", "mir", "zu\u00b7wi\u00b7der", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehr ich dennoch dein Geschicke,", "tokens": ["Ehr", "ich", "den\u00b7noch", "dein", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mein Mund voll Ehrfurcht k\u00fc\u00dft.", "tokens": ["Das", "mein", "Mund", "voll", "Ehr\u00b7furcht", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich schl\u00e4gt die frohe Stunde,", "tokens": ["End\u00b7lich", "schl\u00e4gt", "die", "fro\u00b7he", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dich noch bewegen kann.", "tokens": ["Die", "dich", "noch", "be\u00b7we\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weicht der Schmerz den ich empfunde,", "tokens": ["Weicht", "der", "Schmerz", "den", "ich", "emp\u00b7fun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "+-+-+-