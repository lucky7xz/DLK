ct"], "pos": ["KON", "PWAV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schaffet ihn nach eurem Bilde.", "tokens": ["So", "schaf\u00b7fet", "ihn", "nach", "eu\u00b7rem", "Bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wenn euer Aug kein Gro\u00dfes fa\u00dft,", "tokens": ["Wenn", "eu\u00b7er", "Aug", "kein", "Gro\u00b7\u00dfes", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schlie\u00dft ihn ein in enge Rahmen,", "tokens": ["So", "schlie\u00dft", "ihn", "ein", "in", "en\u00b7ge", "Rah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nehmt einen Gott, der liebt und ha\u00dft,", "tokens": ["Nehmt", "ei\u00b7nen", "Gott", ",", "der", "liebt", "und", "ha\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und liebt und ha\u00dft in seinem Namen.", "tokens": ["Und", "liebt", "und", "ha\u00dft", "in", "sei\u00b7nem", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}