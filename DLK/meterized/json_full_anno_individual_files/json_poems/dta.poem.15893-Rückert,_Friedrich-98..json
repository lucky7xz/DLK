{"dta.poem.15893": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin in andrer Zeit, ich bin in anderm Raum,", "tokens": ["Ich", "bin", "in", "an\u00b7drer", "Zeit", ",", "ich", "bin", "in", "an\u00b7derm", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VAFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Gegenwart Get\u00f6s' erweckt nicht meinen Traum.", "tokens": ["Der", "Ge\u00b7gen\u00b7wart", "Ge\u00b7t\u00f6s'", "er\u00b7weckt", "nicht", "mei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch was von dieser Zeit in meinen Traum mag dringen,", "tokens": ["Doch", "was", "von", "die\u00b7ser", "Zeit", "in", "mei\u00b7nen", "Traum", "mag", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' ihn zu st\u00f6ren wird es sich darein verschlingen.", "tokens": ["Ohn'", "ihn", "zu", "st\u00f6\u00b7ren", "wird", "es", "sich", "da\u00b7rein", "ver\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "PPER", "PRF