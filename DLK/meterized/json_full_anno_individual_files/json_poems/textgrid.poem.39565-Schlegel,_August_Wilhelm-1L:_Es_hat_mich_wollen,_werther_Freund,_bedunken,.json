{"textgrid.poem.39565": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es hat mich wollen, werther Freund, bedunken,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat mich wollen, werther Freund, bedunken,", "tokens": ["Es", "hat", "mich", "wol\u00b7len", ",", "wert\u00b7her", "Freund", ",", "be\u00b7dun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VMFIN", "$,", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du bisher die Dinte m\u00fc\u00dfen sparen,", "tokens": ["Da\u00df", "du", "bis\u00b7her", "die", "Din\u00b7te", "m\u00fc\u00b7\u00dfen", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df auch die Federn nicht geschnitten waren,", "tokens": ["Da\u00df", "auch", "die", "Fe\u00b7dern", "nicht", "ge\u00b7schnit\u00b7ten", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weshalb dir fast die Autorschaft entsunken.", "tokens": ["We\u00b7shalb", "dir", "fast", "die", "Au\u00b7tor\u00b7schaft", "ent\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch da dein Ruf so herrlich schon gestunken,", "tokens": ["Doch", "da", "dein", "Ruf", "so", "herr\u00b7lich", "schon", "ge\u00b7stun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00dft du ihn auch hinf\u00fcro offenbaren.", "tokens": ["Mu\u00dft", "du", "ihn", "auch", "hin\u00b7f\u00fc\u00b7ro", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf, kratze dich in deinen krausen Haaren,", "tokens": ["Auf", ",", "krat\u00b7ze", "dich", "in", "dei\u00b7nen", "krau\u00b7sen", "Haa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wag' es, in die\u00df gro\u00dfe Fa\u00df zu tunken!", "tokens": ["Und", "wag'", "es", ",", "in", "die\u00df", "gro\u00b7\u00dfe", "Fa\u00df", "zu", "tun\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "PDS", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bald wirst du sehn Kom\u00f6dien draus erwachsen,", "tokens": ["Bald", "wirst", "du", "sehn", "Ko\u00b7m\u00f6\u00b7di\u00b7en", "draus", "er\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VVFIN", "PAV", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Burlesken und Roman' an's Licht gezogen,", "tokens": ["Bur\u00b7les\