{"textgrid.poem.66882": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ist des Menschen Auge tr\u00fcb verschleiert,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist des Menschen Auge tr\u00fcb verschleiert,", "tokens": ["Wie", "ist", "des", "Men\u00b7schen", "Au\u00b7ge", "tr\u00fcb", "ver\u00b7schlei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihm der Mensch in nackter Wahrheit naht!", "tokens": ["Wenn", "ihm", "der", "Mensch", "in", "nack\u00b7ter", "Wahr\u00b7heit", "naht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Treuste selbst scheint ihm ein Apostat,", "tokens": ["Der", "Treus\u00b7te", "selbst", "scheint", "ihm", "ein", "A\u00b7pos\u00b7tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der nur der Seele zarte Wandlung feiert.", "tokens": ["Der", "nur", "der", "See\u00b7le", "zar\u00b7te", "Wand\u00b7lung", "fei\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Am schnellsten steigt im Kurs, was, abgeleiert", "tokens": ["Am", "schnells\u00b7ten", "steigt", "im", "Kurs", ",", "was", ",", "ab\u00b7ge\u0