{"dta.poem.23425": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die deutschen Fahnen zu Paris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr braucht nicht Fahnen und Standarten,", "tokens": ["Ihr", "braucht", "nicht", "Fah\u00b7nen", "und", "Stand\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr habt Erinnerung genug,", "tokens": ["Ihr", "habt", "E\u00b7rin\u00b7ne\u00b7rung", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Genug, genug an Bonaparten,", "tokens": ["Ge\u00b7nug", ",", "ge\u00b7nug", "an", "Bo\u00b7na\u00b7par\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er die Welt in Fesseln schlug.", "tokens": ["Wie", "er", "die", "Welt", "in", "Fes\u00b7seln", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht durch sein Siegen, Pl\u00fcndern, Morden", "tokens": ["Nicht", "durch", "sein", "Sie\u00b7gen", ",", "Pl\u00fcn\u00b7dern", ",", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward er dereinst der Mann der Zeit;", "tokens": ["Ward", "er", "de\u00b7reinst", "der", "Mann", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist was Gro\u00dfes nur geworden", "tokens": ["Er", "ist", "was", "Gro\u00b7\u00dfes", "nur", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "NN", "ADV", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch seiner Zeit Erb\u00e4rmlichkeit.", "tokens": ["Durch", "sei\u00b7ner", "Zeit", "Er\u00b7b\u00e4rm\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr wollt kein Bild vom Zeitenlauf,", "tokens": ["Ihr", "wollt", "kein", "Bild", "vom", "Zei\u00b7ten\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst hingt ihr f\u00fcr die Fahnenfetzen", "tokens": ["Sonst", "hingt", "ihr", "f\u00fcr", "die", "Fah\u00b7nen\u00b7fet\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Euch einen deutschen Schlafrock auf.", "tokens": ["Euch", "ei\u00b7nen", "deut\u00b7schen", "Schla\u00b7frock", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}