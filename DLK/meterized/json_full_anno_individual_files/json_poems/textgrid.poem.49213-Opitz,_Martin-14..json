{"textgrid.poem.49213": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder spricht zu mir: dein Lieb ist nicht dergleichen,", "tokens": ["Ein", "je\u00b7der", "spricht", "zu", "mir", ":", "dein", "Lieb", "ist", "nicht", "derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "PTKNEG", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie du sie zwar beschreibst; ich wei\u00df es warlich nicht,", "tokens": ["Wie", "du", "sie", "zwar", "be\u00b7schreibst", ";", "ich", "wei\u00df", "es", "war\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin fast nicht mehr klug; der scharffen Sinnen Liecht", "tokens": ["Ich", "bin", "fast", "nicht", "mehr", "klug", ";", "der", "scharf\u00b7fen", "Sin\u00b7nen", "Liecht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vermag gar kaum, was wei\u00df und schwartz ist zu erreichen.", "tokens": ["Ver\u00b7mag", "gar", "kaum", ",", "was", "wei\u00df", "und", "schwartz", "ist", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PWS", "VVFIN", "KON", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der so im Lieben noch was wei\u00df herau\u00df zu streichen,", "tokens": ["Der", "so", "im", "Lie\u00b7ben", "noch", "was", "wei\u00df", "her\u00b7au\u00df", "zu", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "KON", "PWS", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Urtheil und Verstandt und kennt auch, was gebricht,", "tokens": ["Durch", "Ur\u00b7theil", "und", "Ver\u00b7standt", "und", "kennt", "auch", ",", "was", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "VVFIN", "ADV", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der liebet noch nicht recht. Wo war ist, was man spricht,", "tokens": ["Der", "lie\u00b7bet", "noch", "nicht", "recht", ".", "Wo", "war", "ist", ",", "was", "man", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "ADJD", "$.", "PWAV", "VAFIN", "VAFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat der, welcher liebt, der Sinnen gar kein Zeichen", "tokens": ["So", "hat", "der", ",", "wel\u00b7cher", "liebt", ",", "der", "Sin\u00b7nen", "gar", "kein", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "$,", "PRELS", "VVFIN", "$,", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ist ein lauter Kind. Wer Sch\u00f6nheit wehlen kan", "tokens": ["Und", "ist", "ein", "lau\u00b7ter", "Kind", ".", "Wer", "Sch\u00f6n\u00b7heit", "weh\u00b7len", "kan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "PIAT", "NN", "$.", "PWS", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und redet recht darvon, der ist ein weiser Mann.", "tokens": ["Und", "re\u00b7det", "recht", "dar\u00b7von", ",", "der", "ist", "ein", "wei\u00b7ser", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$,", "PRELS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df nicht, wie ich doch die Fantasie gelose,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "ich", "doch", "die", "Fan\u00b7ta\u00b7sie", "ge\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und was die s\u00fcsse Sucht noch endlich au\u00df mir macht;", "tokens": ["Und", "was", "die", "s\u00fcs\u00b7se", "Sucht", "noch", "end\u00b7lich", "au\u00df", "mir", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": 