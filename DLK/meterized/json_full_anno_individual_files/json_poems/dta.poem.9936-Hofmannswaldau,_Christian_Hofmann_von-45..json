{"dta.poem.9936": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin auf dem land un\u0303 meer/ dien\u2019 in frieden un\u0303 bey schlachte\u0303/", "tokens": ["Ich", "bin", "auf", "dem", "land", "u\u00f1", "meer", "/", "dien'", "in", "frie\u00b7den", "u\u00f1", "bey", "schlacht\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "KON", "NN", "$(", "ADV", "APPR", "NN", "KON", "APPR", "NE", "$("], "meter": "-+--+-++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ruhm mich offtermals begleit; ich die/ so nach ehren trachten.", "tokens": ["Ruhm", "mich", "off\u00b7ter\u00b7mals", "be\u00b7gleit", ";", "ich", "die", "/", "so", "nach", "eh\u00b7ren", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "$.", "PPER", "ART", "$(", "ADV", "APPR", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Weit und nahe/ laut und stille/ wie es kommt/ werd ich geh\u00f6rt/", "tokens": ["Weit", "und", "na\u00b7he", "/", "laut", "und", "stil\u00b7le", "/", "wie", "es", "kommt", "/", "werd", "ich", "ge\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$(", "ADJD", "KON", "VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "VAFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Allen menschen ich gefalle/ wenn mein herr ist wohl gelehrt.", "tokens": ["Al\u00b7len", "men\u00b7schen", "ich", "ge\u00b7fal\u00b7le", "/", "wenn", "mein", "herr", "ist", "wohl", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}