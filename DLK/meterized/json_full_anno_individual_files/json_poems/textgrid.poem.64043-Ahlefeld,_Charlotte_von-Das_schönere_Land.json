{"textgrid.poem.64043": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Das sch\u00f6nere Land", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So willst Du nun die traute Heimath fliehen,", "tokens": ["So", "willst", "Du", "nun", "die", "trau\u00b7te", "Hei\u00b7math", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Dir des Lebens Morgenroth getagt? \u2013", "tokens": ["Wo", "Dir", "des", "Le\u00b7bens", "Mor\u00b7gen\u00b7roth", "ge\u00b7tagt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In fern entlegne L\u00e4nder willst Du ziehen,", "tokens": ["In", "fern", "ent\u00b7leg\u00b7ne", "L\u00e4n\u00b7der", "willst", "Du", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil Dir das Schicksal manchen Wunsch versagt?", "tokens": ["Weil", "Dir", "das", "Schick\u00b7sal", "man\u00b7chen", "Wunsch", "ver\u00b7sagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So nimm den Wanderstab und suche Frieden \u2013", "tokens": ["So", "nimm", "den", "Wan\u00b7der\u00b7stab", "und", "su\u00b7che", "Frie\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Ferne lindre Deinen bittern Schmerz,", "tokens": ["Die", "Fer\u00b7ne", "lind\u00b7re", "Dei\u00b7nen", "bit\u00b7tern", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und giesse sanft, was hier Dir nicht beschieden,", "tokens": ["Und", "gies\u00b7se", "sanft", ",", "was", "hier", "Dir", "nicht", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PRELS", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Freuden F\u00fclle in Dein sehnend Herz.", "tokens": ["Der", "Freu\u00b7den", "F\u00fcl\u00b7le", "in", "Dein", "seh\u00b7nend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbmit Zuversicht hoff' ich sie dort zu finden,", "tokens": ["\u00bb", "mit", "Zu\u00b7ver\u00b7sicht", "hoff'", "ich", "sie", "dort", "zu", "fin\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn einem sch\u00f6nern Lande eil' ich zu;", "tokens": ["Denn", "ei\u00b7nem", "sch\u00f6\u00b7nern", "Lan\u00b7de", "eil'", "ich", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort weicht die Nacht vom tr\u00fcben Blick des Blinden,", "tokens": ["Dort", "weicht", "die", "Nacht", "vom", "tr\u00fc\u00b7ben", "Blick", "des", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort l\u00e4chelt dem Gequ\u00e4lten stille Ruh.", "tokens": ["Dort", "l\u00e4\u00b7chelt", "dem", "Ge\u00b7qu\u00e4l\u00b7ten", "stil\u00b7le", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort rieseln durch die Fluren Balsamquellen", "tokens": ["Dort", "rie\u00b7seln", "durch", "die", "Flu\u00b7ren", "Bal\u00b7sam\u00b7quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und heilen mild den Wandrer der sich naht,", "tokens": ["Und", "hei\u00b7len", "mild", "den", "Wand\u00b7rer", "der", "sich", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ART", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und eines lichtern Tages Strahlen hellen", "tokens": ["Und", "ei\u00b7nes", "lich\u00b7tern", "Ta\u00b7ges", "Strah\u00b7len", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir dort den dornenlosen, heitern Pfad.\u00ab", "tokens": ["Mir", "dort", "den", "dor\u00b7nen\u00b7lo\u00b7sen", ",", "hei\u00b7tern", "Pfad", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie heisst das Land, das Dir so freundlich winket?", "tokens": ["Wie", "heisst", "das", "Land", ",", "das", "Dir", "so", "freund\u00b7lich", "win\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch mir erdr\u00fcckt manch' banger Gram das Herz,", "tokens": ["Auch", "mir", "er\u00b7dr\u00fcckt", "man\u00b7ch'", "ban\u00b7ger", "Gram", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und nur durch graue Nebelwolken blinket", "tokens": ["Und", "nur", "durch", "grau\u00b7e", "Ne\u00b7bel\u00b7wol\u00b7ken", "blin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Stern der Hoffnung matt in meinen Schmerz.", "tokens": ["Der", "Stern", "der", "Hoff\u00b7nung", "matt", "in", "mei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lass mich mit Dir das sch\u00f6ne Land erreichen,", "tokens": ["Lass", "mich", "mit", "Dir", "das", "sch\u00f6\u00b7ne", "Land", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In welcher Ferne d\u00e4mmert es empor?", "tokens": ["In", "wel\u00b7cher", "Fer\u00b7ne", "d\u00e4m\u00b7mert", "es", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ach einer Zauberinsel muss es gleichen,", "tokens": ["Ach", "ei\u00b7ner", "Zau\u00b7be\u00b7rin\u00b7sel", "muss", "es", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die zum Asyl sich eine Fee erkohr.", "tokens": ["Die", "zum", "A\u00b7syl", "sich", "ei\u00b7ne", "Fee", "er\u00b7kohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbbist Du entschlossen, muthig ihm zu nahen,", "tokens": ["\u00bb", "bist", "Du", "ent\u00b7schlos\u00b7sen", ",", "mut\u00b7hig", "ihm", "zu", "na\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,", "ADJD", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wirf gleich mir des Lebens B\u00fcrde ab.", "tokens": ["So", "wirf", "gleich", "mir", "des", "Le\u00b7bens", "B\u00fcr\u00b7de", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPER", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann wird es Dich in Himmelsglanz empfahen,", "tokens": ["Dann", "wird", "es", "Dich", "in", "Him\u00b7mels\u00b7glanz", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn seine dunkle Pforte ist das Grab,", "tokens": ["Denn", "sei\u00b7ne", "dunk\u00b7le", "Pfor\u00b7te", "ist", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht schauerlich ist es hinab zu steigen,", "tokens": ["Nicht", "schau\u00b7er\u00b7lich", "ist", "es", "hin\u00b7ab", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Gern sieht man ja das dr\u00fcckende Gewand", "tokens": ["Gern", "sieht", "man", "ja", "das", "dr\u00fc\u00b7cken\u00b7de", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.7": {"text": "Am m\u00fcden Abend aufgel\u00f6st entweichen,", "tokens": ["Am", "m\u00fc\u00b7den", "A\u00b7bend", "auf\u00b7ge\u00b7l\u00f6st", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Winkt s\u00fcsse Ruh' uns an des Schlummers Hand.\u00ab", "tokens": ["Winkt", "s\u00fcs\u00b7se", "Ruh'", "uns", "an", "des", "Schlum\u00b7mers", "Hand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbder K\u00f6rper ist ein Sohn der schweren Erde \u2013", "tokens": ["\u00bb", "der", "K\u00f6r\u00b7per", "ist", "ein", "Sohn", "der", "schwe\u00b7ren", "Er\u00b7de", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sinkt zur\u00fcck in seiner Mutter Schooss;", "tokens": ["Er", "sinkt", "zu\u00b7r\u00fcck", "in", "sei\u00b7ner", "Mut\u00b7ter", "Scho\u00b7oss", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Geist, befreit von irdischer Beschwerde,", "tokens": ["Der", "Geist", ",", "be\u00b7freit", "von", "ir\u00b7di\u00b7scher", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ringt siegend sich zu ew'ger Dauer los,", "tokens": ["Ringt", "sie\u00b7gend", "sich", "zu", "ew'\u00b7ger", "Dau\u00b7er", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und aufw\u00e4rts strebend in die h\u00f6hern R\u00e4ume,", "tokens": ["Und", "auf\u00b7w\u00e4rts", "stre\u00b7bend", "in", "die", "h\u00f6\u00b7hern", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fchlt er nicht mehr den unbezwungnen Schmerz,", "tokens": ["F\u00fchlt", "er", "nicht", "mehr", "den", "un\u00b7be\u00b7zwung\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es fliehn des Lebens Bilder hin wie Tr\u00e4ume", "tokens": ["Es", "fliehn", "des", "Le\u00b7bens", "Bil\u00b7der", "hin", "wie", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKVZ", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und seine Heimath winkt ihm himmelw\u00e4rts.\u00ab", "tokens": ["Und", "sei\u00b7ne", "Hei\u00b7math", "winkt", "ihm", "him\u00b7mel\u00b7w\u00e4rts", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So willst Du nun die traute Heimath fliehen,", "tokens": ["So", "willst", "Du", "nun", "die", "trau\u00b7te", "Hei\u00b7math", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Dir des Lebens Morgenroth getagt? \u2013", "tokens": ["Wo", "Dir", "des", "Le\u00