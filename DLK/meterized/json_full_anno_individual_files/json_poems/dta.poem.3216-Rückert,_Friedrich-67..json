{"dta.poem.3216": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott, der dir manches Leid im langen Leben gab,", "tokens": ["Gott", ",", "der", "dir", "man\u00b7ches", "Leid", "im", "lan\u00b7gen", "Le\u00b7ben", "gab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PIAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und endlich Ausruh dir von allen gab im Grab,", "tokens": ["Und", "end\u00b7lich", "Aus\u00b7ruh", "dir", "von", "al\u00b7len", "gab", "im", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PPER", "APPR", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hab' ich gebeten oft, dich nur zu \u00fcberheben", "tokens": ["Hab'", "ich", "ge\u00b7be\u00b7ten", "oft", ",", "dich", "nur", "zu", "\u00fc\u00b7ber\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "ADV", "$,", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des einen, da\u00df du mich auch m\u00fcssest \u00fcberleben.", "tokens": ["Des", "ei\u00b7nen", ",", "da\u00df", "du", "mich", "auch", "m\u00fcs\u00b7sest", "\u00fc\u00b7berl\u00b7e\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "PPER", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit Gott nun hab' ich dir die Augen zugethan,", "tokens": ["Mit", "Gott", "nun", "hab'", "ich", "dir", "die", "Au\u00b7gen", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So da\u00df ich, ohne dich zu kr\u00e4nken, sterben kann.", "tokens": ["So", "da\u00df", "ich", ",", "oh\u00b7ne", "dich", "zu", "kr\u00e4n\u00b7ken", ",", "ster\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}