{"textgrid.poem.60596": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Lob des H\u00f6chsten", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Sions H\u00f6hen hin, erhebt auf Engelschwingen", "tokens": ["Zu", "Si\u00b7ons", "H\u00f6\u00b7hen", "hin", ",", "er\u00b7hebt", "auf", "En\u00b7gel\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich diese gl\u00e4nzende Gestalt?", "tokens": ["Mich", "die\u00b7se", "gl\u00e4n\u00b7zen\u00b7de", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Am Teiche Siloa soll ich dem Herrn lobsingen,", "tokens": ["Am", "Tei\u00b7che", "Si\u00b7loa", "soll", "ich", "dem", "Herrn", "lob\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im dunkeln Cedernwald?", "tokens": ["Im", "dun\u00b7keln", "Ce\u00b7dern\u00b7wald", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier, wo Isai Sohn von heiligem Vergn\u00fcgen,", "tokens": ["Hier", ",", "wo", "I\u00b7sai", "Sohn", "von", "hei\u00b7li\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NE", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Von Gott entz\u00fcckt, von Gott allein", "tokens": ["Von", "Gott", "ent\u00b7z\u00fcckt", ",", "von", "Gott", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur g\u00fcldnen Harfe sang? Und alle Winde schwiegen,", "tokens": ["Zur", "g\u00fcld\u00b7nen", "Har\u00b7fe", "sang", "?", "Und", "al\u00b7le", "Win\u00b7de", "schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$.", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und horchend schwieg der Hayn!", "tokens": ["Und", "hor\u00b7chend", "schwieg", "der", "Hayn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Cedern, schweigt umher, und rauschet nur vom weiten!", "tokens": ["Ihr", "Ce\u00b7dern", ",", "schweigt", "um\u00b7her", ",", "und", "rau\u00b7schet", "nur", "vom", "wei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn meine ganze Seele gl\u00fcht.", "tokens": ["Denn", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm will ich singen, ihm, dem Herrn, auf neuen Saiten,", "tokens": ["Ihm", "will", "ich", "sin\u00b7gen", ",", "ihm", ",", "dem", "Herrn", ",", "auf", "neu\u00b7en", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PPER", "$,", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Herrn ein hohes Lied!", "tokens": ["Dem", "Herrn", "ein", "ho\u00b7hes", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Herr, wer ist dir gleich, Gott \u00fcber alle G\u00f6tter,", "tokens": ["O", "Herr", ",", "wer", "ist", "dir", "gleich", ",", "Gott", "\u00fc\u00b7ber", "al\u00b7le", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$,", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein Gott, herrlich, weise, mild,", "tokens": ["Al\u00b7lein", "Gott", ",", "herr\u00b7lich", ",", "wei\u00b7se", ",", "mild", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJD", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gerecht, auch wann du z\u00fcrnst, und im entflammten Wetter", "tokens": ["Ge\u00b7recht", ",", "auch", "wann", "du", "z\u00fcrnst", ",", "und", "im", "ent\u00b7flamm\u00b7ten", "Wet\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PWAV", "PPER", "VVFIN", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Grimm die Erde schilt!", "tokens": ["Dein", "Grimm", "die", "Er\u00b7de", "schilt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du, du allein bist gro\u00df! Was kann der Staub dir geben?", "tokens": ["Du", ",", "du", "al\u00b7lein", "bist", "gro\u00df", "!", "Was", "kann", "der", "Staub", "dir", "ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADV", "VAFIN", "ADJD", "$.", "PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es stimme meiner Saiten Klang", "tokens": ["Es", "stim\u00b7me", "mei\u00b7ner", "Sai\u00b7ten", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Lied der Engel ein, und auch mein ganzes Leben", "tokens": ["Ins", "Lied", "der", "En\u00b7gel", "ein", ",", "und", "auch", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$,", "KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sey dir ein Lobgesang!", "tokens": ["Sey", "dir", "ein", "Lob\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zu Sions H\u00f6hen hin, erhebt auf Engelschwingen", "tokens": [