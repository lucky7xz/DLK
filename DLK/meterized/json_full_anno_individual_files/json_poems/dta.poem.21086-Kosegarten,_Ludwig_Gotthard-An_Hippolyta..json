{"dta.poem.21086": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Hippolyta.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Immer h\u00f6her, immer schwanker,               ", "tokens": ["Im\u00b7mer", "h\u00f6\u00b7her", ",", "im\u00b7mer", "schwan\u00b7ker", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blume, sprosst dein Halm empor.", "tokens": ["Blu\u00b7me", ",", "sprosst", "dein", "Halm", "em\u00b7por", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer r\u00f6ther, immer frischer", "tokens": ["Im\u00b7mer", "r\u00f6\u00b7ther", ",", "im\u00b7mer", "fri\u00b7scher"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Prangst du in dem Blumenflor.", "tokens": ["Prangst", "du", "in", "dem", "Blu\u00b7men\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aufgekost vom Lenzges\u00e4usel,", "tokens": ["Auf\u00b7ge\u00b7kost", "vom", "Lenz\u00b7ge\u00b7s\u00e4u\u00b7sel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aufgek\u00fcsst vom lauen Strahl,", "tokens": ["Auf\u00b7ge\u00b7k\u00fcsst", "vom", "lau\u00b7en", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00d6ffnest du dich tausendbl\u00e4ttrig", "tokens": ["\u00d6ff\u00b7nest", "du", "dich", "tau\u00b7send\u00b7bl\u00e4tt\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und durchduftest Hayn und Thal.", "tokens": ["Und", "durch\u00b7duf\u00b7test", "Hayn", "und", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Wer bewahrt mir meine Blume,", "tokens": ["Wer", "be\u00b7wahrt", "mir", "mei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass kein Frevel sie entweih,", "tokens": ["Dass", "kein", "Fre\u00b7vel", "sie", "ent\u00b7weih", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dass kein frecher Sturm sie knicke,", "tokens": ["Dass", "kein", "fre\u00b7cher", "Sturm", "sie", "kni\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dass der Mehlthau Schmeicheley", "tokens": ["Dass", "der", "Mehlt\u00b7hau", "Schmei\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Bl\u00e4tter nicht versenge,", "tokens": ["Ih\u00b7re", "Bl\u00e4t\u00b7ter", "nicht", "ver\u00b7sen\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dass nicht schw\u00fcle Fantasie", "tokens": ["Dass", "nicht", "schw\u00fc\u00b7le", "Fan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihres Halmes Saft verzehre,", "tokens": ["Ih\u00b7res", "Hal\u00b7mes", "Saft", "ver\u00b7zeh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wer bewahrt, wer schirmet sie?", "tokens": ["Wer", "be\u00b7wahrt", ",", "wer", "schir\u00b7met", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einen Engel und noch Einen", "tokens": ["Ei\u00b7nen", "En\u00b7gel", "und", "noch", "Ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kenn' ich, meine Lieblingin,", "tokens": ["Kenn'", "ich", ",", "mei\u00b7ne", "Lieb\u00b7lin\u00b7gin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die die Sch\u00fcchterne beschirmen:", "tokens": ["Die", "die", "Sch\u00fcch\u00b7ter\u00b7ne", "be\u00b7schir\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$."], "meter": "--+---+-", "measure": "anapaest.init"}, "line.4": {"text": "Th\u00e4tigkeit und reinen Sinn.", "tokens": ["Th\u00e4\u00b7tig\u00b7keit", "und", "rei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jene sch\u00fctzt dich vor dir selber;", "tokens": ["Je\u00b7ne", "sch\u00fctzt", "dich", "vor", "dir", "sel\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser scheucht die Frechheit weg.", "tokens": ["Die\u00b7ser", "scheucht", "die", "Frech\u00b7heit", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Beyde leiten sanft und sicher", "tokens": ["Bey\u00b7de", "lei\u00b7ten", "sanft", "und", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich der Jugend Blumenweg.", "tokens": ["Dich", "der", "Ju\u00b7gend", "Blu\u00b7men\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieblich ist der Lenz des Jahres,", "tokens": ["Lieb\u00b7lich", "ist", "der", "Lenz", "des", "Jah\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieblich, doch gef\u00e4hrlich auch.", "tokens": ["Lieb\u00b7lich", ",", "doch", "ge\u00b7f\u00e4hr\u00b7lich", "auch", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm ents\u00e4useln Gift und Balsam,", "tokens": ["Ihm", "ent\u00b7s\u00e4u\u00b7seln", "Gift", "und", "Bal\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebensodem, Todeshauch.", "tokens": ["Le\u00b7ben\u00b7so\u00b7dem", ",", "To\u00b7des\u00b7hauch", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebling ist der Lenz des Lebens,", "tokens": ["Lieb\u00b7ling", "ist", "der", "Lenz", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber auch gefahrenvoll;", "tokens": ["A\u00b7ber", "auch", "ge\u00b7fah\u00b7ren\u00b7voll", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seinem Bl\u00fcthenkelch entduften", "tokens": ["Sei\u00b7nem", "Bl\u00fc\u00b7then\u00b7kelch", "ent\u00b7duf\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lebensweh und Lebenswohl.", "tokens": ["Le\u00b7bens\u00b7weh", "und", "Le\u00b7bens\u00b7wohl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Welches Ahnen, welches Bangen", "tokens": ["Wel\u00b7ches", "Ah\u00b7nen", ",", "wel\u00b7ches", "Ban\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwellt des M\u00e4dchens junge Brust?", "tokens": ["Schwellt", "des", "M\u00e4d\u00b7chens", "jun\u00b7ge", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches unbekannte Sehnen?", "tokens": ["Wel\u00b7ches", "un\u00b7be\u00b7kann\u00b7te", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche tr\u00e4umerische Lust?", "tokens": ["Wel\u00b7che", "tr\u00e4u\u00b7me\u00b7ri\u00b7sche", "Lust", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Staunen, dieses W\u00e4hnen,", "tokens": ["Die\u00b7ses", "Stau\u00b7nen", ",", "die\u00b7ses", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses d\u00e4mmernde Gef\u00fchl", "tokens": ["Die\u00b7ses", "d\u00e4m\u00b7mern\u00b7de", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ruf' es an das Licht, Geliebte!", "tokens": ["Ruf'", "es", "an", "das", "Licht", ",", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gib ihm Namen, Zweck und Ziel!", "tokens": ["Gib", "ihm", "Na\u00b7men", ",", "Zweck", "und", "Ziel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jedes Sehnen, meine Theure,", "tokens": ["Je\u00b7des", "Seh\u00b7nen", ",", "mei\u00b7ne", "Theu\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das du dir nicht laut bekennst,", "tokens": ["Das", "du", "dir", "nicht", "laut", "be\u00b7kennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder sch\u00fcchterne Gedanke,", "tokens": ["Je\u00b7der", "sch\u00fcch\u00b7ter\u00b7ne", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den du deinem Gott nicht nennst,", "tokens": ["Den", "du", "dei\u00b7nem", "Gott", "nicht", "nennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Wunsch, der scheu und bl\u00f6de", "tokens": ["Je\u00b7der", "Wunsch", ",", "der", "scheu", "und", "bl\u00f6\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus des Reinen Gegenwart", "tokens": ["Aus", "des", "Rei\u00b7nen", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wegbebt und ins Dunkel fl\u00fcchtet \u2014", "tokens": ["Weg\u00b7bebt", "und", "ins", "Dun\u00b7kel", "fl\u00fcch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "APPRART", "NN", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Freundin, ist nicht lautrer Art.", "tokens": ["Freun\u00b7din", ",", "ist", "nicht", "laut\u00b7rer", "Art", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jede Stimmung, meine Traute,", "tokens": ["Je\u00b7de", "Stim\u00b7mung", ",", "mei\u00b7ne", "Trau\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in Tr\u00fcbsinn sich verstimmt,", "tokens": ["Die", "in", "Tr\u00fcb\u00b7sinn", "sich", "ver\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Itzt in tr\u00e4gem Traum sich wieget,", "tokens": ["Itzt", "in", "tr\u00e4\u00b7gem", "Traum", "sich", "wie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Itzt in feigen Thr\u00e4nen schwimmt,", "tokens": ["Itzt", "in", "fei\u00b7gen", "Thr\u00e4\u00b7nen", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der Seele kranke Fibern", "tokens": ["Die", "der", "See\u00b7le", "kran\u00b7ke", "Fi\u00b7bern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Itzt erschlaffet, itzt verspannt,", "tokens": ["Itzt", "er\u00b7schlaf\u00b7fet", ",", "itzt", "ver\u00b7spannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stammt vom Erebus, und werde", "tokens": ["Stammt", "vom", "E\u00b7re\u00b7bus", ",", "und", "wer\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "KON", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den Erebus verbannt.", "tokens": ["In", "den", "E\u00b7re\u00b7bus", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jeder Geck, der sch\u00f6ngegl\u00e4ttet,", "tokens": ["Je\u00b7der", "Geck", ",", "der", "sch\u00f6n\u00b7ge\u00b7gl\u00e4t\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ngefirnisst um dich kriecht,", "tokens": ["Sch\u00f6n\u00b7ge\u00b7fir\u00b7nisst", "um", "dich", "kriecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich in jeder deiner Launen", "tokens": ["Sich", "in", "je\u00b7der", "dei\u00b7ner", "Lau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlangenschmeidig schmiegt und f\u00fcgt,", "tokens": ["Schlan\u00b7gen\u00b7schmei\