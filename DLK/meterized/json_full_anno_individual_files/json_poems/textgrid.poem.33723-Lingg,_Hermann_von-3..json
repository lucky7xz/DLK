{"textgrid.poem.33723": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesteht, da\u00df ich die Schranken \u00fcbersprungen,", "tokens": ["Ge\u00b7steht", ",", "da\u00df", "ich", "die", "Schran\u00b7ken", "\u00fc\u00b7bers\u00b7prun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Raum, in welchem eure Vorsicht wollte,", "tokens": ["Den", "Raum", ",", "in", "wel\u00b7chem", "eu\u00b7re", "Vor\u00b7sicht", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich mein Gl\u00fcck nur darin finden sollte,", "tokens": ["Da\u00df", "ich", "mein", "Gl\u00fcck", "nur", "da\u00b7rin", "fin\u00b7den", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein gut Gesch\u00f6pf zu sein, das euch gelungen!", "tokens": ["Ein", "gut", "Ge\u00b7sch\u00f6pf", "zu", "sein", ",", "das", "euch", "ge\u00b7lun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VAINF", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Menschen Stolz, die Freiheit wird erzwungen;", "tokens": ["Des", "Men\u00b7schen", "Stolz", ",", "die", "Frei\u00b7heit", "wird", "er\u00b7zwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch keine Macht gab's, die nicht heimlich grollte,", "tokens": ["Noch", "kei\u00b7ne", "Macht", "gab's", ",", "die", "nicht", "heim\u00b7lich", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn eine j\u00fcngre, die bisher ihr zollte,", "tokens": ["Wenn", "ei\u00b7ne", "j\u00fcng\u00b7re", ",", "die", "bis\u00b7her", "ihr", "zoll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun sich auf einmal \u00fcber sie geschwungen.", "tokens": ["Nun", "sich", "auf", "ein\u00b7mal", "\u00fc\u00b7ber", "sie", "ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Durch Widerstand erw\u00e4chst die Wucht der Eiche,", "tokens": ["Durch", "Wi\u00b7der\u00b7stand", "er\u00b7w\u00e4chst", "die", "Wucht", "der", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Eisen wird geh\u00e4rtet in den Feuern,", "tokens": ["Das", "Ei\u00b7sen", "wird", "ge\u00b7h\u00e4r\u00b7tet", "in", "den", "Feu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und glaubt ihr nicht vom Menschengeist das Gleiche?", "tokens": ["Und", "glaubt", "ihr", "nicht", "vom", "Men\u00b7schen\u00b7geist", "das", "Glei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Wort, mit dem durch jedes Meer wir steuern:", "tokens": ["Das", "Wort", ",", "mit", "dem", "durch", "je\u00b7des", "Meer", "wir", "steu\u00b7ern", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "APPR", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df jeder Widerstand der Tatkraft weiche,", "tokens": ["Da\u00df", "je\u00b7der", "Wi\u00b7der\u00b7stand", "der", "Tat\u00b7kraft", "wei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dies gibt der Welt ihr ewig Selbsterneuern.", "tokens": ["Dies", "gibt", "der", "Welt", "ihr", "e\u00b7wig", "Selbs\u00b7ter\u00b7neu\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gesteht, da\u00df ich die Schranken \u00fcbersprungen,", "tokens": ["Ge\u00b7steht", ",", "da\u00df", "ich", "