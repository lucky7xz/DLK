{"textgrid.poem.52170": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Seelchen", "genre": "verse", "period": "N.A.", "pub_year": 1871, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lag im Gras auf einer Alp,", "tokens": ["Ich", "lag", "im", "Gras", "auf", "ei\u00b7ner", "Alp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sel'ge Bl\u00e4uen starrt ich auf \u2013", "tokens": ["In", "sel'\u00b7ge", "Bl\u00e4u\u00b7en", "starrt", "ich", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir war, als ob auf meiner Brust", "tokens": ["Mir", "war", ",", "als", "ob", "auf", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich etwas sacht betastete.", "tokens": ["Mich", "et\u00b7was", "sacht", "be\u00b7tas\u00b7te\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich blickte schr\u00e4g. Ein Falter sa\u00df", "tokens": ["Ich", "blick\u00b7te", "schr\u00e4g", ".", "Ein", "Fal\u00b7ter", "sa\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf meinem grauen Wanderrock.", "tokens": ["Auf", "mei\u00b7nem", "grau\u00b7en", "Wan\u00b7der\u00b7rock", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Seelchen war's, das flugbereit,", "tokens": ["Mein", "Seel\u00b7chen", "wa\u00b7r's", ",", "das", "flug\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Schwingen \u00f6ffnend, zitterte.", "tokens": ["Die", "Schwin\u00b7gen", "\u00f6ff\u00b7nend", ",", "zit\u00b7ter\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sind die Schwingen ihm gef\u00e4rbt?", "tokens": ["Wie", "sind", "die", "Schwin\u00b7gen", "ihm", "ge\u00b7f\u00e4rbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie leuchten blank, betupft mit Blut.", "tokens": ["Sie", "leuch\u00b7ten", "blank", ",", "be\u00b7tupft", "mit", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich lag im Gras auf einer Alp,", "tokens": ["Ich", "lag", "im", "Gras", "auf", "ei\u00b7ner", "Alp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sel'ge Bl\u00e4uen starrt ich auf \u2013", "tokens": ["In", "sel'\