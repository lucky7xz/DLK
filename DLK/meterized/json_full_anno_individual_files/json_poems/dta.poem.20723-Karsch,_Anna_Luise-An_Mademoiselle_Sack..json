{"dta.poem.20723": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  Mademoiselle Sack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Priesterkind! den Musen nachzusteigen,               ", "tokens": ["Du", "Pries\u00b7ter\u00b7kind", "!", "den", "Mu\u00b7sen", "nach\u00b7zu\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist eines Wandrers M\u00fch, der, Berge die sich zeigen,", "tokens": ["Ist", "ei\u00b7nes", "Wand\u00b7rers", "M\u00fch", ",", "der", ",", "Ber\u00b7ge", "die", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "$,", "PRELS", "$,", "NN", "ART", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Leicht zu erforschen denkt, er gehet Tage lang", "tokens": ["Leicht", "zu", "er\u00b7for\u00b7schen", "denkt", ",", "er", "ge\u00b7het", "Ta\u00b7ge", "lang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "VVFIN", "$,", "PPER", "VVFIN", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist noch nicht dem Berg am Fu\u00df gekommen,", "tokens": ["Und", "ist", "noch", "nicht", "dem", "Berg", "am", "Fu\u00df", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von welchem er das Haupt so deutlich wahrgenommen:", "tokens": ["Von", "wel\u00b7chem", "er", "das", "Haupt", "so", "deut\u00b7lich", "wahr\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Solch ein beschwerlich Werk ists, ehe der Gesang", "tokens": ["Solch", "ein", "be\u00b7schwer\u00b7lich", "Werk", "ists", ",", "e\u00b7he", "der", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ART", "ADJD", "NN", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Lesens w\u00fcrdig wird. Da murren selbst die S\u00f6hne", "tokens": ["Des", "Le\u00b7sens", "w\u00fcr\u00b7dig", "wird", ".", "Da", "mur\u00b7ren", "selbst", "die", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$.", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Apollens auf den Mislaut ihrer T\u00f6ne.", "tokens": ["A\u00b7pol\u00b7lens", "auf", "den", "Mis\u00b7laut", "ih\u00b7rer", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Eh bei mir selbst mein Lied den halben Beifall fand,", "tokens": ["Eh", "bei", "mir", "selbst", "mein", "Lied", "den", "hal\u00b7ben", "Bei\u00b7fall", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sind sieben Lenze hingegr\u00fcnet,", "tokens": ["Sind", "sie\u00b7ben", "Len\u00b7ze", "hin\u00b7ge\u00b7gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und ob es bei der Welt den Beifall noch verdienet,", "tokens": ["Und", "ob", "es", "bei", "der", "Welt", "den", "Bei\u00b7fall", "noch", "ver\u00b7die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bleibt mir noch zweifelhaft, noch immer unbekannt.", "tokens": ["Bleibt", "mir", "noch", "zwei\u00b7fel\u00b7haft", ",", "noch", "im\u00b7mer", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Von der Natur gewaltig fortgetrieben", "tokens": ["Von", "der", "Na\u00b7tur", "ge\u00b7wal\u00b7tig", "fort\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Hab ich Gedanken hingeschrieben,", "tokens": ["Hab", "ich", "Ge\u00b7dan\u00b7ken", "hin\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Erst niedrig, ohne Schmuck, und rauh,", "tokens": ["Erst", "nied\u00b7rig", ",", "oh\u00b7ne", "Schmuck", ",", "und", "rauh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUI", "NN", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ich w\u00e4hlte, ich verwarf; so wirft ein Kind den Bau", "tokens": ["Ich", "w\u00e4hl\u00b7te", ",", "ich", "ver\u00b7warf", ";", "so", "wirft", "ein", "Kind", "den", "Bau"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von Karten auf dem Tische nieder,", "tokens": ["Von", "Kar\u00b7ten", "auf", "dem", "Ti\u00b7sche", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Hebt schnell die Bl\u00e4tter auf, und baut das H\u00e4uschen", "tokens": ["Hebt", "schnell", "die", "Bl\u00e4t\u00b7ter", "auf", ",", "und", "baut", "das", "H\u00e4usc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "wieder", "tokens": ["wie\u00b7der"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Und freuet des Gesch\u00f6pfes sich.", "tokens": ["Und", "freu\u00b7et", "des", "Ge\u00b7sch\u00f6p\u00b7fes", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ich baute eben so, war k\u00fchn, blieb unverdrossen,", "tokens": ["Ich", "bau\u00b7te", "e\u00b7ben", "so", ",", "war", "k\u00fchn", ",", "blieb", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "VAFIN", "ADJD", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn gleich mein Lied nicht Meisterliedern glich;", "tokens": ["Wenn", "gleich", "mein", "Lied", "nicht", "Meis\u00b7ter\u00b7lie\u00b7dern", "glich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Du aber wurdest \u00e4rgerlich,", "tokens": ["Du", "a\u00b7ber", "wur\u00b7dest", "\u00e4r\u00b7ger\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "War Dir der Vers nicht halb geflossen.", "tokens": ["War", "Dir", "der", "Vers", "nicht", "halb", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sei dankbar der Natur; sie gab Dir F\u00e4higkeit", "tokens": ["Sei", "dank\u00b7bar", "der", "Na\u00b7tur", ";", "sie", "gab", "Dir", "F\u00e4\u00b7hig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zu Werken, die ich nicht verstehe.", "tokens": ["Zu", "Wer\u00b7ken", ",", "die", "ich", "nicht", "ver\u00b7ste\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Du machst Dir selbst im Anwand Deiner Zeit", "tokens": ["Du", "machst", "Dir", "selbst", "im", "An\u00b7wand", "Dei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Den wei\u00dfen Puz, den ich im Laden suchen gehe.", "tokens": ["Den", "wei\u00b7\u00dfen", "Puz", ",", "den", "ich", "im", "La\u00b7den", "su\u00b7chen", "ge\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sei gl\u00fccklich in der Welt! ward nicht die Muse Dein,", "tokens": ["Sei", "gl\u00fcck\u00b7lich", "in", "der", "Welt", "!", "ward", "nicht", "die", "Mu\u00b7se", "Dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "NN", "$.", "VAFIN", "PTKNEG", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So soll ein Musenfreund Dir mehr als Ph\u00f6bus seyn!", "tokens": ["So", "soll", "ein", "Mu\u00b7sen\u00b7freund", "Dir", "mehr", "als", "Ph\u00f6\u00b7bus", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "PIAT", "KOKOM", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}