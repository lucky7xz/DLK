{"dta.poem.166": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie im Garten gerne bey sich haben  \n wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! linder Zephyr auf! mach dich zur Anemonen,", "tokens": ["Auf", "!", "lin\u00b7der", "Ze\u00b7phyr", "auf", "!", "mach", "dich", "zur", "A\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ADJA", "NN", "PTKVZ", "$.", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wie du ", "tokens": ["So", "wie", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Besiehe jeden Busch/ durchsuche Stadt und Wald/", "tokens": ["Be\u00b7sie\u00b7he", "je\u00b7den", "Busch", "/", "durch\u00b7su\u00b7che", "Stadt", "und", "Wald", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$(", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieh in den Kammern zu/ darinn sie pflegt zu wohnen.", "tokens": ["Sieh", "in", "den", "Kam\u00b7mern", "zu", "/", "da\u00b7rinn", "sie", "pflegt", "zu", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKZU", "$(", "PAV", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "In ihren Angesicht. Die g\u00f6ttliche Gestalt/", "tokens": ["In", "ih\u00b7ren", "An\u00b7ge\u00b7sicht", ".", "Die", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verr\u00e4th das Himmels-Bild/ wenn du es siehest/ bald/", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "das", "Him\u00b7mels\u00b7Bild", "/", "wenn", "du", "es", "sie\u00b7hest", "/", "bald", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Denn ein beliebter Blick den andern pflegt zu lohnen.", "tokens": ["Denn", "ein", "be\u00b7lieb\u00b7ter", "Blick", "den", "an\u00b7dern", "pflegt", "zu", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nimm sie/ wenn du sie siehst/ in deinem sanfften Schoo\u00df/", "tokens": ["Nimm", "sie", "/", "wenn", "du", "sie", "siehst", "/", "in", "dei\u00b7nem", "sanff\u00b7ten", "Schoo\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOUS", "PPER", "PPER", "VVFIN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verh\u00fclle sie in dich/ bedeck ihr Angesichte/", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "sie", "in", "dich", "/", "be\u00b7deck", "ihr", "An\u00b7ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$(", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wehr da\u00df nicht tr\u00fcbe Lufft auf meine Sch\u00f6ne sto\u00df.", "tokens": ["Wehr", "da\u00df", "nicht", "tr\u00fc\u00b7be", "Lufft", "auf", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PTKNEG", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Ach! ", "tokens": ["Ach", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Nimm ", "tokens": ["Nimm"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Bring mir den Himmel her/ mit doppelt hellem Lichte.", "tokens": ["Bring", "mir", "den", "Him\u00b7mel", "her", "/", "mit", "dop\u00b7pelt", "hel\u00b7lem", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}}}}