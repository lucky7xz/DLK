{"textgrid.poem.41247": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Witz und Tugend", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n ist nicht Homer, der Dichter aller Zeiten,", "tokens": ["Wie", "sch\u00f6n", "ist", "nicht", "Ho\u00b7mer", ",", "der", "Dich\u00b7ter", "al\u00b7ler", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PTKNEG", "NE", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie reizend, wie gelehrt, wie reich an Trefflichkeiten!", "tokens": ["Wie", "rei\u00b7zend", ",", "wie", "ge\u00b7lehrt", ",", "wie", "reich", "an", "Treff\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "PWAV", "VVPP", "$,", "PWAV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch auch nur eine That rechtschaffner Menschenhuld,", "tokens": ["Doch", "auch", "nur", "ei\u00b7ne", "That", "recht\u00b7schaff\u00b7ner", "Men\u00b7schen\u00b7huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der wahren M\u00e4\u00dfigung, der Gro\u00dfmuth, der Geduld,", "tokens": ["Der", "wah\u00b7ren", "M\u00e4\u00b7\u00dfi\u00b7gung", ",", "der", "Gro\u00df\u00b7muth", ",", "der", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART"