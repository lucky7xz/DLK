{"textgrid.poem.39872": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und drau\u00dfen pfeift ihm zu der Sturm,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und drau\u00dfen pfeift ihm zu der Sturm,", "tokens": ["Und", "drau\u00b7\u00dfen", "pfeift", "ihm", "zu", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es spinnt ihn ein der Regen,", "tokens": ["Es", "spinnt", "ihn", "ein", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es sausen ihm die Speere nach,", "tokens": ["Es", "sau\u00b7sen", "ihm", "die", "Spee\u00b7re", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klirren Schwerter entgegen.", "tokens": ["Und", "klir\u00b7ren", "Schwer\u00b7ter", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In Wind und Wetter schickt nach ihm", "tokens": ["In", "Wind", "und", "Wet\u00b7ter", "schickt", "nach", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Greisen Fl\u00fcche der Norden;", "tokens": ["Des", "Grei\u00b7sen", "Fl\u00fc\u00b7che", "der", "Nor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die K\u00e4mpfer hielten \u00fcber ihn Tag,", "tokens": ["Die", "K\u00e4mp\u00b7fer", "hiel\u00b7ten", "\u00fc\u00b7ber", "ihn", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und friedlos ist er worden.", "tokens": ["Und", "fried\u00b7los", "ist", "er", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er schweifet in den Kl\u00fcften um,", "tokens": ["Er", "schwei\u00b7fet", "in", "den", "Kl\u00fcf\u00b7ten", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht Wohnung in den W\u00e4ldern,", "tokens": ["Sucht", "Woh\u00b7nung", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In sp\u00e4ter Abendd\u00e4mmrung Grau'n", "tokens": ["In", "sp\u00e4\u00b7ter", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "Grau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wagt er sich nach den Feldern.", "tokens": ["Wagt", "er", "sich", "nach", "den", "Fel\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Da kehrt er bei den K\u00e4mpen ein,", "tokens": ["Da", "kehrt", "er", "bei", "den", "K\u00e4m\u00b7pen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft Salz und Brod sich geben,", "tokens": ["L\u00e4\u00dft", "Salz", "und", "Brod", "sich", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er deckt die Augen mit der Hand", "tokens": ["Er", "deckt", "die", "Au\u00b7gen", "mit", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und i\u00dft mit Hast und Beben.", "tokens": ["Und", "i\u00dft", "mit", "Hast", "und", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch z\u00fcndet man die Lampen an,", "tokens": ["Doch", "z\u00fcn\u00b7det", "man", "die", "Lam\u00b7pen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So f\u00e4hrt er auf vom Sitze,", "tokens": ["So", "f\u00e4hrt", "er", "auf", "vom", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nicht verratend ihm der Stral", "tokens": ["Da\u00df", "nicht", "ver\u00b7ra\u00b7tend", "ihm", "der", "Stral"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "VVPP", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In's M\u00f6rderantlitz blitze.", "tokens": ["In's", "M\u00f6r\u00b7der\u00b7ant\u00b7litz", "blit\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Entwichen ist er auf die Flur \u2013", "tokens": ["Ent\u00b7wi\u00b7chen", "ist", "er", "auf", "die", "Flur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit ihm Brod gebrochen,", "tokens": ["Die", "mit", "ihm", "Brod", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wetzen das Messer hinter ihm;", "tokens": ["Sie", "wet\u00b7zen", "das", "Mes\u00b7ser", "hin\u00b7ter", "ihm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Schuld will sein gerochen.", "tokens": ["Die", "Schuld", "will", "sein", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So scheucht's ihn in dem Land umher", "tokens": ["So", "scheucht's", "ihn", "in", "dem", "Land", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcnf sch\u00f6ne J\u00fcnglingsjahre;", "tokens": ["F\u00fcnf", "sch\u00f6\u00b7ne", "J\u00fcng\u00b7lings\u00b7jah\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm kommt kein Becher mehr zur Hand,", "tokens": ["Ihm", "kommt", "kein", "Be\u00b7cher", "mehr", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Kranz mehr in die Haare.", "tokens": ["Kein", "Kranz", "mehr", "in", "die", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Bei seinen Feinden wohnt die Braut,", "tokens": ["Bei", "sei\u00b7nen", "Fein\u00b7den", "wohnt", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wei\u00df nicht, was sie treibet.", "tokens": ["Er", "wei\u00df", "nicht", ",", "was", "sie", "trei\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wei\u00df nicht, ob sie weint oder lacht,", "tokens": ["Er", "wei\u00df", "nicht", ",", "ob", "sie", "weint", "o\u00b7der", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und ob sie ein Anderer weibet.", "tokens": ["Und", "ob", "sie", "ein", "An\u00b7de\u00b7rer", "wei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Und wie das f\u00fcnfte Jahr ist um,", "tokens": ["Und", "wie", "das", "f\u00fcnf\u00b7te", "Jahr", "ist", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VAFIN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wankt er zu Thorsteins Schwelle;", "tokens": ["Wankt", "er", "zu", "Thor\u00b7steins", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der blinde Greis, dort sitzt er noch", "tokens": ["Der", "blin\u00b7de", "Greis", ",", "dort", "sitzt", "er", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gram auf der alten Stelle.", "tokens": ["Im", "Gram", "auf", "der", "al\u00b7ten", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Da st\u00fcrzt der J\u00fcngling vor ihn hin:", "tokens": ["Da", "st\u00fcrzt", "der", "J\u00fcng\u00b7ling", "vor", "ihn", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbbei dir ist kein Vergeben,", "tokens": ["\u00bb", "bei", "dir", "ist", "kein", "Ver\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich lege mein Haupt in deinen Schoos,", "tokens": ["Ich", "le\u00b7ge", "mein", "Haupt", "in", "dei\u00b7nen", "Schoos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Fluch l\u00e4\u00dft mich nicht leben.\u00ab", "tokens": ["Dein", "Fluch", "l\u00e4\u00dft", "mich", "nicht", "le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dem Greise zuckt's wie Jugendkraft", "tokens": ["Dem", "Grei\u00b7se", "zuckt's", "wie", "Ju\u00b7gend\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seinen welken Armen,", "tokens": ["In", "sei\u00b7nen", "wel\u00b7ken", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die F\u00e4uste fassen des Feindes Haupt,", "tokens": ["Die", "F\u00e4us\u00b7te", "fas\u00b7sen", "des", "Fein\u00b7des", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie fassen es ohn' Erbarmen.", "tokens": ["Sie", "fas\u00b7sen", "es", "ohn'", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Doch als er hielt so fest gedr\u00fcckt", "tokens": ["Doch", "als", "er", "hielt", "so", "fest", "ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Haupt an seinen Lenden,", "tokens": ["Das", "Haupt", "an", "sei\u00b7nen", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am warmen Leben schaudert's ihn", "tokens": ["Am", "war\u00b7men", "Le\u00b7ben", "schau\u00b7dert's", "ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Fluch doch zu vollenden.", "tokens": ["Den", "Fluch", "doch", "zu", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da kommt sein junger Enkel auch", "tokens": ["Da", "kommt", "sein", "jun\u00b7ger", "En\u00b7kel", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Kindeslust gesprungen,", "tokens": ["In", "Kin\u00b7des\u00b7lust", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und um den Fremdling, wie zum Schutz,", "tokens": ["Und", "um", "den", "Fremd\u00b7ling", ",", "wie", "zum", "Schutz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4lt er den Arm geschlungen.", "tokens": ["H\u00e4lt", "er", "den", "Arm", "ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Jetzt will dem Alten, aufgethaut,", "tokens": ["Jetzt", "will", "dem", "Al\u00b7ten", ",", "auf\u00b7ge\u00b7thaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Faust nicht l\u00e4nger sich ballen,", "tokens": ["Die", "Faust", "nicht", "l\u00e4n\u00b7ger", "sich", "bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jetzt l\u00e4\u00dft er \u00fcber des J\u00fcnglings Haupt", "tokens": ["Jetzt", "l\u00e4\u00dft", "er", "\u00fc\u00b7ber", "des", "J\u00fcng\u00b7lings", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Finger spielend wallen:", "tokens": ["Die", "Fin\u00b7ger", "spie\u00b7lend", "wal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "\u00bbdeine Wang' ist weich, deine Stirn' ist hoch,", "tokens": ["\u00bb", "dei\u00b7ne", "Wang'", "ist", "weich", ",", "dei\u00b7ne", "Stirn'", "ist", "hoch", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dein Haar ist lang und flachsen;", "tokens": ["Dein", "Haar", "ist", "lang", "und", "flach\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es sitzt das Haupt am besten doch", "tokens": ["Es", "sitzt", "das", "Haupt", "am", "bes\u00b7ten", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKA", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da, wo es ist gewachsen.", "tokens": ["Da", ",", "wo", "es", "ist", "ge\u00b7wach\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ja, trag' es auf dem schlanken Hals", "tokens": ["Ja", ",", "trag'", "es", "auf", "dem", "schlan\u00b7ken", "Hals"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem Hof und Garten:", "tokens": ["In", "mei\u00b7nem", "Hof", "und", "Gar\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du sollst an Sohnes Statt mein Feld,", "tokens": ["Du", "sollst", "an", "Soh\u00b7nes", "Statt", "mein", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lang' ich's will, mir warten!", "tokens": ["So", "lang'", "ich's", "will", ",", "mir", "war\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VMFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "F\u00e4ll' Holz aus meinem Walde dort,", "tokens": ["F\u00e4ll'", "Holz", "aus", "mei\u00b7nem", "Wal\u00b7de", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bau' dir ein Haus daneben!", "tokens": ["Bau'", "dir", "ein", "Haus", "da\u00b7ne\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt wird mir wohl und d\u00e4ucht mir gar,", "tokens": ["Jetzt", "wird", "mir", "wohl", "und", "d\u00e4ucht", "mir", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Kind sei wieder am Leben.\u00ab", "tokens": ["Mein", "Kind", "sei", "wie\u00b7der", "am", "Le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "Der J\u00fcngling schnellte sein Haupt empor,", "tokens": ["Der", "J\u00fcng\u00b7ling", "schnell\u00b7te", "sein", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat rasch sich aufgeschwungen.", "tokens": ["Hat", "rasch", "sich", "auf\u00b7ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PRF", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem blinden Greisen die Z\u00e4hr' entquoll,", "tokens": ["Dem", "blin\u00b7den", "Grei\u00b7sen", "die", "Z\u00e4hr'", "ent\u00b7quoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Thr\u00e4ne str\u00f6mte dem Jungen.", "tokens": ["Die", "Thr\u00e4\u00b7ne", "str\u00f6m\u00b7te", "dem", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.19": {"line.1": {"text": "Und drau\u00dfen pfeift ihm zu der Sturm,", "tokens": ["Und", "drau\u00b7\u00dfen", "pfeift", "ihm", "zu", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es spinnt ihn ein der Regen,", "tokens": ["Es", "spinnt", "ihn", "ein", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es sausen ihm die Speere nach,", "tokens": ["Es", "sau\u00b7sen", "ihm", "die", "Spee\u00b7re", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klirren Schwerter entgegen.", "tokens": ["Und", "klir\u00b7ren", "Schwer\u00b7ter", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.20": {"line.1": {"text": "In Wind und Wetter schickt nach ihm", "tokens": ["In", "Wind", "und", "Wet\u00b7ter", "schickt", "nach", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Greisen Fl\u00fcche der Norden;", "tokens": ["Des", "Grei\u00b7sen", "Fl\u00fc\u00b7che", "der", "Nor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die K\u00e4mpfer hielten \u00fcber ihn Tag,", "tokens": ["Die", "K\u00e4mp\u00b7fer", "hiel\u00b7ten", "\u00fc\u00b7ber", "ihn", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und friedlos ist er worden.", "tokens": ["Und", "fried\u00b7los", "ist", "er", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Er schweifet in den Kl\u00fcften um,", "tokens": ["Er", "schwei\u00b7fet", "in", "den", "Kl\u00fcf\u00b7ten", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht Wohnung in den W\u00e4ldern,", "tokens": ["Sucht", "Woh\u00b7nung", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In sp\u00e4ter Abendd\u00e4mmrung Grau'n", "tokens": ["In", "sp\u00e4\u00b7ter", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "Grau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wagt er sich nach den Feldern.", "tokens": ["Wagt", "er", "sich", "nach", "den", "Fel\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.22": {"line.1": {"text": "Da kehrt er bei den K\u00e4mpen ein,", "tokens": ["Da", "kehrt", "er", "bei", "den", "K\u00e4m\u00b7pen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft Salz und Brod sich geben,", "tokens": ["L\u00e4\u00dft", "Salz", "und", "Brod", "sich", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text