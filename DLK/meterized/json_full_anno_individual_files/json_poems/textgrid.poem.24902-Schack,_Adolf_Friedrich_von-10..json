{"textgrid.poem.24902": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abendliche Geister wandeln", "tokens": ["A\u00b7bend\u00b7li\u00b7che", "Geis\u00b7ter", "wan\u00b7deln"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Durch das Laubwerk hin und wieder,", "tokens": ["Durch", "das", "Laub\u00b7werk", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, berauscht vom Duft der Mandeln,", "tokens": ["Doch", ",", "be\u00b7rauscht", "vom", "Duft", "der", "Man\u00b7deln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinken sie in Schlummer nieder.", "tokens": ["Sin\u00b7ken", "sie", "in", "Schlum\u00b7mer", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Funkelnd, gro\u00df wie eine Sonne,", "tokens": ["Fun\u00b7kelnd", ",", "gro\u00df", "wie", "ei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gie\u00dft der Wunderstern vom S\u00fcden,", "tokens": ["Gie\u00dft", "der", "Wun\u00b7ders\u00b7tern", "vom", "S\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Gie\u00dft Canopus s\u00fc\u00dfre Wonne,", "tokens": ["Gie\u00dft", "Ca\u00b7no\u00b7pus", "s\u00fc\u00df\u00b7re", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dfern Traumglanz auf die M\u00fcden.", "tokens": ["Hei\u00b7\u00dfern", "Traum\u00b7glanz", "auf", "die", "M\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun noch einmal, Nacht der N\u00e4chte,", "tokens": ["Nun", "noch", "ein\u00b7mal", ",", "Nacht", "der", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Zauberweib vom Morgenlande,", "tokens": ["Zau\u00b7ber\u00b7weib", "vom", "Mor\u00b7gen\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeig noch einmal dich als echte", "tokens": ["Zeig", "noch", "ein\u00b7mal", "dich", "als", "ech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "PPER", "KOUS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sultanin im Prachtgewande!", "tokens": ["Sul\u00b7ta\u00b7nin", "im", "Pracht\u00b7ge\u00b7wan\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einmal noch im Purpurflore,", "tokens": ["Ein\u00b7mal", "noch", "im", "Pur\u00b7pur\u00b7flo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der um Thal und H\u00fcgel walle,", "tokens": ["Der", "um", "Thal", "und", "H\u00fc\u00b7gel", "wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zieh herein durch diese Thore", "tokens": ["Zieh", "her\u00b7ein", "durch", "die\u00b7se", "Tho\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu der alten K\u00f6nigshalle!", "tokens": ["Zu", "der", "al\u00b7ten", "K\u00f6\u00b7nigs\u00b7hal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Feur'ge Meteore lasse", "tokens": ["Feur'\u00b7ge", "Me\u00b7te\u00b7o\u00b7re", "las\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Himmelsw\u00f6lbung schie\u00dfen", "tokens": ["Durch", "die", "Him\u00b7mels\u00b7w\u00f6l\u00b7bung", "schie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf G\u00e4rten und Terrasse", "tokens": ["Und", "auf", "G\u00e4r\u00b7ten", "und", "Ter\u00b7ras\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Rote Flammen niedergie\u00dfen!", "tokens": ["Ro\u00b7te", "Flam\u00b7men", "nie\u00b7der\u00b7gie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bunte Wunderlampen h\u00e4nge,", "tokens": ["Bun\u00b7te", "Wun\u00b7der\u00b7lam\u00b7pen", "h\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie Aladdin besessen,", "tokens": ["Wie", "sie", "A\u00b7lad\u00b7din", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "In die Lauben, in die G\u00e4nge,", "tokens": ["In", "die", "Lau\u00b7ben", ",", "in", "die", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An die Zweige der Cypressen!", "tokens": ["An", "die", "Zwei\u00b7ge", "der", "Cyp\u00b7res\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wirf empor die Silberwellen", "tokens": ["Wirf", "em\u00b7por", "die", "Sil\u00b7ber\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus den Alabasterschalen,", "tokens": ["Aus", "den", "A\u00b7la\u00b7bas\u00b7ter\u00b7scha\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie hell wie Naphthaquellen", "tokens": ["Da\u00df", "sie", "hell", "wie", "Naph\u00b7thaq\u00b7uel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der G\u00e4rten D\u00e4mmrung strahlen!", "tokens": ["Durch", "der", "G\u00e4r\u00b7ten", "D\u00e4mm\u00b7rung", "strah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf den fl\u00fcssigen Krystallen,", "tokens": ["Auf", "den", "fl\u00fcs\u00b7si\u00b7gen", "Krys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie kreisend sich verschlingen,", "tokens": ["Wie", "sie", "krei\u00b7send", "sich", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie steigen, wie sie fallen,", "tokens": ["Wie", "sie", "stei\u00b7gen", ",", "wie", "sie", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag ein Lied des Ostens klingen!", "tokens": ["Mag", "ein", "Lied", "des", "Os\u00b7tens", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja, du nahst dich! Durch die Cedern", "tokens": ["Ja", ",", "du", "nahst", "dich", "!", "Durch", "die", "Ce\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4uselt wollustvolles Fl\u00fcstern,", "tokens": ["S\u00e4u\u00b7selt", "wol\u00b7lust\u00b7vol\u00b7les", "Fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00e4tschernd in den Marmorb\u00e4dern", "tokens": ["Pl\u00e4t\u00b7schernd", "in", "den", "Mar\u00b7mor\u00b7b\u00e4\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Regen sich die Wellen l\u00fcstern.", "tokens": ["Re\u00b7gen", "sich", "die", "Wel\u00b7len", "l\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hei\u00dfer atmet's in den Rosen,", "tokens": ["Hei\u00b7\u00dfer", "at\u00b7met's", "in", "den", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heller leuchtet die Limone,", "tokens": ["Hel\u00b7ler", "leuch\u00b7tet", "die", "Li\u00b7mo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Mond, im regungslosen", "tokens": ["Wie", "ein", "Mond", ",", "im", "re\u00b7gungs\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmel ihrer Bl\u00e4tterkrone,", "tokens": ["Him\u00b7mel", "ih\u00b7rer", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und in allen Korridoren", "tokens": ["Und", "in", "al\u00b7len", "Kor\u00b7ri\u00b7do\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der K\u00f6schke goldnen Gittern", "tokens": ["Mit", "der", "K\u00f6schke", "gold\u00b7nen", "Git\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Scheint das Zauberschlo\u00df der Mohren", "tokens": ["Scheint", "das", "Zau\u00b7ber\u00b7schlo\u00df", "der", "Moh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von geheimer Lust zu zittern.", "tokens": ["Von", "ge\u00b7hei\u00b7mer", "Lust", "zu", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich indes auf goldnem Polster,", "tokens": ["Ich", "in\u00b7des", "auf", "gold\u00b7nem", "Pols\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei von W\u00fcnschen und Bed\u00fcrfen,", "tokens": ["Frei", "von", "W\u00fcn\u00b7schen", "und", "Be\u00b7d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal will ich noch in vollster", "tokens": ["Ein\u00b7mal", "will", "ich", "noch", "in", "volls\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seligkeit das Dasein schl\u00fcrfen.", "tokens": ["Se\u00b7lig\u00b7keit", "das", "Da\u00b7sein", "schl\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "La\u00df die duft'gen Flocken stieben,", "tokens": ["La\u00df", "die", "duft'\u00b7gen", "Flo\u00b7cken", "stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die den Schlaf herniedertauen,", "tokens": ["Die", "den", "Schlaf", "her\u00b7nie\u00b7der\u00b7tau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Traume mich die sieben", "tokens": ["Und", "im", "Trau\u00b7me", "mich", "die", "sie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPER", "ART", "CARD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Himmel des Propheten schauen!", "tokens": ["Him\u00b7mel", "des", "Pro\u00b7phe\u00b7ten", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Abendliche Geister wandeln", "tokens": ["A\u00b7bend\u00b7li\u00b7che", "Geis\u00b7ter", "wan\u00b7deln"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Durch das Laubwerk hin und wieder,", "tokens": ["Durch", "das", "Laub\u00b7werk", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, berauscht vom Duft der Mandeln,", "tokens": ["Doch", ",", "be\u00b7rauscht", "vom", "Duft", "der", "Man\u00b7deln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinken sie in Schlummer nieder.", "tokens": ["Sin\u00b7ken", "sie", "in", "Schlum\u00b7mer", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Funkelnd, gro\u00df wie eine Sonne,", "tokens": ["Fun\u00b7kelnd", ",", "gro\u00df", "wie", "ei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gie\u00dft der Wunderstern vom S\u00fcden,", "tokens": ["Gie\u00dft", "der", "Wun\u00b7ders\u00b7tern", "vom", "S\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Gie\u00dft Canopus s\u00fc\u00dfre Wonne,", "tokens": ["Gi