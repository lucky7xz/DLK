{"dta.poem.7678": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo jeder misversteht den andern unwillk\u00fcrlich,", "tokens": ["Wo", "je\u00b7der", "mis\u00b7ver\u00b7steht", "den", "an\u00b7dern", "un\u00b7will\u00b7k\u00fcr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und misverstanden selbst zu seyn klagt ungeb\u00fcrlich;", "tokens": ["Und", "mis\u00b7ver\u00b7stan\u00b7den", "selbst", "zu", "seyn", "klagt", "un\u00b7ge\u00b7b\u00fcr\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "VAINF", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was bleibt da Lernenden zu lernen? Misverstand;", "tokens": ["Was", "bleibt", "da", "Ler\u00b7nen\u00b7den", "zu", "ler\u00b7nen", "?", "Mis\u00b7ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "NN", "PTKZU", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da lerne lieber nichts! Das ist gewi\u00df Verstand.", "tokens": ["Da", "ler\u00b7ne", "lie\u00b7ber", "nichts", "!", "Das", "ist", "ge\u00b7wi\u00df", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "$.", "PDS", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}