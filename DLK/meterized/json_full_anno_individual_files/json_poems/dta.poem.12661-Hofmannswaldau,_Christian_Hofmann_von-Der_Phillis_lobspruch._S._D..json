{"dta.poem.12661": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Phillis lobspruch.  \n S. D.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "D Du vormals gr\u00fcnes feld!", "tokens": ["D", "Du", "vor\u00b7mals", "gr\u00fc\u00b7nes", "feld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O ihr b\u00fcsch und auen!", "tokens": ["O", "ihr", "b\u00fcsch", "und", "au\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "KON", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mein palast und mein gezelt,", "tokens": ["Mein", "pa\u00b7last", "und", "mein", "ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jtzt ein \u00f6des grauen!", "tokens": ["Jtzt", "ein", "\u00f6\u00b7des", "grau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "O ihr b\u00e4che, die ihr klar", "tokens": ["O", "ihr", "b\u00e4\u00b7che", ",", "die", "ihr", "klar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "$,", "PRELS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hinzu rauschen pfleget", "tokens": ["Hin\u00b7zu", "rau\u00b7schen", "pfle\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da, wo Pan der Nymphen-schaar", "tokens": ["Da", ",", "wo", "Pan", "der", "Nym\u00b7phen\u00b7schaar"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Offtermals verjaget.", "tokens": ["Off\u00b7ter\u00b7mals", "ver\u00b7ja\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Meine Phillis zwingt mich euch", "tokens": ["Mei\u00b7ne", "Phil\u00b7lis", "zwingt", "mich", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute nacht zu geben,", "tokens": ["Gu\u00b7te", "nacht", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jhr seyd traurig, tod und bleich.", "tokens": ["Ihr", "seyd", "trau\u00b7rig", ",", "tod", "und", "bleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ist gantz mein leben,", "tokens": ["Sie", "ist", "gantz", "mein", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Euch ist durch des herbstes noth", "tokens": ["Euch", "ist", "durch", "des", "herbs\u00b7tes", "noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle pracht vergangen,", "tokens": ["Al\u00b7le", "pracht", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sie ist wei\u00df und sonnen-roth", "tokens": ["Sie", "ist", "wei\u00df", "und", "son\u00b7nen\u00b7roth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den frischen wangen.", "tokens": ["Auf", "den", "fri\u00b7schen", "wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Bey euch st\u00fcrmt es ohne ruh", "tokens": ["Bey", "euch", "st\u00fcrmt", "es", "oh\u00b7ne", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in allen h\u00f6len,", "tokens": ["Und", "in", "al\u00b7len", "h\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Phyllis weht ein theil mir zu", "tokens": ["Phyl\u00b7lis", "weht", "ein", "theil", "mir", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhrer edlen seelen;", "tokens": ["Ih\u00b7rer", "ed\u00b7len", "see\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bey euch mu\u00df ohn unterla\u00df", "tokens": ["Bey", "euch", "mu\u00df", "ohn", "un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich die lufft ergiessen,", "tokens": ["Sich", "die", "lufft", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sie wird nur von thr\u00e4nen na\u00df", "tokens": ["Sie", "wird", "nur", "von", "thr\u00e4\u00b7nen", "na\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Um die nachtzeit fliessen.", "tokens": ["Um", "die", "nacht\u00b7zeit", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Keine sonne lacht mich an,", "tokens": ["Kei\u00b7ne", "son\u00b7ne", "lacht", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhr gesicht von fernen", "tokens": ["Ihr", "ge\u00b7sicht", "von", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVPP", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist, was mich ergetzen kan,", "tokens": ["Ist", ",", "was", "mich", "er\u00b7get\u00b7zen", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trotz den lichten sternen,", "tokens": ["Trotz", "den", "lich\u00b7ten", "ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ich kan in der Phillis schoo\u00df", "tokens": ["Ich", "kan", "in", "der", "Phil\u00b7lis", "schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NE", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Steten fr\u00fchling sp\u00fchren,", "tokens": ["Ste\u00b7ten", "fr\u00fch\u00b7ling", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Bey euch m\u00f6cht ich nackt und blo\u00df", "tokens": ["Bey", "euch", "m\u00f6cht", "ich", "nackt", "und", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADJD", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In der k\u00e4lt erfrieren.", "tokens": ["In", "der", "k\u00e4lt", "er\u00b7frie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Darum soll nur sie allein", "tokens": ["Da\u00b7rum", "soll", "nur", "sie", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ADV", "PPER", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Mir an statt der felder", "tokens": ["Mir", "an", "statt", "der", "fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPR", "ART", "NN"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.3": {"text": "Und an statt der berge seyn,", "tokens": ["Und", "an", "statt", "der", "ber\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPR", "ART", "VVFIN", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Hier sind meine w\u00e4lder:", "tokens": ["Hier", "sind", "mei\u00b7ne", "w\u00e4l\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meine blumen sind allhie,", "tokens": ["Mei\u00b7ne", "blu\u00b7men", "sind", "all\u00b7hie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo ich ohne leiden", "tokens": ["Wo", "ich", "oh\u00b7ne", "lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Meine seele sp\u00e4t und fr\u00fch", "tokens": ["Mei\u00b7ne", "see\u00b7le", "sp\u00e4t", "und", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sicher werde weiden.", "tokens": ["Si\u00b7cher", "wer\u00b7de", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Kein betr\u00fcbtes sinnen-weh", "tokens": ["Kein", "be\u00b7tr\u00fcb\u00b7tes", "sin\u00b7nen\u00b7weh"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll mich hie erschrecken,", "tokens": ["Soll", "mich", "hie", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jhrer weissen arme schnee", "tokens": ["Ih\u00b7rer", "weis\u00b7sen", "ar\u00b7me", "schnee"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mich treulich decken,", "tokens": ["Wird", "mich", "treu\u00b7lich", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mein verliebtes hertze soll", "tokens": ["Mein", "ver\u00b7lieb\u00b7tes", "hert\u00b7ze", "soll"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwischen ihren br\u00fcsten,", "tokens": ["Zwi\u00b7schen", "ih\u00b7ren", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Als den b\u00fcgeln, welche voll", "tokens": ["Als", "den", "b\u00fc\u00b7geln", ",", "wel\u00b7che", "voll"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "S\u00fcsser freude, nisten.", "tokens": ["S\u00fcs\u00b7ser", "freu\u00b7de", ",", "nis\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Dieses ist mein keyserthum,", "tokens": ["Die\u00b7ses", "ist", "mein", "key\u00b7ser\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Di\u00df sind meine sch\u00e4tze,", "tokens": ["Di\u00df", "sind", "mei\u00b7ne", "sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was hat sonst bey mir den ruhm,", "tokens": ["Was", "hat", "sonst", "bey", "mir", "den", "ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Da\u00df es mich ergetze?", "tokens": ["Da\u00df", "es", "mich", "er\u00b7get\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dieses ist das rechte ziel", "tokens": ["Die\u00b7ses", "ist", "das", "rech\u00b7te", "ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner m\u00fch auf erden:", "tokens": ["Mei\u00b7ner", "m\u00fch", "auf", "er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Was mein hertz gedenckt und will,", "tokens": ["Was", "mein", "hertz", "ge\u00b7denckt", "und", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "KON", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df mir Phyllis werden.", "tokens": ["Mu\u00df", "mir", "Phyl\u00b7lis", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Zeucht ein kauffmann hin und her", "tokens": ["Zeucht", "ein", "kauf\u00b7fmann", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Uber stock und steine,", "tokens": ["U\u00b7ber", "stock", "und", "stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch die klippen, durch das meer,", "tokens": ["Durch", "die", "klip\u00b7pen", ",", "durch", "das", "meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die w\u00fcsten haine,", "tokens": ["Durch", "die", "w\u00fcs\u00b7ten", "hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Was er suchet f\u00fcr und f\u00fcr,", "tokens": ["Was", "er", "su\u00b7chet", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich mag gedencken,", "tokens": ["Und", "ich", "mag", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mu\u00df mir meiner Phyllis zier", "tokens": ["Mu\u00df", "mir", "mei\u00b7ner", "Phyl\u00b7lis", "zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Reicher vorrath schencken.", "tokens": ["Rei\u00b7cher", "vor\u00b7rath", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Viel erzwingen ihre lust", "tokens": ["Viel", "er\u00b7zwin\u00b7gen", "ih\u00b7re", "lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den wilden kriegen,", "tokens": ["Auf", "den", "wil\u00b7den", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da sie offt in reiff und frost", "tokens": ["Da", "sie", "offt", "in", "reiff", "und", "frost"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJD", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Unterm himmel liegen;", "tokens": ["Un\u00b7term", "him\u00b7mel", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Unterm himmel darff ich nicht", "tokens": ["Un\u00b7term", "him\u00b7mel", "darff", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Reiff und frost ertragen,", "tokens": ["Reiff", "und", "frost", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Gleichwol giebet mir mein licht,", "tokens": ["Gleich\u00b7wol", "gie\u00b7bet", "mir", "mein", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN",