{"dta.poem.18946": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "25.  \n  An die Zahnlose M\u00e4gerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Dv hattest vier z\u00e4hn vor zway jahren/", "tokens": ["Dv", "hat\u00b7test", "vier", "z\u00e4hn", "vor", "zway", "jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "CARD", "VVFIN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von denen sto\u00df dir au\u00df der hust das letzte jahr", "tokens": ["Von", "de\u00b7nen", "sto\u00df", "dir", "au\u00df", "der", "hust", "das", "letz\u00b7te", "jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein paar/ vnd dieses Jahr ein paar/", "tokens": ["Ein", "paar", "/", "vnd", "die\u00b7ses", "Jahr", "ein", "paar", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "$(", "KON", "PDAT", "NN", "ART", "PIAT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df numehr weder z\u00e4hn noch husten du zuspahren.", "tokens": ["Da\u00df", "nu\u00b7mehr", "we\u00b7der", "z\u00e4hn", "noch", "hus\u00b7ten", "du", "zu\u00b7spah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "VVFIN", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}