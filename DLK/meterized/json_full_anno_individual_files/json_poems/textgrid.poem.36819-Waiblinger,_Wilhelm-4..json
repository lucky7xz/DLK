{"textgrid.poem.36819": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der St\u00e4dte Raffael ist Neapel, Freund!", "tokens": ["Der", "St\u00e4d\u00b7te", "Raf\u00b7fael", "ist", "Nea\u00b7pel", ",", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "NE", "$,", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das f\u00fchlten wohl Roms alte Tyrannen, das", "tokens": ["Das", "f\u00fchl\u00b7ten", "wohl", "Roms", "al\u00b7te", "Ty\u00b7ran\u00b7nen", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "ADV", "NE", "ADJA", "NN", "$,", "PRELS"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des fels'gen Capris Ungeheuer,", "tokens": ["Des", "fels'\u00b7gen", "Cap\u00b7ris", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jener bepurpurte bl\u00f6de Wahnwitz,", "tokens": ["Je\u00b7ner", "be\u00b7pur\u00b7pur\u00b7te", "bl\u00f6\u00b7de", "Wahn\u00b7witz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+--++-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Der auf verme\u00df'ner Br\u00fccke Puteolis", "tokens": ["Der", "auf", "ver\u00b7me\u00df'\u00b7ner", "Br\u00fc\u00b7cke", "Pu\u00b7te\u00b7o\u00b7lis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Meerbusen \u00fcberschritt, der entmenschte Narr,", "tokens": ["Meer\u00b7bu\u00b7sen", "\u00fc\u00b7bersc\u00b7hritt", ",", "der", "ent\u00b7menschte", "Narr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der hier gesungen und gebadet,", "tokens": ["Der", "hier", "ge\u00b7sun\u00b7gen", "und", "ge\u00b7ba\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo er gemordet die eigne Mutter.", "tokens": ["Wo", "er", "ge\u00b7mor\u00b7det", "die", "eig\u00b7ne", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch, ob auch Ischia's feurige Traube mir", "tokens": ["Doch", ",", "ob", "auch", "Is\u00b7chia's", "feu\u00b7ri\u00b7ge", "Trau\u00b7be", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ADV", "NE", "ADJA", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nektar verhei\u00dft, ob auch um Amalfis Fels", "tokens": ["Nek\u00b7tar", "ver\u00b7hei\u00dft", ",", "ob", "auch", "um", "A\u00b7mal\u00b7fis", "Fels"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Gern meinem Geist in duft'ger Ferne", "tokens": ["Gern", "mei\u00b7nem", "Geist", "in", "duft'\u00b7ger", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dorische Tempel dem Meer entsteigen,", "tokens": ["Do\u00b7ri\u00b7sche", "Tem\u00b7pel", "dem", "Meer", "ent\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ob auch durchs Schattengr\u00fcn von Camaldoli", "tokens": ["Ob", "auch", "durchs", "Schat\u00b7ten\u00b7gr\u00fcn", "von", "Ca\u00b7mal\u00b7do\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Vorgebirg' und bl\u00fchenden Inseln all'", "tokens": ["Die", "Vor\u00b7ge\u00b7bir\u00b7g'", "und", "bl\u00fc\u00b7hen\u00b7den", "In\u00b7seln", "all'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "PIAT"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im sch\u00f6nen Elemente schimmern", "tokens": ["Im", "sch\u00f6\u00b7nen", "E\u00b7le\u00b7men\u00b7te", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und aus dem Berge Gew\u00f6lk aufwirbelt,", "tokens": ["Und", "aus", "dem", "Ber\u00b7ge", "Ge\u00b7w\u00f6lk", "auf\u00b7wir\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch treibt's zur\u00fcck mich. Wehmuth erf\u00fcllt mich schon", "tokens": ["Doch", "treibt's", "zu\u00b7r\u00fcck", "mich", ".", "Weh\u00b7muth", "er\u00b7f\u00fcllt", "mich", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "PPER", "$.", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Und kind'sche Wonne, denk' ich die S\u00e4ulen mir", "tokens": ["Und", "kin\u00b7d'\u00b7sche", "Won\u00b7ne", ",", "denk'", "ich", "die", "S\u00e4u\u00b7len", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PPER"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der gold'nen Basilik', an alter", "tokens": ["Der", "gold'\u00b7nen", "Ba\u00b7si\u00b7li\u00b7k'", ",", "an", "al\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mauer, am stillen begr\u00fcnten Platze,", "tokens": ["Mau\u00b7er", ",", "am", "stil\u00b7len", "be\u00b7gr\u00fcn\u00b7ten", "Plat\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Wo an Ramesses thebischem Obelisk", "tokens": ["Wo", "an", "Ra\u00b7mes\u00b7ses", "the\u00b7bi\u00b7schem", "O\u00b7be\u00b7lisk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Brunnen pl\u00e4tschert, einsame Stra\u00dfen auch,", "tokens": ["Der", "Brun\u00b7nen", "pl\u00e4t\u00b7schert", ",", "ein\u00b7sa\u00b7me", "Stra\u00b7\u00dfen", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hier Kuppeln in der Abendr\u00f6the,", "tokens": ["Hier", "Kup\u00b7peln", "in", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort des zertr\u00fcmmerten Colosseums", "tokens": ["Dort", "des", "zer\u00b7tr\u00fcm\u00b7mer\u00b7ten", "Co\u00b7los\u00b7se\u00b7ums"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In Sonnenflammen athmende Riesenwand", "tokens": ["In", "Son\u00b7nen\u00b7flam\u00b7men", "ath\u00b7men\u00b7de", "Rie\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Prachtvoll mir zeigen! Trauernde Roma, hier", "tokens": ["Pracht\u00b7voll", "mir", "zei\u00b7gen", "!", "Trau\u00b7ern\u00b7de", "Ro\u00b7ma", ",", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "PPER", "VVINF", "$.", "ADJA", "NN", "$,", "ADV"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der V\u00f6lker gro\u00dfem Gott, dem ew'gen", "tokens": ["Der", "V\u00f6l\u00b7ker", "gro\u00b7\u00dfem", "Gott", ",", "dem", "ew'\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schicksal geheiligt ert\u00f6nt mein Lied dir.", "tokens": ["Schick\u00b7sal", "ge\u00b7hei\u00b7ligt", "er\u00b7t\u00f6nt", "mein", "Lied", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Zweimal hast du mit eiserner Hand die Welt", "tokens": ["Zwei\u00b7mal", "hast", "du", "mit", "ei\u00b7ser\u00b7ner", "Hand", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gedr\u00fcckt, Herrschs\u00fcchtige, gr\u00f6\u00dfer als du war nur", "tokens": ["Ge\u00b7dr\u00fcckt", ",", "Herrschs\u00fcch\u00b7ti\u00b7ge", ",", "gr\u00f6\u00b7\u00dfer", "als", "du", "war", "nur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "NN", "$,", "ADJD", "KOKOM", "PPER", "VAFIN", "ADV"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Schicksal, drum auch zweimal hat's dir", "tokens": ["Das", "Schick\u00b7sal", ",", "drum", "auch", "zwei\u00b7mal", "hat's", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PAV", "ADV", "ADV", "VAFIN", "PPER"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Strafend entwunden den schweren Scepter,", "tokens": ["Stra\u00b7fend", "ent\u00b7wun\u00b7den", "den", "schwe\u00b7ren", "Scep\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Den K\u00f6n'ge, Senatoren, C\u00e4sare einst", "tokens": ["Den", "K\u00f6n'\u00b7ge", ",", "Se\u00b7na\u00b7to\u00b7ren", ",", "C\u00e4\u00b7sa\u00b7re", "einst"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NE", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gef\u00fchrt, und unerbittlicher noch zuletzt", "tokens": ["Ge\u00b7f\u00fchrt", ",", "und", "un\u00b7er\u00b7bitt\u00b7li\u00b7cher", "noch", "zu\u00b7letzt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "ADJD", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dreifach gekr\u00f6nte Priester, deren", "tokens": ["Drei\u00b7fach", "ge\u00b7kr\u00f6n\u00b7te", "Pries\u00b7ter", ",", "de\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PAV", "ADJA", "NN", "$,", "PRELAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiliger Waffe der Hohenstaufen", "tokens": ["Hei\u00b7li\u00b7ger", "Waf\u00b7fe", "der", "Ho\u00b7hen\u00b7stau\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Gro\u00dfherz'ger Heldenstamm als ein Opfer sank", "tokens": ["Gro\u00df\u00b7her\u00b7z'\u00b7ger", "Hel\u00b7den\u00b7stamm", "als", "ein", "Op\u00b7fer", "sank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der V\u00f6lkerblindheit, denen die Kaiserhand", "tokens": ["Der", "V\u00f6l\u00b7ker\u00b7blind\u00b7heit", ",", "de\u00b7nen", "die", "Kai\u00b7ser\u00b7hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den B\u00fcgel hielt, und deren Bannstrahl", "tokens": ["Den", "B\u00fc\u00b7gel", "hielt", ",", "und", "de\u00b7ren", "Bann\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nige st\u00fcrzte vom Thron der V\u00e4ter.", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "st\u00fcrz\u00b7te", "vom", "Thron", "der", "V\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Ach, s\u00e4nft'ge nun, o Rom, dein tyrannisch Herz,", "tokens": ["Ach", ",", "s\u00e4nft'\u00b7ge", "nun", ",", "o", "Rom", ",", "dein", "ty\u00b7ran\u00b7nisch", "Herz", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "$,", "FM", "NE", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und beuge dich der Zeit. Der gefallene", "tokens": ["Und", "beu\u00b7ge", "dich", "der", "Zeit", ".", "Der", "ge\u00b7fal\u00b7le\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Herrschgier'ge Engel rang vergebens", "tokens": ["Herrschgier'\u00b7ge", "En\u00b7gel", "rang", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst mit dem Himmel um seine Krone.", "tokens": ["Einst", "mit", "dem", "Him\u00b7mel", "um", "sei\u00b7ne", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Im Grabe deiner gro\u00dfen Auguste, wo", "tokens": ["Im", "Gra\u00b7be", "dei\u00b7ner", "gro\u00b7\u00dfen", "Au\u00b7gus\u00b7te", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Britannicus ein heuchlerisch Todtenmahl", "tokens": ["Bri\u00b7tan\u00b7ni\u00b7cus", "ein", "heuch\u00b7le\u00b7risch", "Tod\u00b7ten\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJD", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Geehrt, vergi\u00dft in Spiel und Stierkampf", "tokens": ["Ge\u00b7ehrt", ",", "ver\u00b7gi\u00dft", "in", "Spiel", "und", "Stier\u00b7kampf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun das entartete Volk die Vorwelt.", "tokens": ["Nun", "das", "ent\u00b7ar\u00b7te\u00b7te", "Volk", "die", "Vor\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Des Forums Siegesb\u00f6gen und Tempel, jetzt", "tokens": ["Des", "Fo\u00b7rums", "Sie\u00b7ges\u00b7b\u00f6\u00b7gen", "und", "Tem\u00b7pel", ",", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchzieht sie nur schwerm\u00fcthiger M\u00f6nche Schwarm,", "tokens": ["Durch\u00b7zieht", "sie", "nur", "schwer\u00b7m\u00fct\u00b7hi\u00b7ger", "M\u00f6n\u00b7che", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "ia