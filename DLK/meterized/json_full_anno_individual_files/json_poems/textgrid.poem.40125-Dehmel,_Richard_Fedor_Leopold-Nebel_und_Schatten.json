{"textgrid.poem.40125": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Nebel und Schatten", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun still, mein Schritt, im stillen Nebelfeld!", "tokens": ["Nun", "still", ",", "mein", "Schritt", ",", "im", "stil\u00b7len", "Ne\u00b7bel\u00b7feld", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hier r\u00fchrt kein Leben mehr an meine Ruhe:", "tokens": ["hier", "r\u00fchrt", "kein", "Le\u00b7ben", "mehr", "an", "mei\u00b7ne", "Ru\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "hier darf ich f\u00fchlen, da\u00df ich einsam bin.", "tokens": ["hier", "darf", "ich", "f\u00fch\u00b7len", ",", "da\u00df", "ich", "ein\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Laut; kein Hauch; der bleiche Abend h\u00e4lt", "tokens": ["Kein", "Laut", ";", "kein", "Hauch", ";", "der", "blei\u00b7che", "A\u00b7bend", "h\u00e4lt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "im dichten Mantel schwer die Luft gefangen.", "tokens": ["im", "dich\u00b7ten", "Man\u00b7tel", "schwer", "die", "Luft", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Herz nur h\u00f6r' ich noch; doch kein Verlangen", "tokens": ["Mein", "Herz", "nur", "h\u00f6r'", "ich", "noch", ";", "doch", "kein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "ADV", "$.", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nach Leben ist dies Klopfen, \u2013 Lust und Schmerz", "tokens": ["nach", "Le\u00b7ben", "ist", "dies", "Klop\u00b7fen", ",", "\u2013", "Lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PDS", "NN", "$,", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ruhn hinter mir versunken gleich zwei St\u00fcrmen,", "tokens": ["ruhn", "hin\u00b7ter", "mir", "ver\u00b7sun\u00b7ken", "gleich", "zwei", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die sich umarmen und im Wirbel sterben, \u2013 \u2013", "tokens": ["die", "sich", "um\u00b7ar\u00b7men", "und", "im", "Wir\u00b7bel", "ster\u00b7ben", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PRELS", "PRF", "VVINF", "KON", "APPRART", "NN", "VVINF", "$,", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "was ",