{"dta.poem.10911": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kann man sich auch ohn Gefahr zu den wilden Hauern wagen,", "tokens": ["Kann", "man", "sich", "auch", "ohn", "Ge\u00b7fahr", "zu", "den", "wil\u00b7den", "Hau\u00b7ern", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADV", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die hier recht lebendig scheinen, und so scharfe Waffen tragen?", "tokens": ["Die", "hier", "recht", "le\u00b7ben\u00b7dig", "schei\u00b7nen", ",", "und", "so", "schar\u00b7fe", "Waf\u00b7fen", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$,", "KON", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ja, der eine schl\u00e4ft, der andre reibt sich grunzend; wag es nur,", "tokens": ["Ja", ",", "der", "ei\u00b7ne", "schl\u00e4ft", ",", "der", "and\u00b7re", "reibt", "sich", "grun\u00b7zend", ";", "wag", "es", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "PRF", "ADJD", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Tritt nur n\u00e4her, und beschaue die nat\u00fcrliche Figur", "tokens": ["Tritt", "nur", "n\u00e4\u00b7her", ",", "und", "be\u00b7schau\u00b7e", "die", "na\u00b7t\u00fcr\u00b7li\u00b7che", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Dieser Thier, auch in den B\u00e4umen, die leibhaftige Natur.", "tokens": ["Die\u00b7ser", "Thier", ",", "auch", "in", "den", "B\u00e4u\u00b7men", ",", "die", "leib\u00b7haf\u00b7ti\u00b7ge", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--++-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Schau den Stamm, der Riesen - f\u00f6rmig! scheint nicht seiner", "tokens": ["Schau", "den", "Stamm", ",", "der", "Rie\u00b7sen", "f\u00f6r\u00b7mig", "!", "scheint", "nicht", "sei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$(", "ADJD", "$.", "VVFIN", "PTKNEG", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Wie ihr Urbild, sich zu regen? Lust und Furcht scheint sich zu", "tokens": ["Wie", "ihr", "Ur\u00b7bild", ",", "sich", "zu", "re\u00b7gen", "?", "Lust", "und", "Furcht", "scheint", "sich", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "PRF", "APPR", "ADJA", "$.", "NN", "KON", "NN", "VVFIN", "PRF", "APPR"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "In dem dick verwachsnen Busch. Wenn uns nun die Creatur", "tokens": ["In", "dem", "dick", "ver\u00b7wachs\u00b7nen", "Busch", ".", "Wenn", "uns", "nun", "die", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$.", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Eine Spur zum Sch\u00f6pfer zeiget; zeigt die Kunst hier eine Spur,", "tokens": ["Ei\u00b7ne", "Spur", "zum", "Sch\u00f6p\u00b7fer", "zei\u00b7get", ";", "zeigt", "die", "Kunst", "hier", "ei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$.", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Zu der Creaturen Heer. Folglich kann, zu Gott zu steigen,", "tokens": ["Zu", "der", "Crea\u00b7tu\u00b7ren", "Heer", ".", "Folg\u00b7lich", "kann", ",", "zu", "Gott", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "NN", "VMFIN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Jedem, der sie recht gebraucht, sie die erste Sprosse zeigen.", "tokens": ["Je\u00b7dem", ",", "der", "sie", "recht", "ge\u00b7braucht", ",", "sie", "die", "ers\u00b7te", "Spros\u00b7se", "zei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "----+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}