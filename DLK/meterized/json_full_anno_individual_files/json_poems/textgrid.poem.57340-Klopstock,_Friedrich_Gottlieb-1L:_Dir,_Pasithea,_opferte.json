ch", "tokens": ["Fin\u00b7den", "Op\u00b7fer", ".", "Die", "Glut", "quillt", "vom", "Rauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Rein, und bl\u00e4ulich, und hell, sprudelt empor", "tokens": ["Rein", ",", "und", "bl\u00e4u\u00b7lich", ",", "und", "hell", ",", "spru\u00b7delt", "em\u00b7por"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "$,", "VVFIN", "PTKVZ"], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "W\u00f6lkenden Wohlgeruch.", "tokens": ["W\u00f6l\u00b7ken\u00b7den", "Wohl\u00b7ge\u00b7ruch", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Und die G\u00f6ttinnen fliehen nicht,", "tokens": ["Und", "die", "G\u00f6t\u00b7tin\u00b7nen", "flie\u00b7hen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "L\u00e4cheln ihnen. Es folgt, kehren sie,", "tokens": ["L\u00e4\u00b7cheln", "ih\u00b7nen", ".", "Es", "folgt", ",", "keh\u00b7ren", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Guter V\u00f6gel Geleit, fl\u00f6tend ein Chor", "tokens": ["Gu\u00b7ter", "V\u00f6\u00b7gel", "Ge\u00b7leit", ",", "fl\u00f6\u00b7tend", "ein", "Chor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "ADJD", "ART", "NN"], "meter": 