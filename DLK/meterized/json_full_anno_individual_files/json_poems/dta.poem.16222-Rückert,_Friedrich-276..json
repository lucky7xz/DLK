{"dta.poem.16222": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "276.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es gibt noch Gl\u00fcckliche, wenn du auch keiner bist;", "tokens": ["Es", "gibt", "noch", "Gl\u00fcck\u00b7li\u00b7che", ",", "wenn", "du", "auch", "kei\u00b7ner", "bist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "KOUS", "PPER", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Freud' ist auf der Welt, wenn sie auch dein nicht ist.", "tokens": ["Die", "Freud'", "ist", "auf", "der", "Welt", ",", "wenn", "sie", "auch", "dein", "nicht", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch diese Freud' ist dein, da\u00df viele freun sich k\u00f6nnen,", "tokens": ["Doch", "die\u00b7se", "Freud'", "ist", "dein", ",", "da\u00df", "vie\u00b7le", "freun", "sich", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPOSAT", "$,", "KOUS", "PIS", "VVFIN", "PRF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und diese Freud' allein wird Niemand dir misg\u00f6nnen.", "tokens": ["Und", "die\u00b7se", "Freud'", "al\u00b7lein", "wird", "Nie\u00b7mand", "dir", "mis\u00b7g\u00f6n\u00b7nen", "."], "token_info":