{"dta.poem.4159": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Blutorange.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt' ich heut' ein M\u00e4hrchen sagen", "tokens": ["H\u00f6rt'", "ich", "heut'", "ein", "M\u00e4hr\u00b7chen", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der blutigen Orange", "tokens": ["Von", "der", "blu\u00b7ti\u00b7gen", "O\u00b7ran\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Bl\u00fcthe der Granate.", "tokens": ["Und", "der", "Bl\u00fc\u00b7the", "der", "Gra\u00b7na\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Also sprach der kluge G\u00e4rtner:", "tokens": ["Al\u00b7so", "sprach", "der", "klu\u00b7ge", "G\u00e4rt\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Golden, wie noch heut' die Schale,", "tokens": ["Gol\u00b7den", ",", "wie", "noch", "heut'", "die", "Scha\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00fchten einst die innern S\u00e4fte", "tokens": ["Gl\u00fch\u00b7ten", "einst", "die", "in\u00b7nern", "S\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieser w\u00fcrzigen Orangen", "tokens": ["Die\u00b7ser", "w\u00fcr\u00b7zi\u00b7gen", "O\u00b7ran\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem Baume hier am Ufer.", "tokens": ["Von", "dem", "Bau\u00b7me", "hier", "am", "U\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und ein Spr\u00f6\u00dfling der Granate,", "tokens": ["Und", "ein", "Spr\u00f6\u00df\u00b7ling", "der", "Gra\u00b7na\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--++-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Jung und schlank, wuchs auf daneben.", "tokens": ["Jung", "und", "schlank", ",", "wuchs", "auf", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,", "VVFIN", "APPR", "PDAT", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.11": {"text": "Als der Winter zog von dannen,", "tokens": ["Als", "der", "Win\u00b7ter", "zog", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Trieb das B\u00e4umchen erste Knospen,", "tokens": ["Trieb", "das", "B\u00e4um\u00b7chen", "ers\u00b7te", "Knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und gleich hei\u00dfen Blutes Flammen", "tokens": ["Und", "gleich", "hei\u00b7\u00dfen", "Blu\u00b7tes", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Brachen Bl\u00fcthen aus den Keimen.", "tokens": ["Bra\u00b7chen", "Bl\u00fc\u00b7then", "aus", "den", "Kei\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und die Nachbarin Orange,", "tokens": ["Und", "die", "Nach\u00b7ba\u00b7rin", "O\u00b7ran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Staunend, wie in Liebesandacht,", "tokens": ["Stau\u00b7nend", ",", "wie", "in", "Lie\u00b7be\u00b7san\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Bog die hohen Zweige schmachtend", "tokens": ["Bog", "die", "ho\u00b7hen", "Zwei\u00b7ge", "schmach\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Nach der fremden Gluth hin\u00fcber,", "tokens": ["Nach", "der", "frem\u00b7den", "Gluth", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df die Silberbl\u00fcthen alle", "tokens": ["Da\u00df", "die", "Sil\u00b7ber\u00b7bl\u00fc\u00b7then", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Offnen Auges landw\u00e4rts schauten,", "tokens": ["Off\u00b7nen", "Au\u00b7ges", "land\u00b7w\u00e4rts", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und das Meer nur Gr\u00fcnes sahe.", "tokens": ["Und", "das", "Meer", "nur", "Gr\u00fc\u00b7nes", "sa\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und als nun der Herbst gekommen,", "tokens": ["Und", "als", "nun", "der", "Herbst", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und den ersten goldnen Apfel", "tokens": ["Und", "den", "ers\u00b7ten", "gold\u00b7nen", "Ap\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Pr\u00fcfend ich vom Baume pfl\u00fcckte,", "tokens": ["Pr\u00fc\u00b7fend", "ich", "vom", "Bau\u00b7me", "pfl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ward mir klar der Zweige Schwanken", "tokens": ["Ward", "mir", "klar", "der", "Zwei\u00b7ge", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Und der Bl\u00fcthen seltsam Dr\u00e4ngen:", "tokens": ["Und", "der", "Bl\u00fc\u00b7then", "selt\u00b7sam", "Dr\u00e4n\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Denn gleich hei\u00dfen Blutes Flammen,", "tokens": ["Denn", "gleich", "hei\u00b7\u00dfen", "Blu\u00b7tes", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Voll, wie langverhalt'nes Sehnen,", "tokens": ["Voll", ",", "wie", "lang\u00b7ver\u00b7halt'\u00b7nes", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Flo\u00df der Saft aus goldner Schale.", "tokens": ["Flo\u00df", "der", "Saft", "aus", "gold\u00b7ner", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Also sprach der kluge G\u00e4rtner,", "tokens": ["Al\u00b7so", "sprach", "der", "klu\u00b7ge", "G\u00e4rt\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Und ich pfl\u00fcckte mir Orangen", "tokens": ["Und", "ich", "pfl\u00fcck\u00b7te", "mir", "O\u00b7ran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Von dem seltnen Uferbaume.", "tokens": ["Von", "dem", "selt\u00b7nen", "U\u00b7fer\u00b7bau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist zu Ende nun die Sage,", "tokens": ["Ist", "zu", "En\u00b7de", "nun", "die", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweig' auch ich, und was im Herzen", "tokens": ["Schweig'", "auch", "ich", ",", "und", "was", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "$,", "KON", "PWS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir sich regt mit jedem Schlage,", "tokens": ["Mir", "sich", "regt", "mit", "je\u00b7dem", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sich heute still getrunken", "tokens": ["Hat", "sich", "heu\u00b7te", "still", "ge\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem k\u00fchlen Wundersafte,", "tokens": ["In", "dem", "k\u00fch\u00b7len", "Wun\u00b7der\u00b7saf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und so send' ich ohne Deutung,", "tokens": ["Und", "so", "send'", "ich", "oh\u00b7ne", "Deu\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Freundin, dir die G\u00e4rtnersage.", "tokens": ["Freun\u00b7din", ",", "dir", "die", "G\u00e4rt\u00b7ner\u00b7sa\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}