{"dta.poem.15768": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So th\u00f6richt ist der Mensch nur auf sein Weh beflissen,", "tokens": ["So", "th\u00f6\u00b7richt", "ist", "der", "Mensch", "nur", "auf", "sein", "Weh", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er von seinem Wohl viel minder scheint zu wissen.", "tokens": ["Da\u00df", "er", "von", "sei\u00b7nem", "Wohl", "viel", "min\u00b7der", "scheint", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Selbst seine Sprache zeigt entgegen einem Namen", "tokens": ["Selbst", "sei\u00b7ne", "Spra\u00b7che", "zeigt", "ent\u00b7ge\u00b7gen", "ei\u00b7nem", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcrs Liebe meistens drei, die auf das Leide kamen.", "tokens": ["F\u00fcrs", "Lie\u00b7be", "meis\u00b7tens", "drei", ",", "die", "auf", "das", "Lei\u00b7de", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "CARD", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur eines nennt er gut, o w\u00e4r' es gut nur immer,", "tokens": ["Nur", "ei\u00b7nes", "nennt", "er", "gut", ",", "o", "w\u00e4r'", "es", "gut", "nur", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJD", "$,", "FM", "VAFIN", "PPER", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drei \u00fcbel, b\u00f6s' und schlimm; oft ist noch gutes schlimmer.", "tokens": ["Drei", "\u00fc\u00b7bel", ",", "b\u00f6s'", "und", "schlimm", ";", "oft", "ist", "noch", "gu\u00b7tes", "schlim\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJD", "$,", "ADJD", "KON", "ADJD", "$.", "ADV", "VAFIN", "ADV", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur eines nennt er s\u00fc\u00df, o m\u00f6cht' es rein ihm munden,", "tokens": ["Nur", "ei\u00b7nes", "nennt", "er", "s\u00fc\u00df", ",", "o", "m\u00f6cht'", "es", "rein", "ihm", "mun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJD", "$,", "FM", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drei bitter, saur und herb, dem s\u00fc\u00dfen oft verbunden.", "tokens": ["Drei", "bit\u00b7ter", ",", "saur", "und", "herb", ",", "dem", "s\u00fc\u00b7\u00dfen", "oft", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PRELS", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur eines nennt er sch\u00f6n; es schien ihm unerl\u00e4sslich", "tokens": ["Nur", "ei\u00b7nes", "nennt", "er", "sch\u00f6n", ";", "es", "schien", "ihm", "un\u00b7er\u00b7l\u00e4ss\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem beizugeben auch drei garstig, w\u00fcst' und h\u00e4sslich.", "tokens": ["Dem", "bei\u00b7zu\u00b7ge\u00b7ben", "auch", "drei", "gars\u00b7tig", ",", "w\u00fcst'", "und", "h\u00e4ss\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "CARD", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}