{"textgrid.poem.56094": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Mariae Heimsuchung", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch erging sie's leicht im Anbeginne,", "tokens": ["Noch", "er\u00b7ging", "sie's", "leicht", "im", "An\u00b7be\u00b7gin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "doch im Steigen manchmal ward sie schon", "tokens": ["doch", "im", "Stei\u00b7gen", "manch\u00b7mal", "ward", "sie", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADV", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ihres wunderbaren Leibes inne, \u2013", "tokens": ["ih\u00b7res", "wun\u00b7der\u00b7ba\u00b7ren", "Lei\u00b7bes", "in\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und dann stand sie, atmend, auf den hohn", "tokens": ["und", "dann", "stand", "sie", ",", "at\u00b7mend", ",", "auf", "den", "hohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Judenbergen. Aber nicht das Land,", "tokens": ["Ju\u00b7den\u00b7ber\u00b7gen", ".", "A\u00b7ber", "nicht", "das", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "ihre F\u00fclle war um sie gebreitet;", "tokens": ["ih\u00b7re", "F\u00fcl\u00b7le", "war", "um", "sie", "ge\u00b7brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "gehend f\u00fchlte sie: man \u00fcberschreitet", "tokens": ["ge\u00b7hend", "f\u00fchl\u00b7te", "sie", ":", "man", "\u00fc\u00b7bersc\u00b7hrei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "$.", "PIS", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "nie die Gr\u00f6\u00dfe, die sie jetzt empfand.", "tokens": ["nie", "die", "Gr\u00f6\u00b7\u00dfe", ",", "die", "sie", "jetzt", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und es dr\u00e4ngte sie, die Hand zu legen", "tokens": ["Und", "es", "dr\u00e4ng\u00b7te", "sie", ",", "die", "Hand", "zu", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "auf den andern Leib, der weiter war.", "tokens": ["auf", "den", "an\u00b7dern", "Leib", ",", "der", "wei\u00b7ter", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Frauen schwankten sich entgegen", "tokens": ["Und", "die", "Frau\u00b7en", "schwank\u00b7ten", "sich", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und ber\u00fchrten sich Gewand und Haar.", "tokens": ["und", "be\u00b7r\u00fchr\u00b7ten", "sich", "Ge\u00b7wand", "und", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Jede, voll von ihrem Heiligtume,", "tokens": ["Je\u00b7de", ",", "voll", "von", "ih\u00b7rem", "Hei\u00b7lig\u00b7tu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "sch\u00fctzte sich mit der Gevatterin.", "tokens": ["sch\u00fctz\u00b7te", "sich", "mit", "der", "Ge\u00b7vat\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ach der Heiland in ihr war noch Blume,", "tokens": ["Ach", "der", "Hei\u00b7land", "in", "ihr", "war", "noch", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "APPR", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "doch den T\u00e4ufer in dem Schoo\u00df der Muhme", "tokens": ["doch", "den", "T\u00e4u\u00b7fer", "in", "dem", "Schoo\u00df", "der", "Muh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "ri\u00d