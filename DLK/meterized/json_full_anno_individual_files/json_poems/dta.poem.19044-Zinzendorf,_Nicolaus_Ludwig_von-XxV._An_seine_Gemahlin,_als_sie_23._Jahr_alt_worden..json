{"dta.poem.19044": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxV.  An seine Gemahlin, als sie 23. Jahr  \n alt worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Geh\u00fclffin, die das Lamm mir selber angetraut,", "tokens": ["Ge\u00b7h\u00fclf\u00b7fin", ",", "die", "das", "Lamm", "mir", "sel\u00b7ber", "an\u00b7ge\u00b7traut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die seine Liebes-Hand in meine Hand beschlossen,", "tokens": ["Die", "sei\u00b7ne", "Lie\u00b7bes\u00b7Hand", "in", "mei\u00b7ne", "Hand", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und uns bi\u00df diesen Tag mit Gnaden-Thau begossen;", "tokens": ["Und", "uns", "bi\u00df", "die\u00b7sen", "Tag", "mit", "Gna\u00b7den\u00b7\u00b7T\u00b7hau", "be\u00b7gos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Komm, bete mit mir an! komm, meines K\u00f6nigs Braut!", "tokens": ["Komm", ",", "be\u00b7te", "mit", "mir", "an", "!", "komm", ",", "mei\u00b7nes", "K\u00f6\u00b7nigs", "Braut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "VVFIN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Eh ist allerdings ein sehr geheimer Stand,", "tokens": ["Die", "Eh", "ist", "al\u00b7ler\u00b7dings", "ein", "sehr", "ge\u00b7hei\u00b7mer", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Stand, den unser HErr im Garten schon gesegnet,", "tokens": ["Ein", "Stand", ",", "den", "un\u00b7ser", "Herr", "im", "Gar\u00b7ten", "schon", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem JEsu Gegenwart, o hohe Gunst! begegnet,", "tokens": ["Dem", "Je\u00b7su", "Ge\u00b7gen\u00b7wart", ",", "o", "ho\u00b7he", "Gunst", "!", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "NN", "$,", "FM", "ADJA", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein in der Ewigkeit gekn\u00fcpftes Liebes-Band.", "tokens": ["Ein", "in", "der", "E\u00b7wig\u00b7keit", "ge\u00b7kn\u00fcpf\u00b7tes", "Lie\u00b7bes\u00b7Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zwar wenn man zu der Eh durch Welt-Lust angekiert,", "tokens": ["Zwar", "wenn", "man", "zu", "der", "Eh", "durch", "Welt\u00b7Lust", "an\u00b7ge\u00b7kiert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieselbige vollzieht, nach Art der Hochgebohrnen,", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", "voll\u00b7zieht", ",", "nach", "Art", "der", "Hoch\u00b7ge\u00b7bohr\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ach! aber auch dabey von GOtt nicht Auserkohrnen, (1. Cor. 1.)", "tokens": ["Ach", "!", "a\u00b7ber", "auch", "da\u00b7bey", "von", "Gott", "nicht", "Au\u00b7ser\u00b7kohr\u00b7nen", ",", "(", "1.", "Cor", ".", "1.", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "ordinal", "word", "punct", "ordinal", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "PAV", "APPR", "NN", "PTKNEG", "NN", "$,", "$(", "ADJA", "NN", "$.", "ADJA", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Was Wunder, da\u00df die Reu gleich mit geehligt wird.", "tokens": ["Was", "Wun\u00b7der", ",", "da\u00df", "die", "Reu", "gleich", "mit", "geeh\u00b7ligt", "wird", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "ART", "NE", "ADV", "APPR", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie, spricht man, wilst du denn, da\u00df sie beschaffen sey?", "tokens": ["Wie", ",", "spricht", "man", ",", "wilst", "du", "denn", ",", "da\u00df", "sie", "be\u00b7schaf\u00b7fen", "sey", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PIS", "$,", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du lehrest uns vielleicht die Ehe der Phantasten,", "tokens": ["Du", "leh\u00b7rest", "uns", "viel\u00b7leicht", "die", "E\u00b7he", "der", "Phan\u00b7tas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sich vor Aengstlichkeit und Harm zu Tode fasten?", "tokens": ["Die", "sich", "vor", "A\u00b7engst\u00b7lich\u00b7keit", "und", "Harm", "zu", "To\u00b7de", "fas\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Nein! Freund, die Eh im HErrn ist von dem allen frey.", "tokens": ["Nein", "!", "Freund", ",", "die", "Eh", "im", "Herrn", "ist", "von", "dem", "al\u00b7len", "frey", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "$,", "ART", "NN", "APPRART", "NN", "VAFIN", "APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Knecht der Liebe ist im \u00fcbrigen gefreyt,", "tokens": ["Ein", "Knecht", "der", "Lie\u00b7be", "ist", "im", "\u00fcb\u00b7ri\u00b7gen", "ge\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein F\u00fcrst der Herrlichkeit, des Vaters edle Pflantze,", "tokens": ["Ein", "F\u00fcrst", "der", "Herr\u00b7lich\u00b7keit", ",", "des", "Va\u00b7ters", "ed\u00b7le", "Pflant\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und eine Magd des HErrn prangt in dem Glaubens-Crantze,", "tokens": ["Und", "ei\u00b7ne", "Magd", "des", "Herrn", "prangt", "in", "dem", "Glau\u00b7bens\u00b7Crant\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu solchem Paare reimt sich wol kein Sclaven-Kleid.", "tokens": ["Zu", "sol\u00b7chem", "Paa\u00b7re", "reimt", "sich", "wol", "kein", "Scla\u00b7ven\u00b7Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie sieht denn aber nun die rechte Ehe aus?", "tokens": ["Wie", "sieht", "denn", "a\u00b7ber", "nun", "die", "rech\u00b7te", "E\u00b7he", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sieht sie aus: Die Braut, Jmmanuel verschrieben,", "tokens": ["So", "sieht", "sie", "aus", ":", "Die", "Braut", ",", "Jm\u00b7ma\u00b7nu\u00b7el", "ver\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "$,", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist einem guten Freund im Angedencken blieben,", "tokens": ["Ist", "ei\u00b7nem", "gu\u00b7ten", "Freund", "im", "An\u00b7ge\u00b7den\u00b7cken", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der liebt, der h\u00fctet sie, bi\u00df hin ans Hochzeit-Hau\u00df.", "tokens": ["Der", "liebt", ",", "der", "h\u00fc\u00b7tet", "sie", ",", "bi\u00df", "hin", "ans", "Hoch\u00b7zeit\u00b7Hau\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "$,", "KOUS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und wie nicht Weib noch Mann in JEsu Christo ist,", "tokens": ["Und", "wie", "nicht", "Weib", "noch", "Mann", "in", "Je\u00b7su", "Chris\u00b7to", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKNEG", "NN", "ADV", "NN", "APPR", "NE", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sind sie beyderseits so K\u00e4mmerer, als Freunde", "tokens": ["So", "sind", "sie", "bey\u00b7der\u00b7seits", "so", "K\u00e4m\u00b7me\u00b7rer", ",", "als", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vom rechten Br\u00e4utigam, dem Schrecken unsrer Feinde,", "tokens": ["Vom", "rech\u00b7ten", "Br\u00e4u\u00b7ti\u00b7gam", ",", "dem", "Schre\u00b7cken", "uns\u00b7rer", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da eins das andre denn zu schm\u00fccken nicht vergi\u00dft.", "tokens": ["Da", "eins", "das", "and\u00b7re", "denn", "zu", "schm\u00fc\u00b7cken", "nicht", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "KON", "PTKZU", "VVINF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Zeit des Lebens ist die Zubereitungs-Zeit,", "tokens": ["Die", "Zeit", "des", "Le\u00b7bens", "ist", "die", "Zu\u00b7be\u00b7rei\u00b7tungs\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Monden, die der F\u00fcrst, seit dem er sie erkennet,", "tokens": ["Die", "Mon\u00b7den", ",", "die", "der", "F\u00fcrst", ",", "seit", "dem", "er", "sie", "er\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sie als Jungfer selbst zum Ehe-Bett ernennet,", "tokens": ["Und", "sie", "als", "Jung\u00b7fer", "selbst", "zum", "E\u00b7he\u00b7Bett", "er\u00b7nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur Salbung und Geschmuck der sch\u00f6nen Seelen leiht.", "tokens": ["Zur", "Sal\u00b7bung", "und", "Ge\u00b7schmuck", "der", "sch\u00f6\u00b7nen", "See\u00b7len", "leiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Da mu\u00df die Frau den Mann, der Mann mu\u00df seine Frau,", "tokens": ["Da", "mu\u00df", "die", "Frau", "den", "Mann", ",", "der", "Mann", "mu\u00df", "sei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihm der Br\u00e4utigam zur Pflege \u00fcbergeben,", "tokens": ["Die", "ihm", "der", "Br\u00e4u\u00b7ti\u00b7gam", "zur", "Pfle\u00b7ge", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NE", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Ernst bem\u00fchet seyn zur Hochzeit aufzuheben;", "tokens": ["Mit", "Ernst", "be\u00b7m\u00fc\u00b7het", "seyn", "zur", "Hoch\u00b7zeit", "auf\u00b7zu\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Zweck ist, da\u00df man sich nur zier und auferbau.", "tokens": ["Der", "Zweck", "ist", ",", "da\u00df", "man", "sich", "nur", "zier", "und", "au\u00b7fer\u00b7bau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PIS", "PRF", "ADV", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ein Fleck, ein kleiner Staub, was sonst kein Auge kennt,", "tokens": ["Ein", "Fleck", ",", "ein", "klei\u00b7ner", "Staub", ",", "was", "sonst", "kein", "Au\u00b7ge", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sonderlich die Lieb am allerschwersten siehet,", "tokens": ["Was", "son\u00b7der\u00b7lich", "die", "Lieb", "am", "al\u00b7ler\u00b7schwers\u00b7ten", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das alles ist man hie genau zu sehn bem\u00fchet;", "tokens": ["Das", "al\u00b7les", "ist", "man", "hie", "ge\u00b7nau", "zu", "sehn", "be\u00b7m\u00fc\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PIS", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil Stund und Augenblick zum Hochzeit-Tage rennt.", "tokens": ["Weil", "Stund", "und", "Au\u00b7gen\u0