 "pos": ["ADV", "VVFIN", "APPR", "ADJA", "PTKVZ", "$,", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu unsrer Schm\u00fcckungs-Pracht uns gn\u00e4dig zugeleget,", "tokens": ["Zu", "uns\u00b7rer", "Schm\u00fc\u00b7ckungs\u00b7Pracht", "uns", "gn\u00e4\u00b7dig", "zu\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df er sein Br\u00e4ut\u2019gams-Bild uns recht ins Hertz gepr\u00e4get,", "tokens": ["Bi\u00df", "er", "sein", "Br\u00e4ut'\u00b7gams\u00b7Bild", "uns", "recht", "ins", "Hertz", "ge\u00b7pr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die eilen, und der Tag der Hochzeit n\u00e4her her.", "tokens": ["Die", "ei\u00b7len", ",", "und", "der", "Tag", "der", "Hoch\u00b7zeit", "n\u00e4\u00b7her", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KON", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Die Monden la\u00dft uns doch von heute, liebes Kind!", "tokens": ["Die", "Mon\u00b7den", "la\u00dft", "uns", "doch", "von", "heu\u00b7te", ",", "lie\u00b7bes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie w\u00e4hren noch so lang, vor eintzle Tage halten;", "tokens": ["Sie", "w\u00e4h\u00b7ren", "noch", "so", "lang", ",", "vor", "eintz\u00b7le", "Ta\u00b7ge", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit wir in dem Ernst nicht s\u00e4umen noch erkalten,", "tokens": ["Da\u00b7mit", "wir", "in", "dem", "Ernst", "nicht", "s\u00e4u\u00b7men", "noch", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und uns das Braut-Geschrey mit heller Lampe find.", "tokens": ["Und", "uns", "das", "Braut\u00b7Ge\u00b7schrey", "mit", "hel\u00b7ler", "Lam\u00b7pe", "find", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Gelobet sey der HErr, der von dem Tage an,", "tokens": ["Ge\u00b7lo\u00b7bet", "sey", "der", "Herr", ",", "der", "von", "dem", "Ta\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da vor drey Jahren er dich kr\u00e4stig aufgewecket,", "tokens": ["Da", "vor", "drey", "Jah\u00b7ren", "er", "dich", "kr\u00e4s\u00b7tig", "auf\u00b7ge\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit seiner Heiligkeit dich seliglich erschrecket,", "tokens": ["Mit", "sei\u00b7ner", "Hei\u00b7lig\u00b7keit", "dich", "se\u00b7lig\u00b7lich", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und so ins Licht gestellt, viel Heyl an dir gethan.", "tokens": ["Und", "so", "ins", "Licht", "ge\u00b7stellt", ",", "viel", "Heyl", "an", "dir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVPP", "$,", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Di\u00df ", "tokens": ["Di\u00df"], "token_info": ["word"], "pos": ["PDS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Der Tag, Mari\u00e4 Heyl, fand dich zu JEsu F\u00fcssen,", "tokens": ["Der", "Tag", ",", "Ma\u00b7ri\u00e4", "Heyl", ",", "fand", "dich", "zu", "Je\u00b7su", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "NN", "$,", "VVFIN", "PRF", "APPR", "NE", "NN", "$,"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Vor Liebes-Z\u00e4rtlichkeit, und selger Reu zerfliessen;", "tokens": ["Vor", "Lie\u00b7bes\u00b7Z\u00e4rt\u00b7lich\u00b7keit", ",", "und", "sel\u00b7ger", "Reu", "zer\u00b7flies\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum bindt er dich auch an mit der Mari\u00e4 Theil.", "tokens": ["Drum", "bindt", "er", "dich", "auch", "an", "mit", "der", "Ma\u00b7ri\u00e4", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.18": {"line.1": {"text": "Und solte er dir nicht nunmehr ein gantzes Hertz,", "tokens": ["Und", "sol\u00b7te", "er", "dir", "nicht", "nun\u00b7mehr", "ein", "gant\u00b7zes", "Hertz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat es zugesagt, er wird daran gedencken,", "tokens": ["Er", "hat", "es", "zu\u00b7ge\u00b7sagt", ",", "er", "wird", "da\u00b7ran", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gedencke du doch auch so flei\u00dfig Himmel-werts.", "tokens": ["Ge\u00b7den\u00b7cke", "du", "doch", "auch", "so", "flei\u00b7\u00dfig", "Him\u00b7mel\u00b7werts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "An meiner Pflicht dabey will ich nicht s\u00e4umig seyn;", "tokens": ["An", "mei\u00b7ner", "Pflicht", "da\u00b7bey", "will", "ich", "nicht", "s\u00e4u\u0