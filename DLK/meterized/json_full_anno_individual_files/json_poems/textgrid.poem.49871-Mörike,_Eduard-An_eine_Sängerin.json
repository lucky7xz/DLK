{"textgrid.poem.49871": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An eine S\u00e4ngerin", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll auf der Jungfrau Mund die begeisterte Rede verp\u00f6nt sein,", "tokens": ["Soll", "auf", "der", "Jung\u00b7frau", "Mund", "die", "be\u00b7geis\u00b7ter\u00b7te", "Re\u00b7de", "ver\u00b7p\u00f6nt", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ist euch des tiefern Gef\u00fchls volles Bekenntnis versagt:", "tokens": ["Ist", "euch", "des", "tie\u00b7fern", "Ge\u00b7f\u00fchls", "vol\u00b7les", "Be\u00b7kennt\u00b7nis", "ver\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O wie preis ich die S\u00e4ngerin drum, die, unter der Muse", "tokens": ["O", "wie", "preis", "ich", "die", "S\u00e4n\u00b7ge\u00b7rin", "drum", ",", "die", ",", "un\u00b7ter", "der", "Mu\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "NN", "PPER", "ART", "NN", "PAV", "$,", "PRELS", "$,", "APPR", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Schutz, mir den lieblichen Grund ihres Gem\u00fctes enth\u00fcllt!", "tokens": ["Schutz", ",", "mir", "den", "lieb\u00b7li\u00b7chen", "Grund", "ih\u00b7res", "Ge\u00b7m\u00fc\u00b7tes", "ent\u00b7h\u00fcllt", "!"], "token_info": ["word