{"dta.poem.531": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Englis.  Admiral Franc. Dracus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Besch\u00e4mter Hercules prahl nicht mit deinen S\u00e4ulen/", "tokens": ["Be\u00b7sch\u00e4m\u00b7ter", "Her\u00b7cu\u00b7les", "prahl", "nicht", "mit", "dei\u00b7nen", "S\u00e4u\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bey der alten Welt dich trefflich gro\u00df gemacht/", "tokens": ["So", "bey", "der", "al\u00b7ten", "Welt", "dich", "treff\u00b7lich", "gro\u00df", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PRF", "ADJD", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem edlen ", "tokens": ["Dem", "ed\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wel er sein Krieges-Schiff rund um die Welt gebracht.", "tokens": ["Wel", "er", "sein", "Krie\u00b7ge\u00b7sSchiff", "rund", "um", "die", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Du stunbst bey ", "tokens": ["Du", "stunbst", "bey"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Es solte da der Welt umschrencktes Ende seyn/", "tokens": ["Es", "sol\u00b7te", "da", "der", "Welt", "um\u00b7schrenck\u00b7tes", "En\u00b7de", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein dein Schlu\u00df war nicht des edlen ", "tokens": ["Al\u00b7lein", "dein", "Schlu\u00df", "war", "nicht", "des", "ed\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er sprach/ man schliest die Welt nicht in die Schrancken ein.", "tokens": ["Er", "sprach", "/", "man", "schliest", "die", "Welt", "nicht", "in", "die", "Schran\u00b7cken", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PIS", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er lieff durch Ost und West/ durch S\u00fcden und durch Norden/", "tokens": ["Er", "lieff", "durch", "Ost", "und", "West", "/", "durch", "S\u00fc\u00b7den", "und", "durch", "Nor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$(", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er rie\u00df die Wellen durch/ er fuhr ins wilde Meer/", "tokens": ["Er", "rie\u00df", "die", "Wel\u00b7len", "durch", "/", "er", "fuhr", "ins", "wil\u00b7de", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "$(", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und kahm durch einen Weg der nie erh\u00f6ret worden/", "tokens": ["Und", "kahm", "durch", "ei\u00b7nen", "Weg", "der", "nie", "er\u00b7h\u00f6\u00b7ret", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "ART", "NN", "ART", "ADV", "VVPP", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Rund um die Welt herum nach England wieder her.", "tokens": ["Rund", "um", "die", "Welt", "he\u00b7rum", "nach", "En\u00b7gland", "wie\u00b7der", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APZR", "APPR", "NE", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "So must du/ ", "tokens": ["So", "must", "du", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Das himmlische Gestirn macht seinen Ruhm bekannt", "tokens": ["Das", "himm\u00b7li\u00b7sche", "Ge\u00b7stirn", "macht", "sei\u00b7nen", "Ruhm", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du bist zwar R\u00fchmens werth doch ihm nicht zuvergleichen/", "tokens": ["Du", "bist", "zwar", "R\u00fch\u00b7mens", "werth", "doch", "ihm", "nicht", "zu\u00b7ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADJD", "ADV", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er wird ein Wunder-Held von aller Welt genannt.", "tokens": ["Er", "wird", "ein", "Wun\u00b7der\u00b7Held", "von", "al\u00b7ler", "Welt", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}