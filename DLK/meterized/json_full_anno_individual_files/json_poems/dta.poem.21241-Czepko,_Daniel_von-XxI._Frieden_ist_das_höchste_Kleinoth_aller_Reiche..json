{"dta.poem.21241": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XxI.  \n  Frieden ist das h\u00f6chste Kleinoth  \n aller Reiche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Der Grund ist ja gelegt. Man la\u00df Vns jtzt verblasen.", "tokens": ["Der", "Grund", "ist", "ja", "ge\u00b7legt", ".", "Man", "la\u00df", "Vns", "jtzt", "ver\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$.", "PIS", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sind schon arm genung. Die Welt kan nicht mehr", "tokens": ["Wir", "sind", "schon", "arm", "ge\u00b7nung", ".", "Die", "Welt", "kan", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$.", "ART", "NN", "VMFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Was vbrig bleibt vnd ist/ das wird ja kl\u00fcger rasen. (fort.", "tokens": ["Was", "vb\u00b7rig", "bleibt", "vnd", "ist", "/", "das", "wird", "ja", "kl\u00fc\u00b7ger", "ra\u00b7sen", ".", "(", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "KON", "VAFIN", "$(", "PDS", "VAFIN", "ADV", "ADJD", "VVINF", "$.", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Di\u00df Schiff darff kurtze Ruh/ so trifft es seinen Port.", "tokens": ["Di\u00df", "Schiff", "darff", "kurt\u00b7ze", "Ruh", "/", "so", "trifft", "es", "sei\u00b7nen", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was nicht ", "tokens": ["Was", "nicht"], "token_info": ["word", "word"], "pos": ["PWS", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Zu rechte bringt/ das thut de\u00df ", "tokens": ["Zu", "rech\u00b7te", "bringt", "/", "das", "thut", "de\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "$(", "PDS", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da wohnet Gott: Da pflegt sich Gl\u00fcck vnd Heil zuregen/", "tokens": ["Da", "woh\u00b7net", "Gott", ":", "Da", "pflegt", "sich", "Gl\u00fcck", "vnd", "Heil", "zu\u00b7re\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$.", "ADV", "VVFIN", "PRF", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo man den V\u00f6lckern wil die alte Freyheit/ leihn.", "tokens": ["Wo", "man", "den", "V\u00f6l\u00b7ckern", "wil", "die", "al\u00b7te", "Frey\u00b7heit", "/", "leihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VMFIN", "ART", "ADJA", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Lieb ist Grund/ ist Thron: der kan man nichts versagen:", "tokens": ["Die", "Lieb", "ist", "Grund", "/", "ist", "Thron", ":", "der", "kan", "man", "nichts", "ver\u00b7sa\u00b7gen", ":"], "token_info": ["word", "word", "word", 