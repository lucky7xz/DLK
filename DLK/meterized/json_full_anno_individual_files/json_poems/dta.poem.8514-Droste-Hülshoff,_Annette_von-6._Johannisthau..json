{"dta.poem.8514": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "6.  \n Johannisthau.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist die Zeit nun, wo den blauen Tag", "tokens": ["Es", "ist", "die", "Zeit", "nun", ",", "wo", "den", "blau\u00b7en", "Tag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon leiser weckt der Nachtigallen Schlag,", "tokens": ["Schon", "lei\u00b7ser", "weckt", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo schon die Taube in der Mittagsgluth", "tokens": ["Wo", "schon", "die", "Tau\u00b7be", "in", "der", "Mit\u00b7tags\u00b7gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich trunkner, m\u00fcder breitet ob der Brut,", "tokens": ["Sich", "trunk\u00b7ner", ",", "m\u00fc\u00b7der", "brei\u00b7tet", "ob", "der", "Brut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "ADJD", "VVFIN", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo abends, wenn das Sonnengold zergangen,", "tokens": ["Wo", "a\u00b7bends", ",", "wenn", "das", "Son\u00b7nen\u00b7gold", "zer\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verlorner Funke irrt, des Wurmes Schein,", "tokens": ["Ver\u00b7lor\u00b7ner", "Fun\u00b7ke", "irrt", ",", "des", "Wur\u00b7mes", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An allen Ranken Bl\u00fctenb\u00fcschel hangen,", "tokens": ["An", "al\u00b7len", "Ran\u00b7ken", "Bl\u00fc\u00b7ten\u00b7b\u00fc\u00b7schel", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und D\u00fcfte zieh\u2019n in alle Kammern ein.", "tokens": ["Und", "D\u00fcf\u00b7te", "zieh'n", "in", "al\u00b7le", "Kam\u00b7mern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u201eweck\u2019 mich zur rechten Zeit, mein Kamerad,", "tokens": ["\u201e", "weck'", "mich", "zur", "rech\u00b7ten", "Zeit", ",", "mein", "Ka\u00b7me\u00b7rad", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vers\u00e4umen m\u00f6cht\u2019 ich Sanct Johannis Bad", "tokens": ["Ver\u00b7s\u00e4u\u00b7men", "m\u00f6cht'", "ich", "Sanct", "Jo\u00b7han\u00b7nis", "Bad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um Alles nicht; ich hab\u2019 das ganze Jahr", "tokens": ["Um", "Al\u00b7les", "nicht", ";", "ich", "hab'", "das", "gan\u00b7ze", "Jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PTKNEG", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Darauf gehofft, wenn mir so elend war.", "tokens": ["Da\u00b7rauf", "ge\u00b7hofft", ",", "wenn", "mir", "so", "e\u00b7lend", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jerome, du mochtest immer gut es meinen,", "tokens": ["Je\u00b7ro\u00b7me", ",", "du", "moch\u00b7test", "im\u00b7mer", "gut", "es", "mei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Bist auch, wie ich, nur armer Leute Kind.", "tokens": ["Bist", "auch", ",", "wie", "ich", ",", "nur", "ar\u00b7mer", "Leu\u00b7te", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "PPER", "$,", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch hast du klare Augen und die Deinen,", "tokens": ["Doch", "hast", "du", "kla\u00b7re", "Au\u00b7gen", "und", "die", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ich bin eine Waise und halb blind!", "tokens": ["Und", "ich", "bin", "ei\u00b7ne", "Wai\u00b7se", "und", "halb", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hat schon der Hahn gekr\u00e4ht? ich hab\u2019s verfehlt;", "tokens": ["Hat", "schon", "der", "Hahn", "ge\u00b7kr\u00e4ht", "?", "ich", "hab's", "ver\u00b7fehlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Oft schlaf\u2019 ich fest, wenn mich der Schmerz gequ\u00e4lt.", "tokens": ["Oft", "schlaf'", "ich", "fest", ",", "wenn", "mich", "der", "Schmerz", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob schon die D\u00e4mmrung steigt, ich seh\u2019 es nicht,", "tokens": ["Ob", "schon", "die", "D\u00e4mm\u00b7rung", "steigt", ",", "ich", "seh'", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir f\u00e4hrt\u2019s wie Spinneweben am Gesicht;", "tokens": ["Mir", "f\u00e4hrt's", "wie", "Spin\u00b7ne\u00b7we\u00b7ben", "am", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch d\u00fcnkt mich, h\u00f6r\u2019 im Walde ich Gebimmel", "tokens": ["Doch", "d\u00fcnkt", "mich", ",", "h\u00f6r'", "im", "Wal\u00b7de", "ich", "Ge\u00b7bim\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Peitschenknall; was das f\u00fcr F\u00e4den sind,", "tokens": ["Und", "Peit\u00b7schen\u00b7knall", ";", "was", "das", "f\u00fcr", "F\u00e4\u00b7den", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PWS", "PDS", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die mir am Auge schwimmen? lieber Himmel,", "tokens": ["Die", "mir", "am", "Au\u00b7ge", "schwim\u00b7men", "?", "lie\u00b7ber", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$.", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich bin nicht halb, ich bin beinah\u2019 schon blind.", "tokens": ["Ich", "bin", "nicht", "halb", ",", "ich", "bin", "bei\u00b7nah'", "schon", "blind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier ist der Steg am Anger, weiter will", "tokens": ["Hier", "ist", "der", "Steg", "am", "An\u00b7ger", ",", "wei\u00b7ter", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich mich nicht wagen, hier ist Alles still,", "tokens": ["Ich", "mich", "nicht", "wa\u00b7gen", ",", "hier", "ist", "Al\u00b7les", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "PTKNEG", "VVINF", "$,", "ADV", "VAFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Thau genug f\u00fcr Kranke allzumal", "tokens": ["Und", "Thau", "ge\u00b7nug", "f\u00fcr", "Kran\u00b7ke", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des ganzen Weilers, eh\u2019 der Sonnenstrahl", "tokens": ["Des", "gan\u00b7zen", "Wei\u00b7lers", ",", "eh'", "der", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit seinem scharfen Finger ihn gestrichen", "tokens": ["Mit", "sei\u00b7nem", "schar\u00b7fen", "Fin\u00b7ger", "ihn", "ge\u00b7stri\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und aufgesogen ihn der Morgenwind;", "tokens": ["Und", "auf\u00b7ge\u00b7so\u00b7gen", "ihn", "der", "Mor\u00b7gen\u00b7wind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch ist kein Zweiter wohl hieher geschlichen;", "tokens": ["Doch", "ist", "kein", "Zwei\u00b7ter", "wohl", "hie\u00b7her", "ge\u00b7schli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn, Gott sei Dank, nur Wenige sind blind.", "tokens": ["Denn", ",", "Gott", "sei", "Dank", ",", "nur", "We\u00b7ni\u00b7ge", "sind", "blind", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "VAFIN", "NN", "$,", "ADV", "PIAT", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das ist ein B\u00fcschel \u2014 nein \u2014 doch ", "tokens": ["Das", "ist", "ein", "B\u00fc\u00b7schel", "nein", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "PTKANT", "$(", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich f\u00fchle meine Finger kalt und na\u00df;", "tokens": ["Ich", "f\u00fch\u00b7le", "mei\u00b7ne", "Fin\u00b7ger", "kalt", "und", "na\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Johannes, heiliger Prophet, ich kam", "tokens": ["Jo\u00b7han\u00b7nes", ",", "hei\u00b7li\u00b7ger", "Pro\u00b7phet", ",", "ich", "kam"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In deinem werthen Namen her und nahm", "tokens": ["In", "dei\u00b7nem", "wert\u00b7hen", "Na\u00b7men", "her", "und", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von jenem Thaue, den im W\u00fcstenbrande", "tokens": ["Von", "je\u00b7nem", "Thau\u00b7e", ",", "den", "im", "W\u00fcs\u00b7ten\u00b7bran\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Wolke dir getr\u00e4ufelt, lau und lind,", "tokens": ["Die", "Wol\u00b7ke", "dir", "ge\u00b7tr\u00e4u\u00b7felt", ",", "lau", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df nicht dein Auge in dem hei\u00dfen Sande,", "tokens": ["Da\u00df", "nicht", "dein", "Au\u00b7ge", "in", "dem", "hei\u00b7\u00dfen", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht dein gesegnet Auge werde blind.", "tokens": ["Nicht", "dein", "ge\u00b7seg\u00b7net", "Au\u00b7ge", "wer\u00b7de", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "VVPP", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Gepredigt hast du in der Steppengluth \u2014", "tokens": ["Ge\u00b7pre\u00b7digt", "hast", "du", "in", "der", "Step\u00b7pen\u00b7gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wei\u00dft du auch, wie harte Arbeit thut;", "tokens": ["So", "wei\u00dft", "du", "auch", ",", "wie", "har\u00b7te", "Ar\u00b7beit", "thut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch arm und nicht der Arbeit f\u00e4hig sein,", "tokens": ["Doch", "arm", "und", "nicht", "der", "Ar\u00b7beit", "f\u00e4\u00b7hig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "PTKNEG", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist gewi\u00df die allergr\u00f6\u00dfte Pein.", "tokens": ["Das", "ist", "ge\u00b7wi\u00df", "die", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du hast ja kaum geruht in Mutterarmen,", "tokens": ["Du", "hast", "ja", "kaum", "ge\u00b7ruht", "in", "Mut\u00b7ter\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Warst fr\u00fch ein elternlos, verwais\u2019tes Kind,", "tokens": ["Warst", "fr\u00fch", "ein", "el\u00b7tern\u00b7los", ",", "ver\u00b7wais'\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Woll\u2019 eines armen Knaben dich erbarmen,", "tokens": ["Woll'", "ei\u00b7nes", "ar\u00b7men", "Kna\u00b7ben", "dich", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der eine Waise ist wie du, und blind!\u201c", "tokens": ["Der", "ei\u00b7ne", "Wai\u00b7se", "ist", "wie", "du", ",", "und", "blind", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "KOKOM", "PPER", "$,", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}