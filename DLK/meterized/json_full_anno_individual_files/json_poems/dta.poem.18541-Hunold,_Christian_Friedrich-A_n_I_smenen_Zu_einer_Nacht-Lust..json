{"dta.poem.18541": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  I smenen  \n Zu einer Nacht-Lust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Ismene meine Lust! Mein Leben/ mein Vergn\u00fcgen!", "tokens": ["Is\u00b7me\u00b7ne", "mei\u00b7ne", "Lust", "!", "Mein", "Le\u00b7ben", "/", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zeilen lieffert dir ein treuer Diener ein/", "tokens": ["Die", "Zei\u00b7len", "lief\u00b7fert", "dir", "ein", "treu\u00b7er", "Die\u00b7ner", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von dem du ingeheim kanst mehre Nachricht kriegen/", "tokens": ["Von", "dem", "du", "in\u00b7ge\u00b7heim", "kanst", "meh\u00b7re", "Nach\u00b7richt", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPRART", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was meine Seele plagt/ was meine Seufftzer seyn,", "tokens": ["Was", "mei\u00b7ne", "See\u00b7le", "plagt", "/", "was", "mei\u00b7ne", "Seufft\u00b7zer", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sonne will bereits zu Meer und Wellen rinnen/", "tokens": ["Die", "Son\u00b7ne", "will", "be\u00b7reits", "zu", "Meer", "und", "Wel\u00b7len", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und meine Sonne hat mein Auge nicht gesehn!", "tokens": ["Und", "mei\u00b7ne", "Son\u00b7ne", "hat", "mein", "Au\u00b7ge", "nicht", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach! Glaube! kan ich nicht das Gl\u00fccke noch gewinnen/", "tokens": ["Ach", "!", "Glau\u00b7be", "!", "kan", "ich", "nicht", "das", "Gl\u00fc\u00b7cke", "noch", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird mein Leben gar mit deinem Licht vergehn.", "tokens": ["So", "wird", "mein", "Le\u00b7ben", "gar", "mit", "dei\u00b7nem", "Licht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verdient ein plumper Kerrl denn so viel edle Blicke/", "tokens": ["Ver\u00b7di\u00b7ent", "ein", "plum\u00b7per", "Kerrl", "denn", "so", "viel", "ed\u00b7le", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "---+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Da\u00df ihm der Tag zur Lust und mir zum Schmertzen dient?", "tokens": ["Da\u00df", "ihm", "der", "Tag", "zur", "Lust", "und", "mir", "zum", "Schmert\u00b7zen", "dient", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach jage doch einmahl den Hasen-Kopff zur\u00fccke/", "tokens": ["Ach", "ja\u00b7ge", "doch", "ein\u00b7mahl", "den", "Ha\u00b7sen\u00b7Kopff", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor dem nur Kraut und Kohl/ doch keine Rose gr\u00fcnt.", "tokens": ["Vor", "dem", "nur", "Kraut", "und", "Kohl", "/", "doch", "kei\u00b7ne", "Ro\u00b7se", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NN", "KON", "NN", "$(", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein ausge\u00fcbter Geist wird schon die Mittel finden/", "tokens": ["Dein", "aus\u00b7ge\u00b7\u00fcb\u00b7ter", "Geist", "wird", "schon", "die", "Mit\u00b7tel", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie durch verstellte List der St\u00fcmper wird ber\u00fcckt.", "tokens": ["Wie", "durch", "ver\u00b7stell\u00b7te", "List", "der", "St\u00fcm\u00b7per", "wird", "be\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Liebe Sinnen Meer ist niemahls zu ergr\u00fcnden/", "tokens": ["Der", "Lie\u00b7be", "Sin\u00b7nen", "Meer", "ist", "nie\u00b7mahls", "zu", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wie die Schiffarth offt bey Sand und Felsen gl\u00fcckt.", "tokens": ["Und", "wie", "die", "Schiff\u00b7arth", "offt", "bey", "Sand", "und", "Fel\u00b7sen", "gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du kanst - - Allein ich will nicht Stern' an Himmel setzen/", "tokens": ["Du", "kanst", "Al\u00b7lein", "ich", "will", "nicht", "Stern'", "an", "Him\u00b7mel", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "$(", "ADV", "PPER", "VMFIN", "PTKNEG", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "Weil deiner Klugheit Thron schon mit Verwundrung blitzt.", "tokens": ["Weil", "dei\u00b7ner", "Klug\u00b7heit", "Thron", "schon", "mit", "Ver\u00b7wund\u00b7rung", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nein/ winde du das Garn/ ich folge deinen Netzen/", "tokens": ["Nein", "/", "win\u00b7de", "du", "das", "Garn", "/", "ich", "fol\u00b7ge", "dei\u00b7nen", "Net\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "PPER", "ART", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nur schaue/ das uns nicht Gefahr im Wege sitzt.", "tokens": ["Nur", "schau\u00b7e", "/", "das", "uns", "nicht", "Ge\u00b7fahr", "im", "We\u00b7ge", "sitzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PRELS", "PPER", "PTKNEG", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch sch\u00f6nste/ soll ich ja des Hertzens Sinn entdecken/", "tokens": ["Doch", "sch\u00f6ns\u00b7te", "/", "soll", "ich", "ja", "des", "Hert\u00b7zens", "Sinn", "ent\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So scheint dein Garten mir der beste Sammel-Platz:", "tokens": ["So", "scheint", "dein", "Gar\u00b7ten", "mir", "der", "bes\u00b7te", "Sam\u00b7mel\u00b7Platz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn wird ihn nun die Nacht in ihren Flor verstecken/", "tokens": ["Denn", "wird", "ihn", "nun", "die", "Nacht", "in", "ih\u00b7ren", "Flor", "ver\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So h\u00fcllt ihr Forhang mich und meinen liebsten Schatz.", "tokens": ["So", "h\u00fcllt", "ihr", "For\u00b7hang", "mich", "und", "mei\u00b7nen", "liebs\u00b7ten", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Kein Neider wird uns da wie bey der Sonn' ersehen/", "tokens": ["Kein", "Nei\u00b7der", "wird", "uns", "da", "wie", "bey", "der", "Sonn'", "er\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "KOKOM", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil Venus heller Stern vor die verliebten Wacht.", "tokens": ["Weil", "Ve\u00b7nus", "hel\u00b7ler", "Stern", "vor", "die", "ver\u00b7lieb\u00b7ten", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die B\u00fcsche werden still/ als wie Trabanten stehen/", "tokens": ["Die", "B\u00fc\u00b7sche", "wer\u00b7den", "still", "/", "als", "wie", "Tra\u00b7ban\u00b7ten", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "KOUS", "KOKOM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Indem ihr rauschen gleich die Schleicher lautbar macht.", "tokens": ["In\u00b7dem", "ihr", "rau\u00b7schen", "gleich", "die", "Schlei\u00b7cher", "laut\u00b7bar", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So soll uns Einsamkeit mit Zucker Manna speissen/", "tokens": ["So", "soll", "uns", "Ein\u00b7sam\u00b7keit", "mit", "Zu\u00b7cker", "Man\u00b7na", "speis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wodurch in uns entz\u00fcckt sich Seel und Seele k\u00fc\u00dft.", "tokens": ["Wo\u00b7durch", "in", "uns", "ent\u00b7z\u00fcckt", "sich", "Seel", "und", "See\u00b7le", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "VVFIN", "PRF", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die G\u00f6tter werden uns an ihre Tafel Weisen/", "tokens": ["Die", "G\u00f6t\u00b7ter", "wer\u00b7den", "uns", "an", "ih\u00b7re", "Ta\u00b7fel", "Wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo Wollust aufgedeckt/ und Liebe Wirthin ist.", "tokens": ["Wo", "Wol\u00b7lust", "auf\u00b7ge\u00b7deckt", "/", "und", "Lie\u00b7be", "Wirt\u00b7hin", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$(", "KON", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Rosen sollen sich besch\u00e4mt in Schatten h\u00fcllen/", "tokens": ["Die", "Ro\u00b7sen", "sol\u00b7len", "sich", "be\u00b7sch\u00e4mt", "in", "Schat\u00b7ten", "h\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADJD", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wenn mir dein Nelcken-Strau\u00df auf Purpur Lippen prangt:", "tokens": ["Wenn", "mir", "dein", "Nel\u00b7cken\u00b7Strau\u00df", "auf", "Pur\u00b7pur", "Lip\u00b7pen", "prangt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und wird dein Blumen-Busch die frohen Hande f\u00fcllen/", "tokens": ["Und", "wird", "dein", "Blu\u00b7men\u00b7Busch", "die", "fro\u00b7hen", "Han\u00b7de", "f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So hat der Leib auch schon sein Paradie\u00df erlangt.", "tokens": ["So", "hat", "der", "Leib", "auch", "schon", "sein", "Pa\u00b7ra\u00b7die\u00df", "er\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Aurora kan sich nicht so sch\u00f6n ins gr\u00fcne strecken/", "tokens": ["Au\u00b7ro\u00b7ra", "kan", "sich", "nicht", "so", "sch\u00f6n", "ins", "gr\u00fc\u00b7ne", "stre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "PTKNEG", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn sie bey fr\u00fcher Zeit auf feuchten Grasse sitzt/", "tokens": ["Wenn", "sie", "bey", "fr\u00fc\u00b7her", "Zeit", "auf", "feuch\u00b7ten", "Gras\u00b7se", "sitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Als dein erhitzter Leib die Blumen wird bedecken/", "tokens": ["Als", "dein", "er\u00b7hitz\u00b7ter", "Leib", "die", "Blu\u00b7men", "wird", "be\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn unser Perlen Thau auf Mo\u00df und Kr\u00e4uter spritzt.", "tokens": ["Wenn", "un\u00b7ser", "Per\u00b7len", "Thau", "auf", "Mo\u00df", "und", "Kr\u00e4u\u00b7ter", "spritzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPR", "NN", "