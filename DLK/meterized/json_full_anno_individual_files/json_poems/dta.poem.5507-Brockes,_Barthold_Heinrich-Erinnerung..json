{"dta.poem.5507": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verlange nicht zugleich auf Erden,", "tokens": ["Ver\u00b7lan\u00b7ge", "nicht", "zu\u00b7gleich", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bewundert und geliebt zu werden.", "tokens": ["Be\u00b7wun\u00b7dert", "und", "ge\u00b7liebt", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es irret wer darauf besteht,", "tokens": ["Es", "ir\u00b7ret", "wer", "da\u00b7rauf", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ursach, warum es so geht,", "tokens": ["Die", "Ur\u00b7sach", ",", "wa\u00b7rum", "es", "so", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist diese: ", "tokens": ["Ist", "die\u00b7se", ":"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PDS", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Bewundern; jeder will gefallen.", "tokens": ["Be\u00b7wun\u00b7dern", ";", "je\u00b7der", "will", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja bey den meisten gehts noch schlimmer", "tokens": ["Ja", "bey", "den", "meis\u00b7ten", "gehts", "noch", "schlim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "ART", "PIS", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat die\u00df seine Richtigkeit:", "tokens": ["Und", "hat", "die\u00df", "sei\u00b7ne", "Rich\u00b7tig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Fehler, den man nicht verzeiht.", "tokens": ["Ein", "Feh\u00b7ler", ",", "den", "man", "nicht", "ver\u00b7zeiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hiergegen kann ein grosser Geist", "tokens": ["Hier\u00b7ge\u00b7gen", "kann", "ein", "gros\u00b7ser", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil sonst der h\u00e4m\u2019sche Neid von weiten", "tokens": ["Weil", "sonst", "der", "h\u00e4m'\u00b7sche", "Neid", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Steinen immer auf ihn schmeist,", "tokens": ["Mit", "Stei\u00b7nen", "im\u00b7mer", "auf", "ihn", "schmeist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein ander Mittel sich bereiten,", "tokens": ["Kein", "an\u00b7der", "Mit\u00b7tel", "sich", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als wenn er sanfte Sittsamkeit", "tokens": ["Als", "wenn", "er", "sanf\u00b7te", "Sitt\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In allen seinen Thaten weist.", "tokens": ["In", "al\u00b7len", "sei\u00b7nen", "Tha\u00b7ten", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}