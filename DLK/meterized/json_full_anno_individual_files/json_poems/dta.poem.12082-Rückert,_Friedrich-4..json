{"dta.poem.12082": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Regenwolke zieht den d\u00fcrren Gau entlang,", "tokens": ["Die", "Re\u00b7gen\u00b7wol\u00b7ke", "zieht", "den", "d\u00fcr\u00b7ren", "Gau", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Desselben Wegs wohin ein Wandrer nimmt den Gang.", "tokens": ["Des\u00b7sel\u00b7ben", "Wegs", "wo\u00b7hin", "ein", "Wand\u00b7rer", "nimmt", "den", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wo sie heute gie\u00dft und wo sie morgen tr\u00e4uft,", "tokens": ["Und", "wo", "sie", "heu\u00b7te", "gie\u00dft", "und", "wo", "sie", "mor\u00b7gen", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "KON", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trifft sie den Wanderer, der nirgends ihr entl\u00e4uft.", "tokens": ["Trifft", "sie", "den", "Wan\u00b7de\u00b7rer", ",", "der", "nir\u00b7gends", "ihr", "ent\u00b7l\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "\u201eder Boden unter mir ist d\u00fcrr, der Himmel oben", "tokens": ["\u201e", "der", "Bo\u00b7den", "un\u00b7ter", "mir", "ist", "d\u00fcrr", ",", "der", "Him\u00b7mel", "o\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "APPR", "PPER", "VAFIN", "ADJD", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist tr\u00fcb, und Staub und Na\u00df hat mich zugleich bestoben.", "tokens": ["Ist", "tr\u00fcb", ",", "und", "Staub", "und", "Na\u00df", "hat", "mich", "zu\u00b7gleich", "be\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann hinter mir wird gr\u00fcn die Flur, der Himmel helle;", "tokens": ["Dann", "hin\u00b7ter", "mir", "wird", "gr\u00fcn", "die", "Flur", ",", "der", "Him\u00b7mel", "hel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "ADJD", "ART", "NN", "$,", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir kommt es nicht zu gut, ich bin an andrer Stelle.", "tokens": ["Mir", "kommt", "es", "nicht", "zu", "gut", ",", "ich", "bin", "an", "an\u00b7drer", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch einem gl\u00fccklichern, der hinter mir herschreitet,", "tokens": ["Doch", "ei\u00b7nem", "gl\u00fcck\u00b7li\u00b7chern", ",", "der", "hin\u00b7ter", "mir", "her\u00b7schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist neubegr\u00fcnte Flur, neuklare Luft bereitet.\u201c", "tokens": ["Ist", "neu\u00b7be\u00b7gr\u00fcn\u00b7te", "Flur", ",", "neu\u00b7kla\u00b7re", "Luft", "be\u00b7rei\u00b7tet", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}