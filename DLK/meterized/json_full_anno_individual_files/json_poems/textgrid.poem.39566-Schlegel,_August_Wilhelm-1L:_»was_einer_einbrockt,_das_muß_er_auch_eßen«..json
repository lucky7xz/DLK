{"textgrid.poem.39566": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwas einer einbrockt, das mu\u00df er auch e\u00dfen\u00ab.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas einer einbrockt, das mu\u00df er auch e\u00dfen\u00ab.", "tokens": ["\u00bb", "was", "ei\u00b7ner", "ein\u00b7brockt", ",", "das", "mu\u00df", "er", "auch", "e\u00b7\u00dfen", "\u00ab", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PIS", "VVPP", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$(", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hattet den rostigen Spruch ihr verge\u00dfen,", "tokens": ["Hat\u00b7tet", "den", "ros\u00b7ti\u00b7gen", "Spruch", "ihr", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Als ihr die Xenien botet zum Schmaus?", "tokens": ["Als", "ihr", "die", "Xe\u00b7ni\u00b7en", "bo\u00b7tet", "zum", "Schmaus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was ihr gefrevelt in schw\u00e4rmender Jugend,", "tokens": ["Was", "ihr", "ge\u00b7fre\u00b7velt", "in", "schw\u00e4r\u00b7men\u00b7der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kommt euch, bei reiferer m\u00e