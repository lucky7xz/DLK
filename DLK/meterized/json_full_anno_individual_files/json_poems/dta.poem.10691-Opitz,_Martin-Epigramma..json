{"dta.poem.10691": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr zarte Br\u00fcstelein/ Ihr Zuflucht meiner N\u00f6then/", "tokens": ["Ihr", "zar\u00b7te", "Br\u00fcs\u00b7tel\u00b7ein", "/", "Ihr", "Zu\u00b7flucht", "mei\u00b7ner", "N\u00f6\u00b7then", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Lippen von Corall/ jhr Au\u00dfbundt aller Zierd/", "tokens": ["Ihr", "Lip\u00b7pen", "von", "Co\u00b7rall", "/", "jhr", "Au\u00df\u00b7bundt", "al\u00b7ler", "Zierd", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "$(", "PPOSAT", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00f6ndt jhr mich/ da ich doch euch nur beschawendt/ t\u00f6dten/", "tokens": ["K\u00f6ndt", "jhr", "mich", "/", "da", "ich", "doch", "euch", "nur", "be\u00b7scha\u00b7wendt", "/", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "$(", "KOUS", "PPER", "ADV", "PPER", "ADV", "ADJD", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was soltet jhr wol thun/ wann ich euch anger\u00fchrt?", "tokens": ["Was", "sol\u00b7tet", "jhr", "wol", "thun", "/", "wann", "ich", "euch", "an\u00b7ge\u00b7r