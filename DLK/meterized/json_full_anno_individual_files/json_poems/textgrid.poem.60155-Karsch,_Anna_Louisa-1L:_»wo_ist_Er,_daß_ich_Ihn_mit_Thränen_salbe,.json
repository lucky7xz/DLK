{"textgrid.poem.60155": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwo ist Er, da\u00df ich Ihn mit Thr\u00e4nen salbe,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwo ist Er, da\u00df ich Ihn mit Thr\u00e4nen salbe,", "tokens": ["\u00bb", "wo", "ist", "Er", ",", "da\u00df", "ich", "Ihn", "mit", "Thr\u00e4\u00b7nen", "sal\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Sohn? \u2013 Wo ist Er? bringt Ihn mir!\u00ab", "tokens": ["Mein", "Sohn", "?", "\u2013", "Wo", "ist", "Er", "?", "bringt", "Ihn", "mir", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So klagt die F\u00fcrstin! also \u00e4chzt das halbe", "tokens": ["So", "klagt", "die", "F\u00fcrs\u00b7tin", "!", "al\u00b7so", "\u00e4chzt", "das", "hal\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerri\u00dfne Herz in Ihr!", "tokens": ["Zer\u00b7ri\u00df\u00b7ne", "Herz", "in", "Ihr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach! in der Schlacht, voll von des Helden Ruhme", "tokens": ["Ach", "!", "in", "der", "Schlacht", ",", "voll", "von", "des", "Hel\u00b7den", "Ruh\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dacht Er Gefahr und Jugend nicht;", "tokens": ["Dacht", "Er", "Ge\u00b7fahr", "und", "Ju\u00b7gend", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sank! \u2013 \u2013 So sinkt am Abend eine Blume,", "tokens": ["Er", "sank", "!", "\u2013", "\u2013", "So", "sinkt", "am", "A\u00b7bend", "ei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "$(", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sonst ihr Angesicht", "tokens": ["Die", "sonst", "ihr", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vom Stengel nach der Sonnen Antlitz wandte,", "tokens": ["Vom", "Sten\u00b7gel", "nach", "der", "Son\u00b7nen", "Ant\u00b7litz", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nun gebogen niederh\u00e4ngt;", "tokens": ["Und", "nun", "ge\u00b7bo\u00b7gen", "nie\u00b7der\u00b7h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er, dessen Brust zu grossen Thaten brandte,", "tokens": ["Er", ",", "des\u00b7sen", "Brust", "zu", "gros\u00b7sen", "Tha\u00b7ten", "brand\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Bruder nachgedr\u00e4ngt,", "tokens": ["Dem", "Bru\u00b7der", "nach\u00b7ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Stritt wie ein Held, der unterm Waffenrocke", "tokens": ["Stritt", "wie", "ein", "Held", ",", "der", "un\u00b7term", "Waf\u00b7fen\u00b7ro\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon drey\u00dfigmahl das Feld bezog;", "tokens": ["Schon", "drey\u00b7\u00dfig\u00b7mahl", "das", "Feld", "be\u00b7zog", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Staub trug er auf der jugendlichen Locke,", "tokens": ["Staub", "trug", "er", "auf", "der", "ju\u00b7gend\u00b7li\u00b7chen", "Lo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Die um den Nacken flog.", "tokens": ["Die", "um", "den", "Na\u00b7cken", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Feinde flohn. Er, jung und schon ein Sieger,", "tokens": ["Die", "Fein\u00b7de", "flohn", ".", "Er", ",", "jung", "und", "schon", "ein", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "$,", "ADJD", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfand den Sieg und eilte froh", "tokens": ["Emp\u00b7fand", "den", "Sieg", "und", "eil\u00b7te", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie auszusp\u00e4hn. Den wundgewordnen Tieger", "tokens": ["Sie", "aus\u00b7zu\u00b7sp\u00e4hn", ".", "Den", "wund\u00b7ge\u00b7word\u00b7nen", "Tie\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVIZU", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verfolgt ein L\u00f6we so!", "tokens": ["Ver\u00b7folgt", "ein", "L\u00f6\u00b7we", "so", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit einer Kugel auf der Flucht verschossen,", "tokens": ["Mit", "ei\u00b7ner", "Ku\u00b7gel", "auf", "der", "Flucht", "ver\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Traf hinterlistig ihn der Tod.", "tokens": ["Traf", "hin\u00b7ter\u00b7lis\u00b7tig", "ihn", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, Stelle! wo sein Heldenblut geflossen,", "tokens": ["Du", ",", "Stel\u00b7le", "!", "wo", "sein", "Hel\u00b7den\u00b7blut", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "PWAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bleib ewig purpurroth!", "tokens": ["Bleib", "e\u00b7wig", "pur\u00b7pur\u00b7roth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Klagt ihn, ihr H\u00fcgel! und ihr gr\u00fcnen Auen,", "tokens": ["Klagt", "ihn", ",", "ihr", "H\u00fc\u00b7gel", "!", "und", "ihr", "gr\u00fc\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr W\u00e4lder, klaget ihn bey Ham!", "tokens": ["Ihr", "W\u00e4l\u00b7der", ",", "kla\u00b7get", "ihn", "bey", "Ham", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er fiel; So f\u00e4llt, vom K\u00fcnstler umgehauen,", "tokens": ["Er", "fiel", ";", "So", "f\u00e4llt", ",", "vom", "K\u00fcnst\u00b7ler", "um\u00b7ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der jungen Ceder Stamm;", "tokens": ["Der", "jun\u00b7gen", "Ce\u00b7der", "Stamm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nach ihrem Umfall ein geschnizter G\u00f6tze,", "tokens": ["Nach", "ih\u00b7rem", "Um\u00b7fall", "ein", "ge\u00b7schniz\u00b7ter", "G\u00f6t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird Weyrauch vor ihr aufgestreut.", "tokens": ["Wird", "Wey\u00b7rauch", "vor", "ihr", "auf\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So stirbt ein Held, da\u00df Ihn der Nachruhm setze", "tokens": ["So", "stirbt", "ein", "Held", ",", "da\u00df", "Ihn", "der", "Nach\u00b7ruhm", "set\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hin zur Unsterblichkeit", "tokens": ["Hin", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Mit diesem grossen Muth, der im Gefechte", "tokens": ["Mit", "die\u00b7sem", "gros\u00b7sen", "Muth", ",", "der", "im", "Ge\u00b7fech\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz seinem Geiste beygewohnt,", "tokens": ["Ganz", "sei\u00b7nem", "Geis\u00b7te", "bey\u00b7ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah Er den Tod, der keinerley Geschlechte", "tokens": ["Sah", "Er", "den", "Tod", ",", "der", "kei\u00b7ner\u00b7ley", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und keine Tugend schont!", "tokens": ["Und", "kei\u00b7ne", "Tu\u00b7gend", "schont", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Den Helden-Lorbeer um sein Haupt gewunden,", "tokens": ["Den", "Hel\u00b7den\u00b7Lor\u00b7beer", "um", "sein", "Haupt", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Starb er den Tod f\u00fcrs Vaterland!", "tokens": ["Starb", "er", "den", "Tod", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Welch Dichter Moschus Leyer hat gefunden,", "tokens": ["Welch", "Dich\u00b7ter", "Mo\u00b7schus", "Le\u00b7yer", "hat", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nehme sie zur Hand,", "tokens": ["Der", "neh\u00b7me", "sie", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und singe dieses Helden Tod, und klage", "tokens": ["Und", "sin\u00b7ge", "die\u00b7ses", "Hel\u00b7den", "Tod", ",", "und", "kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "NN", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Laut in der Landes T\u00f6chter Thon:", "tokens": ["Laut", "in", "der", "Lan\u00b7des", "T\u00f6ch\u00b7ter", "Thon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbhier fiel im Fr\u00fcling Gott gelebter Tage,", "tokens": ["\u00bb", "hier", "fiel", "im", "Fr\u00fc\u00b7ling", "Gott", "ge\u00b7leb\u00b7ter", "Ta\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPRART", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein kriegrischer Adon!\u00ab", "tokens": ["Ein", "krie\u00b7gri\u00b7scher", "A\u00b7don", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbwo ist Er, da\u00df ich Ihn mit Thr\u00e4nen salbe,", "tokens": ["\u00bb", "wo", "ist", "Er", ",", "da\u00df", "ich", "Ihn", "mit", "Thr\u00e4\u00b7nen", "sal\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Sohn? \u2013 Wo ist Er? bringt Ihn mir!\u00ab", "tokens": ["Mein", "Sohn", "?", "\u2013", "Wo", "ist", "Er", "?", "bringt", "Ihn", "mir", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So klagt die F\u00fcrstin! also \u00e4chzt das halbe", "tokens": ["So", "klagt", "die", "F\u00fcrs\u00b7tin", "!", "al\u00b7so", "\u00e4chzt", "das", "hal\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerri\u00dfne Herz in Ihr!", "tokens": ["Zer\u00b7ri\u00df\u00b7ne", "Herz", "in", "Ihr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ach! in der Schlacht, voll von des Helden Ruhme", "tokens": ["Ach", "!", "in", "der", "Schlacht", ",", "voll", "von", "des", "Hel\u00b7den", "Ruh\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dacht Er Gefahr und Jugend nicht;", "tokens": ["Dacht", "Er", "Ge\u00b7fahr", "und", "Ju\u00b7gend", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sank! \u2013 \u2013 So sinkt am Abe