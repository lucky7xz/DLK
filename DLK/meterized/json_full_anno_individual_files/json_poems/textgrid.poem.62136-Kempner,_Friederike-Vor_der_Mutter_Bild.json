{"textgrid.poem.62136": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Vor der Mutter Bild", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fast strenge sah sie zu mir nieder \u2013", "tokens": ["Fast", "stren\u00b7ge", "sah", "sie", "zu", "mir", "nie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbgefallen Dir nicht meine Lieder,", "tokens": ["\u00bb", "ge\u00b7fal\u00b7len", "Dir", "nicht", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die ich ja oftmals von Dir singe?", "tokens": ["Die", "ich", "ja", "oft\u00b7mals", "von", "Dir", "sin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich nicht gut und treu und bieder?", "tokens": ["Bin", "ich", "nicht", "gut", "und", "treu", "und", "bie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und tu' ich jemals schlechte Dinge?\u00ab", "tokens": ["Und", "tu'", "ich", "je\u00b7mals", "schlech\u00b7te", "Din\u00b7ge", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdu tuest gut, doch nicht so, wie Du's solltest,", "tokens": ["\u00bb", "du", "tu\u00b7est", "gut", ",", "doch", "nicht", "so", ",", "wie", "Du's", "soll\u00b7test", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "$,", "ADV", "PTKNEG", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und lange nicht so gut, als wie Du's wolltest \u2013", "tokens": ["Und", "lan\u00b7ge", "nicht", "so", "gut", ",", "als", "wie", "Du's", "woll\u00b7test", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "KOKOM", "NE", "VMFIN", "$("], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dir ward das h\u00f6chste, schwerste Ziel: Erringe", "tokens": ["Dir", "ward", "das", "h\u00f6chs\u00b7te", ",", "schwers\u00b7te", "Ziel", ":", "Er\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ganz! Sonst sieht es aus, als wenn Du schmolltest,", "tokens": ["Es", "ganz", "!", "Sonst", "sieht", "es", "aus", ",", "als", "wenn", "Du", "schmoll\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df aufgegeben Dir die gr\u00f6\u00dften Dinge!\u00ab", "tokens": ["Da\u00df", "auf\u