{"textgrid.poem.34585": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "8. Lust der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Lust der Welt? Nichts als ein Fastnachtsspiel,", "tokens": ["Was", "ist", "die", "Lust", "der", "Welt", "?", "Nichts", "als", "ein", "Fast\u00b7nachts\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lange Zeit gehofft, in kurtzer Zeit verschwindet,", "tokens": ["So", "lan\u00b7ge", "Zeit", "ge\u00b7hofft", ",", "in", "kurt\u00b7zer", "Zeit", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da unsre Masqven uns nicht hafften, wie man wil,", "tokens": ["Da", "uns\u00b7re", "Mas\u00b7qven", "uns", "nicht", "haff\u00b7ten", ",", "wie", "man", "wil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "$,", "PWAV", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da der Anschlag nicht den Ausschlag recht empfindet.", "tokens": ["Und", "da", "der", "An\u00b7schlag", "nicht", "den", "Aus\u00b7schlag", "recht", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es gehet uns wie dem, der Feuerwercke macht,", "tokens": ["Es", "ge\u00b7het", "uns", "wie", "dem", ",", "der", "Feu\u00b7er\u00b7wer\u00b7cke", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ein Augenblick verzehrt offt eines Jahres Sorgen;", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "ver\u00b7zehrt", "offt", "ei\u00b7nes", "Jah\u00b7res", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man schaut, wie unser Flei\u00df von Kindern wird verlacht,", "tokens": ["Man", "schaut", ",", "wie", "un\u00b7ser", "Flei\u00df", "von", "Kin\u00b7dern", "wird", "ver\u00b7lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Abend tadelt offt den Mittag und den Morgen.", "tokens": ["Der", "A\u00b7bend", "ta\u00b7delt", "offt", "den", "Mit\u00b7tag", "und", "den", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir fluchen offt auf dis, was gestern war gethan,", "tokens": ["Wir", "flu\u00b7chen", "offt", "auf", "dis", ",", "was", "ge\u00b7stern", "war", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDS", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und was man heute k\u00fcst, mu\u00df morgen eckel heissen,", "tokens": ["Und", "was", "man", "heu\u00b7te", "k\u00fcst", ",", "mu\u00df", "mor\u00b7gen", "ec\u00b7kel", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "VVFIN", "$,", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Reimen, die ich itzt geduldig lesen kan,", "tokens": ["Die", "Rei\u00b7men", ",", "die", "ich", "itzt", "ge\u00b7dul\u00b7dig", "le\u00b7sen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die werd ich wohl vielleicht zur Morgenzeit zerreissen.", "tokens": ["Die", "werd", "ich", "wohl", "viel\u00b7leicht", "zur", "Mor\u00b7gen\u00b7zeit", "zer\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir kennen uns, und dis, was unser ist, offt nicht,", "tokens": ["Wir", "ken\u00b7nen", "uns", ",", "und", "dis", ",", "was", "un\u00b7ser", "ist", ",", "offt", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PDS", "$,", "PWS", "PPER", "VAFIN", "$,", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wir tretten unsern Ku\u00df offt selbst mit steiffen F\u00fcssen,", "tokens": ["Wir", "tret\u00b7ten", "un\u00b7sern", "Ku\u00df", "offt", "selbst", "mit", "steif\u00b7fen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man merckt, wie unser Wuntsch ihm selber wiederspricht,", "tokens": ["Man", "merckt", ",", "wie", "un\u00b7ser", "Wunt\u00b7sch", "ihm", "sel\u00b7ber", "wie\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Und wie wir Lust und Zeit als Sclaven dienen m\u00fcssen.", "tokens": ["Und", "wie", "wir", "Lust", "und", "Zeit", "als", "Scla\u00b7ven", "die\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "KON", "NN", "KOUS", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was ist denn diese Lust, und ihre Macht und Pracht?", "tokens": ["Was", "ist", "denn", "die\u00b7se", "Lust", ",", "und", "ih\u00b7re", "Macht", "und", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PDAT", "NN", "$,", "KON", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": 