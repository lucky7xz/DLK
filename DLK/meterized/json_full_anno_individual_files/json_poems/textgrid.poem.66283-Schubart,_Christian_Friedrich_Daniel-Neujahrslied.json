{"textgrid.poem.66283": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrslied", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allvater, Gott und Herr der Zeit!", "tokens": ["All\u00b7va\u00b7ter", ",", "Gott", "und", "Herr", "der", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erh\u00f6r' uns arme Waisen,", "tokens": ["Er\u00b7h\u00f6r'", "uns", "ar\u00b7me", "Wai\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die dich voll Kinderherzlichkeit", "tokens": ["Die", "dich", "voll", "Kin\u00b7der\u00b7herz\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem Liede preisen,", "tokens": ["In", "ei\u00b7nem", "Lie\u00b7de", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Einf\u00e4ltig ist's, doch fromm und wahr.", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "ist's", ",", "doch", "fromm", "und", "wahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir singen dies zum neuen Jahr.", "tokens": ["Wir", "sin\u00b7gen", "dies", "zum", "neu\u00b7en", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erh\u00f6r' uns, lieber Gott.", "tokens": ["Er\u00b7h\u00f6r'", "uns", ",", "lie\u00b7ber", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da\u00df du vom Himmel niederblickst,", "tokens": ["Da\u00df", "du", "vom", "Him\u00b7mel", "nie\u00b7der\u00b7blickst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Geist von unserm Leben;", "tokens": ["Du", "Geist", "von", "un\u00b7serm", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du die ganze Welt begl\u00fcckst,", "tokens": ["Da\u00df", "du", "die", "gan\u00b7ze", "Welt", "be\u00b7gl\u00fcckst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie m\u00fcde wirst im Geben:", "tokens": ["Nie", "m\u00fc\u00b7de", "wirst", "im", "Ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Daf\u00fcr steigt heut' im Zeitenlauf", "tokens": ["Da\u00b7f\u00fcr", "steigt", "heut'", "im", "Zei\u00b7ten\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der Waisen Dank zu dir hinauf;", "tokens": ["Der", "Wai\u00b7sen", "Dank", "zu", "dir", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erh\u00f6r' uns, lieber Gott!", "tokens": ["Er\u00b7h\u00f6r'", "uns", ",", "lie\u00b7ber", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch weil du, Gott, so gn\u00e4dig bist,", "tokens": ["Doch", "weil", "du", ",", "Gott", ",", "so", "gn\u00e4\u00b7dig", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "NN", "$,", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gibst uns ungemessen,", "tokens": ["Und", "gibst", "uns", "un\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn der Mensch auch ", "tokens": ["Und", "wenn", "der", "Mensch", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "So flehen wir: Ach, wende nicht", "tokens": ["So", "fle\u00b7hen", "wir", ":", "Ach", ",", "wen\u00b7de", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ITJ", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von unsrer Welt dein Angesicht,", "tokens": ["Von", "uns\u00b7rer", "Welt", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erh\u00f6r' uns, lieber Gott!", "tokens": ["Er\u00b7h\u00f6r'", "uns", ",", "lie\u00b7ber", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Streck aus der Wolke deine Hand", "tokens": ["Streck", "aus", "der", "Wol\u00b7ke", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit einem Sonnenschilde,", "tokens": ["Mit", "ei\u00b7nem", "Son\u00b7nen\u00b7schil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00fctze unser Vaterland", "tokens": ["Und", "sch\u00fct\u00b7ze", "un\u00b7ser", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deiner Vatermilde.", "tokens": ["Nach", "dei\u00b7ner", "Va\u00b7ter\u00b7mil\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lenk ab von uns des Mordgeists Wuth,", "tokens": ["Lenk", "ab", "von", "uns", "des", "Mord\u00b7geists", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seinen Durst nach unserm Blut.", "tokens": ["Und", "sei\u00b7nen", "Durst", "nach", "un\u00b7serm", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erh\u00f6r' uns, lieber Gott.", "tokens": ["Er\u00b7h\u00f6r'", "uns", ",", "lie\u00b7ber", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gro\u00df ist, o Vater, die Gefahr", "tokens": ["Gro\u00df", "ist", ",", "o", "Va\u00b7ter", ",", "die", "Ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "FM", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr unsern Christenglauben.", "tokens": ["F\u00fcr", "un\u00b7sern", "Chris\u00b7ten\u00b7glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Frevler wollen vom Altar", "tokens": ["Die", "Frev\u00b7ler", "wol\u00b7len", "vom", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kreuz der S\u00fchnung rauben.", "tokens": ["Das", "Kreuz", "der", "S\u00fch\u00b7nung", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "K\u00fchn schm\u00e4hn sie Christum deinen Sohn.", "tokens": ["K\u00fchn", "schm\u00e4hn", "sie", "Chris\u00b7tum", "dei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Steur' ihrem Wahn und ihrem Hohn,", "tokens": ["Steur'", "ih\u00b7rem", "Wahn", "und", "ih\u00b7rem", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erh\u00f6r' uns, lieber Gott.", "tokens": ["Er\u00b7h\u00f6r'", "uns", ",", "lie\u00b7ber", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gib F\u00fcrsten, die dein Ebenbild", "tokens": ["Gib", "F\u00fcrs\u00b7ten", ",", "die", "dein", "E\u00b7ben\u00b7bild"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Unrecht nie entweihen:", "tokens": ["Durch", "Un\u00b7recht", "nie", "ent\u00b7wei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie seien V\u00e4ter, streng und mild", "tokens": ["Sie", "sei\u00b7en", "V\u00e4\u00b7ter", ",", "streng", "und", "mild"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer Kinder Reihen.", "tokens": ["In", "ih\u00b7rer", "Kin\u00b7der", "Rei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verleih der lichtbed\u00fcrft'gen Welt", "tokens": ["Ver\u00b7leih", "der", "licht\u00b7be\u00b7d\u00fcrft'\u00b7gen", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aufkl\u00e4rung, wie sie ", "tokens": ["Auf\u00b7kl\u00e4\u00b7rung", ",", "wie", "sie"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Erh\u00f6r' uns, lieber Gott.", "tokens": ["Er\u00b7h\u00f6r'", "uns", ",", "lie\u00b7ber", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bewahr' uns deutsche Herzlichkeit", "tokens": ["Be\u00b7wahr'", "uns", "deut\u00b7sche", "Herz\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alte Biedersitte.", "tokens": ["Und", "al\u00b7te", "Bie\u00b7der\u00b7sit\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die segenreiche Fruchtbarkeit", "tokens": ["Die", "se\u00b7gen\u00b7rei\u00b7che", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Umstrahle unsre H\u00fctte;", "tokens": ["Um\u00b7strah\u00b7le", "uns\u00b7re", "H\u00fct\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verhindre, da\u00df der Geiz nicht nimmt,", "tokens": ["Ver\u00b7hind\u00b7re", ",", "da\u00df", "der", "Geiz", "nicht", "nimmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punc