{"dta.poem.384": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An ein Frauen-Zimmer so niemand  \n haben will.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du spielest aller Orts wo man nur Karten regt/", "tokens": ["Du", "spie\u00b7lest", "al\u00b7ler", "Orts", "wo", "man", "nur", "Kar\u00b7ten", "regt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PWAV", "PIS", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "We\u00f1 man nun nach dem Daus und dem Triumphe fr\u00e4gt/", "tokens": ["We\u00f1", "man", "nun", "nach", "dem", "Daus", "und", "dem", "Tri\u00b7um\u00b7phe", "fr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "So ist gar keiner dar/ drum must du unten liegen/", "tokens": ["So", "ist", "gar", "kei\u00b7ner", "dar", "/", "drum", "must", "du", "un\u00b7ten", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "PTKVZ", "$(", "PAV", "VMFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kanst zu keiner Zeit mit deinen Karten stegen.", "tokens": ["Und", "kanst", "zu", "kei\u00b7ner", "Zeit", "mit", "dei\u00b7nen", "Kar\u00b7ten", "ste\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun aber sitzst du still/ du bist allzeit labet/", "tokens": ["Nun", "a\u00b7ber", "sitz\u00b7st", "du", "still", "/", "du", "bist", "all\u00b7zeit", "la\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein eintzig Karten-Blat zu deinen Diensten steht/", "tokens": ["Kein", "eint\u00b7zig", "Kar\u00b7ten\u00b7Blat", "zu", "dei\u00b7nen", "Diens\u00b7ten", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch so du in der Zeit das Spielen k\u00f6nnen lassen/", "tokens": ["Doch", "so", "du", "in", "der", "Zeit", "das", "Spie\u00b7len", "k\u00f6n\u00b7nen", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So w\u00fcrdest du nicht jetzt zu deinem Schimpffe passen.", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "nicht", "jetzt", "zu", "dei\u00b7nem", "Schimpf\u00b7fe", "pas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}