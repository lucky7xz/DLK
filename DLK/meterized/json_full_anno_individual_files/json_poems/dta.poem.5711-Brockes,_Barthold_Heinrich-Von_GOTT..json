{"dta.poem.5711": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen Vor-Urtheilen frey", "tokens": ["Von", "al\u00b7len", "Vor\u00b7Urt\u00b7hei\u00b7len", "frey"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird die Vernunfft nun ferner nicht begehren,", "tokens": ["Wird", "die", "Ver\u00b7nunfft", "nun", "fer\u00b7ner", "nicht", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ohn\u2019allen Nutzen, zu erkl\u00e4ren", "tokens": ["Ohn'\u00b7al\u00b7len", "Nut\u00b7zen", ",", "zu", "er\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was eigentlich ein ", "tokens": ["Was", "ei\u00b7gent\u00b7lich", "ein"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "ART"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Was heiss\u2019t Verh\u00e4ngni\u00df sonst? Ein Wort, das uns verf\u00fchrt.", "tokens": ["Was", "heiss't", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "sonst", "?", "Ein", "Wort", ",", "das", "uns", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "ADV", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zwar ist es wahr, da\u00df eine Ursach immer", "tokens": ["Zwar", "ist", "es", "wahr", ",", "da\u00df", "ei\u00b7ne", "Ur\u00b7sach", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein\u2019 andere ber\u00fchrt,", "tokens": ["Ein'", "an\u00b7de\u00b7re", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die eine trifft, es fehlet nimmer,", "tokens": ["Die", "ei\u00b7ne", "trifft", ",", "es", "feh\u00b7let", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die, so ihr folgt. Allein,", "tokens": ["Die", ",", "so", "ihr", "folgt", ".", "Al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPER", "VVFIN", "$.", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Geht man best\u00e4ndig fort; so f\u00fchret diese Kette", "tokens": ["Geht", "man", "be\u00b7st\u00e4n\u00b7dig", "fort", ";", "so", "f\u00fch\u00b7ret", "die\u00b7se", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Uns endlich doch zuletzt zu einem ", "tokens": ["Uns", "end\u00b7lich", "doch", "zu\u00b7letzt", "zu", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So sie zuerst bewegt: Wodurch die Welt entstunde:", "tokens": ["So", "sie", "zu\u00b7erst", "be\u00b7wegt", ":", "Wo\u00b7durch", "die", "Welt", "ent\u00b7stun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "$.", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und also geht der Schlu\u00df der Stoiker zu Grunde.", "tokens": ["Und", "al\u00b7so", "geht", "der", "Schlu\u00df", "der", "Stoi\u00b7ker", "zu", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}