"Am Fu\u00df des Berges noch die magern Halmen", "tokens": ["Am", "Fu\u00df", "des", "Ber\u00b7ges", "noch", "die", "ma\u00b7gern", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Grases, das im Fr\u00fchlings Ueberflu\u00df", "tokens": ["Des", "Gra\u00b7ses", ",", "das", "im", "Fr\u00fch\u00b7lings", "Ue\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort gr\u00fcnte. O, der singe Psalmen", "tokens": ["Dort", "gr\u00fcn\u00b7te", ".", "O", ",", "der", "sin\u00b7ge", "Psal\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Brod nicht suchen mu\u00df!", "tokens": ["Der", "Brod", "nicht", "su\u00b7chen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch wenig Brod bey Freunden deines gleichen", "tokens": ["Doch", "we\u00b7nig", "Brod", "bey", "Freun\u00b7den", "dei\u00b7nes", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey innrer Ruh, ist lieblicher dem Gaum", "tokens": ["Bey", "inn\u00b7rer", "Ruh", ",", "ist", "lieb\u00b7li\u00b7cher", "dem", "Gaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VAFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Tafeln unzufriedner Reichen,", "tokens": ["Als", "Ta\u00b7feln", "un\u00b7zu\u00b7fried\u00b7ner", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ihrer Freunde Traum.", "tokens": ["Als", "ih\u00b7rer", "Freun\u00b7de", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sieh doch, ein V\u00f6lckchen H\u00fchner! ruhig lagen", "tokens": ["Sieh", "doch", ",", "ein", "V\u00f6l\u00b7ck\u00b7chen", "H\u00fch\u00b7ner", "!", "ru\u00b7hig", "la\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "$,", "ART", "ADJA", "NN", "$.", "ADJD", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im hohen welkgewordnen Grase sie.", "tokens": ["Im", "ho\u00b7hen", "welk\u00b7ge\u00b7word\u00b7nen", "Gra\u00b7se", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Flieht nicht vor uns, wir Dichter jagen", "tokens": ["Flieht", "nicht", "vor", "uns", ",", "wir", "Dich\u00b7ter", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den frommen Vogel nie,", "tokens": ["Den", "from\u00b7men", "Vo\u00b7gel", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der ohne Lippe mit dem Schnabel k\u00fcssen", "tokens": ["Der", "oh\u00b7ne", "Lip\u00b7pe", "mit", "dem", "Schna\u00b7bel", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gattin kann, von gleichgeschaffner Art.", "tokens": ["Die", "Gat\u00b7tin", "kann", ",", "von", "gleich\u00b7ge\u00b7schaff\u00b7ner", "Art", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gott, den die H\u00fcgel h\u00f6ren m\u00fcssen", "tokens": ["Gott", ",", "den", "die", "H\u00fc\u00b7gel", "h\u00f6\u00b7ren", "m\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "VMINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hat alles Fleisch gepaart.", "tokens": ["Hat", "al\u00b7les", "Fleisch", "ge\u00b7paart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Auch dich erschuf sein Wille nicht zum Feinde", "tokens": ["Auch", "dich", "er\u00b7schuf", "sein", "Wil\u00b7le", "nicht", "zum", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der M\u00e4dchen, aber keines bindet dich;", "tokens": ["Der", "M\u00e4d\u00b7chen", ",", "a\u00b7ber", "kei\u00b7nes", "bin\u00b7det", "dich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct