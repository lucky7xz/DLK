{"textgrid.poem.25522": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So kann denn selbst die fromme treue Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So kann denn selbst die fromme treue Liebe", "tokens": ["So", "kann", "denn", "selbst", "die", "from\u00b7me", "treu\u00b7e", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gro\u00dfe Sturm zum Schiffbruch' seyn?", "tokens": ["Der", "gro\u00b7\u00dfe", "Sturm", "zum", "Schiff\u00b7bruch'", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich tr\u00e4umte sonst, ihr leises L\u00fcftchen triebe", "tokens": ["Ich", "tr\u00e4um\u00b7te", "sonst", ",", "ihr", "lei\u00b7ses", "L\u00fcft\u00b7chen", "trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den leichten Nachen dieses Lebens", "tokens": ["Den", "leich\u00b7ten", "Na\u00b7chen", "die\u00b7ses", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In deinen Port, o Ruh'! hinein?", "tokens": ["In", "dei\u00b7nen", "Port", ",", "o", "Ruh'", "!", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach! seh' ich dich den Todesbecher trinken,", "tokens": ["Ach", "!", "seh'", "ich", "dich", "den", "To\u00b7des\u00b7be\u00b7cher", "trin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So will ich fort, Romeo, will ihn dir", "tokens": ["So", "will", "ich", "fort", ",", "Ro\u00b7meo", ",", "will", "ihn", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "$,", "NE", "$,", "VMFIN", "PPER", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Entringen, will dir hin zu F\u00fc\u00dfen sinken,", "tokens": ["Ent\u00b7rin\u00b7gen", ",", "will", "dir", "hin", "zu", "F\u00fc\u00b7\u00dfen", "sin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mich um dich klammern, schluchzend bitten:", "tokens": ["Mich", "um", "dich", "klam\u00b7mern", ",", "schluch\u00b7zend", "bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bleib, gro\u00dfe Seele, bleib doch hier!", "tokens": ["Bleib", ",", "gro\u00b7\u00dfe", "See\u00b7le", ",", "bleib", "doch", "hier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch, Julie! wenn du nicht einen Tropfen", "tokens": ["Doch", ",", "Ju\u00b7lie", "!", "wenn", "du", "nicht", "ei\u00b7nen", "Trop\u00b7fen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NE", "$.", "KOUS", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "F\u00fcr dich hast, dann bewein' ich dich!", "tokens": ["F\u00fcr", "dich", "hast", ",", "dann", "be\u00b7wein'", "ich", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mu\u00df nicht die Angst den Lebensquell verstopfen?", "tokens": ["Mu\u00df", "nicht", "die", "Angst", "den", "Le\u00b7bens\u00b7quell", "ver\u00b7stop\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Denn la\u00df ihn flie\u00dfen, und er windet", "tokens": ["Denn", "la\u00df", "ihn", "flie\u00b7\u00dfen", ",", "und", "er", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Durch S\u00fcmpf' ins Thal des Todes sich.", "tokens": ["Durch", "S\u00fcmpf'", "ins", "Thal", "des", "To\u00b7des", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sieh, Amarant! auch mich kannst du verlieren.", "tokens": ["Sieh", ",", "A\u00b7ma\u00b7rant", "!", "auch", "mich", "kannst", "du", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "ADV", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Geschieht's, beweine du mich dann!", "tokens": ["Ge\u00b7schieht's", ",", "be\u00b7wei\u00b7ne", "du", "mich", "dann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Doch auf den Pfad des Todes dich zu f\u00fchren:", "tokens": ["Doch", "auf", "den", "Pfad", "des", "To\u00b7des", "dich", "zu", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Das soll es nicht! denn, ", "tokens": ["Das", "soll", "es", "nicht", "!", "denn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$.", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Das soll es nicht! Es k\u00f6nnt' uns ewig scheiden;", "tokens": ["Das", "soll", "es", "nicht", "!", "Es", "k\u00f6nnt'", "uns", "e\u00b7wig", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$.", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und fliegt mein Geist zum Himmel hin", "tokens": ["Und", "fliegt", "mein", "Geist", "zum", "Him\u00b7mel", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Schon itzt voraus, die zweite meiner Freuden", "tokens": ["Schon", "itzt", "vo\u00b7raus", ",", "die", "zwei\u00b7te", "mei\u00b7ner", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ist, dort auch, die: da\u00df ich auf ewig", "tokens": ["Ist", ",", "dort", "auch", ",", "die", ":", "da\u00df", "ich", "auf", "e\u00b7wig"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "ADV", "ADV", "$,", "PRELS", "$.", "KOUS", "PPER", "APPR", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Bei deiner Liebe selig bin!", "tokens": ["Bei", "dei\u00b7ner", "Lie\u00b7be", "se\u00b7lig", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Das soll es nicht! des Herzens voller G\u00fcte,", "tokens": ["Das", "soll", "es", "nicht", "!", "des", "Her\u00b7zens", "vol\u00b7ler", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Des Kopfs voll Geist, ist diese Welt", "tokens": ["Des", "Kopfs", "voll", "Geist", ",", "ist", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Kaum werth, allein bed\u00fcrftig; und was bl\u00fchte", "tokens": ["Kaum", "werth", ",", "al\u00b7lein", "be\u00b7d\u00fcrf\u00b7tig", ";", "und", "was", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "KON", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "So frisch der Lorbeer, den die Ehre", "tokens": ["So", "frisch", "der", "Lor\u00b7beer", ",", "den", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "F\u00fcr dich in ihren H\u00e4nden h\u00e4lt?", "tokens": ["F\u00fcr", "dich", "in", "ih\u00b7ren", "H\u00e4n\u00b7den", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wenn aber du den Kelch (dem Thoren \u2013 tr\u00fcbe,", "tokens": ["Wenn", "a\u00b7ber", "du", "den", "Kelch", "(", "dem", "Tho\u00b7ren", "\u2013", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "$(", "ART", "NN", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dem Weisen \u2013 klar,) noch vor mir leerst \u2013", "tokens": ["Dem", "Wei\u00b7sen", "\u2013", "klar", ",", ")", "noch", "vor", "mir", "leerst", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJD", "$,", "$(", "ADV", "APPR", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ach! bin ich nicht ein M\u00e4dchen? und voll Liebe?", "tokens": ["Ach", "!", "bin", "ich", "nicht", "ein", "M\u00e4d\u00b7chen", "?", "und", "voll", "Lie\u00b7be", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "$.", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "O guter Gott! und all' ihr Engel!", "tokens": ["O", "gu\u00b7ter", "Gott", "!", "und", "all'", "ihr", "En\u00b7gel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "KON", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Mir, mir den Todestrank zuerst!", "tokens": ["Mir", ",", "mir", "den", "To\u00b7des\u00b7trank", "zu\u00b7erst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So kann denn selbst die fromme treue Liebe", "tokens": ["So", "kann", "denn", "selbst", "die", "from\u00b7me", "treu\u00b7e", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gro\u00dfe Sturm zum Schiffbruch' seyn?", "tokens": ["Der", "gro\u00b7\u00dfe", "Sturm", "zum", "Schiff\u00b7bruch'", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich tr\u00e4umte sonst, ihr leises L\u00fcftchen triebe", "tokens": ["Ich", "tr\u00e4um\u00b7te", "sonst", ",", "ihr", "