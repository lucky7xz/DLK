le", "hat\u00b7ten", "dei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Str\u00f6mten aus der Quelle", "tokens": ["Str\u00f6m\u00b7ten", "aus", "der", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.26": {"text": "So vollauf, da\u00df seines", "tokens": ["So", "vol\u00b7lauf", ",", "da\u00df", "sei\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "PPOSAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Antheils Fern und Nahes", "tokens": ["An\u00b7theils", "Fern", "und", "Na\u00b7hes"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.28": {"text": "Froh ward wie voll Weines;", "tokens": ["Froh", "ward", "wie", "voll", "Wei\u00b7nes", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KOKOM", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Rings ein Meer von L\u00e4cheln", "tokens": ["Rings", "ein", "Meer", "von", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "War ein allgemeines.", "tokens": ["War", "ein", "all\u00b7ge\u00b7mei\u00b7nes", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.31": {"text": "Kann ein Meer verschwinden", "tokens": ["Kann", "ein", "Meer", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text":