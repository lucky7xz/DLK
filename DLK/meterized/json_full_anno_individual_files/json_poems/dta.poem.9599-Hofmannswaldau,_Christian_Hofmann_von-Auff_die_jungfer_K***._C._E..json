{"dta.poem.9599": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff die jungfer K***.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cupido warff im zorn die fackel aus der hand/", "tokens": ["Cu\u00b7pi\u00b7do", "warff", "im", "zorn", "die", "fa\u00b7ckel", "aus", "der", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach: was n\u00fctzt mir mehr dein wunder-voller brand/", "tokens": ["Und", "sprach", ":", "was", "n\u00fctzt", "mir", "mehr", "dein", "wun\u00b7der\u00b7vol\u00b7ler", "brand", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da sich die hertzen itzt in harten stein verkehren/", "tokens": ["Da", "sich", "die", "hert\u00b7zen", "itzt", "in", "har\u00b7ten", "stein", "ver\u00b7keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deine funcken nichts/ denn schnee und ey\u00df/ geb\u00e4hren?", "tokens": ["Und", "dei\u00b7ne", "fun\u00b7cken", "nichts", "/", "denn", "schnee", "und", "ey\u00df", "/", "ge\u00b7b\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIS", "$(", "KON", "VVFIN", "KON", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn feuer/ das wol eh den himmel angesteckt/", "tokens": ["Denn", "feu\u00b7er", "/", "das", "wol", "eh", "den", "him\u00b7mel", "an\u00b7ge\u00b7steckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "PDS", "ADV", "KOUS", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und drauff die unter-welt durch seinen blitz erschreckt/", "tokens": ["Und", "drauff", "die", "un\u00b7ter\u00b7welt", "durch", "sei\u00b7nen", "blitz", "er\u00b7schreckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wei\u00df mehr von keiner glut/ und seine kr\u00e4ffte schwinden/", "tokens": ["Wei\u00df", "mehr", "von", "kei\u00b7ner", "glut", "/", "und", "sei\u00b7ne", "kr\u00e4ff\u00b7te", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die stahl und eisen sonst/ wie d\u00fcnnes rohr/ entz\u00fcnden.", "tokens": ["Die", "stahl", "und", "ei\u00b7sen", "sonst", "/", "wie", "d\u00fcn\u00b7nes", "rohr", "/", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADV", "$(", "KOKOM", "PDS", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verkehrt/ ihr flammen/ auch in nebel/ dampff und nacht/", "tokens": ["Ver\u00b7kehrt", "/", "ihr", "flam\u00b7men", "/", "auch", "in", "ne\u00b7bel", "/", "dampff", "und", "nacht", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "PPER", "VVINF", "$(", "ADV", "APPR", "NE", "$(", "PAV", "KON", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und bleibt durch diesen fluch bey aller welt veracht!", "tokens": ["Und", "bleibt", "durch", "die\u00b7sen", "fluch", "bey", "al\u00b7ler", "welt", "ver\u00b7acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So rieff er noch zuletzt zu den verloschnen br\u00e4nden/", "tokens": ["So", "rieff", "er", "noch", "zu\u00b7letzt", "zu", "den", "ver\u00b7lo\u00b7schnen", "br\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wolte sich hiermit nach seinem bogen wenden;", "tokens": ["Und", "wol\u00b7te", "sich", "hier\u00b7mit", "nach", "sei\u00b7nem", "bo\u00b7gen", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PAV", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach aber! weil er blind und gar zu sicher war/", "tokens": ["Ach", "a\u00b7ber", "!", "weil", "er", "blind", "und", "gar", "zu", "si\u00b7cher", "war", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "KOUS", "PPER", "ADJD", "KON", "ADV", "PTKA", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So hat\u2019 inzwischen ihm die lose Nymphen-schaar", "tokens": ["So", "hat'", "in\u00b7zwi\u00b7schen", "ihm", "die", "lo\u00b7se", "Nym\u00b7phen\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den scharff-gespitzten pfeil und auffgespannten bogen", "tokens": ["Den", "scha\u00b7rff\u00b7ge\u00b7spitz\u00b7ten", "pfeil", "und", "auff\u00b7ge\u00b7spann\u00b7ten", "bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Durch gantz geschwinde list entr\u00fcckt und weggezogen.", "tokens": ["Durch", "gantz", "ge\u00b7schwin\u00b7de", "list", "ent\u00b7r\u00fcckt", "und", "weg\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Cupido ward so bald des diebstahls nicht gewahr/", "tokens": ["Cu\u00b7pi\u00b7do", "ward", "so", "bald", "des", "dieb\u00b7stahls", "nicht", "ge\u00b7wahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "ART", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als er sehr kl\u00e4glich that; bald r\u00e4ufft er sich ins haar/", "tokens": ["Als", "er", "sehr", "kl\u00e4g\u00b7lich", "that", ";", "bald", "r\u00e4ufft", "er", "sich", "ins", "haar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bald schlug er an die brust/ dann fing er an zu weinen/", "tokens": ["Bald", "schlug", "er", "an", "die", "brust", "/", "dann", "fing", "er", "an", "zu", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df erd und himmel selbst empfindlich musten scheinen.", "tokens": ["Da\u00df", "erd", "und", "him\u00b7mel", "selbst", "emp\u00b7find\u00b7lich", "mus\u00b7ten", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wiewohl di\u00df alles war den Nymphen nur ein spott/", "tokens": ["Wie\u00b7wohl", "di\u00df", "al\u00b7les", "war", "den", "Nym\u00b7phen", "nur", "ein", "spott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PIS", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sie h\u00f6hnten noch dazu den kleinen liebes-gott/", "tokens": ["Sie", "h\u00f6hn\u00b7ten", "noch", "da\u00b7zu", "den", "klei\u00b7nen", "lie\u00b7bes\u00b7gott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der/ weil ihm sein gewehr so r\u00e4ubrisch war genommen/", "tokens": ["Der", "/", "weil", "ihm", "sein", "ge\u00b7wehr", "so", "r\u00e4u\u00b7brisch", "war", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "PPOSAT", "ADV", "ADV", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Jtzt ohne bogen nicht dorfft in den himmel kommen.", "tokens": ["Jtzt", "oh\u00b7ne", "bo\u00b7gen", "nicht", "dorfft", "in", "den", "him\u00b7mel", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Er ri\u00df den flor hinweg/ lieff einsam hin und her/", "tokens": ["Er", "ri\u00df", "den", "flor", "hin\u00b7weg", "/", "lieff", "ein\u00b7sam", "hin", "und", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "ADV", "$(", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und klagte den verlust/ als er von ohngefehr", "tokens": ["Und", "klag\u00b7te", "den", "ver\u00b7lust", "/", "als", "er", "von", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die art\u2019ge K*** beym nahmen h\u00f6rte nennen/", "tokens": ["Die", "art'\u00b7ge", "K", "*", "*", "*", "beym", "nah\u00b7men", "h\u00f6r\u00b7te", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "XY", "XY", "XY", "XY", "APPRART", "VVFIN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und drauff begierig ward sie in person zu kennen.", "tokens": ["Und", "drauff", "be\u00b7gie\u00b7rig", "ward", "sie", "in", "per\u00b7son", "zu", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "VAFIN", "PPER", "APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jhr nahme/ sprach er/ fl\u00f6st mir wieder hoffnung ein/", "tokens": ["Ihr", "nah\u00b7me", "/", "sprach", "er", "/", "fl\u00f6st", "mir", "wie\u00b7der", "hoff\u00b7nung", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "ADV", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich werde mehr durch sie/ als iemals/ gl\u00fccklich seyn:", "tokens": ["Ich", "wer\u00b7de", "mehr", "durch", "sie", "/", "als", "ie\u00b7mals", "/", "gl\u00fcck\u00b7lich", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "$(", "KOKOM", "ADV", "$(", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und ob schon gram und leid mir itzt die fl\u00fcgel binden;", "tokens": ["Und", "ob", "schon", "gram", "und", "leid", "mir", "itzt", "die", "fl\u00fc\u00b7gel", "bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "KON", "ADJD", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Doch wieder s\u00fcssen trost in ihrem umbgang finden.", "tokens": ["Doch", "wie\u00b7der", "s\u00fcs\u00b7sen", "trost", "in", "ih\u00b7rem", "umb\u00b7gang", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wer kennt den zunder nicht und die verdeckte glut/", "tokens": ["Wer", "kennt", "den", "zun\u00b7der", "nicht", "und", "die", "ver\u00b7deck\u00b7te", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und was ein k\u00fchnstock sonst vor seltne wunder thut?", "tokens": ["Und", "was", "ein", "k\u00fchn\u00b7stock", "sonst", "vor", "selt\u00b7ne", "wun\u00b7der", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Gewi\u00df! hier findet man die nahrung s\u00fcsser flammen/", "tokens": ["Ge\u00b7wi\u00df", "!", "hier", "fin\u00b7det", "man", "die", "nah\u00b7rung", "s\u00fcs\u00b7ser", "flam\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die selbst der himmel nicht kan richten noch verdammen.", "tokens": ["Die", "selbst", "der", "him\u00b7mel", "nicht", "kan", "rich\u00b7ten", "noch", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKNEG", "VMFIN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So sprach er gantz erfreut/ als eben drauff geschah/", "tokens": ["So", "sprach", "er", "gantz", "er\u00b7freut", "/", "als", "e\u00b7ben", "drauff", "ge\u00b7schah", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$(", "KOKOM", "ADV", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df er die K*** urpl\u00f6tzlich vor sich sah/", "tokens": ["Da\u00df", "er", "die", "K", "*", "*", "*", "ur\u00b7pl\u00f6tz\u00b7lich", "vor", "sich", "sah", "/"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "XY", "XY", "XY", "XY", "ADJD", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Er sah den klugen mund/ die augen/ brust und wangen/", "tokens": ["Er", "sah", "den", "klu\u00b7gen", "mund", "/", "die", "au\u00b7gen", "/", "brust", "und", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ach! dacht er bey sich selbst: was hertzen werd\u2019 ich fang