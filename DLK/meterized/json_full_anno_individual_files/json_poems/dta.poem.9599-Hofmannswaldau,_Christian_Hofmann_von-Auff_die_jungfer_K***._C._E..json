fa\u00b7ckeln", "/", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "ADV", "$(", "KOUS", "PPOSAT", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Di\u00df war sein letztes wort/ worauff er gleich zur hand", "tokens": ["Di\u00df", "war", "sein", "letz\u00b7tes", "wort", "/", "wo\u00b7rauff", "er", "gleich", "zur", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "PWAV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Sich mit der K*** durch eyd und pflicht verband/", "tokens": ["Sich", "mit", "der", "K", "*", "*", "*", "durch", "eyd", "und", "pflicht", "ver\u00b7band", "/"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "XY", "XY", "XY", "XY", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Die itzt/ wohin sie nur die s\u00fcssen augen wendet/", "tokens": ["Die", "itzt", "/", "wo\u00b7hin", "sie", "nur", "die", "s\u00fcs\u00b7sen", "au\u00b7gen", "wen\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Verwirrung/ feur und glut durch tausend hertzen sendet.", "tokens": ["Ver\u00b7wir\u00b7rung", "/", "feur", "und", "glut", "durch", "tau\u00b7send", "hert\u00b7zen", "sen\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "p