{"textgrid.poem.40653": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch Wiedersehn! Zerst\u00f6rung und Entsetzen!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Wiedersehn! Zerst\u00f6rung und Entsetzen!", "tokens": ["Welch", "Wie\u00b7der\u00b7sehn", "!", "Zer\u00b7st\u00f6\u00b7rung", "und", "Ent\u00b7set\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein pr\u00e4cht'ger Vollmondhimmel war dein Tr\u00e4umen;", "tokens": ["Ein", "pr\u00e4cht'\u00b7ger", "Voll\u00b7mond\u00b7him\u00b7mel", "war", "dein", "Tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt prasseln Sterne, fallend, in den R\u00e4umen,", "tokens": ["Jetzt", "pras\u00b7seln", "Ster\u00b7ne", ",", "fal\u00b7lend", ",", "in", "den", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durchras't von Blitzeskn\u00e4ueln, Wolkenfetzen.", "tokens": ["Durch\u00b7ras't", "von", "Blit\u00b7zes\u00b7kn\u00e4u\u00b7eln", ",", "Wol\u00b7ken\u00b7fet\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Ich beb' \u2013 und soll vielleicht dich gl\u00fccklich sch\u00e4tzen!", "tokens": ["Ich", "beb'", "\u2013", "und", "soll", "viel\u00b7leicht", "dich", "gl\u00fcck\u00b7lich", "sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VMFIN", "ADV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Krankheit vielleicht ist h\u00f6hern Lebens Sch\u00e4umen.", "tokens": ["Krank\u00b7heit", "viel\u00b7leicht", "ist", "h\u00f6\u00b7hern", "Le\u00b7bens", "Sch\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wir sehn das schwarze Zauberro\u00df sich b\u00e4umen,", "tokens": ["Wir", "sehn", "das", "schwar\u00b7ze", "Zau\u00b7berr\u00b7o\u00df", "sich", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wild rei\u00dft es aus, gespornt, in scheuen S\u00e4tzen.", "tokens": ["Wild", "rei\u00dft", "es", "aus", ",", "ge\u00b7spornt", ",", "in", "scheu\u00b7en", "S\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein k\u00fchner Reiter ohne Zaum und Decken,", "tokens": ["Ein", "k\u00fch\u00b7ner", "Rei\u00b7ter", "oh\u00b7ne", "Zaum", "und", "De\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text":