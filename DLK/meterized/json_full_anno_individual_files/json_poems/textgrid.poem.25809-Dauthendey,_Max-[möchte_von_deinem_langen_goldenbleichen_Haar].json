{"textgrid.poem.25809": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[m\u00f6chte von deinem langen goldenbleichen Haar]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chte von deinem langen goldenbleichen Haar", "tokens": ["M\u00f6ch\u00b7te", "von", "dei\u00b7nem", "lan\u00b7gen", "gol\u00b7den\u00b7blei\u00b7chen", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ein Lager mir bekleiden.", "tokens": ["Ein", "La\u00b7ger", "mir", "be\u00b7klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seide w\u00e4re Stroh, Sammet \u2013 Igelhaut,", "tokens": ["Sei\u00b7de", "w\u00e4\u00b7re", "Stroh", ",", "Sam\u00b7met", "\u2013", "I\u00b7gel\u00b7haut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "NN", "$(", "NE", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Aber dein Haar ist wie ein golden Wolkenbett,", "tokens": ["A\u00b7ber", "dein", "Haar", "ist", "wie", "ein", "gol\u00b7den", "Wol\u00b7ken\u00b7bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wie man's am Abend glei\u00dfend nur im \u00c4ther schaut. \u2013", "tokens": ["Wie", "man's", "am", "A\u00b7bend", "glei\u00b7\u00dfend", "nur", "im", "\u00c4\u00b7ther", "schaut", ".", "\u2013"], "token_info": ["word", "word"