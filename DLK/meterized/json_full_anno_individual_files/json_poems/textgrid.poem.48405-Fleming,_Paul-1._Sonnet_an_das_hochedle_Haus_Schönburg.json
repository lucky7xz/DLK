{"textgrid.poem.48405": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1. Sonnet an das hochedle Haus Sch\u00f6nburg", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nburg, du sch\u00f6nes Haus, wie tustu ietzund klagen,", "tokens": ["Sch\u00f6n\u00b7burg", ",", "du", "sch\u00f6\u00b7nes", "Haus", ",", "wie", "tus\u00b7tu", "ie\u00b7tzund", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,", "PWAV", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "indem ein gro\u00dfes Teil von deiner Sch\u00f6nheit f\u00e4llt", "tokens": ["in\u00b7dem", "ein", "gro\u00b7\u00dfes", "Teil", "von", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und wird gerissen hin, darvon die Mei\u00dfner Welt", "tokens": ["und", "wird", "ge\u00b7ris\u00b7sen", "hin", ",", "dar\u00b7von", "die", "Mei\u00df\u00b7ner", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "PTKVZ", "$,", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und Ieder, wer dich kennt, mit Trauren wei\u00df zu sagen!", "tokens": ["und", "Ie\u00b7der", ",", "wer", "dich", "kennt", ",", "mit", "Trau\u00b7ren", "wei\u00df", "zu", "sa\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie solte diesen Fall denn unbetrauret tragen", "tokens": ["Wie", "sol\u00b7te", "die\u00b7sen", "Fall", "denn", "un\u00b7be\u00b7trau\u00b7ret", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PDAT", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ich, der ich ohne dich in lauter Trauren bin", "tokens": ["ich", ",", "der", "ich", "oh\u00b7ne", "dich", "in", "lau\u00b7ter", "Trau\u00b7ren", "bin"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "PRF", "APPR", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und gleichsam lebe tot? ich, den du mich vorhin", "tokens": ["und", "gleich\u00b7sam", "le\u00b7be", "tot", "?", "ich", ",", "den", "du", "mich", "vor\u00b7hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADJD", "$.", "PPER", "$,", "PRELS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "mit Gnade dir erkauft? Drumb weil mir deine Plagen", "tokens": ["mit", "Gna\u00b7de", "dir", "er\u00b7kauft", "?", "Drumb", "weil", "mir", "dei\u00b7ne", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$.", "PAV", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und \u00fcbergro\u00dfes Leid durch Herz und Seele geht,", "tokens": ["und", "\u00fc\u00b7ber\u00b7gro\u00b7\u00dfes", "Leid", "durch", "Herz", "und", "See\u00b7le", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "wolan, so nimb von dem, der dir zu eigen steht", "tokens": ["wo\u00b7lan", ",", "so", "nimb", "von", "dem", ",", "der", "dir", "zu", "ei\u00b7gen", "steht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "APPR", "ART", "$,", "PRELS", "PPER", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "mit Allem, was er ist, die Schrift zu einem Pfande", "tokens": ["mit", "Al\u00b7lem", ",", "was", "er", "ist", ",", "die", "Schrift", "zu", "ei\u00b7nem", "Pfan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VAFIN", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der reinen Dankbarkeit, die Schrift, die Trauerschrift,", "tokens": ["der", "rei\u00b7nen", "Dank\u00b7bar\u00b7keit", ",", "die", "Schrift", ",", "die", "Trau\u00b7er\u00b7schrift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "die mit dir weinen soll! Was f\u00f6rder dich betrifft,", "tokens": ["die", "mit", "dir", "wei\u00b7nen", "soll", "!", "Was", "f\u00f6r\u00b7der", "dich", "be\u00b7tr\u00b7ifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "VMFIN", "$.", "PWS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "so scheine, sch\u00f6nes Haus, dem lieben Vaterlande!", "tokens": ["so", "schei\u00b7ne", ",", "sch\u00f6\u00b7nes", "Haus", ",", "dem", "lie\u00b7ben", "Va\u00b7ter\u00b7lan\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nburg, du sch\u00f6nes Haus, wie tustu ietzund klagen,", "tokens": ["Sch\u00f6n\u00b7burg", ",", "du", "sch\u00f6\u00b7nes", "Haus", ",", "wie", "tus\u00b7tu", "ie\u00b7tzund", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,", "PWAV", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "indem ein gro\u00dfes Teil von deiner Sch\u00f6nheit f\u00e4llt", "tokens": ["in\u00b7dem", "ein", "gro\u00b7\u00dfes", "Teil", "von", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und wird gerissen hin, darvon die Mei\u00dfner Welt", "tokens": ["und", "wird", "ge\u00b7ris\u00b7sen", "hin", ",", "dar\u00b7von", "die", "Mei\u00df\u00b7ner", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "PTKVZ", "$,", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und Ieder, wer dich kennt, mit Trauren wei\u00df zu sagen!", "tokens": ["und"