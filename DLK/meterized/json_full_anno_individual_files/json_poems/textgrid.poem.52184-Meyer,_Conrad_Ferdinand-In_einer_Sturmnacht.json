{"textgrid.poem.52184": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "In einer Sturmnacht", "genre": "verse", "period": "N.A.", "pub_year": 1883, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4hrt der Wind gewaltig durch die Nacht,", "tokens": ["Es", "f\u00e4hrt", "der", "Wind", "ge\u00b7wal\u00b7tig", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In seine gellen Pfeifen bl\u00e4st der F\u00f6hn.", "tokens": ["In", "sei\u00b7ne", "gel\u00b7len", "Pfei\u00b7fen", "bl\u00e4st", "der", "F\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Prophetisch k\u00e4mpft am Himmel eine Schlacht", "tokens": ["Pro\u00b7phe\u00b7tisch", "k\u00e4mpft", "am", "Him\u00b7mel", "ei\u00b7ne", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcberschreit ein wimmernd Sterbgest\u00f6hn.", "tokens": ["Und", "\u00fc\u00b7bersc\u00b7hreit", "ein", "wim\u00b7mernd", "Sterb\u00b7ge\u00b7st\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was jetzt d\u00e4monenhaft in L\u00fcften zieht,", "tokens": ["Was", "jetzt", "d\u00e4\u00b7mo\u00b7nen\u00b7haft", "in", "L\u00fcf\u00b7ten", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Eh das Jahrhundert schlie\u00dft, erf\u00fcllt's die Zeit \u2013", "tokens": ["Eh", "das", "Jahr\u00b7hun\u00b7dert", "schlie\u00dft", ",", "er\u00b7f\u00fcllt's", "die", "Zeit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "In Sturmespausen klingt das Friedelied", "tokens": ["In", "Stur\u00b7me\u00b7spau\u00b7sen", "klingt", "das", "Frie\u00b7de\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus einer fernen, fernen Seligkeit.", "tokens": ["Aus", "ei\u00b7ner", "fer\u00b7nen", ",", "fer\u00b7nen", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Ampel, die in leichten Ketten hangt,", "tokens": ["Die", "Am\u00b7pel", ",", "die", "in", "leich\u00b7ten", "Ket\u00b7ten", "hangt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hellt meiner Kammer weite D\u00e4mmerung.", "tokens": ["Hellt", "mei\u00b7ner", "Kam\u00b7mer", "wei\u00b7te", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und wann die Decke be