{"textgrid.poem.66833": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: So la\u00dft mich schrein! Ihr sollt den Herzschlag h\u00f6ren!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So la\u00dft mich schrein! Ihr sollt den Herzschlag h\u00f6ren!", "tokens": ["So", "la\u00dft", "mich", "schrein", "!", "Ihr", "sollt", "den", "Herz\u00b7schlag", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es geht nicht mehr, die Schurkerei zermalmt", "tokens": ["Es", "geht", "nicht", "mehr", ",", "die", "Schur\u00b7ke\u00b7rei", "zer\u00b7malmt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Geist mir noch. So mu\u00df ich ", "tokens": ["Den", "Geist", "mir", "noch", ".", "So", "mu\u00df", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ADV", "$.", "ADV", "VMFIN", "PPER"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Da drau\u00dfen bl\u00fcht der gr\u00fcne Sommer mir", "tokens": ["Da", "drau\u00b7\u00dfen", "bl\u00fcht", "der", "gr\u00fc\u00b7ne", "Som\u00b7mer", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In frischer Unschuld Purpurglut entgegen,", "tokens": ["In", "fri\u00b7scher", "Un\u00b7schuld", "Pur\u00b7pur\u00b7glut", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Glocken l\u00e4uten heimlich durch das Tal,", "tokens": ["Die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "heim\u00b7lich", "durch", "das", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Rosen duften durch die milden L\u00fcfte,", "tokens": ["Und", "Ro\u00b7sen", "duf\u00b7ten", "durch", "die", "mil\u00b7den", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit Fichtenhauch von Bergesh\u00f6h verm\u00e4hlt.", "tokens": ["Mit", "Fich\u00b7ten\u00b7hauch", "von", "Ber\u00b7ges\u00b7h\u00f6h", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch knirschend rasselt, qualmausspeiend, heulend", "tokens": ["Doch", "knir\u00b7schend", "ras\u00b7selt", ",", "qual\u00b7maus\u00b7spei\u00b7end", ",", "heu\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Des Menschenelends Walzenunget\u00fcm", "tokens": ["Des", "Men\u00b7sche\u00b7ne\u00b7lends", "Wal\u00b7ze\u00b7nun\u00b7ge\u00b7t\u00fcm"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Erbarmungslos durchs Bl\u00fctenreich der Seele.", "tokens": ["Er\u00b7bar\u00b7mungs\u00b7los", "durchs", "Bl\u00fc\u00b7ten\u00b7reich", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Es br\u00fcllt der Grund, und Gott, der Geist, erdonnert.", "tokens": ["Es", "br\u00fcllt", "der", "Grund", ",", "und", "Gott", ",", "der", "Geist", ",", "er\u00b7don\u00b7nert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "NN", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Kommt aus den Kerkern mir, Zuchth\u00e4usler, kommt!", "tokens": ["Kommt", "aus", "den", "Ker\u00b7kern", "mir", ",", "Zucht\u00b7h\u00e4us\u00b7ler", ",", "kommt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "An eure harten Herzen will ich klopfen", "tokens": ["An", "eu\u00b7re", "har\u00b7ten", "Her\u00b7zen", "will", "ich", "klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nach Menschlichkeit, die diese Welt verriet.", "tokens": ["Nach", "Menschlich\u00b7keit", ",", "die", "die\u00b7se", "Welt", "ver\u00b7riet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Ihr seid doch Kerle, wenn ihr Schurken seid,", "tokens": ["Ihr", "seid", "doch", "Ker\u00b7le", ",", "wenn", "ihr", "Schur\u00b7ken", "seid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und meinen Hut zieh ich vor euch zu Boden", "tokens": ["Und", "mei\u00b7nen", "Hut", "zieh", "ich", "vor", "euch", "zu", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Aus Achtung t\u00fcchtigen Halunkentums.", "tokens": ["Aus", "Ach\u00b7tung", "t\u00fcch\u00b7ti\u00b7gen", "Ha\u00b7lun\u00b7ken\u00b7tums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doch dieser st\u00fcmpernden Banditensippe,", "tokens": ["Doch", "die\u00b7ser", "st\u00fcm\u00b7pern\u00b7den", "Ban\u00b7di\u00b7ten\u00b7sip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Die mit Moral Erb\u00e4rmlichkeit maskiert", "tokens": ["Die", "mit", "Mo\u00b7ral", "Er\u00b7b\u00e4rm\u00b7lich\u00b7keit", "mas\u00b7kiert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und die Gemeinheit \u00bbvornehm\u00ab \u00fcbert\u00fcncht,", "tokens": ["Und", "die", "Ge\u00b7mein\u00b7heit", "\u00bb", "vor\u00b7nehm", "\u00ab", "\u00fc\u00b7ber\u00b7t\u00fcncht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "ADJD", "$(", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Kehr' ich, von \u00dcbelkeit geplagt, den R\u00fccken.", "tokens": ["Kehr'", "ich", ",", "von", "\u00dc\u00b7bel\u00b7keit", "ge\u00b7plagt", ",", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Hol' euch der Geier, eh der Fr\u00fchhahn kr\u00e4ht!", "tokens": ["Hol'", "euch", "der", "Gei\u00b7er", ",", "eh", "der", "Fr\u00fch\u00b7hahn", "kr\u00e4ht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Krepiert am Giftsud eurer Heuchelei,", "tokens": ["Kre\u00b7piert", "am", "Gif\u00b7tsud", "eu\u00b7rer", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Am Fallstrick baumelt eurer Pfiffigkeiten,", "tokens": ["Am", "Fall\u00b7strick", "bau\u00b7melt", "eu\u00b7rer", "Pfif\u00b7fig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Im Eiterozean der L\u00fcg' ersauft!", "tokens": ["Im", "Ei\u00b7te\u00b7ro\u00b7ze\u00b7an", "der", "L\u00fcg'", "er\u00b7sauft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Verkohlt im Scheiterhaufen eurer Schande,", "tokens": ["Ver\u00b7kohlt", "im", "Schei\u00b7ter\u00b7hau\u00b7fen", "eu\u00b7rer", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Die Hochmutsguillotine k\u00f6pf' euch, scharf", "tokens": ["Die", "Hoch\u00b7muts\u00b7guil\u00b7lo\u00b7ti\u00b7ne", "k\u00f6pf'", "euch", ",", "scharf"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Schie\u00df' euch die eigne Frechheit \u00fcbern Haufen!", "tokens": ["Schie\u00df'", "euch", "die", "eig\u00b7ne", "Frech\u00b7heit", "\u00fc\u00b7bern", "Hau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Aus meiner weichen J\u00fcnglingsbrust ausrod' ich", "tokens": ["Aus", "mei\u00b7ner", "wei\u00b7chen", "J\u00fcng\u00b7lings\u00b7brust", "aus\u00b7rod'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Des Mitleids Wurzel, die um euch sich schlingt.", "tokens": ["Des", "Mit\u00b7leids", "Wur\u00b7zel", ",", "die", "um", "euch", "sich", "schlingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Mitleid wird Schuld, verkuppelt sich's der F\u00e4ulnis,", "tokens": ["Mit\u00b7leid", "wird", "Schuld", ",", "ver\u00b7kup\u00b7pelt", "sich's", "der", "F\u00e4ul\u00b7nis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Die frech-gefr\u00e4\u00dfig Menschenwert zerst\u00f6rt,", "tokens": ["Die", "frech\u00b7ge\u00b7fr\u00e4\u00b7\u00dfig", "Men\u00b7schen\u00b7wert", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Duldung Verbrechen, S\u00fcnde die Vers\u00f6hnung,", "tokens": ["Dul\u00b7dung", "Ver\u00b7bre\u00b7chen", ",", "S\u00fcn\u00b7de", "die", "Ver\u00b7s\u00f6h\u00b7nung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.35": {"text": "Und Ha\u00df zum Tode heilige Menschenpflicht.", "tokens": ["Und", "Ha\u00df", "zum", "To\u00b7de", "hei\u00b7li\u00b7ge", "Men\u00b7schen\u00b7pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Mit Hassesschrei, auf Schwingen der Verachtung,", "tokens": ["Mit", "Has\u00b7ses\u00b7schrei", ",", "auf", "Schwin\u00b7gen", "der", "Ver\u00b7ach\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Im dunklen Aug' der Wahrheit Blitz und Schrecken,", "tokens": ["Im", "dunk\u00b7len", "Aug'", "der", "Wahr\u00b7heit", "Blitz", "und", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "So rauscht der Aar durch Pestglut und Verwesung", "tokens": ["So", "rauscht", "der", "Aar", "durch", "Pest\u00b7glut", "und", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Den Kordilleren k\u00fchner Zeiten zu.", "tokens": ["Den", "Kor\u00b7dil\u00b7le\u00b7ren", "k\u00fch\u00b7ner", "Zei\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Tief unten stink, du ekelhaftes Aas,", "tokens": ["Tief", "un\u00b7ten", "stink", ",", "du", "e\u00b7kel\u00b7haf\u00b7tes", "Aas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Wo Kr\u00e4h'n und Raben weidend Atzung halten", "tokens": ["Wo", "Kr\u00e4h'n", "und", "Ra\u00b7ben", "wei\u00b7dend", "At\u00b7zung", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Und sich behaglich m\u00e4sten mit dem Kot", "tokens": ["Und", "sich", "be\u00b7hag\u00b7lich", "m\u00e4s\u00b7ten", "mit", "dem", "Kot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Der Feigheit, Roheit, Dummheit, Faulheit, L\u00fcge!", "tokens": ["Der", "Feig\u00b7heit", ",", "Ro\u00b7heit", ",", "Dumm\u00b7heit", ",", "Faul\u00b7heit", ",", "L\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Im gr\u00fcnen Schlamm der Knechtsgewohnheit w\u00e4lzen", "tokens": ["Im", "gr\u00fc\u00b7nen", "Schlamm", "der", "Knechts\u00b7ge\u00b7wohn\u00b7heit", "w\u00e4l\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Die fetten Ottern des Jahrhunderts sich", "tokens": ["Die", "fet\u00b7ten", "Ot\u00b7tern", "des", "Jahr\u00b7hun\u00b7derts", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PRF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Und ringeln sich und lecken sich die Schw\u00e4nze", "tokens": ["Und", "rin\u00b7geln", "sich", "und", "le\u00b7cken", "sich", "die", "Schw\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Und zischen tausendz\u00fcngig giftig auf,", "tokens": ["Und", "zi\u00b7schen", "tau\u00b7send\u00b7z\u00fcn\u00b7gig", "gif\u00b7tig", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Fliegt \u00fcber sie der stolze Weih der Wahrheit,", "tokens": ["Fliegt", "\u00fc\u00b7ber", "sie", "der", "stol\u00b7ze", "Weih", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "w