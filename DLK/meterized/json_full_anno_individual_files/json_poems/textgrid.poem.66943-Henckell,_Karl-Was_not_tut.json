{"textgrid.poem.66943": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Was not tut", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nur robust! Wie du dich stellst,", "tokens": ["Sei", "nur", "ro\u00b7bust", "!", "Wie", "du", "dich", "stellst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$.", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steh fest und lach der Spie\u00dfer,", "tokens": ["Steh", "fest", "und", "lach", "der", "Spie\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die dich zertreten, wenn du f\u00e4llst,", "tokens": ["Die", "dich", "zer\u00b7tre\u00b7ten", ",", "wenn", "du", "f\u00e4llst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die edlen Kannegie\u00dfer!", "tokens": ["Die", "ed\u00b7len", "Kan\u00b7ne\u00b7gie\u00b7\u00dfer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn hassen, hassen mu\u00df der Wicht,", "tokens": ["Denn", "has\u00b7sen", ",", "has\u00b7sen", "mu\u00df", "der", "Wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ein beschr\u00e4nkter Tropf ist,", "