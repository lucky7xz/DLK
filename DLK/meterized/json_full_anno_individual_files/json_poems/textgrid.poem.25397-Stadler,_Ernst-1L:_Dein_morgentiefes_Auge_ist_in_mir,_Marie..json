{"textgrid.poem.25397": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein morgentiefes Auge ist in mir, Marie.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein morgentiefes Auge ist in mir, Marie.", "tokens": ["Dein", "mor\u00b7gen\u00b7tie\u00b7fes", "Au\u00b7ge", "ist", "in", "mir", ",", "Ma\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich f\u00fchle, wie es durch die D\u00e4mmerung mich umf\u00e4ngt", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "wie", "es", "durch", "die", "D\u00e4m\u00b7me\u00b7rung", "mich", "um\u00b7f\u00e4ngt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der weiten Kirche. Stille will ich knien und warten, wie", "tokens": ["Der", "wei\u00b7ten", "Kir\u00b7che", ".", "Stil\u00b7le", "will", "ich", "kni\u00b7en", "und", "war\u00b7ten", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$,", "PWAV"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Dein Tag aus den erbl\u00fchten Heiligenfenstern zu mir dr\u00e4ngt.", "tokens": ["Dein", "Tag", "aus", "den", "er\u00b7bl\u00fch\u00b7ten", "Hei\u00b7li\u00b7gen\u00b7fens\u00b7tern", "zu", "mir", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie kommt er sanft und gut", "tokens": ["Wie", "kommt", "er", "sanft", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "und wie mit v\u00e4terlicher Hand", "tokens": ["und", "wie", "mit", "v\u00e4\u00b7ter\u00b7li\u00b7cher", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umschwichtigend. Wann wars,", "tokens": ["Um\u00b7schwich\u00b7ti\u00b7gend", ".", "Wann", "wars", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$.", "PWAV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "da\u00df er mit grellen Fratzen mich genarrt,", "tokens": ["da\u00df", "er", "mit", "grel\u00b7len", "Frat\u00b7zen", "mich", "ge\u00b7narrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf Vorstadtgassen,", "tokens": ["Auf", "Vor\u00b7stadt\u00b7gas\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "wenn mein Hunger nirgends sich ein Obdach fand \u2013", "tokens": ["wenn", "mein", "Hun\u00b7ger", "nir\u00b7gends", "sich", "ein", "Ob\u00b7dach", "fand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Oder in grauen Stuben mich", "tokens": ["O\u00b7der", "in", "grau\u00b7en", "Stu\u00b7ben", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "aus fremden Blicken angestarrt?", "tokens": ["aus", "frem\u00b7den", "Bli\u00b7cken", "an\u00b7ge\u00b7starrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun str\u00f6mt er warm wie Sommerregen \u00fcber mein Gesicht", "tokens": ["Nun", "str\u00f6mt", "er", "warm", "wie", "Som\u00b7mer\u00b7re\u00b7gen", "\u00fc\u00b7ber", "mein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und wie dein Atem voller Rosenduft, Marie,", "tokens": ["Und", "wie", "dein", "A\u00b7tem", "vol\u00b7ler", "Ro\u00b7sen\u00b7duft", ",", "Ma\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und meiner Seele dumpf verwirrt Get\u00f6n hebt sanft sein Licht", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le", "dumpf", "ver\u00b7wirrt", "Ge\u00b7t\u00f6n", "hebt", "sanft", "sein", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "ADJD", "NN", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "In deines Lebens morgenreine Melodie.", "tokens": ["In", "dei\u00b7nes", "Le\u00b7bens", "mor\u00b7g