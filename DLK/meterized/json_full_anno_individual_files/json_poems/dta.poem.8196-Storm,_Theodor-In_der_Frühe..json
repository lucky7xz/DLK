{"dta.poem.8196": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "In der Fr\u00fche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00e4hne kr\u00e4hn den Morgen wach;", "tokens": ["Die", "H\u00e4h\u00b7ne", "kr\u00e4hn", "den", "Mor\u00b7gen", "wach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun einer hier, nun einer dort,", "tokens": ["Nun", "ei\u00b7ner", "hier", ",", "nun", "ei\u00b7ner", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "$,", "ADV", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So kr\u00e4ht es nun von Ort zu Ort.", "tokens": ["So", "kr\u00e4ht", "es", "nun", "von", "Ort", "zu", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in der Ferne stirbt der Klang \u2014", "tokens": ["Und", "in", "der", "Fer\u00b7ne", "stirbt", "der", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich h\u00f6re nichts, ich horche lang'.", "tokens": ["Ich", "h\u00f6\u00b7re", "nichts", ",", "ich", "hor\u00b7che", "lang'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr wackern H\u00e4hne, kr\u00e4het doch!", "tokens": ["Ihr", "wa\u00b7ckern", "H\u00e4h\u00b7ne", ",", "kr\u00e4\u00b7het", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie schlafen immer, immer noch.", "tokens": ["Sie", "schla\u00b7fen", "im\u00b7mer", ",", "im\u00b7mer", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}