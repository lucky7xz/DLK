{"textgrid.poem.43197": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann's nicht schauen, dieses tr\u00e4ge Leben,", "tokens": ["Ich", "kann's", "nicht", "schau\u00b7en", ",", "die\u00b7ses", "tr\u00e4\u00b7ge", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir graut ob dieser m\u00fcssigen Gestalten,", "tokens": ["Mir", "graut", "ob", "die\u00b7ser", "m\u00fcs\u00b7si\u00b7gen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die l\u00e4ssig spielen mit des Mantels Falten", "tokens": ["Die", "l\u00e4s\u00b7sig", "spie\u00b7len", "mit", "des", "Man\u00b7tels", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und marionettenhaft die Glieder heben.", "tokens": ["Und", "ma\u00b7ri\u00b7o\u00b7net\u00b7ten\u00b7haft", "die", "Glie\u00b7der", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Oft zuckt es auf in ihres Blicks Umnachtung,", "tokens": ["Oft", "zuckt", "es", "auf", "in", "ih\u00b7res", "Blicks", "Um\u00b7nach\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es flackert dann ein sinnlich-weiches Lachen", "tokens": ["Es", "fla\u00b7ckert", "dann", "ein", "sinn\u00b7lich\u00b7wei\u00b7ches", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um ihren Mund, als wollten sie erwachen", "tokens": ["Um", "ih\u00b7ren", "Mund", ",", "als", "woll\u00b7ten", "sie", "er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "KOUS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus ihrer unbewu\u00dften Selbstverachtung.", "tokens": ["Aus", "ih\u00b7rer", "un\u00b7be\u00b7wu\u00df\u00b7ten", "Selbst\u00b7ver\u00b7ach\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir ist zu Muthe oft, als z\u00f6gen Leichen,", "tokens": ["Mir", "ist", "zu", "Mu\u00b7the", "oft", ",", "als", "z\u00f6\u00b7gen", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die k\u00fcnstlich nur in's Leben r\u00fcckgerufen,", "tokens": ["Die", "k\u00fcnst\u00b7lich", "nur", "in's", "Le\u00b7ben", "r\u00fcck\u00b7ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An mir vorbei, hinab die Marmorstufen,", "tokens": ["An", "mir", "vor\u00b7bei", ",", "hin\u00b7ab", "die", "Mar\u00b7mor\u00b7stu\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um wieder in die Gr\u00fcfte zu entweichen.", "tokens": ["Um", "wie\u00b7der", "in", "die", "Gr\u00fcf\u00b7te", "zu", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich kann's nicht schauen, dieses tr\u00e4ge Leben,", "tokens": ["Ich", "kann's", "nicht", "schau\u00b7en", ",", "die\u00b7ses", "tr\u00e4\u00b7ge", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir graut ob dieser m\u00fcssigen Gestalten,", "tokens": ["Mir", "graut", "ob", "die\u00b7ser", "m\u00fcs\u00b7si\u00b7gen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die l\u00e4ssig spielen mit des Mantels Falten", "tokens": ["Die", "l\u00e4s\u00b7sig", "spie\u00b7len", "mit", "des", "Man\u00b7tels", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und marionettenhaft die Glieder heben.", "tokens": ["Und", "ma\u00b7ri\u00b7o\u00b7net\u00