{"textgrid.poem.25502": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was versch\u00f6nern! was verstecken!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was versch\u00f6nern! was verstecken!", "tokens": ["Was", "ver\u00b7sch\u00f6\u00b7nern", "!", "was", "ver\u00b7ste\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVINF", "$.", "PWS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur gemalt so wie ich bin!", "tokens": ["Nur", "ge\u00b7malt", "so", "wie", "ich", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ADV", "KOKOM", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle kleine Sommerflecken,", "tokens": ["Al\u00b7le", "klei\u00b7ne", "Som\u00b7mer\u00b7fle\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie da sind, treulich hin!", "tokens": ["Wie", "sie", "da", "sind", ",", "treu\u00b7lich", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch was half mir diese Bitte?", "tokens": ["Doch", "was", "half", "mir", "die\u00b7se", "Bit\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine", "tokens": ["Sei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ob mein Herz darunter litte?", "tokens": ["Ob", "mein", "Herz", "da\u00b7run\u00b7ter", "lit\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ner malt' er Stirn' und Haar,", "tokens": ["Sch\u00f6\u00b7ner", "malt'", "er", "Stirn'", "und", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ner malt' er Kinn und Nase,", "tokens": ["Sch\u00f6\u00b7ner", "malt'", "er", "Kinn", "und", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aergert' ich mich noch so sehr:", "tokens": ["A\u00b7er\u00b7gert'", "ich", "mich", "noch", "so", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Frag' ihn, Liebster, ob er rase?", "tokens": ["Frag'", "ihn", ",", "Liebs\u00b7ter", ",", "ob", "er", "ra\u00b7se", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn das bin ich nimmermehr.", "tokens": ["Denn", "das", "bin", "ich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und was sollen diese L\u00fcgen?", "tokens": ["Und", "was", "sol\u00b7len", "die\u00b7se", "L\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bildet sich der Mann wohl ein", "tokens": ["Bil\u00b7det", "sich", "der", "Mann", "wohl", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich gef\u00e4llig zu betr\u00fcgen,", "tokens": ["Mich", "ge\u00b7f\u00e4l\u00b7lig", "zu", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ner, als ich bin, zu seyn?", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "ich", "bin", ",", "zu", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ob die Sch\u00f6nheit mich empfehle?", "tokens": ["Ob", "die", "Sch\u00f6n\u00b7heit", "mich", "emp\u00b7feh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist meine Sorge nicht,", "tokens": ["Das", "ist", "mei\u00b7ne", "Sor\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du liebest Nantens Seele,", "tokens": ["Denn", "du", "lie\u00b7best", "Nan\u00b7tens", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thoren, nichts als ihr Gesicht.", "tokens": ["Tho\u00b7ren", ",", "nichts", "als", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alles hie\u00df ich diesem Tropfe,", "tokens": ["Al\u00b7les", "hie\u00df", "ich", "die\u00b7sem", "Trop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesem St\u00fcmper, endlich gut,", "tokens": ["Die\u00b7sem", "St\u00fcm\u00b7per", ",", "end\u00b7lich", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber brennen nicht im Kopfe", "tokens": ["A\u00b7ber", "bren\u00b7nen", "nicht", "im", "Kop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Augen voller Glut?", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "vol\u00b7ler", "Glut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Natur nicht, blo\u00df dem Gl\u00fccke", "tokens": ["Der", "Na\u00b7tur", "nicht", ",", "blo\u00df", "dem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dank' ich die\u00df, mein Amarant,", "tokens": ["Dank'", "ich", "die\u00df", ",", "mein", "A\u00b7ma\u00b7rant", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der erste deiner Blicke", "tokens": ["Denn", "der", "ers\u00b7te", "dei\u00b7ner", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Setzte pl\u00f6tzlich sie in Brand.", "tokens": ["Setz\u00b7te", "pl\u00f6tz\u00b7lich", "sie", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O die Augen! o die Augen!", "tokens": ["O", "die", "Au\u00b7gen", "!", "o", "die", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "FM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schade was f\u00fcr das Gesicht;", "tokens": ["Scha\u00b7de", "was", "f\u00fcr", "das", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRELS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha! zum Maler mag er taugen,", "tokens": ["Ha", "!", "zum", "Ma\u00b7ler", "mag", "er", "tau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Geliebten taugt er nicht.", "tokens": ["Zum", "Ge\u00b7lieb\u00b7ten", "taugt", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Dieses Schmachten, dieses Sehnen,", "tokens": ["Die\u00b7ses", "Schmach\u00b7ten", ",", "die\u00b7ses", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dacht' ich so in meinem Sinn',", "tokens": ["Dacht'", "ich", "so", "in", "mei\u00b7nem", "Sinn'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese halb versteckten Thr\u00e4nen \u2013", "tokens": ["Die\u00b7se", "halb", "ver\u00b7steck\u00b7ten", "Thr\u00e4\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kurz, die Seele malt er hin.", "tokens": ["Kurz", ",", "die", "See\u00b7le", "malt", "er", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und wie will ich in dem Bilde", "tokens": ["Und", "wie", "will", "ich", "in", "dem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gern vor Amaranten stehn!", "tokens": ["Gern", "vor", "A\u00b7ma\u00b7ran\u00b7ten", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er zornig ist, so milde,", "tokens": ["Wenn", "er", "zor\u00b7nig", "ist", ",", "so", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er seufzt, so freundlich sehn!", "tokens": ["Wenn", "er", "seufzt", ",", "so", "freund\u00b7lich", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wenn er betet, wenn er dichtet,", "tokens": ["Wenn", "er", "be\u00b7tet", ",", "wenn", "er", "dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlaf' er oder sey er wach,", "tokens": ["Schlaf'", "er", "o\u00b7der", "sey", "er", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer nur auf ihn gerichtet", "tokens": ["Im\u00b7mer", "nur", "auf", "ihn", "ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folget ihm mein Auge nach.", "tokens": ["Fol\u00b7get", "ihm", "mein", "Au\u00b7ge", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Will er, b\u00f6ser Menschen m\u00fcde,", "tokens": ["Will", "er", ",", "b\u00f6\u00b7ser", "Men\u00b7schen", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In sein Kabinet entfliehn,", "tokens": ["In", "sein", "Ka\u00b7bi\u00b7net", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O wie soll ihm Trost und Friede", "tokens": ["O", "wie", "soll", "ihm", "Trost", "und", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Bild' entgegen ziehn!", "tokens": ["Von", "dem", "Bild'", "ent\u00b7ge\u00b7gen", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und nun sieh! \u2013 ich m\u00f6cht' ihn schlagen! \u2013", "tokens": ["Und", "nun", "sieh", "!", "\u2013", "ich", "m\u00f6cht'", "ihn", "schla\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$.", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh die Augen, lieber Mann!", "tokens": ["Sieh", "die", "Au\u00b7gen", ",", "lie\u00b7ber", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es nicht als wenn sie sagen:", "tokens": ["Ist", "es", "nicht", "als", "wenn", "sie", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "KOKOM", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbgeh doch, sieh uns nicht so an!\u00ab", "tokens": ["\u00bb", "geh", "doch", ",", "sieh", "uns", "nicht", "so", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "VVIMP", "PPER", "PTKNEG", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und nicht wahr, so saure Z\u00fcge", "tokens": ["Und", "nicht", "wahr", ",", "so", "sau\u00b7re", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADJD", "$,", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machen dir im Ansehn Qual?", "tokens": ["Ma\u00b7chen", "dir", "im", "An\u00b7sehn", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du frohere? So fliege", "tokens": ["Willst", "du", "fro\u00b7he\u00b7re", "?", "So", "flie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ADJA", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen zum Original'.", "tokens": ["Mor\u00b7gen", "zum", "O\u00b7ri\u00b7gi\u00b7nal'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.15": {"line.1": {"text": "Was versch\u00f6nern! was verstecken!", "tokens": ["Was", "ver\u00b7sch\u00f6\u00b7nern", "!", "was", "ver\u00b7ste\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVINF", "$.", "PWS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur gemalt so wie ich bin!", "tokens": ["Nur", "ge\u00b7malt", "so", "wie", "ich", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ADV", "KOKOM", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle kleine Sommerflecken,", "tokens": ["Al\u00b7le", "klei\u00b7ne", "Som\u00b7mer\u00b7fle\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie da sind, treulich hin!", "tokens": ["Wie", "sie", "da", "sind", ",", "treu\u00b7lich", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Doch was half mir diese Bitte?", "tokens": ["Doch", "was", "half", "mir", "die\u00b7se", "Bit\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine", "tokens": ["Sei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ob mein Herz darunter litte?", "tokens": ["Ob", "mein", "Herz", "da\u00b7run\u00b7ter", "lit\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ner malt' er Stirn' und Haar,", "tokens": ["Sch\u00f6\u00b7ner", "malt'", "er", "Stirn'", "und", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Sch\u00f6ner malt' er Kinn und Nase,", "tokens": ["Sch\u00f6\u00b7ner", "malt'", "er", "Kinn", "und", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aergert' ich mich noch so sehr:", "tokens": ["A\u00b7er\u00b7gert'", "ich", "mich", "noch", "so", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Frag' ihn, Liebster, ob er rase?", "tokens": ["Frag'", "ihn", ",", "Liebs\u00b7ter", ",", "ob", "er", "ra\u00b7se", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn das bin ich nimmermehr.", "tokens": ["Denn", "das", "bin", "ich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Und was sollen diese L\u00fcgen?", "tokens": ["Und", "was", "sol\u00b7len", "die\u00b7se", "L\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bildet sich der Mann wohl ein", "tokens": ["Bil\u00b7det", "sich", "der", "Mann", "wohl", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich gef\u00e4llig zu betr\u00fcgen,", "tokens": ["Mich", "ge\u00b7f\u00e4l\u00b7lig", "zu", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ner, als ich bin, zu seyn?", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "ich", "bin", ",", "zu", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Ob die Sch\u00f6nheit mich empfehle?", "tokens": ["Ob", "die", "Sch\u00f6n\u00b7heit", "mich", "emp\u00b7feh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist meine Sorge nicht,", "tokens": ["Das", "ist", "mei\u00b7ne", "Sor\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du liebest Nantens Seele,", "tokens": ["Denn", "du", "lie\u00b7best", "Nan\u00b7tens", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thoren, nichts als ihr Gesicht.", "tokens": ["Tho\u00b7ren", ",", "nichts", "als", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Alles hie\u00df ich diesem Tropfe,", "tokens": ["Al\u00b7les", "hie\u00df", "ich", "die\u00b7sem", "Trop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesem St\u00fcmper, endlich gut,", "tokens": ["Die\u00b7sem", "St\u00fcm\u00b7per", ",", "end\u00b7lich", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber brennen nicht im Kopfe", "tokens": ["A\u00b7ber", "bren\u00b7nen", "nicht", "im", "Kop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Augen voller Glut?", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "vol\u00b7ler", "Glut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Der Natur nicht, blo\u00df dem Gl\u00fccke", "tokens": ["Der", "Na\u00b7tur", "nicht", ",", "blo\u00df", "dem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dank' ich die\u00df, mein Amarant,", "tokens": ["Dank'", "ich", "die\u00df", ",", "mein", "A\u00b7ma\u00b7rant", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der erste deiner Blicke", "tokens": ["Denn", "der", "ers\u00b7te", "dei\u00b7ner", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Setzte pl\u00f6tzlich sie in Brand.", "tokens": ["Setz\u00b7te", "pl\u00f6tz\u00b7lich", "sie", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "O die Augen! o die Augen!", "tokens": ["O", "die", "Au\u00b7gen", "!", "o", "die", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "FM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schade was f\u00fcr das Gesicht;", "tokens": ["Scha\u00b7de", "was", "f\u00fcr", "das", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRELS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha! zum Maler mag er taugen,", "tokens": ["Ha", "!", "zum", "Ma\u00b7ler", "mag", "er", "tau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Geliebten taugt er nicht.", "tokens": ["Zum", "Ge\u00b7lieb\u00b7ten", "taugt", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Dieses Schmachten, dieses Sehnen,", "tokens": ["Die\u00b7ses", "Schmach\u00b7ten", ",", "die\u00b7ses", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dacht' ich so in meinem Sinn',", "tokens": ["Dacht'", "ich", "so", "in", "mei\u00b7nem", "Sinn'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese halb versteckten Thr\u00e4nen \u2013", "tokens": ["Die\u00b7se", "halb", "ver\u00b7steck\u00b7ten", "Thr\u00e4\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kurz, die Seele malt er hin.", "tokens": ["Kurz", ",", "die", "See\u00b7le", "malt", "er", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Und wie will ich in dem Bilde", "tokens": ["Und", "wie", "will", "ich", "in", "dem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gern vor Amaranten stehn!", "tokens": ["Gern", "vor", "A\u00b7ma\u00b7ran\u00b7ten", "stehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er zornig ist, so milde,", "tokens": ["Wenn", "er", "zor\u00b7nig", "ist", ",", "so", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er seufzt, so freundlich sehn!", "tokens": ["Wenn", "er", "seufzt", ",", "so", "freund\u00b7lich", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Wenn er betet, wenn er dichtet,", "tokens": ["Wenn", "er", "be\u00b7tet", ",", "wenn", "er", "dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlaf' er oder sey er wach,", "tokens": ["Schlaf'", "er", "o\u00b7der", "sey", "er", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer nur auf ihn gerichtet", "tokens": ["Im\u00b7mer", "nur", "auf", "ihn", "ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folget ihm mein Auge nach.", "tokens": ["Fol\u00b7get", "ihm", "mein", "Au\u00b7ge", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Will er, b\u00f6ser Menschen m\u00fcde,", "tokens": ["Will", "er", ",", "b\u00f6\u00b7ser", "Men\u00b7schen", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In sein Kabinet entfliehn,", "tokens": ["In", "sein", "Ka\u00b7bi\u00b7net", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O wie soll ihm Trost und Friede", "tokens": ["O", "wie", "soll", "ihm", "Trost", "und", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Bild' entgegen ziehn!", "tokens": ["Von", "dem", "Bild'", "ent\u00b7ge\u00b7gen", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Und nun sieh! \u2013 ich m\u00f6cht' ihn schlagen! \u2013", "tokens": ["Und", "nun", "sieh", "!", "\u2013", "ich", "m\u00f6cht'", "ihn", "schla\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$.", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh die Augen, lieber Mann!", "tokens": ["Sieh", "die", "Au\u00b7gen", ",", "lie\u00b7ber", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es nicht als wenn sie sagen:", "tokens": ["Ist", "es", "nicht", "als", "wenn", "sie", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "KOKOM", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbgeh doch, sieh uns nicht so an!\u00ab", "tokens": ["\u00bb", "geh", "doch", ",", "sieh", "uns", "nicht", "so", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "VVIMP", "PPER", "PTKNEG", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Und nicht wahr, so saure Z\u00fcge", "tokens": ["Und", "nicht", "wahr", ",", "so", "sau\u00b7re", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADJD", "$,", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machen dir im Ansehn Qual?", "tokens": ["Ma\u00b7chen", "dir", "im", "An\u00b7sehn", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du frohere? So fliege", "tokens": ["Willst", "du", "fro\u00b7he\u00b7re", "?", "So", "flie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ADJA", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen zum Original'.", "tokens": ["Mor\u00b7gen", "zum", "O\u00b7ri\u00b7gi\u00b7nal'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}