{"dta.poem.13577": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Wehmuth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O trau' den zwei Gesellen,", "tokens": ["O", "trau'", "den", "zwei", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Liebchen, nimmermehr,", "tokens": ["Mein", "Lieb\u00b7chen", ",", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die geh'n wie Wind und Wellen,", "tokens": ["Die", "geh'n", "wie", "Wind", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott wei\u00df: wohin, woher. \u2014", "tokens": ["Gott", "wei\u00df", ":", "wo\u00b7hin", ",", "wo\u00b7her", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$.", "PWAV", "$,", "PWAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}