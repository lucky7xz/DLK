{"textgrid.poem.66500": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du mein Gestirn, dem in der Fr\u00fche", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mein Gestirn, dem in der Fr\u00fche", "tokens": ["Du", "mein", "Ge\u00b7stirn", ",", "dem", "in", "der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein erster Gru\u00df begl\u00fcckt erwacht!", "tokens": ["Mein", "ers\u00b7ter", "Gru\u00df", "be\u00b7gl\u00fcckt", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du meines Tages Sorg' und M\u00fche,", "tokens": ["Du", "mei\u00b7nes", "Ta\u00b7ges", "Sor\u00b7g'", "und", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die leuchtend meines Ringens lacht!", "tokens": ["Die", "leuch\u00b7tend", "mei\u00b7nes", "Rin\u00b7gens", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Hoffnungspol im Abendreigen,", "tokens": ["Mein", "Hoff\u00b7nungs\u00b7pol", "im", "A\u00b7ben\u00b7drei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Leidenschaft beruhigt quillt!", "tokens": ["Wenn", "Lei\u00b7den\u00b7schaft", "be\u00b7ru\u00b7higt", "quillt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Traum, mein Sagen und mein Schweigen,", "tokens": ["Mein", "Traum", ",", "mein", "Sa\u00b7gen", "und", "mein", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du hei\u00dfgeliebtes Menschenbild!", "tokens": ["Du", "hei\u00df\u00b7ge\u00b7lieb\u00b7tes", "Men\u00b7schen\u00b7bild", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du magst, in deines Daseins Bl\u00fcthe", "tokens": ["Du", "magst", ",", "in", "dei\u00b7nes", "Da\u00b7seins", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich selig sonnend, nicht verstehn", "tokens": ["Dich", "se\u00b7lig", "son\u00b7nend", ",", "nicht", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJD", "VVPP", "$,", "PTKNEG", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Regungen, die durch's Gem\u00fcthe", "tokens": ["Die", "Re\u00b7gun\u00b7gen", ",", "die", "durch's", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Mir selbst unfa\u00dfbar, qu\u00e4lend gehn.", "tokens": ["Mir", "selbst", "un\u