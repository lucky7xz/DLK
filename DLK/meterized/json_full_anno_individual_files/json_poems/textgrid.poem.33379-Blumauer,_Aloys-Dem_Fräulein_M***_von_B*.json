{"textgrid.poem.33379": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Dem Fr\u00e4ulein M*** von B*", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Freundin, die mir j\u00fcngst durch einen k\u00f6stlichen", "tokens": ["Der", "Freun\u00b7din", ",", "die", "mir", "j\u00fcngst", "durch", "ei\u00b7nen", "k\u00f6st\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit besond'rer Kunst bereiteten", "tokens": ["Und", "mit", "be\u00b7son\u00b7d'\u00b7rer", "Kunst", "be\u00b7rei\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kapaun bewiesen, da\u00df die Ehre", "tokens": ["Ka\u00b7paun", "be\u00b7wie\u00b7sen", ",", "da\u00df", "die", "Eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem Thiere, das die H\u00fchnerwelt", "tokens": ["Von", "ei\u00b7nem", "Thie\u00b7re", ",", "das", "die", "H\u00fch\u00b7ner\u00b7welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr unschmackhaft und ungenie\u00dfbar h\u00e4lt,", "tokens": ["F\u00fcr", "un\u00b7schmack\u00b7haft", "und", "un\u00b7ge\u00b7nie\u00df\u00b7bar", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bei Menschen noch zu retten w\u00e4re,", "tokens": ["Bei", "Men\u00b7schen", "noch", "zu", "ret\u00b7ten", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn man ihm nur mit guter Art den Saft,", "tokens": ["Wenn", "man", "ihm", "nur", "mit", "gu\u00b7ter", "Art", "den", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den es verlor, durch Austern wieder schafft,", "tokens": ["Den", "es", "ver\u00b7lor", ",", "durch", "Aus\u00b7tern", "wie\u00b7der", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der schick' ich f\u00fcr das k\u00f6stliche Gerichte \u2013", "tokens": ["Der", "schick'", "ich", "f\u00fcr", "das", "k\u00f6st\u00b7li\u00b7che", "Ge\u00b7rich\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "F\u00fcr den Kapaun aus ihrer Hand \u2013", "tokens": ["F\u00fcr", "den", "Ka\u00b7paun", "aus", "ih\u00b7rer", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hier einen Hahn aus Griechenland,", "tokens": ["Hier", "ei\u00b7nen", "Hahn", "aus", "Grie\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der (wie zum mind'sten die Geschichte", "tokens": ["Der", "(", "wie", "zum", "min\u00b7d'\u00b7sten", "die", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "KOKOM", "APPRART", "ADJA", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Von ihm erz\u00e4hlt) so tapfer als galant", "tokens": ["Von", "ihm", "er\u00b7z\u00e4hlt", ")", "so", "tap\u00b7fer", "als", "ga\u00b7lant"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$(", "ADV", "ADJD", "KOKOM", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In ganz Athen nicht seines gleichen fand,", "tokens": ["In", "ganz", "A\u00b7then", "nicht", "sei\u00b7nes", "glei\u00b7chen", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NE", "PTKNEG", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Der stets den sch\u00f6nsten Kamm getragen,", "tokens": ["Der", "stets", "den", "sch\u00f6ns\u00b7ten", "Kamm", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit jedem Hahn zur Wette sich geschlagen,", "tokens": ["Mit", "je\u00b7dem", "Hahn", "zur", "Wet\u00b7te", "sich", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und darum bei Athens gesammter H\u00fchnerschaar", "tokens": ["Und", "da\u00b7rum", "bei", "A\u00b7thens", "ge\u00b7samm\u00b7ter", "H\u00fch\u00b7ner\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Beliebt, und \u00fcberall \u2013 der Hahn im Korbe war,", "tokens": ["Be\u00b7liebt", ",", "und", "\u00fc\u00b7be\u00b7rall", "\u2013", "der", "Hahn", "im", "Kor\u00b7be", "war", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADV", "$(", "ART", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kurz, den die eckelste der griechischen Damen,", "tokens": ["Kurz", ",", "den", "die", "ec\u00b7kels\u00b7te", "der", "grie\u00b7chi\u00b7schen", "Da\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "ART", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Die sich um ihn oft in die Haare kamen,", "tokens": ["Die", "sich", "um", "ihn", "oft", "in", "die", "Haa\u00b7re", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "In seiner Art gewi\u00df so schmackhaft fand,", "tokens": ["In", "sei\u00b7ner", "Art", "ge\u00b7wi\u00df", "so", "schmack\u00b7haft", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Als ich j\u00fcngst den Kapaun aus meiner Freundin Hand;", "tokens": ["Als", "ich", "j\u00fcngst", "den", "Ka\u00b7paun", "aus", "mei\u00b7ner", "Freun\u00b7din", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Den aber, weil er seit so manchem Jahr", "tokens": ["Den", "a\u00b7ber", ",", "weil", "er", "seit", "so", "man\u00b7chem", "Jahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "KOUS", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ein bischen alt und z\u00e4h' geworden war,", "tokens": ["Ein", "bi\u00b7schen", "alt", "und", "z\u00e4h'", "ge\u00b7wor\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ein deutscher Koch, von dem haut ", "tokens": ["Ein", "deut\u00b7scher", "Koch", ",", "von", "dem", "haut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "$,", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.26": {"text": "F\u00fcr unseren Geschmack von Neuem zubereitet;", "tokens": ["F\u00fcr", "un\u00b7se\u00b7ren", "Ge\u00b7schmack", "von", "Neu\u00b7em", "zu\u00b7be\u00b7rei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den schick' ich hier, und gebe dann", "tokens": ["Den", "schick'", "ich", "hier", ",", "und", "ge\u00b7be", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "F\u00fcr einen deutschen, gallisch appretirten Hahn", "tokens": ["F\u00fcr", "ei\u00b7nen", "deut\u00b7schen", ",", "gal\u00b7lisch", "ap\u00b7pre\u00b7tir\u00b7ten", "Hahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "(zum Dank f\u00fcr die gehabte M\u00fche)", "tokens": ["(", "zum", "Dank", "f\u00fcr", "die", "ge\u00b7hab\u00b7te", "M\u00fc\u00b7he", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ihr einen griech'schen Hahn in einer deutschen Br\u00fche.", "tokens": ["Ihr", "ei\u00b7nen", "griech'\u00b7schen", "Hahn", "in", "ei\u00b7ner", "deut\u00b7schen", "Br\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Freundin, die mir j\u00fcngst durch einen k\u00f6stlichen", "tokens": ["Der", "Freun\u00b7din", ",", "die", "mir", "j\u00fcngst", "durch", "ei\u00b7nen", "k\u00f6st\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit besond'rer Kunst bereiteten", "tokens": ["Und", "mit", "be\