{"dta.poem.9717": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Holl\u00e4ndischen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Veit nimmt eine hur zum weibe/ nicht um sch\u00f6nheit/ stand", "tokens": ["Veit", "nimmt", "ei\u00b7ne", "hur", "zum", "wei\u00b7be", "/", "nicht", "um", "sch\u00f6n\u00b7heit", "/", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "VVFIN", "$(", "PTKNEG", "APPR", "NN", "$(", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "und gut/", "tokens": ["und", "gut", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Nein! nur blo\u00df aus geilen l\u00fcsten/ weil sie seinen willen thut/", "tokens": ["Nein", "!", "nur", "blo\u00df", "aus", "gei\u00b7len", "l\u00fcs\u00b7ten", "/", "weil", "sie", "sei\u00b7nen", "wil\u00b7len", "thut", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "ADV", "APPR", "ADJA", "NN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Jtzt d\u00fcnckt er sich viel zu seyn/ und vor andern wol gepostet/", "tokens": ["Jtzt", "d\u00fcnckt", "er", "sich", "viel", "zu", "seyn", "/", "und", "vor", "an\u00b7dern", "wol", "ge\u00b7pos\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKZU", "VAINF", "$(", "KON