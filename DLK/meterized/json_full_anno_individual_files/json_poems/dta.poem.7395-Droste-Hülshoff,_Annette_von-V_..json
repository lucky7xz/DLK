{"dta.poem.7395": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "V .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Tief tiefe Nacht, am Schreine nur der Maus geheimes", "tokens": ["Tief", "tie\u00b7fe", "Nacht", ",", "am", "Schrei\u00b7ne", "nur", "der", "Maus", "ge\u00b7hei\u00b7mes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "APPRART", "NN", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nagen r\u00fcttelt,", "tokens": ["Na\u00b7gen", "r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der Horizont ein rinnend Sieb, aus dem sich Kohlenstaub", "tokens": ["Der", "Ho\u00b7ri\u00b7zont", "ein", "rin\u00b7nend", "Sieb", ",", "aus", "dem", "sich", "Koh\u00b7len\u00b7staub"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "$,", "APPR", "PRELS", "PRF", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "entsch\u00fcttelt,", "tokens": ["ent\u00b7sch\u00fct\u00b7telt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die Tr\u00e4ume ziehen, schwer wie Blei und leicht wie Dunst,", "tokens": ["Die", "Tr\u00e4u\u00b7me", "zie\u00b7hen", ",", "schwer", "wie", "Blei", "und", "leicht", "wie", "Dunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "KOKOM", "NN", "KON", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "um Flaum und Streue,", "tokens": ["um", "Flaum", "und", "Streu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "In Gold der hagere Poet, der d\u00fcrre Klepper w\u00fchlt im", "tokens": ["In", "Gold", "der", "ha\u00b7ge\u00b7re", "Po\u00b7et", ",", "der", "d\u00fcr\u00b7re", "Klep\u00b7per", "w\u00fchlt", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "APPRART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Heue,", "tokens": ["Heu\u00b7e", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Vom Kranze tr\u00e4umt die Braut, vom Helm", "tokens": ["Vom", "Kran\u00b7ze", "tr\u00e4umt", "die", "Braut", ",", "vom", "Helm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Krieger, und vom Strick der Schelm.", "tokens": ["Der", "Krie\u00b7ger", ",", "und", "vom", "Strick", "der", "Schelm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In jener Kammer, wo sich matt der Fenster tiefes Grau", "tokens": ["In", "je\u00b7ner", "Kam\u00b7mer", ",", "wo", "sich", "matt", "der", "Fens\u00b7ter", "tie\u00b7fes", "Grau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PRF", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "schattiret,", "tokens": ["schat\u00b7ti\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "H\u00f6rst du ein Rieseln, wie die Luft der Steppe zarten Staub", "tokens": ["H\u00f6rst", "du", "ein", "Rie\u00b7seln", ",", "wie", "die", "Luft", "der", "Step\u00b7pe", "zar\u00b7ten", "Staub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "entf\u00fchret?", "tokens": ["ent\u00b7f\u00fch\u00b7ret", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und ein Ges\u00e4usel, wie im Glas gefangner Bremse Fl\u00fcgel", "tokens": ["Und", "ein", "Ge\u00b7s\u00e4u\u00b7sel", ",", "wie", "im", "Glas", "ge\u00b7fang\u00b7ner", "Brem\u00b7se", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "APPRART", "NN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "wispelt?", "tokens": ["wis\u00b7pelt", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Vielleicht 'ne Sanduhr die verrinnt? ein M\u00e4uschen das im", "tokens": ["Viel\u00b7leicht", "'ne", "San\u00b7duhr", "die", "ver\u00b7rinnt", "?", "ein", "M\u00e4u\u00b7schen", "das", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "VVFIN", "$.", "ART", "NN", "ART", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kalke rispelt?", "tokens": ["Kal\u00b7ke", "ris\u00b7pelt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "So scharf es geht, so bohrend ein", "tokens": ["So", "scharf", "es", "geht", ",", "so", "boh\u00b7rend", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie Sensenwetzen am Gestein.", "tokens": ["Wie", "Sen\u00b7sen\u00b7wet\u00b7zen", "am", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dort am Hange \u2014 Phosphorlicht, wie's kranken Gliedern", "tokens": ["Und", "dort", "am", "Han\u00b7ge", "Phos\u00b7phor\u00b7licht", ",", "wie's", "kran\u00b7ken", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$(", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sich entwickelt?", "tokens": ["sich", "ent\u00b7wi\u00b7ckelt", "?"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ein gr\u00fcnlich Leuchten, das wie Flaum mit hundert F\u00e4den", "tokens": ["Ein", "gr\u00fcn\u00b7lich", "Leuch\u00b7ten", ",", "das", "wie", "Flaum", "mit", "hun\u00b7dert", "F\u00e4\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "KOKOM", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wirrt und prickelt,", "tokens": ["wirrt", "und", "pri\u00b7ckelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Gestaltlos, nur ein gl\u00fcher Punkt in Mitten wo die Fasern", "tokens": ["Ge\u00b7stalt\u00b7los", ",", "nur", "ein", "gl\u00fc\u00b7her", "Punkt", "in", "Mit\u00b7ten", "wo", "die", "Fa\u00b7sern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "quellen,", "tokens": ["quel\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Mit klingelndem Ges\u00e4usel sich an der Phiole W\u00e4nde schnellen,", "tokens": ["Mit", "klin\u00b7geln\u00b7dem", "Ge\u00b7s\u00e4u\u00b7sel", "sich", "an", "der", "Phi\u00b7o\u00b7le", "W\u00e4n\u00b7de", "schnel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Und dr\u00fcber, wo der Schein zerfleu\u00dft,", "tokens": ["Und", "dr\u00fc\u00b7ber", ",", "wo", "der", "Schein", "zer\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein dunkler Augenspiegel glei\u00dft.", "tokens": ["Ein", "dunk\u00b7ler", "Au\u00b7gen\u00b7spie\u00b7gel", "glei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und immer krimmelts, wimmelts fort, die gr\u00fcne Wand des", "tokens": ["Und", "im\u00b7mer", "krim\u00b7melts", ",", "wim\u00b7melts", "fort", ",", "die", "gr\u00fc\u00b7ne", "Wand", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Glases streifend,", "tokens": ["Gla\u00b7ses", "strei\u00b7fend", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ein gl\u00fcher gieriger Polyp, vergebens nach der Beute greifend,", "tokens": ["Ein", "gl\u00fc\u00b7her", "gie\u00b7ri\u00b7ger", "Po\u00b7lyp", ",", "ver\u00b7ge\u00b7bens", "nach", "der", "Beu\u00b7te", "grei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und immer starrt das Auge her, als ob kein Augenlied es", "tokens": ["Und", "im\u00b7mer", "starrt", "das", "Au\u00b7ge", "her", ",", "als", "ob", "kein", "Au\u00b7gen\u00b7lied", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOKOM", "KOUS", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "schatte,", "tokens": ["schat\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Ein dunkles Haar, ein Nacken hebt sich langsam an des", "tokens": ["Ein", "dunk\u00b7les", "Haar", ",", "ein", "Na\u00b7cken", "hebt", "sich", "lang\u00b7sam", "an", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Tisches Platte,", "tokens": ["Ti\u00b7sches", "Plat\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Dann pl\u00f6tzlich schlie\u00dft sich eine Hand", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", "schlie\u00dft", "sich", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und im Moment der Schein verschwand.", "tokens": ["Und", "im", "Mo\u00b7ment", "der", "Schein", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es tappt die Diel' entlang, es stampft wie M\u00e4nnertritt auf", "tokens": ["Es", "tappt", "die", "Diel'", "ent\u00b7lang", ",", "es", "stampft", "wie", "M\u00e4n\u00b7ner\u00b7tritt", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "KOKOM", "NN", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "weichen Sohlen,", "tokens": ["wei\u00b7chen", "Soh\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Behutsam tastend an der Wand will Jemand Rathes sich", "tokens": ["Be\u00b7hut\u00b7sam", "tas\u00b7tend", "an", "der", "Wand", "will", "Je\u00b7mand", "Ra\u00b7thes", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "VMFIN", "PIS", "NN", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "erholen,", "tokens": ["er\u00b7ho\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Dann leise klinkt der Th\u00fcre Schlo\u00df, die losgezognen Riegel", "tokens": ["Dann", "lei\u00b7se", "klinkt", "der", "Th\u00fc\u00b7re", "Schlo\u00df", ",", "die", "los\u00b7ge\u00b7zog\u00b7nen", "Rie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "pfeifen,", "tokens": ["pfei\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Durch das Gemach, verzitternd, scheu, gie\u00dft sich ein matter", "tokens": ["Durch", "das", "Ge\u00b7mach", ",", "ver\u00b7zit\u00b7ternd", ",", "scheu", ",", "gie\u00dft", "sich", "ein", "mat\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "$,", "ADJD", "$,", "VVFIN", "PRF", "ART", "ADJA"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "D\u00e4mmerstreifen,", "tokens": ["D\u00e4m\u00b7mer\u00b7strei\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und in dem Rahmen, duftumweht", "tokens": ["Und", "in", "dem", "Rah\u00b7men", ",", "duf\u00b7tum\u00b7weht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im Nachtgewand der T\u00e4uscher steht.", "tokens": ["Im", "Nacht\u00b7ge\u00b7wand", "der", "T\u00e4u\u00b7scher", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie ist die st\u00e4mmige Gestalt zum sehnenharten Knorren", "tokens": ["Wie", "ist", "die", "st\u00e4m\u00b7mi\u00b7ge", "Ge\u00b7stalt", "zum", "seh\u00b7nen\u00b7har\u00b7ten", "Knor\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "worden!", "tokens": ["wor\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["VAPP", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wie manches, manches graue Haar schattirt sich an der", "tokens": ["Wie", "man\u00b7ches", ",", "man\u00b7ches", "grau\u00b7e", "Haar", "schat\u00b7tirt", "sich", "an", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PIAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schl\u00e4fe Borden!", "tokens": ["Schl\u00e4\u00b7fe", "Bor\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "O, diese Falten um den Mund, wo leise Kummerz\u00fcge", "tokens": ["O", ",", "die\u00b7se", "Fal\u00b7ten", "um", "den", "Mund", ",", "wo", "lei\u00b7se", "Kum\u00b7mer\u00b7z\u00fc\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word