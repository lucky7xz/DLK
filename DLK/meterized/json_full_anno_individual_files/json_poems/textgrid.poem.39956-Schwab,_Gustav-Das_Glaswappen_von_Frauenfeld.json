{"textgrid.poem.39956": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Glaswappen von Frauenfeld", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht in hellrem Glanze", "tokens": ["Es", "steht", "in", "hell\u00b7rem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Wappen in der Welt", "tokens": ["Kein", "Wap\u00b7pen", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als das von Frauenfeld.", "tokens": ["Als", "das", "von", "Frau\u00b7en\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf buntem Glas, im lichten Bilderkranze", "tokens": ["Auf", "bun\u00b7tem", "Glas", ",", "im", "lich\u00b7ten", "Bil\u00b7der\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Umschlingt es eine bl\u00fchende Romanze.", "tokens": ["Um\u00b7schlingt", "es", "ei\u00b7ne", "bl\u00fc\u00b7hen\u00b7de", "Ro\u00b7man\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schaut! dort zu oberst reitet \u2013", "tokens": ["Schaut", "!", "dort", "zu", "o\u00b7berst", "rei\u00b7tet", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Hengste lauter Glut,", "tokens": ["Die", "Hengs\u00b7te", "lau\u00b7ter", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das edelste Vollblut \u2013", "tokens": ["Das", "e\u00b7dels\u00b7te", "Voll\u00b7blut", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Graf, vom sch\u00f6nsten T\u00f6chterlein begleitet,", "tokens": ["Der", "Graf", ",", "vom", "sch\u00f6ns\u00b7ten", "T\u00f6ch\u00b7ter\u00b7lein", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur Jagd, und Diener gnug, und Alles schreitet.", "tokens": ["Zur", "Jagd", ",", "und", "Die\u00b7ner", "gnug", ",", "und", "Al\u00b7les", "schrei\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "NN", "ADV", "$,", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun links \u2013 die Jagd begonnen;", "tokens": ["Nun", "links", "\u2013", "die", "Jagd", "be\u00b7gon\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein gr\u00fcn und sonnig Bild:", "tokens": ["Ein", "gr\u00fcn", "und", "son\u00b7nig", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Walde Hund und Wild;", "tokens": ["Im", "Wal\u00b7de", "Hund", "und", "Wild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch tief im Busch und aus dem Licht der Sonnen", "tokens": ["Doch", "tief", "im", "Busch", "und", "aus", "dem", "Licht", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Ritter bei der Maid in Liebeswonnen.", "tokens": ["Ein", "Rit\u00b7ter", "bei", "der", "Maid", "in", "Lie\u00b7bes\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jetzt \u2013 vor dem Grafen stehet", "tokens": ["Jetzt", "\u2013", "vor", "dem", "Gra\u00b7fen", "ste\u00b7het"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Buhle, herzenswund;", "tokens": ["Der", "Buh\u00b7le", ",", "her\u00b7zens\u00b7wund", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Alles bekennt sein Mund.", "tokens": ["Al\u00b7les", "be\u00b7kennt", "sein", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Graf, vom gelben M\u00e4hnenhaar umwehet,", "tokens": ["Der", "Graf", ",", "vom", "gel\u00b7ben", "M\u00e4h\u00b7nen\u00b7haar", "um\u00b7we\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kehrt ab das Haupt und h\u00f6rt nicht was er flehet.", "tokens": ["Kehrt", "ab", "das", "Haupt", "und", "h\u00f6rt", "nicht", "was", "er", "fle\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "KON", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dann \u2013 vor der Klosterpforte", "tokens": ["Dann", "\u2013", "vor", "der", "Klos\u00b7ter\u00b7pfor\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am moosigen Geb\u00e4u", "tokens": ["Am", "moo\u00b7si\u00b7gen", "Ge\u00b7b\u00e4u"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein M\u00e4gdlein, nicht in Reu',", "tokens": ["Ein", "M\u00e4gd\u00b7lein", ",", "nicht", "in", "Reu'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Fu\u00dff\u00e4llig vor dem Abt, dem einz'gen Horte,", "tokens": ["Fu\u00df\u00b7f\u00e4l\u00b7lig", "vor", "dem", "Abt", ",", "dem", "einz'\u00b7gen", "Hor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf ihrer Lippe schweben Flehensworte.", "tokens": ["Auf", "ih\u00b7rer", "Lip\u00b7pe", "schwe\u00b7ben", "Fle\u00b7hens\u00b7wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Rechts aber \u2013 vor dem Grafen", "tokens": ["Rechts", "a\u00b7ber", "\u2013", "vor", "dem", "Gra\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Abt, im h\u00e4rnen Kleid;", "tokens": ["Der", "Abt", ",", "im", "h\u00e4r\u00b7nen", "Kleid", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er klagt der Tochter Leid;", "tokens": ["Er", "klagt", "der", "Toch\u00b7ter", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sein Blick beschw\u00f6ret, nicht zu hart zu strafen.", "tokens": ["Sein", "Blick", "be\u00b7schw\u00f6\u00b7ret", ",", "nicht", "zu", "hart", "zu", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PTKNEG", "PTKA", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Vaterauge geht der Zorn schon schlafen.", "tokens": ["Im", "Va\u00b7te\u00b7rau\u00b7ge", "geht", "der", "Zorn", "schon", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und nun \u2013 der Graf in Handen", "tokens": ["Und", "nun", "\u2013", "der", "Graf", "in", "Han\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4lt dort ein Pergament,", "tokens": ["H\u00e4lt", "dort", "ein", "Per\u00b7ga\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die farb'ge Letter brennt.", "tokens": ["Die", "fa\u00b7rb'\u00b7ge", "Let\u00b7ter", "brennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das T\u00f6chterlein, schamrot, hat eingestanden,", "tokens": ["Das", "T\u00f6ch\u00b7ter\u00b7lein", ",", "scham\u00b7rot", ",", "hat", "ein\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Lehn empf\u00e4ngt es an der Statt von Banden.", "tokens": ["Ein", "Lehn", "emp\u00b7f\u00e4ngt", "es", "an", "der", "Statt", "von", "Ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Zuletzt \u2013 die Burg erbauet;", "tokens": ["Zu\u00b7letzt", "\u2013", "die", "Burg", "er\u00b7bau\u00b7et", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit frischem Ziegelstein", "tokens": ["Mit", "fri\u00b7schem", "Zie\u00b7gel\u00b7stein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gedeckt, wie l\u00e4dt sie ein!", "tokens": ["Ge\u00b7deckt", ",", "wie", "l\u00e4dt", "sie", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und durch das Thor ein l\u00e4chelnd Paar, getrauet,", "tokens": ["Und", "durch", "das", "Thor", "ein", "l\u00e4\u00b7chelnd", "Paar", ",", "ge\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zieht ein der Ritter mit der Braut, o schauet!", "tokens": ["Zieht", "ein", "der", "Rit\u00b7ter", "mit", "der", "Braut", ",", "o", "schau\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "APPR", "ART", "NN", "$,", "FM", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mitten \u2013 lichtdurchdrungen", "tokens": ["Und", "mit\u00b7ten", "\u2013", "licht\u00b7durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADV", "$(", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In purpurnem Gewand", "tokens": ["In", "pur\u00b7pur\u00b7nem", "Ge\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Ein Frauenbild, zur Hand", "tokens": ["Ein", "Frau\u00b7en\u00b7bild", ",", "zur", "Hand"], "token_info": [