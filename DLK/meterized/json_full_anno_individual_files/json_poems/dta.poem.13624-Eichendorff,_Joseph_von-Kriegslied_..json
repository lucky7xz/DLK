{"dta.poem.13624": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Greiffen nicht mehr fliegen,", "tokens": ["Die", "Greif\u00b7fen", "nicht", "mehr", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lindw\u00fcrm' auf hei\u00dfem Sand", "tokens": ["Lind\u00b7w\u00fcrm'", "auf", "hei\u00b7\u00dfem", "Sand"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nicht mehr mit L\u00f6wen kriegen,", "tokens": ["Nicht", "mehr", "mit", "L\u00f6\u00b7wen", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ihren B\u00e4uchen liegen", "tokens": ["Auf", "ih\u00b7ren", "B\u00e4u\u00b7chen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Drachen im platten Land.", "tokens": ["Die", "Dra\u00b7chen", "im", "plat\u00b7ten", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}