{"textgrid.poem.47725": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[tief in Nacht und Dunkel]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief in Nacht und Dunkel", "tokens": ["Tief", "in", "Nacht", "und", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lag ein Hort verborgen; \u2013", "tokens": ["Lag", "ein", "Hort", "ver\u00b7bor\u00b7gen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ferne meinem Ahnen,", "tokens": ["Fer\u00b7ne", "mei\u00b7nem", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Meinem Auge weit,", "tokens": ["Mei\u00b7nem", "Au\u00b7ge", "weit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Lebte tief im Walde,", "tokens": ["Leb\u00b7te", "tief", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dienet' in der Wildni\u00df", "tokens": ["Die\u00b7net'", "in", "der", "Wild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Eine zarte Maid.", "tokens": ["Ei\u00b7ne", "zar\u00b7te", "Maid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ausgejagt von Unmuth \u2013", "tokens": ["Aus\u00b7ge\u00b7jagt", "von", "Un\u00b7muth", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Denn Verlust erlitt ich", "tokens": ["Denn", "Ver\u00b7lust", "er\u00b7litt", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bitterlich und schwer \u2013", "tokens": ["Bit\u00b7ter\u00b7lich", "und", "schwer", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Durch die Lande ritt ich.", "tokens": ["Durch", "die", "Lan\u00b7de", "ritt", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mied bekannte Wege,", "tokens": ["Mied", "be\u00b7kann\u00b7te", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Suchte wilde Stege,", "tokens": ["Such\u00b7te", "wil\u00b7de", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Schweifte weit umher.", "tokens": ["Schweif\u00b7te", "weit", "um\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Das behende, gute", "tokens": ["Das", "be\u00b7hen\u00b7de", ",", "gu\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "R\u00f6\u00dfchen und die eigne,", "tokens": ["R\u00f6\u00df\u00b7chen", "und", "die", "eig\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Junge Kraft erm\u00fcdend,", "tokens": ["Jun\u00b7ge", "Kraft", "er\u00b7m\u00fc\u00b7dend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Linderung dem Herzen", "tokens": ["Lin\u00b7de\u00b7rung", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Zu erreiten, hofft' ich,", "tokens": ["Zu", "er\u00b7rei\u00b7ten", ",", "hofft'", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Aber meine Schmerzen", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Brannten nur noch mehr.", "tokens": ["Brann\u00b7ten", "nur", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Allgemach anitzo", "tokens": ["All\u00b7ge\u00b7mach", "a\u00b7nit\u00b7zo"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Seine graue Schwinge", "tokens": ["Sei\u00b7ne", "grau\u00b7e", "Schwin\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Breitete der Abend", "tokens": ["Brei\u00b7te\u00b7te", "der", "A\u00b7bend"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00dcber das Revier;", "tokens": ["\u00dc\u00b7ber", "das", "Re\u00b7vier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Meine Blicke suchten", "tokens": ["Mei\u00b7ne", "Bli\u00b7cke", "such\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lang nach einem Schirme", "tokens": ["Lang", "nach", "ei\u00b7nem", "Schir\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Vor der Nacht Bedrohni\u00df,", "tokens": ["Vor", "der", "Nacht", "Be\u00b7droh\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Endlich in das Auge", "tokens": ["End\u00b7lich", "in", "das", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Fiel mir ein Quartier.", "tokens": ["Fiel", "mir", "ein", "Quar\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "An die Krippe hier", "tokens": ["An", "die", "Krip\u00b7pe", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Stellet' ich den Renner,", "tokens": ["Stel\u00b7let'", "ich", "den", "Ren\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Trat in eine Stube,", "tokens": ["Trat", "in", "ei\u00b7ne", "Stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Eine d\u00e4mmerhelle,", "tokens": ["Ei\u00b7ne", "d\u00e4m\u00b7mer\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Barg in einen Winkel", "tokens": ["Barg", "in", "ei\u00b7nen", "Win\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Hinter einen Tisch mich,", "tokens": ["Hin\u00b7ter", "ei\u00b7nen", "Tisch", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Und begann zu feufzen,", "tokens": ["Und", "be\u00b7gann", "zu", "feuf\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Und begann zu weinen;", "tokens": ["Und", "be\u00b7gann", "zu", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Hei\u00df vom Auge tr\u00e4ufte", "tokens": ["Hei\u00df", "vom", "Au\u00b7ge", "tr\u00e4uf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Z\u00e4hr' auf Z\u00e4hre mir.", "tokens": ["Z\u00e4hr'", "auf", "Z\u00e4h\u00b7re", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hin zu mir, dem D\u00fcstern,", "tokens": ["Hin", "zu", "mir", ",", "dem", "D\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der sie nicht bemerkte,", "tokens": ["Der", "sie", "nicht", "be\u00b7merk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sp\u00e4hte von der Seite,", "tokens": ["Sp\u00e4h\u00b7te", "von", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Blickte scharf und sinnend", "tokens": ["Blick\u00b7te", "scharf", "und", "sin\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jenes sch\u00f6ne Kind;", "tokens": ["Je\u00b7nes", "sch\u00f6\u00b7ne", "Kind", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Brachte mir ein Glas nun", "tokens": ["Brach\u00b7te", "mir", "ein", "Glas", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ein von Schaume wei\u00dfes,", "tokens": ["Ein", "von", "Schau\u00b7me", "wei\u00b7\u00dfes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und ich sah in's Aug' ihr,", "tokens": ["Und", "ich", "sah", "in's", "Aug'", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Und es traf ihr Blick mich", "tokens": ["Und", "es", "traf", "ihr", "Blick", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Wie ein Strahl des Himmels", "tokens": ["Wie", "ein", "Strahl", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Tr\u00f6stlich und gelind.", "tokens": ["Tr\u00f6st\u00b7lich", "und", "ge\u00b7lind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Alsofort ein Dringen", "tokens": ["Al\u00b7so\u00b7fort", "ein", "Drin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Im Gem\u00fcthe sp\u00fcrt' ich,", "tokens": ["Im", "Ge\u00b7m\u00fc\u00b7the", "sp\u00fcrt'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ich ihr die Spende,", "tokens": ["Da\u00df", "ich", "ihr", "die", "Spen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die sie reichte, zutrank", "tokens": ["Die", "sie", "reich\u00b7te", ",", "zu\u00b7trank"], "t