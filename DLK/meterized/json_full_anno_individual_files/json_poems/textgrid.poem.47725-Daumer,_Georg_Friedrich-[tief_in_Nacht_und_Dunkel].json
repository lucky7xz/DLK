okens": ["Mei\u00b7ner", "See\u00b7le", "Tr\u00fcb\u00b7sal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Aufgefunden war mir,", "tokens": ["Auf\u00b7ge\u00b7fun\u00b7den", "war", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Was das Herz bedurfte,", "tokens": ["Was", "das", "Herz", "be\u00b7durf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Aufgegangen war mir", "tokens": ["Auf\u00b7ge\u00b7gan\u00b7gen", "war", "mir"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Tief in Nacht und Dunkel", "tokens": ["Tief", "in", "Nacht", "und", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Der ersehnte Tag.", "tokens": ["Der", "er\u00b7sehn\u00b7te", "Tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}