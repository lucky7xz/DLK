{"textgrid.poem.33070": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Allein steht eine Buche", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allein steht eine Buche", "tokens": ["Al\u00b7lein", "steht", "ei\u00b7ne", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Entfernt vom Waldesplan,", "tokens": ["Ent\u00b7fernt", "vom", "Wal\u00b7des\u00b7plan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Str\u00e4uchern nur umgeben", "tokens": ["Von", "Str\u00e4u\u00b7chern", "nur", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu klein, sie zu erstreben,", "tokens": ["Zu", "klein", ",", "sie", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "An die sie sich nicht lehnen kann.", "tokens": ["An", "die", "sie", "sich", "nicht", "leh\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wie sie so alleine", "tokens": ["Doch", "wie", "sie", "so", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort wurzelt stolz und stark,", "tokens": ["Dort", "wur\u00b7zelt", "stolz", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verk\u00fcnden Wuchs und Krone,", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "Wuchs", "und", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df tief im Innern wohne", "tokens": ["Da\u00df", "tief", "im", "In\u00b7nern", "woh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein kr\u00e4ftiges und edles Mark.", "tokens": ["Ein", "kr\u00e4f\u00b7ti\u00b7ges", "und", "ed\u00b7les", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es mag der Sturm umpeitschen", "tokens": ["Es", "mag", "der", "Sturm", "um\u00b7peit\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wild umtoben sie;", "tokens": ["Und", "wild", "um\u00b7to\u00b7ben", "sie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er mag die Zweige knicken,", "tokens": ["Er", "mag", "die", "Zwei\u00b7ge", "kni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Blitze sie umz\u00fccken,", "tokens": ["Die", "Blit\u00b7ze", "sie", "um\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Wipfel beugt sie feige nie!", "tokens": ["Den", "Wip\u00b7fel", "beugt", "sie", "fei\u00b7ge", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch unter diesem Wipfel", "tokens": ["Doch", "un\u00b7ter", "die\u00b7sem", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4ngt sicher manches Nest,", "tokens": ["H\u00e4ngt", "si\u00b7cher", "man\u00b7ches", "Nest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Bl\u00fcthe Knospen streben,", "tokens": ["Zur", "Bl\u00fc\u00b7the", "Knos\u00b7pen", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hebt sich zu h\u00f6h'rem Leben", "tokens": ["Hebt", "sich", "zu", "h\u00f6h'\u00b7rem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am Stamm empor der Epheu fest.", "tokens": ["Am", "Stamm", "em\u00b7por", "der", "E\u00b7pheu", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O, Baum, in deiner H\u00f6he,", "tokens": ["O", ",", "Baum", ",", "in", "dei\u00b7ner", "H\u00f6\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie gl\u00fccklich scheinst du mir!", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "scheinst", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Starke bei den Schwachen,", "tokens": ["Die", "Star\u00b7ke", "bei", "den", "Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darfst du sie stolz bewachen", "tokens": ["Darfst", "du", "sie", "stolz", "be\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Alles schaut hinauf zu dir!", "tokens": ["Und", "Al\u00b7les", "schaut", "hin\u00b7auf", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da reget sie die Zweige", "tokens": ["Da", "re\u00b7get", "sie", "die", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und fl\u00fcstert leis' und lind:", "tokens": ["Und", "fl\u00fcs\u00b7tert", "leis'", "und", "lind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwohl sch\u00f6n ist's, da\u00df ich ihnen,", "tokens": ["\u00bb", "wohl", "sch\u00f6n", "ist's", ",", "da\u00df", "ich", "ih\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Schwachen, hier kann dienen,", "tokens": ["Den", "Schwa\u00b7chen", ",", "hier", "kann", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch bin ich drum nicht frohgesinnt.", "tokens": ["Doch", "bin", "ich", "drum", "nicht", "froh\u00b7ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie ich allein hier stehe", "tokens": ["Wie", "ich", "al\u00b7lein", "hier", "ste\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ganz auf mich selbst gestellt,", "tokens": ["Ganz", "auf", "mich", "selbst", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r' unter meinem Dache", "tokens": ["W\u00e4r'", "un\u00b7ter", "mei\u00b7nem", "Da\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich lieber doch die schwache", "tokens": ["Ich", "lie\u00b7ber", "doch", "die", "schwa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Feldblume, die mein Schutz erh\u00e4lt.", "tokens": ["Feld\u00b7blu\u00b7me", ",", "die", "mein", "Schutz", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "W\u00e4r' lieber selbst das V\u00f6glein,", "tokens": ["W\u00e4r'", "lie\u00b7ber", "selbst", "das", "V\u00f6\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das s\u00fc\u00df mir Lieder singt,", "tokens": ["Das", "s\u00fc\u00df", "mir", "Lie\u00b7der", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am liebsten wohl der warme", "tokens": ["Am", "liebs\u00b7ten", "wohl", "der", "war\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Epheu, der seine Arme", "tokens": ["E\u00b7pheu", ",", "der", "sei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Lieb' und Treue um mich schlingt!\u00ab", "tokens": ["In", "Lieb'", "und", "Treu\u00b7e", "um", "mich", "schlingt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"