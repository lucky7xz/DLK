, "line.2": {"text": "Doch d\u00fcnkt er uns zu lang;", "tokens": ["Doch", "d\u00fcnkt", "er", "uns", "zu", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann endigt sich der lange Rath,", "tokens": ["Wann", "en\u00b7digt", "sich", "der", "lan\u00b7ge", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ihnen etwa bang?", "tokens": ["Ist", "ih\u00b7nen", "et\u00b7wa", "bang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Schon steht die Sonn am Himmel hoch,", "tokens": ["Schon", "steht", "die", "Sonn", "am", "Him\u00b7mel", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht tr\u00e4g im blauen Zelt,", "tokens": ["Nicht", "tr\u00e4g", "im", "blau\u00b7en", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wir verziehen immer noch,", "tokens": ["Und", "wir", "ver\u00b7zie\u00b7hen", "im\u00b7mer", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu hauen in dem Feld!\u00ab", "tokens": ["Zu", "hau\u00b7en", "in", "dem", "Feld", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punc