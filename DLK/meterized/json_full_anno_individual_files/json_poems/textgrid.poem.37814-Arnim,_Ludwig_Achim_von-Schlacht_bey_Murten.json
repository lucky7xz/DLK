{"textgrid.poem.37814": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schlacht bey Murten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeitung flog von Land zu Land,", "tokens": ["Die", "Zei\u00b7tung", "flog", "von", "Land", "zu", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Murten liegt Burgund!", "tokens": ["Vor", "Mur\u00b7ten", "liegt", "Bur\u00b7gund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder eilt f\u00fcrs Vaterland,", "tokens": ["Und", "je\u00b7der", "eilt", "f\u00fcrs", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu streiten mit Burgund.", "tokens": ["Zu", "strei\u00b7ten", "mit", "Bur\u00b7gund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "NE", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Im Feld vor einem gr\u00fcnen Wald,", "tokens": ["Im", "Feld", "vor", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rief Knecht und Reutersmann,", "tokens": ["Rief", "Knecht", "und", "Reu\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Laut rief von Lothringen Renald:", "tokens": ["Laut", "rief", "von", "Loth\u00b7rin\u00b7gen", "Re\u00b7nald", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00bbwir wollen vorne dran.", "tokens": ["\u00bb", "wir", "wol\u00b7len", "vor\u00b7ne", "dran", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die F\u00fchrer hielten kurzen Rath,", "tokens": ["Die", "F\u00fch\u00b7rer", "hiel\u00b7ten", "kur\u00b7zen", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch d\u00fcnkt er uns zu lang;", "tokens": ["Doch", "d\u00fcnkt", "er", "uns", "zu", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann endigt sich der lange Rath,", "tokens": ["Wann", "en\u00b7digt", "sich", "der", "lan\u00b7ge", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ihnen etwa bang?", "tokens": ["Ist", "ih\u00b7nen", "et\u00b7wa", "bang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon steht die Sonn am Himmel hoch,", "tokens": ["Schon", "steht", "die", "Sonn", "am", "Him\u00b7mel", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht tr\u00e4g im blauen Zelt,", "tokens": ["Nicht", "tr\u00e4g", "im", "blau\u00b7en", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wir verziehen immer noch,", "tokens": ["Und", "wir", "ver\u00b7zie\u00b7hen", "im\u00b7mer", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu hauen in dem Feld!\u00ab", "tokens": ["Zu", "hau\u00b7en", "in", "dem", "Feld", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbzwar furchtbar knallte Karls Gesch\u00fctz,", "tokens": ["\u00bb", "zwar", "furcht\u00b7bar", "knall\u00b7te", "Karls", "Ge\u00b7sch\u00fctz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man gab darum nicht viel;", "tokens": ["Man", "gab", "da\u00b7rum", "nicht", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man achtete nicht in der Hitz,", "tokens": ["Man", "ach\u00b7te\u00b7te", "nicht", "in", "der", "Hitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ob der und jener fiel.", "tokens": ["Ob", "der", "und", "je\u00b7ner", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Im weiten Kreise blizt das Schwerdt,", "tokens": ["Im", "wei\u00b7ten", "Krei\u00b7se", "blizt", "das", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auslangt der lange Spie\u00df;", "tokens": ["Aus\u00b7langt", "der", "lan\u00b7ge", "Spie\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Blut d\u00fcrstete das breite Schwerdt,", "tokens": ["Blut", "d\u00fcrs\u00b7te\u00b7te", "das", "brei\u00b7te", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blut trank der lange Spie\u00df.", "tokens": ["Blut", "trank", "der", "lan\u00b7ge", "Spie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Welsche k\u00e4mpfte kurze Zeit,", "tokens": ["Der", "Wel\u00b7sche", "k\u00e4mpf\u00b7te", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Knecht und Ritter lief;", "tokens": ["Der", "Knecht", "und", "Rit\u00b7ter", "lief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das weite Feld ward \u00fcberstreut", "tokens": ["Das", "wei\u00b7te", "Feld", "ward", "\u00fc\u00b7bers\u00b7treut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Speeren Kniees tief.", "tokens": ["Mit", "Spee\u00b7ren", "Kni\u00b7ees", "tief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der floh zum Strauch \u2013 der floh zum Hayn", "tokens": ["Der", "floh", "zum", "Strauch", "\u2013", "der", "floh", "zum", "Hayn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$(", "ART", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vorm hellen Sonnen-Licht,", "tokens": ["Vorm", "hel\u00b7len", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel sprangen in die See hinein,", "tokens": ["Viel", "spran\u00b7gen", "in", "die", "See", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und d\u00fcrsteten doch nicht.", "tokens": ["Und", "d\u00fcrs\u00b7te\u00b7ten", "doch", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie schwammen wie der Enten Schaar", "tokens": ["Sie", "schwam\u00b7men", "wie", "der", "En\u00b7ten", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wasser hin und her,", "tokens": ["Im", "Was\u00b7ser", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4r es wilder Enten Schaar", "tokens": ["Als", "w\u00e4r", "es", "wil\u00b7der", "En\u00b7ten", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Scho\u00df man sie im Ger\u00f6hr.", "tokens": ["Scho\u00df", "man", "sie", "im", "Ge\u00b7r\u00f6hr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Auf Schiffen fuhr man in den See,", "tokens": ["Auf", "Schif\u00b7fen", "fuhr", "man", "in", "den", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlug sie mit Rudern todt.", "tokens": ["Schlug", "sie", "mit", "Ru\u00b7dern", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Waidwort war nur Ach und Weh,", "tokens": ["Das", "Waid\u00b7wort", "war", "nur", "Ach", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die gr\u00fcne See ward roth.", "tokens": ["Die", "gr\u00fc\u00b7ne", "See", "ward", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Viel klommen auf die B\u00e4ume hoch,", "tokens": ["Viel", "klom\u00b7men", "auf", "die", "B\u00e4u\u00b7me", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die scho\u00df man wie die Kr\u00e4hn;", "tokens": ["Die", "scho\u00df", "man", "wie", "die", "Kr\u00e4hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Fittich fehlten ihnen noch,", "tokens": ["Die", "Fit\u00b7tich", "fehl\u00b7ten", "ih\u00b7nen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie mocht der Wind nicht wehn.", "tokens": ["Sie", "mocht", "der", "Wind", "nicht", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Zwo Meilen lang bedeckte sich,", "tokens": ["Zwo", "Mei\u00b7len", "lang", "be\u00b7deck\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Land mit Tod und Blut", "tokens": ["Das", "Land", "mit", "Tod", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Land, der Strauch, die Rose glich", "tokens": ["Das", "Land", ",", "der", "Strauch", ",", "die", "Ro\u00b7se", "glich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem schwarzen Menschenblut.", "tokens": ["Dem", "schwar\u00b7zen", "Men\u00b7schen\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Den Bergen war die Sonne nah,", "tokens": ["Den", "Ber\u00b7gen", "war", "die", "Son\u00b7ne", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns den Sieg gebracht;", "tokens": ["Die", "uns", "den", "Sieg", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welschen, die man leben sah,", "tokens": ["Die", "Wel\u00b7schen", ",", "die", "man", "le\u00b7ben", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die dankten es der Nacht.", "tokens": ["Die", "dank\u00b7ten", "es", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ein Lager einem Marktplatz gleich", "tokens": ["Ein", "La\u00b7ger", "ei\u00b7nem", "Markt\u00b7platz", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam in der Schweizer Hand.", "tokens": ["Kam", "in", "der", "Schwei\u00b7zer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Karl machte schnell den Bettler reich,", "tokens": ["Karl", "mach\u00b7te", "schnell", "den", "Bett\u00b7ler", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im armen Schweizerland.", "tokens": ["Im", "ar\u00b7men", "Schwei\u00b7zer\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Schachzabel ist ein K\u00f6nigsspiel,", "tokens": ["Schach\u00b7za\u00b7bel", "ist", "ein", "K\u00f6\u00b7nigs\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jezt spielts der Eidgeno\u00df,", "tokens": ["Jezt", "spielts", "der", "Eid\u00b7ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er nahm ihm seiner Fenden viel,", "tokens": ["Er", "nahm", "ihm", "sei\u00b7ner", "Fen\u00b7den", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seite stand ihm blo\u00df.", "tokens": ["Die", "Sei\u00b7te", "stand", "ihm", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Die Rochen halfen ihm nicht viel,", "tokens": ["Die", "Ro\u00b7chen", "hal\u00b7fen", "ihm", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rosse litten Noth;", "tokens": ["Die", "Ros\u00b7se", "lit\u00b7ten", "Noth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wende sich, wohin er will,", "tokens": ["Er", "wen\u00b7de", "sich", ",", "wo\u00b7hin", "er", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schachmatt ist ihm gedroht.\u00ab", "tokens": ["Schach\u00b7matt", "ist", "ihm", "ge\u00b7droht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.17": {"line.1": {"text": "Der hatte selbst die Hand am Schwerdt,", "tokens": ["Der", "hat\u00b7te", "selbst", "die", "Hand", "am", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der diesen Reim gemacht;", "tokens": ["Der", "die\u00b7sen", "Reim", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis Abends m\u00e4ht' er mit dem Schwerdt,", "tokens": ["Bis", "A\u00b7bends", "m\u00e4ht'", "er", "mit", "dem", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Nachts sang er die Schlacht.", "tokens": ["Des", "Nachts", "sang", "er", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.18": {"line.1": {"text": "Er schwang die Saiten und das Schwerdt,", "tokens": ["Er", "schwang", "die", "Sai\u00b7ten", "und", "das", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fiedler und Soldat,", "tokens": ["Ein", "Fied\u00b7ler", "und", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Herren und den M\u00e4dchen werth,", "tokens": ["Den", "Her\u00b7ren", "und", "den", "M\u00e4d\u00b7chen", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem T\u00e4nzer und Pr\u00e4lat.", "tokens": ["Dem", "T\u00e4n\u00b7zer", "und", "Pr\u00e4\u00b7lat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Die mich gebahr, das gute Weib,", "tokens": ["Die", "mich", "ge\u00b7bahr", ",", "das", "gu\u00b7te", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie k\u00fc\u00dfte mich, und Veit,", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "mich", ",", "und", "Veit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hei\u00df Veit, so sprach das gute Weib!", "tokens": ["Hei\u00df", "Veit", ",", "so", "sprach", "das", "gu\u00b7te", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Veit hei\u00df ich immerseit.", "tokens": ["Veit", "hei\u00df", "ich", "im\u00b7mer\u00b7seit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Die Zeitung flog von Land zu Land,", "tokens": ["Die", "Zei\u00b7tung", "flog", "von", "Land", "zu", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Murten liegt Burgund!", "tokens": ["Vor", "Mur\u00b7ten", "liegt", "Bur\u00b7gund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder eilt f\u00fcrs Vaterland,", "tokens": ["Und", "je\u00b7der", "eilt", "f\u