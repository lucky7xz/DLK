ri"}, "line.3": {"text": "Die Fittich fehlten ihnen noch,", "tokens": ["Die", "Fit\u00b7tich", "fehl\u00b7ten", "ih\u00b7nen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie mocht der Wind nicht wehn.", "tokens": ["Sie", "mocht", "der", "Wind", "nicht", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Zwo Meilen lang bedeckte sich,", "tokens": ["Zwo", "Mei\u00b7len", "lang", "be\u00b7deck\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Land mit Tod und Blut", "tokens": ["Das", "Land", "mit", "Tod", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Land, der Strauch, die Rose glich", "tokens": ["Das", "Land", ",", "der", "Strauch", ",", "die", "Ro\u00b7se", "glich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem schwarzen Menschenblut.", "tokens": ["Dem", "schwar\u00b7zen", "Men\u00b7schen\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Den Bergen war die Sonne nah,", "tokens": ["Den", "Ber\u00b7gen", "war", "die", "Son\u00b7ne", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns den Sieg gebracht;", "tokens": ["Die", "uns", "den", "Sieg", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welschen, die man leben sah,", "tokens": ["Die", "Wel\u00b7schen", ",", "die", "man", "le\u00b7ben", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die dankten es der Nacht.", "tokens": ["Die", "dank\u00b7ten", "es", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "Ein Lager einem Marktplatz gleich", "tokens": ["Ein", "La\u00b7ger", "ei\u00b7nem", "Markt\u00b7platz", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam in der Schweizer Hand.", "tokens": ["Kam", "in", "der", "Schwei\u00b7zer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Karl machte schnell den Bettler reich,", "tokens": ["Karl", "mach\u00b7te", "schnell", "den", "Bett\u00b7ler", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im armen Schweizerland.", "tokens": ["Im", "ar\u00b7men", "Schwei\u00b7zer\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.34": {"line.1": {"text": "Schachzabel ist ein K\u00f6nigsspiel,", "tokens": ["Schach\u00b7za\u00b7bel", "ist", "ein", "K\u00f6\u00b7nigs\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jezt spielts der Eidgeno\u00df,", "tokens": ["Jezt", "spielts", "der", "Eid\u00b7ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er nahm ihm seiner Fenden viel,", "tokens": ["Er", "nahm", "ihm", "sei\u00b7ner", "Fen\u00b7den", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seite stand ihm blo\u00df.", "tokens": ["Die", "Sei\u00b7te", "stand", "ihm", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.35": {"line.1": {"text": "Die Rochen halfen ihm nicht viel,", "tokens": ["Die", "Ro\u00b7chen", "hal\u00b7fen", "ihm", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rosse litten Noth;", "tokens": ["Die", "Ros\u00b7se", "lit\u00b7ten", "Noth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wende sich, wohin er will,", "tokens": ["Er", "wen\u00b7de", "sich", ",", "wo\u00b7hin", "er", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schachmatt ist ihm gedroht.\u00ab", "tokens": ["Schach\u00b7matt", "ist", "ihm", "ge\u00b7droht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.36": {"line.1": {"text": "Der hatte selbst die Hand am Schwerdt,", "tokens": ["Der", "hat\u00b7te", "selbst", "die", "Hand", "am", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der diesen Reim gemacht;", "tokens": ["Der", "die\u00b7sen", "Reim", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis Abends m\u00e4ht' er mit dem Schwerdt,", "tokens": ["Bis", "A\u00b7bends", "m\u00e4ht'", "er", "mit", "dem", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Nachts sang er die Schlacht.", "tokens": ["Des", "Nachts", "sang", "er", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.37": {"line.1": {"text": "Er schwang die Saiten und das Schwerdt,", "tokens": ["Er", "schwang", "die", "Sai\u00b7ten", "und", "das", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fiedler und Soldat,", "tokens": ["Ein", "Fied\u00b7ler", "und", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Herren und den M\u00e4dchen werth,", "tokens": ["Den", "Her\u00b7ren", "und", "den", "M\u00e4d\u00b7chen", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem T\u00e4nzer und Pr\u00e4lat.", "tokens": ["Dem", "T\u00e4n\u00b7zer", "und", "Pr\u00e4\u00b7lat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.38": {"line.1": {"text": "Die mich gebahr, das gute Weib,", "tokens": ["Die", "mich", "ge\u00b7bahr", ",", "das", "gu\u00b7te", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie k\u00fc\u00dfte mich, und Veit,", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "mich", ",", "und", "Veit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hei\u00df Veit, so sprach das gute Weib!", "tokens": ["Hei\u00df", "Veit", ",", "so", "sprach", "das", "gu\u00b7te", "Weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+