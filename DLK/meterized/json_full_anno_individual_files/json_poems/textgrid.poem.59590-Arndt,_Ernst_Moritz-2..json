s": ["Die", "stol\u00b7ze", "Pflicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Erzittert nicht,", "tokens": ["Er\u00b7zit\u00b7tert", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mag Land und Meer vergehen,", "tokens": ["Mag", "Land", "und", "Meer", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie wird mit Gott bestehen.", "tokens": ["Sie", "wird", "mit", "Gott", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drum walt' es Gott, der alles kann,", "tokens": ["Drum", "walt'", "es", "Gott", ",", "der", "al\u00b7les", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vater in den H\u00f6hen!", "tokens": ["Der", "Va\u00b7ter", "in", "den", "H\u00f6\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist der rechte Held und Mann", "tokens": ["Er", "ist", "der", "rech\u00b7te", "Held", "und", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird es wohl verstehen.", "tokens": ["Und", "wird", "es", "wohl", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer Gott vertraut,", "tokens": ["Wer", "Gott", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Hat wohl gebaut", "tokens": ["Hat", "wohl", "ge\u00b7baut"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Im Tode und im Leben:", "tokens": ["Im", "To\u00b7de", "und", "im", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Recht wird oben schweben.", "tokens": ["Sein", "Recht", "wird", "o\u00b7ben", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}