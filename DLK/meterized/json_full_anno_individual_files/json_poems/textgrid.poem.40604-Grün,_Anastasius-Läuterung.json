{"textgrid.poem.40604": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4uterung", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo war, wo ist, wo wird sie sein,", "tokens": ["Wo", "war", ",", "wo", "ist", ",", "wo", "wird", "sie", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stunde, wahrem Gl\u00fcck erlesen?", "tokens": ["Die", "Stun\u00b7de", ",", "wah\u00b7rem", "Gl\u00fcck", "er\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist nicht und sie wird nicht sein,", "tokens": ["Sie", "ist", "nicht", "und", "sie", "wird", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "KON", "PPER", "VAFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sie ist immer nur ", "tokens": ["Denn", "sie", "ist", "im\u00b7mer", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir m\u00e4keln viel, bis sie entrinnt,", "tokens": ["Wir", "m\u00e4\u00b7keln", "viel", ",", "bis", "sie", "ent\u00b7rinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie d\u00e4ucht uns sch\u00f6n, wenn wir sie missen,", "tokens": ["Sie", "d\u00e4ucht", "uns", "sch\u00f6n", ",", "wenn", "wir", "sie", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df wir gl\u00fccklich waren, wissen", "tokens": ["Und", "da\u00df", "wir", "gl\u00fcck\u00b7lich", "wa\u00b7ren", ",", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir erst, wenn wir es nimmer sind.", "tokens": ["Wir", "erst", ",", "wenn", "wir", "es", "nim\u00b7mer", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo ist der Mann, wann wird er kommen,", "tokens": ["Wo", "ist", "der", "Mann", ",", "wann", "wird", "er", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den alle Tugendzierden adeln?", "tokens": ["Den", "al\u00b7le", "Tu\u00b7gend\u00b7zier\u00b7den", "a\u00b7deln", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Steht er dir nah, noch so vollkommen,", "tokens": ["Steht", "er", "dir", "nah", ",", "noch", "so", "voll\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wei\u00dft du die\u00df und das zu tadeln;", "tokens": ["Doch", "wei\u00dft", "du", "die\u00df", "und", "das", "zu", "ta\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "KON", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erst wenn er schied und nimmer kehrt,", "tokens": ["Erst", "wenn", "er", "schied", "und", "nim\u00b7mer", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ergl\u00e4nzen hell dir seine Gaben,", "tokens": ["Er\u00b7gl\u00e4n\u00b7zen", "hell", "dir", "sei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und eines Menschen ganzen Werth", "tokens": ["Und", "ei\u00b7nes", "Men\u00b7schen", "gan\u00b7zen", "Werth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu kennen, m\u00fc\u00dft ihr ihn begraben.", "tokens": ["Zu", "ken\u00b7nen", ",", "m\u00fc\u00dft", "ihr", "ihn", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was lieb dir, wird dir lieber sein,", "tokens": ["Was", "lieb", "dir", ",", "wird", "dir", "lie\u00b7ber", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "$,", "VAFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch schmerzlich lieber durch die ", "tokens": ["Noch", "schmerz\u00b7lich", "lie\u00b7ber", "durch", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Blick auf! wie schlingt sie gl\u00e4nzend rein", "tokens": ["Blick", "auf", "!", "wie", "schlingt", "sie", "gl\u00e4n\u00b7zend", "rein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den goldnen Zauber um die Sterne!", "tokens": ["Den", "gold\u00b7nen", "Zau\u00b7ber", "um", "die", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie webt die blaue Schleierluft", "tokens": ["Sie", "webt", "die", "blau\u00b7e", "Schlei\u00b7er\u00b7luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um des Gebirges schroffe Zinnen,", "tokens": ["Um", "des", "Ge\u00b7bir\u00b7ges", "schrof\u00b7fe", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df eingeh\u00fcllt in weichen Duft", "tokens": ["Da\u00df", "ein\u00b7ge\u00b7h\u00fcllt", "in", "wei\u00b7chen", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die H\u00e4rten des Gesteins zerrinnen.", "tokens": ["Die", "H\u00e4r\u00b7ten", "des", "Ge\u00b7steins", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Blick nieder, wo von ihrem Gru\u00df", "tokens": ["Blick", "nie\u00b7der", ",", "wo", "von", "ih\u00b7rem", "Gru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Friedhofh\u00fcgel wogend schwellen,", "tokens": ["Die", "Fried\u00b7hof\u00b7h\u00fc\u00b7gel", "wo\u00b7gend", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des dunkeln Stromes gr\u00fcne Wellen,", "tokens": ["Des", "dun\u00b7keln", "Stro\u00b7mes", "gr\u00fc\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der so viel Liebes scheiden mu\u00df!", "tokens": ["Der", "so", "viel", "Lie\u00b7bes", "schei\u00b7den", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sp\u00fclen Makel weg und Fehle, \u2013", "tokens": ["Sie", "sp\u00fc\u00b7len", "Ma\u00b7kel", "weg", "und", "Feh\u00b7le", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie ein Schwan beim Wellenschein,", "tokens": ["Und", "wie", "ein", "Schwan", "beim", "Wel\u00b7len\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Dr\u00fcberflug ahnt deine Seele:", "tokens": ["Im", "Dr\u00fc\u00b7berf\u00b7lug", "ahnt", "dei\u00b7ne", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier bad' ich einst den Fittig rein.", "tokens": ["Hier", "bad'", "ich", "einst", "den", "Fit\u00b7tig", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo war, wo ist, wo wird sie sein,", "tokens": ["Wo", "war", ",", "wo", "ist", ",", "wo", "wird", "sie", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stunde, wahrem Gl\u00fcck erlesen?", "tokens": ["Die", "Stun\u00b7de", ",", "wah\u00b7rem", "Gl\u00fcck", "er\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist nicht und sie wird nicht sein,", "tokens": ["Sie", "ist", "nicht", "und", "sie", "wird", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "wor