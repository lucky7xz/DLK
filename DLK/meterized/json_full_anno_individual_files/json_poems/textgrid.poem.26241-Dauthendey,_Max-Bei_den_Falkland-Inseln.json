{"textgrid.poem.26241": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Bei den Falkland-Inseln", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne wollte nicht untergehen,", "tokens": ["Die", "Son\u00b7ne", "woll\u00b7te", "nicht", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die hohe, sie wollte heut Helden sehen.", "tokens": ["Die", "ho\u00b7he", ",", "sie", "woll\u00b7te", "heut", "Hel\u00b7den", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Es k\u00e4mpft das Geschwader des Grafen Spee, \u2013", "tokens": ["Es", "k\u00e4mpft", "das", "Ge\u00b7schwa\u00b7der", "des", "Gra\u00b7fen", "Spee", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "NE", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Granaten brummen und krachen und heulen.", "tokens": ["Gra\u00b7na\u00b7ten", "brum\u00b7men", "und", "kra\u00b7chen", "und", "heu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVFIN", "KON", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und aufrecht stehen des Salzwassers S\u00e4ulen,", "tokens": ["Und", "auf\u00b7recht", "ste\u00b7hen", "des", "Salz\u00b7was\u00b7sers", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wei\u00df ragt der Gischt aus runder See.", "tokens": ["Wei\u00df", "ragt", "der", "Gischt", "aus", "run\u00b7der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Treffer schlagen wie eiserne Keulen", "tokens": ["Die", "Tref\u00b7fer", "schla\u00b7gen", "wie", "ei\u00b7ser\u00b7ne", "Keu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ins Admiralschiff, das neigt sich nach Lee.", "tokens": ["Ins", "Ad\u00b7mi\u00b7ral\u00b7schiff", ",", "das", "neigt", "sich", "nach", "Lee", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "VVFIN", "PRF", "APPR", "NE", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Sonne wollte nicht untergehen,", "tokens": ["Die", "Son\u00b7ne", "woll\u00b7te", "nicht", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die hohe, sie wollte heut Helden sehen.", "tokens": ["Die", "ho\u00b7he", ",", "sie", "woll\u00b7te", "heut", "Hel\u00b7den", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Noch einmal die deutsche Flagge blinkt,", "tokens": ["Noch", "ein\u00b7mal", "die", "deut\u00b7sche", "Flag\u00b7ge", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und alle Mann stramm an den Gesch\u00fctzen,", "tokens": ["Und", "al\u00b7le", "Mann", "stramm", "an", "den", "Ge\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und alle schwenken mit Hurra die M\u00fctzen.", "tokens": ["Und", "al\u00b7le", "schwen\u00b7ken", "mit", "Hur\u00b7ra", "die", "M\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Graf auf der Br\u00fccke den S\u00f6hnen winkt,", "tokens": ["Der", "Graf", "auf", "der", "Br\u00fc\u00b7cke", "den", "S\u00f6h\u00b7nen", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die, wie im Sieg einst, im Tod ihn st\u00fctzen.", "tokens": ["Die", ",", "wie", "im", "Sieg", "einst", ",", "im", "Tod", "ihn", "st\u00fct\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "APPRART", "NN", "ADV", "$,", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sein Schiff, es feuert noch, al