{"dta.poem.16202": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "256.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch kann was er will, wenn er will was er kann;", "tokens": ["Der", "Mensch", "kann", "was", "er", "will", ",", "wenn", "er", "will", "was", "er", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PWS", "PPER", "VMFIN", "$,", "KOUS", "PPER", "VMFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Ist wohl ein guter Spruch, doch gn\u00fcgt er nicht dem Mann.", "tokens": ["Ist", "wohl", "ein", "gu\u00b7ter", "Spruch", ",", "doch", "gn\u00fcgt", "er", "nicht", "dem", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Mensch kann was er will, wenn er will was er soll;", "tokens": ["Der", "Mensch", "kann", "was", "er", "will", ",", "wenn", "er", "will", "was", "er", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PWS", "PPER", "VMFIN", "$,", "KOUS", "PPER", "VMFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In diesem ist das Ma\u00df der Mannestugend voll.", "tokens": ["In", "die\u00b7sem", "ist", "das", "Ma\u00df", "der", "Man\u00b7nes\u00b7tu\u00b7gend", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist der Zauberbann, womit du alles stillst:", "tokens": ["Das", "ist", "der", "Zau\u00b7ber\u00b7bann", ",", "wo\u00b7mit", "du", "al\u00b7les", "stillst", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wolle nur was du sollst, so kanst du was du willst.", "tokens": ["Wol\u00b7le", "nur", "was", "du", "sollst", ",", "so", "kanst", "du", "was", "du", "willst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PWS", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "PWS", "PPER", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}