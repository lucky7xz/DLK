{"textgrid.poem.57029": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Zukunftssorgen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Korf, den Ahnung leicht erschreckt,", "tokens": ["Korf", ",", "den", "Ah\u00b7nung", "leicht", "er\u00b7schreckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sieht den Himmel schon bedeckt", "tokens": ["sieht", "den", "Him\u00b7mel", "schon", "be\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "von Ballonen jeder Gr\u00f6\u00dfe", "tokens": ["von", "Bal\u00b7lo\u00b7nen", "je\u00b7der", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "und verfertigt ganze St\u00f6\u00dfe", "tokens": ["und", "ver\u00b7fer\u00b7tigt", "gan\u00b7ze", "St\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "von Entw\u00fcrfen zu Statuten", "tokens": ["von", "Ent\u00b7w\u00fcr\u00b7fen", "zu", "Sta\u00b7tu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "eines Klubs zur resoluten", "tokens": ["ei\u00b7nes", "Klubs", "zur", "re\u00b7so\u00b7lu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wahrung der gedachten Zone", "tokens": ["Wah\u00b7rung", "der", "ge\u00b7dach\u00b7ten", "Zo\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "vor der Willk\u00fcr der Ballone.", "tokens": ["vor", "der", "Will\u00b7k\u00fcr", "der", "Bal\u00b7lo\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Doch er ahnt schon, ach, beim Schreiben", "tokens": ["Doch", "er", "ahnt", "schon", ",", "ach", ",", "beim", "Schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ITJ", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seinen Klub im R\u00fcckstand bleiben:", "tokens": ["sei\u00b7nen", "Klub", "im", "R\u00fcck\u00b7stand", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "d\u00e4mmrig, d\u00fcnkt ihn, wird die Luft", "tokens": ["d\u00e4mm\u00b7rig", ",", "d\u00fcnkt", "ihn", ",", "wird", "die", "Luft"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und die Landschaft Grab und Gruft.", "tokens": ["und", "die", "Land\u00b7schaft", "Grab", "und", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er begibt sich drum der Feder,", "tokens": ["Er", "be\u00b7gibt", "sich", "drum", "der", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "steckt das Licht an (wie dann jeder),", "tokens": ["steckt", "das", "Licht", "an", "(", "wie", "dann", "je\u00b7der", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "$(", "KOKOM", "ADV", "PIS", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "tritt damit bei Palmstr\u00f6m ein,", "tokens": ["tritt", "da\u00b7mit", "bei", "Palm\u00b7str\u00f6m", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und so sitzen sie zu zwein.", "tokens": ["und", "so", "sit\u00b7zen", "sie", "zu", "zwein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Endlich, nach vier langen Stunden,", "tokens": ["End\u00b7lich", ",", "nach", "vier", "lan\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ist der Albdruck \u00fcberwunden.", "tokens": ["ist", "der", "Alb\u00b7druck", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Palmstr\u00f6m bricht zuerst den Bann:", "tokens": ["Palm\u00b7str\u00f6m", "bricht", "zu\u00b7erst", "den", "Bann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Korf, so spricht er, sei ein Mann!", "tokens": ["Korf", ",", "so", "spricht", "er", ",", "sei", "ein", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du vergreifst dich im Jahrzehnt:", "tokens": ["Du", "ver\u00b7greifst", "dich", "im", "Jahr\u00b7zehnt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch wird all das erst ersehnt,", "tokens": ["Noch", "wird", "all", "das", "erst", "er\u00b7sehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "PDS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "was, vom Geist dir vorgegaukelt,", "tokens": ["was", ",", "vom", "Geist", "dir", "vor\u00b7ge\u00b7gau\u00b7kelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "heut dein Haupt schon \u00fcberschaukelt.", "tokens": ["heut", "dein", "Haupt", "schon", "\u00fc\u00b7bersc\u00b7hau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Korf entrafft sich dem Gesicht.", "tokens": ["Korf", "en\u00b7trafft", "sich", "dem", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemand fliegt im goldnen Licht!", "tokens": ["Nie\u00b7mand", "fliegt", "im", "gold\u00b7nen", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er verl\u00f6scht die Kerze schweigend.", "tokens": ["Er", "ver\u00b7l\u00f6scht", "die", "Ker\u00b7ze", "schwei\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch dann, auf die Sonne zeigend,", "tokens": ["Doch", "dann", ",", "auf", "die", "Son\u00b7ne", "zei\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "spricht er: Wenn nicht jetzt, so einst \u2013", "tokens": ["spricht", "er", ":", "Wenn", "nicht", "jetzt", ",", "so", "einst", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "KOUS", "PTKNEG", "ADV", "$,", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "kommt es, da\u00df du nicht mehr scheinst,", "tokens": ["kommt", "es", ",", "da\u00df", "du", "nicht", "mehr", "scheinst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "wenigstens nicht uns, den \u2013 grausend", "tokens": ["we\u00b7nigs\u00b7tens", "nicht", "uns", ",", "den", "\u2013", "grau\u00b7send"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "PTKNEG", "PPER", "$,", "ART", "$(", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "sag ich's \u2013: Unteren Zehntausend! ...", "tokens": ["sag", "ich's", "\u2013", ":", "Un\u00b7te\u00b7ren", "Zehn\u00b7tau\u00b7send", "!", "..."], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "$(", "$.", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wieder sitzt v. Korf danach", "tokens": ["Wie\u00b7der", "sitzt", "v.", "Korf", "da\u00b7nach"], "token_info": ["word", "word", "abbreviation", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "stumm in seinem Schreibgemach", "tokens": ["stumm", "in", "sei\u00b7nem", "Schreib\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und entwirft Statuten eines", "tokens": ["und", "ent\u00b7wirft", "Sta\u00b7tu\u00b7ten", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klubs zum Schutz des Sonnenscheines.", "tokens": ["Klubs", "zum", "Schutz", "des", "Son\u00b7nen\u00b7schei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Korf, den Ahnung leicht erschreckt,", "tokens": ["Korf", ",", "den", "Ah\u00b7nung", "leicht", "er\u00b7schreckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"tex