{"textgrid.poem.48200": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcberla\u00df es der Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erscheint dir etwas unerh\u00f6rt,", "tokens": ["Er\u00b7scheint", "dir", "et\u00b7was", "un\u00b7er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du tiefsten Herzens emp\u00f6rt,", "tokens": ["Bist", "du", "tiefs\u00b7ten", "Her\u00b7zens", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "B\u00e4ume nicht auf, versuch's nicht mit Streit,", "tokens": ["B\u00e4u\u00b7me", "nicht", "auf", ",", "ver\u00b7such's", "nicht", "mit", "Streit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$,", "NE", "PTKNEG", "APPR", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ber\u00fchr es nicht, \u00fcberla\u00df es der Zeit.", "tokens": ["Be\u00b7r\u00fchr", "es", "nicht", ",", "\u00fc\u00b7berl\u00b7a\u00df", "es", "der", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Am ersten Tag wirst du feige dich schelten,", "tokens": ["Am", "ers\u00b7ten", "Tag", "wirst", "du", "fei\u00b7ge", "dich", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["A