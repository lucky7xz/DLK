{"textgrid.poem.53596": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6rperkultur", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wie sich auch die wei\u00dfen Glieder ranken;", "tokens": ["Und", "wie", "sich", "auch", "die", "wei\u00b7\u00dfen", "Glie\u00b7der", "ran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und wie sie sich, wenn die letzten H\u00fcllen sanken,", "tokens": ["und", "wie", "sie", "sich", ",", "wenn", "die", "letz\u00b7ten", "H\u00fcl\u00b7len", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "$,", "KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "woll\u00fcstig aalt \u2013", "tokens": ["wol\u00b7l\u00fcs\u00b7tig", "aalt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "es kann mich nicht von meiner Brunst erl\u00f6sen.", "tokens": ["es", "kann", "mich", "nicht", "von", "mei\u00b7ner", "Brunst", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es ist doch alles, teure Voyeusen,", "tokens": ["Es", "ist", "doch", "al\u00b7les", ",", "teu\u00b7re", "Voy\u00b7e\u00b7u\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "bezahlt! bezahlt!", "tokens": ["be\u00b7zahlt", "!", "be\u00b7zahlt", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$.", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es rast die Polizei. Die Kommissare,", "tokens": ["Es", "rast", "die", "Po\u00b7li\u00b7zei", ".", "Die", "Kom\u00b7mis\u00b7sa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sie nutzen dies als eine wunderbare", "tokens": ["sie", "nut\u00b7zen", "dies", "als", "ei\u00b7ne", "wun\u00b7der\u00b7ba\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Reklame aus.", "tokens": ["Re\u00b7kla\u00b7me", "aus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "\u00bbdie Orgje\u00ab. Und: \u00bbEntkleidet bis zum Nabel\u00ab", "tokens": ["\u00bb", "die", "Org\u00b7je", "\u00ab", ".", "Und", ":", "\u00bb", "Ent\u00b7klei\u00b7det", "bis", "zum", "Na\u00b7bel", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "$.", "KON", "$.", "$(", "VVFIN", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "(von unten her) \u2013 und: \u00bbWelch ein S\u00fcndenbabel!", "tokens": ["(", "von", "un\u00b7ten", "her", ")", "\u2013", "und", ":", "\u00bb", "Welch", "ein", "S\u00fcn\u00b7den\u00b7ba\u00b7bel", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "PTKVZ", "$(", "$(", "KON", "$.", "$(", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Welch Pfreudenhaus!\u00ab", "tokens": ["Welch", "Pfreu\u00b7den\u00b7haus", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Du Polizei vom Alexanderpl\u00e4tzchen!", "tokens": ["Du", "Po\u00b7li\u00b7zei", "vom", "A\u00b7lex\u00b7an\u00b7der\u00b7pl\u00e4tz\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es liebt doch jeder gern sein eigenes Sch\u00e4tzchen", "tokens": ["Es", "liebt", "doch", "je\u00b7der", "gern", "sein", "ei\u00b7ge\u00b7nes", "Sch\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "und sein Pl\u00e4sir.", "tokens": ["und", "sein", "Pl\u00e4\u00b7sir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dies Schauspiel war, zum Beispiel, f\u00fcr die D\u00fcmmern.", "tokens": ["Dies", "Schau\u00b7spiel", "war", ",", "zum", "Bei\u00b7spiel", ",", "f\u00fcr", "die", "D\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "$,", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du mu\u00dft dich aber nicht um alles k\u00fcmmern \u2013", "tokens": ["Du", "mu\u00dft", "dich", "a\u00b7ber", "nicht", "um", "al\u00b7les", "k\u00fcm\u00b7mern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wir schenkens dir.", "tokens": ["wir", "schen\u00b7kens", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und, Presse, du! La\u00df das Moralgeflenne \u2013", "tokens": ["Und", ",", "Pres\u00b7se", ",", "du", "!", "La\u00df", "das", "Mo\u00b7ral\u00b7ge\u00b7flen\u00b7ne", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "$.", "VVIMP", "ART", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "willst du, da\u00df ich dir etwas Schlimmeres nenne", "tokens": ["willst", "du", ",", "da\u00df", "ich", "dir", "et\u00b7was", "Schlim\u00b7me\u00b7res", "nen\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "PIAT", "NN", "VVFIN"], "meter": "---+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "als dies Lokal?", "tokens": ["als", "dies", "Lo\u00b7kal", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Die gaben nackt sich hin im Lasterloche.", "tokens": ["Die", "ga\u00b7ben", "nackt", "sich", "hin", "im", "Las\u00b7ter\u00b7lo\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure