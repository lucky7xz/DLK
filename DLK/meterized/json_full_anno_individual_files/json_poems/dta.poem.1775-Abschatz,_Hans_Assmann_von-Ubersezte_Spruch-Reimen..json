{"dta.poem.1775": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer auff Best\u00e4ndigkeit gerichtet seine Sinnen/", "tokens": ["Wer", "auff", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "ge\u00b7rich\u00b7tet", "sei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVPP", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "L\u00e4st sich den wilden Schwarm des Volckes nicht gewin-", "tokens": ["L\u00e4st", "sich", "den", "wil\u00b7den", "Schwarm", "des", "Vol\u00b7ckes", "nicht", "ge\u00b7win"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "TRUNC"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "nen/", "tokens": ["nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Wie sauer ein Tyrann zu seinen Worten sieht/", "tokens": ["Wie", "sau\u00b7er", "ein", "Ty\u00b7rann", "zu", "sei\u00b7nen", "Wor\u00b7ten", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie sehr er seinen Kopff zu brechen sich bem\u00fcht/", "tokens": ["Wie", "sehr", "er", "sei\u00b7nen", "Kopff", "zu", "bre\u00b7chen", "sich", "be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wenig giebt er nach/ wie sehr die Winde blasen/", "tokens": ["So", "we\u00b7nig", "giebt", "er", "nach", "/", "wie", "sehr", "die", "Win\u00b7de", "bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "$(", "KOKOM", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wenig achtet er derselben tolles Rasen.", "tokens": ["So", "we\u00b7nig", "ach\u00b7tet", "er", "der\u00b7sel\u00b7ben", "tol\u00b7les", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und fiele gleich die Welt vor seinen Augen ein/", "tokens": ["Und", "fie\u00b7le", "gleich", "die", "Welt", "vor", "sei\u00b7nen", "Au\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wird sein freyer Sinn doch unerschrocken seyn.", "tokens": ["So", "wird", "sein", "frey\u00b7er", "Sinn", "doch", "un\u00b7er\u00b7schro\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}