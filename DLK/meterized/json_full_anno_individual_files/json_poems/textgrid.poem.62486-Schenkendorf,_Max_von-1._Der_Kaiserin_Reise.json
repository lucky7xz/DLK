{"textgrid.poem.62486": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1. Der Kaiserin Reise", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weht mich an, ihr Fr\u00fchlingswinde,", "tokens": ["Weht", "mich", "an", ",", "ihr", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Heimat milde Luft,", "tokens": ["Mei\u00b7ner", "Hei\u00b7mat", "mil\u00b7de", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bring' mir spielend, bring' mir linde", "tokens": ["Bring'", "mir", "spie\u00b7lend", ",", "bring'", "mir", "lin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderbaren Bl\u00fctenduft!", "tokens": ["Wun\u00b7der\u00b7ba\u00b7ren", "Bl\u00fc\u00b7ten\u00b7duft", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oeffne mir, du Starke, Sch\u00f6ne,", "tokens": ["Oeff\u00b7ne", "mir", ",", "du", "Star\u00b7ke", ",", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mailand, nun dein gastlich Thor,", "tokens": ["Mai\u00b7land", ",", "nun", "dein", "gast\u00b7lich", "Thor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Klingt ihr s\u00fc\u00dfen Liebest\u00f6ne", "tokens": ["Klingt", "ihr", "s\u00fc\u00b7\u00dfen", "Lie\u00b7be\u00b7st\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wieder in mein trunknes Ohr.", "tokens": ["Wie\u00b7der", "in", "mein", "trunk\u00b7nes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist es nicht das Land der Wonnen,", "tokens": ["Ist", "es", "nicht", "das", "Land", "der", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Kindheit Blumenau,", "tokens": ["Mei\u00b7ner", "Kind\u00b7heit", "Blu\u00b7me\u00b7nau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Lebens Morgensonnen,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Mor\u00b7gen\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ich endlich wiederschau'?", "tokens": ["Die", "ich", "end\u00b7lich", "wie\u00b7der\u00b7schau'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "R\u00fcckw\u00e4rts doch mit starken Banden", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "doch", "mit", "star\u00b7ken", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieht mich ein geheimes Wort,", "tokens": ["Zieht", "mich", "ein", "ge\u00b7hei\u00b7mes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach den frommen deutschen Landen", "tokens": ["Nach", "den", "from\u00b7men", "deut\u00b7schen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zieht mich's fern und n\u00f6rdlich fort.", "tokens": ["Zieht", "mich's", "fern", "und", "n\u00f6rd\u00b7lich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bittre Qual dort hat mein Leben,", "tokens": ["Bitt\u00b7re", "Qual", "dort", "hat", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Vaterland verkl\u00e4rt;", "tokens": ["Wie", "das", "Va\u00b7ter\u00b7land", "ver\u00b7kl\u00e4rt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Ph\u00f6nix sah ich schweben", "tokens": ["Ei\u00b7nen", "Ph\u00f6\u00b7nix", "sah", "ich", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den Flammen unversehrt.", "tokens": ["Aus", "den", "Flam\u00b7men", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier in Otto's alter Krone", "tokens": ["Hier", "in", "Ot\u00b7to's", "al\u00b7ter", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hab' ich den Gemahl gesehn;", "tokens": ["Hab'", "ich", "den", "Ge\u00b7mahl", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, von einem andern Throne", "tokens": ["Ach", ",", "von", "ei\u00b7nem", "an\u00b7dern", "Thro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00fcssen leer die Stufen stehn!", "tokens": ["M\u00fcs\u00b7sen", "leer", "die", "Stu\u00b7fen", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "R\u00fcckw\u00e4rts dann mit frischen Sinnen", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "dann", "mit", "fri\u00b7schen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das heil'ge deutsche Reich,", "tokens": ["In", "das", "heil'\u00b7ge", "deut\u00b7sche", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem w\u00fcrdigsten Beginnen,", "tokens": ["Zu", "dem", "w\u00fcr\u00b7digs\u00b7ten", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Herz, mein Herz, sei stark und weich!", "tokens": ["Herz", ",", "mein", "Herz", ",", "sei", "stark", "und", "weich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df die Kr\u00e4fte nicht veralten,", "tokens": ["Da\u00df", "die", "Kr\u00e4f\u00b7te", "nicht", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gottes Frist sich nicht vers\u00e4umt,", "tokens": ["Got\u00b7tes", "Frist", "sich", "nicht", "ver\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die tr\u00e4ume sich gestalten,", "tokens": ["Da\u00df", "die", "tr\u00e4u\u00b7me", "sich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die manch frommes Haupt getr\u00e4umt.", "tokens": ["Die", "manch", "from\u00b7mes", "Haupt", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hab' ich dennoch mi\u00dfverstanden", "tokens": ["Hab'", "ich", "den\u00b7noch", "mi\u00df\u00b7ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das geheime Zauberwort?", "tokens": ["Das", "ge\u00b7hei\u00b7me", "Zau\u00b7ber\u00b7wort", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer nach den deutschen Landen \u2013", "tokens": ["Nim\u00b7mer", "nach", "den", "deut\u00b7schen", "Lan\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiter, dunkler f\u00fchrt mich's fort.", "tokens": ["Wei\u00b7ter", ",", "dunk\u00b7ler", "f\u00fchrt", "mich's", "fort", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00e4rket mich mit Brod und Oele,", "tokens": ["St\u00e4r\u00b7ket", "mich", "mit", "Brod", "und", "O\u00b7e\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Mit dem Todessacrament;", "tokens": ["Mit", "dem", "To\u00b7des\u00b7sa\u00b7cra\u00b7ment", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott, mein Sch\u00f6pfer! ich befehle", "tokens": ["Gott", ",", "mein", "Sch\u00f6p\u00b7fer", "!", "ich", "be\u00b7feh\u00b7le"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meinen Geist in deine H\u00e4nd'.", "tokens": ["Mei\u00b7nen", "Geist", "in", "dei\u00b7ne", "H\u00e4nd'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fahret wohl, ihr Erdentr\u00e4ume,", "tokens": ["Fah\u00b7ret", "wohl", ",", "ihr", "Er\u00b7den\u00b7tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du, mein Kaiser und Gemahl!", "tokens": ["Du", ",", "mein", "Kai\u00b7ser", "und", "Ge\u00b7mahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oeffnet euch ihr Himmelsr\u00e4ume,", "tokens": ["Oeff\u00b7net", "euch", "ihr", "Him\u00b7mels\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchte, leuchte, sel'ger Strahl!", "tokens": ["Leuch\u00b7te", ",", "leuch\u00b7te", ",", "sel'\u00b7ger", "Strahl", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf, hinauf mit starken Schritten", "tokens": ["Auf", ",", "hin\u00b7auf", "mit", "star\u00b7ken", "Schrit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den hellen ew'gen Tag,", "tokens": ["In", "den", "hel\u00b7len", "ew'\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich dort f\u00fcr Deutschland bitten", "tokens": ["Da\u00df", "ich", "dort", "f\u00fcr", "Deutschland", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NE", "VVINF"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und auf Deutschland schauen mag!", "tokens": ["Und", "auf", "Deutschland", "schau\u00b7en", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Weht mich an, ihr Fr\u00fchlingswinde,", "tokens": ["Weht", "mich", "an", ",", "ihr", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Heimat milde Luft,", "tokens": ["Mei\u00b7ner", "Hei\u00b7mat", "mil\u00b7de", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bring' mir spielend, bring' mir linde", "tokens": ["Bring'", "mir", "spie\u00b7lend", ",", "bring'", "mir", "lin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderbaren Bl\u00fctenduft!", "tokens": ["Wun\u00b7der\u00b7ba\u00b7ren", "Bl\u00fc\u00b7ten\u00b7duft", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oeffne mir, du Starke, Sch\u00f6ne,", "tokens": ["Oeff\u00b7ne", "mir", ",", "du", "Star\u00b7ke", ",", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mailand, nun dein gastlich Thor,", "tokens": ["Mai\u00b7land", ",", "nun", "dein", "gast\u00b7lich", "Thor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Klingt ihr s\u00fc\u00dfen Liebest\u00f6ne", "tokens": ["Klingt", "ihr", "s\u00fc\u00b7\u00dfen", "Lie\u00b7be\u00b7st\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wieder in mein trunknes Ohr.", "tokens": ["Wie\u00b7der", "in", "mein", "trunk\u00b7nes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ist es nicht das Land der Wonnen,", "tokens": ["Ist", "es", "nicht", "das", "Land", "der", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Kindheit Blumenau,", "tokens": ["Mei\u00b7ner", "Kind\u00b7heit", "Blu\u00b7me\u00b7nau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"