{"dta.poem.10331": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wandelte unter den B\u00e4umen", "tokens": ["Ich", "wan\u00b7del\u00b7te", "un\u00b7ter", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit meinem Gram allein;", "tokens": ["Mit", "mei\u00b7nem", "Gram", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam das alte Tr\u00e4umen,", "tokens": ["Da", "kam", "das", "al\u00b7te", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schlich mir in's Herz hinein.", "tokens": ["Und", "schlich", "mir", "in's", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer hat Euch die\u00df W\u00f6rtlein gelehret,", "tokens": ["Wer", "hat", "Euch", "die\u00df", "W\u00f6rt\u00b7lein", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihr V\u00f6glein in luftiger H\u00f6h?", "tokens": ["Ihr", "V\u00f6\u00b7glein", "in", "luf\u00b7ti\u00b7ger", "H\u00f6h", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Schweigt still, wenn mein Herz es h\u00f6ret,", "tokens": ["Schweigt", "still", ",", "wenn", "mein", "Herz", "es", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann thut es noch einmal so weh.", "tokens": ["Dann", "thut", "es", "noch", "ein\u00b7mal", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ees kam ein Jungfr\u00e4ulein gegangen,", "tokens": ["\u201e", "es", "kam", "ein", "Jung\u00b7fr\u00e4u\u00b7lein", "ge\u00b7gan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sang es immerfort,", "tokens": ["Die", "sang", "es", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da haben wir V\u00f6glein gefangen", "tokens": ["Da", "ha\u00b7ben", "wir", "V\u00f6\u00b7glein", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das h\u00fcbsche, goldne Wort.\u201c", "tokens": ["Das", "h\u00fcb\u00b7sche", ",", "gold\u00b7ne", "Wort", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das sollt Ihr mir nicht mehr erz\u00e4hlen,", "tokens": ["Das", "sollt", "Ihr", "mir", "nicht", "mehr", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr V\u00f6glein wunderschlau;", "tokens": ["Ihr", "V\u00f6\u00b7glein", "wun\u00b7der\u00b7schlau", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wollt meinen Kummer mir stehlen,", "tokens": ["Ihr", "wollt", "mei\u00b7nen", "Kum\u00b7mer", "mir", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich aber Niemanden trau'.", "tokens": ["Ich", "a\u00b7ber", "Nie\u00b7man\u00b7den", "trau'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}