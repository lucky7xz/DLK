{"dta.poem.9874": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Allegorisch Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Amanda liebstes kind/ du brustlatz kalter hertzen/", "tokens": ["A\u00b7man\u00b7da", "liebs\u00b7tes", "kind", "/", "du", "brust\u00b7latz", "kal\u00b7ter", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der liebe feuerzeug/ goldschachtel edler zier/", "tokens": ["Der", "lie\u00b7be", "feu\u00b7er\u00b7zeug", "/", "gold\u00b7schach\u00b7tel", "ed\u00b7ler", "zier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der seuffzer blasebalg/ des traurens l\u00f6sch-papier/", "tokens": ["Der", "seuff\u00b7zer", "bla\u00b7se\u00b7balg", "/", "des", "trau\u00b7rens", "l\u00f6schpa\u00b7pier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Sandb\u00fcchse meiner pein/ und baum-\u00f6hl meiner schmertzen/", "tokens": ["Sand\u00b7b\u00fcch\u00b7se", "mei\u00b7ner", "pein", "/", "und", "baum\u00b7\u00f6hl", "mei\u00b7ner", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "KON", "NN", "PPOSAT", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Du speise meiner lust/ du flamme meiner kertzen/", "tokens": ["Du", "spei\u00b7se", "mei\u00b7ner", "lust", "/", "du", "flam\u00b7me", "mei\u00b7ner", "kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachtst\u00fclchen meiner ruh/ der Poesie clystier/", "tokens": ["Nacht\u00b7st\u00fcl\u00b7chen", "mei\u00b7ner", "ruh", "/", "der", "Poe\u00b7sie", "clys\u00b7tier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Des mundes alecant/ der augen lust-revier/", "tokens": ["Des", "mun\u00b7des", "a\u00b7le\u00b7cant", "/", "der", "au\u00b7gen", "lust\u00b7re\u00b7vier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der complementen sitz/ du meisterin zu schertzen/", "tokens": ["Der", "com\u00b7ple\u00b7men\u00b7ten", "sitz", "/", "du", "meis\u00b7te\u00b7rin", "zu", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der tugend quodlibet/ calender meiner zeit/", "tokens": ["Der", "tu\u00b7gend", "quod\u00b7li\u00b7bet", "/", "ca\u00b7len\u00b7der", "mei\u00b7ner", "zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du andachts-fackelchen/ du quell der fr\u00f6ligkeit/", "tokens": ["Du", "an\u00b7dachts\u00b7fa\u00b7ckel\u00b7chen", "/", "du", "quell", "der", "fr\u00f6\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$(", "PPER", "NN", "ART", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Du tieffer abgrund du voll tausend guter morgen/", "tokens": ["Du", "tief\u00b7fer", "ab\u00b7grund", "du", "voll", "tau\u00b7send", "gu\u00b7ter", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "PPER", "ADJD", "CARD", "ADJA", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der zungen honigseim/ des hertzens marcipan/", "tokens": ["Der", "zun\u00b7gen", "ho\u00b7ni\u00b7gseim", "/", "des", "hert\u00b7zens", "mar\u00b7ci\u00b7pan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie man sonsten dich mein kind beschreiben kan.", "tokens": ["Und", "wie", "man", "sons\u00b7ten", "dich", "mein", "kind", "be\u00b7schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lichtputze meiner noth/ und flederwisch der sorgen.", "tokens": ["Licht\u00b7put\u00b7ze", "mei\u00b7ner", "noth", "/", "und", "fle\u00b7der\u00b7wisch", "der", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}