{"dta.poem.3320": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Da auf rauschendem Gefieder", "tokens": ["Da", "auf", "rau\u00b7schen\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zephyr uns den Fr\u00fchling bringt:", "tokens": ["Ze\u00b7phyr", "uns", "den", "Fr\u00fch\u00b7ling", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So erwacht die Freude wieder;", "tokens": ["So", "er\u00b7wacht", "die", "Freu\u00b7de", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles lacht und scherzt und singt.", "tokens": ["Al\u00b7les", "lacht", "und", "scherzt", "und", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tanzt, o tanzet, junge Sch\u00f6nen!", "tokens": ["Tanzt", ",", "o", "tan\u00b7zet", ",", "jun\u00b7ge", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner sanften Leyer nach,", "tokens": ["Mei\u00b7ner", "sanf\u00b7ten", "Le\u00b7yer", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welche nie mit leichtern T\u00f6nen", "tokens": ["Wel\u00b7che", "nie", "mit", "leich\u00b7tern", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter meinen H\u00e4nden sprach.", "tokens": ["Un\u00b7ter", "mei\u00b7nen", "H\u00e4n\u00b7den", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles f\u00fchlet nun die Triebe,", "tokens": ["Al\u00b7les", "f\u00fch\u00b7let", "nun", "die", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein Herze stets verschwur:", "tokens": ["Die", "kein", "Her\u00b7ze", "stets", "ver\u00b7schwur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles ladet euch zur Liebe,", "tokens": ["Al\u00b7les", "la\u00b7det", "euch", "zur", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jugend, Fr\u00fchling und Natur.", "tokens": ["Ju\u00b7gend", ",", "Fr\u00fch\u00b7ling", "und", "Na\u00b7tur", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie bekannt wird euerm Ohre", "tokens": ["Wie", "be\u00b7kannt", "wird", "eu\u00b7erm", "Oh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun die Stimme schlauer Lust!", "tokens": ["Nun", "die", "Stim\u00b7me", "schlau\u00b7er", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wie straubt im regen Flohre", "tokens": ["Und", "wie", "straubt", "im", "re\u00b7gen", "Floh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sich die halbumflohrte Brust!", "tokens": ["Sich", "die", "hal\u00b7bum\u00b7flohr\u00b7te", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}