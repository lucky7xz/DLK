{"dta.poem.4449": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Du dachtest nicht die Thaten fremder Krieger,", "tokens": ["Du", "dach\u00b7test", "nicht", "die", "Tha\u00b7ten", "frem\u00b7der", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Heerden, die der Feind trieb von zertretner Trift,", "tokens": ["Nicht", "Heer\u00b7den", ",", "die", "der", "Feind", "trieb", "von", "zer\u00b7tret\u00b7ner", "Trift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du nanntest den Verlust ein Opfer f\u00fcr den Sieger,", "tokens": ["Du", "nann\u00b7test", "den", "Ver\u00b7lust", "ein", "Op\u00b7fer", "f\u00fcr", "den", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der alle Sieger \u00fcbertrift.", "tokens": ["Der", "al\u00b7le", "Sie\u00b7ger", "\u00fc\u00b7bert\u00b7rift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}