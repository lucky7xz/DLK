{"textgrid.poem.59576": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An den General Grafen Philipp Schwerin", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwerter sollen helfen, meinst du Stolzer?", "tokens": ["Schwer\u00b7ter", "sol\u00b7len", "hel\u00b7fen", ",", "meinst", "du", "Stol\u00b7zer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwerter? Was am Schwerte blinkt, zerhieb", "tokens": ["Schwer\u00b7ter", "?", "Was", "am", "Schwer\u00b7te", "blinkt", ",", "zer\u00b7hieb"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PWS", "APPRART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oft das Gr\u00f6\u00dfte; was nicht Tugend sch\u00e4rfte,", "tokens": ["Oft", "das", "Gr\u00f6\u00df\u00b7te", ";", "was", "nicht", "Tu\u00b7gend", "sch\u00e4rf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "PWS", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00e4ndet im Eisen.", "tokens": ["Sch\u00e4n\u00b7det", "im", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Leuen l\u00f6se, Tigerrachen \u00f6ffne!", "tokens": ["Leu\u00b7en", "l\u00f6\u00b7se", ",", "Ti\u00b7ger\u00b7ra\u00b7chen", "\u00f6ff\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00df die Schlange zischen! Schlangengift,", "tokens": ["La\u00df", "die", "Schlan\u00b7ge", "zi\u00b7schen", "!", "Schlan\u00b7gen\u00b7gift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$.", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Katzenbosheit reicht nicht an des Greuels", "tokens": ["Kat\u00b7zen\u00b7bos\u00b7heit", "reicht", "nicht", "an", "des", "Greu\u00b7els"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blutige Spitze.", "tokens": ["Blu\u00b7ti\u00b7ge", "Spit\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "O des Greuels, wann die schwarze Schande", "tokens": ["O", "des", "Greu\u00b7els", ",", "wann", "die", "schwar\u00b7ze", "Schan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihren Schmutz ergie\u00dft, da\u00df aller Klang", "tokens": ["Ih\u00b7ren", "Schmutz", "er\u00b7gie\u00dft", ",", "da\u00df", "al\u00b7ler", "Klang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hoher Worte, aller Klang des Busens", "tokens": ["Ho\u00b7her", "Wor\u00b7te", ",", "al\u00b7ler", "Klang", "des", "Bu\u00b7sens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Z\u00fcrnend verstummet!", "tokens": ["Z\u00fcr\u00b7nend", "ver\u00b7stum\u00b7met", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Schau' umher, wo findest du die M\u00e4nner,", "tokens": ["Schau'", "um\u00b7her", ",", "wo", "fin\u00b7dest", "du", "die", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die so laut, als nach der Mutter Brust", "tokens": ["Die", "so", "laut", ",", "als", "nach", "der", "Mut\u00b7ter", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schreit das Kind, die Zeit ruft? Du erblassest,", "tokens": ["Schreit", "das", "Kind", ",", "die", "Zeit", "ruft", "?", "Du", "er\u00b7blas\u00b7sest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schweigen gibt Antwort.", "tokens": ["Schwei\u00b7gen", "gibt", "Ant\u00b7wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Werde blasser noch! Wo sind die Tr\u00e4ume,", "tokens": ["Wer\u00b7de", "blas\u00b7ser", "noch", "!", "Wo", "sind", "die", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$.", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die auch dich bet\u00f6rten, dies Geschlecht", "tokens": ["Die", "auch", "dich", "be\u00b7t\u00f6r\u00b7ten", ",", "dies", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "PDS", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00f6nne frei sein, wolle frei sein, Gecken", "tokens": ["K\u00f6n\u00b7ne", "frei", "sein", ",", "wol\u00b7le", "frei", "sein", ",", "Ge\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "ADJD", "VAINF", "$,", "VMFIN", "ADJD", "VAINF", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fchlen, was Mann ist.", "tokens": ["F\u00fch\u00b7len", ",", "was", "Mann", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Ha! Du f\u00fchlst ihn, tr\u00e4gst die hohen Bilder", "tokens": ["Ha", "!", "Du", "f\u00fchlst", "ihn", ",", "tr\u00e4gst", "die", "ho\u00b7hen", "Bil\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem frischen Schritt, im frischen Ton,", "tokens": ["In", "dem", "fri\u00b7schen", "Schritt", ",", "im", "fri\u00b7schen", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In des Auges Zorn und in der Locken", "tokens": ["In", "des", "Au\u00b7ges", "Zorn", "und", "in", "der", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fliegendem Wehen.", "tokens": ["Flie\u00b7gen\u00b7dem", "We\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Auf denn! L\u00fcge wird nicht freien Br\u00fcsten,", "tokens": ["Auf", "denn", "!", "L\u00fc\u00b7ge", "wird", "nicht", "frei\u00b7en", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$.", "NN", "VAFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was die Zeit verschreit; das Deine bleibt.", "tokens": ["Was", "die", "Zeit", "ver\u00b7schreit", ";", "das", "Dei\u00b7ne", "bleibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "$.", "ART", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gottgeboren \u2013 stehe denn, ein Gleichnis", "tokens": ["Gott\u00b7ge\u00b7bo\u00b7ren", "\u2013", "ste\u00b7he", "denn", ",", "ein", "Gleich\u00b7nis"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$(", "VVFIN", "ADV", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Edlerer V\u00e4ter!", "tokens": ["Ed\u00b7le\u00b7rer", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Steh olympisch hell im Sonnenschimmer,", "tokens": ["Steh", "o\u00b7lym\u00b7pisch", "hell", "im", "Son\u00b7nen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wann es unten nachtet! Sei dir gleich!", "tokens": ["Wann", "es", "un\u00b7ten", "nach\u00b7tet", "!", "Sei", "dir", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$.", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn olympisch wird der Tag erstehen,", "tokens": ["Denn", "o\u00b7lym\u00b7pisch", "wird", "der", "Tag", "er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dessen wir hoffen.", "tokens": ["Des\u00b7sen", "wir", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Her die Hand! Und mutig la\u00df uns ringen!", "tokens": ["Her", "die", "Hand", "!", "Und", "mu\u00b7tig", "la\u00df", "uns", "rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "KON", "ADJD", "VVIMP", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gottes Krieger! Auch des Wortes Stahl", "tokens": ["Got\u00b7tes", "Krie\u00b7ger", "!", "Auch", "des", "Wor\u00b7tes", "Stahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Spr\u00fchet Blitze, hauet Schwerteshiebe", "tokens": ["Spr\u00fc\u00b7het", "Blit\u00b7ze", ",", "hau\u00b7et", "Schwer\u00b7tes\u00b7hie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schartig auf Schande.", "tokens": ["Schar\u00b7tig", "auf", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Gottes Krieger sind wir, Feuertaufe", "tokens": ["Got\u00b7tes", "Krie\u00b7ger", "sind", "wir", ",", "Feu\u00b7er\u00b7tau\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "NN", "VAFIN", "PPER", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hie\u00df der Styx, worin man uns getaucht,", "tokens": ["Hie\u00df", "der", "Styx", ",", "wo\u00b7rin", "man", "uns", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch die schwerste Arbeit zu den Sternen", "tokens": ["Durch", "die", "schwers\u00b7te", "Ar\u00b7beit", "zu", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sollen wir streben.", "tokens": ["Sol\u00b7len", "wir", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Flammen zeugen Flammen, Seelen Seelen,", "tokens": ["Flam\u00b7men", "zeu\u00b7gen", "Flam\u00b7men", ",", "See\u00b7len", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wort w\u00e4lzt Wort, und Tat treibt Tat \u2013 so braust", "tokens": ["Wort", "w\u00e4lzt", "Wort", ",", "und", "Tat", "treibt", "Tat", "\u2013", "so", "braust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "NN", "$,", "KON", "NN", "VVFIN", "NN", "$(", "ADV", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00e4nnertugend voller Strom der Zukunft", "tokens": ["M\u00e4n\u00b7ner\u00b7tu\u00b7gend", "vol\u00b7ler", "Strom", "der", "Zu\u00b7kunft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Leuchtend entgegen. ", "tokens": ["Leuch\u00b7tend", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Schwerter sollen helfen, meinst du Stolzer?", "tokens": ["Schwer\u00b7ter", "sol\u00b7len", "hel\u00b7fen", ",", "meinst", "du", "Stol\u00b7zer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwerter? Was am Schwerte blinkt, zerhieb", "tokens": ["Schwer\u00b7ter", "?", "Was", "am", "Schwer\u00b7te", "blinkt", ",", "zer\u00b7hieb"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PWS", "APPRART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oft das Gr\u00f6\u00dfte; was nicht Tugend sch\u00e4rfte,", 