{"textgrid.poem.66260": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Deutsche", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der biedre Deutsche spricht nicht viel;", "tokens": ["Der", "bied\u00b7re", "Deut\u00b7sche", "spricht", "nicht", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kurz ist sein Wort, stark sein Gef\u00fchl.", "tokens": ["Kurz", "ist", "sein", "Wort", ",", "stark", "sein", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist ein Z\u00f6gling der Natur;", "tokens": ["Er", "ist", "ein", "Z\u00f6g\u00b7ling", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Handschlag gilt ihm mehr als Schwur.", "tokens": ["Ein", "Hand\u00b7schlag", "gilt", "ihm", "mehr", "als", "Schwur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott liebt er, ist den Obern treu", "tokens": ["Gott", "liebt", "er", ",", "ist", "den", "O\u00b7bern", "treu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VAFIN", "ART", "ADJA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Gold und doch kein Sklav' dabei.", "tokens": ["Wie", "Gold", "und", "doch", "kein", "Skla\u00b7v'", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ADV", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Gerad und ehrlich ist sein Brauch.", "tokens": ["Ge\u00b7rad", "und", "ehr\u00b7lich", "ist", "sein", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So wie er spricht, so denkt er auch.", "tokens": ["So", "wie", "er", "spricht", ",", "so", "denkt", "er", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der biedre Deutsche spricht nicht viel;", "tokens": ["Der", "bied\u00b7re", "Deut\u00b7sche", "spricht", "nicht", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kurz ist sein Wort, stark sein Gef\u00fchl.", "tokens": ["Kurz", "ist", "sein", "Wort", ",", "stark", "sein", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist ein Z\u00f6gling der Natur;", "tokens": ["Er", "ist", "ein", "Z\u00f6g\u00b7ling", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "