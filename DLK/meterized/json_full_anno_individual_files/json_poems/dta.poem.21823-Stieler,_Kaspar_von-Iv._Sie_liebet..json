{"dta.poem.21823": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Sie liebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Soll denn/ salzgeschmolzne Tr\u00e4hnen/", "tokens": ["Soll", "denn", "/", "salz\u00b7ge\u00b7schmolz\u00b7ne", "Tr\u00e4h\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kummer/ Weh/ Ach/ Schmerzen/", "tokens": ["Kum\u00b7mer", "/", "Weh", "/", "Ach", "/", "Schmer\u00b7zen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "ITJ", "$(", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "ich einmahl von eurem Ende sagen?", "tokens": ["ich", "ein\u00b7mahl", "von", "eu\u00b7rem", "En\u00b7de", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Verschwindet! ich werde begl\u00fckkt", "tokens": ["Ver\u00b7schwin\u00b7det", "!", "ich", "wer\u00b7de", "be\u00b7gl\u00fckkt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "die sch\u00f6ne Melene hat nach mir geschikkt.", "tokens": ["die", "sch\u00f6\u00b7ne", "Me\u00b7le\u00b7ne", "hat", "nach", "mir", "ge\u00b7schikkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Sollte sie mich tr\u00f6sten wollen/", "tokens": ["Soll\u00b7te", "sie", "mich", "tr\u00f6s\u00b7ten", "wol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "oder wird sie mir mein Urteil sprechen:", "tokens": ["o\u00b7der", "wird", "sie", "mir", "mein", "Ur\u00b7teil", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wil sie sich mit Spotten an mir r\u00e4chen?", "tokens": ["wil", "sie", "sich", "mit", "Spot\u00b7ten", "an", "mir", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Verschwindet Gedanken! sie liebt/", "tokens": ["Ver\u00b7schwin\u00b7det", "Ge\u00b7dan\u00b7ken", "!", "sie", "liebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "PPER", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Manchen Tag hab\u2019 ich geweinet/", "tokens": ["Man\u00b7chen", "Tag", "hab'", "ich", "ge\u00b7wei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn ihr nicht mein Leiden gieng zu Herze\u0303/", "tokens": ["wenn", "ihr", "nicht", "mein", "Lei\u00b7den", "gieng", "zu", "Her\u00b7z\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "APPR", "NE", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wenn ihr Sinn so war versteinet/", "tokens": ["wenn", "ihr", "Sinn", "so", "war", "ver\u00b7stei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df sie nicht gedacht\u2019 an meine Schmerzen:", "tokens": ["da\u00df", "sie", "nicht", "ge\u00b7dacht'", "an", "mei\u00b7ne", "Schmer\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nu ist es verkehret. Sie spricht", "tokens": ["Nu", "ist", "es", "ver\u00b7keh\u00b7ret", ".", "Sie", "spricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Soll ich mich darauff verlassen/", "tokens": ["Soll", "ich", "mich", "dar\u00b7auff", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PAV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "oder spielt sie nur mit falschen Worten?", "tokens": ["o\u00b7der", "spielt", "sie", "nur", "mit", "fal\u00b7schen", "Wor\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! so wolt\u2019 ich lieber blassen/", "tokens": ["Ach", "!", "so", "wolt'", "ich", "lie\u00b7ber", "blas\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "eh\u2019 ich k\u00e4hm\u2019 an ihres Zimmers Pforten:", "tokens": ["eh'", "ich", "k\u00e4hm'", "an", "ih\u00b7res", "Zim\u00b7mers", "Pfor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nein. Edeler Wahrheit Bestand", "tokens": ["Nein", ".", "E\u00b7de\u00b7ler", "Wahr\u00b7heit", "Be\u00b7stand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ADJA", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "bekr\u00f6nt sie und beut mir die liebende Hand.", "tokens": ["be\u00b7kr\u00f6nt", "sie", "und", "beut", "mir", "die", "lie\u00b7ben\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Nun fahr hin/ Mord-bringend Leiden/", "tokens": ["Nun", "fahr", "hin", "/", "Mord\u00b7brin\u00b7gend", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$(", "NE", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Furcht/ Bek\u00fcmmern/ Zweifelzagen/ wei-", "tokens": ["Furcht", "/", "Be\u00b7k\u00fcm\u00b7mern", "/", "Zwei\u00b7fel\u00b7za\u00b7gen", "/", "wei"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "hin! ihr st\u00f6rer meiner Freuden!", "tokens": ["hin", "!", "ihr", "st\u00f6\u00b7rer", "mei\u00b7ner", "Freu\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "forthin hat mein Antliz au\u00dfgebleichet.", "tokens": ["for\u00b7thin", "hat", "mein", "Ant\u00b7liz", "au\u00df\u00b7ge\u00b7blei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bekr\u00e4nzt mich und ruffet: Gl\u00fckk zu!", "tokens": ["Be\u00b7kr\u00e4nzt", "mich", "und", "ruf\u00b7fet", ":", "Gl\u00fckk", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$.", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "der St\u00fcrm ist vor\u00fcber/ mir bleibet die Ruh.", "tokens": ["der", "St\u00fcrm", "ist", "vor\u00b7\u00fc\u00b7ber", "/", "mir", "blei\u00b7bet", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}