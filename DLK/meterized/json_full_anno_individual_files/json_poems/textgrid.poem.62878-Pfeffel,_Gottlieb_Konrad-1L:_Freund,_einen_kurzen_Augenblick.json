etra"}, "line.63": {"text": "O da\u00df ich dich erschaffen m\u00f6chte,", "tokens": ["O", "da\u00df", "ich", "dich", "er\u00b7schaf\u00b7fen", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.64": {"text": "Geweyhter Feen-Aufenthalt,", "tokens": ["Ge\u00b7weyh\u00b7ter", "Feen\u00b7Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.65": {"text": "Du Schinznach f\u00fcr die zwey Geschlechte!", "tokens": ["Du", "Schinz\u00b7nach", "f\u00fcr", "die", "zwey", "Ge\u00b7schlech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Doch wenn wir dieses Heiligthum", "tokens": ["Doch", "wenn", "wir", "die\u00b7ses", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Entz\u00fcckungsvoller Sympathien", "tokens": ["Ent\u00b7z\u00fc\u00b7ckungs\u00b7vol\u00b7ler", "Sym\u00b7pa\u00b7thi\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.68": {"text": "Hienieden, Freunde, nicht beziehen,", "tokens": ["Hien\u00b7ie\u00b7den", ",", "Freun\u00b7de", ",", "nicht", "be\u00b7zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.69": {"text": "So giebt es ein Elysium.", "tokens": ["So", "giebt", "es", "ein", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word",