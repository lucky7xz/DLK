{"textgrid.poem.62878": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, einen kurzen Augenblick", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, einen kurzen Augenblick", "tokens": ["Freund", ",", "ei\u00b7nen", "kur\u00b7zen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War ich bey dir: doch wie viel Gl\u00fcck", "tokens": ["War", "ich", "bey", "dir", ":", "doch", "wie", "viel", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$.", "ADV", "KOKOM", "PIAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Lag nicht in diesem Augenblick!", "tokens": ["Lag", "nicht", "in", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freundschaft z\u00e4hlt nicht so die Stunden,", "tokens": ["Die", "Freund\u00b7schaft", "z\u00e4hlt", "nicht", "so", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Adam Riese z\u00e4hlen lehrt,", "tokens": ["Wie", "A\u00b7dam", "Rie\u00b7se", "z\u00e4h\u00b7len", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An ihrem Busen durchempfunden", "tokens": ["An", "ih\u00b7rem", "Bu\u00b7sen", "dur\u00b7ch\u00b7em\u00b7pfun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Ist eine ganze Seklen werth;", "tokens": ["Ist", "ei\u00b7ne", "gan\u00b7ze", "Se\u00b7klen", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Unmerkbar lang, gleich den Aeonen,", "tokens": ["Un\u00b7merk\u00b7bar", "lang", ",", "gleich", "den", "A\u00b7e\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Die der entz\u00fcckte Mahomet", "tokens": ["Die", "der", "ent\u00b7z\u00fcck\u00b7te", "Ma\u00b7ho\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Einst in des Weltbaus obern Zonen", "tokens": ["Einst", "in", "des", "Welt\u00b7baus", "o\u00b7bern", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Am Thron der h\u00f6chsten Majest\u00e4t", "tokens": ["Am", "Thron", "der", "h\u00f6chs\u00b7ten", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Durchlebet, oder doch gew\u00e4hnet", "tokens": ["Durch\u00b7le\u00b7bet", ",", "o\u00b7der", "doch", "ge\u00b7w\u00e4h\u00b7net"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Durchlebt zu haben. Gabriel", "tokens": ["Durch\u00b7lebt", "zu", "ha\u00b7ben", ".", "Ga\u00b7bri\u00b7el"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVPP", "PTKZU", "VAINF", "$.", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Trug ihn zum Ahnherrn Ismael", "tokens": ["Trug", "ihn", "zum", "Ahn\u00b7herrn", "Is\u00b7mael"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Dahin, wo Gott die Tugend kr\u00f6net.", "tokens": ["Da\u00b7hin", ",", "wo", "Gott", "die", "Tu\u00b7gend", "kr\u00f6\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Er sah, was noch kein Auge sah,", "tokens": ["Er", "sah", ",", "was", "noch", "kein", "Au\u00b7ge", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und h\u00f6rte, was kein Ohr geh\u00f6ret,", "tokens": ["Und", "h\u00f6r\u00b7te", ",", "was", "kein", "Ohr", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und als er wieder umgekehret", "tokens": ["Und", "als", "er", "wie\u00b7der", "um\u00b7ge\u00b7keh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Auf seinen Sopha; siehe da!", "tokens": ["Auf", "sei\u00b7nen", "So\u00b7pha", ";", "sie\u00b7he", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVIMP", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "War er bey zwanzigtausend Jahren", "tokens": ["War", "er", "bey", "zwan\u00b7zig\u00b7tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Von Haus, und fand noch Zeit genug", "tokens": ["Von", "Haus", ",", "und", "fand", "noch", "Zeit", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "ADV", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Den angef\u00fcllten Wasserkrug,", "tokens": ["Den", "an\u00b7ge\u00b7f\u00fcll\u00b7ten", "Was\u00b7ser\u00b7krug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Den, als er zum Olymp gefahren,", "tokens": ["Den", ",", "als", "er", "zum", "O\u00b7lymp", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "Sein Fu\u00df vom Tisch herunter stie\u00df,", "tokens": ["Sein", "Fu\u00df", "vom", "Tisch", "her\u00b7un\u00b7ter", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Vor seinem Falle zu bewahren.", "tokens": ["Vor", "sei\u00b7nem", "Fal\u00b7le", "zu", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So zeitvoll sind im Paradies,", "tokens": ["So", "zeit\u00b7voll", "sind", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Am Quell des Sch\u00f6nen und des Guten,", "tokens": ["Am", "Quell", "des", "Sch\u00f6\u00b7nen", "und", "des", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und diesseits der Gestirne mir", "tokens": ["Und", "dies\u00b7seits", "der", "Ge\u00b7stir\u00b7ne", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Bey deiner Gattin und bey dir,", "tokens": ["Bey", "dei\u00b7ner", "Gat\u00b7tin", "und", "bey", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "PPER", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "O Freund, die fl\u00fcchtigen Minuten.", "tokens": ["O", "Freund", ",", "die", "fl\u00fcch\u00b7ti\u00b7gen", "Mi\u00b7nu\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Wenn dieses Bl\u00e4ttchen einer liest,", "tokens": ["Wenn", "die\u00b7ses", "Bl\u00e4tt\u00b7chen", "ei\u00b7ner", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "F\u00fcr den es nicht geschrieben ist,", "tokens": ["F\u00fcr", "den", "es", "nicht", "ge\u00b7schrie\u00b7ben", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wie wird er des Phantasten lachen?", "tokens": ["Wie", "wird", "er", "des", "Phan\u00b7tas\u00b7ten", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Das mag er! Soll ich ihm daf\u00fcr", "tokens": ["Das", "mag", "er", "!", "Soll", "ich", "ihm", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "$.", "VMFIN", "PPER", "PPER", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Noch eine saure Miene machen,", "tokens": ["Noch", "ei\u00b7ne", "sau\u00b7re", "Mie\u00b7ne", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Da\u00df er ein Gl\u00fcck verkennt, das wir", "tokens": ["Da\u00df", "er", "ein", "Gl\u00fcck", "ver\u00b7kennt", ",", "das", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Mehr als das ganze Pohlen sch\u00e4tzen,", "tokens": ["Mehr", "als", "das", "gan\u00b7ze", "Poh\u00b7len", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Um dessen letzten Scheidestrich", "tokens": ["Um", "des\u00b7sen", "letz\u00b7ten", "Schei\u00b7de\u00b7strich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Therese, K\u00e4the, Friederich", "tokens": ["The\u00b7re\u00b7se", ",", "K\u00e4\u00b7the", ",", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.40": {"text": "Die Federn und die Lanzen wetzen?", "tokens": ["Die", "Fe\u00b7dern", "und", "die", "Lan\u00b7zen", "wet\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Das Herz bestimmt der Dinge Werth,", "tokens": ["Das", "Herz", "be\u00b7stimmt", "der", "Din\u00b7ge", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "An ihm schleift jeder seine Brille.", "tokens": ["An", "ihm", "schleift", "je\u00b7der", "sei\u00b7ne", "Bril\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Ist Sympathie ein Steckenpferd,", "tokens": ["Ist", "Sym\u00b7pa\u00b7thie", "ein", "Ste\u00b7cken\u00b7pferd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "So sey sie meine Lieblingsgrille.", "tokens": ["So", "sey", "sie", "mei\u00b7ne", "Lieb\u00b7lings\u00b7gril\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "O, Heil mir, Freunde, da\u00df auch Ihr", "tokens": ["O", ",", "Heil", "mir", ",", "Freun\u00b7de", ",", "da\u00df", "auch", "Ihr"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "PPER", "$,", "NN", "$,", "KOUS", "ADV", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Das mystische Concert verstehet,", "tokens": ["Das", "mys\u00b7ti\u00b7sche", "Con\u00b7cert", "ver\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Bey dessen Symphonien mir", "tokens": ["Bey", "des\u00b7sen", "Sym\u00b7pho\u00b7ni\u00b7en", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Die kurze Zeit so sch\u00f6n vergehet!", "tokens": ["Die", "kur\u00b7ze", "Zeit", "so", "sch\u00f6n", "ver\u00b7ge\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Doch h\u00e4tt ich Euch wohl ausgesp\u00e4het?", "tokens": ["Doch", "h\u00e4tt", "ich", "Euch", "wohl", "aus\u00b7ge\u00b7sp\u00e4\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "H\u00e4tt Euch so schnell mein Herz gew\u00e4hlt?", "tokens": ["H\u00e4tt", "Euch", "so", "schnell", "mein", "Herz", "ge\u00b7w\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "W\u00e4r es von dem, der uns beseelt,", "tokens": ["W\u00e4r", "es", "von", "dem", ",", "der", "uns", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Nicht in die himmlischen Accorden", "tokens": ["Nicht", "in", "die", "himm\u00b7li\u00b7schen", "Ac\u00b7cor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Der Sympathie gestimmet worden.", "tokens": ["Der", "Sym\u00b7pa\u00b7thie", "ge\u00b7stim\u00b7met", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Ja, Freund, ja holde Zoe, du,", "tokens": ["Ja", ",", "Freund", ",", "ja", "hol\u00b7de", "Zoe", ",", "du", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ADV", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Die ich voll Stolzes Freundin nenne", "tokens": ["Die", "ich", "voll", "Stol\u00b7zes", "Freun\u00b7din", "nen\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.56": {"text": "Und, schlo\u00df gleich Gott mein Auge zu,", "tokens": ["Und", ",", "schlo\u00df", "gleich", "Gott", "mein", "Au\u00b7ge", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Mit allen ihren Reitzen kenne,", "tokens": ["Mit", "al\u00b7len", "ih\u00b7ren", "Reit\u00b7zen", "ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Auch Euch weis' ich ein H\u00fcttchen an", "tokens": ["Auch", "Euch", "weis'", "ich", "ein", "H\u00fctt\u00b7chen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Auf meiner Hesperiden-Insel,", "tokens": ["Auf", "mei\u00b7ner", "Hes\u00b7pe\u00b7ri\u00b7den\u00b7In\u00b7sel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Die sich, nach Platons frommem Plan,", "tokens": ["Die", "sich", ",", "nach", "Pla\u00b7tons", "from\u00b7mem", "Plan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.61": {"text": "Mein k\u00fchner schw\u00e4rmerischer Pinsel", "tokens": ["Mein", "k\u00fch\u00b7ner", "schw\u00e4r\u00b7me\u00b7ri\u00b7scher", "Pin\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.62": {"text": "F\u00fcr Biederseelen ausgemahlt.", "tokens": ["F\u00fcr", "Bie\u00b7der\u00b7see\u00b7len", "aus\u00b7ge\u00b7mahlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "O da\u00df ich dich erschaffen m\u00f6chte,", "tokens": ["O", "da\u00df", "ich", "dich", "er\u00b7schaf\u00b7fen", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.64": {"text": "Geweyhter Feen-Aufenthalt,", "tokens": ["Ge\u00b7weyh\u00b7ter", "Feen\u00b7Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.65": {"text": "Du Schinznach f\u00fcr die zwey Geschlechte!", "tokens": ["Du", "Schinz\u00b7nach", "f\u00fcr", "die", "zwey", "Ge\u00b7schlech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.66": {"text": "Doch wenn wir dieses Heiligthum", "tokens": ["Doch", "wenn", "wir", "die\u00b7ses", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Entz\u00fcckungsvoller Sympathien", "tokens": ["Ent\u00b7z\u00fc\u00b7ckungs\u00b7vol\u00b7ler", "Sym\u00b7pa\u00b7thi\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.68": {"text": "Hienieden, Freunde, nicht beziehen,", "tokens": ["Hien\u00b7ie\u00b7den", ",", "Freun\u00b7de", ",", "nicht", "be\u00b7zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.69": {"text": "So giebt es ein Elysium.", "tokens": ["So", "giebt", "es", "ein", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund, einen kurzen Augenblick", "tokens": ["Freund", ",", "ei\u00b7nen", "kur\u00b7zen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War ich bey dir: doch wie viel Gl\u00fcck", "tokens": ["War", "ich", "bey", "dir", ":", "doch", "wie", "viel", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$.", "ADV", "KOKOM", "PIAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Lag nicht in diesem Augenblick!", "tokens": ["Lag", "nicht", "in", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freundschaft z\u00e4hlt nicht so die Stunden,", "tokens": ["Die", "Freund\u00b7schaft", "z\u00e4hlt", "nicht", "so", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Adam Riese z\u00e4hlen lehrt,", "tokens": ["Wie", "A\u00b7dam", "Rie\u00b7se", "z\u00e4h\u00b7len", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An ihrem Busen durchempfunden", "tokens": ["An", "ih\u00b7rem", "Bu\u00b7sen", "dur\u00b7ch\u00b7em\u00b7pfun\u00b7den"]