ct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Still, wie der Balsam fleu\u00dft;", "tokens": ["Still", ",", "wie", "der", "Bal\u00b7sam", "fleu\u00dft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hell, wie Sommertage,", "tokens": ["Und", "hell", ",", "wie", "Som\u00b7mer\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So helle war sein Geist.", "tokens": ["So", "hel\u00b7le", "war", "sein", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Er soll den Tod nicht sehen,", "tokens": ["Er", "soll", "den", "Tod", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gotterf\u00fcllte Mann,", "tokens": ["Der", "got\u00b7ter\u00b7f\u00fcll\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis er von jenen H\u00f6hen", "tokens": ["Bis", "er", "von", "je\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Mittler sehen kann.", "tokens": ["Den", "Mitt\u00b7ler", "se\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Er sieht ihn! mit Entz\u00fccken", "tokens": ["Er", "sieht", "ihn", "!", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dr\u00fcckt er ihn an die Brust;", "tokens": ["Dr\u00fcckt", "er", "ihn", "an", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herauf von Jesu Blicken", "tokens": ["Her\u00b7auf", "von", "Je\u00b7su", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Flimmt ihm des Himmels Lust.", "tokens": ["Flimmt", "ihm", "des", "Him\u00b7mels", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Seht nun den frommen Alten", "tokens": ["Seht", "nun", "den", "from\u00b7men", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit flammendem Gesicht", "tokens": ["Mit", "flam\u00b7men\u00b7dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die welken H\u00e4nde falten,", "tokens": ["Die", "wel\u00b7ken", "H\u00e4n\u00b7de", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00f6ret, was er spricht:", "tokens": ["Und", "h\u00f6\u00b7ret", ",", "was", "er", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Mit silbergrauen Haaren", "tokens": ["Mit", "sil\u00b7ber\u00b7grau\u00b7en", "Haa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann ich im Frieden nun", "tokens": ["Kann", "ich", "im", "Frie\u00b7den", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu meinen V\u00e4tern fahren,", "tokens": ["Zu", "mei\u00b7nen", "V\u00e4\u00b7tern", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um sanft, wie sie, zu ruhn.", "tokens": ["Um", "sanft", ",", "wie", "sie", ",", "zu", "ruhn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "$,", "PWAV", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Die F\u00fclle meiner Freuden,", "tokens": ["Die", "F\u00fcl\u00b7le", "mei\u00b7ner", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die H\u00fclfe aus den H\u00f6hn,", "tokens": ["Die", "H\u00fcl\u00b7fe", "aus", "den", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Licht