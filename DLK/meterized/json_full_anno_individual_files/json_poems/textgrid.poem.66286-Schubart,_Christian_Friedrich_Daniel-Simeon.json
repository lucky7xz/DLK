measure": "iambic.tri"}, "line.2": {"text": "Gekr\u00fcmmt von Harm und Noth,", "tokens": ["Ge\u00b7kr\u00fcmmt", "von", "Harm", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu meinen V\u00e4tern fahren:", "tokens": ["Zu", "mei\u00b7nen", "V\u00e4\u00b7tern", "fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sterb' ich seinen Tod.", "tokens": ["So", "sterb'", "ich", "sei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Zwar werd' ich Ihn nicht sehen", "tokens": ["Zwar", "werd'", "ich", "Ihn", "nicht", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch hier, wie Simeon;", "tokens": ["Noch", "hier", ",", "wie", "Si\u00b7me\u00b7on", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch \u00fcber jenen H\u00f6hen", "tokens": ["Doch", "\u00fc\u00b7ber", "je\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwartet Er mich schon.", "tokens": ["Er\u00b7war\u00b7tet", "Er", "mich", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Dann 