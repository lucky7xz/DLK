{"textgrid.poem.66286": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Simeon", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt heut an eurem Stabe,", "tokens": ["Kommt", "heut", "an", "eu\u00b7rem", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Halbverweste schon,", "tokens": ["Ihr", "Halb\u00b7ver\u00b7wes\u00b7te", "schon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und denkt am nahen Grabe", "tokens": ["Und", "denkt", "am", "na\u00b7hen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An einen Simeon.", "tokens": ["An", "ei\u00b7nen", "Si\u00b7me\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er betet in dem Tempel", "tokens": ["Er", "be\u00b7tet", "in", "dem", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinauf zu seinem Gott,", "tokens": ["Hin\u00b7auf", "zu", "sei\u00b7nem", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wird uns ein Exempel", "tokens": ["Und", "wird", "uns", "ein", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von einem sch\u00f6nen Tod.", "tokens": ["Von", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Still flo\u00dfen seine Tage,", "tokens": ["Still", "flo\u00b7\u00dfen", "sei\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Still, wie der Balsam fleu\u00dft;", "tokens": ["Still", ",", "wie", "der", "Bal\u00b7sam", "fleu\u00dft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hell, wie Sommertage,", "tokens": ["Und", "hell", ",", "wie", "Som\u00b7mer\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So helle war sein Geist.", "tokens": ["So", "hel\u00b7le", "war", "sein", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er soll den Tod nicht sehen,", "tokens": ["Er", "soll", "den", "Tod", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gotterf\u00fcllte Mann,", "tokens": ["Der", "got\u00b7ter\u00b7f\u00fcll\u00b7te", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis er von jenen H\u00f6hen", "tokens": ["Bis", "er", "von", "je\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Mittler sehen kann.", "tokens": ["Den", "Mitt\u00b7ler", "se\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er sieht ihn! mit Entz\u00fccken", "tokens": ["Er", "sieht", "ihn", "!", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dr\u00fcckt er ihn an die Brust;", "tokens": ["Dr\u00fcckt", "er", "ihn", "an", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herauf von Jesu Blicken", "tokens": ["Her\u00b7auf", "von", "Je\u00b7su", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Flimmt ihm des Himmels Lust.", "tokens": ["Flimmt", "ihm", "des", "Him\u00b7mels", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Seht nun den frommen Alten", "tokens": ["Seht", "nun", "den", "from\u00b7men", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit flammendem Gesicht", "tokens": ["Mit", "flam\u00b7men\u00b7dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die welken H\u00e4nde falten,", "tokens": ["Die", "wel\u00b7ken", "H\u00e4n\u00b7de", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00f6ret, was er spricht:", "tokens": ["Und", "h\u00f6\u00b7ret", ",", "was", "er", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit silbergrauen Haaren", "tokens": ["Mit", "sil\u00b7ber\u00b7grau\u00b7en", "Haa\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann ich im Frieden nun", "tokens": ["Kann", "ich", "im", "Frie\u00b7den", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu meinen V\u00e4tern fahren,", "tokens": ["Zu", "mei\u00b7nen", "V\u00e4\u00b7tern", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um sanft, wie sie, zu ruhn.", "tokens": ["Um", "sanft", ",", "wie", "sie", ",", "zu", "ruhn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "$,", "PWAV", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die F\u00fclle meiner Freuden,", "tokens": ["Die", "F\u00fcl\u00b7le", "mei\u00b7ner", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die H\u00fclfe aus den H\u00f6hn,", "tokens": ["Die", "H\u00fcl\u00b7fe", "aus", "den", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Licht der blinden Heiden,", "tokens": ["Das", "Licht", "der", "blin\u00b7den", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Trost hab' ich gesehn.", "tokens": ["Den", "Trost", "hab'", "ich", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Nun wird sein Glaube gr\u00f6\u00dfer", "tokens": ["Nun", "wird", "sein", "Glau\u00b7be", "gr\u00f6\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sein Entz\u00fccken steigt;", "tokens": ["Und", "sein", "Ent\u00b7z\u00fc\u00b7cken", "steigt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun dr\u00fcckt er den Erl\u00f6ser", "tokens": ["Nun", "dr\u00fcckt", "er", "den", "Er\u00b7l\u00f6\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fest an sein Herz und schweigt.", "tokens": ["Fest", "an", "sein", "Herz", "und", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Herr, soll ich alt an Jahren,", "tokens": ["Herr", ",", "soll", "ich", "alt", "an", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gekr\u00fcmmt von Harm und Noth,", "tokens": ["Ge\u00b7kr\u00fcmmt", "von", "Harm", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu meinen V\u00e4tern fahren:", "tokens": ["Zu", "mei\u00b7nen", "V\u00e4\u00b7tern", "fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sterb' ich seinen Tod.", "tokens": ["So", "sterb'", "ich", "sei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zwar werd' ich Ihn nicht sehen", "tokens": ["Zwar", "werd'", "ich", "Ihn", "nicht", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch hier, wie Simeon;", "tokens": ["Noch", "hier", ",", "wie", "Si\u00b7me\u00b7on", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch \u00fcber jenen H\u00f6hen", "tokens": ["Doch", "\u00fc\u00b7ber", "je\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwartet Er mich schon.", "tokens": ["Er\u00b7war\u00b7tet", "Er", "mich", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann t\u00f6nen meine Lieder:", "tokens": ["Dann", "t\u00f6\u00b7nen", "mei\u00b7ne", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heil mir! nun seh' ich Ihn!", "tokens": ["Heil", "mir", "!", "nun", "seh'", "ich", "Ihn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Himmel hallen wieder!", "tokens": ["Die", "Him\u00b7mel", "hal\u00b7len", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heil dir! Nun siehst du Ihn.", "tokens": ["Heil", "dir", "!", "Nun", "siehst", "du", "Ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Kommt heut an eurem Stabe,", "tokens": ["Kommt", "heut", "an", "eu\u00b7rem", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iam