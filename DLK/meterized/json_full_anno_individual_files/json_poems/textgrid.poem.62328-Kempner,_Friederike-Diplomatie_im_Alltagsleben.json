unct"], "pos": ["KON", "ART", "PIS", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob er sich was n\u00fctzen kann.", "tokens": ["Ob", "er", "sich", "was", "n\u00fct\u00b7zen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Also \u00fcbt man den Verstand", "tokens": ["Al\u00b7so", "\u00fcbt", "man", "den", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und wird Selbstsucht-Spekulant.", "tokens": ["Und", "wird", "Selbst\u00b7sucht\u00b7Spe\u00b7ku\u00b7lant", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NE", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Das ist Wahrheit, urgemein.", "tokens": ["Das", "ist", "Wahr\u00b7heit", ",", "ur\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Sch\u00e4mt euch doch, ein Mensch zu sein.", "tokens": ["Sch\u00e4mt", "euch", "doch", ",", "ein", "Mensch", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6ret, was das Neueste ist:", "tokens": ["H\u00f6\u00b7ret", ",", "was", "das", "Neu\u00b7es\u00b7te", "ist", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ob m