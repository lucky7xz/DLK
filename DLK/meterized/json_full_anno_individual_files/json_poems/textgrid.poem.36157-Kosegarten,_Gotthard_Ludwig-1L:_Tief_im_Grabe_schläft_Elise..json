{"textgrid.poem.36157": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief im Grabe schl\u00e4ft Elise.", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Grabe schl\u00e4ft Elise.", "tokens": ["Tief", "im", "Gra\u00b7be", "schl\u00e4ft", "E\u00b7li\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der lindre Sonnenstrahl,", "tokens": ["Und", "der", "lind\u00b7re", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und das junge Gr\u00fcn der Wiese,", "tokens": ["Und", "das", "jun\u00b7ge", "Gr\u00fcn", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Schlag der Nachtigall,", "tokens": ["Und", "der", "Schlag", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wecken ihr nicht Lust noch Wonne.", "tokens": ["We\u00b7cken", "ihr", "nicht", "Lust", "noch", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! in ihrer k\u00fchlen Gruft", "tokens": ["Ach", "!", "in", "ih\u00b7rer", "k\u00fch\u00b7len", "Gruft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weht kein s\u00fc\u00dfer Bl\u00fcthenduft,", "tokens": ["Weht", "kein", "s\u00fc\u00b7\u00dfer", "Bl\u00fc\u00b7then\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fruchtet keine goldne Sonne.", "tokens": ["Fruch\u00b7tet", "kei\u00b7ne", "gold\u00b7ne", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! Was frommt uns Fr\u00fchlingsmilde,", "tokens": ["Ach", "!", "Was", "frommt", "uns", "Fr\u00fch\u00b7lings\u00b7mil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Sch\u00f6pfung junge Pracht?", "tokens": ["Und", "der", "Sch\u00f6p\u00b7fung", "jun\u00b7ge", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem hellen Lenzgefilde", "tokens": ["In", "dem", "hel\u00b7len", "Lenz\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallen wir in Trauertracht,", "tokens": ["Wal\u00b7len", "wir", "in", "Trau\u00b7er\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Danken nicht f\u00fcr deine Gabe,", "tokens": ["Dan\u00b7ken", "nicht", "f\u00fcr", "dei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lenz, f\u00fcr deine Freude dir!", "tokens": ["Lenz", ",", "f\u00fcr", "dei\u00b7ne", "Freu\u00b7de", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn Elisa ist nicht hier.", "tokens": ["Denn", "E\u00b7li\u00b7sa", "ist", "nicht", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach! Elisa schl\u00e4ft im Grabe.", "tokens": ["Ach", "!", "E\u00b7li\u00b7sa", "schl\u00e4ft", "im", "Gra\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "War sie nicht so fromm, so weise?", "tokens": ["War", "sie", "nicht", "so", "fromm", ",", "so", "wei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War gef\u00e4llig, mild' und gut!", "tokens": ["War", "ge\u00b7f\u00e4l\u00b7lig", ",", "mild'", "und", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War auf ihrer Lebensreise", "tokens": ["War", "auf", "ih\u00b7rer", "Le\u00b7bens\u00b7rei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer frei und froh von Muth!", "tokens": ["Im\u00b7mer", "frei", "und", "froh", "von", "Muth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! sie war uns Stolz und Freude!", "tokens": ["Ach", "!", "sie", "war", "uns", "Stolz", "und", "Freu\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! sie war uns Licht und Rath!", "tokens": ["Ach", "!", "sie", "war", "uns", "Licht", "und", "Rath", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unverzagt in Edelthat!", "tokens": ["Un\u00b7ver\u00b7zagt", "in", "E\u00b7delt\u00b7hat", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tr\u00f6stend und getrost im Leide.", "tokens": ["Tr\u00f6s\u00b7tend", "und", "ge\u00b7trost", "im", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O wie manche Wonnestunden", "tokens": ["O", "wie", "man\u00b7che", "Won\u00b7nes\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben wir mit ihr verscherzt!", "tokens": ["Ha\u00b7ben", "wir", "mit", "ihr", "ver\u00b7scherzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie manche herbe Wunden", "tokens": ["O", "wie", "man\u00b7che", "her\u00b7be", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben wir mit ihr verscherzt!", "tokens": ["Ha\u00b7ben", "wir", "mit", "ihr", "ver\u00b7scherzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Liebe, ihre Treue", "tokens": ["Ih\u00b7re", "Lie\u00b7be", ",", "ih\u00b7re", "Treu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lindert' alles Herzeleid,", "tokens": ["Lin\u00b7dert'", "al\u00b7les", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Strahlte frohe Heiterkeit", "tokens": ["Strahl\u00b7te", "fro\u00b7he", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ueber unsre kleine Reihe!", "tokens": ["Ue\u00b7ber", "uns\u00b7re", "klei\u00b7ne", "Rei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Abends in der Gei\u00dfblattlaube", "tokens": ["A\u00b7bends", "in", "der", "Gei\u00df\u00b7blatt\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sa\u00dfen wir im K\u00fchlgel\u00fcft,", "tokens": ["Sa\u00b7\u00dfen", "wir", "im", "K\u00fchl\u00b7ge\u00b7l\u00fcft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kosteten die goldne Traube,", "tokens": ["Kos\u00b7te\u00b7ten", "die", "gold\u00b7ne", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Athmeten das Thauged\u00fcft.", "tokens": ["Ath\u00b7me\u00b7ten", "das", "Thau\u00b7ge\u00b7d\u00fcft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "K\u00fcnftig sollst du, Laube, trauern,", "tokens": ["K\u00fcnf\u00b7tig", "sollst", "du", ",", "Lau\u00b7be", ",", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "$,", "NN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn Elisa ist nicht hier.", "tokens": ["Denn", "E\u00b7li\u00b7sa", "ist", "nicht", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach! Elisa kehrt zu dir", "tokens": ["Ach", "!", "E\u00b7li\u00b7sa", "kehrt", "zu", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "NE", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimmer aus des Grabes Schauern.", "tokens": ["Nim\u00b7mer", "aus", "des", "Gra\u00b7bes", "Schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00fcnftig wird sie dich nicht pflegen,", "tokens": ["K\u00fcnf\u00b7tig", "wird", "sie", "dich", "nicht", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O, du duftig Blumrevier!", "tokens": ["O", ",", "du", "duf\u00b7tig", "Blum\u00b7re\u00b7vier", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wartung, Wahrung, Pfleg' und Segen", "tokens": ["War\u00b7tung", ",", "Wah\u00b7rung", ",", "Pfleg'", "und", "Se\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmst du f\u00f6rder nicht von ihr.", "tokens": ["Nimmst", "du", "f\u00f6r\u00b7der", "nicht", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Helle Rosen, sanfte Nelken,", "tokens": ["Hel\u00b7le", "Ro\u00b7sen", ",", "sanf\u00b7te", "Nel\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Myrte, Raute, Rosmarin,", "tokens": ["Myr\u00b7te", ",", "Rau\u00b7te", ",", "Ros\u00b7ma\u00b7rin", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Welket alle, welkt nur hin \u2013", "tokens": ["Wel\u00b7ket", "al\u00b7le", ",", "welkt", "nur", "hin", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dfte doch Elisa welken!", "tokens": ["Mu\u00df\u00b7te", "doch", "E\u00b7li\u00b7sa", "wel\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "All' die Freuden vor'ger Jahre", "tokens": ["All'", "die", "Freu\u00b7den", "vor'\u00b7ger", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flohn mit ihr die Gruft hinab;", "tokens": ["Flohn", "mit", "ihr", "die", "Gruft", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Starrten mit ihr auf der Bahre,", "tokens": ["Starr\u00b7ten", "mit", "ihr", "auf", "der", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanken mit ihr tief ins Grab!", "tokens": ["San\u00b7ken", "mit", "ihr", "tief", "ins", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von uns ist sie, von uns gangen,", "tokens": ["Von", "uns", "ist", "sie", ",", "von", "uns", "gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Traute, Freundliche,", "tokens": ["Uns\u00b7re", "Trau\u00b7te", ",", "Freund\u00b7li\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Achtet nicht auf unser Weh,", "tokens": ["Ach\u00b7tet", "nicht", "auf", "un\u00b7ser", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wei\u00df nicht unser hei\u00df Verlangen.", "tokens": ["Wei\u00df", "nicht", "un\u00b7ser", "hei\u00df", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Flie\u00dft ihr, flie\u00dft ihr, stille Thr\u00e4nen,", "tokens": ["Flie\u00dft", "ihr", ",", "flie\u00dft", "ihr", ",", "stil\u00b7le", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der goldne Tag erwacht.", "tokens": ["Wenn", "der", "gold\u00b7ne", "Tag", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruf' ihr, unser banges Sehnen,", "tokens": ["Ruf