{"dta.poem.11933": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar viele Geister gehn beim Menschen aus und ein,", "tokens": ["Gar", "vie\u00b7le", "Geis\u00b7ter", "gehn", "beim", "Men\u00b7schen", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selber wei\u00df er nicht, ob b\u00f6s' ob gut sie seyn.", "tokens": ["Und", "sel\u00b7ber", "wei\u00df", "er", "nicht", ",", "ob", "b\u00f6s'", "ob", "gut", "sie", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADJD", "KOUS", "ADJD", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er merkt es nicht, bis sie zuletzt sich selbst verrathen,", "tokens": ["Er", "merkt", "es", "nicht", ",", "bis", "sie", "zu\u00b7letzt", "sich", "selbst", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ausbr\u00fctend im Gem\u00fct gut' oder b\u00f6se Thaten.", "tokens": ["Aus\u00b7br\u00fc\u00b7tend", "im", "Ge\u00b7m\u00fct", "gut'", "o\u00b7der", "b\u00f6\u00b7se", "Tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es war ein Nest gebaut an meines Hauses Wand", "tokens": ["Es", "war", "ein", "Nest", "ge\u00b7baut", "an", "mei\u00b7nes", "Hau\u00b7ses", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im tiefen Mauerritz hart unterm Fensterrand.", "tokens": ["Im", "tie\u00b7fen", "Mau\u00b7er\u00b7ritz", "hart", "un\u00b7term", "Fens\u00b7ter\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vom Boden auf zu hoch, zu tief vom Fenster oben;", "tokens": ["Vom", "Bo\u00b7den", "auf", "zu", "hoch", ",", "zu", "tief", "vom", "Fens\u00b7ter", "o\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was in dem Neste sei, ich konnt' es nicht erproben.", "tokens": ["Was", "in", "dem", "Nes\u00b7te", "sei", ",", "ich", "konnt'", "es", "nicht", "er\u00b7pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Im ersten Morgengraun, im letzten Abendschimmer,", "tokens": ["Im", "ers\u00b7ten", "Mor\u00b7gen\u00b7graun", ",", "im", "letz\u00b7ten", "A\u00b7bend\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Flog etwas aus und ein mir unter'n Augen immer.", "tokens": ["Flog", "et\u00b7was", "aus", "und", "ein", "mir", "un\u00b7ter'n", "Au\u00b7gen", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "KON", "ART", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch eh ich mich besann, so war es schon vorbei,", "tokens": ["Doch", "eh", "ich", "mich", "be\u00b7sann", ",", "so", "war", "es", "schon", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob es der Zwietracht Spatz, des Friedens Schwalbe sei.", "tokens": ["Ob", "es", "der", "Zwiet\u00b7racht", "Spatz", ",", "des", "Frie\u00b7dens", "Schwal\u00b7be", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "ART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So bis zum Fr\u00fchlingsend' erhielt sichs still im Neste,", "tokens": ["So", "bis", "zum", "Fr\u00fch\u00b7lings\u00b7end'", "er\u00b7hielt", "sichs", "still", "im", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "PIS", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch um die Sommerwend' erwachten laute G\u00e4ste.", "tokens": ["Doch", "um", "die", "Som\u00b7mer\u00b7wend'", "er\u00b7wach\u00b7ten", "lau\u00b7te", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sie flogen, flatterten und schwirrten allenthalben,", "tokens": ["Sie", "flo\u00b7gen", ",", "flat\u00b7ter\u00b7ten", "und", "schwirr\u00b7ten", "al\u00b7len\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und froh erkannt' ich erst, es waren junge Schwalben.", "tokens": ["Und", "froh", "er\u00b7kannt'", "ich", "erst", ",", "es", "wa\u00b7ren", "jun\u00b7ge", "Schwal\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}