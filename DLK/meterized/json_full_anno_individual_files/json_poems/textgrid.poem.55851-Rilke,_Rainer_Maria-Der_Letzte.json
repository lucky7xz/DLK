{"textgrid.poem.55851": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Letzte", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe kein Vaterhaus,", "tokens": ["Ich", "ha\u00b7be", "kein", "Va\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und habe auch keines verloren;", "tokens": ["und", "ha\u00b7be", "auch", "kei\u00b7nes", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "meine Mutter hat mich in die Welt hinaus", "tokens": ["mei\u00b7ne", "Mut\u00b7ter", "hat", "mich", "in", "die", "Welt", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "APZR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "geboren.", "tokens": ["ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Da steh ich nun in der Welt und geh", "tokens": ["Da", "steh", "ich", "nun", "in", "der", "Welt", "und", "geh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "in die Welt immer tiefer hinein,", "tokens": ["in", "die", "Welt", "im\u00b7mer", "tie\u00b7fer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "und habe mein Gl\u00fcck und habe mein Weh", "tokens": ["und", "ha\u00b7be", "mein", "Gl\u00fcck", "und", "ha\u00b7be", "mein", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "KON", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und habe jedes allein.", "tokens": ["und", "ha\u00b7be", "je\u00b7des", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Und bin doch manch eines Erbe.", "tokens": ["Und", "bin", "doch", "manch", "ei\u00b7nes", "Er\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Mit drei Zweigen hat mein Geschlecht gebl\u00fcht", "tokens": ["Mit", "drei", "Zwei\u00b7gen", "hat", "mein", "Ge\u00b7schlecht", "ge\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "auf sieben Schl\u00f6ssern im Wald,", "tokens": ["auf", "sie\u00b7ben", "Schl\u00f6s\u00b7sern", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "und wurde seines Wappens m\u00fcd", "tokens": ["und", "wur\u00b7de", "sei\u00b7nes", "Wap\u00b7pens", "m\u00fcd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "und war schon viel zu alt; \u2013", "tokens": ["und", "war", "schon", "viel", "zu", "alt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "und was sie mir lie\u00dfen und was ich erwerbe", "tokens": ["und", "was", "sie", "mir", "lie\u00b7\u00dfen", "und", "was", "ich", "er\u00b7wer\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "zum alten Besitze, ist heimatlos.", "tokens": ["zum", "al\u00b7ten", "Be\u00b7sit\u00b7ze", ",", "ist", "hei\u00b7mat\u00b7los", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "In meinen H\u00e4nden, in meinem Schoo\u00df", "tokens": ["In", "mei\u00b7nen", "H\u00e4n\u00b7den", ",", "in", "mei\u00b7nem", "Schoo\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "mu\u00df ich es halten, bis ich sterbe.", "tokens": ["mu\u00df", "ich", "es", "hal\u00b7ten", ",", "bis", "ich", "ster\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Denn was ich fortstelle,", "tokens": ["Denn", "was", "ich", "fort\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["