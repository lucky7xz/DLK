{"dta.poem.3866": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund/ der um vieles  \n betrogen worden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertzens-Freund, nun seh ich wohl,", "tokens": ["Mein", "Hert\u00b7zens\u00b7Freund", ",", "nun", "seh", "ich", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Da er dir s\u00fcsse vorgelogen,", "tokens": ["Da", "er", "dir", "s\u00fcs\u00b7se", "vor\u00b7ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich um ein merckliches betrogen.", "tokens": ["Dich", "um", "ein", "merck\u00b7li\u00b7ches", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "So machts die falsche Artigkeit,", "tokens": ["So", "machts", "die", "fal\u00b7sche", "Ar\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und in der klug gewordnen Zeit", "tokens": ["Und", "in", "der", "klug", "ge\u00b7word\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sind warlich redliche Gem\u00fcther", "tokens": ["Sind", "war\u00b7lich", "red\u00b7li\u00b7che", "Ge\u00b7m\u00fc\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verha\u00dft und p\u00f6belhaffte G\u00fcter.", "tokens": ["Ver\u00b7ha\u00dft", "und", "p\u00f6\u00b7bel\u00b7haff\u00b7te", "G\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da heist es: Jhr ergebner Knecht,", "tokens": ["Da", "heist", "es", ":", "Ihr", "er\u00b7geb\u00b7ner", "Knecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allein die Liebe ist sehr schlecht,", "tokens": ["Al\u00b7lein", "die", "Lie\u00b7be", "ist", "sehr", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und die verstellten Freundschaffts-Blicke", "tokens": ["Und", "die", "ver\u00b7stell\u00b7ten", "Freund\u00b7schaffts\u00b7Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sind voller falsch verha\u00dfter T\u00fccke.", "tokens": ["Sind", "vol\u00b7ler", "falsch", "ver\u00b7ha\u00df\u00b7ter", "T\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Du hast die Wahrheit eingesehn,", "tokens": ["Du", "hast", "die", "Wahr\u00b7heit", "ein\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Du wirst mir willig zugestehn", "tokens": ["Du", "wirst", "mir", "wil\u00b7lig", "zu\u00b7ge\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bey dem Verlust zwey tausend Thaler,", "tokens": ["Bey", "dem", "Ver\u00b7lust", "zwey", "tau\u00b7send", "Tha\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "CARD", "CARD", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Bey dem verlogenen Bezahler,", "tokens": ["Bey", "dem", "ver\u00b7lo\u00b7ge\u00b7nen", "Be\u00b7zah\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "So siehst du nun der Freundschaffts Pflicht.", "tokens": ["So", "siehst", "du", "nun", "der", "Freund\u00b7schaffts", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Drum traue k\u00fcnfftig keinem nicht.", "tokens": ["Drum", "trau\u00b7e", "k\u00fcnff\u00b7tig", "kei\u00b7nem", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und dieses ist dir auch zu rathen.", "tokens": ["Und", "die\u00b7ses", "ist", "dir", "auch", "zu", "ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mich kr\u00e4ncket dein erlittner Schaden,", "tokens": ["Mich", "kr\u00e4n\u00b7cket", "dein", "er\u00b7litt\u00b7ner", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Dir w\u00fcnscht die rege Freundschaffts-Brust,", "tokens": ["Dir", "w\u00fcnscht", "die", "re\u00b7ge", "Freund\u00b7schaffts\u00b7Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df, ", "tokens": ["Da\u00df", ","], "token_info": ["word", "punct"], "pos": ["KOUS", "$,"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Der Himmel mit sehr viel Erg\u00f6tzen", "tokens": ["Der", "Him\u00b7mel", "mit", "sehr", "viel", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "M\u00f6g doppelt wiederum ersetzen.", "tokens": ["M\u00f6g", "dop\u00b7pelt", "wie\u00b7de\u00b7rum", "er\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und schl\u00fc\u00dflich bleibet es dabey,", "tokens": ["Und", "schl\u00fc\u00df\u00b7lich", "blei\u00b7bet", "es", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Da\u00df ich dein Freund und Diener sey.", "tokens": ["Da\u00df", "ich", "dein", "Freund", "und", "Die\u00b7ner", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}