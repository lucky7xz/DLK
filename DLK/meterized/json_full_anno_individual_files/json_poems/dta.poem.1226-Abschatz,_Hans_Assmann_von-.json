{"dta.poem.1226": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein einiges Blicken", "tokens": ["Ein", "ei\u00b7ni\u00b7ges", "Bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der funckelnden Augen/", "tokens": ["Der", "fun\u00b7ckeln\u00b7den", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die mir aussaugen", "tokens": ["Die", "mir", "aus\u00b7sau\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das Blutt vom Hertzen/", "tokens": ["Das", "Blutt", "vom", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Macht mich die Kertzen", "tokens": ["Macht", "mich", "die", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Des Himmels nicht achten.", "tokens": ["Des", "Him\u00b7mels", "nicht", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Um Seufftzer zu schicken", "tokens": ["Um", "Seufft\u00b7zer", "zu", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Will ich mich bem\u00fchen", "tokens": ["Will", "ich", "mich", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Noch Odem zu ziehen/", "tokens": ["Noch", "O\u00b7dem", "zu", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Sonst wolt ich mit Willen/", "tokens": ["Sonst", "wolt", "ich", "mit", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Mein Leiden zu stillen/", "tokens": ["Mein", "Lei\u00b7den", "zu", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Noch heute verschmachten.", "tokens": ["Noch", "heu\u00b7te", "ver\u00b7schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}