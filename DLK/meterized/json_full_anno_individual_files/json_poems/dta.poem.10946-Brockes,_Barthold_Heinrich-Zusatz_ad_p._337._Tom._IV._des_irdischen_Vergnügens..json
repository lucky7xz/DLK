{"dta.poem.10946": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zusatz  \n  ad p. 337. Tom. IV.  des irdischen Vergn\u00fcgens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wirfst du vielleicht mir hierauf ein:", "tokens": ["Wirfst", "du", "viel\u00b7leicht", "mir", "hier\u00b7auf", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Schlu\u00df hat nur vom Trost den Schein;", "tokens": ["Dein", "Schlu\u00df", "hat", "nur", "vom", "Trost", "den", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn, wird gleich aus jedwedem Heut", "tokens": ["Denn", ",", "wird", "gleich", "aus", "jed\u00b7we\u00b7dem", "Heut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein baldigs Gestern; wird jedoch", "tokens": ["Ein", "bal\u00b7digs", "Ge\u00b7stern", ";", "wird", "je\u00b7doch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VAFIN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ein Heut aus jedem Morgen wieder,", "tokens": ["Ein", "Heut", "aus", "je\u00b7dem", "Mor\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und so dr\u00fcckt uns des Leidens Joch", "tokens": ["Und", "so", "dr\u00fcckt", "uns", "des", "Lei\u00b7dens", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Nicht minder heut, als gestern, nieder:", "tokens": ["Nicht", "min\u00b7der", "heut", ",", "als", "ge\u00b7stern", ",", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "$,", "KOUS", "ADV", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So mu\u00df ich die\u00df gestehn. Doch ist auch dieses klar,", "tokens": ["So", "mu\u00df", "ich", "die\u00df", "ge\u00b7stehn", ".", "Doch", "ist", "auch", "die\u00b7ses", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "VVPP", "$.", "KON", "VAFIN", "ADV", "PDAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und von der Tage Flu\u00df unwidersprechlich wahr.", "tokens": ["Und", "von", "der", "Ta\u00b7ge", "Flu\u00df", "un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es ist ein Flu\u00df, der nie verweilet,", "tokens": ["Es", "ist", "ein", "Flu\u00df", ",", "der", "nie", "ver\u00b7wei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der unaufh\u00f6rlich vorwerts eilet,", "tokens": ["Der", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "vor\u00b7werts", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und wie der Sch\u00f6pfer es bestimmt,", "tokens": ["Und", "wie", "der", "Sch\u00f6p\u00b7fer", "es", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein ganz gewisses Ende nimmt.", "tokens": ["Ein", "ganz", "ge\u00b7wis\u00b7ses", "En\u00b7de", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Zu welchem Ende dich, nicht nur jedwede Stunde,", "tokens": ["Zu", "wel\u00b7chem", "En\u00b7de", "dich", ",", "nicht", "nur", "jed\u00b7we\u00b7de", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "$,", "PTKNEG", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So gar ein jegliche Secunde,", "tokens": ["So", "gar", "ein", "jeg\u00b7li\u00b7che", "Se\u00b7cun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die sich all Augenblick verliert,", "tokens": ["Die", "sich", "all", "Au\u00b7gen\u00b7blick", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Unwidersprechlich n\u00e4her f\u00fchrt.", "tokens": ["Un\u00b7wi\u00b7der\u00b7sprech\u00b7lich", "n\u00e4\u00b7her", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wann nun der schnelle Lauf vorbey:", "tokens": ["Wann", "nun", "der", "schnel\u00b7le", "Lauf", "vor\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "So findet sich, da\u00df auch die allerl\u00e4ngste Zeit,", "tokens": ["So", "fin\u00b7det", "sich", ",", "da\u00df", "auch", "die", "al\u00b7ler\u00b7l\u00e4ngs\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bey der gewissen Ewigkeit,", "tokens": ["Bey", "der", "ge\u00b7wis\u00b7sen", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wahrhaftig nicht einmal ein Punkt gewesen sey.", "tokens": ["Wahr\u00b7haf\u00b7tig", "nicht", "ein\u00b7mal", "ein", "Punkt", "ge\u00b7we\u00b7sen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ART", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}