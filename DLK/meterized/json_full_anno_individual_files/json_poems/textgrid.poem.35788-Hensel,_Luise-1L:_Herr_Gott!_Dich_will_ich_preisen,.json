{"textgrid.poem.35788": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Gott! Dich will ich preisen,", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Gott! Dich will ich preisen,", "tokens": ["Herr", "Gott", "!", "Dich", "will", "ich", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lang mein Odem weht;", "tokens": ["So", "lang", "mein", "O\u00b7dem", "weht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O h\u00f6r' auf meine Weisen,", "tokens": ["O", "h\u00f6r'", "auf", "mei\u00b7ne", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O sieh auf mein Gebet!", "tokens": ["O", "sieh", "auf", "mein", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Bin ich im Himmel oben,", "tokens": ["Bin", "ich", "im", "Him\u00b7mel", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lern' ich andern Sang,", "tokens": ["Da", "lern'", "ich", "an\u00b7dern", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da will ich hoch Dich loben", "tokens": ["Da", "will", "ich", "hoch", "Dich", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein ewig Leben lang.", "tokens": ["Mein", "e\u00b7wig", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt la\u00df Dir wohlgefallen", "tokens": ["Jetzt", "la\u00df", "Dir", "wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein treu einf\u00e4ltig Lied;", "tokens": ["Mein", "treu", "ein\u00b7f\u00e4l\u00b7tig", "Lied", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df doch ein Kindlein lallen,", "tokens": ["Mu\u00df", "doch", "ein", "Kin\u00b7dlein", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn es die Mutter sieht.", "tokens": ["Wenn", "es", "die", "Mut\u00b7ter", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun hab' ich auch gesehen,", "tokens": ["Nun", "hab'", "ich", "auch", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Du so v\u00e4terlich;", "tokens": ["Wie", "Du", "so", "v\u00e4\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will nun nichts mehr verstehen", "tokens": ["Will", "nun", "nichts", "mehr", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Dich, mein Vater, Dich.", "tokens": ["Als", "Dich", ",", "mein", "Va\u00b7ter", ",", "Dich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sa\u00df in meiner Kammer,", "tokens": ["Ich", "sa\u00df", "in", "mei\u00b7ner", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah tr\u00fcb' in's Leben hin,", "tokens": ["Sah", "tr\u00fcb'", "in's", "Le\u00b7ben", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele rang in Jammer,", "tokens": ["Die", "See\u00b7le", "rang", "in", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Sorge war mein Sinn:", "tokens": ["Voll", "Sor\u00b7ge", "war", "mein", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da flo\u00df ein heilig Sehnen", "tokens": ["Da", "flo\u00df", "ein", "hei\u00b7lig", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir in das \u00f6de Herz;", "tokens": ["Mir", "in", "das", "\u00f6\u00b7de", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da brach mein Blick in Thr\u00e4nen", "tokens": ["Da", "brach", "mein", "Blick", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hob sich himmelw\u00e4rts.", "tokens": ["Und", "hob", "sich", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Bald sucht' ich Dich von Herzen", "tokens": ["Bald", "sucht'", "ich", "Dich", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bat um Trost und Ruh':", "tokens": ["Und", "bat", "um", "Trost", "und", "Ruh'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wichen alle Schmerzen,", "tokens": ["Da", "wi\u00b7chen", "al\u00b7le", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da kamst und halfest Du.", "tokens": ["Da", "kamst", "und", "hal\u00b7fest", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Daf\u00fcr will ich Dir danken", "tokens": ["Da\u00b7f\u00fcr", "will", "ich", "Dir", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und immer harren Dein;", "tokens": ["Und", "im\u00b7mer", "har\u00b7ren", "Dein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr! la\u00df nur sonder Wanken", "tokens": ["Herr", "!", "la\u00df", "nur", "son\u00b7der", "Wan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich ganz Dein eigen sein.", "tokens": ["Mich", "ganz", "Dein", "ei\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du kennst, mein Gott, die Herzen,", "tokens": ["Du", "kennst", ",", "mein", "Gott", ",", "die", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du siehst mich, wie ich bin;", "tokens": ["Du", "siehst", "mich", ",", "wie", "ich", "bin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wei\u00dft auch alle Schmerzen,", "tokens": ["Du", "wei\u00dft", "auch", "al\u00b7le", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die noch im Busen drin.", "tokens": ["Die", "noch", "im", "Bu\u00b7sen", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du siehst die S\u00fcndentriebe,", "tokens": ["Du", "siehst", "die", "S\u00fcn\u00b7den\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mich zur Erde ziehn,", "tokens": ["Die", "mich", "zur", "Er\u00b7de", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Siehst auch die treue Liebe,", "tokens": ["Siehst", "auch", "die", "treu\u00b7e", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die auf zu Dir will fliehn.", "tokens": ["Die", "auf", "zu", "Dir", "will", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O hilf die S\u00fcnde d\u00e4mpfen!", "tokens": ["O", "hilf", "die", "S\u00fcn\u00b7de", "d\u00e4mp\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lang' ich lebe schon,", "tokens": ["So", "lang'", "ich", "le\u00b7be", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' ich ein stetes K\u00e4mpfen", "tokens": ["Hab'", "ich", "ein", "ste\u00b7tes", "K\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimmer doch den Lohn.", "tokens": ["Und", "nim\u00b7mer", "doch", "den", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mich dr\u00fccken schwere Ketten,", "tokens": ["Mich", "dr\u00fc\u00b7cken", "schwe\u00b7re", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die unertr\u00e4glich sind.", "tokens": ["Die", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr! willst Du mich nicht retten? \u2013", "tokens": ["Herr", "!", "willst", "Du", "mich", "nicht", "ret\u00b7ten", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr, ja, ich bin Dein Kind!", "tokens": ["Herr", ",", "ja", ",", "ich", "bin", "Dein", "Kind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Herr Gott! Dich will ich preisen,", "tokens": ["Herr", "Gott", "!", "Dich", "will", "ich", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So lang mein Odem weht;", "tokens": ["So", "lang", "mein", "O\u00b7dem", "weht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O h\u00f6r' auf meine Weisen,", "tokens": ["O", "h\u00f6r'", "auf", "mei\u00b7ne", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O sieh auf mein Gebet!", "tokens": ["O", "sieh", "auf", "mein", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.14": {"line.1": {"text": "Bin ich im Himmel oben,", "tokens": ["Bin", "ich", "im", "Him\u00b7mel", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lern' ich andern Sang,", "tokens": ["Da", "lern'", "ich", "an\u00b7dern", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da will ich hoch Dich loben", "tokens": ["Da", "will", "