{"dta.poem.7765": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das H\u00f6chste, was der Mensch erstreben soll und kann,", "tokens": ["Das", "H\u00f6chs\u00b7te", ",", "was", "der", "Mensch", "er\u00b7stre\u00b7ben", "soll", "und", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erstreben kann und soll es doch nicht jedermann.", "tokens": ["Er\u00b7stre\u00b7ben", "kann", "und", "soll", "es", "doch", "nicht", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die gro\u00dfe Masse l\u00e4\u00dft am Boden sich gen\u00fcgen,", "tokens": ["Die", "gro\u00b7\u00dfe", "Mas\u00b7se", "l\u00e4\u00dft", "am", "Bo\u00b7den", "sich", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und staunt den Wen'gen nach auf ihren Himmelsfl\u00fcgen.", "tokens": ["Und", "staunt", "den", "Wen'\u00b7gen", "nach", "auf", "ih\u00b7ren", "Him\u00b7mels\u00b7fl\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn der Brahmane, der Nichts Lebendes versehrt,", "tokens": ["Wenn", "der", "Brah\u00b7ma\u00b7ne", ",", "der", "Nichts", "Le\u00b7ben\u00b7des", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PIS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selbst im Sch\u00e4dlichen den Odem Gottes ehrt,", "tokens": ["Und", "selbst", "im", "Sch\u00e4d\u00b7li\u00b7chen", "den", "O\u00b7dem", "Got\u00b7tes", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Allein im Lande wohnt'; es w\u00e4re l\u00e4ngst indessen", "tokens": ["Al\u00b7lein", "im", "Lan\u00b7de", "wohnt'", ";", "es", "w\u00e4\u00b7re", "l\u00e4ngst", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Thieren gro\u00df und klein, und er mit, aufgefressen.", "tokens": ["Von", "Thie\u00b7ren", "gro\u00df", "und", "klein", ",", "und", "er", "mit", ",", "auf\u00b7ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,", "KON", "PPER", "PTKVZ", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum wurzelt neben ihm fleischessende Gemeinheit,", "tokens": ["Drum", "wur\u00b7zelt", "ne\u00b7ben", "ihm", "flei\u00b7sches\u00b7sen\u00b7de", "Ge\u00b7mein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und der Gemeinheit Bl\u00fct' ist des Brahmanen Reinheit.", "tokens": ["Und", "der", "Ge\u00b7mein\u00b7heit", "Bl\u00fct'", "ist", "des", "Brah\u00b7ma\u00b7nen", "Rein\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}