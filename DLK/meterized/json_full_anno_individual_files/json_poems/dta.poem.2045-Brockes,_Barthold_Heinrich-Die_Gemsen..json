{"dta.poem.2045": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Gemsen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonst ist \u00fcberall bekannt, wie sie uns so n\u00fctzlich seyn;", "tokens": ["Sonst", "ist", "\u00fc\u00b7be\u00b7rall", "be\u00b7kannt", ",", "wie", "sie", "uns", "so", "n\u00fctz\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "F\u00fcr die Schwindsucht ist ihr Unschlitt, f\u00fcrs Gesicht die", "tokens": ["F\u00fcr", "die", "Schwind\u00b7sucht", "ist", "ihr", "Un\u00b7schlitt", ",", "f\u00fcrs", "Ge\u00b7sicht", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "APPRART", "NN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Galle gut;", "tokens": ["Gal\u00b7le", "gut", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Gemsenfleisch ist gut zu essen, und den Schwindel heilt", "tokens": ["Gem\u00b7sen\u00b7fleisch", "ist", "gut", "zu", "es\u00b7sen", ",", "und", "den", "Schwin\u00b7del", "heilt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "ihr Blut;", "tokens": ["ihr", "Blut", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Auch die Haut dient uns nicht minder. Stralet nicht", "tokens": ["Auch", "die", "Haut", "dient", "uns", "nicht", "min\u00b7der", ".", "Stra\u00b7let", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "VVFIN", "PTKNEG"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "aus diesem Thier,", "tokens": ["aus", "die\u00b7sem", "Thier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Nebst der Weisheit und der Allmacht, auch des Sch\u00f6-", "tokens": ["Nebst", "der", "Weis\u00b7heit", "und", "der", "All\u00b7macht", ",", "auch", "des", "Sch\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "pfers Lieb' herf\u00fcr?", "tokens": ["pfers", "Lieb'", "her\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}