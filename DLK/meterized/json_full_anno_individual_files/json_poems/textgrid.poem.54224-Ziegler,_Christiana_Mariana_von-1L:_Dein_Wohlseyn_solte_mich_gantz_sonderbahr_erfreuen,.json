 "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich sehe dich im Geist vor mir leibhafftig stehen,", "tokens": ["Ich", "se\u00b7he", "dich", "im", "Geist", "vor", "mir", "leib\u00b7haff\u00b7tig", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und habe bis anher noch nimmer nicht geglaubt,", "tokens": ["Und", "ha\u00b7be", "bis", "an\u00b7her", "noch", "nim\u00b7mer", "nicht", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADV", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df mir ein frembdes Land dich, Werthester, geraubt.", "tokens": ["Da\u00df", "mir", "ein", "fremb\u00b7des", "Land", "dich", ",", "Wert\u00b7hes\u00b7ter", ",", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "$,", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich k\u00fcsse dich, wie vor, mu\u00df ich gleich nur mit Schatten,", "tokens": ["Ich", "k\u00fcs\u00b7se", "dich", ",", "wie", "vor", ",", "mu\u00df", "ich", "gleich", "nur", "mit", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PTKVZ", "$,", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-