{"textgrid.poem.54224": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Wohlseyn solte mich gantz sonderbahr erfreuen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Wohlseyn solte mich gantz sonderbahr erfreuen,", "tokens": ["Dein", "Wohl\u00b7seyn", "sol\u00b7te", "mich", "gantz", "son\u00b7der\u00b7bahr", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmel wird nicht stets mit Zorn Cometen dr\u00e4uen;", "tokens": ["Der", "Him\u00b7mel", "wird", "nicht", "stets", "mit", "Zorn", "Co\u00b7me\u00b7ten", "dr\u00e4u\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zieht offt ein Wetter auf bey schwartzer Mitternacht,", "tokens": ["Zieht", "offt", "ein", "Wet\u00b7ter", "auf", "bey", "schwart\u00b7zer", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fcgt sichs, da\u00df hernach die Sonne Glantz und Pracht", "tokens": ["So", "f\u00fcgt", "sichs", ",", "da\u00df", "her\u00b7nach", "die", "Son\u00b7ne", "Glantz", "und", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "L\u00e4st desto st\u00e4rckerer am Horizont erblicken,", "tokens": ["L\u00e4st", "des\u00b7to", "st\u00e4r\u00b7cke\u00b7rer", "am", "Ho\u00b7ri\u00b7zont", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Geist empfindet schon ein inniges Entz\u00fccken,", "tokens": ["Mein", "Geist", "emp\u00b7fin\u00b7det", "schon", "ein", "in\u00b7ni\u00b7ges", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wenn ein geliebter Brief davon mir Nachricht giebt,", "tokens": ["Wenn", "ein", "ge\u00b7lieb\u00b7ter", "Brief", "da\u00b7von", "mir", "Nach\u00b7richt", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den ich so starck als dich, mein Seladon, geliebt.", "tokens": ["Den", "ich", "so", "starck", "als", "dich", ",", "mein", "Se\u00b7la\u00b7don", ",", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "$,", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Liebe hei\u00dft vor dich mich auch entfernet wachen,", "tokens": ["Die", "Lie\u00b7be", "hei\u00dft", "vor", "dich", "mich", "auch", "ent\u00b7fer\u00b7net", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PRF", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich dencke tausendmahl: was mu\u00df er jetzt wohl machen?", "tokens": ["Ich", "den\u00b7cke", "tau\u00b7send\u00b7mahl", ":", "was", "mu\u00df", "er", "jetzt", "wohl", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Hertz begleitet dich mit gleich geme\u00dfnen Schritt,", "tokens": ["Mein", "Hertz", "be\u00b7glei\u00b7tet", "dich", "mit", "gleich", "ge\u00b7me\u00df\u00b7nen", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und suchet jeden Stein, den nur dein Fu\u00df betritt.", "tokens": ["Und", "su\u00b7chet", "je\u00b7den", "Stein", ",", "den", "nur", "dein", "Fu\u00df", "be\u00b7tritt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Sehnsucht l\u00e4st mich nicht dir von der Seite gehen,", "tokens": ["Die", "Sehn\u00b7sucht", "l\u00e4st", "mich", "nicht", "dir", "von", "der", "Sei\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich sehe dich im Geist vor mir leibhafftig stehen,", "tokens": ["Ich", "se\u00b7he", "dich", "im", "Geist", "vor", "mir", "leib\u00b7haff\u00b7tig", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und habe bis anher noch nimmer nicht geglaubt,", "tokens": ["Und", "ha\u00b7be", "bis", "an\u00b7her", "noch", "nim\u00b7mer", "nicht", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADV", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df mir ein frembdes Land dich, Werthester, geraubt.", "tokens": ["Da\u00df", "mir", "ein", "fremb\u00b7des", "Land", "dich", ",", "Wert\u00b7hes\u00b7ter", ",", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "$,", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich k\u00fcsse dich, wie vor, mu\u00df ich gleich nur mit Schatten,", "tokens": ["Ich", "k\u00fcs\u00b7se", "dich", ",", "wie", "vor", ",", "mu\u00df", "ich", "gleich", "nur", "mit", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PTKVZ", "$,", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Das ist, mit deinem Bild, mich unterdessen gatten,", "tokens": ["Das", "ist", ",", "mit", "dei\u00b7nem", "Bild", ",", "mich", "un\u00b7ter\u00b7des\u00b7sen", "gat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und bleibe dir getreu auch in Abwesenheit,", "tokens": ["Und", "blei\u00b7be", "dir", "ge\u00b7treu", "auch", "in", "Ab\u00b7we\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Weil ich mein Hertze dir und keinen sonst geweyht.", "tokens": ["Weil", "ich", "mein", "Hert\u00b7ze", "dir", "und", "kei\u00b7nen", "sonst", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "PPER", "KON", "PIAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nichts Frembdes, glaub es mir, wird leichte mich beth\u00f6ren,", "tokens": ["Nichts", "Fremb\u00b7des", ",", "glaub", "es", "mir", ",", "wird", "leich\u00b7te", "mich", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "$,", "VVFIN", "PPER", "PPER", "$,", "VAFIN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mein Ohr will nur von dir und keinen andern h\u00f6ren;", "tokens": ["Mein", "Ohr", "will", "nur", "von", "dir", "und", "kei\u00b7nen", "an\u00b7dern", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPR", "PPER", "KON", "PIAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du bist und bleibest doch mein Liebstes auf der Welt,", "tokens": ["Du", "bist", "und", "blei\u00b7best", "doch", "mein", "Liebs\u00b7tes", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das meine Seele reitzt, und mir allein gef\u00e4llt.", "tokens": ["Das", "mei\u00b7ne", "See\u00b7le", "reitzt", ",", "und", "mir", "al\u00b7lein", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zwar sitz ich oftermals nicht anders als in Schlummer,", "tokens": ["Zwar", "sitz", "ich", "of\u00b7ter\u00b7mals", "nicht", "an\u00b7ders", "als", "in", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und mache mir dabey den ungerechten Kummer:", "tokens": ["Und", "ma\u00b7che", "mir", "da\u00b7bey", "den", "un\u00b7ge\u00b7rech\u00b7ten", "Kum\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Vielleicht hat deine Treu auf Spiegel-Gla\u00df gebaut,", "tokens": ["Viel\u00b7leicht", "hat", "dei\u00b7ne", "Treu", "auf", "Spie\u00b7gel\u00b7Gla\u00df", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo man das bald vergi\u00dft, was man darinnen schaut,", "tokens": ["Wo", "man", "das", "bald", "ver\u00b7gi\u00dft", ",", "was", "man", "da\u00b7rin\u00b7nen", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PDS", "ADV", "VVFIN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch will ich meinen Geist so viel als m\u00f6glich fassen,", "tokens": ["Doch", "will", "ich", "mei\u00b7nen", "Geist", "so", "viel", "als", "m\u00f6g\u00b7lich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "PIAT", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ob mich das Schicksal gleich scheint noch so sehr zu hassen.", "tokens": ["Ob", "mich", "das", "Schick\u00b7sal", "gleich", "scheint", "noch", "so", "sehr", "zu", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Hofnung heist bey mir das allersch\u00f6nste Wort,", "tokens": ["Die", "Hof\u00b7nung", "heist", "bey", "mir", "das", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die f\u00fchret mich bereits schon an Vergn\u00fcgungs-Port.", "tokens": ["Die", "f\u00fch\u00b7ret", "mich", "be\u00b7reits", "schon", "an", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Befl\u00fcgle deinen Lauff, und eile bald zur\u00fccke,", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "dei\u00b7nen", "Lauff", ",", "und", "ei\u00b7le", "bald", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Seele sehnet sich nach deinen holden Blicke,", "tokens": ["Die", "See\u00b7le", "seh\u00b7net", "sich", "nach", "dei\u00b7nen", "hol\u00b7den", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und kan es noch nicht seyn, verdopple Brief und Schrifft,", "tokens": ["Und", "kan", "es", "noch", "nicht", "seyn", ",", "ver\u00b7dopp\u00b7le", "Brief", "und", "Schrifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VAINF", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Weil sonst nichts in der Welt mir ein Vergn\u00fcgen stifft.", "tokens": ["Weil", "sonst", "nichts", "in", "der", "Welt", "mir", "ein", "Ver\u00b7gn\u00fc\u00b7gen", "stifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Wohlseyn solte mich gantz sonderbahr erfreuen,", "tokens": ["Dein", "Wohl\u00b7seyn", "sol\u00b7te", "mich", "gantz", "son\u00b7der\u00b7bahr", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmel wird nicht stets mit Zorn Cometen dr\u00e4uen;", "tokens": ["Der", "Him\u00b7mel", "wird", "nicht", "stets", "mit", "Zorn", "Co\u00b7me\u00b7ten", "dr\u00e4u\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zieht offt ein Wetter auf bey schwartzer Mitternacht,", "tokens": ["Zieht", "offt", "ein", "Wet\u00b7ter", "auf", "bey", "schwart\u00b7zer", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fcgt sichs, da\u00df hernach die Sonne Glantz und Pracht", "tokens": ["So", "f\u00fcgt", "sichs", ",", "da\u00df", "her\u00b7nach", "die", "Son\u00b7ne", "Glantz", "und", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word"