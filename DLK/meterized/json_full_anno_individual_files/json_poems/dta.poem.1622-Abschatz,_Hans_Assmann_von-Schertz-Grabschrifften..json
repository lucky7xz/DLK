{"dta.poem.1622": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-Grabschrifften.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Schau Wandersmann/ hier lieget deines gleichen/ ", "tokens": ["Schau", "Wan\u00b7ders\u00b7mann", "/", "hier", "lie\u00b7get", "dei\u00b7nes", "glei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "ADV", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerbrechlich Thon und auffgebla\u00dfner Koth/", "tokens": ["Zer\u00b7brech\u00b7lich", "Thon", "und", "auff\u00b7ge\u00b7bla\u00df\u00b7ner", "Koth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Leg einen Stein/ und schreibe drauff zum Zeichen:", "tokens": ["Leg", "ei\u00b7nen", "Stein", "/", "und", "schrei\u00b7be", "drauff", "zum", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "KON", "VVFIN", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier liegt der Prei\u00df von allen Kr\u00fcgeln todt;", "tokens": ["Hier", "liegt", "der", "Prei\u00df", "von", "al\u00b7len", "Kr\u00fc\u00b7geln", "todt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch/ k\u00f6nt es seyn/ so w\u00fcnscht ich mich begraben/", "tokens": ["Doch", "/", "k\u00f6nt", "es", "seyn", "/", "so", "w\u00fcnscht", "ich", "mich", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PPER", "VAINF", "$(", "ADV", "VVFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo so viel Kr\u00fcg ihr edles Grabmahl haben.", "tokens": ["Wo", "so", "viel", "Kr\u00fcg", "ihr", "ed\u00b7les", "Grab\u00b7mahl", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "(zu Rom auff dem so genannten Scherben-Berge.) ", "tokens": ["(", "zu", "Rom", "auff", "dem", "so", "ge\u00b7nann\u00b7ten", "Scher\u00b7ben\u00b7Ber\u00b7ge", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NE", "APPR", "ART", "ADV", "ADJA", "NN", "$.", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Zwar bleibt mein Ruhm der Nachwelt unverholen/", "tokens": ["Zwar", "bleibt", "mein", "Ruhm", "der", "Nach\u00b7welt", "un\u00b7ver\u00b7ho\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich baut aus Thon des klugen T\u00f6pffers Hand/", "tokens": ["Mich", "baut", "aus", "Thon", "des", "klu\u00b7gen", "T\u00f6pf\u00b7fers", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein stoltzes Kleid war Purpur der Violen/", "tokens": ["Mein", "stolt\u00b7zes", "Kleid", "war", "Pur\u00b7pur", "der", "Vi\u00b7o\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "ART", "NN", "$("], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ich ward durch Glutt dem Golde gleich erkannt.", "tokens": ["Ich", "ward", "durch", "Glutt", "dem", "Gol\u00b7de", "gleich", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie offt hab ich der Frauen Mund getr\u00e4ncket/", "tokens": ["Wie", "offt", "hab", "ich", "der", "Frau\u00b7en", "Mund", "ge\u00b7tr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ihr den Safft der Ceres eingesencket.", "tokens": ["Und", "ihr", "den", "Safft", "der", "Ce\u00b7res", "ein\u00b7ge\u00b7sen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es neidten mich die edlen Porcellanen/", "tokens": ["Es", "neid\u00b7ten", "mich", "die", "ed\u00b7len", "Por\u00b7cel\u00b7la\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gold/ Silber/ Zinn/ trug Feindschafft gegen mir/", "tokens": ["Gold", "/", "Sil\u00b7ber", "/", "Zinn", "/", "trug", "Feind\u00b7schafft", "ge\u00b7gen", "mir", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "VVFIN", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bi\u00df endlich sich/ nicht sonder ihr Ermahnen/", "tokens": ["Bi\u00df", "end\u00b7lich", "sich", "/", "nicht", "son\u00b7der", "ihr", "Er\u00b7mah\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PRF", "$(", "PTKNEG", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein frecher Feind gewagt an meine Zier/", "tokens": ["Ein", "fre\u00b7cher", "Feind", "ge\u00b7wagt", "an", "mei\u00b7ne", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich Aermstes must am Steffans Tage stertzen/", "tokens": ["Ich", "A\u00b7erms\u00b7tes", "must", "am", "Stef\u00b7fans", "Ta\u00b7ge", "stert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Dieweil ich nicht mit Steinen konte schertzen.", "tokens": ["Die\u00b7weil", "ich", "nicht", "mit", "Stei\u00b7nen", "kon\u00b7te", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch soll darum mein Nahme nicht erliegen;", "tokens": ["Doch", "soll", "da\u00b7rum", "mein", "Nah\u00b7me", "nicht", "er\u00b7lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Bruder theilt Junonens Nectar aus;", "tokens": ["Mein", "Bru\u00b7der", "theilt", "Ju\u00b7no\u00b7nens", "Nec\u00b7tar", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beym Wassermann werd ich die Stelle kriegen/", "tokens": ["Beym", "Was\u00b7ser\u00b7mann", "werd", "ich", "die", "Stel\u00b7le", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da schm\u00fcckt mein Glantz das blaue Sternen-Hau\u00df.", "tokens": ["Da", "schm\u00fcckt", "mein", "Glantz", "das", "blau\u00b7e", "Ster\u00b7nen\u00b7Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Kr\u00fcge gro\u00df und klein/ lasst Thr\u00e4nen fl\u00fcssen/", "tokens": ["Ihr", "Kr\u00fc\u00b7ge", "gro\u00df", "und", "klein", "/", "lasst", "Thr\u00e4\u00b7nen", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$(", "VVFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr werdet mir in kurtzen folgen m\u00fcssen.", "tokens": ["Ihr", "wer\u00b7det", "mir", "in", "kurt\u00b7zen", "fol\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}