{"textgrid.poem.34026": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Geschlossener Kreis", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht vermogte die Traube den Wein noch l\u00e4nger zu halten,", "tokens": ["Nicht", "ver\u00b7mog\u00b7te", "die", "Trau\u00b7be", "den", "Wein", "noch", "l\u00e4n\u00b7ger", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Als man sie kelterte, war sie dem Zerspringen schon nah;", "tokens": ["Als", "man", "sie", "kel\u00b7ter\u00b7te", ",", "war", "sie", "dem", "Zer\u00b7sprin\u00b7gen", "schon", "nah", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Auch nicht konnte das Fa\u00df, das starke, den feurigen fesseln,", "tokens": ["Auch", "nicht", "konn\u00b7te", "das", "Fa\u00df", ",", "das", "star\u00b7ke", ",", "den", "feu\u00b7ri\u00b7gen", "fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VMFIN", "ART", "NN", "$,", "PDS", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wenn man nicht schnell ihn gezapft, h\u00e4tt' er sich selber befreit;", "tokens": ["Wenn", "man", "nicht", "schnell", "ihn", "ge\u00b7zapft", ",", "h\u00e4tt'", "er", "sich", "sel\u00b7ber", "be\u00b7freit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADJD", "PPER", "VVPP", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Noch viel weniger h\u00e4lt ihn der Dichter, der ihn getrunken,", "tokens": ["Noch", "viel", "we\u00b7ni\u00b7ger", "h\u00e4lt", "ihn", "der", "Dich\u00b7ter", ",", "der", "ihn", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Jetzt zur\u00fcck, als Gedicht fliegt er schon wieder davon;", "tokens": ["Jetzt", "zu\u00b7r\u00fcck", ",", "als", "Ge\u00b7dicht", "fliegt", "er", "schon", "wie\u00b7der", "da\u00b7von", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "NN", "VVFIN", "PPER", "ADV", "ADV", "PAV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "M\u00f6g' es den H\u00f6rer berauschen, und m\u00f6g' er nicht eher ern\u00fcchtern,", "tokens": ["M\u00f6g'", "es", "den", "H\u00f6\u00b7rer", "be\u00b7rau\u00b7schen", ",", "und", "m\u00f6g'", "er", "nicht", "e\u00b7her", "er\u00b7n\u00fcch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Bis er Reben gepflanzt, da\u00df sich vollende der Kreis!", "tokens": ["Bis", "er", "Re\u00b7ben", "ge\u00b7pflanzt", ",", "da\u