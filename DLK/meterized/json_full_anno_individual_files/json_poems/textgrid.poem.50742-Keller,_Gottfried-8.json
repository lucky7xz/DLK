{"textgrid.poem.50742": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist das f\u00fcr ein Schrein und Peitschenknallen?", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "Schrein", "und", "Peit\u00b7schen\u00b7knal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fenster zittern von der Hufen Klang;", "tokens": ["Die", "Fens\u00b7ter", "zit\u00b7tern", "von", "der", "Hu\u00b7fen", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zw\u00f6lf Rosse keuchen an dem straffen Strang,", "tokens": ["Zw\u00f6lf", "Ros\u00b7se", "keu\u00b7chen", "an", "dem", "straf\u00b7fen", "Strang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Fuhrmannsfl\u00fcche durch die Gasse schallen.", "tokens": ["Und", "Fuhr\u00b7manns\u00b7fl\u00fc\u00b7che", "durch", "die", "Gas\u00b7se", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der auf den freien Bergen ist gefallen,", "tokens": ["Der", "auf", "den", "frei\u00b7en", "Ber\u00b7gen", "ist", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "w