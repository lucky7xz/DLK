{"dta.poem.1613": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eisen-H\u00fcttel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist es Zeit zu wachen ", "tokens": ["Nun", "ist", "es", "Zeit", "zu", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh Deutschlands Freyheit stirbt/", "tokens": ["Eh", "Deutschlands", "Frey\u00b7heit", "stirbt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und in dem weiten Rachen", "tokens": ["Und", "in", "dem", "wei\u00b7ten", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Crocodils verdirbt.", "tokens": ["Des", "Cro\u00b7co\u00b7dils", "ver\u00b7dirbt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Herbey/ da\u00df man die Kr\u00f6tten/", "tokens": ["Her\u00b7bey", "/", "da\u00df", "man", "die", "Kr\u00f6t\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PIS", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die unsern Rhein betretten/", "tokens": ["Die", "un\u00b7sern", "Rhein", "be\u00b7tret\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit aller Macht zur\u00fccke", "tokens": ["Mit", "al\u00b7ler", "Macht", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur Son und Seine schicke.", "tokens": ["Zur", "Son", "und", "Sei\u00b7ne", "schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Feind braucht Gold und Eisen", "tokens": ["Der", "Feind", "braucht", "Gold", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wendt Stahl und Silber an/", "tokens": ["Wendt", "Stahl", "und", "Sil\u00b7ber", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der deutschen Welt zu weisen/", "tokens": ["Der", "deut\u00b7schen", "Welt", "zu", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was List und Hochmutt kan;", "tokens": ["Was", "List", "und", "Hoch\u00b7mutt", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Last euch das Geld in H\u00e4nden", "tokens": ["Last", "euch", "das", "Geld", "in", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Augen nicht verblenden/", "tokens": ["Die", "Au\u00b7gen", "nicht", "ver\u00b7blen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Damit euch hinterm R\u00fccken", "tokens": ["Da\u00b7mit", "euch", "hin\u00b7term", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die F\u00e4ssel nicht bestricken.", "tokens": ["Die", "F\u00e4s\u00b7sel", "nicht", "be\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Lasst Lerch und Falcken fliegen/", "tokens": ["Lasst", "Lerch", "und", "Fal\u00b7cken", "flie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sezt alle Kr\u00e4ffte bey/", "tokens": ["Sezt", "al\u00b7le", "Kr\u00e4ff\u00b7te", "bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihnen zu besiegen", "tokens": ["Mit", "ih\u00b7nen", "zu", "be\u00b7sie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Hahnes Pralerey.", "tokens": ["Des", "Hah\u00b7nes", "Pra\u00b7le\u00b7rey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er prangt mit euren Federn/", "tokens": ["Er", "prangt", "mit", "eu\u00b7ren", "Fe\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum m\u00fcsst ihr ihn ent\u00e4dern/", "tokens": ["Drum", "m\u00fcsst", "ihr", "ihn", "en\u00b7t\u00e4\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ieder sich bem\u00fchen", "tokens": ["Und", "ie\u00b7der", "sich", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Seine weg zu ziehen.", "tokens": ["Das", "Sei\u00b7ne", "weg", "zu", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wollt ihr euch unterwinden", "tokens": ["Wollt", "ihr", "euch", "un\u00b7ter\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu thun was sich geb\u00fchrt/", "tokens": ["Zu", "thun", "was", "sich", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRELS", "PRF", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Hermann wird sich finden/", "tokens": ["Ein", "Her\u00b7mann", "wird", "sich", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der euch an Reihen f\u00fchrt.", "tokens": ["Der", "euch", "an", "Rei\u00b7hen", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lasst euch verstellten Frieden", "tokens": ["Lasst", "euch", "ver\u00b7stell\u00b7ten", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Schlaffe nicht erm\u00fcden.", "tokens": ["Zum", "Schlaf\u00b7fe", "nicht", "er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Mu\u00df man die Ruh erjagen.", "tokens": ["Mu\u00df", "man", "die", "Ruh", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}