{"dta.poem.19523": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlacht der B\u00e4ume.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Hier am Sarazenenthurme,", "tokens": ["Hier", "am", "Sa\u00b7ra\u00b7ze\u00b7nen\u00b7thur\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die Stra\u00dfe hielt geschlossen,", "tokens": ["Der", "die", "Stra\u00b7\u00dfe", "hielt", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist in manchem wilden Sturme", "tokens": ["Ist", "in", "man\u00b7chem", "wil\u00b7den", "Stur\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsch und welsches Blut geflossen.", "tokens": ["Deutsch", "und", "wel\u00b7sches", "Blut", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PWAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun sich in des Thales R\u00e4umen", "tokens": ["Nun", "sich", "in", "des", "Tha\u00b7les", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4nger nicht die V\u00f6lker morden,", "tokens": ["L\u00e4n\u00b7ger", "nicht", "die", "V\u00f6l\u00b7ker", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringen noch mit ihren B\u00e4umen", "tokens": ["Rin\u00b7gen", "noch", "mit", "ih\u00b7ren", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier der S\u00fcden und der Norden.", "tokens": ["Hier", "der", "S\u00fc\u00b7den", "und", "der", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}