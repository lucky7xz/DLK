{"textgrid.poem.64237": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Bei der Kriegserkl\u00e4rung Frankreichs", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ob zerkl\u00fcftet und zergliedert", "tokens": ["Und", "ob", "zer\u00b7kl\u00fcf\u00b7tet", "und", "zer\u00b7glie\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des deutschen Volkes Herrlichkeit,", "tokens": ["Des", "deut\u00b7schen", "Vol\u00b7kes", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So tief ist's, Welscher, nicht erniedert,", "tokens": ["So", "tief", "ist's", ",", "Wel\u00b7scher", ",", "nicht", "er\u00b7nie\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PWAT", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es dem Schlag die Wange leiht.", "tokens": ["Da\u00df", "es", "dem", "Schlag", "die", "Wan\u00b7ge", "leiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl ging uns Ungl\u00fcck und Bet\u00f6rung,", "tokens": ["Wohl", "ging", "uns", "Un\u00b7gl\u00fcck", "und", "Be\u00b7t\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein b\u00f6ser Schatte, lange nach,", "tokens": ["Ein", "b\u00f6\u00b7ser", "Schat\u00b7te", ",", "lan\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch nun genug der Selbstzerst\u00f6rung,", "tokens": ["Doch", "nun", "ge\u00b7nug", "der", "Selbst\u00b7zer\u00b7st\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Genug des Zwistes und der Schmach!", "tokens": ["Ge\u00b7nug", "des", "Zwis\u00b7tes", "und", "der", "Schmach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl fiel dein kaiserlich Geschmeide,", "tokens": ["Wohl", "fiel", "dein", "kai\u00b7ser\u00b7lich", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Germania, dir von Brust und Haupt,", "tokens": ["Ger\u00b7ma\u00b7nia", ",", "dir", "von", "Brust", "und", "Haupt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl hat von deinem reichen Kleide", "tokens": ["Wohl", "hat", "von", "dei\u00b7nem", "rei\u00b7chen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Manch' edel St\u00fcck der Feind geraubt,", "tokens": ["Man\u00b7ch'", "e\u00b7del", "St\u00fcck", "der", "Feind", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wohl hadern rings noch deine S\u00f6hne,", "tokens": ["Wohl", "ha\u00b7dern", "rings", "noch", "dei\u00b7ne", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stark ist das Unrecht, schwach das Recht,", "tokens": ["Stark", "ist", "das", "Un\u00b7recht", ",", "schwach", "das", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Fern von des Friedens heil'ger Sch\u00f6ne,", "tokens": ["Fern", "von", "des", "Frie\u00b7dens", "heil'\u00b7ger", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schwer ringend schafft noch dies Geschlecht: \u2013", "tokens": ["Schwer", "rin\u00b7gend", "schafft", "noch", "dies", "Ge\u00b7schlecht", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ADV", "PDS", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Doch hebt der Erbfeind frech die H\u00e4nde", "tokens": ["Doch", "hebt", "der", "Erb\u00b7feind", "frech", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach unsrer Br\u00fcder Wappenschild,", "tokens": ["Nach", "uns\u00b7rer", "Br\u00fc\u00b7der", "Wap\u00b7pen\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ist der Hader all' zu Ende,", "tokens": ["Dann", "ist", "der", "Ha\u00b7der", "all'", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Streit im Elternhaus gestillt,", "tokens": ["Der", "Streit", "im", "El\u00b7tern\u00b7haus", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Nord und S\u00fcd im heil'gen Grimme", "tokens": ["Und", "Nord", "und", "S\u00fcd", "im", "heil'\u00b7gen", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vereint der Ruf der Ehre sie,", "tokens": ["Ver\u00b7eint", "der", "Ruf", "der", "Eh\u00b7re", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und donnernd t\u00f6nt's aus Einer Stimme:", "tokens": ["Und", "don\u00b7nernd", "t\u00f6nt's", "aus", "Ei\u00b7ner", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbhie deutsches Schwert und Deutschland hie.\u00ab", "tokens": ["\u00bb", "hie", "deut\u00b7sches", "Schwert", "und", "Deutschland", "hie", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "KON", "NE", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das ist kein Krieg um die Chim\u00e4re", "tokens": ["Das", "ist", "kein", "Krieg", "um", "die", "Chi\u00b7m\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Thronenglanz und F\u00fcrstenruhm:", "tokens": ["Von", "Thro\u00b7nen\u00b7glanz", "und", "F\u00fcrs\u00b7ten\u00b7ruhm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist der Kampf um Deutschlands Ehre", "tokens": ["Das", "ist", "der", "Kampf", "um", "Deutschlands", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und jedes deutsche Heiligtum.", "tokens": ["Und", "je\u00b7des", "deut\u00b7sche", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es tritt vor seines Hauses Pforte,", "tokens": ["Es", "tritt", "vor", "sei\u00b7nes", "Hau\u00b7ses", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das frecher \u00dcbermut bedroht,", "tokens": ["Das", "fre\u00b7cher", "\u00dc\u00b7ber\u00b7mut", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das deutsche Volk mit zorn'gem Worte", "tokens": ["Das", "deut\u00b7sche", "Volk", "mit", "zorn'\u00b7gem", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kampf auf Leben und auf Tod.", "tokens": ["Zum", "Kampf", "auf", "Le\u00b7ben", "und", "auf", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er zwingt das Schwert uns in die H\u00e4nde:", "tokens": ["Er", "zwingt", "das", "Schwert", "uns", "in", "die", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlan, so sei's nicht mehr gesenkt,", "tokens": ["Wo\u00b7hlan", ",", "so", "sei's", "nicht", "mehr", "ge\u00b7senkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis sich das Schicksal ganz vollende,", "tokens": ["Bis", "sich", "das", "Schick\u00b7sal", "ganz", "voll\u00b7en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem sich der Feind entgegen dr\u00e4ngt.", "tokens": ["Dem", "sich", "der", "Feind", "ent\u00b7ge\u00b7gen", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf! Werft den Friedebrecher nieder,", "tokens": ["Auf", "!", "Werft", "den", "Frie\u00b7de\u00b7bre\u00b7cher", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er uns nie mehr schaden kann,", "tokens": ["Da\u00df", "er", "uns", "nie", "mehr", "scha\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die edeln Marken nehmt ihm wieder,", "tokens": ["Die", "e\u00b7deln", "Mar\u00b7ken", "nehmt", "ihm", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die er in b\u00f6ser Zeit gewann.", "tokens": ["Die", "er", "in", "b\u00f6\u00b7ser", "Zeit", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00dft seh'n, ob nicht zum Vaterlande", "tokens": ["La\u00dft", "seh'n", ",", "ob", "nicht", "zum", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Herz des Elsa\u00df wieder neigt,", "tokens": ["Das", "Herz", "des", "El\u00b7sa\u00df", "wie\u00b7der", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ihr ihm, statt der alten Schande,", "tokens": ["Wenn", "ihr", "ihm", ",", "statt", "der", "al\u00b7ten", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Spiegel deutscher Ehre zeigt.", "tokens": ["Den", "Spie\u00b7gel", "deut\u00b7scher", "Eh\u00b7re", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Mit Einem Zeichen nur gewinnen", "tokens": ["Mit", "Ei\u00b7nem", "Zei\u00b7chen", "nur", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das alte Reichsland werdet ihr:", "tokens": ["Das", "al\u00b7te", "Reichs\u00b7land", "wer\u00b7det", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Pflanzt auf des freien Stra\u00dfburg Zinnen", "tokens": ["Pflanzt", "auf", "des", "frei\u00b7en", "Stra\u00df\u00b7burg", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des neuen deutschen Reichs Panier!", "tokens": ["Des", "neu\u00b7en", "deut\u0