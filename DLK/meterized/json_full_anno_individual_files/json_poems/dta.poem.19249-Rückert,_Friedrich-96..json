{"dta.poem.19249": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Augen, die getrennt im eignen Kreise stehn,", "tokens": ["Zwei", "Au\u00b7gen", ",", "die", "ge\u00b7trennt", "im", "eig\u00b7nen", "Krei\u00b7se", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "ADJD", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch dasselbe Ding als eins, nicht doppelt sehn,", "tokens": ["Und", "doch", "das\u00b7sel\u00b7be", "Ding", "als", "eins", ",", "nicht", "dop\u00b7pelt", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "NN", "KOUS", "PIS", "$,", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie sind das sch\u00f6nste Bild von zweier Seelen Innung,", "tokens": ["Sie", "sind", "das", "sch\u00f6ns\u00b7te", "Bild", "von", "zwei\u00b7er", "See\u00b7len", "In\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ganz zu einer macht grundeinige Gesinnung.", "tokens": ["Die", "ganz", "zu", "ei\u00b7ner", "macht", "grun\u00b7dei\u00b7ni\u00b7ge", "Ge\u00b7sin\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PIS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den gleichen Gegenstand sehn also gleich die beiden,", "tokens": ["Den", "glei\u00b7chen", "Ge\u00b7gen\u00b7stand", "sehn", "al\u00b7so", "gleich", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ART", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sie als zwei ihn gar nicht k\u00f6nnen unterscheiden.", "tokens": ["Das", "sie", "als", "zwei", "ihn", "gar", "nicht", "k\u00f6n\u00b7nen", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KOKOM", "CARD", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dis v\u00f6llig gleiche Sehn hat aber zur Bedingung", "tokens": ["Dis", "v\u00f6l\u00b7lig", "glei\u00b7che", "Sehn", "hat", "a\u00b7ber", "zur", "Be\u00b7din\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des innern Sehgewebs Durchdringung und Verschlingung.", "tokens": ["Des", "in\u00b7nern", "Seh\u00b7ge\u00b7webs", "Durch\u00b7drin\u00b7gung", "und", "Ver\u00b7schlin\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Anlag' und Stimmung ist sich so harmonisch gleich,", "tokens": ["An\u00b7lag'", "und", "Stim\u00b7mung", "ist", "sich", "so", "har\u00b7mo\u00b7nisch", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PRF", "ADV", "ADJD", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da\u00df ganz unm\u00f6glich wird Zwiespalt in ihrem Reich.", "tokens": ["Da\u00df", "ganz", "un\u00b7m\u00f6g\u00b7lich", "wird", "Zwies\u00b7palt", "in", "ih\u00b7rem", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und nie, wenn Krankheit nicht und Rausch den Frieden bricht,", "tokens": ["Und", "nie", ",", "wenn", "Krank\u00b7heit", "nicht", "und", "Rausch", "den", "Frie\u00b7den", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "PTKNEG", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kommt Doppelsichtigkeit in ihre Weltansicht.", "tokens": ["Kommt", "Dop\u00b7pel\u00b7sich\u00b7tig\u00b7keit", "in", "ih\u00b7re", "Welt\u00b7an\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}