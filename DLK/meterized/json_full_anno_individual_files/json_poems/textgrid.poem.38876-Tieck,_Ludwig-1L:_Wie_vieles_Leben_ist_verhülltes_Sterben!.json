{"textgrid.poem.38876": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie vieles Leben ist verh\u00fclltes Sterben!", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie vieles Leben ist verh\u00fclltes Sterben!", "tokens": ["Wie", "vie\u00b7les", "Le\u00b7ben", "ist", "ver\u00b7h\u00fcll\u00b7tes", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie mancher wird im Sterben erst erwachen!", "tokens": ["Wie", "man\u00b7cher", "wird", "im", "Ster\u00b7ben", "erst", "er\u00b7wa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie wen'ge nur die Gluth zur Flamme fachen!", "tokens": ["Wie", "wen'\u00b7ge", "nur", "die", "Gluth", "zur", "Flam\u00b7me", "fa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Seltne Lebensmuth mit Leben erben!", "tokens": ["Wie", "Selt\u00b7ne", "Le\u00b7bens\u00b7muth", "mit", "Le\u00b7ben", "er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie d\u00fcnken sich zu seyn, entfliehn dem herben", "tokens": ["Sie", "d\u00fcn\u00b7ken", "sich", "zu", "seyn", ",", "ent\u00b7fliehn", "dem", "her\u00b7ben"], "token_in