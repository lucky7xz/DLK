{"textgrid.poem.34787": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Damen, sie verstehen,", "tokens": ["Die\u00b7se", "Da\u00b7men", ",", "sie", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie man Dichter ehren mu\u00df:", "tokens": ["Wie", "man", "Dich\u00b7ter", "eh\u00b7ren", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gaben mir ein Mittagessen,", "tokens": ["Ga\u00b7ben", "mir", "ein", "Mit\u00b7ta\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir und meinem Genius.", "tokens": ["Mir", "und", "mei\u00b7nem", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ach! die Suppe war vortrefflich,", "tokens": ["Ach", "!", "die", "Sup\u00b7pe", "war", "vor\u00b7treff\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Wein hat mich erquickt,", "tokens": ["Und", "der", "Wein", "hat", "mich", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Gefl\u00fcgel, das war g\u00f6ttlich,", "tokens": ["Das", "Ge\u00b7fl\u00fc\u00b7gel", ",", "das", "war", "g\u00f6tt\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und der Hase war gespickt.", "tokens": ["Und", "der", "Ha\u00b7se", "war", "ge\u00b7spickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sprachen, glaub ich, von der Dichtkunst,", "tokens": ["Spra\u00b7chen", ",", "glaub", "ich", ",", "von", "der", "Dicht\u00b7kunst", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und ich wurde endlich satt;", "tokens": ["Und", "ich", "wur\u00b7de", "end\u00b7lich", "satt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich dankte f\u00fcr die Ehre,", "tokens": ["Und", "ich", "dank\u00b7te", "f\u00fcr", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die man mir erwiesen hat.", "tokens": ["Die", "man", "mir", "er\u00b7wie\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1"