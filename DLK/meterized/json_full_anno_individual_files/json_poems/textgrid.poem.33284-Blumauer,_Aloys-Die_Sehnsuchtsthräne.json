{"textgrid.poem.33284": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Die Sehnsuchtsthr\u00e4ne", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "B\u00e4nglich wird mir, und der Minne", "tokens": ["B\u00e4ng\u00b7lich", "wird", "mir", ",", "und", "der", "Min\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leiden wachen auf in mir; \u2013", "tokens": ["Lei\u00b7den", "wa\u00b7chen", "auf", "in", "mir", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rinne, warmes Thr\u00e4nchen, rinne,", "tokens": ["Rin\u00b7ne", ",", "war\u00b7mes", "Thr\u00e4n\u00b7chen", ",", "rin\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh, noch viele folgen dir.", "tokens": ["Sieh", ",", "noch", "vie\u00b7le", "fol\u00b7gen", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PIS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum weilet ihr so lange", "tokens": ["Wa\u00b7rum", "wei\u00b7let", "ihr", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den Augenwimpern mir?", "tokens": ["An", "den", "Au\u00b7gen\u00b7wim\u00b7pern", "mir", "?"], 