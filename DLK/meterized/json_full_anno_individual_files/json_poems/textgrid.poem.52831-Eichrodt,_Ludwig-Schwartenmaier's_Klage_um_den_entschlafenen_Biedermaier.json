de", "zei\u00b7ten\u00b7weis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freut den liebensw\u00fcrd'gen Greis.", "tokens": ["Freut", "den", "lie\u00b7bens\u00b7w\u00fcrd'\u00b7gen", "Greis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Brachten es die Jahreszeiten,", "tokens": ["Brach\u00b7ten", "es", "die", "Jah\u00b7res\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder sonst Gelegenheiten,", "tokens": ["O\u00b7der", "sonst", "Ge\u00b7le\u00b7gen\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ertrug und machte Spa\u00df,", "tokens": ["Wer", "er\u00b7trug", "und", "mach\u00b7te", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der Biedermaier ba\u00df?", "tokens": ["Als", "der", "Bie\u00b7der\u00b7mai\u00b7er", "ba\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Himmel! war die Lug', das Laster,", "tokens": ["Him\u00b7mel", "!", "war", "die", "Lug'", ",", "das", "Las\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "li