{"dta.poem.19300": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der gro\u00dfe Astronom sprach: Alle Himmelsflur", "tokens": ["Der", "gro\u00b7\u00dfe", "Ast\u00b7ro\u00b7nom", "sprach", ":", "Al\u00b7le", "Him\u00b7mels\u00b7flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab' ich durchforscht und nicht entdeckt von Gott die Spur.", "tokens": ["Hab'", "ich", "durch\u00b7forscht", "und", "nicht", "ent\u00b7deckt", "von", "Gott", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "KON", "PTKNEG", "VVPP", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat er nicht recht gesagt? Bei Mond- und Sonnenflecken,", "tokens": ["Hat", "er", "nicht", "recht", "ge\u00b7sagt", "?", "Bei", "Mon\u00b7d", "und", "Son\u00b7nen\u00b7fle\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$.", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Im Sternennebel dort, ist Gott nicht zu entdecken.", "tokens": ["Im", "Ster\u00b7nen\u00b7ne\u00b7bel", "dort", ",", "ist", "Gott", "nicht", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "VAFIN", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Des Sehrohrs Scharfblick sieht den Unsichtbaren nicht,", "tokens": ["Des", "Seh\u00b7rohrs", "Scharf\u00b7blick", "sieht", "den", "Un\u00b7sicht\u00b7ba\u00b7ren", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den nicht berechnen kann Zahl, Gr\u00f6\u00dfe, Ma\u00df, Gewicht.", "tokens": ["Den", "nicht", "be\u00b7rech\u00b7nen", "kann", "Zahl", ",", "Gr\u00f6\u00b7\u00dfe", ",", "Ma\u00df", ",", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "VMFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer Gott will finden dort, der mu\u00df ihn mit sich bringen;", "tokens": ["Wer", "Gott", "will", "fin\u00b7den", "dort", ",", "der", "mu\u00df", "ihn", "mit", "sich", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VVINF", "ADV", "$,", "ART", "VMFIN", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur wenn er ist in dir, siehst du ihn in den Dingen.", "tokens": ["Nur", "wenn", "er", "ist", "in", "dir", ",", "siehst", "du", "ihn", "in", "den", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VAFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}