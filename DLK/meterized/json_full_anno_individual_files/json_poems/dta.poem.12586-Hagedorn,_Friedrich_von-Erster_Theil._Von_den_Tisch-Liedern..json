{"dta.poem.12586": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gesundheit! vor allen den Gaben,", "tokens": ["Ge\u00b7sund\u00b7heit", "!", "vor", "al\u00b7len", "den", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Sterbliche w\u00fcnschen und haben,", "tokens": ["Die", "Sterb\u00b7li\u00b7che", "w\u00fcn\u00b7schen", "und", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VAFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nimmst du mit Recht den Vorzug ein.", "tokens": ["Nimmst", "du", "mit", "Recht", "den", "Vor\u00b7zug", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dir soll die Sch\u00f6nheit sich setzen.", "tokens": ["Nach", "dir", "soll", "die", "Sch\u00f6n\u00b7heit", "sich", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Euch redlich erworbenen Sch\u00e4tzen", "tokens": ["Euch", "red\u00b7lich", "er\u00b7wor\u00b7be\u00b7nen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Will ich die dritte Stelle weihn.", "tokens": ["Will", "ich", "die", "drit\u00b7te", "Stel\u00b7le", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was bleibt f\u00fcr die vierte zur\u00fccke?", "tokens": ["Was", "bleibt", "f\u00fcr", "die", "vier\u00b7te", "zu\u00b7r\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Ich wei\u00df schon; die geb\u2019 ich dem Gl\u00fccke,", "tokens": ["Ich", "wei\u00df", "schon", ";", "die", "geb'", "ich", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bey jungen Freunden jung zu seyn.", "tokens": ["Bey", "jun\u00b7gen", "Freun\u00b7den", "jung", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}