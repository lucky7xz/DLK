{"textgrid.poem.49235": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein schlauer Vogel kan des Stellers Leim entschleichen,", "tokens": ["Ein", "schlau\u00b7er", "Vo\u00b7gel", "kan", "des", "Stel\u00b7lers", "Leim", "ent\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Fisch schaut, da\u00df er nicht dem Netze nahe geht,", "tokens": ["Der", "Fisch", "schaut", ",", "da\u00df", "er", "nicht", "dem", "Net\u00b7ze", "na\u00b7he", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von wegen seiner Vers' ist sicher der Poet,", "tokens": ["Von", "we\u00b7gen", "sei\u00b7ner", "Ver\u00b7s'", "ist", "si\u00b7cher", "der", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Soldaten m\u00fcssen viel der Waffen halben weichen.", "tokens": ["Sol\u00b7da\u00b7ten", "m\u00fcs\u00b7sen", "viel", "der", "Waf\u00b7fen", "hal\u00b7ben", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ART", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa