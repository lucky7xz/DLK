{"textgrid.poem.55477": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "58", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wen ein guter Geist besessen,", "tokens": ["Wen", "ein", "gu\u00b7ter", "Geist", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt sich das Ged\u00e4chtnis rein;", "tokens": ["H\u00e4lt", "sich", "das", "Ge\u00b7d\u00e4cht\u00b7nis", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles \u00dcbel sei vergessen,", "tokens": ["Al\u00b7les", "\u00dc\u00b7bel", "sei", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingedenk der Lust zu sein!", "tokens": ["Ein\u00b7ge\u00b7denk", "der", "Lust", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleib ein fr\u00f6hliches Verm\u00e4chtnis", "tokens": ["Bleib", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Ver\u00b7m\u00e4cht\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jed Ergetzen, jede Ruh;", "tokens": ["Jed", "Er\u00b7get\u00b7zen", ",", "je\u00b7de", "Ruh", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."