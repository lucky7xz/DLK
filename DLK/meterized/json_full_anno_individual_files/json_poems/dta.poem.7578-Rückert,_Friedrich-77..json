{"dta.poem.7578": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus jungen Augen sieh die Welt stets neu entfaltet;", "tokens": ["Aus", "jun\u00b7gen", "Au\u00b7gen", "sieh", "die", "Welt", "stets", "neu", "ent\u00b7fal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Glaubs deinen alten nicht, sie sei mit dir gealtet.", "tokens": ["Glaubs", "dei\u00b7nen", "al\u00b7ten", "nicht", ",", "sie", "sei", "mit", "dir", "ge\u00b7al\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "PTKNEG", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein alter Vogel lernt nicht mehr; kommt her, ihr jungen,", "tokens": ["Ein", "al\u00b7ter", "Vo\u00b7gel", "lernt", "nicht", "mehr", ";", "kommt", "her", ",", "ihr", "jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADV", "$.", "VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und singen lernt von uns, doch nicht wie wir gesungen;", "tokens": ["Und", "sin\u00b7gen", "lernt", "von", "uns", ",", "doch", "nicht", "wie", "wir", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "APPR", "PPER", "$,", "ADV", "PTKNEG", "KOKOM", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nein, immer besser zu! denn Alles mu\u00df auf Erden", "tokens": ["Nein", ",", "im\u00b7mer", "bes\u00b7ser", "zu", "!", "denn", "Al\u00b7les", "mu\u00df", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "PTKVZ", "$.", "KON", "PIS", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch immer besser, auch der Sang der V\u00f6gel werden.", "tokens": ["Doch", "im\u00b7mer", "bes\u00b7ser", ",", "auch", "der", "Sang", "der", "V\u00f6\u00b7gel", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ADV", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und macht ihr's besser nicht, so denkt doch da\u00df ihr's macht;", "tokens": ["Und", "macht", "ih\u00b7r's", "bes\u00b7ser", "nicht", ",", "so", "denkt", "doch", "da\u00df", "ih\u00b7r's", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKNEG", "$,", "ADV", "VVFIN", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wir haben ebendas zu unsrer Zeit gedacht.", "tokens": ["Wir", "ha\u00b7ben", "e\u00b7ben\u00b7das", "zu", "uns\u00b7rer", "Zeit", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was ist die Aehnlichkeit, und was der Unterschied?", "tokens": ["Was", "ist", "die", "A\u00b7ehn\u00b7lich\u00b7keit", ",", "und", "was", "der", "Un\u00b7ter\u00b7schied", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "KON", "PWS", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wir sangen, und ihr singt, das neuste sch\u00f6nste Lied.", "tokens": ["Wir", "san\u00b7gen", ",", "und", "ihr", "singt", ",", "das", "neus\u00b7te", "sch\u00f6ns\u00b7te", "Lied", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}