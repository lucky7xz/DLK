{"dta.poem.8819": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Komm, denn ohne dich die Seele durch den Wein erlab'", "tokens": ["Komm", ",", "denn", "oh\u00b7ne", "dich", "die", "See\u00b7le", "durch", "den", "Wein", "er\u00b7lab'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "APPR", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Komm zu mir und nimm mein Leben, denn was Bess'res", "tokens": ["Komm", "zu", "mir", "und", "nimm", "mein", "Le\u00b7ben", ",", "denn", "was", "Bess'\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "KON", "VVIMP", "PPOSAT", "NN", "$,", "KON", "PWS", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Vor den Hufen deines Rosses streut' ich meine Lieder", "tokens": ["Vor", "den", "Hu\u00b7fen", "dei\u00b7nes", "Ros\u00b7ses", "streut'", "ich", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Doch du sprachst: Auf Steinen trab' ich, \u00fcber Perlen", "tokens": ["Doch", "du", "sprachst", ":", "Auf", "Stei\u00b7nen", "trab'", "ich", ",", "\u00fc\u00b7ber", "Per\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "APPR", "NN", "VVFIN", "P