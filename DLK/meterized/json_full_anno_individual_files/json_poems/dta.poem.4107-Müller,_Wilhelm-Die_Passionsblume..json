{"dta.poem.4107": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Passionsblume.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deren sch\u00f6ner Bl\u00fcthenstern", "tokens": ["De\u00b7ren", "sch\u00f6\u00b7ner", "Bl\u00fc\u00b7thens\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+----", "measure": "unknown.measure.di"}, "line.2": {"text": "Uns in mildem, wei\u00dfen Glanze", "tokens": ["Uns", "in", "mil\u00b7dem", ",", "wei\u00b7\u00dfen", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt das Marterthum des Herrn;", "tokens": ["Zeigt", "das", "Mar\u00b7ter\u00b7thum", "des", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Voller Bl\u00fcthen seh' ich immer", "tokens": ["Vol\u00b7ler", "Bl\u00fc\u00b7then", "seh'", "ich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich vor ihrem Fenster stehn:", "tokens": ["Dich", "vor", "ih\u00b7rem", "Fens\u00b7ter", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Willst du denn, als eitler Schimmer,", "tokens": ["Willst", "du", "denn", ",", "als", "eit\u00b7ler", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur in Farb' und Duft vergehn?", "tokens": ["Nur", "in", "Fa\u00b7rb'", "und", "Duft", "ver\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ward dir kein geheimes Leben,", "tokens": ["Ward", "dir", "kein", "ge\u00b7hei\u00b7mes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unverwelklicher Natur,", "tokens": ["Un\u00b7ver\u00b7wel\u00b7kli\u00b7cher", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Von dem Heiland eingegeben,", "tokens": ["Von", "dem", "Hei\u00b7land", "ein\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dich pflanzt' in unsre Flur,", "tokens": ["Der", "dich", "pflanzt'", "in", "uns\u00b7re", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ein Bild von seinen Leiden,", "tokens": ["Als", "ein", "Bild", "von", "sei\u00b7nen", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinem bittern Liebestod,", "tokens": ["Sei\u00b7nem", "bit\u00b7tern", "Lie\u00b7be\u00b7stod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df daran wir sollen weiden", "tokens": ["Da\u00df", "da\u00b7ran", "wir", "sol\u00b7len", "wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PAV", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Seel' in Lust und Noth?", "tokens": ["Uns\u00b7re", "Seel'", "in", "Lust", "und", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hast du nicht in stillen Stunden,", "tokens": ["Hast", "du", "nicht", "in", "stil\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil'ge Blum', ihr zugehaucht", "tokens": ["Heil'\u00b7ge", "Blum'", ",", "ihr", "zu\u00b7ge\u00b7haucht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Geheimni\u00df von den Wunden,", "tokens": ["Das", "Ge\u00b7heim\u00b7ni\u00df", "von", "den", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Dorn, in Blut getaucht?", "tokens": ["Von", "dem", "Dorn", ",", "in", "Blut", "ge\u00b7taucht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Esther schl\u00e4ft, und Tr\u00e4ume schlie\u00dfen", "tokens": ["E\u00b7sther", "schl\u00e4ft", ",", "und", "Tr\u00e4u\u00b7me", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der reinen Seele Schrein:", "tokens": ["Auf", "der", "rei\u00b7nen", "See\u00b7le", "Schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df aus deinem Sterne flie\u00dfen", "tokens": ["La\u00df", "aus", "dei\u00b7nem", "Ster\u00b7ne", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Strahl zu ihr hinein!", "tokens": ["Ei\u00b7nen", "Strahl", "zu", "ihr", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}