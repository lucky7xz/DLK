{"dta.poem.19374": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "107.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du willst nach dem Rath von jedem Thoren fragen,", "tokens": ["Wenn", "du", "willst", "nach", "dem", "Rath", "von", "je\u00b7dem", "Tho\u00b7ren", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du wie jener Mann zuletzt den Esel tragen;", "tokens": ["Wirst", "du", "wie", "je\u00b7ner", "Mann", "zu\u00b7letzt", "den", "E\u00b7sel", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "PDAT", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "(die Fabel ist bekannt) der wandernd seinen Sohn", "tokens": ["(", "die", "Fa\u00b7bel", "ist", "be\u00b7kannt", ")", "der", "wan\u00b7dernd", "sei\u00b7nen", "Sohn"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$(", "ART", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst auf den Esel lud, der war beladen schon.", "tokens": ["Erst", "auf", "den", "E\u00b7sel", "lud", ",", "der", "war", "be\u00b7la\u00b7den", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der erste, der es sah, sprach: O verkehrte Sitten!", "tokens": ["Der", "ers\u00b7te", ",", "der", "es", "sah", ",", "sprach", ":", "O", "ver\u00b7kehr\u00b7te", "Sit\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "NE", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Vater geht zu Fu\u00df, das S\u00f6hnchen ist beritten.", "tokens": ["Der", "Va\u00b7ter", "geht", "zu", "Fu\u00df", ",", "das", "S\u00f6hn\u00b7chen", "ist", "be\u00b7rit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da setzt der Vater sich, dem Esel d\u00fcnkt es schwer,", "tokens": ["Da", "setzt", "der", "Va\u00b7ter", "sich", ",", "dem", "E\u00b7sel", "d\u00fcnkt", "es", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anstatt des Sohnes auf, der Sohn l\u00e4uft nebenher.", "tokens": ["An\u00b7statt", "des", "Soh\u00b7nes", "auf", ",", "der", "Sohn", "l\u00e4uft", "ne\u00b7ben\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+----", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Ein andrer, der es sah, sprach: Welcher Thorenritt!", "tokens": ["Ein", "an\u00b7drer", ",", "der", "es", "sah", ",", "sprach", ":", "Wel\u00b7cher", "Tho\u00b7ren\u00b7ritt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Vater reitet fort und nimmt den Sohn nicht mit.", "tokens": ["Der", "Va\u00b7ter", "rei\u00b7tet", "fort", "und", "nimmt", "den", "Sohn", "nicht", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Vater nimmt geschwind den Sohn zu sich hinauf,", "tokens": ["Der", "Va\u00b7ter", "nimmt", "ge\u00b7schwind", "den", "Sohn", "zu", "sich", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit der Doppellast der Esel stockt im Lauf.", "tokens": ["Und", "mit", "der", "Dop\u00b7pel\u00b7last", "der", "E\u00b7sel", "stockt", "im", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der dritte, der es sah, sprach: Welche Barbarei!", "tokens": ["Der", "drit\u00b7te", ",", "der", "es", "sah", ",", "sprach", ":", "Wel\u00b7che", "Bar\u00b7ba\u00b7rei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Thier erliegt, wenn ihr nicht absteigt alle zwei.", "tokens": ["Das", "Thier", "er\u00b7liegt", ",", "wenn", "ihr", "nicht", "ab\u00b7steigt", "al\u00b7le", "zwei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "PIAT", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Vater steigt zugleich mit seinem Sohn hernieder;", "tokens": ["Der", "Va\u00b7ter", "steigt", "zu\u00b7gleich", "mit", "sei\u00b7nem", "Sohn", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der tr\u00e4ge Esel r\u00fchrt nicht flinker drum die Glieder.", "tokens": ["Der", "tr\u00e4\u00b7ge", "E\u00b7sel", "r\u00fchrt", "nicht", "flin\u00b7ker", "drum", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADJD", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der vierte, der es sah, sprach: Viel geschwinder k\u00e4met", "tokens": ["Der", "vier\u00b7te", ",", "der", "es", "sah", ",", "sprach", ":", "Viel", "ge\u00b7schwin\u00b7der", "k\u00e4\u00b7met"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr fort, wenn ihr die Last dem armen Thier abn\u00e4hmet.", "tokens": ["Ihr", "fort", ",", "wenn", "ihr", "die", "Last", "dem", "ar\u00b7men", "Thier", "ab\u00b7n\u00e4h\u00b7met", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Der Vater mit dem Sohn nimmt auf sich das Gep\u00e4ck,", "tokens": ["Der", "Va\u00b7ter", "mit", "dem", "Sohn", "nimmt", "auf", "sich", "das", "Ge\u00b7p\u00e4ck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das entladne Thier will gar nicht mehr vom Fleck.", "tokens": ["Und", "das", "ent\u00b7lad\u00b7ne", "Thier", "will", "gar", "nicht", "mehr", "vom", "Fleck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Da sprach der f\u00fcnfte, der es sah, der war ein Gauch:", "tokens": ["Da", "sprach", "der", "f\u00fcnf\u00b7te", ",", "der", "es", "sah", ",", "der", "war", "ein", "Gauch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tragt ihr des Esels Last, tragt doch den Esel auch!", "tokens": ["Tragt", "ihr", "des", "E\u00b7sels", "Last", ",", "tragt", "doch", "den", "E\u00b7sel", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$,", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Den Esel packten Sohn und Vater hier und da,", "tokens": ["Den", "E\u00b7sel", "pack\u00b7ten", "Sohn", "und", "Va\u00b7ter", "hier", "und", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und trugen ihn ins Dorf, es war zum Gl\u00fccke nah.", "tokens": ["Und", "tru\u00b7gen", "ihn", "ins", "Dorf", ",", "es", "war", "zum", "Gl\u00fc\u00b7cke", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}