{"textgrid.poem.34017": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Apollo von Belvedere", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sch\u00f6n, wie du, ist, soll dich einst zerschlagen!", "tokens": ["Wer", "sch\u00f6n", ",", "wie", "du", ",", "ist", ",", "soll", "dich", "einst", "zer\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PWAV", "PPER", "$,", "VAFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sprach der Meister, als er dich vollendet", "tokens": ["So", "sprach", "der", "Meis\u00b7ter", ",", "als", "er", "dich", "voll\u00b7en\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und vor dir stand, von deinem Glanz geblendet:", "tokens": ["Und", "vor", "dir", "stand", ",", "von", "dei\u00b7nem", "Glanz", "ge\u00b7blen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er hatte Nichts bei diesem Wort zu wagen.", "tokens": ["Er", "hat\u00b7te", "Nichts", "bei", "die\u00b7sem", "Wort", "zu", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn, wen auch noch seit deines Ursprungs Tagen", "tokens": ["Denn", ",", "wen", "auch", "noch", "seit", "dei\u00b7nes", "Ur\u00b7sprungs", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die neidische Natur hieher gesendet,", "tokens": ["Die", "nei\u00b7di\u00b7sche", "Na\u00b7tur", "hie\u00b7her", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier hat sich immer sein Triumph geendet,", "tokens": ["Hier", "hat", "sich", "im\u00b7mer", "sein", "Tri\u00b7umph", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein J\u00fcngling stand noch vor dir, als mit Zagen.", "tokens": ["Kein", "J\u00fcng\u00b7ling", "stand", "noch", "vor", "dir", ",", "als", "mit", "Za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,", "KOUS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, k\u00f6nnte selbst in Zukunft Einer kommen,", "tokens": ["Ja", ",", "k\u00f6nn\u00b7te", "selbst", "in", "Zu\u00b7kunft", "Ei\u00b7ner", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "ADV", "APPR", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir gleich und dennoch f\u00e4hig, dich zu hassen,", "tokens": ["Dir", "gleich", "und", "den\u00b7noch", "f\u00e4\u00b7hig", ",", "dich", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er w\u00fcrde nimmer b\u00fc\u00dfen sein Gel\u00fcste:", "tokens": ["Er", "w\u00fcr\u00b7de", "nim\u00b7mer", "b\u00fc\u00b7\u00dfen", "sein", "Ge\u00b7l\u00fcs\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er h\u00e4tte kaum die Axt zur Hand genommen,", "tokens": ["Er", "h\u00e4t\u00b7te", "kaum", "die", "Axt", "zur", "Hand", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So m\u00fc\u00dft' er sie schon wieder fallen lassen,", "tokens": ["So", "m\u00fc\u00dft'", "er", "sie", "schon", "wie\u00b7der", "fal\u00b7len", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil er schon dadurch h\u00e4\u00dflich werden m\u00fc\u00dfte.", "tokens": ["Weil", "er", "schon", "da\u00b7durch", "h\u00e4\u00df\u00b7lich", "wer\u00b7den", "m\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer sch\