{"textgrid.poem.43990": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wiltu mir dein Angedencken", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiltu mir dein Angedencken", "tokens": ["Wil\u00b7tu", "mir", "dein", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur noch mit zur Reise schencken,", "tokens": ["Nur", "noch", "mit", "zur", "Rei\u00b7se", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh ich auf ein schweres Wort", "tokens": ["Geh", "ich", "auf", "ein", "schwe\u00b7res", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch einmahl so freudig fort.", "tokens": ["Noch", "ein\u00b7mahl", "so", "freu\u00b7dig", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Solche Wunden m\u00fc\u00dfen schmerzen,", "tokens": ["Sol\u00b7che", "Wun\u00b7den", "m\u00fc\u00b7\u00dfen", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Qual zerri\u00dfner Herzen", "tokens": ["Wenn", "die", "Qual", "zer\u00b7ri\u00df\u00b7ner", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der lezten guten Nacht", "tokens": ["Mit", "der", "lez\u00b7ten", "gu\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den K\u00fc\u00dfen Seufzer macht.", "tokens": ["Aus", "den", "K\u00fc\u00b7\u00dfen", "Seuf\u00b7zer", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich dich ins Blut geschrieben,", "tokens": ["Da\u00df", "ich", "dich", "ins", "Blut", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das bezeugt mein treues Lieben,", "tokens": ["Das", "be\u00b7zeugt", "mein", "treu\u00b7es", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "De\u00dfen angenehmer List", "tokens": ["De\u00b7\u00dfen", "an\u00b7ge\u00b7neh\u00b7mer", "List"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Freyheit dienstbahr ist.", "tokens": ["Dei\u00b7ne", "Frey\u00b7heit", "dienst\u00b7bahr", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deiner Augen scharfe Blicke", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "schar\u00b7fe", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die unsichtbahren Stricke,", "tokens": ["Sind", "die", "un\u00b7sicht\u00b7bah\u00b7ren", "Stri\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die du mir ans Herz gelegt,", "tokens": ["Die", "du", "mir", "ans", "Herz", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mir jezt vor Wehmuth schl\u00e4gt.", "tokens": ["Das", "mir", "jezt", "vor", "Weh\u00b7muth", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zung und Sprache stockt im Munde,", "tokens": ["Zung", "und", "Spra\u00b7che", "stockt", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da des Abschieds schwere Stunde", "tokens": ["Da", "des", "Ab\u00b7schieds", "schwe\u00b7re", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Schlag von Donner klingt", "tokens": ["Wie", "ein", "Schlag", "von", "Don\u00b7ner", "klingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mich mit Verdru\u00df umringt.", "tokens": ["Und", "mich", "mit", "Ver\u00b7dru\u00df", "um\u00b7ringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, was werden meine Sinnen", "tokens": ["Ach", ",", "was", "wer\u00b7den", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor Gefahr und Angst gewinnen,", "tokens": ["Vor", "Ge\u00b7fahr", "und", "Angst", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mich dein entfernter Geist", "tokens": ["Wenn", "mich", "dein", "ent\u00b7fern\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur mit blo\u00dfen Tr\u00e4umen speist.", "tokens": ["Nur", "mit", "blo\u00b7\u00dfen", "Tr\u00e4u\u00b7men", "speist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unterde\u00dfen mu\u00df ich leiden,", "tokens": ["Un\u00b7ter\u00b7de\u00b7\u00dfen", "mu\u00df", "ich", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir Gl\u00fcck und Zeit bescheiden;", "tokens": ["Was", "mir", "Gl\u00fcck", "und", "Zeit", "be\u00b7schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Schmerzen und Verdru\u00df", "tokens": ["Die\u00b7ser", "Schmer\u00b7zen", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat den ganzen Trost: Ich mu\u00df.", "tokens": ["Hat", "den", "gan\u00b7zen", "Trost", ":", "Ich", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ja, ich mu\u00df, doch wider Willen;", "tokens": ["Ja", ",", "ich", "mu\u00df", ",", "doch", "wi\u00b7der", "Wil\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt dich also nur im Stillen", "tokens": ["Halt", "dich", "al\u00b7so", "nur", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erwarthe, bis ein Tag", "tokens": ["Und", "er\u00b7wart\u00b7he", ",", "bis", "ein", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Liebe cr\u00f6nen mag.", "tokens": ["Uns\u00b7re", "Lie\u00b7be", "cr\u00f6\u00b7nen", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Bis mich Sarg und Staub umfangen,", "tokens": ["Bis", "mich", "Sarg", "und", "Staub", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt nur Phillis mein Verlangen,", "tokens": ["Bleibt", "nur", "Phil\u00b7lis", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Dauer meiner Treu", "tokens": ["Und", "die", "Dau\u00b7er", "mei\u00b7ner", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4ft mir noch im Grabe bey.", "tokens": ["Schl\u00e4ft", "mir", "noch", "im", "Gra\u00b7be", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "L\u00e4st auch du dich nicht verf\u00fchren,", "tokens": ["L\u00e4st", "auch", "du", "dich", "nicht", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll mich diese Grabschrift zieren:", "tokens": ["Soll", "mich", "die\u00b7se", "Grab\u00b7schrift", "zie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses hier verscharrte Blut", "tokens": ["Die\u00b7ses", "hier", "ver\u00b7scharr\u00b7te", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hegt noch in der Asche Glut.", "tokens": ["Hegt", "noch", "in", "der", "A\u00b7sche", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wiltu mir dein Angedencken", "tokens": ["Wil\u00b7tu", "mir", "dein", "An\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur noch mit zur Reise schencken,", "tokens": ["Nur", "noch", "mit", "zur", "Rei\u00b7se", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh ich auf ein schweres Wort", "tokens": ["Geh", "ich", "auf", "ein", "schwe\u00b7res", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch einmahl so freudig fort.", "tokens": ["Noch", "ein\u00b7mahl", "so", "freu\u00b7dig", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Solche Wunden m\u00fc\u00dfen schmerzen,", "tokens": ["Sol\u00b7che", "Wun\u00b7den", "m\u00fc\u00b7\u00dfen", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Qual zerri\u00dfner Herzen", "tokens": ["Wenn", "die"