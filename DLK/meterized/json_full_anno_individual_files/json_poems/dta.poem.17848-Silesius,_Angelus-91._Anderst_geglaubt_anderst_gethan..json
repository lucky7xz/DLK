{"dta.poem.17848": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "91.  Anderst geglaubt/ anderst gethan.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Man glaubt es seeger seyn zu geben als zu nehmen;", "tokens": ["Man", "glaubt", "es", "see\u00b7ger", "seyn", "zu", "ge\u00b7ben", "als", "zu", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "VAINF", "PTKZU", "VVINF", "KOKOM", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch wil man gar schlecht zum geben sich bequ\u00e4men.", "tokens": ["Und", "doch", "wil", "man", "gar", "schlecht", "zum", "ge\u00b7ben", "sich", "be\u00b7qu\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "ADV", "ADJD", "APPRART", "VVFIN", "PRF", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}}}}