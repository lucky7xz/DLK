{"dta.poem.20299": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Als Flavia einsmahls an einem  \n groben sack arbeitete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was macht doch Flavia mit ihrer weissen hand?", "tokens": ["Was", "macht", "doch", "Fla\u00b7via", "mit", "ih\u00b7rer", "weis\u00b7sen", "hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bald hebet sie sie auff/ bald sencket sie sie nieder.", "tokens": ["Bald", "he\u00b7bet", "sie", "sie", "auff", "/", "bald", "sen\u00b7cket", "sie", "sie", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich deucht/ ein grobes tuch/ ein nichtiges gewand", "tokens": ["Mich", "deucht", "/", "ein", "gro\u00b7bes", "tuch", "/", "ein", "nich\u00b7ti\u00b7ges", "ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bem\u00fcht den sch\u00f6nen arm/ und plagt die zarten glieder.", "tokens": ["Be\u00b7m\u00fcht", "den", "sch\u00f6\u00b7nen", "arm", "/", "und", "plagt", "die", "zar\u00b7ten", "glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJD", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach sch\u00f6nste Flavia/ so mu\u00df denn deinen muth", "tokens": ["Ach", "sch\u00f6ns\u00b7te", "Fla\u00b7via", "/", "so", "mu\u00df", "denn", "dei\u00b7nen", "muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "NE", "$(", "ADV", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ein ungestalter zwirn und schlechte leinwand binden?", "tokens": ["Ein", "un\u00b7ge\u00b7stal\u00b7ter", "zwirn", "und", "schlech\u00b7te", "lein\u00b7wand", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch weil dein werther leib im sacke busse thut/", "tokens": ["Doch", "weil", "dein", "wert\u00b7her", "leib", "im", "sa\u00b7cke", "bus\u00b7se", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So sage mir doch auch den zufall deiner s\u00fcnden.", "tokens": ["So", "sa\u00b7ge", "mir", "doch", "auch", "den", "zu\u00b7fall", "dei\u00b7ner", "s\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich irre. Flavia will lebens-g\u00f6ttin seyn;", "tokens": ["Ich", "ir\u00b7re", ".", "Fla\u00b7via", "will", "le\u00b7bens\u00b7g\u00f6t\u00b7tin", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "VMFIN", "NE", "VAINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der faden/ den sie zeucht/ tr\u00e4gt tausend m\u00e4nner leben/", "tokens": ["Der", "fa\u00b7den", "/", "den", "sie", "zeucht", "/", "tr\u00e4gt", "tau\u00b7send", "m\u00e4n\u00b7ner", "le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ART", "PPER", "VVFIN", "$(", "VVFIN", "CARD", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch f\u00fchrt sie ihn mit flei\u00df nicht allzu zart und klein/", "tokens": ["Doch", "f\u00fchrt", "sie", "ihn", "mit", "flei\u00df", "nicht", "all\u00b7zu", "zart", "und", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "PTKNEG", "PTKA", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es m\u00f6chte sonst zu viel allhier der leichen geben.", "tokens": ["Es", "m\u00f6ch\u00b7te", "sonst", "zu", "viel", "all\u00b7hier", "der", "lei\u00b7chen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKA", "PIS", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}