{"dta.poem.21212": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Allmacht Sohn hat \u00fcberwunden!", "tokens": ["Der", "All\u00b7macht", "Sohn", "hat", "\u00fc\u00b7berw\u00b7un\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir preisen dich, sieghafter Held,", "tokens": ["Wir", "prei\u00b7sen", "dich", ",", "sieg\u00b7haf\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bedecket mit glorreichen Wunden,", "tokens": ["Be\u00b7de\u00b7cket", "mit", "glor\u00b7rei\u00b7chen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcrs Heil von einer ganzen Welt!", "tokens": ["F\u00fcrs", "Heil", "von", "ei\u00b7ner", "gan\u00b7zen", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Freche von unheilgen Saamen,", "tokens": ["Der", "Fre\u00b7che", "von", "un\u00b7heil\u00b7gen", "Saa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der, Tugend, dein Gef\u00fchl verlohr,", "tokens": ["Der", ",", "Tu\u00b7gend", ",", "dein", "Ge\u00b7f\u00fchl", "ver\u00b7lohr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Entweihe nicht der Christen Namen,", "tokens": ["Ent\u00b7wei\u00b7he", "nicht", "der", "Chris\u00b7ten", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und singe nicht in unser Chor!", "tokens": ["Und", "sin\u00b7ge", "nicht", "in", "un\u00b7ser", "Chor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}