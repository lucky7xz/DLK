{"dta.poem.13225": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu Sohn, mein Schwieger-Sohn! besteig bald jenen Thron", "tokens": ["\u201e", "du", "Sohn", ",", "mein", "Schwie\u00b7ger\u00b7Sohn", "!", "be\u00b7steig", "bald", "je\u00b7nen", "Thron"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eden mein Gemahl besa\u00df! erwirb auch jene Kron,", "tokens": ["\u201e", "den", "mein", "Ge\u00b7mahl", "be\u00b7sa\u00df", "!", "er\u00b7wirb", "auch", "je\u00b7ne", "Kron", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ef\u00fcr die dein Stammen-Haus schon einen Wall erbaute,", "tokens": ["\u201e", "f\u00fcr", "die", "dein", "Stam\u00b7men\u00b7Haus", "schon", "ei\u00b7nen", "Wall", "er\u00b7bau\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eals ihm die Christenheit die Rettung anvertraute.", "tokens": ["\u201e", "als", "ihm", "die", "Chris\u00b7ten\u00b7heit", "die", "Ret\u00b7tung", "an\u00b7ver\u00b7trau\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "w