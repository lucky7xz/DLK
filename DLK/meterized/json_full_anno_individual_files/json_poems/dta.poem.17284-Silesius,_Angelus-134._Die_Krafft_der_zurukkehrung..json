{"dta.poem.17284": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "134.  Die Krafft der zurukkehrung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wann du dich meine Seel zuruk hinein begiebst/", "tokens": ["Wann", "du", "dich", "mei\u00b7ne", "Seel", "zu\u00b7ruk", "hin\u00b7ein", "be\u00b7giebst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PPOSAT", "NN", "NE", "APZR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wirstu was du warst/ und was du Ehrst und Liebst.", "tokens": ["So", "wirs\u00b7tu", "was", "du", "warst", "/", "und", "was", "du", "Ehrst", "und", "Liebst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "PPER", "VAFIN", "$(", "KON", "PWS", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}