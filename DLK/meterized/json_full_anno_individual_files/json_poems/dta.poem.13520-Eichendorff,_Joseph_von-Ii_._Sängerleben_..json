{"dta.poem.13520": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .   S\u00e4ngerleben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Singen kann ich nicht wie Du", "tokens": ["Sin\u00b7gen", "kann", "ich", "nicht", "wie", "Du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "KOKOM", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie ich nicht der und jener,", "tokens": ["Und", "wie", "ich", "nicht", "der", "und", "je\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PTKNEG", "ART", "KON", "PDS", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kannst Du's besser, sing' frisch zu!", "tokens": ["Kannst", "Du's", "bes\u00b7ser", ",", "sing'", "frisch", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADJD", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Andre singen wieder sch\u00f6ner,", "tokens": ["And\u00b7re", "sin\u00b7gen", "wie\u00b7der", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Droben an dem Himmelsthor", "tokens": ["Dro\u00b7ben", "an", "dem", "Him\u00b7mel\u00b7sthor"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird's ", "tokens": ["Wird's"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}}}}