{"dta.poem.14462": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An den heiligen Joseph.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Seitdem ist wohl Viel anders worden,", "tokens": ["Seit\u00b7dem", "ist", "wohl", "Viel", "an\u00b7ders", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Treulieb auf Erden ist ausgestorben.", "tokens": ["Treu\u00b7lieb", "auf", "Er\u00b7den", "ist", "aus\u00b7ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wem k\u00f6nnt' ich's, au\u00dfer Dir, wohl klagen,", "tokens": ["Wem", "k\u00f6nnt'", "ich's", ",", "au\u00b7\u00dfer", "Dir", ",", "wohl", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "$,", "APPR", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie oft in kummervollen Tagen", "tokens": ["Wie", "oft", "in", "kum\u00b7mer\u00b7vol\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein ganzes Herz hier hofft und bangt,", "tokens": ["Mein", "gan\u00b7zes", "Herz", "hier", "hofft", "und", "bangt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nach der Heimath immer fort verlangt!", "tokens": ["Und", "nach", "der", "Hei\u00b7math", "im\u00b7mer", "fort", "ver\u00b7langt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}