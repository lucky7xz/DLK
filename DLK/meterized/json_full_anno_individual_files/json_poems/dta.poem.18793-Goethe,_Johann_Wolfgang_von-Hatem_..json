{"dta.poem.18793": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hatem .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Locken! haltet mich gefangen", "tokens": ["Lo\u00b7cken", "!", "hal\u00b7tet", "mich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Kreise des Gesichts!", "tokens": ["In", "dem", "Krei\u00b7se", "des", "Ge\u00b7sichts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch geliebten braunen Schlangen", "tokens": ["Euch", "ge\u00b7lieb\u00b7ten", "brau\u00b7nen", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu erwiedern hab\u2019 ich nichts.", "tokens": ["Zu", "er\u00b7wie\u00b7dern", "hab'", "ich", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur dies Herz es ist von Dauer,", "tokens": ["Nur", "dies", "Herz", "es", "ist", "von", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwillt in jugendlichstem Flor;", "tokens": ["Schwillt", "in", "ju\u00b7gend\u00b7lichs\u00b7tem", "Flor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Schnee und Nebelschauer", "tokens": ["Un\u00b7ter", "Schnee", "und", "Ne\u00b7bel\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rast ein Aetna dir hervor.", "tokens": ["Rast", "ein", "A\u00b7et\u00b7na", "dir", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Du besch\u00e4mst wie Morgenr\u00f6the", "tokens": ["Du", "be\u00b7sch\u00e4mst", "wie", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Gipfel ernste Wand,", "tokens": ["Je\u00b7ner", "Gip\u00b7fel", "erns\u00b7te", "Wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und noch einmal f\u00fchlet Hatem", "tokens": ["Und", "noch", "ein\u00b7mal", "f\u00fch\u00b7let", "Ha\u00b7tem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchlingshauch und Sommerbrand.", "tokens": ["Fr\u00fch\u00b7lings\u00b7hauch", "und", "Som\u00b7mer\u00b7brand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schenke her! Noch eine Flasche!", "tokens": ["Schen\u00b7ke", "her", "!", "Noch", "ei\u00b7ne", "Fla\u00b7sche", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Becher bring ich Ihr!", "tokens": ["Die\u00b7sen", "Be\u00b7cher", "bring", "ich", "Ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Findet sie ein H\u00e4ufchen Asche,", "tokens": ["Fin\u00b7det", "sie", "ein", "H\u00e4uf\u00b7chen", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt sie: der verbrannte mir.", "tokens": ["Sagt", "sie", ":", "der", "ver\u00b7brann\u00b7te", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ART", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}