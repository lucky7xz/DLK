{"dta.poem.11922": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der du im Lichte bist, und bist in mir das Licht,", "tokens": ["Der", "du", "im", "Lich\u00b7te", "bist", ",", "und", "bist", "in", "mir", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VAFIN", "$,", "KON", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nehme was du gibst, und andres will ich nicht.", "tokens": ["Ich", "neh\u00b7me", "was", "du", "gibst", ",", "und", "and\u00b7res", "will", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$,", "KON", "PIS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du gabest mir den Drang, so klar dein Lob zu sagen,", "tokens": ["Du", "ga\u00b7best", "mir", "den", "Drang", ",", "so", "klar", "dein", "Lob", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Mund und Ohr von mir und Welt es konnt' ertragen.", "tokens": ["Als", "Mund", "und", "Ohr", "von", "mir", "und", "Welt", "es", "konnt'", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "KON", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du gabest mir die Kunst, nicht sch\u00f6ner uns zu l\u00fcgen,", "tokens": ["Du", "ga\u00b7best", "mir", "die", "Kunst", ",", "nicht", "sch\u00f6\u00b7ner", "uns", "zu", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PTKNEG", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als, Welt und ich, wir sind, doch sch\u00f6ner uns zu f\u00fcgen.", "tokens": ["Als", ",", "Welt", "und", "ich", ",", "wir", "sind", ",", "doch", "sch\u00f6\u00b7ner", "uns", "zu", "f\u00fc\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NN", "KON", "PPER", "$,", "PPER", "VAFIN", "$,", "ADV", "ADJA", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das bleibe mir bewu\u00dft: Nur Gottes Macht besiegeln", "tokens": ["Das", "blei\u00b7be", "mir", "be\u00b7wu\u00dft", ":", "Nur", "Got\u00b7tes", "Macht", "be\u00b7sie\u00b7geln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$.", "ADV", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wollt' ich in der Natur, nicht drin mich g\u00f6ttlich spiegeln.", "tokens": ["Wollt'", "ich", "in", "der", "Na\u00b7tur", ",", "nicht", "drin", "mich", "g\u00f6tt\u00b7lich", "spie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "$,", "PTKNEG", "ADV", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und darum dank' ich dir f\u00fcr jeden hellen Blick,", "tokens": ["Und", "da\u00b7rum", "dank'", "ich", "dir", "f\u00fcr", "je\u00b7den", "hel\u00b7len", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den du mich lie\u00dfest thun in Leben-Tod-Geschick.", "tokens": ["Den", "du", "mich", "lie\u00b7\u00dfest", "thun", "in", "Le\u00b7ben\u00b7To\u00b7dGe\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJD", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich danke dir, da\u00df du die Augen mir erschlossen,", "tokens": ["Ich", "dan\u00b7ke", "dir", ",", "da\u00df", "du", "die", "Au\u00b7gen", "mir", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die von au\u00dfen auch dein Glanz in mich geflossen.", "tokens": ["Durch", "die", "von", "au\u00b7\u00dfen", "auch", "dein", "Glanz", "in", "mich", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ADV", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich will, solange mir zum Sehn die Augen taugen,", "tokens": ["Ich", "will", ",", "so\u00b7lan\u00b7ge", "mir", "zum", "Sehn", "die", "Au\u00b7gen", "tau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur deinen Glanz aus Stern- und Blumenaugen saugen.", "tokens": ["Nur", "dei\u00b7nen", "Glanz", "aus", "Stern", "und", "Blu\u00b7men\u00b7au\u00b7gen", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "TRUNC", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und soll dem Auge nun das \u00e4u\u00dfre Licht erblinden,", "tokens": ["Und", "soll", "dem", "Au\u00b7ge", "nun", "das", "\u00e4u\u00df\u00b7re", "Licht", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df als innres dich in meiner Seele finden.", "tokens": ["So", "la\u00df", "als", "inn\u00b7res", "dich", "in", "mei\u00b7ner", "See\u00b7le", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "KOUS", "PIS", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ich habe gnug gesehn, um lebenslang zu malen", "tokens": ["Ich", "ha\u00b7be", "gnug", "ge\u00b7sehn", ",", "um", "le\u00b7bens\u00b7lang", "zu", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KOUI", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Bild, wie dein Gesch\u00f6pf nicht stralt, doch sollte stralen.", "tokens": ["Ein", "Bild", ",", "wie", "dein", "Ge\u00b7sch\u00f6pf", "nicht", "stralt", ",", "doch", "soll\u00b7te", "stra\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}