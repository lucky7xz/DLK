{"dta.poem.11769": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo naht der s\u00fc\u00dfe Strom dem bittern Flutenschoo\u00dfe,", "tokens": ["Wo", "naht", "der", "s\u00fc\u00b7\u00dfe", "Strom", "dem", "bit\u00b7tern", "Flu\u00b7ten\u00b7schoo\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begegnen sich zwei Fisch', ein kleiner und der gro\u00dfe.", "tokens": ["Be\u00b7geg\u00b7nen", "sich", "zwei", "Fisch'", ",", "ein", "klei\u00b7ner", "und", "der", "gro\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "CARD", "NN", "$,", "ART", "ADJA", "KON", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Entgegen schwimmen sie sich so auf ihrer Bahn,", "tokens": ["Ent\u00b7ge\u00b7gen", "schwim\u00b7men", "sie", "sich", "so", "auf", "ih\u00b7rer", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Alswie von hier und dort ein Meerschiff und ein Kahn.", "tokens": ["Als\u00b7wie", "von", "hier", "und", "dort", "ein", "Meer\u00b7schiff", "und", "ein", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "KON", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und w\u00e4hrend um ihr Haupt die Wasserorgeln summen,", "tokens": ["Und", "w\u00e4h\u00b7rend", "um", "ihr", "Haupt", "die", "Was\u00b7ser\u00b7or\u00b7geln", "sum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begr\u00fc\u00dfen in der Flut sich laut die beiden Stummen.", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfen", "in", "der", "Flut", "sich", "laut", "die", "bei\u00b7den", "Stum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mein Vetter, ei, wohin? Mein Bruder, ei, woher?", "tokens": ["Mein", "Vet\u00b7ter", ",", "ei", ",", "wo\u00b7hin", "?", "Mein", "Bru\u00b7der", ",", "ei", ",", "wo\u00b7her", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$,", "PWAV", "$.", "PPOSAT", "NN", "$,", "ITJ", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich aus dem Meer ins Land. Ich aus dem Land ins Meer.", "tokens": ["Ich", "aus", "dem", "Meer", "ins", "Land", ".", "Ich", "aus", "dem", "Land", "ins", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPRART", "NN", "$.", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was f\u00fchret dich so fern? Was treibet dich so weit?", "tokens": ["Was", "f\u00fch\u00b7ret", "dich", "so", "fern", "?", "Was", "trei\u00b7bet", "dich", "so", "weit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "$.", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Hoffnung bessrer Stern. Die Unzufriedenheit.", "tokens": ["Der", "Hoff\u00b7nung", "bess\u00b7rer", "Stern", ".", "Die", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich will ins stille Land aus Wogenaufruhr steuern,", "tokens": ["Ich", "will", "ins", "stil\u00b7le", "Land", "aus", "Wo\u00b7gen\u00b7auf\u00b7ruhr", "steu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um zu entgehn des Meers gefr\u00e4\u00df'gen Ungeheuern.", "tokens": ["Um", "zu", "ent\u00b7gehn", "des", "Meers", "ge\u00b7fr\u00e4\u00df'\u00b7gen", "Un\u00b7ge\u00b7heu\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich will mich aus der Eng' hinaus ins Weite fristen,", "tokens": ["Ich", "will", "mich", "aus", "der", "Eng'", "hin\u00b7aus", "ins", "Wei\u00b7te", "fris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "APZR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entgehn des Menschenvolks Nachstellungen und Listen.", "tokens": ["Ent\u00b7gehn", "des", "Men\u00b7schen\u00b7volks", "Nach\u00b7stel\u00b7lun\u00b7gen", "und", "Lis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Das trieb dich, Vetter? Das hat, Bruder, dich gezogen?", "tokens": ["Das", "trieb", "dich", ",", "Vet\u00b7ter", "?", "Das", "hat", ",", "Bru\u00b7der", ",", "dich", "ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "NN", "$.", "PDS", "VAFIN", "$,", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hoffnung t\u00e4uschte dich. Du hast dich selbst betrogen.", "tokens": ["Die", "Hoff\u00b7nung", "t\u00e4uschte", "dich", ".", "Du", "hast", "dich", "selbst", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Du steuerst in dein Grab. Du segelst in den Tod.", "tokens": ["Du", "steu\u00b7erst", "in", "dein", "Grab", ".", "Du", "se\u00b7gelst", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hinaus, hinein, hinab, hinauf ist gleich die Noth.", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7ein", ",", "hin\u00b7ab", ",", "hin\u00b7auf", "ist", "gleich", "die", "Noth", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PTKVZ", "$,", "ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und stehn wir in der Mitt' unschl\u00fcssig still deswegen,", "tokens": ["Und", "stehn", "wir", "in", "der", "Mitt'", "un\u00b7schl\u00fcs\u00b7sig", "still", "des\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da die Natur uns gab die Flossen, uns zu regen?", "tokens": ["Da", "die", "Na\u00b7tur", "uns", "gab", "die", "Flos\u00b7sen", ",", "uns", "zu", "re\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Und da gerade hier sich im Zusammenflu\u00df", "tokens": ["Und", "da", "ge\u00b7ra\u00b7de", "hier", "sich", "im", "Zu\u00b7sam\u00b7men\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "PRF", "APPRART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Landes und des Meers Gefahr begegnen mu\u00df?", "tokens": ["Des", "Lan\u00b7des", "und", "des", "Meers", "Ge\u00b7fahr", "be\u00b7geg\u00b7nen", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "So folge deinem Zug! Gehorche deinem Triebe!", "tokens": ["So", "fol\u00b7ge", "dei\u00b7nem", "Zug", "!", "Ge\u00b7hor\u00b7che", "dei\u00b7nem", "Trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was weiter hat ein Fisch als seine Lust und Liebe?", "tokens": ["Was", "wei\u00b7ter", "hat", "ein", "Fisch", "als", "sei\u00b7ne", "Lust", "und", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "KOKOM", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Du gr\u00fc\u00dfe mir das Land! Du gr\u00fc\u00df mir sch\u00f6n das Meer!", "tokens": ["Du", "gr\u00fc\u00b7\u00dfe", "mir", "das", "Land", "!", "Du", "gr\u00fc\u00df", "mir", "sch\u00f6n", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leb wohl, auf Wiedersehn! Wir sehn uns nimmermehr.", "tokens": ["Leb", "wohl", ",", "auf", "Wie\u00b7der\u00b7sehn", "!", "Wir", "sehn", "uns", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "APPR", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Ein Fischer horcht' erstaunt, der beide wollte fangen;", "tokens": ["Ein", "Fi\u00b7scher", "horcht'", "er\u00b7staunt", ",", "der", "bei\u00b7de", "woll\u00b7te", "fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcber'm Staunen sind sie diesmal ihm entgangen.", "tokens": ["Und", "\u00fc\u00b7ber'm", "Stau\u00b7nen", "sind", "sie", "dies\u00b7mal", "ihm", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}