{"dta.poem.7763": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Flaschenk\u00fcrbisse sprach stolz ein K\u00fcchentopf:", "tokens": ["Zum", "Fla\u00b7schen\u00b7k\u00fcr\u00b7bis\u00b7se", "sprach", "stolz", "ein", "K\u00fc\u00b7chen\u00b7topf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie bist du gegen mich ein unerfahrner Tropf.", "tokens": ["Wie", "bist", "du", "ge\u00b7gen", "mich", "ein", "un\u00b7er\u00b7fahr\u00b7ner", "Tropf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mich formte Flei\u00df und M\u00fch, dem Nutzen hier zu dienen;", "tokens": ["Mich", "form\u00b7te", "Flei\u00df", "und", "M\u00fch", ",", "dem", "Nut\u00b7zen", "hier", "zu", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist, ich wei\u00df nicht wie, alswie aus nichts erschienen.", "tokens": ["Du", "bist", ",", "ich", "wei\u00df", "nicht", "wie", ",", "als\u00b7wie", "aus", "nichts", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "KOKOM", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Sonne w\u00e4rmte dich, weil mich das Feuer hitzte;", "tokens": ["Die", "Son\u00b7ne", "w\u00e4rm\u00b7te", "dich", ",", "weil", "mich", "das", "Feu\u00b7er", "hitz\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Schatten ruhtest du, weil ich am Herde schwitzte.", "tokens": ["Im", "Schat\u00b7ten", "ruh\u00b7test", "du", ",", "weil", "ich", "am", "Her\u00b7de", "schwitz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und jetzt bist du herein, sag an wozu, gekommen;", "tokens": ["Und", "jetzt", "bist", "du", "her\u00b7ein", ",", "sag", "an", "wo\u00b7zu", ",", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "PWAV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was n\u00fctzest du, nachdem man dich vom Zweig genommen?", "tokens": ["Was", "n\u00fct\u00b7zest", "du", ",", "nach\u00b7dem", "man", "dich", "vom", "Zweig", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Flaschenk\u00fcrbis sprach: Was ists worauf du pochst?", "tokens": ["Der", "Fla\u00b7schen\u00b7k\u00fcr\u00b7bis", "sprach", ":", "Was", "ists", "wo\u00b7rauf", "du", "pochst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VAFIN", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich k\u00fchle das Getr\u00e4nk, wenn du die Speise kochst.", "tokens": ["Ich", "k\u00fch\u00b7le", "das", "Ge\u00b7tr\u00e4nk", ",", "wenn", "du", "die", "Spei\u00b7se", "kochst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Voll k\u00fchlen Saftes wuchs ich einst, nun ist die H\u00f6le", "tokens": ["Voll", "k\u00fch\u00b7len", "Saf\u00b7tes", "wuchs", "ich", "einst", ",", "nun", "ist", "die", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gef\u00fcllt mit frischer Flut, Wein, Honig, Milch und Oele.", "tokens": ["Ge\u00b7f\u00fcllt", "mit", "fri\u00b7scher", "Flut", ",", "Wein", ",", "Ho\u00b7nig", ",", "Milch", "und", "O\u00b7e\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}}, "stanza.7": {"line.1": {"text": "Zwei von ungleichem Stamm, sind wir an gleicher St\u00e4te", "tokens": ["Zwei", "von", "un\u00b7glei\u00b7chem", "Stamm", ",", "sind", "wir", "an", "glei\u00b7cher", "St\u00e4\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "APPR", "ADJA", "NN", "$,", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Desselben Haushalts nur verschiednes Hausger\u00e4the.", "tokens": ["Des\u00b7sel\u00b7ben", "Haus\u00b7halts", "nur", "ver\u00b7schied\u00b7nes", "Haus\u00b7ge\u00b7r\u00e4\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du ein Gef\u00e4\u00df der Glut, ich ein Gef\u00e4\u00df der Huld,", "tokens": ["Du", "ein", "Ge\u00b7f\u00e4\u00df", "der", "Glut", ",", "ich", "ein", "Ge\u00b7f\u00e4\u00df", "der", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$,", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist unser Schicksal doch weder Verdienst noch Schuld.", "tokens": ["Ist", "un\u00b7ser", "Schick\u00b7sal", "doch", "we\u00b7der", "Ver\u00b7dienst", "noch", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}