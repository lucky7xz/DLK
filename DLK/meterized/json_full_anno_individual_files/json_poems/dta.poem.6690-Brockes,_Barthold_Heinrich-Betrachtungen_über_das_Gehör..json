{"dta.poem.6690": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir das Seuffzen und das Klagen", "tokens": ["Wenn", "wir", "das", "Seuff\u00b7zen", "und", "das", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Andromach\u2019 und Marianen sp\u00fcren;", "tokens": ["Der", "An\u00b7dro\u00b7mach'", "und", "Ma\u00b7ri\u00b7a\u00b7nen", "sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So k\u00f6nnen die verstellte Plagen", "tokens": ["So", "k\u00f6n\u00b7nen", "die", "ver\u00b7stell\u00b7te", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schreyen, Winseln, Weinen, Zagen,", "tokens": ["Das", "Schre\u00b7yen", ",", "Win\u00b7seln", ",", "Wei\u00b7nen", ",", "Za\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch ein geheimes Dencken,", "tokens": ["Durch", "ein", "ge\u00b7hei\u00b7mes", "Den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als k\u00f6nten sie in solchen Bildern,", "tokens": ["Als", "k\u00f6n\u00b7ten", "sie", "in", "sol\u00b7chen", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die uns so Geist als Kunst nat\u00fcrlich schildern,", "tokens": ["Die", "uns", "so", "Geist", "als", "Kunst", "na\u00b7t\u00fcr\u00b7lich", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "KOUS", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Uns reitzen und Vergn\u00fcgen schencken,", "tokens": ["Uns", "reit\u00b7zen", "und", "Ver\u00b7gn\u00fc\u00b7gen", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Seele, welche denckt, trifft tausend Anmuth an,", "tokens": ["Die", "See\u00b7le", ",", "wel\u00b7che", "denckt", ",", "trifft", "tau\u00b7send", "An\u00b7muth", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn sie den Flu\u00df erdichter Z\u00e4hren sieht.", "tokens": ["Wenn", "sie", "den", "Flu\u00df", "er\u00b7dich\u00b7ter", "Z\u00e4h\u00b7ren", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Man schaut mit fr\u00f6lichem Gem\u00fcth,", "tokens": ["Man", "schaut", "mit", "fr\u00f6\u00b7li\u00b7chem", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wie man den Schmertz nat\u00fcrlich bilden kan.", "tokens": ["Wie", "man", "den", "Schmertz", "na\u00b7t\u00fcr\u00b7lich", "bil\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sollt\u2019 es wol m\u00f6glich seyn, da\u00df solche Lust uns r\u00fchrte,", "tokens": ["Sollt'", "es", "wol", "m\u00f6g\u00b7lich", "seyn", ",", "da\u00df", "sol\u00b7che", "Lust", "uns", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn solche vorgestellte Z\u00e4hren", "tokens": ["Wenn", "sol\u00b7che", "vor\u00b7ge\u00b7stell\u00b7te", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Recht ernstlich und nat\u00fcrlich w\u00e4ren,", "tokens": ["Recht", "ernst\u00b7lich", "und", "na\u00b7t\u00fcr\u00b7lich", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und wenn man wahre Klagen sp\u00fcrte?", "tokens": ["Und", "wenn", "man", "wah\u00b7re", "Kla\u00b7gen", "sp\u00fcr\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}