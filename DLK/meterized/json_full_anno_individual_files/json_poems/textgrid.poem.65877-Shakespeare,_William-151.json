{"textgrid.poem.65877": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "151", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb' ist zu jung, sie wei\u00df nichts von Gewissen;", "tokens": ["Lieb'", "ist", "zu", "jung", ",", "sie", "wei\u00df", "nichts", "von", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch, wer wei\u00df es nicht? Gewissen stammt von ihr.", "tokens": ["Und", "doch", ",", "wer", "wei\u00df", "es", "nicht", "?", "Ge\u00b7wis\u00b7sen", "stammt", "von", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum la\u00df mich, lieber Dieb, f\u00fcr meinen Fehl nicht b\u00fc\u00dfen!", "tokens": ["Drum", "la\u00df", "mich", ",", "lie\u00b7ber", "Dieb", ",", "f\u00fcr", "mei\u00b7nen", "Fehl", "nicht", "b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "ADV", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst teilt dein holdes Selbst die gleiche Schuld mit mir.", "tokens": ["Sonst", "teilt", "dein", "hol\u00b7des", "Selbst", "die", "glei\u00b7che", "Schuld", "mit", "mir", "."], "token_info": ["word", "word", "word"