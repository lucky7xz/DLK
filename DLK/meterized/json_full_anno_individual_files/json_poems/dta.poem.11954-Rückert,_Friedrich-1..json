{"dta.poem.11954": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als das Kamel von Gott sich H\u00f6rner wollt' erbitten,", "tokens": ["Als", "das", "Ka\u00b7mel", "von", "Gott", "sich", "H\u00f6r\u00b7ner", "wollt'", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "PRF", "NE", "VMFIN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wurden ihm noch dazu die Ohren abgeschnitten;", "tokens": ["Wur\u00b7den", "ihm", "noch", "da\u00b7zu", "die", "Oh\u00b7ren", "ab\u00b7ge\u00b7schnit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PAV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Wie seines eignen Schmucks Beraubung mancher litt,", "tokens": ["Wie", "sei\u00b7nes", "eig\u00b7nen", "Schmucks", "Be\u00b7rau\u00b7bung", "man\u00b7cher", "litt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ungen\u00fcgsam er um fremden Vorzug stritt.", "tokens": ["Weil", "un\u00b7ge\u00b7n\u00fcg\u00b7sam", "er", "um", "frem\u00b7den", "Vor\u00b7zug", "stritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sieh deines Thieres Kopf, o Treiber des Kamehles!", "tokens": ["Sieh", "dei\u00b7nes", "Thie\u00b7res", "Kopf", ",", "o", "Trei\u00b7ber", "des", "Ka\u00b7meh\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beim Ohre das ihm fehlt, gedenke deines Fehles!", "tokens": ["Beim", "Oh\u00b7re", "das", "ihm", "fehlt", ",", "ge\u00b7den\u00b7ke", "dei\u00b7nes", "Feh\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}