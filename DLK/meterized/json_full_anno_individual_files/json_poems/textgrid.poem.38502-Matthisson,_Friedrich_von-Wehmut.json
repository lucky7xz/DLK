{"textgrid.poem.38502": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wehmut", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonnenvergoldet fl\u00fcstert ihr, o Linden!", "tokens": ["Son\u00b7nen\u00b7ver\u00b7gol\u00b7det", "fl\u00fcs\u00b7tert", "ihr", ",", "o", "Lin\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "FM", "NE", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von der leiseren Herbstluft sanft umlispelt,", "tokens": ["Von", "der", "lei\u00b7se\u00b7ren", "Herbst\u00b7luft", "sanft", "um\u00b7lis\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Banges Ahnden nahender Wintertrauer", "tokens": ["Ban\u00b7ges", "Ahn\u00b7den", "na\u00b7hen\u00b7der", "Win\u00b7ter\u00b7trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Mir in die Seele!", "tokens": ["Mir", "in", "die", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Senken nicht diese bunten Rasenblumen,", "tokens": ["Sen\u00b7ken", "nicht", "die\u00b7se", "bun\u00b7ten", "Ra\u00b7sen\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Einst des buhlenden Sommerwests Gespielen,", "tokens": ["Einst", "des", "buh\u00b7len\u00b7den", "Som\u00b7mer\u00b7wests", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schon dem fr\u00fchen Tode die Farbenh\u00e4upter", "tokens": ["Schon", "dem", "fr\u00fc\u00b7hen", "To\u00b7de", "die", "Far\u00b7ben\u00b7h\u00e4up\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Traurend entgegen?", "tokens": ["Trau\u00b7rend", "ent\u00b7ge\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach! es wird ihnen, wenn im kalten Grabe,", "tokens": ["Ach", "!", "es", "wird", "ih\u00b7nen", ",", "wenn", "im", "kal\u00b7ten", "Gra\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPER", "$,", "KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unter silbernen Floken, sie nun ruhen,", "tokens": ["Un\u00b7ter", "sil\u00b7ber\u00b7nen", "Flo\u00b7ken", ",", "sie", "nun", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bald der allverheerende Nord das grause", "tokens": ["Bald", "der", "all\u00b7ver\u00b7hee\u00b7ren\u00b7de", "Nord", "das", "grau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Todtenlied heulen!", "tokens": ["Tod\u00b7ten\u00b7lied", "heu\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Traurend durchirr' ich dann die Eisgefilde,", "tokens": ["Trau\u00b7rend", "durch\u00b7irr'", "ich", "dann", "die", "Eis\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schaue weinend die St\u00e4te wo sie prangten:", "tokens": ["Schau\u00b7e", "wei\u00b7nend", "die", "St\u00e4\u00b7te", "wo", "sie", "prang\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn wie sie verbl\u00fchten auch meiner Jugend", "tokens": ["Denn", "wie", "sie", "ver\u00b7bl\u00fch\u00b7ten", "auch", "mei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Fl\u00fcchtige Freuden!", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ge", "Freu\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Sonnenvergoldet fl\u00fcstert ihr, o Linden!", "tokens": ["Son\u00b7nen\u00b7ver\u00b7gol\u00b7det", "fl\u00fcs\u00b7tert", "ihr", ",", "o", "Lin\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "FM", "NE", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "l