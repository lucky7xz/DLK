{"dta.poem.3277": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Muse bey den Hirten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.85", "da:0.14"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "O artigste der Musen,", "tokens": ["O", "ar\u00b7tigs\u00b7te", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ART", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Um deren vollen Busen", "tokens": ["Um", "de\u00b7ren", "vol\u00b7len", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die frischen Rosen d\u00fcften!", "tokens": ["Die", "fri\u00b7schen", "Ro\u00b7sen", "d\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Willst du auf unsern Triften", "tokens": ["Willst", "du", "auf", "un\u00b7sern", "Trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit armen Hirten weiden,", "tokens": ["Mit", "ar\u00b7men", "Hir\u00b7ten", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und aus den St\u00e4dten scheiden?", "tokens": ["Und", "aus", "den", "St\u00e4d\u00b7ten", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin der Stadt entgangen:", "tokens": ["Ich", "bin", "der", "Stadt", "ent\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da war ich wie gefangen.", "tokens": ["Da", "war", "ich", "wie", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da will man Musen dingen:", "tokens": ["Da", "will", "man", "Mu\u00b7sen", "din\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sollen iedem singen,", "tokens": ["Sie", "sol\u00b7len", "ie\u00b7dem", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bey ieder Hochzeit leyern,", "tokens": ["Bey", "ie\u00b7der", "Hoch\u00b7zeit", "ley\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Nahmenstage feyern.", "tokens": ["Und", "Nah\u00b7mens\u00b7ta\u00b7ge", "fey\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bey euch lacht meinen Saiten", "tokens": ["Bey", "euch", "lacht", "mei\u00b7nen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Die Freyheit g\u00fcldner Zeiten:", "tokens": ["Die", "Frey\u00b7heit", "g\u00fcld\u00b7ner", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich mag die g\u00fcldnen Saiten", "tokens": ["Ich", "mag", "die", "g\u00fcld\u00b7nen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Dem P\u00f6bel nicht verdingen:", "tokens": ["Dem", "P\u00f6\u00b7bel", "nicht", "ver\u00b7din\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ich mag nicht iedem singen.", "tokens": ["Ich", "mag", "nicht", "ie\u00b7dem", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Muse, sey gegr\u00fcsset!", "tokens": ["O", "Mu\u00b7se", ",", "sey", "ge\u00b7gr\u00fcs\u00b7set", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier, wo man lacht und k\u00fcsset,", "tokens": ["Hier", ",", "wo", "man", "lacht", "und", "k\u00fcs\u00b7set", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df unter Nachtigallen", "tokens": ["La\u00df", "un\u00b7ter", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein s\u00fcsses Lied erschallen!", "tokens": ["Dein", "s\u00fcs\u00b7ses", "Lied", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}