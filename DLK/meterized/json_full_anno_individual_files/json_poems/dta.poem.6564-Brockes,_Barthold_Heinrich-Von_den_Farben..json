{"dta.poem.6564": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kein C\u00f6rper scheinet Wei\u00df, als durch die Rauhigkeit;", "tokens": ["Kein", "C\u00f6r\u00b7per", "schei\u00b7net", "Wei\u00df", ",", "als", "durch", "die", "Rau\u00b7hig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NE", "$,", "KOUS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man findet allezeit in ihnen Ungleichheiten,", "tokens": ["Man", "fin\u00b7det", "al\u00b7le\u00b7zeit", "in", "ih\u00b7nen", "Un\u00b7gleich\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die aber doch des Lichtes Strahl nicht schw\u00e4chen,", "tokens": ["Die", "a\u00b7ber", "doch", "des", "Lich\u00b7tes", "Strahl", "nicht", "schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wol aber, (wenn sie sich auf ihnen brechen,)", "tokens": ["Wol", "a\u00b7ber", ",", "(", "wenn", "sie", "sich", "auf", "ih\u00b7nen", "bre\u00b7chen", ",", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "$(", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Die Menge von sich ab auf allen Seiten treiben,", "tokens": ["Die", "Men\u00b7ge", "von", "sich", "ab", "auf", "al\u00b7len", "Sei\u00b7ten", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "PTKVZ", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So, da\u00df sie sich starck an die Nerven reiben;", "tokens": ["So", ",", "da\u00df", "sie", "sich", "starck", "an", "die", "Ner\u00b7ven", "rei\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wodurch denn unsre Augen", "tokens": ["Wo\u00b7durch", "denn", "uns\u00b7re", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Weisse kaum zu dulden taugen.", "tokens": ["Das", "Weis\u00b7se", "kaum", "zu", "dul\u00b7den", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}