{"dta.poem.2760": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin vergn\u00fcgt/ dein sch\u00f6ner Rosen-Mund", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "/", "dein", "sch\u00f6\u00b7ner", "Ro\u00b7sen\u00b7Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat meinen Geist und Seele recht erquicket;", "tokens": ["Hat", "mei\u00b7nen", "Geist", "und", "See\u00b7le", "recht", "er\u00b7quic\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Ku\u00df macht mir des Hertzens Meynung kund/", "tokens": ["Dein", "Ku\u00df", "macht", "mir", "des", "Hert\u00b7zens", "Mey\u00b7nung", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gleicher Trieb hat meine Seel entz\u00fccket.", "tokens": ["Und", "glei\u00b7cher", "Trieb", "hat", "mei\u00b7ne", "Seel", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich sage di\u00df/ du hast nun obgesiegt;", "tokens": ["Ich", "sa\u00b7ge", "di\u00df", "/", "du", "hast", "nun", "ob\u00b7ge\u00b7siegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin vergn\u00fcgt.", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich bin vergn\u00fcgt/ dein Hertze erquickend Ku\u00df", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "/", "dein", "Hert\u00b7ze", "er\u00b7qui\u00b7ckend", "Ku\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PPOSAT", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Begegnet stets der Gegenwart der Seele/", "tokens": ["Be\u00b7geg\u00b7net", "stets", "der", "Ge\u00b7gen\u00b7wart", "der", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh\u2019 diese noch zur\u00fccke lauffen mu\u00df", "tokens": ["Eh'", "die\u00b7se", "noch", "zu\u00b7r\u00fc\u00b7cke", "lauf\u00b7fen", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wart meiner schon in der Corallen-H\u00f6le.", "tokens": ["Wart", "mei\u00b7ner", "schon", "in", "der", "Coral\u00b7len\u00b7H\u00f6le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich f\u00fchle wol/ woran mir alles ligt/", "tokens": ["Ich", "f\u00fch\u00b7le", "wol", "/", "wo\u00b7ran", "mir", "al\u00b7les", "ligt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAV", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin vergn\u00fcgt.", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich bin vergn\u00fcgt/ dein heisser Ku\u00df entdecket/", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "/", "dein", "heis\u00b7ser", "Ku\u00df", "ent\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie treu du liebst! wie redlich deine Flammen!", "tokens": ["Wie", "treu", "du", "liebst", "!", "wie", "red\u00b7lich", "dei\u00b7ne", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$.", "PWAV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es brennt ein Feur/ das im Verborgnen steckt/", "tokens": ["Es", "brennt", "ein", "Feur", "/", "das", "im", "Ver\u00b7borg\u00b7nen", "steckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bi\u00df endlich schl\u00e4gt die lichte Loh zusammen:", "tokens": ["Bi\u00df", "end\u00b7lich", "schl\u00e4gt", "die", "lich\u00b7te", "Loh", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So auch ein Hertz/ das alles \u00fcberwiegt/", "tokens": ["So", "auch", "ein", "Hertz", "/", "das", "al\u00b7les", "\u00fc\u00b7berw\u00b7iegt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "ART", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin vergn\u00fcgt.", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich bin vergn\u00fcgt/ in deinem s\u00fcssen Ku\u00df\u2019", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "/", "in", "dei\u00b7nem", "s\u00fcs\u00b7sen", "Ku\u00df'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hab ich noch mehr als Ambrosin geschmecket/", "tokens": ["Hab", "ich", "noch", "mehr", "als", "A\u00b7mbro\u00b7sin", "ge\u00b7schme\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist f\u00fcrwahr der Anmuth Uberflu\u00df.", "tokens": ["Es", "ist", "f\u00fcr\u00b7wahr", "der", "An\u00b7muth", "U\u00b7ber\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Aufboth/ der stets neue Lust erwecket.", "tokens": ["Ein", "Auf\u00b7both", "/", "der", "stets", "neu\u00b7e", "Lust", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein ander denck/ ob er dergleichen kriegt/", "tokens": ["Ein", "an\u00b7der", "denck", "/", "ob", "er", "derg\u00b7lei\u00b7chen", "kriegt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$(", "KOUS", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin vergn\u00fcgt.", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich bin vergn\u00fcgt/ und ruh in deiner Scho\u00df/", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "/", "und", "ruh", "in", "dei\u00b7ner", "Scho\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "KON", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hyelle Schatz und Sammelplatz der Freuden/", "tokens": ["Hyel\u00b7le", "Schatz", "und", "Sam\u00b7mel\u00b7platz", "der", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Liebe macht mich aller Sorgen lo\u00df.", "tokens": ["Die", "Lie\u00b7be", "macht", "mich", "al\u00b7ler", "Sor\u00b7gen", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich will bey dir in nichts als Rosen weiden/", "tokens": ["Ich", "will", "bey", "dir", "in", "nichts", "als", "Ro\u00b7sen", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "APPR", "PIS", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sage stets/ weil es der Himmel f\u00fcgt/", "tokens": ["Und", "sa\u00b7ge", "stets", "/", "weil", "es", "der", "Him\u00b7mel", "f\u00fcgt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin vergn\u00fcgt.", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}