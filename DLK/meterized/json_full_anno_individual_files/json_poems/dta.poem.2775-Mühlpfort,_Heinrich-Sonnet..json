{"dta.poem.2775": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht mich Melandern an/ wie ich so mager worden/", "tokens": ["Seht", "mich", "Me\u00b7lan\u00b7dern", "an", "/", "wie", "ich", "so", "ma\u00b7ger", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$(", "PWAV", "PPER", "ADV", "ADJD", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie kaum die welcke Haut die d\u00fcrren Knochen deckt.", "tokens": ["Wie", "kaum", "die", "wel\u00b7cke", "Haut", "die", "d\u00fcr\u00b7ren", "Kno\u00b7chen", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seht/ die ihr mich geliebt/ nur da\u00df ihr nicht erschreckt", "tokens": ["Seht", "/", "die", "ihr", "mich", "ge\u00b7liebt", "/", "nur", "da\u00df", "ihr", "nicht", "er\u00b7schreckt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "PRELS", "PPER", "PRF", "VVPP", "$(", "ADV", "KOUS", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn als ich fl\u00fcchtig ward/ verlie\u00df der Menschen Orden/", "tokens": ["Denn", "als", "ich", "fl\u00fcch\u00b7tig", "ward", "/", "ver\u00b7lie\u00df", "der", "Men\u00b7schen", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hat sich ein rauher Sturm von dem beeisten Norden/", "tokens": ["Hat", "sich", "ein", "rau\u00b7her", "Sturm", "von", "dem", "be\u00b7eis\u00b7ten", "Nor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf meinen Kopff gemacht/ weil mich kein Dach bedeckt.", "tokens": ["Auf", "mei\u00b7nen", "Kopff", "ge\u00b7macht", "/", "weil", "mich", "kein", "Dach", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$(", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Selbst die Verfolgung hat bey mir die Reu erweckt/", "tokens": ["Selbst", "die", "Ver\u00b7fol\u00b7gung", "hat", "bey", "mir", "die", "Reu", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR", "PPER", "ART", "NE", "VVPP", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "So da\u00df ich bitten mu\u00df/ man woll mich nicht ermorden.", "tokens": ["So", "da\u00df", "ich", "bit\u00b7ten", "mu\u00df", "/", "man", "woll", "mich", "nicht", "er\u00b7mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$(", "PIS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich will nach Jauchendorff/ umb dort zu b\u00fcssen/ ziehn.", "tokens": ["Ich", "will", "nach", "Jau\u00b7chen\u00b7dorff", "/", "umb", "dort", "zu", "b\u00fcs\u00b7sen", "/", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "$(", "APPR", "ADV", "PTKZU", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Beym Gerber mag ich nicht des Lebens Rest beschliessen.", "tokens": ["Beym", "Ger\u00b7ber", "mag", "ich", "nicht", "des", "Le\u00b7bens", "Rest", "be\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Seht meine Augen an/ wie sie mit Thr\u00e4nen fliessen!", "tokens": ["Seht", "mei\u00b7ne", "Au\u00b7gen", "an", "/", "wie", "sie", "mit", "Thr\u00e4\u00b7nen", "flies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "PWAV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich will im neuen Jahr all Ubelthaten fliehn.", "tokens": ["Ich", "will", "im", "neu\u00b7en", "Jahr", "all", "U\u00b7belt\u00b7ha\u00b7ten", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nun Bre\u00dflau gute Nacht/ ich komme schwerlich wieder/", "tokens": ["Nun", "Bre\u00df\u00b7lau", "gu\u00b7te", "Nacht", "/", "ich", "kom\u00b7me", "schwer\u00b7lich", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADJD", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Such einst zu Jauchendorff die hinterbliebnen Glieder.", "tokens": ["Such", "einst", "zu", "Jau\u00b7chen\u00b7dorff", "die", "hin\u00b7ter\u00b7blieb\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}