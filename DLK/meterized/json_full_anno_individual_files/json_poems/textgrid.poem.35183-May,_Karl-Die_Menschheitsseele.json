enta"}, "line.6": {"text": "Verklang in mir der Wehmuth heilger Ton;", "tokens": ["Ver\u00b7klang", "in", "mir", "der", "Weh\u00b7muth", "heil\u00b7ger", "Ton", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es legte sich um mich der Hoffnung Schleier \u2013 \u2013", "tokens": ["Es", "leg\u00b7te", "sich", "um", "mich", "der", "Hoff\u00b7nung", "Schlei\u00b7er", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PRF", "ART", "NN", "NN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du warst verschwunden, warst der Welt entflohn.", "tokens": ["Du", "warst", "ver\u00b7schwun\u00b7den", ",", "warst", "der", "Welt", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}