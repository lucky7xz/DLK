{"textgrid.poem.35183": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Die Menschheitsseele", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war bei dir, in einem andern Leben,", "tokens": ["Ich", "war", "bei", "dir", ",", "in", "ei\u00b7nem", "an\u00b7dern", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch, ein andres Leben war es nicht.", "tokens": ["Und", "doch", ",", "ein", "and\u00b7res", "Le\u00b7ben", "war", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah dich wie in Lichtes Fluthen schweben,", "tokens": ["Ich", "sah", "dich", "wie", "in", "Lich\u00b7tes", "Flut\u00b7hen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch und doch gebrach es mir an Licht.", "tokens": ["Und", "doch", "und", "doch", "ge\u00b7brach", "es", "mir", "an", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich war bei dir, ich wei\u00df nicht, ob am Tage,", "tokens": ["Ich", "war", "bei", "dir", ",", "ich", "wei\u00df", "nicht", ",", "ob", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ob auch vielleicht in sternenarmer Nacht,", "tokens": ["Ob", "auch", "viel\u00b7leicht", "in", "ster\u00b7nen\u00b7ar\u00b7mer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und finde keine Antwort auf die Frage,", "tokens": ["Und", "fin\u00b7de", "kei\u00b7ne", "Ant\u00b7wort", "auf", "die", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Welch Intervall mich dir emporgebracht.", "tokens": ["Welch", "In\u00b7ter\u00b7vall", "mich", "dir", "em\u00b7por\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es schien mir wie in unbekannter Ferne,", "tokens": ["Es", "schien", "mir", "wie", "in", "un\u00b7be\u00b7kann\u00b7ter", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch war diese Ferne mir bekannt;", "tokens": ["Und", "doch", "war", "die\u00b7se", "Fer\u00b7ne", "mir", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDAT", "NN", "PPER", "ADJD", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Du strahltest wie auf einem andern Sterne,", "tokens": ["Du", "strahl\u00b7test", "wie", "auf", "ei\u00b7nem", "an\u00b7dern", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch war dieser Stern mein Vaterland.", "tokens": ["Und", "doch", "war", "die\u00b7ser", "Stern", "mein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir trafen uns so weltenabgelegen,", "tokens": ["Wir", "tra\u00b7fen", "uns", "so", "wel\u00b7ten\u00b7ab\u00b7ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich wei\u00df es nicht, in welchem Geisterreich;", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "in", "wel\u00b7chem", "Geis\u00b7ter\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du kamst wie eine Fremde mir entgegen,", "tokens": ["Du", "kamst", "wie", "ei\u00b7ne", "Frem\u00b7de", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und doch und doch erkannte ich dich gleich.", "tokens": ["Und", "doch", "und", "doch", "er\u00b7kann\u00b7te", "ich", "dich", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich hatte dich so oft, so gern gesehen,", "tokens": ["Ich", "hat\u00b7te", "dich", "so", "oft", ",", "so", "gern", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als pilgernd ich zum Morgenlande kam;", "tokens": ["Als", "pil\u00b7gernd", "ich", "zum", "Mor\u00b7gen\u00b7lan\u00b7de", "kam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah dich leiden, und so ists geschehen,", "tokens": ["Ich", "sah", "dich", "lei\u00b7den", ",", "und", "so", "ists", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich dein Bild im Herzen mit mir nahm.", "tokens": ["Da\u00df", "ich", "dein", "Bild", "im", "Her\u00b7zen", "mit", "mir", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du gingst von dort nach allen, allen Landen,", "tokens": ["Du", "gingst", "von", "dort", "nach", "al\u00b7len", ",", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "APPR", "PIAT", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch, wo du gr\u00fc\u00dftest, dankte man dir kaum.", "tokens": ["Doch", ",", "wo", "du", "gr\u00fc\u00df\u00b7test", ",", "dank\u00b7te", "man", "dir", "kaum", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So bliebst du unbeachtet, unverstanden,", "tokens": ["So", "bliebst", "du", "un\u00b7be\u00b7ach\u00b7tet", ",", "un\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein armes Weib der Menschheit Jugendtraum.", "tokens": ["Ein", "ar\u00b7mes", "Weib", "der", "Menschheit", "Ju\u00b7gend\u00b7traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun war ich bei dir, jetzt, emporgetragen", "tokens": ["Nun", "war", "ich", "bei", "dir", ",", "jetzt", ",", "em\u00b7por\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$,", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von meiner Liebe, die dir treu verblieb,", "tokens": ["Von", "mei\u00b7ner", "Lie\u00b7be", ",", "die", "dir", "treu", "ver\u00b7blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn wie sie dich geliebt in jenen Tagen,", "tokens": ["Denn", "wie", "sie", "dich", "ge\u00b7liebt", "in", "je\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So hat dich meine Seele jetzt noch lieb.", "tokens": ["So", "hat", "dich", "mei\u00b7ne", "See\u00b7le", "jetzt", "noch", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie mein Herz dein Weh mit dir gelitten,", "tokens": ["Und", "wie", "mein", "Herz", "dein", "Weh", "mit", "dir", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Menschheit gro\u00dfes, selbstverschuldet Leid,", "tokens": ["Der", "Menschheit", "gro\u00b7\u00dfes", ",", "selbst\u00b7ver\u00b7schul\u00b7det", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "So hab ich muthig stets f\u00fcr dich gestritten", "tokens": ["So", "hab", "ich", "mut\u00b7hig", "stets", "f\u00fcr", "dich", "ge\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und bin f\u00fcr dich auch ferner kampfbereit.", "tokens": ["Und", "bin", "f\u00fcr", "dich", "auch", "fer\u00b7ner", "kampf\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mir ist ja die Erkenntni\u00df aufgegangen,", "tokens": ["Mir", "ist", "ja", "die", "Er\u00b7kennt\u00b7ni\u00df", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die leider nicht ein Jeder in sich tr\u00e4gt,", "tokens": ["Die", "lei\u00b7der", "nicht", "ein", "Je\u00b7der", "in", "sich", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ART", "PIS", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df der Verwandtschaft Bande uns umfangen", "tokens": ["Da\u00df", "der", "Ver\u00b7wandt\u00b7schaft", "Ban\u00b7de", "uns", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df mein Puls grad wie der deine schl\u00e4gt.", "tokens": ["Und", "da\u00df", "mein", "Puls", "grad", "wie", "der", "dei\u00b7ne", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "KOKOM", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wei\u00df es, da\u00df ich mit dir steh und falle,", "tokens": ["Ich", "wei\u00df", "es", ",", "da\u00df", "ich", "mit", "dir", "steh", "und", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df deine Zukunft auch die meine ist", "tokens": ["Da\u00df", "dei\u00b7ne", "Zu\u00b7kunft", "auch", "die", "mei\u00b7ne", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "PPOSAT", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und da\u00df als leiser Ton ich mit erschalle", "tokens": ["Und", "da\u00df", "als", "lei\u00b7ser", "Ton", "ich", "mit", "er\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "KOUS", "ADJA", "NN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In dem Accorde, dessen Klang du bist.", "tokens": ["In", "dem", "Ac\u00b7cor\u00b7de", ",", "des\u00b7sen", "Klang", "du", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELAT", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Als dieser Ton bin ich emporgeklungen", "tokens": ["Als", "die\u00b7ser", "Ton", "bin", "ich", "em\u00b7por\u00b7ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch heut zu dir und klinge fort und fort;", "tokens": ["Auch", "heut", "zu", "dir", "und", "klin\u00b7ge", "fort", "und", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als dieser Ton hab ich auch mitgesungen", "tokens": ["Als", "die\u00b7ser", "Ton", "hab", "ich", "auch", "mit\u00b7ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Klagelied, dein holdes Friedenswort.", "tokens": ["Dein", "Kla\u00b7ge\u00b7lied", ",", "dein", "hol\u00b7des", "Frie\u00b7dens\u00b7wort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wei\u00df es wohl, es wird umsonst erklingen,", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "es", "wird", "um\u00b7sonst", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So viel der Mensch vom V\u00f6lkerfrieden spricht;", "tokens": ["So", "viel", "der", "Mensch", "vom", "V\u00f6l\u00b7ker\u00b7frie\u00b7den", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihn kann ja nur die wahre Liebe bringen,", "tokens": ["Ihn", "kann", "ja", "nur", "die", "wah\u00b7re", "Lie\u00b7be", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "wo