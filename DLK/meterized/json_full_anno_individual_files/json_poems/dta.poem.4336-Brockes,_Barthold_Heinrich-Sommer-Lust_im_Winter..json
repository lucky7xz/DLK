{"dta.poem.4336": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sommer-Lust im Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Leser! denke nicht,", "tokens": ["Mein", "Le\u00b7ser", "!", "den\u00b7ke", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ob sey die\u00df Kleinigkeit, und nicht der M\u00fche wehrt:", "tokens": ["Ob", "sey", "die\u00df", "Klei\u00b7nig\u00b7keit", ",", "und", "nicht", "der", "M\u00fc\u00b7he", "wehrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PDS", "NN", "$,", "KON", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O nein! es l\u00e4sset die Figur", "tokens": ["O", "nein", "!", "es", "l\u00e4s\u00b7set", "die", "Fi\u00b7gur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bl\u00e4tter, die den B\u00e4umen gleich, die nette Arbeit der", "tokens": ["Der", "Bl\u00e4t\u00b7ter", ",", "die", "den", "B\u00e4u\u00b7men", "gleich", ",", "die", "net\u00b7te", "Ar\u00b7beit", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Natur", "tokens": ["Na\u00b7tur"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Vom Grossen uns im Kleinen sehn;", "tokens": ["Vom", "Gros\u00b7sen", "uns", "im", "Klei\u00b7nen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was die Anmuht noch vermehrt", "tokens": ["Und", "was", "die", "An\u00b7muht", "noch", "ver\u00b7mehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist, da\u00df zugleich die Farben sch\u00f6n.", "tokens": ["Ist", ",", "da\u00df", "zu\u00b7gleich", "die", "Far\u00b7ben", "sch\u00f6n", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zumahlen, wenn der Schnee dem, was so lieblich gr\u00fcnt,", "tokens": ["Zu\u00b7mah\u00b7len", ",", "wenn", "der", "Schnee", "dem", ",", "was", "so", "lieb\u00b7lich", "gr\u00fcnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ART", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu einem weissen Grunde dient.", "tokens": ["Zu", "ei\u00b7nem", "weis\u00b7sen", "Grun\u00b7de", "dient", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die holde Sch\u00f6nheit geht so weit,", "tokens": ["Die", "hol\u00b7de", "Sch\u00f6n\u00b7heit", "geht", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das frische, recht durchlauchtge Gr\u00fcn", "tokens": ["Das", "fri\u00b7sche", ",", "recht", "durch\u00b7laucht\u00b7ge", "Gr\u00fcn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ist von so reizender und frischer Lieblichkeit,", "tokens": ["Ist", "von", "so", "rei\u00b7zen\u00b7der", "und", "fri\u00b7scher", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df auch unaufmerksame Augen,", "tokens": ["Da\u00df", "auch", "un\u00b7auf\u00b7merk\u00b7sa\u00b7me", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Sich kaum von ihrem Schmuck zu ziehn,", "tokens": ["Sich", "kaum", "von", "ih\u00b7rem", "Schmuck", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von ihrem Glanz sich wegzuwenden, taugen.", "tokens": ["Von", "ih\u00b7rem", "Glanz", "sich", "weg\u00b7zu\u00b7wen\u00b7den", ",", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer wollte denn ohn\u2019 Aufwand, sonder Geld,", "tokens": ["Wer", "woll\u00b7te", "denn", "ohn'", "Auf\u00b7wand", ",", "son\u00b7der", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ohne M\u00fch\u2019, was uns so wohl gef\u00e4llt,", "tokens": ["Und", "oh\u00b7ne", "M\u00fch'", ",", "was", "uns", "so", "wohl", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Sommer nicht im Winter sehn?", "tokens": ["Den", "Som\u00b7mer", "nicht", "im", "Win\u00b7ter", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wollte, mit erlaubter Augen-Lust,", "tokens": ["Wer", "woll\u00b7te", ",", "mit", "er\u00b7laub\u00b7ter", "Au\u00b7gen\u00b7Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und mit dadurch ger\u00fchrter Brust,", "tokens": ["Und", "mit", "da\u00b7durch", "ge\u00b7r\u00fchr\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch selbst im Frost, mit Lust, den Sch\u00f6pfer nicht erh\u00f6hn?", "tokens": ["Auch", "selbst", "im", "Frost", ",", "mit", "Lust", ",", "den", "Sch\u00f6p\u00b7fer", "nicht", "er\u00b7h\u00f6hn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "APPR", "NN", "$,", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}