{"dta.poem.4073": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Aether .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Treu und freundlich, wie du, erzog der G\u00f6tter", "tokens": ["Treu", "und", "freund\u00b7lich", ",", "wie", "du", ",", "er\u00b7zog", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "PPER", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und Menschen", "tokens": ["und", "Men\u00b7schen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Keiner, o Vater Aether! mich auf; noch ehe die", "tokens": ["Kei\u00b7ner", ",", "o", "Va\u00b7ter", "A\u00b7e\u00b7ther", "!", "mich", "auf", ";", "noch", "e\u00b7he", "die"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "FM", "NN", "NN", "$.", "PPER", "PTKVZ", "$.", "ADV", "KOUS", "ART"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Mutter", "tokens": ["Mut\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "In die Arme mich nahm und ihre Br\u00fcste mich", "tokens": ["In", "die", "Ar\u00b7me", "mich", "nahm", "und", "ih\u00b7re", "Br\u00fcs\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "KON", "PPOSAT", "NN", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "tr\u00e4nkten,", "tokens": ["tr\u00e4nk\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Fa\u00dftest du z\u00e4rtlich mich an, und gossest himmli-", "tokens": ["Fa\u00df\u00b7test", "du", "z\u00e4rt\u00b7lich", "mich", "an", ",", "und", "gos\u00b7sest", "himm\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "schen Trank mir,", "tokens": ["schen", "Trank", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Mir den heiligen Odem zuerst in den keimenden", "tokens": ["Mir", "den", "hei\u00b7li\u00b7gen", "O\u00b7dem", "zu\u00b7erst", "in", "den", "kei\u00b7men\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "PIAT"], "meter": "+-+--+-+-+-+--", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Busen.", "tokens": ["Bu\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Nicht von irdischer Kost gedeihen einzig die Wesen,", "tokens": ["Nicht", "von", "ir\u00b7di\u00b7scher", "Kost", "ge\u00b7dei\u00b7hen", "ein\u00b7zig", "die", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVINF", "ADJD", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aber du n\u00e4hrest sie all' mit deinem Nektar, o", "tokens": ["A\u00b7ber", "du", "n\u00e4h\u00b7rest", "sie", "all'", "mit", "dei\u00b7nem", "Nek\u00b7tar", ",", "o"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "$,", "FM"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Vater!", "tokens": ["Va\u00b7ter", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und es dr\u00e4ngt sich und rinnt aus deiner ewigen", "tokens": ["Und", "es", "dr\u00e4ngt", "sich", "und", "rinnt", "aus", "dei\u00b7ner", "e\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "F\u00fclle", "tokens": ["F\u00fcl\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Die beseelende Luft durch alle R\u00f6hren des Lebens.", "tokens": ["Die", "be\u00b7see\u00b7len\u00b7de", "Luft", "durch", "al\u00b7le", "R\u00f6h\u00b7ren", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Darum lieben die Wesen dich auch und ringen und", "tokens": ["Da\u00b7rum", "lie\u00b7ben", "die", "We\u00b7sen", "dich", "auch", "und", "rin\u00b7gen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPER", "ADV", "KON", "VVFIN", "KON"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "streben", "tokens": ["stre\u00b7ben"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Unaufh\u00f6rlich hinauf nach dir in freudigem Wachs-", "tokens": ["Un\u00b7auf\u00b7h\u00f6r\u00b7lich", "hin\u00b7auf", "nach", "dir", "in", "freu\u00b7di\u00b7gem", "Wachs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "PPER", "APPR", "ADJA", "TRUNC"], "meter": "-----+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "thum.", "tokens": ["thum", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Himmlischer! sucht nicht dich mit ihren Augen die", "tokens": ["Himm\u00b7li\u00b7scher", "!", "sucht", "nicht", "dich", "mit", "ih\u00b7ren", "Au\u00b7gen", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PTKNEG", "PRF", "APPR", "PPOSAT", "NN", "ART"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Pflanze,", "tokens": ["Pflan\u00b7ze", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Streckt nach dir die sch\u00fcchternen Arme der niedrige", "tokens": ["Streckt", "nach", "dir", "die", "sch\u00fcch\u00b7ter\u00b7nen", "Ar\u00b7me", "der", "nied\u00b7ri\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Strauch nicht?", "tokens": ["Strauch", "nicht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Da\u00df er dich finde, zerbricht der gefangene Same", "tokens": ["Da\u00df", "er", "dich", "fin\u00b7de", ",", "zer\u00b7bricht", "der", "ge\u00b7fan\u00b7ge\u00b7ne", "Sa\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "die H\u00fclse;", "tokens": ["die", "H\u00fcl\u00b7se", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Da\u00df er belebt von dir in deiner Welle sich bade,", "tokens": ["Da\u00df", "er", "be\u00b7lebt", "von", "dir", "in", "dei\u00b7ner", "Wel\u00b7le", "sich", "ba\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Sch\u00fcttelt der Wald den Schnee, wie ein \u00fcberl\u00e4stig", "tokens": ["Sch\u00fct\u00b7telt", "der", "Wald", "den", "Schnee", ",", "wie", "ein", "\u00fc\u00b7berl\u00b7\u00e4s\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "ADJD"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "Gewand ab.", "tokens": ["Ge\u00b7wand", "ab", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-++", "measure": "unknown.measure.di"}, "line.20": {"text": "Auch die Fische kommen herauf und h\u00fcpfen ver-", "tokens": ["Auch", "die", "Fi\u00b7sche", "kom\u00b7men", "her\u00b7auf", "und", "h\u00fcp\u00b7fen", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "TRUNC"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "langend", "tokens": ["lan\u00b7gend"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Ueber die gl\u00e4nzende Fl\u00e4che des Stroms, als be-", "tokens": ["Ue\u00b7ber", "die", "gl\u00e4n\u00b7zen\u00b7de", "Fl\u00e4\u00b7che", "des", "Stroms", ",", "als", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "TRUNC"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.23": {"text": "gehrten auch diese", "tokens": ["gehr\u00b7ten", "auch", "die\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "PDAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Aus der Wiege zu dir; auch den edeln Thieren", "tokens": ["Aus", "der", "Wie\u00b7ge", "zu", "dir", ";", "auch", "den", "e\u00b7deln", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "der Erde", "tokens": ["der", "Er\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Wird zum Fluge der Schritt, wenn oft das ge-", "tokens": ["Wird", "zum", "Flu\u00b7ge", "der", "Schritt", ",", "wenn", "oft", "das", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "$,", "KOUS", "ADV", "ART", "TRUNC"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "waltige Sehnen,", "tokens": ["wal\u00b7ti\u00b7ge", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Die geheime Liebe zu dir sie ergreift, sie hinauf-", "tokens": ["Die", "ge\u00b7hei\u00b7me", "Lie\u00b7be", "zu", "dir", "sie", "er\u00b7greift", ",", "sie", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PPER", "VVFIN", "$,", "PPER", "TRUNC"], "meter": "+-+-+---+-+--+", "measure": "iambic.hexa.chol"}, "line.29": {"text": "zieht.", "tokens": ["zieht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Stolz verachtet den Boden das Ro\u00df, wie gebogener", "tokens": ["Stolz", "ver\u00b7ach\u00b7tet", "den", "Bo\u00b7den", "das", "Ro\u00df", ",", "wie", "ge\u00b7bo\u00b7ge\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PWAV", "NN"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Stahl strebt", "tokens": ["Stahl", "strebt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "In die H\u00f6he sein Hals, mit der Hufe ber\u00fchrt", "tokens": ["In", "die", "H\u00f6\u00b7he", "sein", "Hals", ",", "mit", "der", "Hu\u00b7fe", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VVPP"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.33": {"text": "es den Sand kaum.", "tokens": ["es", "den", "Sand", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.34": {"text": "Wie zum Scherze, ber\u00fchrt der Fu\u00df der Hirsche", "tokens": ["Wie", "zum", "Scher\u00b7ze", ",", "be\u00b7r\u00fchrt", "der", "Fu\u00df", "der", "Hir\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.35": {"text": "den Grashalm.", "tokens": ["den", "Gras\u00b7halm", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "H\u00fcpft, wie ein Zephyr, \u00fcber den Bach der rei\u00dfend", "tokens": ["H\u00fcpft", ",", "wie", "ein", "Ze\u00b7phyr", ",", "\u00fc\u00b7ber", "den", "Bach", "der", "rei\u00b7\u00dfend"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "ADJD"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "hinabsch\u00e4umt,", "tokens": ["hin\u00b7ab\u00b7sch\u00e4umt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Hin und wieder schweift, kaum sichtbar durch die", "tokens": ["Hin", "und", "wie\u00b7der", "schweift", ",", "kaum", "sicht\u00b7bar", "durch", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "VVFIN", "$,", "ADV", "ADJD", "APPR", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Geb\u00fcsche.", "tokens": ["Ge\u00b7b\u00fc\u00b7sche", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Aber des Aethers Lieblinge, sie, die gl\u00fccklichen V\u00f6gel", "tokens": ["A\u00b7ber", "des", "A\u00b7e\u00b7thers", "Lieb\u00b7lin\u00b7ge", ",", "sie", ",", "die", "gl\u00fcck\u00b7li\u00b7chen", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "+--+--++-+-+--+-", "measure": "dactylic.di.plus"}, "line.41": {"text": "Wohnen und spielen vergn\u00fcgt in der ewigen Halle", "tokens": ["Woh\u00b7nen", "und", "spie\u00b7len", "ver\u00b7gn\u00fcgt", "in", "der", "e\u00b7wi\u00b7gen", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVINF", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.42": {"text": "des Vaters!", "tokens": ["des", "Va\u00b7ters", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.43": {"text": "Raums genug ist f\u00fcr alle. Der Pfad ist keinem", "tokens": ["Raums", "ge\u00b7nug", "ist", "f\u00fcr", "al\u00b7le", ".", "Der", "Pfad", "ist", "kei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "APPR", "PIS", "$.", "ART", "NN", "VAFIN", "PIAT"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "bezeichnet,", "tokens": ["be\u00b7zeich\u00b7net", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.45": {"text": "Und es regen sich frei im Hause die Gro\u00dfen und", "tokens": ["Und", "es", "re\u00b7gen", "sich", "frei", "im", "Hau\u00b7se", "die", "Gro\u00b7\u00dfen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "PRF", "ADJD", "APPRART", "NN", "ART", "NN", "KON"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.46": {"text": "Kleinen.", "tokens": ["Klei\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Ueber dem Haupt frohlocken sie mir und es sehnt", "tokens": ["Ue\u00b7ber", "dem", "Haupt", "froh\u00b7lo\u00b7cken", "sie", "mir", "und", "es", "sehnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wo