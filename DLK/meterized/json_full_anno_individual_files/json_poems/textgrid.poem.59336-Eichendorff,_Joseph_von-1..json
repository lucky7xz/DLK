{"textgrid.poem.59336": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkst du noch jenes Abends, still vor Sehnen,", "tokens": ["Denkst", "du", "noch", "je\u00b7nes", "A\u00b7bends", ",", "still", "vor", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo wir zum letztenmal im Park beisammen?", "tokens": ["Wo", "wir", "zum", "letz\u00b7ten\u00b7mal", "im", "Park", "bei\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fchl standen rings des Abendrotes Flammen,", "tokens": ["K\u00fchl", "stan\u00b7den", "rings", "des", "A\u00b7ben\u00b7dro\u00b7tes", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich scherzte wild \u2013 du l\u00e4cheltest durch Tr\u00e4nen.", "tokens": ["Ich", "scherz\u00b7te", "wild", "\u2013", "du", "l\u00e4\u00b7chel\u00b7test", "durch", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So spielt der Wahnsinn lieblich mit den Schmerzen", "tokens": ["So", "spielt", "der", "Wahn\u00b7sinn", "lieb\u00b7lich", "mit", "den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An j\u00e4her Schl\u00fcfte Rand, die nach ihm trachten;", "tokens": ["An", "j\u00e4\u00b7her", "Schl\u00fcf\u00b7te", "Rand", ",", "die", "nach", "ihm", "trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er mag der lauernden Gefahr nicht achten;", "tokens": ["Er", "mag", "der", "lau\u00b7ern\u00b7den", "Ge\u00b7fahr", "nicht", "ach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er hat den Tod ja schon im \u00f6den Herzen.", "tokens": ["Er", "hat", "den", "Tod", "ja", "schon", "im", "\u00f6\u00b7den", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob du die Mutter auch belogst, betr\u00fcbtest,", "tokens": ["Ob", "du", "die", "Mut\u00b7ter", "auch", "be\u00b7logst", ",", "be\u00b7tr\u00fcb\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was andre Leute dr\u00fcber deuten, sagen \u2013", "tokens": ["Was", "and\u00b7re", "Leu\u00b7te", "dr\u00fc\u00b7ber", "deu\u00b7ten", ",", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "PAV", "VVINF", "$,", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sonst scheu \u2013 heut mochst du nichts nach allem fragen,", "tokens": ["Sonst", "scheu", "\u2013", "heut", "mochst", "du", "nichts", "nach", "al\u00b7lem", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ADV", "VVFIN", "PPER", "PIS", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir einzig zeigen nur, wie du mich liebtest.", "tokens": ["Mir", "ein\u00b7zig", "zei\u00b7gen", "nur", ",", "wie", "du", "mich", "lieb\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aus dem Hause heimlich so entwichen,", "tokens": ["Und", "aus", "dem", "Hau\u00b7se", "heim\u00b7lich", "so", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gabst du ins Feld mir schweigend das Geleite,", "tokens": ["Gabst", "du", "ins", "Feld", "mir", "schwei\u00b7gend", "das", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vor uns das Tal, das hoffnungsreiche, weite,", "tokens": ["Vor", "uns", "das", "Tal", ",", "das", "hoff\u00b7nungs\u00b7rei\u00b7che", ",", "wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und hinter uns kam grau die Nacht geschlichen.", "tokens": ["Und", "hin\u00b7ter", "uns", "kam", "grau", "die", "Nacht", "ge\u00b7schli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du gehst nun fort, sprachst du, ich bleib alleine;", "tokens": ["Du", "gehst", "nun", "fort", ",", "sprachst", "du", ",", "ich", "bleib", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach! d\u00fcrft ich alles lassen, still und heiter", "tokens": ["Ach", "!", "d\u00fcrft", "ich", "al\u00b7les", "las\u00b7sen", ",", "still", "und", "hei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PIS", "VVINF", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit dir so ziehn hinab und immer weiter \u2013", "tokens": ["Mit", "dir", "so", "ziehn", "hin\u00b7ab", "und", "im\u00b7mer", "wei\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PTKVZ", "KON", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah dich an \u2013 es spielten bleiche Scheine", "tokens": ["Ich", "sah", "dich", "an", "\u2013", "es", "spiel\u00b7ten", "blei\u00b7che", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So wunderbar um Locken dir und Glieder;", "tokens": ["So", "wun\u00b7der\u00b7bar", "um", "Lo\u00b7cken", "dir", "und", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So ruhig, fremd warst du mir nie erschienen,", "tokens": ["So", "ru\u00b7hig", ",", "fremd", "warst", "du", "mir", "nie", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es war, als sagten die versteinten Mienen,", "tokens": ["Es", "war", ",", "als", "sag\u00b7ten", "die", "ver\u00b7stein\u00b7ten", "Mie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was du verschwiegst: Wir sehn uns niemals wieder!", "tokens": ["Was", "du", "ver\u00b7schwiegst", ":", "Wir", "sehn", "uns", "nie\u00b7mals", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denkst du noch jenes Abends, still vor Sehnen,