{"text": "So wunderbar um Locken dir und Glieder;", "tokens": ["So", "wun\u00b7der\u00b7bar", "um", "Lo\u00b7cken", "dir", "und", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So ruhig, fremd warst du mir nie erschienen,", "tokens": ["So", "ru\u00b7hig", ",", "fremd", "warst", "du", "mir", "nie", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es war, als sagten die versteinten Mienen,", "tokens": ["Es", "war", ",", "als", "sag\u00b7ten", "die", "ver\u00b7stein\u00b7ten", "Mie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was du verschwiegst: Wir sehn uns niemals wieder!", "tokens": ["Was", "du", "ver\u00b7schwiegst", ":", "Wir", "sehn", "uns", "nie\u00b7mals", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}