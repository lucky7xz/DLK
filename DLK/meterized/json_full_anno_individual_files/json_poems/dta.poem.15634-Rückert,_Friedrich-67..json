{"dta.poem.15634": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Wissen, wenn es nun will auch den Glauben wissen,", "tokens": ["Das", "Wis\u00b7sen", ",", "wenn", "es", "nun", "will", "auch", "den", "Glau\u00b7ben", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Wurzeln fa\u00dft, hat es sie ausgerissen.", "tokens": ["Und", "sei\u00b7ne", "Wur\u00b7zeln", "fa\u00dft", ",", "hat", "es", "sie", "aus\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn einem Glauben so sein Leben wird genommen,", "tokens": ["Wenn", "ei\u00b7nem", "Glau\u00b7ben", "so", "sein", "Le\u00b7ben", "wird", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist das ein Beweis, es m\u00fcss' ein neuer kommen.", "tokens": ["So", "ist", "das", "ein", "Be\u00b7weis", ",", "es", "m\u00fcss'", "ein", "neu\u00b7er", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "A