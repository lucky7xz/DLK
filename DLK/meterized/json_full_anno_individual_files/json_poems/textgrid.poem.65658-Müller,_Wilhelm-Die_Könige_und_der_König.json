{"textgrid.poem.65658": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die K\u00f6nige und der K\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die auf der Erde Thronen mit Schwert und Zepter stehn,", "tokens": ["Die", "auf", "der", "Er\u00b7de", "Thro\u00b7nen", "mit", "Schwert", "und", "Zep\u00b7ter", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie winken: fort von dannen! sobald sie uns ersehn.", "tokens": ["Sie", "win\u00b7ken", ":", "fort", "von", "dan\u00b7nen", "!", "so\u00b7bald", "sie", "uns", "er\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKVZ", "APPR", "ADV", "$.", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie wollen uns verschlie\u00dfen die H\u00e4fen und das Land,", "tokens": ["Sie", "wol\u00b7len", "uns", "ver\u00b7schlie\u00b7\u00dfen", "die", "H\u00e4\u00b7fen", "und", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie wollen uns verschlie\u00dfen Ohr, Auge, Herz und Hand.", "tokens": ["Sie", "wol\u00b7len", "uns", "ver\u00b7schlie\u00b7\u00dfen", "Ohr", ",", "Au\u00b7ge", ",", "Herz", "und", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "wor