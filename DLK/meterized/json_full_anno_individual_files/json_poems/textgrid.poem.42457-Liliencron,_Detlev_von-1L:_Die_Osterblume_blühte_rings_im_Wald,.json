{"textgrid.poem.42457": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Osterblume bl\u00fchte rings im Wald,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Osterblume bl\u00fchte rings im Wald,", "tokens": ["Die", "Os\u00b7ter\u00b7blu\u00b7me", "bl\u00fch\u00b7te", "rings", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und regenfeuchte, weiche Fr\u00fchlingsluft", "tokens": ["Und", "re\u00b7gen\u00b7feuch\u00b7te", ",", "wei\u00b7che", "Fr\u00fch\u00b7lings\u00b7luft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Spielt leise \u00fcber gr\u00fcne Wintersaat.", "tokens": ["Spielt", "lei\u00b7se", "\u00fc\u00b7ber", "gr\u00fc\u00b7ne", "Win\u00b7ter\u00b7saat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Heck des Holzes standen ich und du;", "tokens": ["Am", "Heck", "des", "Hol\u00b7zes", "stan\u00b7den", "ich", "und", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das erste Stelldichein: Ein Bursche noch", "tokens": ["Das", "ers\u00b7te", "Stell\u00b7dich\u00b7ein", ":", "Ein", "Bur\u00b7sche", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN",