{"textgrid.poem.40646": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Insel birgt ihr Haupt in D\u00e4mmernissen, \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Insel birgt ihr Haupt in D\u00e4mmernissen, \u2013", "tokens": ["Die", "In\u00b7sel", "birgt", "ihr", "Haupt", "in", "D\u00e4m\u00b7mer\u00b7nis\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sterbeschleier ist's der Todgeweihten,", "tokens": ["Der", "Ster\u00b7be\u00b7schlei\u00b7er", "ist's", "der", "Tod\u00b7ge\u00b7weih\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den um ihr Antlitz Nebelfl\u00f6re breiten;", "tokens": ["Den", "um", "ihr", "Ant\u00b7litz", "Ne\u00b7bel\u00b7fl\u00f6\u00b7re", "brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Opfer will im Opferkleid sich wissen.", "tokens": ["Das", "Op\u00b7fer", "will", "im", "Op\u00b7fer\u00b7kleid", "sich", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum mag den Sonnengott sie gerne missen,", "tokens": ["Drum", "mag", "den", "Son\u00b7nen\u00b7gott", "sie", "ger\u00b7ne", "mis\u00b7sen", ",