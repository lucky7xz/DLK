{"dta.poem.18540": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  L esbien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Bricht Lesbia den Brief durch ihre sch\u00f6nen H\u00e4nde/", "tokens": ["Bricht", "Les\u00b7bia", "den", "Brief", "durch", "ih\u00b7re", "sch\u00f6\u00b7nen", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So bricht sie auch dabey des Hertzens-Siegel auf;", "tokens": ["So", "bricht", "sie", "auch", "da\u00b7bey", "des", "Hert\u00b7zens\u00b7Sie\u00b7gel", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ist: Da\u00df ich zugleich ihr tausend K\u00fcsse sende;", "tokens": ["Da\u00df", "ist", ":", "Da\u00df", "ich", "zu\u00b7gleich", "ihr", "tau\u00b7send", "K\u00fcs\u00b7se", "sen\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "$.", "KOUS", "PPER", "ADV", "PPOSAT", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jedoch ich legte sie ihr lieber selber drauf.", "tokens": ["Je\u00b7doch", "ich", "leg\u00b7te", "sie", "ihr", "lie\u00b7ber", "sel\u00b7ber", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Worauf? Auf ihre Brust? Ach ja/ und auf die Lippen/", "tokens": ["Wo\u00b7rauf", "?", "Auf", "ih\u00b7re", "Brust", "?", "Ach", "ja", "/", "und", "auf", "die", "Lip\u00b7pen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "APPR", "PPOSAT", "NN", "$.", "ITJ", "ADV", "$(", "KON", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo der Gedancken Schiff gantz sicher Landen kan;", "tokens": ["Wo", "der", "Ge\u00b7dan\u00b7cken", "Schiff", "gantz", "si\u00b7cher", "Lan\u00b7den", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und scheitert es zuletzt an ihren Marmor Klippen/", "tokens": ["Und", "schei\u00b7tert", "es", "zu\u00b7letzt", "an", "ih\u00b7ren", "Mar\u00b7mor", "Klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So schau' ich doch Vergn\u00fcgt den sch\u00f6nen Schiffbruch an.", "tokens": ["So", "schau'", "ich", "doch", "Ver\u00b7gn\u00fcgt", "den", "sch\u00f6\u00b7nen", "Schiff\u00b7bruch", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich falle von der H\u00f6h' in Amors tieffe Wellen.", "tokens": ["Ich", "fal\u00b7le", "von", "der", "H\u00f6h'", "in", "A\u00b7mors", "tief\u00b7fe", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach z\u00fcrne/ sch\u00f6nste nicht/ wenn ich zu weit will gehn/", "tokens": ["Ach", "z\u00fcr\u00b7ne", "/", "sch\u00f6ns\u00b7te", "nicht", "/", "wenn", "ich", "zu", "weit", "will", "gehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "PTKA", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur z\u00fcrne/ wenn ichs nicht nach w\u00fcrden vor kan stellen/", "tokens": ["Nur", "z\u00fcr\u00b7ne", "/", "wenn", "ichs", "nicht", "nach", "w\u00fcr\u00b7den", "vor", "kan", "stel\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PIS", "PTKNEG", "APPR", "VAFIN", "APPR", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df ich es nicht zu vor durch deine Gunst gesehn.", "tokens": ["Da\u00df", "ich", "es", "nicht", "zu", "vor", "durch", "dei\u00b7ne", "Gunst", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "PTKZU", "APPR", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn deine G\u00fctigkeit gleicht sich den Rosen-Str\u00e4uchen/", "tokens": ["Denn", "dei\u00b7ne", "G\u00fc\u00b7tig\u00b7keit", "gleicht", "sich", "den", "Ro\u00b7sen\u00b7Str\u00e4u\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Die mir die Schmeicheley von oben sch\u00f6n verspricht:", "tokens": ["Die", "mir", "die", "Schmei\u00b7che\u00b7ley", "von", "o\u00b7ben", "sch\u00f6n", "ver\u00b7spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch will ich auf den Grund und nach den Stiele reichen/", "tokens": ["Doch", "will", "ich", "auf", "den", "Grund", "und", "nach", "den", "Stie\u00b7le", "rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So heist es: Freund zur\u00fcck/ weil Dorn und Nadel sticht.", "tokens": ["So", "heist", "es", ":", "Freund", "zu\u00b7r\u00fcck", "/", "weil", "Dorn", "und", "Na\u00b7del", "sticht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$.", "NN", "PTKVZ", "$(", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach Nadel! Da\u00df ich mich nur k\u00f6nte wieder r\u00e4chen/", "tokens": ["Ach", "Na\u00b7del", "!", "Da\u00df", "ich", "mich", "nur", "k\u00f6n\u00b7te", "wie\u00b7der", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "KOUS", "PPER", "PRF", "ADV", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So offt mein treues Blut durch deinen Stich zerflie\u00dft!", "tokens": ["So", "offt", "mein", "treu\u00b7es", "Blut", "durch", "dei\u00b7nen", "Stich", "zer\u00b7flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und das man Lesbien das Urtheil m\u00f6chte sprechen:", "tokens": ["Und", "das", "man", "Les\u00b7bi\u00b7en", "das", "Ur\u00b7theil", "m\u00f6ch\u00b7te", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der lasse wider Blut/ der reines Blut vergie\u00dft.", "tokens": ["Der", "las\u00b7se", "wi\u00b7der", "Blut", "/", "der", "rei\u00b7nes", "Blut", "ver\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Verwundre dich nur nicht/ da\u00df ich di\u00df kan begehren/", "tokens": ["Ver\u00b7wund\u00b7re", "dich", "nur", "nicht", "/", "da\u00df", "ich", "di\u00df", "kan", "be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$(", "KOUS", "PPER", "PDS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Noch mehr/ ich wolte dir die Straffe selber thun/", "tokens": ["Noch", "mehr", "/", "ich", "wol\u00b7te", "dir", "die", "Straf\u00b7fe", "sel\u00b7ber", "thun", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "L\u00e4\u00dft den entz\u00fcckten Leib auf Sammt und Seide ruhn.", "tokens": ["L\u00e4\u00dft", "den", "ent\u00b7z\u00fcck\u00b7ten", "Leib", "auf", "Sammt", "und", "Sei\u00b7de", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du hast mir Qvaal genug und Schmertzen zugef\u00fcget/", "tokens": ["Du", "hast", "mir", "Qvaal", "ge\u00b7nug", "und", "Schmert\u00b7zen", "zu\u00b7ge\u00b7f\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ADV", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn ich um deinen Schatz vergebens seufftzen mu\u00df.", "tokens": ["Wenn", "ich", "um", "dei\u00b7nen", "Schatz", "ver\u00b7ge\u00b7bens", "seufft\u00b7zen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dein Reichthum macht mich arm/ weil er vergraben lieget/", "tokens": ["Dein", "Reicht\u00b7hum", "macht", "mich", "arm", "/", "weil", "er", "ver\u00b7gra\u00b7ben", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$(", "KOUS", "PPER", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und mangel plaget mich bey deinen Uberflu\u00df.", "tokens": ["Und", "man\u00b7gel", "pla\u00b7get", "mich", "bey", "dei\u00b7nen", "U\u00b7ber\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich seh' an deinen Baum die sch\u00f6nsten Apffel hangen/", "tokens": ["Ich", "seh'", "an", "dei\u00b7nen", "Baum", "die", "sch\u00f6ns\u00b7ten", "Apf\u00b7fel", "han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und mu\u00df wie ", "tokens": ["Und", "mu\u00df", "wie"], "token_info": ["word", "word", "word"], "pos": ["KON", "VMFIN", "KOKOM"], "meter": "+--", "measure": "dactylic.init"}, "line.31": {"text": "Der Nectar str\u00f6hmet recht durch deine Purpur Wangen/", "tokens": ["Der", "Nec\u00b7tar", "str\u00f6h\u00b7met", "recht", "durch", "dei\u00b7ne", "Pur\u00b7pur", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und meine Seele mu\u00df dabey vor Durst vergehn.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "mu\u00df", "da\u00b7bey", "vor", "Durst", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach strenge Lesbia! stellt mich nur das Gel\u00fccke", "tokens": ["Ach", "stren\u00b7ge", "Les\u00b7bia", "!", "stellt", "mich", "nur", "das", "Ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "NE", "$.", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Nur noch einmahl zu den/ was deine Brust erh\u00f6ht/", "tokens": ["Nur", "noch", "ein\u00b7mahl", "zu", "den", "/", "was", "dei\u00b7ne", "Brust", "er\u00b7h\u00f6ht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "$(", "PWS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So bei\u00df ich dir gewi\u00df vor Grimm davon zwey St\u00fccke/", "tokens": ["So", "bei\u00df", "ich", "dir", "ge\u00b7wi\u00df", "vor", "Grimm", "da\u00b7von", "zwey", "St\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "NE", "PAV", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Weil meine Unruh doch hier niemahls Stille steht.", "tokens": ["Weil", "mei\u00b7ne", "Un\u00b7ruh", "doch", "hier", "nie\u00b7mahls", "Stil\u00b7le", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch/ edle Freundin nein/ das angenehme Beben/", "tokens": ["Doch", "/", "ed\u00b7le", "Freun\u00b7din", "nein", "/", "das", "an\u00b7ge\u00b7neh\u00b7me", "Be\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADJA", "NN", "PTKANT", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das Venus stoltzes Schlo\u00df durch heisse Gluth bewegt/", "tokens": ["Das", "Ve\u00b7nus", "stolt\u00b7zes", "Schlo\u00df", "durch", "heis\u00b7se", "Gluth", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ist zwar der Ruhe Tod/ doch auch mein neues Leben/", "tokens": ["Ist", "zwar", "der", "Ru\u00b7he", "Tod", "/", "doch", "auch", "mein", "neu\u00b7es", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$(", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Weil sich das Blut dadurch in allen Adern regt.", "tokens": ["Weil", "sich", "das", "Blut", "da\u00b7durch", "in", "al\u00b7len", "A\u00b7dern", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PRF", "ART", "NN", "PAV", "APPR", "PIAT", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Drum werd ich ja daran nicht selbst mein Hencker werden/", "tokens": ["Drum", "werd", "ich", "ja", "da\u00b7ran", "nicht", "selbst", "mein", "Hen\u00b7cker", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PAV", "PTKNEG", "ADV", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Nein/ sch\u00f6nste Lesbia/ mein Vorsatz sey verbannt:", "tokens": ["Nein", "/", "sch\u00f6ns\u00b7te", "Les\u00b7bia", "/", "mein", "Vor\u00b7satz", "sey", "ver\u00b7bannt", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ADJA", "NN", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Und stehet itzt mein Fu\u00df auf dornen fremder Erden", "tokens": ["Und", "ste\u00b7het", "itzt", "mein", "Fu\u00df", "auf", "dor\u00b7nen", "frem\u00b7der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So ruffe mich zu dir in das gelobte Land.", "tokens": ["So", "ruf\u00b7fe", "mich", "zu", "dir", "in", "das", "ge\u00b7lob\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dein Willen ist mein Thun/ drum schreib' ob ich soll kommen/", "tokens": ["Dein", "Wil\u00b7len", "ist", "mein", "Thun", "/", "drum", "schreib'", "ob", "ich", "soll", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$(", "PAV", "VVFIN", "KOUS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ich wei\u00df das mich dein Aug' in wenig Tagen schaut.", "tokens": ["Ich", "wei\u00df", "das", "mich", "dein", "Aug'", "in", "we\u00b7nig", "Ta\u00b7gen", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und zwar so hab' ich nichts an G\u00fctern abgenommen/", "tokens": ["Und", "zwar", "so", "hab'", "ich", "nichts", "an", "G\u00fc\u00b7tern", "ab\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "PIS", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Die deiner Augen-Strahl dem Hertzen an vertraut.", "tokens": ["Die", "dei\u00b7ner", "Au\u00b7gen\u00b7Strahl", "dem", "Hert\u00b7zen", "an", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Nein/ nein/ mein Reichthum kan dir noch die Rente zahlen/", "tokens": ["Nein", "/", "nein", "/", "mein", "Reicht\u00b7hum", "kan", "dir", "noch", "die", "Ren\u00b7te", "zah\u00b7len", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PTKANT", "$(", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Ach da\u00df die Lippen nur das Zahlbret m\u00f6chten seyn/", "tokens": ["Ach", "da\u00df", "die", "Lip\u00b7pen", "nur", "das", "Zahl\u00b7bret", "m\u00f6c