{"textgrid.poem.31380": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df alle St\u00e4ublein / mein / und lauter Zungen w\u00e4ren /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df alle St\u00e4ublein / mein / und lauter Zungen w\u00e4ren /", "tokens": ["Da\u00df", "al\u00b7le", "St\u00e4ub\u00b7lein", "/", "mein", "/", "und", "lau\u00b7ter", "Zun\u00b7gen", "w\u00e4\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$(", "PPOSAT", "$(", "KON", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und iedes meiner Haar' ein helle Wei\u00dfheit Flamm!", "tokens": ["und", "ie\u00b7des", "mei\u00b7ner", "Haa\u00b7r'", "ein", "hel\u00b7le", "Wei\u00df\u00b7heit", "Flamm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "ich wolt zu GOTTES Lob / sie binden all zusamm.", "tokens": ["ich", "wolt", "zu", "GoT\u00b7TES", "Lob", "/", "sie", "bin\u00b7den", "all", "zu\u00b7samm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "NN", "$(", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach da\u00df mein Mund die Welt vollmachte seiner Ehren!", "tokens": ["Ach", "da\u00df", "mein", "Mund", "die", "Welt", "voll\u00b7mach\u00b7te", "sei\u00b7ner", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wollst meinem Lebens Baum viel Lobesfr\u00fccht bescheren.", "tokens": ["Wollst", "mei\u00b7nem", "Le\u00b7bens", "Baum", "viel", "Lo\u00b7bes\u00b7fr\u00fccht", "be\u00b7sche\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NE", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "nur werd die Pflicht verricht; verdorret schon der Stamm.", "tokens": ["nur", "werd", "die", "Pflicht", "ver\u00b7richt", ";", "ver\u00b7dor\u00b7ret", "schon", "der", "Stamm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$.", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der Leib bleib auf dem plaz: nur werd gepreist dein Nam.", "tokens": ["der", "Leib", "bleib", "auf", "dem", "plaz", ":", "nur", "werd", "ge\u00b7preist", "dein", "Nam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "nicht sein-nur deines Ruhms erhebung / mein begehren", "tokens": ["nicht", "sein\u00b7\u00b7nur", "dei\u00b7nes", "Ruhms", "er\u00b7he\u00b7bung", "/", "mein", "be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "NN", "$(", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und ei