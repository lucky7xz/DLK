{"dta.poem.14555": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kranke Kind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Geputzt zum Sonntag heute", "tokens": ["Ge\u00b7putzt", "zum", "Sonn\u00b7tag", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieh'n sie das Thal entlang,", "tokens": ["Zieh'n", "sie", "das", "Thal", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Kind gr\u00fc\u00dft alle Leute,", "tokens": ["Das", "Kind", "gr\u00fc\u00dft", "al\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch niemand sagt ihm Dank.", "tokens": ["Doch", "nie\u00b7mand", "sagt", "ihm", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}