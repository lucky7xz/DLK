{"textgrid.poem.25233": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier bei der Eichengruppe war's.", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier bei der Eichengruppe war's.", "tokens": ["Hier", "bei", "der", "Ei\u00b7chen\u00b7grup\u00b7pe", "wa\u00b7r'", "s."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "NE"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der greisen B\u00e4ume knorrige Reckenglieder", "tokens": ["Der", "grei\u00b7sen", "B\u00e4u\u00b7me", "knor\u00b7ri\u00b7ge", "Re\u00b7cken\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umspro\u00df das bronzegelbe Fr\u00fchlingslaub", "tokens": ["Um\u00b7spro\u00df", "das", "bron\u00b7ze\u00b7gel\u00b7be", "Fr\u00fch\u00b7lings\u00b7laub"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Kinderlocken zart.", "tokens": ["Wie", "Kin\u00b7der\u00b7lo\u00b7cken", "zart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die schwarze Drossel schl\u00fcpfte durch die \u00c4ste,", "tokens": ["Die", "schwar\u00b7ze", "Dros\u00b7sel", "schl\u00fcpf\u00b7te", "durch", "die", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Liebchen fl\u00f6tend und ihr Nestlein planen