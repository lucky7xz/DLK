{"dta.poem.19313": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist unsrer Handlungen Beweggrund, wie sie sagen,", "tokens": ["Ist", "uns\u00b7rer", "Hand\u00b7lun\u00b7gen", "Be\u00b7weg\u00b7grund", ",", "wie", "sie", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Gl\u00fcckseligkeit allein, wie sind wir zu beklagen!", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "al\u00b7lein", ",", "wie", "sind", "wir", "zu", "be\u00b7kla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Denn die Gl\u00fcckseligkeit, wo ist sie zu erfragen?", "tokens": ["Denn", "die", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", ",", "wo", "ist", "sie", "zu", "er\u00b7fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ist sie zu ersp\u00e4hn? wo ist sie zu erjagen?", "tokens": ["Wo", "ist", "sie", "zu", "er\u00b7sp\u00e4hn", "?", "wo", "ist", "sie", "zu", "er\u00b7ja\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$.", "PWAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Diese Gl\u00fcckseligkeit, die jeder will erreichen,", "tokens": ["Die\u00b7se", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", ",", "die", "je\u00b7der", "will", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Je n\u00e4her er ihr kommt, scheint weiter zu entweichen.", "tokens": ["Je", "n\u00e4\u00b7her", "er", "ihr", "kommt", ",", "scheint", "wei\u00b7ter", "zu", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Diese Gl\u00fcckseligkeit, die jeder w\u00fcnscht und sucht,", "tokens": ["Die\u00b7se", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", ",", "die", "je\u00b7der", "w\u00fcnscht", "und", "sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ist einem Schatten gleich best\u00e4ndig auf der Flucht.", "tokens": ["Ist", "ei\u00b7nem", "Schat\u00b7ten", "gleich", "be\u00b7st\u00e4n\u00b7dig", "auf", "der", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Bald scheint der Schatten rechts, bald links an uns zu streifen,", "tokens": ["Bald", "scheint", "der", "Schat\u00b7ten", "rechts", ",", "bald", "links", "an", "uns", "zu", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "ADV", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun vor, nun hinter uns, und nirgend zu ergreifen.", "tokens": ["Nun", "vor", ",", "nun", "hin\u00b7ter", "uns", ",", "und", "nir\u00b7gend", "zu", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "APPR", "PPER", "$,", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Diese Gl\u00fcckseligkeit, ein Trugbild manigfalt,", "tokens": ["Die\u00b7se", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", ",", "ein", "Trug\u00b7bild", "ma\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Lockt jeden anderen in anderer Gestalt.", "tokens": ["Lockt", "je\u00b7den", "an\u00b7de\u00b7ren", "in", "an\u00b7de\u00b7rer", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}