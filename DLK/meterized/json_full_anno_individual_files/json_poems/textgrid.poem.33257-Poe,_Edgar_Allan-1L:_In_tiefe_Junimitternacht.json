{"textgrid.poem.33257": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: In tiefe Junimitternacht", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In tiefe Junimitternacht", "tokens": ["In", "tie\u00b7fe", "Ju\u00b7ni\u00b7mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mystische Mond herniederwacht.", "tokens": ["Der", "mys\u00b7ti\u00b7sche", "Mond", "her\u00b7nie\u00b7der\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Einschl\u00e4fernde Nebel dunsten leise", "tokens": ["Ein\u00b7schl\u00e4\u00b7fern\u00b7de", "Ne\u00b7bel", "duns\u00b7ten", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Heraus aus seinem goldnen Kreise", "tokens": ["He\u00b7raus", "aus", "sei\u00b7nem", "gold\u00b7nen", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und triefen sanft wie Schlummerlieder", "tokens": ["Und", "trie\u00b7fen", "sanft", "wie", "Schlum\u00b7mer\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Tropfen um Tropfen sachte nieder", "tokens": ["Trop\u00b7fen", "um", "Trop\u00b7fen", "sach\u00b7te", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "PTKVZ"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Auf H\u00f6hen, schimmernd wie Opal,", "tokens": ["Auf", "H\u00f6\u00b7hen", ",", "schim\u00b7mernd", "wie", "O\u00b7pal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und in das allumfassende Tal.", "tokens": ["Und", "in", "das", "al\u00b7lum\u00b7fas\u00b7sen\u00b7de", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Auf einem Grab nickt Rosmarin,", "tokens": ["Auf", "ei\u00b7nem", "Grab", "nickt", "Ros\u00b7ma\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Tr\u00e4g lehnt die Lilie dr\u00fcber hin.", "tokens": ["Tr\u00e4g", "lehnt", "die", "Li\u00b7lie", "dr\u00fc\u00b7ber", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von leerem Nebel \u00fcberdacht", "tokens": ["Von", "lee\u00b7rem", "Ne\u00b7bel", "\u00fc\u00b7berd\u00b7acht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Fault die Ruine hinein in Nacht.", "tokens": ["Fault", "die", "Ru\u00b7i\u00b7ne", "hin\u00b7ein", "in", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Wie Lethe sieh den Weiher ruhn,", "tokens": ["Wie", "Le\u00b7the", "sieh", "den", "Wei\u00b7her", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Scheint tiefen, tiefen Schlaf zu tun,", "tokens": ["Scheint", "tie\u00b7fen", ",", "tie\u00b7fen", "Schlaf", "zu", "tun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht um die Welt erwachte er nun.", "tokens": ["Nicht", "um", "die", "Welt", "er\u00b7wach\u00b7te", "er", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Alle Sch\u00f6nheit schl\u00e4ft! \u2013 und ach! wo liegt", "tokens": ["Al\u00b7le", "Sch\u00f6n\u00b7heit", "schl\u00e4ft", "!", "\u2013", "und", "ach", "!", "wo", "liegt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$.", "$(", "KON", "XY", "$.", "PWAV", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "(ihr Fenster den Himmeln ge\u00f6ffnet) \u2013 wo liegt", "tokens": ["(", "ihr", "Fens\u00b7ter", "den", "Him\u00b7meln", "ge\u00b7\u00f6ff\u00b7net", ")", "\u2013", "wo", "liegt"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "ART", "NN", "VVPP", "$(", "$(", "PWAV", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.18": {"text": "Irene, vom Schicksal eingewiegt!", "tokens": ["I\u00b7re\u00b7ne", ",", "vom", "Schick\u00b7sal", "ein\u00b7ge\u00b7wiegt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "O Sch\u00f6nste! \u2013 ach! ich steh' betroffen:", "tokens": ["O", "Sch\u00f6ns\u00b7te", "!", "\u2013", "ach", "!", "ich", "steh'", "be\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "$(", "ITJ", "$.", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fenster weit dem Nachtwind offen?", "tokens": ["Das", "Fens\u00b7ter", "weit", "dem", "Nacht\u00b7wind", "of\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die L\u00fcfte fallen im Mondenschein", "tokens": ["Die", "L\u00fcf\u00b7te", "fal\u00b7len", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Baum herab durchs Gitter ein \u2013", "tokens": ["Vom", "Baum", "her\u00b7ab", "durchs", "Git\u00b7ter", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie fl\u00fcchten fl\u00fcsternd wie Geisterschar", "tokens": ["Sie", "fl\u00fcch\u00b7ten", "fl\u00fcs\u00b7ternd", "wie", "Geis\u00b7ter\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Durch dein Gemach und sto\u00dfen gar", "tokens": ["Durch", "dein", "Ge\u00b7mach", "und", "sto\u00b7\u00dfen", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am Bett den bunten Baldachin", "tokens": ["Am", "Bett", "den", "bun\u00b7ten", "Bal\u00b7da\u00b7ch\u00b7in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So schaurig her, so schaurig hin", "tokens": ["So", "schau\u00b7rig", "her", ",", "so", "schau\u00b7rig", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00dcber des Auges geschlossene Glut,", "tokens": ["\u00dc\u00b7ber", "des", "Au\u00b7ges", "ge\u00b7schlos\u00b7se\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Darunter die schlummernde Seele ruht,", "tokens": ["Da\u00b7run\u00b7ter", "die", "schlum\u00b7mern\u00b7de", "See\u00b7le", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Da\u00df Schatten gleich Gespenstern weben", "tokens": ["Da\u00df", "Schat\u00b7ten", "gleich", "Ge\u00b7spens\u00b7tern", "we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und Wand und Boden irr beleben.", "tokens": ["Und", "Wand", "und", "Bo\u00b7den", "irr", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "O liebe Dame, banget dir?", "tokens": ["O", "lie\u00b7be", "Da\u00b7me", ",", "ban\u00b7get", "dir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Warum und was nur tr\u00e4umst du hier?", "tokens": ["Wa\u00b7rum", "und", "was", "nur", "tr\u00e4umst", "du", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "PWS", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Gewi\u00df, du kamst von fernstem Meer,", "tokens": ["Ge\u00b7wi\u00df", ",", "du", "kamst", "von", "ferns\u00b7tem", "Meer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Wunder, in diesen Garten her!", "tokens": ["Ein", "Wun\u00b7der", ",", "in", "die\u00b7sen", "Gar\u00b7ten", "her", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Seltsam deine Bl\u00e4sse! Seltsam dein Kleid!", "tokens": ["Selt\u00b7sam", "dei\u00b7ne", "Bl\u00e4s\u00b7se", "!", "Selt\u00b7sam", "dein", "Kleid", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Die Locken l\u00e4nger als jederzeit!", "tokens": ["Die", "Lo\u00b7cken", "l\u00e4n\u00b7ger", "als", "je\u00b7der\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Seltsam die d\u00fcstere Feierlichkeit!", "tokens": ["Selt\u00b7sam", "die", "d\u00fcs\u00b7te\u00b7re", "Fei\u00b7er\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie schl\u00e4ft! Und wie sie dauernd ruht,", "tokens": ["Sie", "schl\u00e4ft", "!", "Und", "wie", "sie", "dau\u00b7ernd", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ruhe sie auch tief! Und gut", "tokens": ["So", "ru\u00b7he", "sie", "auch", "tief", "!", "Und", "gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab Himmel sie in heiliger Hut!", "tokens": ["Hab", "Him\u00b7mel", "sie", "in", "hei\u00b7li\u00b7ger", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Heiliger sie jetzt und der Raum,", "tokens": ["Hei\u00b7li\u00b7ger", "sie", "jetzt", "und", "der", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "ART", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Schwerm\u00fctiger sie als je ihr Traum.", "tokens": ["Schwer\u00b7m\u00fc\u00b7ti\u00b7ger", "sie", "als", "je", "ihr", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "O Gott! la\u00df nie ihren Schlaf vergehn,", "tokens": ["O", "Gott", "!", "la\u00df", "nie", "ih\u00b7ren", "Schlaf", "ver\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ihr Auge nie sich \u00f6ffnen und sehn,", "tokens": ["Ihr", "Au\u00b7ge", "nie", "sich", "\u00f6ff\u00b7nen", "und", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Indes die Gespenster vor\u00fcberwehn!", "tokens": ["In\u00b7des", "die", "Ge\u00b7spens\u00b7ter", "vor\u00b7\u00fc\u00b7berw\u00b7ehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Meine Liebe, sie schl\u00e4ft! Wie dauernd sie ruht,", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", ",", "sie", "schl\u00e4ft", "!", "Wie", "dau\u00b7ernd", "sie", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "$.", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So ruhe sie auch tief und gut;", "tokens": ["So", "ru\u00b7he", "sie", "auch", "tief", "und", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leis krieche um sie die W\u00fcrmerbrut!", "tokens": ["Leis", "krie\u00b7che", "um", "sie", "die", "W\u00fcr\u00b7mer\u00b7brut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "M\u00f6g fern im Forst, in D\u00fcster und Duft,", "tokens": ["M\u00f6g", "fern", "im", "Forst", ",", "in", "D\u00fcs\u00b7ter", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "F\u00fcr sie sich auftun eine Gruft \u2013", "tokens": ["F\u00fcr", "sie", "sich", "auf\u00b7tun", "ei\u00b7ne", "Gruft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eine Gruft, die oft das schwarze Tor", "tokens": ["Ei\u00b7ne", "Gruft", ",", "die", "oft", "das", "schwar\u00b7ze", "Tor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aufwarf vor bangem Trauerchor,", "tokens": ["Auf\u00b7warf", "vor", "ban\u00b7gem", "Trau\u00b7er\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Triumphierend \u00fcber den Wappenflor", "tokens": ["Tri\u00b7um\u00b7phie\u00b7rend", "\u00fc\u00b7ber", "den", "Wap\u00b7pen\u00b7flor"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+--+-+", "m