{"dta.poem.23072": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "152. Was ein Heiliger hat/ da\u00df ist der  \n andren auch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Was hier die Heiligen mit grosser m\u00fch erlangt/", "tokens": ["Was", "hier", "die", "Hei\u00b7li\u00b7gen", "mit", "gros\u00b7ser", "m\u00fch", "er\u00b7langt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "ADJA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird in der Seeligkeit mir alls umb sonst geschankt.", "tokens": ["Wird", "in", "der", "See\u00b7lig\u00b7keit", "mir", "alls", "umb", "sonst", "ge\u00b7schankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "ADV", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}