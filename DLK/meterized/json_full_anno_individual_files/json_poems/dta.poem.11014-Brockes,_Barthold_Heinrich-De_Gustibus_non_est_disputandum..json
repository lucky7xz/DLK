{"dta.poem.11014": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De  \n Gustibus non est disputandum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist recht, wenn man verbietet, vom Geschmack zu dispu-\ntiren,", "tokens": ["Es", "ist", "recht", ",", "wenn", "man", "ver\u00b7bie\u00b7tet", ",", "vom", "Ge\u00b7schmack", "zu", "dis\u00b7pu", "ti\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$,", "APPRART", "NN", "APPR", "TRUNC", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Aber wie? W\u00e4r alle Welt kl\u00e4rlich hier zu \u00fcberf\u00fchren,", "tokens": ["A\u00b7ber", "wie", "?", "W\u00e4r", "al\u00b7le", "Welt", "kl\u00e4r\u00b7lich", "hier", "zu", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "VAFIN", "PIAT", "NN", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Da\u00df auch Meynungen, Geschmack, und nichts anders in", "tokens": ["Da\u00df", "auch", "Mey\u00b7nun\u00b7gen", ",", "Ge\u00b7schmack", ",", "und", "nichts", "an\u00b7ders", "in"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "$,", "NN", "$,", "KON", "PIS", "ADV", "APPR"], "meter": "--++--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "W\u00e4r zur Wahrheit und zum Frieden wohl kein be\u00dfrer Weg", "tokens": ["W\u00e4r", "zur", "Wahr\u00b7heit", "und", "zum", "Frie\u00b7den", "wohl", "kein", "be\u00df\u00b7rer", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "KON", "APPRART", "NN", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Als da\u00df so, wie im Geschmack, durch Verbindung beyder", "tokens": ["Als", "da\u00df", "so", ",", "wie", "im", "Ge\u00b7schmack", ",", "durch", "Ver\u00b7bin\u00b7dung", "bey\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV", "$,", "PWAV", "APPRART", "NN", "$,", "APPR", "NN", "PTKVZ"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Man von allen raisonniren, nimmer disputiren m\u00fcsse.", "tokens": ["Man", "von", "al\u00b7len", "rai\u00b7son\u00b7ni\u00b7ren", ",", "nim\u00b7mer", "dis\u00b7pu\u00b7ti\u00b7ren", "m\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIS", "VVINF", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}