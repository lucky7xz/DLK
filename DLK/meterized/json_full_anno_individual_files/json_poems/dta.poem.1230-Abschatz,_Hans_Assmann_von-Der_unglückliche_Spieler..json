{"dta.poem.1230": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der ungl\u00fcckliche Spieler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich mich zu spielen wagen?", "tokens": ["Soll", "ich", "mich", "zu", "spie\u00b7len", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hertzen wird mir abgeschlagen/", "tokens": ["Hert\u00b7zen", "wird", "mir", "ab\u00b7ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Amor kehret bey dir ein/", "tokens": ["A\u00b7mor", "keh\u00b7ret", "bey", "dir", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Klebern Buben Stelle/", "tokens": ["An", "des", "Kle\u00b7bern", "Bu\u00b7ben", "Stel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich auch f\u00fcr Urtheil f\u00e4lle/", "tokens": ["Was", "ich", "auch", "f\u00fcr", "Ur\u00b7theil", "f\u00e4l\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df das Spiel verlohren seyn.", "tokens": ["Mu\u00df", "das", "Spiel", "ver\u00b7loh\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}