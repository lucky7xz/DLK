{"dta.poem.15567": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lern' auf die Augen thun, wenn nichts dir soll misgl\u00fccken;", "tokens": ["Lern'", "auf", "die", "Au\u00b7gen", "thun", ",", "wenn", "nichts", "dir", "soll", "mis\u00b7gl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,", "KOUS", "PIS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn dir was misf\u00e4llt, lern' eines zuzudr\u00fccken.", "tokens": ["Und", "wenn", "dir", "was", "mis\u00b7f\u00e4llt", ",", "lern'", "ei\u00b7nes", "zu\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$,", "VVFIN", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}