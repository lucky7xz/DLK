{"textgrid.poem.48112": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Die Stra\u00dfenkehrer", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Norden bl\u00e4st der eisige Wind,", "tokens": ["Aus", "Nor\u00b7den", "bl\u00e4st", "der", "ei\u00b7si\u00b7ge", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "die Flocken wirbeln auf allen Wegen \u2013", "tokens": ["die", "Flo\u00b7cken", "wir\u00b7beln", "auf", "al\u00b7len", "We\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "nimm Schippe und Besen und komm geschwind,", "tokens": ["nimm", "Schip\u00b7pe", "und", "Be\u00b7sen", "und", "komm", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "komm mit, wir wollen die Stra\u00dfe fegen.", "tokens": ["komm", "mit", ",", "wir", "wol\u00b7len", "die", "Stra\u00b7\u00dfe", "fe\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Wie ein Tanzboden, blank und rein,", "tokens": ["Wie", "ein", "Tanz\u00b7bo\u00b7den", ",", "blank", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PTKVZ", "KON", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "mu\u00df heute abend die Stra\u00dfe sein.", "tokens": ["mu\u00df", "heu\u00b7te", "a\u00b7bend", "die", "Stra\u00b7\u00dfe", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie geben im Schlo\u00df den Fastnachtsball;", "tokens": ["Sie", "ge\u00b7ben", "im", "Schlo\u00df", "den", "Fast\u00b7nachts\u00b7ball", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "da d\u00fcrfen wir unten am Fenster stehen", "tokens": ["da", "d\u00fcr\u00b7fen", "wir", "un\u00b7ten", "am", "Fens\u00b7ter", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "und d\u00fcrfen lauschen der Geigen Schall", "tokens": ["und", "d\u00fcr\u00b7fen", "lau\u00b7schen", "der", "Gei\u00b7gen", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "VVINF", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und all die gl\u00e4nzenden Kutschen sehen,", "tokens": ["und", "all", "die", "gl\u00e4n\u00b7zen\u00b7den", "Kut\u00b7schen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "fege, du, fege die Stra\u00dfe rein!", "tokens": ["fe\u00b7ge", ",", "du", ",", "fe\u00b7ge", "die", "Stra\u00b7\u00dfe", "rein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Der Pfaff hat recht, wenn der Winter droht:", "tokens": ["Der", "Pfaff", "hat", "recht", ",", "wenn", "der", "Win\u00b7ter", "droht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Arbeit, Alte, die w\u00e4rmt die Glieder!", "tokens": ["Ar\u00b7beit", ",", "Al\u00b7te", ",", "die", "w\u00e4rmt", "die", "Glie\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wir haben kein Holz, wir haben kein Brot . . .", "tokens": ["Wir", "ha\u00b7ben", "kein", "Holz", ",", "wir", "ha\u00b7ben", "kein", "Brot", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$.", "$.", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ihr himmlischen Flocken nur tanzt hernieder.", "tokens": ["ihr", "himm\u00b7li\u00b7schen", "Flo\u00b7cken", "nur", "tanzt", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Sollt ihr die Helfer der Armen sein? \u2013", "tokens": ["Sollt", "ihr", "die", "Hel\u00b7fer", "der", "Ar\u00b7men", "sein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "VAINF", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Fege, du, fege die Stra\u00dfe rein!", "tokens": ["Fe\u00b7ge", ",", "du", ",", "fe\u00b7ge", "die", "Stra\u00b7\u00dfe", "rein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Aus Norden bl\u00e4st der eisige Wind,", "tokens": ["Aus", "Nor\u00b7den", "bl\u00e4st", "der", "ei\u00b7si\u00b7ge", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "die Flocken wirbeln auf allen Wegen \u2013", "tokens": ["die", "Flo\u00b7cken", "wir\u00b7beln", "auf", "al\u00b7len", "We\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "nimm Schippe und Besen und komm geschwind,", "tokens": ["nimm", "Schip\u00b7pe", "und", "Be\u00b7sen", "und", "komm", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "komm mit, wir wollen die Stra\u00dfe fegen.", "tokens": ["komm", "mit", ",", "wir", "wol\u00b7len", "die", "Stra\u00b7\u00dfe", "fe\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER