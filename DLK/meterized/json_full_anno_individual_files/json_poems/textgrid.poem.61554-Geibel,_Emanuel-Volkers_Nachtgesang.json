 "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es macht mich stark wie firner Wein,", "tokens": ["Es", "macht", "mich", "stark", "wie", "fir\u00b7ner", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das sollen Heunenwunden", "tokens": ["Das", "sol\u00b7len", "Heu\u00b7nen\u00b7wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bekunden.", "tokens": ["Be\u00b7kun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.12": {"line.1": {"text": "Ihr K\u00f6n'ge, sonder Zagen", "tokens": ["Ihr", "K\u00f6n'\u00b7ge", ",", "son\u00b7der", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlaft sanft, ich halte Wacht;", "tokens": ["Schlaft", "sanft", ",", "ich", "hal\u00b7te", "Wacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Glanz aus alten Tagen", "tokens": ["Ein", "Glanz", "aus", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erleuchtet mir die Nacht.", "tokens": ["Er\u00b7leuch\u00b7tet", "mir", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und kommt die Fr\u00fch' im blut'gen Kleid:", "tokens": ["Und", "kommt", "die", "Fr\u00fch'", "im", "blut'\u00b7gen", "Kleid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott gr\u00fc\u00df' dich, grimmer Schwerterstreit!", "tokens": ["Gott", "gr\u00fc\u00df'", "dich", ",", "grim\u00b7mer", "Schwer\u00b7ter\u00b7streit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann magst du, Tod, zum Reigen", "tokens": ["Dann", "magst", "du", ",", "Tod", ",", "zum", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns geigen!", "tokens": ["Uns", "gei\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}