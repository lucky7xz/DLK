{"textgrid.poem.61554": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Volkers Nachtgesang", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die lichten Sterne funkeln", "tokens": ["Die", "lich\u00b7ten", "Ster\u00b7ne", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hernieder kalt und stumm;", "tokens": ["Her\u00b7nie\u00b7der", "kalt", "und", "stumm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Waffen klirrt's im Dunkeln,", "tokens": ["Von", "Waf\u00b7fen", "klirrt's", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Tod schleicht drau\u00dfen um.", "tokens": ["Der", "Tod", "schleicht", "drau\u00b7\u00dfen", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schweb' hoch hinauf, mein Geigenklang!", "tokens": ["Schweb'", "hoch", "hin\u00b7auf", ",", "mein", "Gei\u00b7gen\u00b7klang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchbrich die Nacht mit klarem Sang!", "tokens": ["Durch\u00b7brich", "die", "Nacht", "mit", "kla\u00b7rem", "Sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du wei\u00dft den Spuk von dannen", "tokens": ["Du", "wei\u00dft", "den", "Spuk", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu bannen.", "tokens": ["Zu", "ban\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Wohl finster ist die Stunde,", "tokens": ["Wohl", "fins\u00b7ter", "ist", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch hell sind Mut und Schwert;", "tokens": ["Doch", "hell", "sind", "Mut", "und", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In meines Herzens Grunde", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht aller Freuden Herd.", "tokens": ["Steht", "al\u00b7ler", "Freu\u00b7den", "Herd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Lebenslust, wie reich du bl\u00fchst!", "tokens": ["O", "Le\u00b7bens\u00b7lust", ",", "wie", "reich", "du", "bl\u00fchst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Heldenblut, wie k\u00fchn du gl\u00fchst!", "tokens": ["O", "Hel\u00b7den\u00b7blut", ",", "wie", "k\u00fchn", "du", "gl\u00fchst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie gleicht der Sonn' im Scheiden", "tokens": ["Wie", "gleicht", "der", "Sonn'", "im", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr beiden.", "tokens": ["Ihr", "bei\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PIAT", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Ich denke hoher Ehren,", "tokens": ["Ich", "den\u00b7ke", "ho\u00b7her", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sturmlust'ger Jugendzeit,", "tokens": ["Sturm\u00b7lust'\u00b7ger", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da wir mit scharfen Speeren", "tokens": ["Da", "wir", "mit", "schar\u00b7fen", "Spee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinjauchzten in den Streit.", "tokens": ["Hin\u00b7jauchz\u00b7ten", "in", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hei Schildgekrach im Sachsenkrieg!", "tokens": ["Hei", "Schild\u00b7ge\u00b7krach", "im", "Sach\u00b7sen\u00b7krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf unsern Bannern sa\u00df der Sieg,", "tokens": ["Auf", "un\u00b7sern", "Ban\u00b7nern", "sa\u00df", "der", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als wir die ersten Narben", "tokens": ["Als", "wir", "die", "ers\u00b7ten", "Nar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erwarben.", "tokens": ["Er\u00b7war\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Mein gr\u00fcnes Heimatleben,", "tokens": ["Mein", "gr\u00fc\u00b7nes", "Hei\u00b7mat\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie tauchst du mir empor!", "tokens": ["Wie", "tauchst", "du", "mir", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Schwarzwalds Wipfel weben", "tokens": ["Des", "Schwarz\u00b7walds", "Wip\u00b7fel", "we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Her\u00fcber an mein Ohr;", "tokens": ["Her\u00b7\u00fc\u00b7ber", "an", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So s\u00e4uselt's in der Rebenflur,", "tokens": ["So", "s\u00e4u\u00b7selt's", "in", "der", "Re\u00b7ben\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So braust der Rhein, darauf ich fuhr", "tokens": ["So", "braust", "der", "Rhein", ",", "da\u00b7rauf", "ich", "fuhr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "PAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit meinem Lieb zu zweien", "tokens": ["Mit", "mei\u00b7nem", "Lieb", "zu", "zwei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Maien.", "tokens": ["Im", "Mai\u00b7en", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "O Minne! wunders\u00fc\u00dfe,", "tokens": ["O", "Min\u00b7ne", "!", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Rosenhag in Blust,", "tokens": ["Du", "Ro\u00b7sen\u00b7hag", "in", "Blust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich gr\u00fc\u00dfe dich, ich gr\u00fc\u00dfe", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "ich", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich heut aus tiefster Brust!", "tokens": ["Dich", "heut", "aus", "tiefs\u00b7ter", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du roter Mund, gedenk' ich dein,", "tokens": ["Du", "ro\u00b7ter", "Mund", ",", "ge\u00b7denk'", "ich", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es macht mich stark wie firner Wein,", "tokens": ["Es", "macht", "mich", "stark", "wie", "fir\u00b7ner", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das sollen Heunenwunden", "tokens": ["Das", "sol\u00b7len", "Heu\u00b7nen\u00b7wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bekunden.", "tokens": ["Be\u00b7kun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Ihr K\u00f6n'ge, sonder Zagen", "tokens": ["Ihr", "K\u00f6n'\u00b7ge", ",", "son\u00b7der", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlaft sanft, ich halte Wacht;", "tokens": ["Schlaft", "sanft", ",", "ich", "hal\u00b7te", "Wacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Glanz aus alten Tagen", "tokens": ["Ein", "Glanz", "aus", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erleuchtet mir die Nacht.", "tokens": ["Er\u00b7leuch\u00b7tet", "mir", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und kommt die Fr\u00fch' im blut'gen Kleid:", "tokens": ["Und", "kommt", "die", "Fr\u00fch'", "im", "blut'\u00b7gen", "Kleid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott gr\u00fc\u00df' dich, grimmer Schwerterstreit!", "tokens": ["Gott", "gr\u00fc\u00df'", "dich", ",", "grim\u00b7mer", "Schwer\u00b7ter\u00b7streit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann magst du, Tod, zum Reigen", "tokens": ["Dann", "magst", "du", ",", "Tod", ",", "zum", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns geigen!", "tokens": ["Uns", "gei\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Die lichten Sterne funkeln", "tokens": ["Die", "lich\u00b7ten", "Ster\u00b7ne", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herni