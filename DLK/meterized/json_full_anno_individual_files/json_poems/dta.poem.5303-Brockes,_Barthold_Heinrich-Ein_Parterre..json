{"dta.poem.5303": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Parterre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOtt, was hast du doch alhier,", "tokens": ["Mein", "Gott", ",", "was", "hast", "du", "doch", "al\u00b7hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser Blumen bunten Zier,", "tokens": ["In", "die\u00b7ser", "Blu\u00b7men", "bun\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf diesem bunten Schau-Platz, mir", "tokens": ["Auf", "die\u00b7sem", "bun\u00b7ten", "Schau\u00b7Platz", ",", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "F\u00fcr Weisheit, Lieb\u2019 und Macht gewiesen!", "tokens": ["F\u00fcr", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht", "ge\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach sey daf\u00fcr, da\u00df es so wunder sch\u00f6n,", "tokens": ["Ach", "sey", "da\u00b7f\u00fcr", ",", "da\u00df", "es", "so", "wun\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PAV", "$,", "KOUS", "PPER", "ADV", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von mir und allen, die es sehn,", "tokens": ["Von", "mir", "und", "al\u00b7len", ",", "die", "es", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gelobt, ger\u00fchmet und gepriesen!", "tokens": ["Ge\u00b7lobt", ",", "ge\u00b7r\u00fch\u00b7met", "und", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So rief ich, als ich j\u00fcngst den Platz,", "tokens": ["So", "rief", "ich", ",", "als", "ich", "j\u00fcngst", "den", "Platz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worauf ich kurtz vorher der Beeten Schrancken", "tokens": ["Wo\u00b7rauf", "ich", "kurtz", "vor\u00b7her", "der", "Bee\u00b7ten", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADJD", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Buchsbaum mit geschlungnen Rancken,", "tokens": ["Von", "Buchs\u00b7baum", "mit", "ge\u00b7schlung\u00b7nen", "Ran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht viereckt, wie gew\u00f6hnlich, fassen,", "tokens": ["Nicht", "vier\u00b7eckt", ",", "wie", "ge\u00b7w\u00f6hn\u00b7lich", ",", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PWAV", "ADJD", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und hier und da mit rohtem Sand", "tokens": ["Und", "hier", "und", "da", "mit", "roh\u00b7tem", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bunten Striemchen zieren lassen;", "tokens": ["Und", "bun\u00b7ten", "Striem\u00b7chen", "zie\u00b7ren", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als, sag ich, ich hier diesen Ort,", "tokens": ["Als", ",", "sag", "ich", ",", "ich", "hier", "die\u00b7sen", "Ort", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "$,", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bedeckt, erf\u00fcllt mit einem Schatz,", "tokens": ["Be\u00b7deckt", ",", "er\u00b7f\u00fcllt", "mit", "ei\u00b7nem", "Schatz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von bunten Tulipanen, fand.", "tokens": ["Von", "bun\u00b7ten", "Tu\u00b7li\u00b7pa\u00b7nen", ",", "fand", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Regel-rechte Symmetrie,", "tokens": ["Die", "Re\u00b7gel\u00b7rech\u00b7te", "Sym\u00b7me\u00b7trie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Des Bodens, der in manchem bunten Strich", "tokens": ["Des", "Bo\u00b7dens", ",", "der", "in", "man\u00b7chem", "bun\u00b7ten", "Strich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Selbst bunten Blumen glich,", "tokens": ["Selbst", "bun\u00b7ten", "Blu\u00b7men", "glich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Stand mit der bunten Blumen Menge,", "tokens": ["Stand", "mit", "der", "bun\u00b7ten", "Blu\u00b7men", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und dem fast funckelnden Gepr\u00e4nge,", "tokens": ["Und", "dem", "fast", "fun\u00b7ckeln\u00b7den", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "In einer solchen Harmonie,", "tokens": ["In", "ei\u00b7ner", "sol\u00b7chen", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df jeder, der es sah, erstaunet stille stund,", "tokens": ["Da\u00df", "je\u00b7der", ",", "der", "es", "sah", ",", "er\u00b7stau\u00b7net", "stil\u00b7le", "stund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und, f\u00fcr Verwunderung, so gleich kaum sprechen kunt.", "tokens": ["Und", ",", "f\u00fcr", "Ver\u00b7wun\u00b7de\u00b7rung", ",", "so", "gleich", "kaum", "spre\u00b7chen", "kunt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "$,", "ADV", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich sah ihn j\u00fcngst, recht inniglich vergn\u00fcget,", "tokens": ["Ich", "sah", "ihn", "j\u00fcngst", ",", "recht", "in\u00b7nig\u00b7lich", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Von meinen kleinen Weinberg an,", "tokens": ["Von", "mei\u00b7nen", "klei\u00b7nen", "Wein\u00b7berg", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "An dessen gr\u00fcnem Fu\u00df es lieget;", "tokens": ["An", "des\u00b7sen", "gr\u00fc\u00b7nem", "Fu\u00df", "es", "lie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es waren mir, als ich den bunten Schimmer sahe,", "tokens": ["Es", "wa\u00b7ren", "mir", ",", "als", "ich", "den", "bun\u00b7ten", "Schim\u00b7mer", "sa\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Lust, die Freuden Thr\u00e4nen nahe,", "tokens": ["F\u00fcr", "Lust", ",", "die", "Freu\u00b7den", "Thr\u00e4\u00b7nen", "na\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und fing ich gleich, so bald ich mich besann,", "tokens": ["Und", "fing", "ich", "gleich", ",", "so", "bald", "ich", "mich", "be\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wie zuerst, noch einmahl wieder an:", "tokens": ["So", "wie", "zu\u00b7erst", ",", "noch", "ein\u00b7mahl", "wie\u00b7der", "an", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "$,", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein GOtt, was hast du doch alhier,", "tokens": ["Mein", "Gott", ",", "was", "hast", "du", "doch", "al\u00b7hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser Blumen bunten Zier,", "tokens": ["In", "die\u00b7ser", "Blu\u00b7men", "bun\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf diesem bunten Schau-Platz, mir", "tokens": ["Auf", "die\u00b7sem", "bun\u00b7ten", "Schau\u00b7Platz", ",", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "F\u00fcr Weisheit, Lieb\u2019 und Macht gewiesen!", "tokens": ["F\u00fcr", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht", "ge\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach sey daf\u00fcr, da\u00df es so wunder sch\u00f6n", "tokens": ["Ach", "sey", "da\u00b7f\u00fcr", ",", "da\u00df", "es", "so", "wun\u00b7der", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PAV", "$,", "KOUS", "PPER", "ADV", "ADJA", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von mir und allen, die es sehn,", "tokens": ["Von", "mir", "und", "al\u00b7len", ",", "die", "es", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gelobt, ger\u00fchmet und gepriesen!", "tokens": ["Ge\u00b7lobt", ",", "ge\u00b7r\u00fch\u00b7met", "und", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}