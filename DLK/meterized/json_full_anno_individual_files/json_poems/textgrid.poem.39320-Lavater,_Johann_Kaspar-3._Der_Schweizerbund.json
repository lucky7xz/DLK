{"textgrid.poem.39320": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "3. Der Schweizerbund", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey ewig heilig, Schweizerbund!", "tokens": ["Sey", "e\u00b7wig", "hei\u00b7lig", ",", "Schwei\u00b7zer\u00b7bund", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind vom Joche frey!", "tokens": ["Wir", "sind", "vom", "Jo\u00b7che", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heil schwur uns unsrer V\u00e4ter Mund!", "tokens": ["Heil", "schwur", "uns", "uns\u00b7rer", "V\u00e4\u00b7ter", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heil gab uns ihre Treu.", "tokens": ["Heil", "gab", "uns", "ih\u00b7re", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tyrannen herrschten weit und breit", "tokens": ["Ty\u00b7ran\u00b7nen", "herrschten", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In unserm Vaterland,", "tokens": ["In", "un\u00b7serm", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz voll Stolz und Grausamkeit", "tokens": ["Das", "Herz", "voll", "Stolz", "und", "Grau\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Mord in ihrer Hand!", "tokens": ["Und", "Mord", "in", "ih\u00b7rer", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie truzten, Recht, dir mit Gewalt,", "tokens": ["Sie", "truz\u00b7ten", ",", "Recht", ",", "dir", "mit", "Ge\u00b7walt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald l\u00f6schten sie die Glut", "tokens": ["Bald", "l\u00f6schten", "sie", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der geilen L\u00fcste, raubten bald", "tokens": ["Der", "gei\u00b7len", "L\u00fcs\u00b7te", ",", "raub\u00b7ten", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das schwei\u00dferrungne Gut.", "tokens": ["Das", "schwei\u00b7\u00dfer\u00b7rung\u00b7ne", "Gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbwas, freye Menschen, dulden wir", "tokens": ["\u00bb", "was", ",", "frey\u00b7e", "Men\u00b7schen", ",", "dul\u00b7den", "wir"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PWS", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch lang das Sclavenjoch?", "tokens": ["Noch", "lang", "das", "Scla\u00b7ven\u00b7joch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tyrannen, wi\u00dft! wir sind, wie ihr", "tokens": ["Ty\u00b7ran\u00b7nen", ",", "wi\u00dft", "!", "wir", "sind", ",", "wie", "ihr"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gut, sind Menschen doch\u00ab.", "tokens": ["So", "gut", ",", "sind", "Men\u00b7schen", "doch", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "VAFIN", "NN", "ADV", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So dacht' ein Patriotenklee,", "tokens": ["So", "dacht'", "ein", "Pat\u00b7ri\u00b7o\u00b7ten\u00b7klee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Unmuth gings einher:", "tokens": ["Voll", "Un\u00b7muth", "gings", "ein\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwenn's auch das Leben kostete,", "tokens": ["\u00bb", "wenn's", "auch", "das", "Le\u00b7ben", "kos\u00b7te\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das dulden wir nicht mehr\u00ab.", "tokens": ["Das", "dul\u00b7den", "wir", "nicht", "mehr", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gerecht, o Arnold, ist dein Schmerz!", "tokens": ["Ge\u00b7recht", ",", "o", "Ar\u00b7nold", ",", "ist", "dein", "Schmerz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NE", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmein Vater, ach, ist blind!", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "ach", ",", "ist", "blind", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ITJ", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tyrann\u00ab! \u2013 ja blute Sohnes Herz! \u2013,", "tokens": ["Ty\u00b7rann", "\u00ab", "!", "\u2013", "ja", "blu\u00b7te", "Soh\u00b7nes", "Herz", "!", "\u2013", ","], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$(", "$.", "$(", "ADV", "ADJA", "NN", "NN", "$.", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbum meinetwillen blind\u00ab!", "tokens": ["\u00bb", "um", "mei\u00b7net\u00b7wil\u00b7len", "blind", "\u00ab", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUI", "ADV", "ADJD", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Von Staufach dich vertreibt und h\u00f6hnt", "tokens": ["Von", "Stau\u00b7fach", "dich", "ver\u00b7treibt", "und", "h\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Landvogts \u00dcbermuth,", "tokens": ["Des", "Land\u00b7vogts", "\u00dc\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der dir dein neues Haus mi\u00dfg\u00f6nnt,", "tokens": ["Der", "dir", "dein", "neu\u00b7es", "Haus", "mi\u00df\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebaut aus eignem Gut!", "tokens": ["Ge\u00b7baut", "aus", "eig\u00b7nem", "Gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und du nimmst willig, Walther F\u00fcrst,", "tokens": ["Und", "du", "nimmst", "wil\u00b7lig", ",", "Walt\u00b7her", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich der Bedr\u00e4ngten an.", "tokens": ["Dich", "der", "Be\u00b7dr\u00e4ng\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie wissen, da\u00df du helfen wirst,", "tokens": ["Sie", "wis\u00b7sen", ",", "da\u00df", "du", "hel\u00b7fen", "wirst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo mann nur helfen kann!", "tokens": ["Wo", "mann", "nur", "hel\u00b7fen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Des Vaterlandes Jammer liegt", "tokens": ["Des", "Va\u00b7ter\u00b7lan\u00b7des", "Jam\u00b7mer", "liegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Eurer Schulter schwer!", "tokens": ["Auf", "Eu\u00b7rer", "Schul\u00b7ter", "schwer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sehet alles Recht besiegt", "tokens": ["Ihr", "se\u00b7het", "al\u00b7les", "Recht", "be\u00b7siegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles hofnungleer.", "tokens": ["Und", "al\u00b7les", "hof\u00b7nun\u00b7gleer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Erbliket manches sch\u00f6ne Thal", "tokens": ["Er\u00b7bli\u00b7ket", "man\u00b7ches", "sch\u00f6\u00b7ne", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und manche Alpenh\u00f6h,", "tokens": ["Und", "man\u00b7che", "Al\u00b7pen\u00b7h\u00f6h", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alles, Menschen ohne Zahl,", "tokens": ["Und", "al\u00b7les", ",", "Men\u00b7schen", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Unmuth, Ach und Weh!", "tokens": ["Voll", "Un\u00b7muth", ",", "Ach", "und", "Weh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ITJ", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auch weint das k\u00fcnfftige Geschlecht", "tokens": ["Auch", "weint", "das", "k\u00fcnff\u00b7ti\u00b7ge", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut in der Helden Ohr:", "tokens": ["Laut", "in", "der", "Hel\u00b7den", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u00bbhebt, V\u00e4ter, \u2013 denn Gott hilft dem Recht \u2013", "tokens": ["\u00bb", "hebt", ",", "V\u00e4\u00b7ter", ",", "\u2013", "denn", "Gott", "hilft", "dem", "Recht", "\u2013"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "$(", "KON", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Zu Gott die H\u00e4nd empor\u00ab!", "tokens": ["Zu", "Gott", "die", "H\u00e4nd", "em\u00b7por", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKVZ", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da schwuren sie den theuren Eid", "tokens": ["Da", "schwu\u00b7ren", "sie", "den", "theu\u00b7ren", "Eid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlugen Hand in Hand", "tokens": ["Und", "schlu\u00b7gen", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu retten von der Dienstbarkeit", "tokens": ["Zu", "ret\u00b7ten", "von", "der", "Dienst\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das liebe Vaterland.", "tokens": ["Das", "lie\u00b7be", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die stille felsigte Natur", "tokens": ["Die", "stil\u00b7le", "fel\u00b7sig\u00b7te", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah sie auf ihrem Knie,", "tokens": ["Sah", "sie", "auf", "ih\u00b7rem", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Himmel h\u00f6rte Gott den Schwur", "tokens": ["Im", "Him\u00b7mel", "h\u00f6r\u00b7te", "Gott", "den", "Schwur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und blickte Muth auf sie.", "tokens": ["Und", "blick\u00b7te", "Muth", "auf", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Still dr\u00fckte jeder seinem Freund", "tokens": ["Still", "dr\u00fck\u00b7te", "je\u00b7der", "sei\u00b7nem", "Freund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Hand: \u00bbSey Patriot\u00ab!", "tokens": ["Die", "Hand", ":", "\u00bb", "Sey", "Pat\u00b7ri\u00b7ot", "\u00ab", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "VAFIN", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder schwur, indem er weint,", "tokens": ["Und", "je\u00b7der", "schwur", ",", "in\u00b7dem", "er", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tyrannie den Tod.", "tokens": ["Der", "Ty\u00b7ran\u00b7nie", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Es kam die lang erseufzte Nacht,", "tokens": ["Es", "kam", "die", "lang", "er\u00b7seufz\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sie umarmten sich", "tokens": ["Und", "sie", "um\u00b7arm\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und stiegen jeder wolbewacht", "tokens": ["Und", "stie\u00b7gen", "je\u00b7der", "wol\u00b7be\u00b7wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON