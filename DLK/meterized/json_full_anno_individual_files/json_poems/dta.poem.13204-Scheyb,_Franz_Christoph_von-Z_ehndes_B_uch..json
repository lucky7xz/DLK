{"dta.poem.13204": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas geb\u2019 ich aber euch zum Pfand der Danckbarkeit?", "tokens": ["\u201e", "was", "geb'", "ich", "a\u00b7ber", "euch", "zum", "Pfand", "der", "Dan\u00b7ck\u00b7bar\u00b7keit", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "\u201enach GOtt habt ihr den Thron von der Gefahr befreyt.", "tokens": ["\u201e", "nach", "Gott", "habt", "ihr", "den", "Thron", "von", "der", "Ge\u00b7fahr", "be\u00b7freyt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201enichts ist, was ich von GOtt zu meinem Schuz begehrte,", "tokens": ["\u201e", "nichts", "ist", ",", "was", "ich", "von", "Gott", "zu", "mei\u00b7nem", "Schuz", "be\u00b7gehr\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "$,", "PWS", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewas er nicht wunderbar durch euer Amt gew\u00e4hrte.", "tokens": ["\u201e", "was", "er", "nicht", "wun\u00b7der\u00b7bar", "durch", "eu\u00b7er", "Amt", "ge\u00b7w\u00e4hr\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eso starck der Feinde Macht, so gro\u00df war eure Treu;", "tokens": ["\u201e", "so", "starck", "der", "Fein\u00b7de", "Macht", ",", "so", "gro\u00df", "war", "eu\u00b7re", "Treu", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "ART", "NN", "NN", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "270\u201dSo sagt! was euch zum Lohn das angenehmste sey.", "tokens": ["\"", "So", "sagt", "!", "was", "euch", "zum", "Lohn", "das", "an\u00b7ge\u00b7nehms\u00b7te", "sey", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$.", "PWS", "PPER", "APPRART", "NN", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eselbst die Verwunderung der Welt kann Zeugnis geben,", "tokens": ["\u201e", "selbst", "die", "Ver\u00b7wun\u00b7de\u00b7rung", "der", "Welt", "kann", "Zeug\u00b7nis", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "ART", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda\u00df eure Kr\u00e4fte nur nach meiner Wohlfart streben.", "tokens": ["\u201e", "da\u00df", "eu\u00b7re", "Kr\u00e4f\u00b7te", "nur", "nach", "mei\u00b7ner", "Wohl\u00b7fart", "stre\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}