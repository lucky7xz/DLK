{"textgrid.poem.31699": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fr\u00fchlingswolken wehen,", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7wol\u00b7ken", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Tal ist frisch und gr\u00fcn,", "tokens": ["Das", "Tal", "ist", "frisch", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob auf den n\u00e4chsten H\u00f6hen", "tokens": ["Ob", "auf", "den", "n\u00e4chs\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl schon die Reben bl\u00fchn?", "tokens": ["Wohl", "schon", "die", "Re\u00b7ben", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schon lange ist's, gar lange,", "tokens": ["Schon", "lan\u00b7ge", "ist's", ",", "gar", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da hab ich sie gepflanzt,", "tokens": ["Da", "hab", "ich", "sie", "ge\u00b7pflanzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort wo am Bergeshange", "tokens": ["Dort", "wo", "am", "Ber\u00b7ges\u00b7han\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Nacht die Elfe tanzt.", "tokens": ["Bei", "Nacht", "die", "El\u00b7fe", "tanzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bat um ihren Segen,", "tokens": ["Ich", "bat", "um", "ih\u00b7ren", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich flehte: \u00bbLichter Geist,", "tokens": ["Ich", "fleh\u00b7te", ":", "\u00bb", "Lich\u00b7ter", "Geist", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O m\u00f6chtest du doch pflegen", "tokens": ["O", "m\u00f6ch\u00b7test", "du", "doch", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die junge Saat zumeist.", "tokens": ["Die", "jun\u00b7ge", "Saat", "zu\u00b7meist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du lockst die Morgenwolke,", "tokens": ["Du", "lockst", "die", "Mor\u00b7gen\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df sie sich rauschend senkt,", "tokens": ["Da\u00df", "sie", "sich", "rau\u00b7schend", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie dem Blumenvolke", "tokens": ["Da\u00df", "sie", "dem", "Blu\u00b7men\u00b7vol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den sanften Regen schenkt.", "tokens": ["Den", "sanf\u00b7ten", "Re\u00b7gen", "schenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn deine Kinder sind sie,", "tokens": ["Denn", "dei\u00b7ne", "Kin\u00b7der", "sind", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Bl\u00fcten rot und wei\u00df,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "rot", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du schlingest zum Gewind sie", "tokens": ["Du", "schlin\u00b7gest", "zum", "Ge\u00b7wind", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Knospe, Blatt und Reis.", "tokens": ["Mit", "Knos\u00b7pe", ",", "Blatt", "und", "Reis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O sieh auch meine Reben", "tokens": ["O", "sieh", "auch", "mei\u00b7ne", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit holdem Auge an,", "tokens": ["Mit", "hol\u00b7dem", "Au\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mir im Herbste beben", "tokens": ["Da\u00df", "mir", "im", "Herbs\u00b7te", "be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel goldne Trauben dran.\u00ab", "tokens": ["Viel", "gold\u00b7ne", "Trau\u00b7ben", "dran", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So sprach ich, und erf\u00fcllet", "tokens": ["So", "sprach", "ich", ",", "und", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist, was ich einst gedacht,", "tokens": ["Ist", ",", "was", "ich", "einst", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Sehnen ist gestillet:", "tokens": ["Mein", "Seh\u00b7nen", "ist", "ge\u00b7stil\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn lieblich \u00fcber Nacht,", "tokens": ["Denn", "lieb\u00b7lich", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als sich ein Meer ergossen", "tokens": ["Als", "sich", "ein", "Meer", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Bl\u00fcten rot und wei\u00df,", "tokens": ["Von", "Bl\u00fc\u00b7ten", "rot", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erhuben alle Sprossen", "tokens": ["Er\u00b7hu\u00b7ben", "al\u00b7le", "Spros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein junges Bl\u00fctenreis,", "tokens": ["Ein", "jun\u00b7ges", "Bl\u00fc\u00b7ten\u00b7reis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und duftend ziehn die Ranken", "tokens": ["Und", "duf\u00b7tend", "ziehn", "die", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Himmel ihre Bahn,", "tokens": ["Zum", "Him\u00b7mel", "ih\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie winden an den schlanken", "tokens": ["Sie", "win\u00b7den", "an", "den", "schlan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Burgt\u00fcrmen sich hinan.", "tokens": ["Burg\u00b7t\u00fcr\u00b7men", "sich", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Mich deucht: nach stillem Gr\u00fc\u00dfen", "tokens": ["Mich", "deucht", ":", "nach", "stil\u00b7lem", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "All ihr Verlangen geht,", "tokens": ["All", "ihr", "Ver\u00b7lan\u00b7gen", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Rosenstrauch zu k\u00fcssen,", "tokens": ["Den", "Ro\u00b7sen\u00b7strauch", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der hoch im Erker steht.", "tokens": ["Der", "hoch", "im", "Er\u00b7ker", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Fr\u00fchlingswolken wehen,", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7wol\u00b7ken", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Tal ist frisch und gr\u00fcn,", "tokens": ["Das", "Tal", "ist", "frisch", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "i