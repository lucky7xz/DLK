rhin umschw\u00e4rme,", "tokens": ["Bl\u00fch\u00b7te", "nicht", ",", "da\u00df", "ihn", "dein", "Schwin\u00b7gen", "von", "vor\u00b7hin", "um\u00b7schw\u00e4r\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+---+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "pl\u00f6tzlich sein Wipfel von Stille? Und \u00fcber ihr,", "tokens": ["pl\u00f6tz\u00b7lich", "sein", "Wip\u00b7fel", "von", "Stil\u00b7le", "?", "Und", "\u00fc\u00b7ber", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "APPR", "NN", "$.", "KON", "APPR", "PPER", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "war sie nicht Sonne, war sie nicht Sommer, die W\u00e4rme,", "tokens": ["war", "sie", "nicht", "Son\u00b7ne", ",", "war", "sie", "nicht", "Som\u00b7mer", ",", "die", "W\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "diese unz\u00e4hlige W\u00e4rme aus dir?", "tokens": ["die\u00b7se", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "W\u00e4r\u00b7me", "aus", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-++--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber er trug auch, er trug, dein Baum der Ekstase.", "tokens": ["A\u00b7ber", "er", "trug", "auch", ",", "er", "trug", ",", "dein", "Baum", "der", "Eks\u00b7ta\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "wo