{"textgrid.poem.48481": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast, o liebstes Lieb, mein Herz' in deinem Herzen!", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast, o liebstes Lieb, mein Herz' in deinem Herzen!", "tokens": ["Du", "hast", ",", "o", "liebs\u00b7tes", "Lieb", ",", "mein", "Her\u00b7z'", "in", "dei\u00b7nem", "Her\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dir, in dir es ist, nach dem ich w\u00fcndsche sehr,", "tokens": ["In", "dir", ",", "in", "dir", "es", "ist", ",", "nach", "dem", "ich", "w\u00fcnd\u00b7sche", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "PPER", "VAFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das ich such' \u00fcberall mit ach! wie gro\u00dfen Schmerzen,", "tokens": ["das", "ich", "such'", "\u00fc\u00b7be\u00b7rall", "mit", "ach", "!", "wie", "gro\u00b7\u00dfen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "ADV", "APPR", "ITJ", "$.", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in dir, in dir es ist und sonsten nirgends mehr.", "tokens": ["in", "dir", ",", "in", "dir", "es", "ist", "und", "sons\u00b7ten", "nir\u00b7gends", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "PPER", "VAFIN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! Liebste, lasse mich dein M\u00fcndelein betrachten,", "tokens": ["Ach", "!", "Liebs\u00b7te", ",", "las\u00b7se", "mich", "dein", "M\u00fcn\u00b7del\u00b7ein", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "tu doch die Lippen auf, auf da\u00df ich sehe drein!", "tokens": ["tu", "doch", "die", "Lip\u00b7pen", "auf", ",", "auf", "da\u00df", "ich", "se\u00b7he", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$,", "APPR", "KOUS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Ach! ach! wie \u00e4ngstet sichs! Itzt wird es gar verschmachten,", "tokens": ["Ach", "!", "ach", "!", "wie", "\u00e4ngs\u00b7tet", "sichs", "!", "Itzt", "wird", "es", "gar", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "XY", "$.", "PWAV", "VVFIN", "PIS", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "weil es so mit Gewalt dir mu\u00df gefangen sein.", "tokens": ["weil", "es", "so", "mit", "Ge\u00b7walt", "dir", "mu\u00df", "ge\u00b7fan\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "