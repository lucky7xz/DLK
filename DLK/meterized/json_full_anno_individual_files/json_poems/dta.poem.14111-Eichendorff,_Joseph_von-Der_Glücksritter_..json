{"dta.poem.14111": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fccksritter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und bald r\u00fcckt sie sacht zu mir:", "tokens": ["Und", "bald", "r\u00fcckt", "sie", "sacht", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201ehast Du deren mehr?\u201c \u2014", "tokens": ["\u201e", "hast", "Du", "de\u00b7ren", "mehr", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "PDS", "ADV", "$.", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie Sie sehn. \u2014 \u201eDrei Kannen schier,", "tokens": ["Wie", "Sie", "sehn", ".", "\u201e", "Drei", "Kan\u00b7nen", "schier", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$.", "$(", "$(", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und das lauter Klebebier!\u201c \u2014", "tokens": ["Und", "das", "lau\u00b7ter", "Kle\u00b7be\u00b7bier", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "'s wird mir gar nicht schwer.", "tokens": ["'s", "wird", "mir", "gar", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}