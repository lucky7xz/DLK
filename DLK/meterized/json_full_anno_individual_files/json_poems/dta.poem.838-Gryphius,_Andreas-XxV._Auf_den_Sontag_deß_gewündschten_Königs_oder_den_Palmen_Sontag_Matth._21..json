{"dta.poem.838": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxV.  Auf den Sontag de\u00df gew\u00fcndschten  \n K\u00f6nigs/ oder den Palmen Sontag/  \n Matth. 21.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Schaw Zion/ schaw dein Printz von welchem l\u00e4ngst ge- ", "tokens": ["Schaw", "Zi\u00b7on", "/", "schaw", "dein", "Printz", "von", "wel\u00b7chem", "l\u00e4ngst", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "VVFIN", "PPOSAT", "NN", "APPR", "PRELS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schrieben/ ", "tokens": ["schrie\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dein Seeligmacher k\u00f6m\u2019t/ der willigst alles thutt", "tokens": ["Dein", "See\u00b7lig\u00b7ma\u00b7cher", "k\u00f6m't", "/", "der", "wil\u00b7ligst", "al\u00b7les", "thutt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "ART", "VMFIN", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was Gott sein Vatter schleu\u00dft/ in dessen sanfften mutt", "tokens": ["Was", "Gott", "sein", "Vat\u00b7ter", "schleu\u00dft", "/", "in", "des\u00b7sen", "sanff\u00b7ten", "mutt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "PPOSAT", "NN", "VVFIN", "$(", "APPR", "PDS", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch einig rechte Trew/ (die sonst verschwunden/) blieben/", "tokens": ["Noch", "ei\u00b7nig", "rech\u00b7te", "Trew", "/", "(", "die", "sonst", "ver\u00b7schwun\u00b7den", "/", ")", "blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$(", "$(", "ART", "ADV", "VVPP", "$(", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er ists der Helffer heist/ der few\u2019rig dich zu lieben", "tokens": ["Er", "ists", "der", "Helf\u00b7fer", "heist", "/", "der", "fe\u00b7w'\u00b7rig", "dich", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$(", "ART", "ADJD", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vnd srey zu machen eyl\u2019t der durch sein tew\u2019res Blutt", "tokens": ["Vnd", "srey", "zu", "ma\u00b7chen", "eyl't", "der", "durch", "sein", "te\u00b7w'\u00b7res", "Blutt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PTKZU", "VVINF", "VVFIN", "ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Lescht deiner fl\u00fcche Plitz/ vnd deiner Straffen glutt/", "tokens": ["Lescht", "dei\u00b7ner", "fl\u00fc\u00b7che", "Plitz", "/", "vnd", "dei\u00b7ner", "Straf\u00b7fen", "glutt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd einzeucht/ das nicht d\u00fcrff\u2019st ewig seyn vertrieben", "tokens": ["Vnd", "ein\u00b7zeucht", "/", "das", "nicht", "d\u00fcrff'st", "e\u00b7wig", "seyn", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "$(", "PDS", "PTKNEG", "ADV", "ADJD", "VAINF", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Hosanna Davids Kind: Hosanna h\u00f6chster GOtt!", "tokens": ["Ho\u00b7san\u00b7na", "Da\u00b7vids", "Kind", ":", "Ho\u00b7san\u00b7na", "h\u00f6chs\u00b7ter", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Lob sey dir der du dich gibst in den todt vnd spott:", "tokens": ["Lob", "sey", "dir", "der", "du", "dich", "gibst", "in", "den", "todt", "vnd", "spott", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "PPER", "PRF", "VVFIN", "APPR", "ART", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd einen Knecht f\u00fcr mich/ Mein K\u00f6nig dich erkl\u00e4rest", "tokens": ["Vnd", "ei\u00b7nen", "Knecht", "f\u00fcr", "mich", "/", "Mein", "K\u00f6\u00b7nig", "dich", "er\u00b7kl\u00e4\u00b7rest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "$(", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Lob dir/ der du von vn\u00df die S\u00fcnden b\u00fcrd auffhebst/", "tokens": ["Lob", "dir", "/", "der", "du", "von", "vn\u00df", "die", "S\u00fcn\u00b7den", "b\u00fcrd", "auff\u00b7hebst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PRELS", "PPER", "APPR", "APPR", "ART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr vnser Leben st\u00fcrbst/ f\u00fcr vnser sterben lebst/", "tokens": ["F\u00fcr", "vn\u00b7ser", "Le\u00b7ben", "st\u00fcrbst", "/", "f\u00fcr", "vn\u00b7ser", "ster\u00b7ben", "lebst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "APPR", "PPOSAT", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd vn\u00df f\u00fcr Schande Ruhmb/ vnd Lust f\u00fcr pein gewehrest.", "tokens": ["Vnd", "vn\u00df", "f\u00fcr", "Schan\u00b7de", "Ruhmb", "/", "vnd", "Lust", "f\u00fcr", "pein", "ge\u00b7weh\u00b7rest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$(", "KON", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}