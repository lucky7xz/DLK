{"textgrid.poem.32625": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Mein Br\u00e4utchen.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, ich harre schon der Stunde,", "tokens": ["Gott", ",", "ich", "har\u00b7re", "schon", "der", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ja so begehrenswert,", "tokens": ["Die", "ja", "so", "be\u00b7geh\u00b7rens\u00b7wert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ein Br\u00e4utchen ich umarme,", "tokens": ["Da", "ein", "Br\u00e4ut\u00b7chen", "ich", "um\u00b7ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mein Schicksal mir beschert.", "tokens": ["Das", "mein", "Schick\u00b7sal", "mir", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gerne wollt' ich jetzt schon wissen,", "tokens": ["Ger\u00b7ne", "wollt'", "ich", "jetzt", "schon", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer sie ist, und wie sie ist, \u2013", "tokens": ["Wer", "sie", "ist", ",", "und", "wie", "sie", "ist", ",", "\u2013"],