{"dta.poem.2455": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Obsequium amicos veritas odium  \n parit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Heut h\u00e4lt man den sehr wehrt der mit verf\u00e4lschtem", "tokens": ["Heut", "h\u00e4lt", "man", "den", "sehr", "wehrt", "der", "mit", "ver\u00b7f\u00e4lschtem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADV", "VVFIN", "ART", "APPR", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Munde/", "tokens": ["Mun\u00b7de", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Frey aufzuschneiden wei\u00df/ wer aber Warheit liebt/", "tokens": ["Frey", "auf\u00b7zu\u00b7schnei\u00b7den", "wei\u00df", "/", "wer", "a\u00b7ber", "War\u00b7heit", "liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIZU", "VVFIN", "$(", "PWS", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und gehet gleich hindurch wird offtmals hochbetr\u00fcbt", "tokens": ["Und", "ge\u00b7het", "gleich", "hin\u00b7durch", "wird", "offt\u00b7mals", "hoch\u00b7be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PAV", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man hasst und neidet ihn: Man wil ihn iede Stunde/", "tokens": ["Man", "hasst", "und", "nei\u00b7det", "ihn", ":", "Man", "wil", "ihn", "ie\u00b7de", "Stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PPER", "$.", "PIS", "VMFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Begraben eh er stirbt/ ja auch mit Haut und Haar.", "tokens": ["Be\u00b7gra\u00b7ben", "eh", "er", "stirbt", "/", "ja", "auch", "mit", "Haut", "und", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVFIN", "$(", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch diesem sey also/ Es bleibt doch endlich war:", "tokens": ["Doch", "die\u00b7sem", "sey", "al\u00b7so", "/", "Es", "bleibt", "doch", "end\u00b7lich", "war", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "$(", "PPER", "VVFIN", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Warheit schwimmt empor/ die L\u00fcgen", "tokens": ["Die", "War\u00b7heit", "schwimmt", "em\u00b7por", "/", "die", "L\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "geht zu grunde.", "tokens": ["geht", "zu", "grun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}