{"textgrid.poem.52180": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Ende des Festes", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da mit Sokrates die Freunde tranken,", "tokens": ["Da", "mit", "Sok\u00b7ra\u00b7tes", "die", "Freun\u00b7de", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die H\u00e4upter auf die Polster sanken,", "tokens": ["Und", "die", "H\u00e4up\u00b7ter", "auf", "die", "Pols\u00b7ter", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kam ein J\u00fcngling, kann ich mich entsinnen,", "tokens": ["Kam", "ein", "J\u00fcng\u00b7ling", ",", "kann", "ich", "mich", "ent\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit zwei schlanken Fl\u00f6tenbl\u00e4serinnen.", "tokens": ["Mit", "zwei", "schlan\u00b7ken", "Fl\u00f6\u00b7ten\u00b7bl\u00e4\u00b7se\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aus den Kelchen sch\u00fctten wir die Neigen,", "tokens": ["Aus", "den", "Kel\u00b7chen", "sch\u00fct\u00b7ten", "wir", "die", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die gespr\u00e4chesm\u00fcden Lippen schweigen,", "tokens": ["Die", "ge\u00b7spr\u00e4\u00b7ches\u00b7m\u00fc\u00b7den", "Lip\u00b7pen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Um die welken Kr\u00e4nze zieht ein Singen...", "tokens": ["Um", "die", "wel\u00b7ken", "Kr\u00e4n\u00b7ze", "zieht", "ein", "Sin\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Still! Des Todes Schlummerfl\u00f6ten klingen!", "tokens": ["Still", "!", "Des", "To\u00b7des", "Schlum\u00b7mer\u00b7fl\u00f6\u00b7ten", "klin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Da mit Sokrates die Freunde tranken,", "tokens": ["Da", "mit", "Sok\u00b7ra\u00b7tes", "die", "Freun\u00b7de", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die H\u00e4upter auf die Polster sanken,", "tokens": ["Und", "die", "H\u00e4up\u00b7ter", "auf", "die", "Pols\u00b7ter", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kam ein J\u00fcngling, kann ich mich entsinnen,", "tokens": 