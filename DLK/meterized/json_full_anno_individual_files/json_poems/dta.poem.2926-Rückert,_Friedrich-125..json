{"dta.poem.2926": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "125.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die bessre Seel' ist nicht, die nur hat bessre Kr\u00e4fte,", "tokens": ["Die", "bess\u00b7re", "Seel'", "ist", "nicht", ",", "die", "nur", "hat", "bess\u00b7re", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "$,", "PRELS", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie von Geburt ein Leib vorm andern bessre S\u00e4fte.", "tokens": ["Wie", "von", "Ge\u00b7burt", "ein", "Leib", "vorm", "an\u00b7dern", "bess\u00b7re", "S\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die bessre Seel' ist, die von den auf ihrer Flur", "tokens": ["Die", "bess\u00b7re", "Seel'", "ist", ",", "die", "von", "den", "auf", "ih\u00b7rer", "Flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewachsnen Kr\u00e4ften mehr gebraucht die bessern nur;", "tokens": ["Ge\u00b7wachs\u00b7nen", "Kr\u00e4f\u00b7ten", "mehr", "ge\u00b7braucht", "die", "bes\u00b7sern", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP