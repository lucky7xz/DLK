{"dta.poem.21629": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Sie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Langweilig plump ist auch ein jeder Fu\u00df \u2014", "tokens": ["Lang\u00b7wei\u00b7lig", "plump", "ist", "auch", "ein", "je\u00b7der", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "ADV", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon brennen am Klavier die Schreckenslichter,", "tokens": ["Schon", "bren\u00b7nen", "am", "Kla\u00b7vier", "die", "Schre\u00b7ckens\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Man ahnt gequ\u00e4lt den kommenden Genu\u00df.", "tokens": ["Man", "ahnt", "ge\u00b7qu\u00e4lt", "den", "kom\u00b7men\u00b7den", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da sah ich sie \u2014 die Fee der Mondenstrahlen \u2014", "tokens": ["Da", "sah", "ich", "sie", "die", "Fee", "der", "Mon\u00b7dens\u00b7trah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die rosig unter allen Basen stand \u2014", "tokens": ["Die", "ro\u00b7sig", "un\u00b7ter", "al\u00b7len", "Ba\u00b7sen", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich wett\u2019, die Stiefelchen sind Wallnu\u00dfschalen,", "tokens": ["Ich", "wett'", ",", "die", "Stie\u00b7fel\u00b7chen", "sind", "Wall\u00b7nu\u00df\u00b7scha\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und Spinnweb ist das duftige Gewand!", "tokens": ["Und", "Spinn\u00b7web", "ist", "das", "duf\u00b7ti\u00b7ge", "Ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Hin huschte sie \u2014 da schien es mir, es biegen", "tokens": ["Hin", "huschte", "sie", "da", "schien", "es", "mir", ",", "es", "bie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$(", "ADV", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die St\u00fchle sich der Zauberk\u00f6nigin \u2014 \u2014 \u2014", "tokens": ["Die", "St\u00fch\u00b7le", "sich", "der", "Zau\u00b7ber\u00b7k\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "PRF", "ART", "NN", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die beiden F\u00fc\u00dfchen, sieh zwei lose Fliegen", "tokens": ["Die", "bei\u00b7den", "F\u00fc\u00df\u00b7chen", ",", "sieh", "zwei", "lo\u00b7se", "Flie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "VVIMP", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die huschen neckisch \u00fcber\u2019m Teppich hin.", "tokens": ["Die", "hu\u00b7schen", "ne\u00b7ckisch", "\u00fc\u00b7ber'm", "Tep\u00b7pich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zwei wilde Fliegen hasten sie vor\u00fcber,", "tokens": ["Zwei", "wil\u00b7de", "Flie\u00b7gen", "has\u00b7ten", "sie", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Purr \u2014 surr, so t\u00f6nt\u2019s dem bleichen Tr\u00e4umer zu \u2014", "tokens": ["Purr", "surr", ",", "so", "t\u00f6nt's", "dem", "blei\u00b7chen", "Tr\u00e4u\u00b7mer", "zu"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Da seufzt er auf, sein blaues Aug\u2019 wird tr\u00fcber,", "tokens": ["Da", "seufzt", "er", "auf", ",", "sein", "blau\u00b7es", "Aug'", "wird", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sie fing sein Herz in ihrem kleinen Schuh.", "tokens": ["Sie", "fing", "sein", "Herz", "in", "ih\u00b7rem", "klei\u00b7nen", "Schuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und Hochzeit ward\u2019s. O s\u00fc\u00dfe, fl\u00fccht\u2019ge Stunde,", "tokens": ["Und", "Hoch\u00b7zeit", "ward'", "s.", "O", "s\u00fc\u00b7\u00dfe", ",", "fl\u00fccht'\u00b7ge", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NE", "NE", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da endlich man zum ersten Mal allein!", "tokens": ["Da", "end\u00b7lich", "man", "zum", "ers\u00b7ten", "Mal", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die alte Wanduhr schnarrt\u2019 mit rauhem Munde", "tokens": ["Die", "al\u00b7te", "Wan\u00b7duhr", "schnarrt'", "mit", "rau\u00b7hem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mi\u00dfmuthig just ein lautes \u201eEin.\u201c \u2014", "tokens": ["Mi\u00df\u00b7mut\u00b7hig", "just", "ein", "lau\u00b7tes", "\u201e", "Ein", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "$(", "ART", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da klopfte er an seines Himmels Pforte,", "tokens": ["Da", "klopf\u00b7te", "er", "an", "sei\u00b7nes", "Him\u00b7mels", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein leises Husten scholl zu ihm heraus \u2014 \u2014", "tokens": ["Ein", "lei\u00b7ses", "Hus\u00b7ten", "scholl", "zu", "ihm", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O sch\u00f6ner wohl als alle leeren Worte", "tokens": ["O", "sch\u00f6\u00b7ner", "wohl", "als", "al\u00b7le", "lee\u00b7ren", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADV", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sagt dieser Klang: \u201eTritt ein, du bist zu Haus\u201c \u2014", "tokens": ["Sagt", "die\u00b7ser", "Klang", ":", "\u201e", "Tritt", "ein", ",", "du", "bist", "zu", "Haus", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$.", "$(", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "APPR", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Schnell trat er ein \u2014 vom Kissen fast bedecket", "tokens": ["Schnell", "trat", "er", "ein", "vom", "Kis\u00b7sen", "fast", "be\u00b7de\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "$(", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "So lag sie da, ein wildes V\u00f6gelein \u2014", "tokens": ["So", "lag", "sie", "da", ",", "ein", "wil\u00b7des", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein M\u00e4uschen, das sich zitternd hat verstecket,", "tokens": ["Ein", "M\u00e4u\u00b7schen", ",", "das", "sich", "zit\u00b7ternd", "hat", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da just der graue Kater tritt herein.", "tokens": ["Da", "just", "der", "grau\u00b7e", "Ka\u00b7ter", "tritt", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da warf er sich, sie st\u00fcrmisch k\u00fcssend, nieder.", "tokens": ["Da", "warf", "er", "sich", ",", "sie", "st\u00fcr\u00b7misch", "k\u00fcs\u00b7send", ",", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJD", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sie k\u00fc\u00dfte wieder, doch so bang, so scheu \u2014 \u2014", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "wie\u00b7der", ",", "doch", "so", "bang", ",", "so", "scheu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Kehrt auch die sch\u00f6ne Stunde nimmer wieder,", "tokens": ["Kehrt", "auch", "die", "sch\u00f6\u00b7ne", "Stun\u00b7de", "nim\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sie schaffet sch\u00f6ne Stunden immer neu!", "tokens": ["Sie", "schaf\u00b7fet", "sch\u00f6\u00b7ne", "Stun\u00b7den", "im\u00b7mer", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}