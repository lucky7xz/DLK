{"textgrid.poem.29822": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib mir, wilstu mir was geben, Armut nicht, HErr, Reichthum nicht!", "tokens": ["Gib", "mir", ",", "wils\u00b7tu", "mir", "was", "ge\u00b7ben", ",", "Ar\u00b7mut", "nicht", ",", "Herr", ",", "Reicht\u00b7hum", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VMFIN", "PPER", "PIS", "VVINF", "$,", "NN", "PTKNEG", "$,", "NN", "$,", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dieses m\u00f6cht au\u00df deinen Furchten reissen mich in seine Pflicht;", "tokens": ["Die\u00b7ses", "m\u00f6cht", "au\u00df", "dei\u00b7nen", "Furch\u00b7ten", "reis\u00b7sen", "mich", "in", "sei\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jenes d\u00fcrffte zwingen mich, mich durch Unrecht zu ern\u00e4hren;", "tokens": ["Je\u00b7nes", "d\u00fcrff\u00b7te", "zwin\u00b7gen", "mich", ",", "mich", "durch", "Un\u00b7recht", "zu", "er\u00b7n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "PPER", "$,", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Dorte d\u00fcrfft ich leugnen GOtt, hier den Nechsten arg beschweren.", "tokens": ["Dor\u00b7te", "d\u00fcrfft", "ich", "leug\u00b7nen", "Gott", ",", "hier", "den", "Nechs\u00b7ten", "arg", "be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "NN", "$,", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Gib mir, was mir ist von n\u00f6then! wann dein Wort und Brot ich hab,", "tokens": ["Gib", "mir", ",", "was", "mir", "ist", "von", "n\u00f6\u00b7then", "!", "wann", "dein", "Wort", "und", "Brot", "ich", "hab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWS", "PPER", "VAFIN", "APPR", "ADJA", "$.", "PWAV", "PPOSAT", "NN", "KON", "NN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Hab ich, was mich zeitlich, st\u00e4rcke, hab ich, was mich ewig lab.", "tokens": ["Hab", "ich", ",", "was", "mich", "zeit\u00b7lich", ",", "st\u00e4r\u00b7cke", ",", "hab", "ich", ",", "was", "mich", "e\u00b7wig", "lab."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["NN", "PPER", "$,", "PWS", "PPER", "ADJD", "$,", "VVFIN", "$,", "VAFIN", "PPER", "$,", "PWS", "PPER", "ADJD", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Gib mir, wilstu mir was geben, Armut nicht, HErr, Reichthum nicht!", "tokens": ["Gib", "mir", ",", "wils\u00b7tu", "mir", "was", "ge\u00b7ben", ",", "Ar\u00b7mut", "nicht", ",", "Herr", ",", "Reicht\u00b7hum", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP",