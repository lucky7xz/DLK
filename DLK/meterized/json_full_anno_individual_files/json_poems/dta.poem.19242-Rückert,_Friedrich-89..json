{"dta.poem.19242": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum der Vogel Strau\u00df so garviel Eier legt?", "tokens": ["Wa\u00b7rum", "der", "Vo\u00b7gel", "Strau\u00df", "so", "gar\u00b7viel", "Ei\u00b7er", "legt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "NE", "ADV", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er f\u00fcr alle so garwenig Sorge tr\u00e4gt.", "tokens": ["Weil", "er", "f\u00fcr", "al\u00b7le", "so", "gar\u00b7we\u00b7nig", "Sor\u00b7ge", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er legt sie, ohn' ein Nest zu machen, in den Sand,", "tokens": ["Er", "legt", "sie", ",", "ohn'", "ein", "Nest", "zu", "ma\u00b7chen", ",", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der br\u00fctet sie f\u00fcr ihn im hei\u00dfen Sonnenbrand.", "tokens": ["Der", "br\u00fc\u00b7tet", "sie", "f\u00fcr", "ihn", "im", "hei\u00b7\u00dfen", "Son\u00b7nen\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Fast wollen ihm es gleich die Gans und Ente thun", "tokens": ["Fast", "wol\u00b7len", "ihm", "es", "gleich", "die", "Gans", "und", "En\u00b7te", "thun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Ufer, und im Feld die Wachtel und das Huhn;", "tokens": ["Am", "U\u00b7fer", ",", "und", "im", "Feld", "die", "Wach\u00b7tel", "und", "das", "Huhn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die ihr kunstloses Bett baun zwischen Schilf und Aehren,", "tokens": ["Die", "ihr", "kunst\u00b7lo\u00b7ses", "Bett", "baun", "zwi\u00b7schen", "Schilf", "und", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und ziehn mehr Junge, dan sie k\u00f6nnten selbst ern\u00e4hren.", "tokens": ["Und", "ziehn", "mehr", "Jun\u00b7ge", ",", "dan", "sie", "k\u00f6nn\u00b7ten", "selbst", "er\u00b7n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "ADV", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Daher die junge Brut, von Schalen halb getrennt,", "tokens": ["Da\u00b7her", "die", "jun\u00b7ge", "Brut", ",", "von", "Scha\u00b7len", "halb", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon ihrem Futter nach selbst\u00e4ndig schwimmt und rennt.", "tokens": ["Schon", "ih\u00b7rem", "Fut\u00b7ter", "nach", "selb\u00b7st\u00e4n\u00b7dig", "schwimmt", "und", "rennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dagegen auf dem Baum der Fink, die Schwalb' am Haus,", "tokens": ["Da\u00b7ge\u00b7gen", "auf", "dem", "Baum", "der", "Fink", ",", "die", "Schwalb'", "am", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bringen mit viel mehr M\u00fch viel wen'ger Kinder aus.", "tokens": ["Brin\u00b7gen", "mit", "viel", "mehr", "M\u00fch", "viel", "wen'\u00b7ger", "Kin\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "PIAT", "NN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.7": {"line.1": {"text": "Warum? sie baun ihr Nest in kunstgerechter Enge,", "tokens": ["Wa\u00b7rum", "?", "sie", "baun", "ihr", "Nest", "in", "kunst\u00b7ge\u00b7rech\u00b7ter", "En\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das fasset Eier nicht, noch minder Jung', in Menge.", "tokens": ["Das", "fas\u00b7set", "Ei\u00b7er", "nicht", ",", "noch", "min\u00b7der", "Jung'", ",", "in", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "PTKNEG", "$,", "ADV", "ADV", "NE", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Finke hats aus Moos den Zweigen eingewebt,", "tokens": ["Der", "Fin\u00b7ke", "hats", "aus", "Moos", "den", "Zwei\u00b7gen", "ein\u00b7ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schwalbe hats der Wand mit M\u00f6rtel angeklebt.", "tokens": ["Die", "Schwal\u00b7be", "hats", "der", "Wand", "mit", "M\u00f6r\u00b7tel", "an\u00b7ge\u00b7klebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der Finke mu\u00df gar lang mit W\u00fcrmchen, die er nascht,", "tokens": ["Der", "Fin\u00b7ke", "mu\u00df", "gar", "lang", "mit", "W\u00fcrm\u00b7chen", ",", "die", "er", "nascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJD", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gar lang die Schwalbe mit den M\u00fcckchen, die sie hascht,", "tokens": ["Gar", "lang", "die", "Schwal\u00b7be", "mit", "den", "M\u00fcck\u00b7chen", ",", "die", "sie", "hascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die Kleinen f\u00fcttern, die nicht schwimmen und nicht laufen,", "tokens": ["Die", "Klei\u00b7nen", "f\u00fct\u00b7tern", ",", "die", "nicht", "schwim\u00b7men", "und", "nicht", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und k\u00f6nnen nichts wan schrein nach Fressen und nach Saufen.", "tokens": ["Und", "k\u00f6n\u00b7nen", "nichts", "wan", "schrein", "nach", "Fres\u00b7sen", "und", "nach", "Sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PWAV", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Den Eltern kostet es der kleinen Bissen viel,", "tokens": ["Den", "El\u00b7tern", "kos\u00b7tet", "es", "der", "klei\u00b7nen", "Bis\u00b7sen", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis ihren Jungen w\u00e4chst der Flaum und dann der Kiel.", "tokens": ["Bis", "ih\u00b7ren", "Jun\u00b7gen", "w\u00e4chst", "der", "Flaum", "und", "dann", "der", "Kiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "ADV", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Nun erst der Liebe Bild, die gattentreue Taube,", "tokens": ["Nun", "erst", "der", "Lie\u00b7be", "Bild", ",", "die", "gat\u00b7ten\u00b7treu\u00b7e", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wei\u00dfe zahm im Haus, die blaue wild im Laube,", "tokens": ["Die", "wei\u00b7\u00dfe", "zahm", "im", "Haus", ",", "die", "blau\u00b7e", "wild", "im", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Zieht, wie gepaart sie ist, auch nur ein Kinderpaar,", "tokens": ["Zieht", ",", "wie", "ge\u00b7paart", "sie", "ist", ",", "auch", "nur", "ein", "Kin\u00b7der\u00b7paar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VVPP", "PPER", "VAFIN", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihrer Z\u00e4rtlichkeit mehr ganz unm\u00f6glich war.", "tokens": ["Weil", "ih\u00b7rer", "Z\u00e4rt\u00b7lich\u00b7keit", "mehr", "ganz", "un\u00b7m\u00f6g\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Denn harte Saamen, die sie hat kein andres T\u00f6pfchen", "tokens": ["Denn", "har\u00b7te", "Saa\u00b7men", ",", "die", "sie", "hat", "kein", "and\u00b7res", "T\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu kochen, weicht sie ein in ihrem eignen Kr\u00f6pfchen,", "tokens": ["Zu", "ko\u00b7chen", ",", "weicht", "sie", "ein", "in", "ih\u00b7rem", "eig\u00b7nen", "Kr\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ART", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Und w\u00fcrgt das Futter, das sie nicht f\u00fcr sich verschlungen,", "tokens": ["Und", "w\u00fcrgt", "das", "Fut\u00b7ter", ",", "das", "sie", "nicht", "f\u00fcr", "sich", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hervor und machet satt, selbst hungrig, ihre Jungen.", "tokens": ["Her\u00b7vor", "und", "ma\u00b7chet", "satt", ",", "selbst", "hung\u00b7rig", ",", "ih\u00b7re", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ADJD", "$,", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Sie \u00fcbertrifft an Lieb' allein der Pelikan;", "tokens": ["Sie", "\u00fc\u00b7bert\u00b7rifft", "an", "Lieb'", "al\u00b7lein", "der", "Pe\u00b7li\u00b7kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn keine Wirklichkeit, so ist es doch kein Wahn,", "tokens": ["Wenn", "kei\u00b7ne", "Wirk\u00b7lich\u00b7keit", ",", "so", "ist", "es", "doch", "kein", "Wahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Vielmehr ein hohes Bild, das ewig wahr wird bleiben,", "tokens": ["Viel\u00b7mehr", "ein", "ho\u00b7hes", "Bild", ",", "das", "e\u00b7wig", "wahr", "wird", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Herzen wohnend, wenn sie's aus der Welt vertreiben:", "tokens": ["Im", "Her\u00b7zen", "woh\u00b7nend", ",", "wenn", "sie's", "aus", "der", "Welt", "ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KOUS", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Da\u00df er voll Z\u00e4rtlichkeit sich aufrei\u00dft seine Brust,", "tokens": ["Da\u00df", "er", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", "sich", "auf\u00b7rei\u00dft", "sei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00e4nket seine Brut mit seinem Blut voll Luft.", "tokens": ["Und", "tr\u00e4n\u00b7ket", "sei\u00b7ne", "Brut", "mit", "sei\u00b7nem", "Blut", "voll", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Die ew'ge Mutter ists, die alle tr\u00e4nkt und speiset,", "tokens": ["Die", "ew'\u00b7ge", "Mut\u00b7ter", "ists", ",", "die", "al\u00b7le", "tr\u00e4nkt", "und", "spei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dir, o Mensch, ihr Bild im Wunderspiegel weiset.", "tokens": ["Die", "dir", ",", "o", "Mensch", ",", "ihr", "Bild", "im", "Wun\u00b7der\u00b7spie\u00b7gel", "wei\u00b7set", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}