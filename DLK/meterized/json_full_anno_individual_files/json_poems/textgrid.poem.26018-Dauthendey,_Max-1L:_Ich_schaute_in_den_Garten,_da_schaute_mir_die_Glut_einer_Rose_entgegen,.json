{"textgrid.poem.26018": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich schaute in den Garten, da schaute mir die Glut einer Rose entgegen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schaute in den Garten, da schaute mir die Glut einer Rose entgegen,", "tokens": ["Ich", "schau\u00b7te", "in", "den", "Gar\u00b7ten", ",", "da", "schau\u00b7te", "mir", "die", "Glut", "ei\u00b7ner", "Ro\u00b7se", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ich f\u00fchlte sie aus der Ferne in meiner Hand wie deine Liebe.", "tokens": ["Ich", "f\u00fchl\u00b7te", "sie", "aus", "der", "Fer\u00b7ne", "in", "mei\u00b7ner", "Hand", "wie", "dei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Seit ich dich k\u00fcsse, geht die Zeit der Rosen nicht aus,", "tokens": ["Seit", "ich", "dich", "k\u00fcs\u00b7se", ",", "geht", "die", "Zeit", "der", "Ro\u00b7sen", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Der Garten lacht mit roten Lippen wie du.", "tokens": ["Der", "Gar\u00b7ten", "lacht", "mit", "ro\u00b7ten", "Lip\u00b7pen", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Tag und Nacht sind kaum ein F\u00e4cherschlag,", "tokens": ["Tag", "und", "Nacht", "sind", "kaum", "ein", "F\u00e4\u00b7cher\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und ein Jahr ist nur ein Hahnenschrei,", "tokens": ["Und", "ein", "Jahr", "ist", "nur", "ein", "Hah\u00b7nen\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ich lebe es mit geschlossenen Augen.", "tokens": ["Ich", "le\u00b7be", "es", "mit", "ge\u00b7schlos\u00b7se\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich schaute in den Garten, da schaute mir die Glut einer Rose entgegen,", "tokens": ["Ich", "schau\u00b7te", "in", "den", "Gar\u00b7ten", ",", "da", "schau\u00b7te", "mir", "die", "Glut", "ei\u00b7ner", "Ro\u00b7se", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+--+--+-",