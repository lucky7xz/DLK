{"dta.poem.21172": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Klagen  \n eines ungl\u00fccklichen Liebhabers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Denk ihn hinaus &#8212; den schrecklichen Gedanken,", "tokens": ["Denk", "ihn", "hin\u00b7aus", "&#8212;", "den", "schreck\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APZR", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der m\u00e4chtig dich ergreift!", "tokens": ["Der", "m\u00e4ch\u00b7tig", "dich", "er\u00b7greift", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie schwarz! \u2014 Er liegt auf der gebeugten Seele,", "tokens": ["Wie", "schwarz", "!", "Er", "liegt", "auf", "der", "ge\u00b7beug\u00b7ten", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "$(", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie ein Gebirge liegt.", "tokens": ["Wie", "ein", "Ge\u00b7bir\u00b7ge", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie liebt dich nicht! Tief im zerri\u00dfnen Herzen", "tokens": ["Sie", "liebt", "dich", "nicht", "!", "Tief", "im", "zer\u00b7ri\u00df\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sagts ein geheim Gef\u00fchl.", "tokens": ["Sagts", "ein", "ge\u00b7heim", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald w\u00e4chst es auf, und mit dem lautsten Donner", "tokens": ["Bald", "w\u00e4chst", "es", "auf", ",", "und", "mit", "dem", "lauts\u00b7ten", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ruft es: Sie liebt dich nicht!", "tokens": ["Ruft", "es", ":", "Sie", "liebt", "dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Mitternacht, die dicken Finsternisse", "tokens": ["O", "Mit\u00b7ter\u00b7nacht", ",", "die", "di\u00b7cken", "Fins\u00b7ter\u00b7nis\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind noch nicht finster gnug;", "tokens": ["Sind", "noch", "nicht", "fins\u00b7ter", "gnug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00fclle doch in zehnmal schw\u00e4rzre Schatten", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "doch", "in", "zehn\u00b7mal", "schw\u00e4rz\u00b7re", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den thr\u00e4nenvollen Blick!", "tokens": ["Den", "thr\u00e4\u00b7nen\u00b7vol\u00b7len", "Blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie liebt dich nicht! Ich kan dir nicht entfliehen,", "tokens": ["Sie", "liebt", "dich", "nicht", "!", "Ich", "kan", "dir", "nicht", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedanke, voller Quaal!", "tokens": ["Ge\u00b7dan\u00b7ke", ",", "vol\u00b7ler", "Qua\u00b7al", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df ab, la\u00df ab! Schon blutet dir das Opfer", "tokens": ["La\u00df", "ab", ",", "la\u00df", "ab", "!", "Schon", "blu\u00b7tet", "dir", "das", "Op\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon stirbt das kalte Herz.", "tokens": ["Schon", "stirbt", "das", "kal\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Warum dringt durch die lange Nacht", "tokens": ["Wa\u00b7rum", "dringt", "durch", "die", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein zweifelhafter Stral?", "tokens": ["Ein", "zwei\u00b7fel\u00b7haf\u00b7ter", "Stral", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Hofnung, Hofnung! t\u00e4usche nicht", "tokens": ["O", "Hof\u00b7nung", ",", "Hof\u00b7nung", "!", "t\u00e4u\u00b7sche", "nicht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$.", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ungl\u00fcckseligs Herz!", "tokens": ["Ein", "un\u00b7gl\u00fcck\u00b7se\u00b7ligs", "Herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "La\u00df mich in tiefer Traurigkeit,", "tokens": ["La\u00df", "mich", "in", "tie\u00b7fer", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In der die Seele stirbt!", "tokens": ["In", "der", "die", "See\u00b7le", "stirbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verzweiflung selbst ist Trost f\u00fcr mich,", "tokens": ["Ver\u00b7zwei\u00b7flung", "selbst", "ist", "Trost", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wofern du mich betr\u00fcgst.", "tokens": ["Wo\u00b7fern", "du", "mich", "be\u00b7tr\u00fcgst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zu grausam! \u2014 dennoch lispelst du", "tokens": ["Zu", "grau\u00b7sam", "!", "den\u00b7noch", "lis\u00b7pelst", "du"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$.", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem bangen Herzen ein:", "tokens": ["Dem", "ban\u00b7gen", "Her\u00b7zen", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sey vielleicht \u2014 vielleicht geliebt;", "tokens": ["Ich", "sey", "viel\u00b7leicht", "viel\u00b7leicht", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O niedriger Verrath!", "tokens": ["O", "nied\u00b7ri\u00b7ger", "Ver\u00b7rath", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Meynst du, der schimmernde Betrug", "tokens": ["Meynst", "du", ",", "der", "schim\u00b7mern\u00b7de", "Be\u00b7trug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll Kraft dem Herzen leihn?", "tokens": ["Soll", "Kraft", "dem", "Her\u00b7zen", "leihn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mehr gl\u00fccklich war es, ganz durchbohrt,", "tokens": ["Mehr", "gl\u00fcck\u00b7lich", "war", "es", ",", "ganz", "durch\u00b7bohrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz, o Verzweiflung, dein.", "tokens": ["Ganz", ",", "o", "Ver\u00b7zwei\u00b7flung", ",", "dein", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Umsonst, umsonst! \u2014 Voll Grausamkeit", "tokens": ["Um\u00b7sonst", ",", "um\u00b7sonst", "!", "Voll", "Grau\u00b7sam\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "$.", "$(", "ADJD", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Bet\u00e4ubest du den Schmerz.", "tokens": ["Be\u00b7t\u00e4u\u00b7best", "du", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verbinde meine Wunden dann,", "tokens": ["Ver\u00b7bin\u00b7de", "mei\u00b7ne", "Wun\u00b7den", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rei\u00df sie blutger auf!", "tokens": ["Und", "rei\u00df", "sie", "blut\u00b7ger", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nicht verzweifelungsvoll, oder des s\u00fc\u00dfesten Gl\u00fccks", "tokens": ["Nicht", "ver\u00b7zwei\u00b7fe\u00b7lungs\u00b7voll", ",", "o\u00b7der", "des", "s\u00fc\u00b7\u00dfes\u00b7ten", "Gl\u00fccks"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ungewi\u00df, klaget mein z\u00e4rtliches Herz;", "tokens": ["Un\u00b7ge\u00b7wi\u00df", ",", "kla\u00b7get", "mein", "z\u00e4rt\u00b7li\u00b7ches", "Herz", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nein, ich werde geliebt, und nun, da sie mich liebt,", "tokens": ["Nein", ",", "ich", "wer\u00b7de", "ge\u00b7liebt", ",", "und", "nun", ",", "da", "sie", "mich", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVPP", "$,", "KON", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Bin ich doch dreymal ungl\u00fccklicher noch!", "tokens": ["Bin", "ich", "doch", "drey\u00b7mal", "un\u00b7gl\u00fcck\u00b7li\u00b7cher", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.11": {"line.1": {"text": "Daphne, liebe mich nicht! Ueber uns h\u00e4nget voll Nacht", "tokens": ["Daph\u00b7ne", ",", "lie\u00b7be", "mich", "nicht", "!", "Ue\u00b7ber", "uns", "h\u00e4n\u00b7get", "voll", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "APPR", "PPER", "VVFIN", "ADJD", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schrecklich ein eiserner Himmel herab.", "tokens": ["Schreck\u00b7lich", "ein", "ei\u00b7ser\u00b7ner", "Him\u00b7mel", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nicht ein g\u00fctiger Stral schimmert uns hinter der Nacht,", "tokens": ["Nicht", "ein", "g\u00fc\u00b7ti\u00b7ger", "Stral", "schim\u00b7mert", "uns", "hin\u00b7ter", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Furcht und Entsetzen schwebt rund um uns her.", "tokens": ["Furcht", "und", "Ent\u00b7set\u00b7zen", "schwebt", "rund", "um", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "O parteyisches Gl\u00fcck, warum l\u00e4chelst du nie", "tokens": ["O", "par\u00b7tey\u00b7isc\u00b7hes", "Gl\u00fcck", ",", "wa\u00b7rum", "l\u00e4\u00b7chelst", "du", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Liebender Unschuld und standhafter Treu?", "tokens": ["Lie\u00b7ben\u00b7der", "Un\u00b7schuld", "und", "stand\u00b7haf\u00b7ter", "Treu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ists der Z\u00e4rtlichkeit Loos, immer vom t\u00f6dtlichen Gram,", "tokens": ["Ists", "der", "Z\u00e4rt\u00b7lich\u00b7keit", "Loos", ",", "im\u00b7mer", "vom", "t\u00f6dt\u00b7li\u00b7chen", "Gram", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "$,", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Langsam gequ\u00e4let, das Opfer zu seyn?", "tokens": ["Lang\u00b7sam", "ge\u00b7qu\u00e4\u00b7let", ",", "das", "Op\u00b7fer", "zu", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.13": {"line.1": {"text": "Jetzo, da du mich liebst, Daphne, fa\u00dft mich mein", "tokens": ["Jet\u00b7zo", ",", "da", "du", "mich", "liebst", ",", "Daph\u00b7ne", ",", "fa\u00dft", "mich", "mein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Un\u00fcberwindlich, wie sprech ich ihn aus!", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", ",", "wie", "sprech", "ich", "ihn", "aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! du liebest nur den, welchen ein pl\u00f6tzlicher Sturm", "tokens": ["Ach", "!", "du", "lie\u00b7best", "nur", "den", ",", "wel\u00b7chen", "ein", "pl\u00f6tz\u00b7li\u00b7cher", "Sturm"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ADV", "ART", "$,", "PWAT", "ART", "ADJA", "NN"], "meter":