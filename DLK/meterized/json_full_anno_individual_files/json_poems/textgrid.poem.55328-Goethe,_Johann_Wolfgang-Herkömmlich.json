{"textgrid.poem.55328": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Herk\u00f6mmlich", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Priester werden Messe singen,", "tokens": ["Pries\u00b7ter", "wer\u00b7den", "Mes\u00b7se", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Pfarrer werden pred'gen;", "tokens": ["Und", "die", "Pfar\u00b7rer", "wer\u00b7den", "pre\u00b7d'\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAINF", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jeder wird vor allen Dingen", "tokens": ["Je\u00b7der", "wird", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Meinung sich entled'gen", "tokens": ["Sei\u00b7ner", "Mei\u00b7nung", "sich", "ent\u00b7le\u00b7d'\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PRF", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und sich der Gemeine freuen,", "tokens": ["Und", "sich", "der", "Ge\u00b7mei\u00b7ne", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Die sich um ihn her versammelt,", "tokens": ["Die", "sich", "um", "ihn", "her", "ver\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "APZR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So im Alten wie im Neuen", "tokens": ["So", "im", "Al\u00b7ten", "wie", "im", "Neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Ohngef\u00e4hre Worte stammelt.", "tokens": ["Ohn\u00b7ge\u00b7f\u00e4h\u00b7re", "Wor\u00b7te", "stam\u00b7melt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und so lasset auch die Farben", "tokens": ["Und", "so", "las\u00b7set", "auch", "die", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mich nach meiner Art verk\u00fcnden,", "tokens": ["Mich", "nach", "mei\u00b7ner", "Art", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ohne Wunden, ohne Narben,", "tokens": ["Oh\u00b7ne", "Wun\u00b7den", ",", "oh\u00b7ne", "Nar\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mit der l\u00e4\u00dflichsten der S\u00fcnden.", "tokens": ["Mit", "der", "l\u00e4\u00df\u00b7lichs\u00b7ten", "der", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN", "$."], "meter": "--+---+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Priester werden Messe singen,", "tokens": ["Pries\u00b7ter", "wer\u00b7den", "Mes