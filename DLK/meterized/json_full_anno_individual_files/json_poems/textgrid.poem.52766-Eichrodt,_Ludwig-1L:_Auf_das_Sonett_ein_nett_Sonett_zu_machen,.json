{"textgrid.poem.52766": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf das Sonett ein nett Sonett zu machen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf das Sonett ein nett Sonett zu machen,", "tokens": ["Auf", "das", "So\u00b7nett", "ein", "nett", "So\u00b7nett", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darauf verfallen wird alsbald ein Dichter,", "tokens": ["Da\u00b7rauf", "ver\u00b7fal\u00b7len", "wird", "als\u00b7bald", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sofern ihm sonst nichts beif\u00e4llt, und der Richter", "tokens": ["So\u00b7fern", "ihm", "sonst", "nichts", "bei\u00b7f\u00e4llt", ",", "und", "der", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird leicht bestochen durch die Siebensachen.", "tokens": ["Wird", "leicht", "be\u00b7sto\u00b7chen", "durch", "die", "Sie\u00b7ben\u00b7sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die guten Freunde halten schon das Lachen,", "tokens": ["Die", "gu\u00b7ten", "Freun\u00b7de", "hal\u00b7ten", "schon", "das", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gerne zufrieden ist das Zunftgelichter,", "tokens": ["Ger\u00b7ne", "zu\u00b7frie\u00b7den", "ist", "das", "Zunft\u00b7ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das, selbstbeseelt vom N\u00fcrenberger Trichter,", "tokens": ["Das", ",", "selbst\u00b7be\u00b7seelt", "vom", "N\u00fc\u00b7ren\u00b7ber\u00b7ger", "Trich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unendlich f\u00fchlt, es k\u00f6nn' es auch so machen.", "tokens": ["Un\u00b7end\u00b7lich", "f\u00fchlt", ",", "es", "k\u00f6nn'", "es", "auch", "so", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So den Beruf zur Dichtung aufzuzeigen,", "tokens": ["So", "den", "Be\u00b7ruf", "zur", "Dich\u00b7tung", "auf\u00b7zu\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df ich nur schnelle das Sonett besingen,", "tokens": ["Mu\u00df", "ich", "nur", "schnel\u00b7le", "das", "So\u00b7nett", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos"