{"textgrid.poem.58772": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "20. Heilig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Seraphim dir rufen zu:", "tokens": ["Die", "Se\u00b7ra\u00b7phim", "dir", "ru\u00b7fen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo Heilig, Heilig, Heil'ger du!\u00ab", "tokens": ["\u00bb", "o", "Hei\u00b7lig", ",", "Hei\u00b7lig", ",", "Heil'\u00b7ger", "du", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "ADJA", "$,", "ADJA", "$,", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo soll denn ich Unreiner bleiben?", "tokens": ["Wo", "soll", "denn", "ich", "Un\u00b7rei\u00b7ner", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "KON", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du machst dich S\u00fcndern nicht gemein,", "tokens": ["Du", "machst", "dich", "S\u00fcn\u00b7dern", "nicht", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wer wird ohne Tadel sein? \u2013", "tokens": ["Und", "wer", "wird", "oh\u00b7ne", "Ta\u00b7del", "sein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "VAFIN", "APPR", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will mich Christus einverleiben;", "tokens": ["Ich", "will", "mich", "Chris\u00b7tus", "ein\u00b7ver\u00b7lei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dem hang' ich an, so nimmt er's hin", "tokens": ["Dem", "hang'", "ich", "an", ",", "so", "nimmt", "er's", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und heiligt mich nach seinem Sinn.", "tokens": ["Und", "hei\u00b7ligt", "mich", "nach", "sei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Die Seraphim dir rufen zu:", "tokens": ["Die", "Se\u00b7ra\u00b7phim", "dir", "ru\u00b7fen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo Heilig, Heilig, Heil'ger du!\u00ab", "tokens": ["\u00bb", "o", "Hei\u00b7lig", ",", "Hei\u00b7lig", ",", "Heil'\u00b7ger", "du", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "ADJA", "$,", "ADJA", "$,", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo soll denn ich Unreine