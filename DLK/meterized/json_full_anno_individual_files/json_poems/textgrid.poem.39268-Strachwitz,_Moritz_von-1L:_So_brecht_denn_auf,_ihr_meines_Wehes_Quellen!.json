{"textgrid.poem.39268": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So brecht denn auf, ihr meines Wehes Quellen!", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So brecht denn auf, ihr meines Wehes Quellen!", "tokens": ["So", "brecht", "denn", "auf", ",", "ihr", "mei\u00b7nes", "We\u00b7hes", "Quel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schie\u00dft m\u00e4chtig, klangvoll aus des Herzens Grunde,", "tokens": ["Schie\u00dft", "m\u00e4ch\u00b7tig", ",", "klang\u00b7voll", "aus", "des", "Her\u00b7zens", "Grun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit des Stromes flut'gem Liedermunde", "tokens": ["Und", "mit", "des", "Stro\u00b7mes", "flut'\u00b7gem", "Lie\u00b7der\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00dft brausend auf des Schmerzes T\u00f6ne schwellen!", "tokens": ["La\u00dft", "brau\u00b7send", "auf", "des", "Schmer\u00b7zes", "T\u00f6\u00b7ne", "schwel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soll nimmer Licht der Seele Nacht erhellen?", "tokens": ["Soll", "ni