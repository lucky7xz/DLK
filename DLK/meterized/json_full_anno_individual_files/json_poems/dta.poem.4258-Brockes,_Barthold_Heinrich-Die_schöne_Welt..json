{"dta.poem.4258": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey dem so herrlichen Gepr\u00e4nge", "tokens": ["Bey", "dem", "so", "herr\u00b7li\u00b7chen", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In aller Creaturen Menge,", "tokens": ["In", "al\u00b7ler", "Crea\u00b7tu\u00b7ren", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die ich nicht alle z\u00e4hlen kann,", "tokens": ["Die", "ich", "nicht", "al\u00b7le", "z\u00e4h\u00b7len", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein GOtt! wo heb\u2019 und fang\u2019 ich an,", "tokens": ["Mein", "Gott", "!", "wo", "heb'", "und", "fang'", "ich", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VVFIN", "KON", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die innre Regung meiner Seelen,", "tokens": ["Die", "inn\u00b7re", "Re\u00b7gung", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch sie ger\u00fchret, zu erzehlen!", "tokens": ["Durch", "sie", "ge\u00b7r\u00fch\u00b7ret", ",", "zu", "er\u00b7zeh\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Der ganze Luft-Kreis gl\u00e4nzt und gl\u00fcht,", "tokens": ["Der", "gan\u00b7ze", "Luft\u00b7Kreis", "gl\u00e4nzt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da man, an den sapphirnen Zimmern,", "tokens": ["Da", "man", ",", "an", "den", "sap\u00b7phir\u00b7nen", "Zim\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den Born des Lichts, die Sonne, schimmern,", "tokens": ["Den", "Born", "des", "Lichts", ",", "die", "Son\u00b7ne", ",", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles, voller Strahlen, sieht.", "tokens": ["Und", "al\u00b7les", ",", "vol\u00b7ler", "Strah\u00b7len", ",", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die all\u2019s erf\u00fcllnde Himmels-Gluht,", "tokens": ["Die", "all's", "er\u00b7f\u00fclln\u00b7de", "Him\u00b7mels\u00b7Gluht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In welcher alles gleichsam schwimmet,", "tokens": ["In", "wel\u00b7cher", "al\u00b7les", "gleich\u00b7sam", "schwim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wodurch ein jeder Vorwurf glimmet,", "tokens": ["Wo\u00b7durch", "ein", "je\u00b7der", "Vor\u00b7wurf", "glim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erf\u00fcllt und schm\u00fccket Land und Fluht.", "tokens": ["Er\u00b7f\u00fcllt", "und", "schm\u00fc\u00b7cket", "Land", "und", "Fluht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Fische, die den Eindruck f\u00fchlen,", "tokens": ["Die", "Fi\u00b7sche", ",", "die", "den", "Ein\u00b7druck", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und wimmelnd lermen, schw\u00e4rmen, spielen,", "tokens": ["Und", "wim\u00b7melnd", "ler\u00b7men", ",", "schw\u00e4r\u00b7men", ",", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Belebt, auch in der kalten Fluht,", "tokens": ["Be\u00b7lebt", ",", "auch", "in", "der", "kal\u00b7ten", "Fluht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Lebens-Quell\u2019, der Sonnen Gluht.", "tokens": ["Die", "Le\u00b7bens\u00b7Quell'", ",", "der", "Son\u00b7nen", "Gluht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein jeder Baum, von Laub bekr\u00e4nzet,", "tokens": ["Ein", "je\u00b7der", "Baum", ",", "von", "Laub", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Vergn\u00fcgt das Auge, gl\u00fcht und gl\u00e4nzet.", "tokens": ["Ver\u00b7gn\u00fcgt", "das", "Au\u00b7ge", ",", "gl\u00fcht", "und", "gl\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Es lacht das Feld, die Wiese funkelt", "tokens": ["Es", "lacht", "das", "Feld", ",", "die", "Wie\u00b7se", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "So gar, wo eine Schatten-Nacht,", "tokens": ["So", "gar", ",", "wo", "ei\u00b7ne", "Schat\u00b7ten\u00b7Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mit holder Schw\u00e4rze, sie verdunkelt,", "tokens": ["Mit", "hol\u00b7der", "Schw\u00e4r\u00b7ze", ",", "sie", "ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ist ihre Pracht nicht minder sch\u00f6n,", "tokens": ["Ist", "ih\u00b7re", "Pracht", "nicht", "min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ja noch fast sch\u00f6ner anzuseh\u2019n;", "tokens": ["Ja", "noch", "fast", "sch\u00f6\u00b7ner", "an\u00b7zu\u00b7seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da ihre holde Lieblichkeit,", "tokens": ["Da", "ih\u00b7re", "hol\u00b7de", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mit gleichfalls holder Dunkelheit,", "tokens": ["Mit", "gleich\u00b7falls", "hol\u00b7der", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die nahen Schatten noch erh\u00f6h\u2019n.", "tokens": ["Die", "na\u00b7hen", "Schat\u00b7ten", "noch", "er\u00b7h\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn man nun, bey so hellem Wetter,", "tokens": ["Wenn", "man", "nun", ",", "bey", "so", "hel\u00b7lem", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie alles minder bl\u00fcht als gl\u00fcht,", "tokens": ["Wie", "al\u00b7les", "min\u00b7der", "bl\u00fcht", "als", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs holde Dunkelgr\u00fcn der Bl\u00e4tter,", "tokens": ["Durchs", "hol\u00b7de", "Dun\u00b7kel\u00b7gr\u00fcn", "der", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Gold der gelben Felder sieht;", "tokens": ["Das", "Gold", "der", "gel\u00b7ben", "Fel\u00b7der", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird durch die Pracht, die daraus quillet,", "tokens": ["Wird", "durch", "die", "Pracht", ",", "die", "da\u00b7raus", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seele durch den Blick erf\u00fcllet.", "tokens": ["Die", "See\u00b7le", "durch", "den", "Blick", "er\u00b7f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es scheint, in solchem heitern Schein,", "tokens": ["Es", "scheint", ",", "in", "sol\u00b7chem", "hei\u00b7tern", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ungemein und allgemein,", "tokens": ["Der", "un\u00b7ge\u00b7mein", "und", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der heute gar besonders rein,", "tokens": ["Der", "heu\u00b7te", "gar", "be\u00b7son\u00b7ders", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Erd-Kreis recht verkl\u00e4rt zu seyn.", "tokens": ["Der", "Erd\u00b7Kreis", "recht", "ver\u00b7kl\u00e4rt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mir fiel, als ich die\u00df \u00fcberlegte,", "tokens": ["Mir", "fiel", ",", "als", "ich", "die\u00df", "\u00fc\u00b7ber\u00b7leg\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und die so sch\u00f6ne Welt erwegte,", "tokens": ["Und", "die", "so", "sch\u00f6\u00b7ne", "Welt", "er\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zum Ruhm des Sch\u00f6pfers, dieses ein:", "tokens": ["Zum", "Ruhm", "des", "Sch\u00f6p\u00b7fers", ",", "die\u00b7ses", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein GOtt! da, was so herrlich flammet,", "tokens": ["Mein", "Gott", "!", "da", ",", "was", "so", "herr\u00b7lich", "flam\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADV", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allein aus einer Sonne stammet;", "tokens": ["Al\u00b7lein", "aus", "ei\u00b7ner", "Son\u00b7ne", "stam\u00b7met", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was m\u00fcssen denn durch aller Sonnen Schein,", "tokens": ["Was", "m\u00fcs\u00b7sen", "denn", "durch", "al\u00b7ler", "Son\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In des unme\u00dfbarn Raumes Gr\u00fcnden,", "tokens": ["In", "des", "un\u00b7me\u00df\u00b7barn", "Rau\u00b7mes", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr Wunder nicht zu seh\u2019n, zu finden,", "tokens": ["F\u00fcr", "Wun\u00b7der", "nicht", "zu", "seh'n", ",", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Herrlichkeit gewirket seyn!", "tokens": ["F\u00fcr", "Herr\u00b7lich\u00b7keit", "ge\u00b7wir\u00b7ket", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}