word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "VVPP", "$(", "VVPP", "ADV", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch Gl\u00fcck und Ungl\u00fcck liegt", "tokens": ["Doch", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Schoo\u00df des Wolkengotts, de\u00df Meer und Erden", "tokens": ["Im", "Schoo\u00df", "des", "Wol\u00b7ken\u00b7gotts", ",", "de\u00df", "Meer", "und", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets sind, vor und nach Dir!", "tokens": ["Stets", "sind", ",", "vor", "und", "nach", "Dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PTKVZ", "KON", "APPR", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Der auch die Taube h\u00f6rt, die, schwach, verirrt", "tokens": ["Der", "auch", "die", "Tau\u00b7be", "h\u00f6rt", ",", "die", ",", "schwach", ",", "ver\u00b7irrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "$,", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Land, ins Schiff sich wagt", "tokens": ["Vom", "Land", ",", "ins", "Schiff", "sich", "wagt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Speis