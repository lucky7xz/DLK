{"textgrid.poem.60237": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja Klio redet oft mit liederreichem Munde", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja Klio redet oft mit liederreichem Munde", "tokens": ["Ja", "Klio", "re\u00b7det", "oft", "mit", "lie\u00b7der\u00b7rei\u00b7chem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "NE", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Selbst Jupiters Gedanken nach;", "tokens": ["Selbst", "Ju\u00b7pi\u00b7ters", "Ge\u00b7dan\u00b7ken", "nach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sagte mir, was in Charlottens erster Stunde", "tokens": ["Sie", "sag\u00b7te", "mir", ",", "was", "in", "Char\u00b7lot\u00b7tens", "ers\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die g\u00f6ttliche Lucina sprach.", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Lu\u00b7ci\u00b7na", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Willkommen, Tochter, dich wird Pallas unterweisen!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Toch\u00b7ter", ",", "dich", "wird", "Pal\u00b7las", "un\u00b7ter\u00b7wei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Heil dir, begl\u00fccktgebohrnes Kind,", "tokens": ["Heil", "dir", ",", "be\u00b7gl\u00fcckt\u00b7ge\u00b7bohr\u00b7nes", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich wird die gr\u00f6\u00dfeste der Nationen preisen,", "tokens": ["Dich", "wird", "die", "gr\u00f6\u00b7\u00dfes\u00b7te", "der", "Na\u00b7ti\u00b7o\u00b7nen", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die vom Neptun besch\u00fctzet sind.", "tokens": ["Die", "vom", "Nep\u00b7tun", "be\u00b7sch\u00fct\u00b7zet", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ihr K\u00f6nig wird aus deinem kleinen Vaterlande", "tokens": ["Ihr", "K\u00f6\u00b7nig", "wird", "aus", "dei\u00b7nem", "klei\u00b7nen", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich \u00fcbers Meer zum Throne ziehn,", "tokens": ["Dich", "\u00fc\u00b7bers", "Meer", "zum", "Thro\u00b7ne", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Hymen kn\u00fcpfte nie so diamantne Bande,", "tokens": ["Und", "Hy\u00b7men", "kn\u00fcpf\u00b7te", "nie", "so", "di\u00b7a\u00b7mant\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als zwischen deiner Hand und ihn.", "tokens": ["Als", "zwi\u00b7schen", "dei\u00b7ner", "Hand", "und", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dich wird in seinem Arm Saturnia beneiden;", "tokens": ["Dich", "wird", "in", "sei\u00b7nem", "Arm", "Sa\u00b7tur\u00b7nia", "be\u00b7nei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn du bist seliger als sie.", "tokens": ["Denn", "du", "bist", "se\u00b7li\u00b7ger", "als", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst geliebt, du schmeckst der Liebe s\u00fc\u00dfe Freuden,", "tokens": ["Du", "wirst", "ge\u00b7liebt", ",", "du", "schmeckst", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Quaalen kennst du nie.", "tokens": ["Und", "ih\u00b7re", "Qua\u00b7a\u00b7len", "kennst", "du", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Von allen M\u00fcttern, die der Erde F\u00fcrsten geben,", "tokens": ["Von", "al\u00b7len", "M\u00fct\u00b7tern", ",", "die", "der", "Er\u00b7de", "F\u00fcrs\u00b7ten", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du die gl\u00fccklichste genannt;", "tokens": ["Wirst", "du", "die", "gl\u00fcck\u00b7lichs\u00b7te", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ganzer Heldenkrei\u00df bek\u00f6mmt von dir das Leben", "tokens": ["Ein", "gan\u00b7zer", "Hel\u00b7den\u00b7krei\u00df", "be\u00b7k\u00f6mmt", "von", "dir", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und w\u00e4chset an Minervens Hand.", "tokens": ["Und", "w\u00e4ch\u00b7set", "an", "Mi\u00b7ner\u00b7vens", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Auch hilft die G\u00fctigkeit der sanften Charitinnen,", "tokens": ["Auch", "hilft", "die", "G\u00fc\u00b7tig\u00b7keit", "der", "sanf\u00b7ten", "Cha\u00b7ri\u00b7tin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir sch\u00f6ne T\u00f6chter auferziehn:", "tokens": ["Dir", "sch\u00f6\u00b7ne", "T\u00f6ch\u00b7ter", "auf\u00b7er\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie werden einst als junge K\u00f6niginnen", "tokens": ["Sie", "wer\u00b7den", "einst", "als", "jun\u00b7ge", "K\u00f6\u00b7ni\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem m\u00fctterlichen Arm entfliehn.", "tokens": ["Dem", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Arm", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Durch deren hohen Reiz und Sch\u00f6nheit du Georgen,", "tokens": ["Durch", "de\u00b7ren", "ho\u00b7hen", "Reiz", "und", "Sch\u00f6n\u00b7heit", "du", "Ge\u00b7or\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "KON", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der freyen Britten besten F\u00fcrst,", "tokens": ["Der", "frey\u00b7en", "Brit\u00b7ten", "bes\u00b7ten", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch immer fesselst, wenn du, wie ein Wintermorgen,", "tokens": ["Noch", "im\u00b7mer", "fes\u00b7selst", ",", "wenn", "du", ",", "wie", "ein", "Win\u00b7ter\u00b7mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wei\u00df um die Schl\u00e4fe bist.", "tokens": ["Wei\u00df", "um", "die", "Schl\u00e4\u00b7fe", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Von eurer Eintracht und Geselligkeit bewogen,", "tokens": ["Von", "eu\u00b7rer", "Ein\u00b7tracht", "und", "Ge\u00b7sel\u00b7lig\u00b7keit", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ahmt Albion euch nach, und macht,", "tokens": ["Ahmt", "Al\u00b7bi\u00b7on", "euch", "nach", ",", "und", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Amor niemals mehr mit schlaugespannten Bogen", "tokens": ["Da\u00df", "A\u00b7mor", "nie\u00b7mals", "mehr", "mit", "schlau\u00b7ge\u00b7spann\u00b7ten", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Ehegottes lacht.", "tokens": ["Des", "E\u00b7he\u00b7got\u00b7tes", "lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So sprach die G\u00f6ttin itzt; und ihr, ihr holden T\u00f6chter", "tokens": ["So", "sprach", "die", "G\u00f6t\u00b7tin", "itzt", ";", "und", "ihr", ",", "ihr", "hol\u00b7den", "T\u00f6ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "KON", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des meerumflo\u00dfnen Albions,", "tokens": ["Des", "mee\u00b7rum\u00b7flo\u00df\u00b7nen", "Al\u00b7bions", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihr edlen S\u00f6hne tausendj\u00e4hriger Geschlechter,", "tokens": ["Ihr", "ed\u00b7len", "S\u00f6h\u00b7ne", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7ger", "Ge\u00b7schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Liebt diesen Schmuck des Throns!", "tokens": ["Liebt", "die\u00b7sen", "Schmuck", "des", "Throns", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Liebt diese K\u00f6niginn mit Z\u00e4rtlichkeit und tretet", "tokens": ["Liebt", "die\u00b7se", "K\u00f6\u00b7ni\u00b7ginn", "mit", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "tre\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ihrer Tugend Fu\u00dftritt fr\u00fch;", "tokens": ["In", "ih\u00b7rer", "Tu\u00b7gend", "Fu\u00df\u00b7tritt", "fr\u00fch", ";"], "token_inf