{"dta.poem.2580": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der handlende Mercur und siegende Cupido  \n  Bey H. F. B. u. J. H. G. Hochz. 14. Nov. 1679", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weicht nur ihr G\u00f6tter weicht f\u00fcr meiner Herrligkeit/", "tokens": ["Weicht", "nur", "ihr", "G\u00f6t\u00b7ter", "weicht", "f\u00fcr", "mei\u00b7ner", "Herr\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein m\u00e4chtig Scepter dr\u00fcckt all eure Hoheit nieder/", "tokens": ["Mein", "m\u00e4ch\u00b7tig", "Scep\u00b7ter", "dr\u00fcckt", "all", "eu\u00b7re", "Ho\u00b7heit", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PIAT", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jm K\u00f6cher f\u00fchr' ich Brand und Funcken im Gefider/", "tokens": ["Jm", "K\u00f6\u00b7cher", "f\u00fchr'", "ich", "Brand", "und", "Fun\u00b7cken", "im", "Ge\u00b7fi\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meiner Siege Ruhm erschallet weit und breit.", "tokens": ["Und", "mei\u00b7ner", "Sie\u00b7ge", "Ruhm", "er\u00b7schal\u00b7let", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Himmel ist nicht frey und Jupiter gefangen/", "tokens": ["Der", "Him\u00b7mel", "ist", "nicht", "frey", "und", "Ju\u00b7pi\u00b7ter", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn dieser schnelle Pfeil durch seine Brust gegangen.", "tokens": ["Wenn", "die\u00b7ser", "schnel\u00b7le", "Pfeil", "durch", "sei\u00b7ne", "Brust", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df die Verwegenheit dir angebohren sey/", "tokens": ["Da\u00df", "die", "Ver\u00b7we\u00b7gen\u00b7heit", "dir", "an\u00b7ge\u00b7boh\u00b7ren", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erweist du t\u00e4glich noch mit Worten und mit Wercken/", "tokens": ["Er\u00b7weist", "du", "t\u00e4g\u00b7lich", "noch", "mit", "Wor\u00b7ten", "und", "mit", "Wer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man kan den Ubermuth aus deinen Thaten mercken/", "tokens": ["Man", "kan", "den", "U\u00b7ber\u00b7muth", "aus", "dei\u00b7nen", "Tha\u00b7ten", "mer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Kind/ das nackt und blo\u00df/ gantz ohne Scham und Schen", "tokens": ["Ein", "Kind", "/", "das", "nackt", "und", "blo\u00df", "/", "gantz", "oh\u00b7ne", "Scham", "und", "Schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJD", "KON", "ADV", "$(", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Greift auch die G\u00f6tter an/ ach unbesonner Knabe/", "tokens": ["Greift", "auch", "die", "G\u00f6t\u00b7ter", "an", "/", "ach", "un\u00b7be\u00b7son\u00b7ner", "Kna\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$(", "XY", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gedencke doch wie viel der Himmel Keile habe!", "tokens": ["Ge\u00b7den\u00b7cke", "doch", "wie", "viel", "der", "Him\u00b7mel", "Kei\u00b7le", "ha\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOKOM", "ADV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Donnerkeile sind nur Stroh f\u00fcr meinem Pfeil/", "tokens": ["Die", "Don\u00b7ner\u00b7kei\u00b7le", "sind", "nur", "Stroh", "f\u00fcr", "mei\u00b7nem", "Pfeil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fl\u00fcchtiger Mercur erinn\u2019re dich der Stunden/", "tokens": ["Und", "fl\u00fcch\u00b7ti\u00b7ger", "Mer\u00b7cur", "er\u00b7inn'\u00b7re", "dich", "der", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da sich die Chione umb deinen Hal\u00df gewunden.", "tokens": ["Da", "sich", "die", "Chi\u00b7o\u00b7ne", "umb", "dei\u00b7nen", "Hal\u00df", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sucht nicht wer t\u00f6dlich kranck bey mir noch H\u00fclf und Heil.", "tokens": ["Sucht", "nicht", "wer", "t\u00f6d\u00b7lich", "kranck", "bey", "mir", "noch", "H\u00fclf", "und", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWS", "ADJD", "ADJD", "APPR", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich mache L\u00f6wen zahm und spiele mit den Drachen/", "tokens": ["Ich", "ma\u00b7che", "L\u00f6\u00b7wen", "zahm", "und", "spie\u00b7le", "mit", "den", "Dra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Tyger m\u00fcssen stehn/ die Crocodilen lachen.", "tokens": ["Die", "Ty\u00b7ger", "m\u00fcs\u00b7sen", "stehn", "/", "die", "Cro\u00b7co\u00b7di\u00b7len", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nichts als nur Schadenfroh/ was gleicht sich meinem Ruhm?", "tokens": ["Nichts", "als", "nur", "Scha\u00b7den\u00b7froh", "/", "was", "gleicht", "sich", "mei\u00b7nem", "Ruhm", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ADV", "NN", "$(", "PWS", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die G\u00f6tter brauchen mich zu ihrem Abgesandten/", "tokens": ["Die", "G\u00f6t\u00b7ter", "brau\u00b7chen", "mich", "zu", "ih\u00b7rem", "Ab\u00b7ge\u00b7sand\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin der Scheide-Mann bey Fried und Kriegs Verwandten.", "tokens": ["Ich", "bin", "der", "Schei\u00b7de\u00b7Mann", "bey", "Fried", "und", "Kriegs", "Ver\u00b7wand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ist Beredsamkeit mein eintzig Eigenthum", "tokens": ["So", "ist", "Be\u00b7red\u00b7sam\u00b7keit", "mein", "eint\u00b7zig", "Ei\u00b7gen\u00b7thum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat die Lilien die Vorwelt mir geweyhet/", "tokens": ["Es", "hat", "die", "Li\u00b7li\u00b7en", "die", "Vor\u00b7welt", "mir", "ge\u00b7wey\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil offters meine Zung anmuthig sie erfreuet.", "tokens": ["Weil", "off\u00b7ters", "mei\u00b7ne", "Zung", "an\u00b7mut\u00b7hig", "sie", "er\u00b7freu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du f\u00fchlst die Ohren nur/ ich aber Seel und Geist/", "tokens": ["Du", "f\u00fchlst", "die", "Oh\u00b7ren", "nur", "/", "ich", "a\u00b7ber", "Seel", "und", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$(", "PPER", "ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man kan bey schwartzer Nacht nicht so viel Sterne kennen/", "tokens": ["Man", "kan", "bey", "schwart\u00b7zer", "Nacht", "nicht", "so", "viel", "Ster\u00b7ne", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ADJA", "NN", "PTKNEG", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als von der Liebe Gluth mir treue Hertzen brennen.", "tokens": ["Als", "von", "der", "Lie\u00b7be", "Gluth", "mir", "treu\u00b7e", "Hert\u00b7zen", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was di\u00df grosse Rund in seinem Krei\u00df beschleust", "tokens": ["Und", "was", "di\u00df", "gros\u00b7se", "Rund", "in", "sei\u00b7nem", "Krei\u00df", "be\u00b7schleust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PDS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hengt meiner Regung nach/ empfindt die s\u00fcssen Z\u00fcge/", "tokens": ["Hengt", "mei\u00b7ner", "Re\u00b7gung", "nach", "/", "emp\u00b7findt", "die", "s\u00fcs\u00b7sen", "Z\u00fc\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Womit ich Hertz an Hertz und Seel an Seele f\u00fcge.", "tokens": ["Wo\u00b7mit", "ich", "Hertz", "an", "Hertz", "und", "Seel", "an", "See\u00b7le", "f\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Flamme die du r\u00fchmst brennt viel in Asch und Grau\u00df/", "tokens": ["Die", "Flam\u00b7me", "die", "du", "r\u00fchmst", "brennt", "viel", "in", "Asch", "und", "Grau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "VVFIN", "ADV", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich baue Land und Stadt/ mein handeln und verkehren", "tokens": ["Ich", "bau\u00b7e", "Land", "und", "Stadt", "/", "mein", "han\u00b7deln", "und", "ver\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$(", "PPOSAT", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird uns die neue Welt durch Kauffmanschafft gewehren", "tokens": ["Wird", "uns", "die", "neu\u00b7e", "Welt", "durch", "Kauf\u00b7fman\u00b7schafft", "ge\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was bringt uns nicht vor Gut so manches Segel-Hau\u00df?", "tokens": ["Was", "bringt", "uns", "nicht", "vor", "Gut", "so", "man\u00b7ches", "Se\u00b7gel\u00b7Hau\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir speisen Indien und China auff dem Tische/", "tokens": ["Wir", "spei\u00b7sen", "In\u00b7di\u00b7en", "und", "Chi\u00b7na", "auff", "dem", "Ti\u00b7sche", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NE", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja der beeiste Nord mu\u00df liefern seine Fische.", "tokens": ["Ja", "der", "be\u00b7eis\u00b7te", "Nord", "mu\u00df", "lie\u00b7fern", "sei\u00b7ne", "Fi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es sey/ ein Kauffmann mu\u00df durch Hitze Frost und Schnee", "tokens": ["Es", "sey", "/", "ein", "Kauf\u00b7fmann", "mu\u00df", "durch", "Hit\u00b7ze", "Frost", "und", "Schnee"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "ART", "NN", "VMFIN", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Unruh und Gefahr in frembde L\u00e4nder reisen.", "tokens": ["Durch", "Un\u00b7ruh", "und", "Ge\u00b7fahr", "in", "fremb\u00b7de", "L\u00e4n\u00b7der", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist mein ergebnes Volck nicht seliger zu preisen?", "tokens": ["Ist", "mein", "er\u00b7geb\u00b7nes", "Volck", "nicht", "se\u00b7li\u00b7ger", "zu", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das ruht bey einem Baum/ das schl\u00e4fft in bundtem Klee/", "tokens": ["Das", "ruht", "bey", "ei\u00b7nem", "Baum", "/", "das", "schl\u00e4fft", "in", "bund\u00b7tem", "Klee", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$(", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Treibt kein Gewerbe nicht/ wei\u00df Wechsel nicht zu schl\u00fcssen/", "tokens