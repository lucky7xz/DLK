{"dta.poem.21362": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die M\u00f6rder, die gemeuchelt einst", "tokens": ["Die", "M\u00f6r\u00b7der", ",", "die", "ge\u00b7meu\u00b7chelt", "einst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die theure, wundersame,", "tokens": ["Die", "theu\u00b7re", ",", "wun\u00b7der\u00b7sa\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Goldlockigte Jungfrau Germania \u2013", "tokens": ["Gold\u00b7lo\u00b7ckig\u00b7te", "Jung\u00b7frau", "Ger\u00b7ma\u00b7nia", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sonne, du klagende Flamme!", "tokens": ["Son\u00b7ne", ",", "du", "kla\u00b7gen\u00b7de", "Flam\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl mancher, der sich geborgen geglaubt,", "tokens": ["Wohl", "man\u00b7cher", ",", "der", "sich", "ge\u00b7bor\u00b7gen", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PRF", "VVPP", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lachend auf seinem Schlo\u00df sa\u00df,", "tokens": ["Und", "la\u00b7chend", "auf", "sei\u00b7nem", "Schlo\u00df", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er wird nicht entgehen dem r\u00e4chenden Strang,", "tokens": ["Er", "wird", "nicht", "ent\u00b7ge\u00b7hen", "dem", "r\u00e4\u00b7chen\u00b7den", "Strang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dem Zorne Barbarossas! \u2013 \u2013 \u2013", "tokens": ["Dem", "Zor\u00b7ne", "Bar\u00b7ba\u00b7ros\u00b7sas", "!", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "NE", "$.", "$(", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wie klingen sie lieblich, wie klingen sie s\u00fc\u00df,", "tokens": ["Wie", "klin\u00b7gen", "sie", "lieb\u00b7lich", ",", "wie", "klin\u00b7gen", "sie", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die M\u00e4hrchen der alten Amme!", "tokens": ["Die", "M\u00e4hr\u00b7chen", "der", "al\u00b7ten", "Am\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein abergl\u00e4ubisches Herze jauchzt:", "tokens": ["Mein", "a\u00b7berg\u00b7l\u00e4u\u00b7bi\u00b7sches", "Her\u00b7ze", "jauchzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonne, du klagende Flamme!", "tokens": ["Son\u00b7ne", ",", "du", "kla\u00b7gen\u00b7de", "Flam\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}