{"textgrid.poem.25381": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Alleen der Lichter, die der Flu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Alleen der Lichter, die der Flu\u00df", "tokens": ["Die", "Al\u00b7leen", "der", "Lich\u00b7ter", ",", "die", "der", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ins Dunkel schwemmt, sind schon erblindet", "tokens": ["ins", "Dun\u00b7kel", "schwemmt", ",", "sind", "schon", "er\u00b7blin\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In den streifenden Nebeln.", "tokens": ["In", "den", "strei\u00b7fen\u00b7den", "Ne\u00b7beln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bald sind die Staden eingedeckt. Schon findet", "tokens": ["Bald", "sind", "die", "Sta\u00b7den", "ein\u00b7ge\u00b7deckt", ".", "Schon", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein Laut den Weg mehr aus dem tr\u00e4gen Sumpf,", "tokens": ["Kein", "Laut", "den", "Weg", "mehr", "aus", "dem", "tr\u00e4\u00b7gen", "Sumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "ADV", "APPR", "ART", "AD