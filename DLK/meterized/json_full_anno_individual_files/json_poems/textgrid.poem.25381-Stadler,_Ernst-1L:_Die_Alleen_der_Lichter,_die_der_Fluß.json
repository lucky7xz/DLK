"], "pos": ["PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Legt sich zu mir, ganz still, nur schattenhaft,", "tokens": ["Legt", "sich", "zu", "mir", ",", "ganz", "still", ",", "nur", "schat\u00b7ten\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "nur wie die letzte Welle Dufts", "tokens": ["nur", "wie", "die", "letz\u00b7te", "Wel\u00b7le", "Dufts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "von Schlehdornstr\u00e4uchern abgeweht,", "tokens": ["von", "Schleh\u00b7dorn\u00b7str\u00e4u\u00b7chern", "ab\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Nur wie ein Spalt von Licht, davon doch meine Seele", "tokens": ["Nur", "wie", "ein", "Spalt", "von", "Licht", ",", "da\u00b7von", "doch", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "$,", "PAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "wie ein Fr\u00fchlingsbeet in Bl\u00fcten steht \u2013", "tokens": ["wie", "ein", "Fr\u00fch\u00b7lings\u00b7beet", "in", "Bl\u00fc\u00b7ten", "steht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Ich schreite wie durch G\u00e4rten.", "tokens": ["Ich", "schrei\u00b7te", "wie", "durch", "G\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Bin auf einem gro\u00dfen Platz.", "tokens": ["Bin", "auf", "ei\u00b7nem", "gro\u00b7\u00dfen", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Nebel h\u00e4ngt d\u00fcnn und flimmernd", "tokens": ["Ne\u00b7bel", "h\u00e4ngt", "d\u00fcnn", "und", "flim\u00b7mernd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.26": {"text": "wie durch Silbernetz gesiebt \u2013", "tokens": ["wie", "durch", "Sil\u00b7ber\u00b7netz", "ge\u00b7siebt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und pl\u00f6tzlich wei\u00df ich: hinter diesen Fenstern dort", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "wei\u00df", "ich", ":", "hin\u00b7ter", "die\u00b7sen", "Fens\u00b7tern", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "schl\u00e4ft eine Frau, die mich einmal geliebt,", "tokens": ["schl\u00e4ft", "ei\u00b7ne", "Frau", ",", "die", "mich", "ein\u00b7mal", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und die ich liebte. H\u00fcllen fallen. 