{"textgrid.poem.44157": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bruder, denck an deinen Freund, der dich wohl nicht wieder siehet,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bruder, denck an deinen Freund, der dich wohl nicht wieder siehet,", "tokens": ["Bru\u00b7der", ",", "denck", "an", "dei\u00b7nen", "Freund", ",", "der", "dich", "wohl", "nicht", "wie\u00b7der", "sie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und vergieb mir, wenn ich dich oftermahls aus Noth bem\u00fchet.", "tokens": ["Und", "ver\u00b7gieb", "mir", ",", "wenn", "ich", "dich", "of\u00b7ter\u00b7mahls", "aus", "Noth", "be\u00b7m\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dein so redliches Gem\u00fcthe steht in meinem Herzen fest,", "tokens": ["Dein", "so", "red\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", "steht", "in", "mei\u00b7nem", "Her\u00b7zen", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bis der abgekr\u00e4nckte C\u00f6rper den gefangnen Geist verl\u00e4st.", "tokens": ["Bis", "der", "ab\u00b7ge\u00b7kr\u00e4nck\u00b7te", "C\u00f6r\u00b7per", "den", "ge\u00b7fang\u00b7nen", "Geist", "ver\u00b7l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "H\u00e4tten mich die Meinigen, wie sie von Natur wohl sollen,", "tokens": ["H\u00e4t\u00b7ten", "mich", "die", "Mei\u00b7ni\u00b7gen", ",", "wie", "sie", "von", "Na\u00b7tur", "wohl", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSS", "$,", "PWAV", "PPER", "APPR", "NN", "ADV", "VMFIN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Nach der rechten Eltern Art aus dem Staube rei\u00dfen wollen,", "tokens": ["Nach", "der", "rech\u00b7ten", "El\u00b7tern", "Art", "aus", "dem", "Stau\u00b7be", "rei\u00b7\u00dfen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "O so h\u00e4tt ich manchen Fehler, der mir \u00f6fters schaden kan,", "tokens": ["O", "so", "h\u00e4tt", "ich", "man\u00b7chen", "Feh\u00b7ler", ",", "der", "mir", "\u00f6f\u00b7ters", "scha\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Nicht aus unruhvollem Herzen mir zum Spotte selbst gethan.", "tokens": ["Nicht", "aus", "un\u00b7ruh\u00b7vol\u00b7lem", "Her\u00b7zen", "mir", "zum", "Spot\u00b7te", "selbst", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Lebe du inde\u00dfen wohl! Ich bequeme mich den Schl\u00fc\u00dfen,", "tokens": ["Le\u00b7be", "du", "in\u00b7de\u00b7\u00dfen", "wohl", "!", "Ich", "be\u00b7que\u00b7me", "mich", "den", "Schl\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.10": {"text": "Die mich in das Vaterland ehmahls recht mit Zwang geri\u00dfen.", "tokens": ["Die", "mich", "in", "das", "Va\u00b7ter\u00b7land", "eh\u00b7mahls", "recht", "mit", "Zwang", "ge\u00b7ri\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Gott vermehre dir dein Gl\u00fccke! Lebe wohl und denck an den,", "tokens": ["Gott", "ver\u00b7meh\u00b7re", "dir", "dein", "Gl\u00fc\u00b7cke", "!", "Le\u00b7be", "wohl", "und", "denck", "an", "den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "VVIMP", "ADV", "KON", "VVIMP", "APPR", "ART", "$,"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.12": {"text": "Der anjezo sehnlich w\u00fcntschet, noch ein Blat von dir zu sehn!", "tokens": ["Der", "an\u00b7je\u00b7zo", "sehn\u00b7lich", "w\u00fcnt\u0