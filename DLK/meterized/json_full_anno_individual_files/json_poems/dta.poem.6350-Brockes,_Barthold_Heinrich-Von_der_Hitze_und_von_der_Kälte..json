{"dta.poem.6350": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheinet mit dem Kalck auch eben so zu gehn.", "tokens": ["Es", "schei\u00b7net", "mit", "dem", "Kalck", "auch", "e\u00b7ben", "so", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn seine kleine Theil entz\u00fcndt, gebrennt,", "tokens": ["Wenn", "sei\u00b7ne", "klei\u00b7ne", "Theil", "ent\u00b7z\u00fcndt", ",", "ge\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Wasser angefeucht; zertheilt er sich und trennt,", "tokens": ["Mit", "Was\u00b7ser", "an\u00b7ge\u00b7feucht", ";", "zer\u00b7theilt", "er", "sich", "und", "trennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$.", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umgeben und gedruckt vom ersten Element,", "tokens": ["Um\u00b7ge\u00b7ben", "und", "ge\u00b7druckt", "vom", "ers\u00b7ten", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch deren strengen Druck des Brennens Kr\u00e4fft\u2019 entstehn.", "tokens": ["Durch", "de\u00b7ren", "stren\u00b7gen", "Druck", "des", "Bren\u00b7nens", "Kr\u00e4fft'", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}