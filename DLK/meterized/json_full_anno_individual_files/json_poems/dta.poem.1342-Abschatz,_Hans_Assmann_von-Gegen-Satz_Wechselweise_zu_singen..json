{"dta.poem.1342": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gegen-Satz/ Wechselweise zu singen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Lasset uns nach Zion wallen/", "tokens": ["Las\u00b7set", "uns", "nach", "Zi\u00b7on", "wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Sch\u00e4del-St\u00e4tte sehn/", "tokens": ["Und", "die", "Sch\u00e4\u00b7del\u00b7St\u00e4t\u00b7te", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo dem Sch\u00f6nsten unter allen", "tokens": ["Wo", "dem", "Sch\u00f6ns\u00b7ten", "un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So viel Ubels ist geschehn!", "tokens": ["So", "viel", "U\u00b7bels", "ist", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Last uns ihm ein Grab-Lieb singen!", "tokens": ["Last", "uns", "ihm", "ein", "Grab\u00b7Lieb", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun sie ihn zur Ruhe bringen!", "tokens": ["Nun", "sie", "ihn", "zur", "Ru\u00b7he", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der die Erde selbst lie\u00df werden/", "tokens": ["Der", "die", "Er\u00b7de", "selbst", "lie\u00df", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der den Himmel ausgespannt/", "tokens": ["Der", "den", "Him\u00b7mel", "aus\u00b7ge\u00b7spannt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat nichts Eignes auff der Erden/", "tokens": ["Hat", "nichts", "Eig\u00b7nes", "auff", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Borgt ihm Josephs fremden Sand/", "tokens": ["Borgt", "ihm", "Jo\u00b7se\u00b7phs", "frem\u00b7den", "Sand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wenn er aus der Grufft wird steigen", "tokens": ["Wenn", "er", "aus", "der", "Grufft", "wird", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist der Himmel unser eigen.", "tokens": ["Ist", "der", "Him\u00b7mel", "un\u00b7ser", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "An dem Creutze starb das Leben/", "tokens": ["An", "dem", "Creut\u00b7ze", "starb", "das", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht im Grabes Schatten aus/", "tokens": ["Ruht", "im", "Gra\u00b7bes", "Schat\u00b7ten", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Sonn hat sich begeben", "tokens": ["Und", "die", "Sonn", "hat", "sich", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das d\u00fcstre Todten-Hau\u00df/", "tokens": ["In", "das", "d\u00fcst\u00b7re", "Tod\u00b7ten\u00b7Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber last das tr\u00fcbe Weinen/", "tokens": ["A\u00b7ber", "last", "das", "tr\u00fc\u00b7be", "Wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald wird sie uns wieder scheinen.", "tokens": ["Bald", "wird", "sie", "uns", "wie\u00b7der", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Evens s\u00fcsser Apffel-Bissen", "tokens": ["E\u00b7vens", "s\u00fcs\u00b7ser", "Apf\u00b7fel\u00b7Bis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt die herbe Todes-Post/", "tokens": ["Bringt", "die", "her\u00b7be", "To\u00b7des\u00b7Post", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solche wieder zu vers\u00fcssen", "tokens": ["Sol\u00b7che", "wie\u00b7der", "zu", "ver\u00b7s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat viel Schwei\u00df und Blutt gekost;", "tokens": ["Hat", "viel", "Schwei\u00df", "und", "Blutt", "ge\u00b7kost", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gottes Gnad an statt Napellen", "tokens": ["Got\u00b7tes", "Gnad", "an", "statt", "Na\u00b7pel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4chst an den benetzten Stellen.", "tokens": ["W\u00e4chst", "an", "den", "be\u00b7netz\u00b7ten", "Stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Last uns in den Garten eilen/", "tokens": ["Last", "uns", "in", "den", "Gar\u00b7ten", "ei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Myrrhen-P\u00fcschel stehn/", "tokens": ["Wo", "die", "Myr\u00b7rhen\u00b7P\u00fc\u00b7schel", "stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Seelen auszuheilen/", "tokens": ["Uns\u00b7re", "See\u00b7len", "aus\u00b7zu\u00b7hei\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Balsam-Stauden gehn/", "tokens": ["Zu", "den", "Bal\u00b7sam\u00b7Stau\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier kan man ohn Dornenstechen", "tokens": ["Hier", "kan", "man", "ohn", "Dor\u00b7nen\u00b7ste\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Edle Lebens-Rosen brechen.", "tokens": ["Ed\u00b7le", "Le\u00b7bens\u00b7Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du Durchbrecher harter Steine/", "tokens": ["Du", "Durch\u00b7bre\u00b7cher", "har\u00b7ter", "Stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den kein Marmor halten kan/", "tokens": ["Den", "kein", "Mar\u00b7mor", "hal\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich will dir auch mein Gebeine", "tokens": ["Ich", "will", "dir", "auch", "mein", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu verwahren trauen an!", "tokens": ["Zu", "ver\u00b7wah\u00b7ren", "trau\u00b7en", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du/ das Haubt/ du lebest wieder/", "tokens": ["Du", "/", "das", "Haubt", "/", "du", "le\u00b7best", "wie\u00b7der", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "NN", "$(", "PPER", "VVFIN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erhebst auch deine Glieder.", "tokens": ["Und", "er\u00b7hebst", "auch", "dei\u00b7ne", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schatten mag die Erde decken/", "tokens": ["Schat\u00b7ten", "mag", "die", "Er\u00b7de", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finsternis die Lufft umziehn/", "tokens": ["Fins\u00b7ter\u00b7nis", "die", "Lufft", "um\u00b7ziehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVIZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mich Tod und Nacht erschrecken", "tokens": ["Wenn", "mich", "Tod", "und", "Nacht", "er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich in di\u00df Lager fliehn.", "tokens": ["Will", "ich", "in", "di\u00df", "La\u00b7ger", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDS", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu verschlaffen allen Jammer/", "tokens": ["Zu", "ver\u00b7schlaf\u00b7fen", "al\u00b7len", "Jam\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4hl ich hier die Ruhe-Kammer.", "tokens": ["W\u00e4hl", "ich", "hier", "die", "Ru\u00b7he\u00b7Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun so sey gegr\u00fc\u00dft/ o H\u00f6le/", "tokens": ["Nun", "so", "sey", "ge\u00b7gr\u00fc\u00dft", "/", "o", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$(", "FM", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00df des Lebens Echo klingt/", "tokens": ["Drau\u00df", "des", "Le\u00b7bens", "E\u00b7cho", "klingt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00df f\u00fcr meine matte Seele", "tokens": ["Drau\u00df", "f\u00fcr", "mei\u00b7ne", "mat\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Labsal in dem Tod entspringt!", "tokens": ["Lab\u00b7sal", "in", "dem", "Tod", "ent\u00b7springt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Eh man mich ins Grab soll sencken", "tokens": ["Eh", "man", "mich", "ins", "Grab", "soll", "sen\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Will ich dein zum Trost gedencken.", "tokens": ["Will", "ich", "dein", "zum", "Trost", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}