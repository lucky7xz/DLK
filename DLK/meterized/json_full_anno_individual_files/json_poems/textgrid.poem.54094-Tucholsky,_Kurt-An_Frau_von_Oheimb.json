{"textgrid.poem.54094": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An Frau von Oheimb", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6nn mir das traute Du. Ich kann vor Lachen", "tokens": ["G\u00f6nn", "mir", "das", "trau\u00b7te", "Du", ".", "Ich", "kann", "vor", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PDS", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dich ja nicht siezen \u2013 nimm mir das nicht krumm!", "tokens": ["dich", "ja", "nicht", "sie\u00b7zen", "\u2013", "nimm", "mir", "das", "nicht", "krumm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "$(", "VVIMP", "PPER", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sag mir nur eines: Was sie bei dir machen,", "tokens": ["Sag", "mir", "nur", "ei\u00b7nes", ":", "Was", "sie", "bei", "dir", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PIS", "$.", "PWS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "siehst du das nicht \u2013 den Fez um dich herum?", "tokens": ["siehst", "du", "das", "nicht", "\u2013", "den", "Fez", "um", "dich", "he\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PTKNEG", "$(", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die gehrockeingebundnen B\u00fcrokraten,", "tokens": ["Die", "ge\u00b7hro\u00b7ck\u00b7ein\u00b7ge\u00b7bund\u00b7nen", "B\u00fc\u00b7ro\u00b7kra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "die Talleyrand-Kopien der Diplomaten,", "tokens": ["die", "Tal\u00b7ley\u00b7ran\u00b7dKo\u00b7pi\u00b7en", "der", "Dip\u00b7lo\u00b7ma\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "der aus Liberia \u2013 und selbst der aus Minka . . .", "tokens": ["der", "aus", "Li\u00b7be\u00b7ria", "\u2013", "und", "selbst", "der", "aus", "Min\u00b7ka", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "APPR", "NE", "$(", "KON", "ADV", "ART", "APPR", "NE", "$.", "$.", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Kathinka \u2013!", "tokens": ["Ka\u00b7thin\u00b7ka", "\u2013", "!"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ach, ihr beklagt in wichtigkeitsgeschwollnen", "tokens": ["Ach", ",", "ihr", "be\u00b7klagt", "in", "wich\u00b7tig\u00b7keits\u00b7ge\u00b7schwoll\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und sch\u00f6nen Reden diese Not der Zeit.", "tokens": ["und", "sch\u00f6\u00b7nen", "Re\u00b7den", "die\u00b7se", "Not", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um Autok\u00fchler die kameelhaarwollnen", "tokens": ["Um", "Au\u00b7to\u00b7k\u00fch\u00b7ler", "die", "ka\u00b7me\u00b7el\u00b7haar\u00b7woll\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Schutzdecken . . . Damen mit dem Schleppenkleid . . .", "tokens": ["Schutz\u00b7de\u00b7cken", ".", ".", ".", "Da\u00b7men", "mit", "dem", "Schlep\u00b7pen\u00b7kleid", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$.", "NN", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Du bist so selig, wenn die Schm\u00f6cke schreiben.", "tokens": ["Du", "bist", "so", "se\u00b7lig", ",", "wenn", "die", "Schm\u00f6\u00b7cke", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr quatscht und quatscht. Die Dividenden bleiben.", "tokens": ["Ihr", "quatscht", "und", "quatscht", ".", "Die", "Di\u00b7vi\u00b7den\u00b7den", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es flirrn und flirten Tee- und Kaffee-Trinker . . .", "tokens": ["Es", "flirrn", "und", "flir\u00b7ten", "Tee", "und", "Kaf\u00b7fee\u00b7Trin\u00b7ker", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVINF", "KON", "ADJA", "TRUNC", "KON", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kathinka \u2013!", "tokens": ["Ka\u00b7thin\u00b7ka", "\u2013", "!"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Die Republik gibt sich in deinen R\u00e4umen", "tokens": ["Die", "Re\u00b7pub\u00b7lik", "gibt", "sich", "in", "dei\u00b7nen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "ein Stelldichein. O stell sie wieder weg!", "tokens": ["ein", "Stell\u00b7dich\u00b7ein", ".", "O", "stell", "sie", "wie\u00b7der", "weg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NE", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schlafwandler sind sie, die regierend tr\u00e4umen . . .", "tokens": ["Schlaf\u00b7wand\u00b7ler", "sind", "sie", ",", "die", "re\u00b7gie\u00b7rend", "tr\u00e4u\u00b7men", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und die Reformen sind wie Teegeb\u00e4ck.", "tokens": ["und", "die", "Re\u00b7for\u00b7men", "sind", "wie", "Tee\u00b7ge\u00b7b\u00e4ck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und blickte Salomo auf diese Scheitel,", "tokens": ["Und", "blick\u00b7te", "Sa\u00b7lo\u00b7mo", "auf", "die\u00b7se", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "er spr\u00e4che: Hier ist alles eitel.", "tokens": ["er", "spr\u00e4\u00b7che", ":", "Hier", "ist", "al\u00b7les", "ei\u00b7tel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf hundert rechte G\u00e4ste kommt ein linker . . .", "tokens": ["Auf", "hun\u00b7dert", "rech\u00b7te", "G\u00e4s\u00b7te", "kommt", "ein", "lin\u00b7ker", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "ART", "ADJA", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kathinka \u2013!", "tokens": ["Ka\u00b7thin\u00b7ka", "\u2013", "!"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Kathinka, gutes Kind!", "tokens": ["Ka\u00b7thin\u00b7ka", ",", "gu\u00b7tes", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du bist so niedlich", "tokens": ["Du", "bist", "so", "nied\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "und h\u00e4ltst dich f\u00fcr den Nagel der Saison.", "tokens": ["und", "h\u00e4ltst", "dich", "f\u00fcr", "den", "Na\u00b7gel", "der", "Sai\u00b7son", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geh, h\u00f6r gut zu \u2013 ich sag dirs friedlich:", "tokens": ["Geh", ",", "h\u00f6r", "gut", "zu", "\u2013", "ich", "sag", "dirs", "fried\u00b7lich", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADJD", "PTKZU", "$(", "PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ne gute Stube ist noch kein Salon.", "tokens": ["ne", "gu\u00b7te", "Stu\u00b7be", "ist", "noch", "kein", "Sa\u00b7lon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du wei\u00dft von Politik auch nicht die Bohne.", "tokens": ["Du", "wei\u00dft", "von", "Po\u00b7li\u00b7tik", "auch", "nicht", "die", "Boh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "H\u00f6rst du den Schritt der Proletarier-Bataillone?", "tokens": ["H\u00f6rst", "du", "den", "Schritt", "der", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7er\u00b7Ba\u00b7tail\u00b7lo\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Du kommst zu sp\u00e4t.", "tokens": ["Du", "kommst", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Denn unsre Zeit ist flinker", "tokens": ["Denn", "uns\u00b7re", "Zeit", "ist", "flin\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": " als du, Kathinka.", "tokens": ["als", "du", ",", "Ka\u00b7thin\u00b7ka", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "G\u00f6nn mir das traute Du. Ich kann vor Lachen", "tokens": ["G\u00f6nn", "mir", "das", "trau\u00b7te", "Du", ".", "Ich", "kann", "vor", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PDS", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dich ja nicht siezen \u2013 nimm mir das nicht krumm!", "tokens": ["dich", "ja", "nicht", "sie\u00b7zen", "\u2013", "nimm", "mir", "das", "nicht", "krumm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "$(", "VVIMP", "PPER", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sag mir nur eines: Was sie bei dir machen,", "tokens": ["Sag", "mir", "nur", "ei\u00b7nes", ":", "Was", "sie", "bei", "dir", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PIS", "$.", "PWS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "siehst du das nicht \u2013 den Fez um dich herum?", "tokens": ["siehst", "du", "das", "nicht", "\u2013", "den", "Fez", "um", "dich", "he\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PTKNEG", "$(", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure"