{"dta.poem.20485": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An die Livia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Macht kein funcke meiner flammen/", "tokens": ["Macht", "kein", "fun\u00b7cke", "mei\u00b7ner", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PPOSAT", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Livia/ dir einen brand?", "tokens": ["Li\u00b7via", "/", "dir", "ei\u00b7nen", "brand", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tausend schlagen offt zusammen/", "tokens": ["Tau\u00b7send", "schla\u00b7gen", "offt", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner nimmt doch \u00fcberhand.", "tokens": ["Kei\u00b7ner", "nimmt", "doch", "\u00fc\u00b7ber\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du must k\u00e4lter mir zur pein", "tokens": ["Du", "must", "k\u00e4l\u00b7ter", "mir", "zur", "pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Salamander seyn.", "tokens": ["Als", "ein", "Sa\u00b7la\u00b7man\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Ich gedachte/ da\u00df das plitzen/", "tokens": ["Ich", "ge\u00b7dach\u00b7te", "/", "da\u00df", "das", "plit\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PDS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So aus deinen augen steigt/", "tokens": ["So", "aus", "dei\u00b7nen", "au\u00b7gen", "steigt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch im hertzen m\u00fcste sitzen/", "tokens": ["Auch", "im", "hert\u00b7zen", "m\u00fcs\u00b7te", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und aus hitze w\u00e4r erzeugt;", "tokens": ["Und", "aus", "hit\u00b7ze", "w\u00e4r", "er\u00b7zeugt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So ist es cometen-licht/", "tokens": ["So", "ist", "es", "co\u00b7me\u00b7ten\u00b7licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dessen strahl nur frost verspricht.", "tokens": ["Des\u00b7sen", "strahl", "nur", "frost", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn mein brand denn nicht gewinnet/", "tokens": ["Wenn", "mein", "brand", "denn", "nicht", "ge\u00b7win\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "ADV", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df der nasse zunder dran/", "tokens": ["Mu\u00df", "der", "nas\u00b7se", "zun\u00b7der", "dran", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der aus meinen augen rinnet/", "tokens": ["Der", "aus", "mei\u00b7nen", "au\u00b7gen", "rin\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und versuchen/ was er kan.", "tokens": ["Und", "ver\u00b7su\u00b7chen", "/", "was", "er", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird doch glut durch flut erweckt/", "tokens": ["Wird", "doch", "glut", "durch", "flut", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn das wasser kalck ansteckt.", "tokens": ["Wenn", "das", "was\u00b7ser", "kalck", "an\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! so werde durch mein weinen/", "tokens": ["Ach", "!", "so", "wer\u00b7de", "durch", "mein", "wei\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "APPR", "PPOSAT", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Livia/ von flammen reich;", "tokens": ["Li\u00b7via", "/", "von", "flam\u00b7men", "reich", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "APPR", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch du must auch lieblich scheinen/", "tokens": ["Doch", "du", "must", "auch", "lieb\u00b7lich", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey der reinen sonne gleich/", "tokens": ["Sey", "der", "rei\u00b7nen", "son\u00b7ne", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die den thau/ den sie gebracht/", "tokens": ["Die", "den", "thau", "/", "den", "sie", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$(", "ART", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Augenblicks zu asche macht.", "tokens": ["Au\u00b7gen\u00b7blicks", "zu", "asc\u00b7he", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dir sey meine qvaal befohlen/", "tokens": ["Dir", "sey", "mei\u00b7ne", "qvaal", "be\u00b7foh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lesche die entbrannte pein!", "tokens": ["Le\u00b7sche", "die", "ent\u00b7brann\u00b7te", "pein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine glut soll meinen kohlen", "tokens": ["Dei\u00b7ne", "glut", "soll", "mei\u00b7nen", "koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein schnee-kaltes wasser seyn.", "tokens": ["Ein", "schnee\u00b7kal\u00b7tes", "was\u00b7ser", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo dein ei\u00df noch l\u00e4nger w\u00e4hrt/", "tokens": ["Wo", "dein", "ei\u00df", "noch", "l\u00e4n\u00b7ger", "w\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ich bald in staub verkehrt.", "tokens": ["Bin", "ich", "bald", "in", "staub", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}