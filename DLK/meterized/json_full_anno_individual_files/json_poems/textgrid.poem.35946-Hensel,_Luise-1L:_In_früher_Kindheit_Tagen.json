ine.1": {"text": "Du aber hast in Hulden", "tokens": ["Du", "a\u00b7ber", "hast", "in", "Hul\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seitdem mich stets erh\u00f6rt,", "tokens": ["Seit\u00b7dem", "mich", "stets", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trotz all der bittern Schulden,", "tokens": ["Trotz", "all", "der", "bit\u00b7tern", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die bald mein Herz beschwert.", "tokens": ["Die", "bald", "mein", "Herz", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "O, Dank f\u00fcr jene Stunde,", "tokens": ["O", ",", "Dank", "f\u00fcr", "je\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mir so viel gew\u00e4hrt!", "tokens": ["Die", "mir", "so", "viel", "ge\u00b7w\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Dank dem frommen Munde,", "tokens": ["Und", "Dank", "dem", "from\u00b7men", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"tex