{"textgrid.poem.35946": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: In fr\u00fcher Kindheit Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In fr\u00fcher Kindheit Tagen", "tokens": ["In", "fr\u00fc\u00b7her", "Kind\u00b7heit", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von treuer Hand gepflegt,", "tokens": ["Von", "treu\u00b7er", "Hand", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat Gnade mich getragen,", "tokens": ["Hat", "Gna\u00b7de", "mich", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Mutterliebe tr\u00e4gt. \u2013", "tokens": ["Wie", "Mut\u00b7ter\u00b7lie\u00b7be", "tr\u00e4gt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Mutter that in Bildern", "tokens": ["Die", "Mut\u00b7ter", "that", "in", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir, s\u00fc\u00dfer Jesus! Dich", "tokens": ["Mir", ",", "s\u00fc\u00b7\u00dfer", "Je\u00b7sus", "!", "Dich"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJA", "NE", "$.", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Deine Liebe schildern", "tokens": ["Und", "Dei\u00b7ne", "Lie\u00b7be", "schil\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lehrte glauben mich.", "tokens": ["Und", "lehr\u00b7te", "glau\u00b7ben", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie wies mir, die da kamen", "tokens": ["Sie", "wies", "mir", ",", "die", "da", "ka\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Dir mit ihrem Leid,", "tokens": ["Zu", "Dir", "mit", "ih\u00b7rem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Siechen, Blinden, Lahmen,", "tokens": ["Die", "Sie\u00b7chen", ",", "Blin\u00b7den", ",", "Lah\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Deine Huld befreit.", "tokens": ["Die", "Dei\u00b7ne", "Huld", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Einst sah ich in Gedanken", "tokens": ["Einst", "sah", "ich", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im stillen D\u00e4mmerschein", "tokens": ["Im", "stil\u00b7len", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Daher die Schaaren wanken", "tokens": ["Da\u00b7her", "die", "Schaa\u00b7ren", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dicht sich um Dich reih'n.", "tokens": ["Und", "dicht", "sich", "um", "Dich", "reih'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da dacht' ich: Wenn Er k\u00e4me", "tokens": ["Da", "dacht'", "ich", ":", "Wenn", "Er", "k\u00e4\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und fragte, was ", "tokens": ["Und", "frag\u00b7te", ",", "was"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "PWS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was ich mir dann wohl n\u00e4hme,", "tokens": ["Was", "ich", "mir", "dann", "wohl", "n\u00e4h\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was ich bitten sollt'. \u2013", "tokens": ["Und", "was", "ich", "bit\u00b7ten", "sollt'", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O, rief ich, keine Gaben,", "tokens": ["O", ",", "rief", "ich", ",", "kei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dann werd' ich Alles haben,", "tokens": ["Dann", "werd'", "ich", "Al\u00b7les", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich selbst und Deine Lieb'.", "tokens": ["Dich", "selbst", "und", "Dei\u00b7ne", "Lieb'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Denn beten hei\u00dft ja ringen", "tokens": ["Denn", "be\u00b7ten", "hei\u00dft", "ja", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Deiner Gotteskraft,", "tokens": ["Mit", "Dei\u00b7ner", "Got\u00b7tes\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und beten ist ein zwingen,", "tokens": ["Und", "be\u00b7ten", "ist", "ein", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Alles uns verschafft.", "tokens": ["Das", "Al\u00b7les", "uns", "ver\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die kleinen H\u00e4nde schlossen", "tokens": ["Die", "klei\u00b7nen", "H\u00e4n\u00b7de", "schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich ringend zum Gebet", "tokens": ["Sich", "rin\u00b7gend", "zum", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und schwere Thr\u00e4nen flossen", "tokens": ["Und", "schwe\u00b7re", "Thr\u00e4\u00b7nen", "flos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Waizen, dicht ges\u00e4't.", "tokens": ["Wie", "Wai\u00b7zen", ",", "dicht", "ge\u00b7s\u00e4'", "t."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PWAV", "NN", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O, gieb mir Beten! Beten!", "tokens": ["O", ",", "gieb", "mir", "Be\u00b7ten", "!", "Be\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nichts Andres brauch' ich ja:", "tokens": ["Nichts", "And\u00b7res", "brauch'", "ich", "ja", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich zu Dir kann reden,", "tokens": ["Wenn", "ich", "zu", "Dir", "kann", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann bist Du stets mir nah'. \u2013", "tokens": ["Dann", "bist", "Du", "stets", "mir", "nah'", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und meine Sinne schwanden", "tokens": ["Und", "mei\u00b7ne", "Sin\u00b7ne", "schwan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob meinem hei\u00dfen Fleh'n,", "tokens": ["Ob", "mei\u00b7nem", "hei\u00b7\u00dfen", "Fleh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis mich die M\u00e4gdlein fanden,", "tokens": ["Bis", "mich", "die", "M\u00e4gd\u00b7lein", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir halfen schlafen geh'n. \u2013", "tokens": ["Mir", "hal\u00b7fen", "schla\u00b7fen", "geh'", "n.", "\u2013"], "token_info": ["word", "word", "word", "word", "abbreviation", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "VVFIN", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du aber hast in Hulden", "tokens": ["Du", "a\u00b7ber", "hast", "in", "Hul\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seitdem mich stets erh\u00f6rt,", "tokens": ["Seit\u00b7dem", "mich", "stets", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trotz all der bittern Schulden,", "tokens": ["Trotz", "all", "der", "bit\u00b7tern", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die bald mein Herz beschwert.", "tokens": ["Die", "bald", "mein", "Herz", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "O, Dank f\u00fcr jene Stunde,", "tokens": ["O", ",", "Dank", "f\u00fcr", "je\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mir so viel gew\u00e4hrt!", "tokens": ["Die", "mir", "so", "viel", "ge\u00b7w\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Dank dem frommen Munde,", "tokens": ["Und", "Dank", "dem", "from\u00b7men", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der glauben mich gelehrt!", "tokens": ["Der", "glau\u00b7ben", "mich", "ge\u00b7lehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und Dank dem treuen Engel,", "tokens": ["Und", "Dank", "dem", "treu\u00b7en", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den 