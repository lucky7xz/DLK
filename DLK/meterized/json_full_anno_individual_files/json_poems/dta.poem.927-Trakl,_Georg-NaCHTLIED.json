{"dta.poem.927": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "NaCHTLIED", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Des Unbewegten Odem. Ein Tiergesicht", "tokens": ["Des", "Un\u00b7be\u00b7weg\u00b7ten", "O\u00b7dem", ".", "Ein", "Tier\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erstarrt vor Bl\u00e4ue, ihrer Heiligkeit.", "tokens": ["Er\u00b7starrt", "vor", "Bl\u00e4u\u00b7e", ",", "ih\u00b7rer", "Hei\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gewaltig ist das Schweigen im Stein;", "tokens": ["Ge\u00b7wal\u00b7tig", "ist", "das", "Schwei\u00b7gen", "im", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Die Maske eines n\u00e4chtlichen Vogels. Sanfter Dreiklang", "tokens": ["Die", "Mas\u00b7ke", "ei\u00b7nes", "n\u00e4cht\u00b7li\u00b7chen", "Vo\u00b7gels", ".", "Sanf\u00b7ter", "Drei\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "NN", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verklingt in einem. Elai! dein Antlitz", "tokens": ["Ver\u00b7klingt", "in", "ei\u00b7nem", ".", "E\u00b7lai", "!", "dein", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "$.", "NE", "$.", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Beugt sich sprachlos \u00fcber bl\u00e4uliche Wasser.", "tokens": ["Beugt", "sich", "sprach\u00b7los", "\u00fc\u00b7ber", "bl\u00e4u\u00b7li\u00b7che", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O! ihr stillen Spiegel der Wahrheit.", "tokens": ["O", "!", "ihr", "stil\u00b7len", "Spie\u00b7gel", "der", "Wahr\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "An des Einsamen elfenbeinerner Schl\u00e4fe", "tokens": ["An", "des", "Ein\u00b7sa\u00b7men", "el\u00b7fen\u00b7bei\u00b7ner\u00b7ner", "Schl\u00e4\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erscheint der Abglanz gefallener Engel.", "tokens": ["Er\u00b7scheint", "der", "Ab\u00b7glanz", "ge\u00b7fal\u00b7le\u00b7ner", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}