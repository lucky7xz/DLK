{"dta.poem.5930": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch unsere drey Elementen, die", "tokens": ["Durch", "un\u00b7se\u00b7re", "drey", "E\u00b7le\u00b7men\u00b7ten", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$,", "PRELS"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein fremd Gemisch versammlet, trennt, und sie", "tokens": ["Ein", "fremd", "Ge\u00b7misch", "ver\u00b7samm\u00b7let", ",", "trennt", ",", "und", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,", "VVFIN", "$,", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vereint und unterbricht; entstehen", "tokens": ["Ver\u00b7eint", "und", "un\u00b7ter\u00b7bricht", ";", "ent\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die C\u00f6rper allzumal, ja sie vergehen", "tokens": ["Die", "C\u00f6r\u00b7per", "all\u00b7zu\u00b7mal", ",", "ja", "sie", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf gleiche Weise, wie wir sehen.", "tokens": ["Auf", "glei\u00b7che", "Wei\u00b7se", ",", "wie", "wir", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-