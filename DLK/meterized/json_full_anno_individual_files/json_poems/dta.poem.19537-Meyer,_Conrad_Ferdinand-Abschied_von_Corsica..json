{"dta.poem.19537": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Corsica.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Oelbaumsilber, Myrte, Lorbeer, Pinie,", "tokens": ["O\u00b7el\u00b7baum\u00b7sil\u00b7ber", ",", "Myr\u00b7te", ",", "Lor\u00b7beer", ",", "Pi\u00b7nie", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Bald im Schnee der Heimath denk' ich euer \u2014", "tokens": ["Bald", "im", "Schnee", "der", "Hei\u00b7math", "denk'", "ich", "eu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sanfte Buchten, blaue Meereslinie,", "tokens": ["Sanf\u00b7te", "Buch\u00b7ten", ",", "blau\u00b7e", "Mee\u00b7res\u00b7li\u00b7nie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Auf dem Abend dunkelnd Burggem\u00e4uer!", "tokens": ["Auf", "dem", "A\u00b7bend", "dun\u00b7kelnd", "Burg\u00b7ge\u00b7m\u00e4u\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aus der Schlucht erstrahlend Hirtenfeuer!", "tokens": ["Aus", "der", "Schlucht", "er\u00b7strah\u00b7lend", "Hir\u00b7ten\u00b7feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Lebet, Corsen, wohl, mir lieb geworden!", "tokens": ["Le\u00b7bet", ",", "Cor\u00b7sen", ",", "wohl", ",", "mir", "lieb", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "$,", "PPER", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vor den Kirchen l\u00fcpft ihr leicht die H\u00fcte!", "tokens": ["Vor", "den", "Kir\u00b7chen", "l\u00fcpft", "ihr", "leicht", "die", "H\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gerne knallt ihr und ein bischen Morden", "tokens": ["Ger\u00b7ne", "knallt", "ihr", "und", "ein", "bi\u00b7schen", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Steckt seit alter Zeit euch im Gebl\u00fcte \u2014", "tokens": ["Steckt", "seit", "al\u00b7ter", "Zeit", "euch", "im", "Ge\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df die heil'ge Jungfrau euch beh\u00fcte!", "tokens": ["Da\u00df", "die", "heil'\u00b7ge", "Jung\u00b7frau", "euch", "be\u00b7h\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Klimmend am Gestein des Insellandes", "tokens": ["Klim\u00b7mend", "am", "Ge\u00b7stein", "des", "In\u00b7sel\u00b7lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lebet wohl, ihr hitz'gen, kleinen Pferde!", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "hitz'\u00b7gen", ",", "klei\u00b7nen", "Pfer\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wallend um die Kr\u00fcmmungen des Strandes,", "tokens": ["Wal\u00b7lend", "um", "die", "Kr\u00fcm\u00b7mun\u00b7gen", "des", "Stran\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lebet, Schafe, wohl! Gedr\u00e4ngte Heerde", "tokens": ["Le\u00b7bet", ",", "Scha\u00b7fe", ",", "wohl", "!", "Ge\u00b7dr\u00e4ng\u00b7te", "Heer\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit den weichsten Vlie\u00dfen auf der Erde!", "tokens": ["Mit", "den", "weichs\u00b7ten", "Vlie\u00b7\u00dfen", "auf", "der", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Lebet wohl, ihr grellen Hirtenfl\u00f6ten,", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "grel\u00b7len", "Hir\u00b7ten\u00b7fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um die Gunst der jungen Corsin werbend!", "tokens": ["Um", "die", "Gunst", "der", "jun\u00b7gen", "Cor\u00b7sin", "wer\u00b7bend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lebet wohl, ihr warmen Abendr\u00f6ten,", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "war\u00b7men", "A\u00b7bend\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In den weiten Himmeln selig sterbend,", "tokens": ["In", "den", "wei\u00b7ten", "Him\u00b7meln", "se\u00b7lig", "ster\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Erst die Wolken, dann die Fluten f\u00e4rbend!", "tokens": ["Erst", "die", "Wol\u00b7ken", ",", "dann", "die", "Flu\u00b7ten", "f\u00e4r\u00b7bend", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "M\u00e4rchen, aus dem Tageslicht verschollen,", "tokens": ["M\u00e4r\u00b7chen", ",", "aus", "dem", "Ta\u00b7ges\u00b7licht", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "An Ajaccio's n\u00e4cht'ger Hafenstiege", "tokens": ["An", "A\u00b7jac\u00b7cio's", "n\u00e4cht'\u00b7ger", "Ha\u00b7fen\u00b7stie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Lebe wohl im dumpfen Wogenrollen!", "tokens": ["Le\u00b7be", "wohl", "im", "dum\u00b7pfen", "Wo\u00b7gen\u00b7rol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ehernes Gedr\u00f6hn der hundert Siege", "tokens": ["E\u00b7her\u00b7nes", "Ge\u00b7dr\u00f6hn", "der", "hun\u00b7dert", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "CARD", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Um des todten Welterob'rers Wiege!", "tokens": ["Um", "des", "tod\u00b7ten", "Welterob'\u00b7rers", "Wie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schwer entsagt das Aug der offnen Ferne,", "tokens": ["Schwer", "ent\u00b7sagt", "das", "Aug", "der", "off\u00b7nen", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwer das Ohr dem Meereswellenschlage \u2014", "tokens": ["Schwer", "das", "Ohr", "dem", "Mee\u00b7res\u00b7wel\u00b7len\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unter k\u00e4lt're Sonnen, blass're Sterne", "tokens": ["Un\u00b7ter", "k\u00e4lt'\u00b7re", "Son\u00b7nen", ",", "blass'\u00b7re", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Folget mir, ihr Inselwandertage,", "tokens": ["Fol\u00b7get", "mir", ",", "ihr", "In\u00b7sel\u00b7wan\u00b7der\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und umklingt mich dort, wie eine Sage ...", "tokens": ["Und", "um\u00b7klingt", "mich", "dort", ",", "wie", "ei\u00b7ne", "Sa\u00b7ge", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ART", "NN", "$("], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}}}}