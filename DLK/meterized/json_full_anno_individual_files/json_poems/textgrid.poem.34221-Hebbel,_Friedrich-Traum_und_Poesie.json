{"textgrid.poem.34221": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Traum und Poesie", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4ume und Dichtergebilde sind eng mit einander verschwistert,", "tokens": ["Tr\u00e4u\u00b7me", "und", "Dich\u00b7ter\u00b7ge\u00b7bil\u00b7de", "sind", "eng", "mit", "ein\u00b7an\u00b7der", "ver\u00b7schwis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "APPR", "PRF", "VVPP", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Beide l\u00f6sen sich ab oder erg\u00e4nzen sich still,", "tokens": ["Bei\u00b7de", "l\u00f6\u00b7sen", "sich", "ab", "o\u00b7der", "er\u00b7g\u00e4n\u00b7zen", "sich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber sie wurzeln nicht blo\u00df im tiefsten Bed\u00fcrfni\u00df der Seele,", "tokens": ["A\u00b7ber", "sie", "wur\u00b7zeln", "nicht", "blo\u00df", "im", "tiefs\u00b7ten", "Be\u00b7d\u00fcrf\u00b7ni\u00df", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nein, sie wurzeln zugleich in dem unendlichen All.", "tokens": ["Nein", ",", "sie", "wur\u00b7zeln", "zu\u00b7gleich", "in", "dem", "un\u00b7end\u00b7li\u00b7chen", "All", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "In die wirkliche Welt sind viele m\u00f6gliche and're", "tokens": ["In", "die", "wirk\u00b7li\u00b7che", "Welt", "sind", "vie\u00b7le", "m\u00f6g\u00b7li\u00b7che", "an\u00b7d'\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PIAT", "ADJA", "ADJA"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Eingesponnen, der Schlaf wickelt sie wieder heraus,", "tokens": ["Ein\u00b7ge\u00b7spon\u00b7nen", ",", "der", "Schlaf", "wi\u00b7ckelt", "sie", "wie\u00b7der", "he\u00b7raus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Sei es der dunkle der Nacht, der alle Menschen bew\u00e4ltigt,", "tokens": ["Sei", "es", "der", "dunk\u00b7le", "der", "Nacht", ",", "der", "al\u00b7le", "Men\u00b7schen", "be\u00b7w\u00e4l\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Sei es der helle des Tags, der nur den Dichter bef\u00e4llt,", "tokens": ["Sei", "es", "der", "hel\u00b7le", "des", "Tags", ",", "der", "nur", "den", "Dich\u00b7ter", "be\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "l