{"dta.poem.13619": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sommerschw\u00fcle .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O komm, Geliebte, komm' zu mir zur\u00fccke!", "tokens": ["O", "komm", ",", "Ge\u00b7lieb\u00b7te", ",", "komm'", "zu", "mir", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann ich nur Deine hellen Augen schauen,", "tokens": ["Kann", "ich", "nur", "Dei\u00b7ne", "hel\u00b7len", "Au\u00b7gen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fr\u00f6hlich Gestirn in dem verworr'nen Treiben:", "tokens": ["Fr\u00f6h\u00b7lich", "Ge\u00b7stirn", "in", "dem", "ver\u00b7worr'\u00b7nen", "Trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}