{"textgrid.poem.48957": {"metadata": {"author": {"name": "Kretschmann, Karl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Das offenbare Geheimni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonderlich und wunderbar!", "tokens": ["Son\u00b7der\u00b7lich", "und", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich verhehlte meine Schmerzen", "tokens": ["Ich", "ver\u00b7hehl\u00b7te", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief im liebewunden Herzen,", "tokens": ["Tief", "im", "lie\u00b7be\u00b7wun\u00b7den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4nger schon als Tag und Jahr:", "tokens": ["L\u00e4n\u00b7ger", "schon", "als", "Tag", "und", "Jahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir nur, Doris, meiner Lieben,", "tokens": ["Dir", "nur", ",", "Do\u00b7ris", ",", "mei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NE", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hab' ich sie bekannt, geschrieben,", "tokens": ["Hab'", "ich", "sie", "be\u00b7kannt", ",", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Allen andern barg ich sie.", "tokens": ["Al\u00b7len", "an\u00b7dern", "barg", "ich", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch die andern alle wissen's,", "tokens": ["Doch", "die", "an\u00b7dern", "al\u00b7le", "wis\u00b7sen's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Du nur, Doris, wei\u00dft es nie!", "tokens": ["Du", "nur", ",", "Do\u00b7ris", ",", "wei\u00dft", "es", "nie", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NE", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonderlich und wunderbar!", "tokens": ["Son\u00b7der\u00b7lich", "und", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich verhehlte meine Schmerzen", "tokens": ["Ich", "ver\u00b7hehl\u00b7te", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief im liebewunden Herzen,", "tokens": ["Tief", "im", "lie\u00b7be\u00b7wun\u00b7den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4nger schon als Tag und Jahr:", "tokens": ["L\u00e4n\u00b7ger", "schon", "als", "Tag",