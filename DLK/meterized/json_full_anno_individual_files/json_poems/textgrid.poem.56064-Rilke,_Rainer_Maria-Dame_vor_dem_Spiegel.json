{"textgrid.poem.56064": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Dame vor dem Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie in einem Schlaftrunk Spezerein", "tokens": ["Wie", "in", "ei\u00b7nem", "Schlaf\u00b7trunk", "Spe\u00b7zer\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "l\u00f6st sie leise in dem fl\u00fcssigklaren", "tokens": ["l\u00f6st", "sie", "lei\u00b7se", "in", "dem", "fl\u00fcs\u00b7sig\u00b7kla\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Spiegel ihr erm\u00fcdetes Gebaren;", "tokens": ["Spie\u00b7gel", "ihr", "er\u00b7m\u00fc\u00b7de\u00b7tes", "Ge\u00b7ba\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und sie tut ihr L\u00e4cheln ganz hinein.", "tokens": ["und", "sie", "tut", "ihr", "L\u00e4\u00b7cheln", "ganz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und sie wartet, da\u00df die Fl\u00fcssigkeit", "tokens": ["Und", "sie", "war\u00b7tet", ",", "da\u00df", "die", "Fl\u00fcs\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "davon steigt; dann gie\u00dft sie ihre Haare", "tokens": ["da\u00b7von", "steigt", ";", "dann", "gie\u00dft", "sie", "ih\u00b7re", "Haa\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "in den Spiegel und, die wunderbare", "tokens": ["in", "den", "Spie\u00b7gel", "und", ",", "die", "wun\u00b7der\u00b7ba\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "$,", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schulter hebend aus dem Abendkleid,", "tokens": ["Schul\u00b7ter", "he\u00b7bend", "aus", "dem", "A\u00b7bend\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "trinkt sie still aus ihrem Bild. Sie trinkt,", "tokens": ["trinkt", "sie", "still", "aus", "ih\u00b7rem", "Bild", ".", "Sie", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "was ein Liebender im Taumel tr\u00e4nke,", "tokens": ["was", "ein", "Lie\u00b7ben\u00b7der", "im", "Tau\u00b7mel", "tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "pr\u00fcfend, voller Mi\u00dftraun; und sie winkt", "tokens": ["pr\u00fc\u00b7fend", ",", "vol\u00b7ler", "Mi\u00df\u00b7traun", ";", "und", "sie", "winkt"], "token_info": ["word", "punct", "word", "word", "punct", "word"