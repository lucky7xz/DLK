{"dta.poem.13900": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "V .  \n  Die Heymonskinder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun setzt der Tonk\u00fcnstler, skandirt der Poet,", "tokens": ["Nun", "setzt", "der", "Ton\u00b7k\u00fcnst\u00b7ler", ",", "skan\u00b7dirt", "der", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Der S\u00e4nger giebt himmlischen Schall,", "tokens": ["Der", "S\u00e4n\u00b7ger", "giebt", "himm\u00b7li\u00b7schen", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es l\u00e4chelt Herr Bachus: \u201ewahrhaftig das geht,", "tokens": ["Es", "l\u00e4\u00b7chelt", "Herr", "Ba\u00b7chus", ":", "\u201e", "wahr\u00b7haf\u00b7tig", "das", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NE", "$.", "$(", "ADJD", "PDS", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und's Trinken verstehen sie all'.\u201c", "tokens": ["Un\u00b7d's", "Trin\u00b7ken", "ver\u00b7ste\u00b7hen", "sie", "all'", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PIS", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}