{"textgrid.poem.36481": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3. Die zwei W\u00f6lfe, Vater und Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das S\u00f6hnchen eines Wolfs zerri\u00df ein armes Lamm.", "tokens": ["Das", "S\u00f6hn\u00b7chen", "ei\u00b7nes", "Wolfs", "zer\u00b7ri\u00df", "ein", "ar\u00b7mes", "Lamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als nun der Vater Wolf von einem Zweikampf kam,", "tokens": ["Als", "nun", "der", "Va\u00b7ter", "Wolf", "von", "ei\u00b7nem", "Zwei\u00b7kampf", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und seinen Sohn, den Held, das Lamm zerrei\u00dfen sah,", "tokens": ["Und", "sei\u00b7nen", "Sohn", ",", "den", "Held", ",", "das", "Lamm", "zer\u00b7rei\u00b7\u00dfen", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seiner Heldenthat der Sohn sich r\u00fchmte, da,", "tokens": ["Und", "sei\u00b7ner", "Hel\u00b7den\u00b7that", "der", "Sohn", "sich", "r\u00fchm\u00b7te", ",", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "PRF", "VVFIN", "$,", "KOUS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da sprach der Vater: Narr! weil keine L\u00e4mmer bei\u00dfen,", "tokens": ["Da", "sprach", "der", "Va\u00b7ter", ":", "Narr", "!", "weil", "kei\u00b7ne", "L\u00e4m\u00b7mer", "bei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NE", "$.", "KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kann man sie ja wohl zerrei\u00dfen!", "tokens": ["So", "kann", "man", "sie", "ja", "wohl", "zer\u00b7rei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das S\u00f6hnchen eines Wolfs zerri\u00df ein armes Lamm.", "tokens": ["Das", "S\u00f6hn\u00b7chen", "ei\u00b7nes", "Wolfs", "zer\u00b7ri\u00df", "ein", "ar\u00b7mes", "Lamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als nun der Vater Wolf von einem Zweikampf kam,", "tokens": ["Als", "nun", "der", "Va\u00b7ter", "Wolf", "von", "ei\u00b7nem", "Zwei\u00b7kampf", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und seinen Sohn, den Held, das Lamm zerrei\u00dfen sah,", "tokens": ["Und", "sei\u00b7nen", "Sohn", ",", "den", "Held", ",", "das", "Lamm", "zer\u00b7rei\u00b7\u00dfen", "sah", ","], "tok