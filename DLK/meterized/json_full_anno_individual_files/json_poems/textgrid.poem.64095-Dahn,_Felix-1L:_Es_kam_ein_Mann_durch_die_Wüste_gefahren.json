{"textgrid.poem.64095": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es kam ein Mann durch die W\u00fcste gefahren", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam ein Mann durch die W\u00fcste gefahren", "tokens": ["Es", "kam", "ein", "Mann", "durch", "die", "W\u00fcs\u00b7te", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit drei\u00dfig beladenen Dromedaren.", "tokens": ["Mit", "drei\u00b7\u00dfig", "be\u00b7la\u00b7de\u00b7nen", "Dro\u00b7me\u00b7da\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die trugen Sch\u00e4tze viel hundert Lasten \u2013", "tokens": ["Die", "tru\u00b7gen", "Sch\u00e4t\u00b7ze", "viel", "hun\u00b7dert", "Las\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "CARD", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unter den Zedern wollten sie rasten.", "tokens": ["Un\u00b7ter", "den", "Ze\u00b7dern", "woll\u00b7ten", "sie", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Da, auf schnaubenden Rossen, mit Pfeil und Bogen", "tokens": ["Da", ",", "auf", "schnau\u00b7ben\u00b7den", "Ros\u00b7sen", ",", "mit", "Pfeil", "und", "Bo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Kamen die S\u00f6hne der W\u00fcste geflogen.", "tokens": ["Ka\u00b7men", "die", "S\u00f6h\u00b7ne", "der", "W\u00fcs\u00b7te", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nahmen das Gut und schleiften den Mann", "tokens": ["Und", "nah\u00b7men", "das", "Gut", "und", "schleif\u00b7ten", "den", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu ihres F\u00fcrsten Zelt hindann.", "tokens": ["Zu", "ih\u00b7res", "F\u00fcrs\u00b7ten", "Zelt", "hin\u00b7dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der kam geschritten br\u00e4unlich sch\u00f6n,", "tokens": ["Der", "kam", "ge\u00b7schrit\u00b7ten", "br\u00e4un\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie der L\u00f6we schreitet auf Karmels H\u00f6h'n.", "tokens": ["Wie", "der", "L\u00f6\u00b7we", "schrei\u00b7tet", "auf", "Kar\u00b7mels", "H\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "ART", "NE", "VVFIN", "APPR", "NE", "NN", "NE"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbo schone mein Leben, nimm L\u00f6segeld,", "tokens": ["\u00bb", "o", "scho\u00b7ne", "mein", "Le\u00b7ben", ",", "nimm", "L\u00f6\u00b7se\u00b7geld", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPOSAT", "NN", "$,", "VVIMP", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich f\u00fclle mit Gold dir das ganze Zelt.", "tokens": ["Ich", "f\u00fcl\u00b7le", "mit", "Gold", "dir", "das", "gan\u00b7ze", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Denn Gott gab Segen meinem Stamm \u2013:", "tokens": ["Denn", "Gott", "gab", "Se\u00b7gen", "mei\u00b7nem", "Stamm", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "PPOSAT", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin Isak, der Sohn des Abraham.\u00ab", "tokens": ["Ich", "bin", "I\u00b7sak", ",", "der", "Sohn", "des", "Ab\u00b7ra\u00b7ham", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "ART", "NN", "ART", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Da ri\u00df aus der Scheide der Emir das Schwert:", "tokens": ["Da", "ri\u00df", "aus", "der", "Schei\u00b7de", "der", "E\u00b7mir", "das", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbdank den G\u00f6ttern der Rache, die dich mir gew\u00e4hrt.", "tokens": ["\u00bb", "dank", "den", "G\u00f6t\u00b7tern", "der", "Ra\u00b7che", ",", "die", "dich", "mir", "ge\u00b7w\u00e4hrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Lang fahnd' ich nach dir, lang such' ich dich schon:", "tokens": ["Lang", "fahnd'", "ich", "nach", "dir", ",", "lang", "such'", "ich", "dich", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "$,", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn ich bin Ismael, Hagars Sohn.", "tokens": ["Denn", "ich", "bin", "Is\u00b7mael", ",", "Ha\u00b7gars", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NE", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "In die W\u00fcste, zum Futter der Geier und Raben,", "tokens": ["In", "die", "W\u00fcs\u00b7te", ",", "zum", "Fut\u00b7ter", "der", "Gei\u00b7er", "und", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "So wollt' es ja Sarah, die Treffliche, haben \u2013", "tokens": ["So", "wollt'", "es", "ja", "Sa\u00b7rah", ",", "die", "Treff\u00b7li\u00b7che", ",", "ha\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NE", "$,", "ART", "NN", "$,", "VAFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "In die W\u00fcste verstie\u00df er das Weib und den Knaben,", "tokens": ["In", "die", "W\u00fcs\u00b7te", "ver\u00b7stie\u00df", "er", "das", "Weib", "und", "den", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und Jehova vergalt mit Verhei\u00dfungsgaben!", "tokens": ["Und", "Je\u00b7ho\u00b7va", "ver\u00b7galt", "mit", "Ver\u00b7hei\u00b7\u00dfungs\u00b7ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.12": {"line.1": {"text": "Doch die Palme der W\u00fcste war gn\u00e4d'ger als Gott:", "tokens": ["Doch", "die", "Pal\u00b7me", "der", "W\u00fcs\u00b7te", "war", "gn\u00e4d'\u00b7ger", "als", "Gott", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Die Versto\u00dfenen leben, Jehova zum Spott.", "tokens": ["Die", "Ver\u00b7sto\u00b7\u00dfe\u00b7nen", "le\u00b7ben", ",", "Je\u00b7ho\u00b7va", "zum", "Spott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "NE", "APPRART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.13": {"line.1": {"text": "La\u00df sehn, ob er jetzt dich entrei\u00dft dem Verderben,", "tokens": ["La\u00df", "sehn", ",", "ob", "er", "jetzt", "dich", "ent\u00b7rei\u00dft", "dem", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gottseliges Br\u00fcderlein, du mu\u00dft sterben.\u00ab", "tokens": ["Gott\u00b7se\u00b7li\u00b7ges", "Br\u00fc\u00b7derl\u00b7ein", ",", "du", "mu\u00dft", "ster\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Da hob von den Polstern ein hehres Weib", "tokens": ["Da", "hob", "von", "den", "Pols\u00b7tern", "ein", "heh\u00b7res", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den immer noch k\u00f6niglich sch\u00f6nen Leib.", "tokens": ["Den", "im\u00b7mer", "noch", "k\u00f6\u00b7nig\u00b7lich", "sch\u00f6\u00b7nen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.15": {"line.1": {"text": "Sie zerdr\u00fcckt eine Tr\u00e4ne von Stolz und Harm", "tokens": ["Sie", "zer\u00b7dr\u00fcckt", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "von", "Stolz", "und", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und r\u00fchrt an des Helden erhobenen Arm.", "tokens": ["Und", "r\u00fchrt", "an", "des", "Hel\u00b7den", "er\u00b7ho\u00b7be\u00b7nen", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.16": {"line.1": {"text": "\u00bbo K\u00f6nig der W\u00fcste, du mein Juwel,", "tokens": ["\u00bb", "o", "K\u00f6\u00b7nig", "der", "W\u00fcs\u00b7te", ",", "du", "mein", "Ju\u00b7wel", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "ART", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein L\u00f6we, mein Adler, mein Ismael.", "tokens": ["Mein", "L\u00f6\u00b7we", ",", "mein", "Ad\u00b7ler", ",", "mein", "Is\u00b7mael", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.17": {"line.1": {"text": "Ich bitte zum Dank f\u00fcr ein ganzes Leben:", "tokens": ["Ich", "bit\u00b7te", "zum", "Dank", "f\u00fcr", "ein", "gan\u00b7zes", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mir sollst du den Sohn der Sarah geben.\u00ab", "tokens": ["Mir", "sollst", "du", "den", "Sohn", "der", "Sa\u00b7rah", "ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Und er neigte das Haupt und das Schwert dazu", "tokens": ["Und", "er", "neig\u00b7te", "das", "Haupt", "und", "das", "Schwert", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "PAV"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und k\u00fc\u00dfte im Staub seiner Mutter Schuh.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "im", "Staub", "sei\u00b7ner", "Mut\u00b7ter", "Schuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.19": {"line.1": {"text": "\u00bbsag' Abraham,\u00ab sprach sie zu jenem gewandt,", "tokens": ["\u00bb", "sag'", "Ab\u00b7ra\u00b7ham", ",", "\u00ab", "sprach", "sie", "zu", "je\u00b7nem", "ge\u00b7wandt", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NE", "$,", "$(", "VVFIN", "PPER", "APPR", "PDAT", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u00bbhagar hat mich dir zur\u00fcckgesandt.\u00ab", "tokens": ["\u00bb", "ha\u00b7gar", "hat", "mich", "dir", "zu\u00b7r\u00fcck\u00b7ge\u00b7sandt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.20": {"line.1": {"text": "Es kam ein Mann durch die W\u00fcste gefahren", "tokens": ["Es", "kam", "ein", "Mann", "durch", "die", "W\u00fcs\u00b7te", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit drei\u00dfig beladenen Dromedaren.", "