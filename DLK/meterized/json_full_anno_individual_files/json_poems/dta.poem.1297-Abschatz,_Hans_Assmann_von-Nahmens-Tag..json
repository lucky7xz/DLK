{"dta.poem.1297": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nahmens-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der vielgew\u00fcnschte Tag/ Johann/ ist nun vorhanden/", "tokens": ["Der", "viel\u00b7ge\u00b7w\u00fcnschte", "Tag", "/", "Jo\u00b7hann", "/", "ist", "nun", "vor\u00b7han\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NE", "$(", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Sonne tritt in Krebs/ nachdem ihr goldnes Rad", "tokens": ["Die", "Son\u00b7ne", "tritt", "in", "Krebs", "/", "nach\u00b7dem", "ihr", "gold\u00b7nes", "Rad"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$(", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den h\u00f6chsten Himmels-Stand bereit erlanget hat/", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Him\u00b7mels\u00b7Stand", "be\u00b7reit", "er\u00b7lan\u00b7get", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Kummer-reiches Jahr ist wieder \u00fcberstanden/", "tokens": ["Ein", "Kum\u00b7mer\u00b7rei\u00b7ches", "Jahr", "ist", "wie\u00b7der", "\u00fc\u00b7bers\u00b7tan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir leben allerseits bedr\u00e4ngt in unsern Landen/", "tokens": ["Wir", "le\u00b7ben", "al\u00b7ler\u00b7seits", "be\u00b7dr\u00e4ngt", "in", "un\u00b7sern", "Lan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bedr\u00e4ngter in uns selbst/ durch manches Thr\u00e4nen-Bad/", "tokens": ["Be\u00b7dr\u00e4ng\u00b7ter", "in", "uns", "selbst", "/", "durch", "man\u00b7ches", "Thr\u00e4\u00b7nen\u00b7Bad", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$(", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch \u00fcber Seel und Leib geh\u00e4uffte Frevel-That;", "tokens": ["Durch", "\u00fc\u00b7ber", "Seel", "und", "Leib", "ge\u00b7h\u00e4uff\u00b7te", "Fre\u00b7vel\u00b7That", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erl\u00f6\u00df uns/ grosser GOTT/ von dieses Todes Banden.", "tokens": ["Er\u00b7l\u00f6\u00df", "uns", "/", "gros\u00b7ser", "GoTT", "/", "von", "die\u00b7ses", "To\u00b7des", "Ban\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADJA", "NN", "$(", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vergieb die Schuld/ und gieb/ was unser Hertz erquicke;", "tokens": ["Ver\u00b7gieb", "die", "Schuld", "/", "und", "gieb", "/", "was", "un\u00b7ser", "Hertz", "er\u00b7qui\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$(", "KON", "VVIMP", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "La\u00df unser gantzes Land in vollem Segen stehn/", "tokens": ["La\u00df", "un\u00b7ser", "gant\u00b7zes", "Land", "in", "vol\u00b7lem", "Se\u00b7gen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Damit der Feinde List den Krebs-Gang m\u00f6ge gehn/", "tokens": ["Da\u00b7mit", "der", "Fein\u00b7de", "List", "den", "Krebs\u00b7Gang", "m\u00f6\u00b7ge", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NE", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und unsre Waffen selbst dein starcker Arm begl\u00fccke/", "tokens": ["Und", "uns\u00b7re", "Waf\u00b7fen", "selbst", "dein", "star\u00b7cker", "Arm", "be\u00b7gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "La\u00df deines Dieners Hau\u00df dir unentfallen seyn/", "tokens": ["La\u00df", "dei\u00b7nes", "Die\u00b7ners", "Hau\u00df", "dir", "un\u00b7ent\u00b7fal\u00b7len", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und f\u00fchr uns/ wenn du wilt/ in Himmel selig ein.", "tokens": ["Und", "f\u00fchr", "uns", "/", "wenn", "du", "wilt", "/", "in", "Him\u00b7mel", "se\u00b7lig", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VMFIN", "$(", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}