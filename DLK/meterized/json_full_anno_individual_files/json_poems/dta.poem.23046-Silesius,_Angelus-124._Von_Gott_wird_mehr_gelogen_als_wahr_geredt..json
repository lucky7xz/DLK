{"dta.poem.23046": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "124. Von Gott wird mehr gelogen als  \n wahr geredt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wa\u00df du von Gott ver jahst/ dasselb ist mehr erlogen/", "tokens": ["Wa\u00df", "du", "von", "Gott", "ver", "jahst", "/", "das\u00b7selb", "ist", "mehr", "er\u00b7lo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "NE", "ADV", "$(", "PAV", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wahr: weil du Jhn nur nach dem gesch\u00f6pff erwoge\u0303.", "tokens": ["Als", "wahr", ":", "weil", "du", "Jhn", "nur", "nach", "dem", "ge\u00b7sch\u00f6pff", "er\u00b7wo\u00b7g\u1ebd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$.", "KOUS", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}