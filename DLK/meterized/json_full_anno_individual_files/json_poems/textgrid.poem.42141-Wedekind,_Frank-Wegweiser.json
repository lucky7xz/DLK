{"textgrid.poem.42141": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Wegweiser", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Wassertrinker bin ich nicht geboren,", "tokens": ["Zum", "Was\u00b7ser\u00b7trin\u00b7ker", "bin", "ich", "nicht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das kann euch meine edle Muse zeigen;", "tokens": ["Das", "kann", "euch", "mei\u00b7ne", "ed\u00b7le", "Mu\u00b7se", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie singt beim Wein und f\u00e4llt in tiefes Schweigen,", "tokens": ["Sie", "singt", "beim", "Wein", "und", "f\u00e4llt", "in", "tie\u00b7fes", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sich der letzte Schluck im Bauch verloren.", "tokens": ["Wenn", "sich", "der", "letz\u00b7te", "Schluck", "im", "Bauch", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Wasser hab ich ew'gen Ha\u00df geschworen,", "tokens": ["Dem", "Was\u00b7ser", "hab", "ich", "ew'\u00b7gen", "Ha\u00df", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil ihm der Zauberd\u00fcnste keiner eigen,", "tokens": ["Weil", "ihm", "der", "Zau\u00b7ber\u00b7d\u00fcns\u00b7te", "kei\u00b7ner", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die traumschwer aus dem dunklen Becher steigen,", "tokens": ["Die", "traum\u00b7schwer", "aus", "dem", "dunk\u00b7len", "Be\u00b7cher", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den ich zum Weiser mir des Wegs erkoren.", "tokens": ["Den", "ich", "zum", "Wei\u00b7ser", "mir", "des", "Wegs", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er ist ein gar verst\u00e4ndiger Geselle,", "tokens": ["Er", "ist", "ein", "gar", "ver\u00b7st\u00e4n\u00b7di\u00b7ger", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er dr\u00e4ngt direkt mich zu des Tempels Schwelle", "tokens": ["Er", "dr\u00e4ngt", "di\u00b7rekt", "mich", "zu", "des", "Tem\u00b7pels", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00f6ffnet meinem Blick die dunklen T\u00fcren.", "tokens": ["Und", "\u00f6ff\u00b7net", "mei\u00b7nem", "Blick", "die", "dunk\u00b7len", "T\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Taumel tapp ich nach der heiligen Zelle", "tokens": ["Im", "Tau\u00b7mel", "tapp", "ich", "nach", "der", "hei\u00b7li\u00b7gen", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und mu\u00df des Ortes Weihe nur versp\u00fcren,", "tokens": ["Und", "mu\u00df", "des", "Or\u00b7tes", "Wei\u00b7he", "nur", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann ist's kein Kunstst\u00fcck mehr, mich zu verf\u00fchren.", "tokens": ["Dann", "ist's", "kein", "Kunst\u00b7st\u00fcck", "mehr", ",", "mich", "zu", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zum Wassertrinker bin ich nicht geboren,", "tokens": ["Zum", "Was\u00b7ser\u00b7trin\u00b7ker", "bin", "ich", "nicht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PTKNEG",