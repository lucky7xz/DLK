{"dta.poem.8498": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie steh'n vor deinem Bild und schauen", "tokens": ["Sie", "steh'n", "vor", "dei\u00b7nem", "Bild", "und", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dein verschleiert Augenlicht,", "tokens": ["In", "dein", "ver\u00b7schlei\u00b7ert", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie pr\u00fcfen Lippe, Kinn und Brauen", "tokens": ["Sie", "pr\u00fc\u00b7fen", "Lip\u00b7pe", ",", "Kinn", "und", "Brau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagen dann: du sei\u2019st es nicht;", "tokens": ["Und", "sa\u00b7gen", "dann", ":", "du", "sei'st", "es", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu klar die Stirn, zu voll die Wange,", "tokens": ["Zu", "klar", "die", "Stirn", ",", "zu", "voll", "die", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "$,", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu \u00fcppig in der Locken Hange,", "tokens": ["Zu", "\u00fcp\u00b7pig", "in", "der", "Lo\u00b7cken", "Han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein lieblich, fremdes Angesicht.", "tokens": ["Ein", "lieb\u00b7lich", ",", "frem\u00b7des", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O w\u00fc\u00dften sie es, wie ein treues", "tokens": ["O", "w\u00fc\u00df\u00b7ten", "sie", "es", ",", "wie", "ein", "treu\u00b7es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "$,", "PWAV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gem\u00fcth die kleinsten Z\u00fcge hegt!", "tokens": ["Ge\u00b7m\u00fcth", "die", "kleins\u00b7ten", "Z\u00fc\u00b7ge", "hegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Zucken schon ein fl\u00fccht\u2019ges, scheues,", "tokens": ["Ein", "Zu\u00b7cken", "schon", "ein", "fl\u00fccht'\u00b7ges", ",", "scheu\u00b7es", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Kleinod in die Seele legt.", "tokens": ["Als", "Klei\u00b7nod", "in", "die", "See\u00b7le", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie schon ein Wort von gleichem Klange", "tokens": ["Wie", "schon", "ein", "Wort", "von", "glei\u00b7chem", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gehaucht, dem Feinde selbst, das bange,", "tokens": ["Ge\u00b7haucht", ",", "dem", "Fein\u00b7de", "selbst", ",", "das", "ban\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bewegte Herz entgegen tr\u00e4gt.", "tokens": ["Be\u00b7weg\u00b7te", "Herz", "ent\u00b7ge\u00b7gen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie w\u00fcrden besser mich begreifen,", "tokens": ["Sie", "w\u00fcr\u00b7den", "bes\u00b7ser", "mich", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seh\u2019n deiner Locken dunkeln Haag", "tokens": ["Seh'n", "dei\u00b7ner", "Lo\u00b7cken", "dun\u00b7keln", "Haag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie mich mit leisem Finger streifen,", "tokens": ["Sie", "mich", "mit", "lei\u00b7sem", "Fin\u00b7ger", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als l\u00fcft\u2019 ich sie dem jungen Tag;", "tokens": ["Als", "l\u00fcft'", "ich", "sie", "dem", "jun\u00b7gen", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Flor mich breiten dicht und dichter,", "tokens": ["Den", "Flor", "mich", "brei\u00b7ten", "dicht", "und", "dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df deiner Augen zarte Lichter", "tokens": ["Da\u00df", "dei\u00b7ner", "Au\u00b7gen", "zar\u00b7te", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Sonnenstrahl verletzen mag.", "tokens": ["Kein", "Son\u00b7nen\u00b7strahl", "ver\u00b7let\u00b7zen", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PIAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was fremd, dahin will ich nicht schauen", "tokens": ["Was", "fremd", ",", "da\u00b7hin", "will", "ich", "nicht", "schau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "$,", "PAV", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und will nicht wissen, wo sie brennt,", "tokens": ["Und", "will", "nicht", "wis\u00b7sen", ",", "wo", "sie", "brennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob an der Lipp\u2019, der Wang\u2019, den Brauen,", "tokens": ["Ob", "an", "der", "Lipp'", ",", "der", "Wang'", ",", "den", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flamme, die dein Herz nicht kennt.", "tokens": ["Die", "Flam\u00b7me", ",", "die", "dein", "Herz", "nicht", "kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will nur seh\u2019n in deine Augen,", "tokens": ["Ich", "will", "nur", "seh'n", "in", "dei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den einen frommen Blick nur saugen,", "tokens": ["Den", "ei\u00b7nen", "from\u00b7men", "Blick", "nur", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der leise meinen Namen nennt.", "tokens": ["Der", "lei\u00b7se", "mei\u00b7nen", "Na\u00b7men", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihn, der wie Mondlicht mich umflossen,", "tokens": ["Ihn", ",", "der", "wie", "Mond\u00b7licht", "mich", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "KOKOM", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als in der ernsten Abendzeit", "tokens": ["Als", "in", "der", "erns\u00b7ten", "A\u00b7bend\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sa\u00dfen, Hand in Hand geschlossen,", "tokens": ["Wir", "sa\u00b7\u00dfen", ",", "Hand", "in", "Hand", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dachten Tod und Ewigkeit.", "tokens": ["Und", "dach\u00b7ten", "Tod", "und", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn, der sich von der Sonne Schwinden", "tokens": ["Ihn", ",", "der", "sich", "von", "der", "Son\u00b7ne", "Schwin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Heilig gewendet, mich zu finden,", "tokens": ["Hei\u00b7lig", "ge\u00b7wen\u00b7det", ",", "mich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und l\u00e4chelnd sprach: ich bin bereit!", "tokens": ["Und", "l\u00e4\u00b7chelnd", "sprach", ":", "ich", "bin", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}