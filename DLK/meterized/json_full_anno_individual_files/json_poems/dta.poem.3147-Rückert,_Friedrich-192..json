{"dta.poem.3147": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "192.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Pflanze hat das Jahr zum Leben das sie lebt,", "tokens": ["Die", "Pflan\u00b7ze", "hat", "das", "Jahr", "zum", "Le\u00b7ben", "das", "sie", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sie der Fr\u00fchling weckt, der Winter sie begr\u00e4bt.", "tokens": ["Wo", "sie", "der", "Fr\u00fch\u00b7ling", "weckt", ",", "der", "Win\u00b7ter", "sie", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr Spro\u00dfen und ihr Bl\u00fchn, Vergehn und Neuentstammen", "tokens": ["Ihr", "Spro\u00b7\u00dfen", "und", "ihr", "Bl\u00fchn", ",", "Ver\u00b7gehn", "und", "Neu\u00b7ent\u00b7stam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00e4llt mit des Jahres Kreis unwandelbar zusammen.", "tokens": ["F\u00e4llt", "mit", "des", "Jah\u00b7res", "Kreis", "un\u00b7wan\u00b7del\u00b7bar", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jung ist sie, wenn die Welt ist jung, und alt, wenn alt,", "tokens": ["Jung", "ist", "sie", ",", "wenn", "die", "Welt", "ist", "jung", ",", "und", "alt", ",", "wenn", "alt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Gro\u00dfen kleines Bild in wandelnder Gestalt.", "tokens": ["Des", "Gro\u00b7\u00dfen", "klei\u00b7nes", "Bild", "in", "wan\u00b7deln\u00b7der", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Des Menschen Leben ist nicht solch ein Kreis geschlossen,", "tokens": ["Des", "Men\u00b7schen", "Le\u00b7ben", "ist", "nicht", "solch", "ein", "Kreis", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PTKNEG", "PIAT", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dem Naturumlauf zusammen so geflossen.", "tokens": ["Mit", "dem", "Na\u00b7tur\u00b7um\u00b7lauf", "zu\u00b7sam\u00b7men", "so", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es lenzet, sommert zwar, es herbstet, wintert auch,", "tokens": ["Es", "len\u00b7zet", ",", "som\u00b7mert", "zwar", ",", "es", "herbs\u00b7tet", ",", "win\u00b7tert", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,", "PPER", "VVPP", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht aber mit dem Jahr, nicht mit der L\u00fcfte Hauch.", "tokens": ["Nicht", "a\u00b7ber", "mit", "dem", "Jahr", ",", "nicht", "mit", "der", "L\u00fcf\u00b7te", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es setzt sich davon unabh\u00e4ngig seine Grenzen,", "tokens": ["Es", "setzt", "sich", "da\u00b7von", "un\u00b7ab\u00b7h\u00e4n\u00b7gig", "sei\u00b7ne", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vermag, ob wintern mag die Sch\u00f6pfung, noch zu lenzen.", "tokens": ["Ver\u00b7mag", ",", "ob", "win\u00b7tern", "mag", "die", "Sch\u00f6p\u00b7fung", ",", "noch", "zu", "len\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "VVINF", "VMFIN", "ART", "NN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und legt es einmal sich zum Winterschlummer nieder,", "tokens": ["Und", "legt", "es", "ein\u00b7mal", "sich", "zum", "Win\u00b7ter\u00b7schlum\u00b7mer", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So weckt kein Fr\u00fchlingshauch auf dieser Welt es wieder.", "tokens": ["So", "weckt", "kein", "Fr\u00fch\u00b7lings\u00b7hauch", "auf", "die\u00b7ser", "Welt", "es", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PDAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}