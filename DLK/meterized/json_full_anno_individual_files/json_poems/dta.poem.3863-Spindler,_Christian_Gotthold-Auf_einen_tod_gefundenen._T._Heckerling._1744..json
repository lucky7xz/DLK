{"dta.poem.3863": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen tod gefundenen.  \n  T. Heckerling.  1744.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ich suchte meinen Bissen Brod,", "tokens": ["Ich", "such\u00b7te", "mei\u00b7nen", "Bis\u00b7sen", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man fande/ mich erstarrt und todt,", "tokens": ["Man", "fan\u00b7de", "/", "mich", "er\u00b7starrt", "und", "todt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da man es nicht vermeinet h\u00e4tte;", "tokens": ["Da", "man", "es", "nicht", "ver\u00b7mei\u00b7net", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Strasse war mein Sterbe-Bette.", "tokens": ["Die", "Stras\u00b7se", "war", "mein", "Ster\u00b7be\u00b7Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, Leser! dencke allezeit:", "tokens": ["Drum", ",", "Le\u00b7ser", "!", "den\u00b7cke", "al\u00b7le\u00b7zeit", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vielleicht ist auch mein Tod nicht weit.", "tokens": ["Viel\u00b7leicht", "ist", "auch", "mein", "Tod", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}