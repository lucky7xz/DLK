{"dta.poem.16738": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "97.  Der Heilgen Dauben n\u00e4st.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du ein D\u00e4ublein bist/ und keine Galle hast;", "tokens": ["Wenn", "du", "ein", "D\u00e4ub\u00b7lein", "bist", "/", "und", "kei\u00b7ne", "Gal\u00b7le", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$(", "KON", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So findestu mein Christ im Hertzen JEsu rast.", "tokens": ["So", "fin\u00b7des\u00b7tu", "mein", "Christ", "im", "Hert\u00b7zen", "Je\u00b7su", "rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}