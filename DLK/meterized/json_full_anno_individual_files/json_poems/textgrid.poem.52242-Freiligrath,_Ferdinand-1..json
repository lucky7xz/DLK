{"textgrid.poem.52242": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprach irgendwo in Deutschland eine Tanne:", "tokens": ["Sprach", "ir\u00b7gend\u00b7wo", "in", "Deutschland", "ei\u00b7ne", "Tan\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NE", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbo, k\u00f6nnt' ich hoch als deutscher Kriegsmast ragen!", "tokens": ["\u00bb", "o", ",", "k\u00f6nnt'", "ich", "hoch", "als", "deut\u00b7scher", "Kriegs\u00b7mast", "ra\u00b7gen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "VMFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O, k\u00f6nnt' ich stolz die junge Flagge tragen", "tokens": ["O", ",", "k\u00f6nnt'", "ich", "stolz", "die", "jun\u00b7ge", "Flag\u00b7ge", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des ein'gen Deutschlands in der Nordsee Banne!", "tokens": ["Des", "ein'\u00b7gen", "Deutschlands", "in", "der", "Nord\u00b7see", "Ban\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann w\u00e4r' ich F\u00e4hnrich, ha! wo Mann an Manne", "tokens": ["Dann", "w\u00e4r'", "ich", "F\u00e4hn\u00b7rich", ",", "ha", "!", "wo", "Mann", "an", "Man\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ITJ", "$.", "PWAV", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blutr\u00fcnst'ge Krieger deutsche Seeschlacht schlagen;", "tokens": ["Blut\u00b7r\u00fcnst'\u00b7ge", "Krie\u00b7ger", "deut\u00b7sche", "See\u00b7schlacht", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Wo deutsche Segler, grimm und ohne Zagen,", "tokens": ["Wo", "deut\u00b7sche", "Seg\u00b7ler", ",", "grimm", "und", "oh\u00b7ne", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den fremden Entrer hauen in die Pfanne!", "tokens": ["Den", "frem\u00b7den", "Ent\u00b7rer", "hau\u00b7en", "in", "die", "Pfan\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann leuchtet wohl, die Brust vom Stahl gekerbt,", "tokens": ["Dann", "leuch\u00b7tet", "wohl", ",", "die", "Brust", "vom", "Stahl", "ge\u00b7kerbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Held an mir in des Gefechtes Gluten,", "tokens": ["Ein", "Held", "an", "mir", "in", "des", "Ge\u00b7fech\u00b7