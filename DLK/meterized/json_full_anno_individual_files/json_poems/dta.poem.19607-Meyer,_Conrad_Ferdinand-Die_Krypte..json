{"dta.poem.19607": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Krypte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Baut junge Meister, bauet hell und weit", "tokens": ["Baut", "jun\u00b7ge", "Meis\u00b7ter", ",", "bau\u00b7et", "hell", "und", "weit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Macht, dem Muth, der That, der Gunst der Stunde,", "tokens": ["Der", "Macht", ",", "dem", "Muth", ",", "der", "That", ",", "der", "Gunst", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Dinge wahr und tief gesch\u00f6pfter Kunde,", "tokens": ["Der", "Din\u00b7ge", "wahr", "und", "tief", "ge\u00b7sch\u00f6pf\u00b7ter", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem ganzen Genienkreis der neuen Zeit!", "tokens": ["Dem", "gan\u00b7zen", "Ge\u00b7ni\u00b7en\u00b7kreis", "der", "neu\u00b7en", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Des Lebens unersch\u00f6pften Kr\u00e4ften weiht", "tokens": ["Des", "Le\u00b7bens", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Kr\u00e4f\u00b7ten", "weiht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die freud'ge, lichtdurchfluthete Rotunde \u2014", "tokens": ["Die", "freu\u00b7d'\u00b7ge", ",", "licht\u00b7durch\u00b7flut\u00b7he\u00b7te", "Ro\u00b7tun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Baut auch die Krypte drunter, wo das wunde", "tokens": ["Baut", "auch", "die", "Kryp\u00b7te", "drun\u00b7ter", ",", "wo", "das", "wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "$,", "PWAV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gem\u00fcth sich fl\u00fcchten darf in Einsamkeit:", "tokens": ["Ge\u00b7m\u00fcth", "sich", "fl\u00fcch\u00b7ten", "darf", "in", "Ein\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verge\u00dft die Krypte nicht! Dort soll sich neigen", "tokens": ["Ver\u00b7ge\u00dft", "die", "Kryp\u00b7te", "nicht", "!", "Dort", "soll", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$.", "ADV", "VMFIN", "PRF", "VVFIN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Das heil'ge Haupt, das Dornen scharf umwinden!", "tokens": ["Das", "heil'\u00b7ge", "Haupt", ",", "das", "Dor\u00b7nen", "scharf", "um\u00b7win\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich glaube: Ein'ge werden niedersteigen.", "tokens": ["Ich", "glau\u00b7be", ":", "Ein'\u00b7ge", "wer\u00b7den", "nie\u00b7ders\u00b7tei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort unten werden Ein'ge Trost empfinden.", "tokens": ["Dort", "un\u00b7ten", "wer\u00b7den", "Ein'\u00b7ge", "Trost", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir m\u00f6gen, wenn die Leiden uns umnachten,", "tokens": ["Wir", "m\u00f6\u00b7gen", ",", "wenn", "die", "Lei\u00b7den", "uns", "um\u00b7nach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Gl\u00fcck noch Ruhm, nur gr\u00f6\u00dfern Schmerz betrachten.", "tokens": ["Nicht", "Gl\u00fcck", "noch", "Ruhm", ",", "nur", "gr\u00f6\u00b7\u00dfern", "Schmerz", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "$,", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}