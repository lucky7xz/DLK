{"textgrid.poem.48905": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Lied eines Lappl\u00e4nders", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm Zama, komm! La\u00df deinen Unmuth fahren,", "tokens": ["Komm", "Za\u00b7ma", ",", "komm", "!", "La\u00df", "dei\u00b7nen", "Un\u00b7muth", "fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "$.", "VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O du der Preis", "tokens": ["O", "du", "der", "Preis"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Sch\u00f6nen! komm! In den zerst\u00f6rten Haaren", "tokens": ["Der", "Sch\u00f6\u00b7nen", "!", "komm", "!", "In", "den", "zer\u00b7st\u00f6r\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4ngt mir schon Eis.", "tokens": ["H\u00e4ngt", "mir", "schon", "Eis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Du z\u00fcrnst umsonst. Mir giebt die Liebe Fl\u00fcgel,", "tokens": ["Du", "z\u00fcrnst", "um\u00b7sonst", ".", "Mir", "giebt", "die", "Lie\u00b7be", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-