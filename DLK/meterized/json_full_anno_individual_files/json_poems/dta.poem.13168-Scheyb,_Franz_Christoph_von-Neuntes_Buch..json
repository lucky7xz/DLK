{"dta.poem.13168": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht in Geschmuck und Gold, nicht in der Stoffen Sch\u00e4zen", "tokens": ["Nicht", "in", "Ge\u00b7schmuck", "und", "Gold", ",", "nicht", "in", "der", "Stof\u00b7fen", "Sch\u00e4\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "340Wollt unsers Augs Begier, der Vorwiz sich erg\u00f6zen.", "tokens": ["un\u00b7sers", "Augs", "Be\u00b7gier", ",", "der", "Vor\u00b7wiz", "sich", "er\u00b7g\u00f6\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die\u00df alles hatte man kaum anzusehn im Sinn,", "tokens": ["Die\u00df", "al\u00b7les", "hat\u00b7te", "man", "kaum", "an\u00b7zu\u00b7sehn", "im", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PIS", "ADV", "VVIZU", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es lief ein jeder Blick nur nach der K\u00f6niginn.", "tokens": ["Es", "lief", "ein", "je\u00b7der", "Blick", "nur", "nach", "der", "K\u00f6\u00b7ni\u00b7ginn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was man durch diese Nacht mit tausend Lobes-Spr\u00fcchen", "tokens": ["Was", "man", "durch", "die\u00b7se", "Nacht", "mit", "tau\u00b7send", "Lo\u00b7bes\u00b7Spr\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "APPR", "PDAT", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von ihrer Majest\u00e4t und Tugend vorgestrichen,", "tokens": ["Von", "ih\u00b7rer", "Ma\u00b7jes\u00b7t\u00e4t", "und", "Tu\u00b7gend", "vor\u00b7ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "345Entz\u00fcckte nun das Herz mit so verborgner Macht,", "tokens": ["nun", "das", "Herz", "mit", "so", "ver\u00b7borg\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df es Erstaunens-voll nicht wu\u00dfte, was es dacht.", "tokens": ["Da\u00df", "es", "Er\u00b7stau\u00b7nens\u00b7voll", "nicht", "wu\u00df\u00b7te", ",", "was", "es", "dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gleich kannte man in ihr das Urbild jener Gaben,", "tokens": ["Gleich", "kann\u00b7te", "man", "in", "ihr", "das", "Ur\u00b7bild", "je\u00b7ner", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wovon die Tugenden so viel gesprochen haben.", "tokens": ["Wo\u00b7von", "die", "Tu\u00b7gen\u00b7den", "so", "viel", "ge\u00b7spro\u00b7chen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}