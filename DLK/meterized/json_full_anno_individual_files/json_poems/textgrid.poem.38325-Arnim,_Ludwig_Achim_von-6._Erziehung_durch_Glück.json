{"textgrid.poem.38325": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "6. Erziehung durch Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Gott, du bist, wie mans begehrt,", "tokens": ["Ach", "Gott", ",", "du", "bist", ",", "wie", "mans", "be\u00b7gehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VAFIN", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist uns, was wir wollen;", "tokens": ["Du", "bist", "uns", ",", "was", "wir", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist ganz gut und ganz verkehrt,", "tokens": ["Du", "bist", "ganz", "gut", "und", "ganz", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lied kommt aus dir gequollen", "tokens": ["Lied", "kommt", "aus", "dir", "ge\u00b7quol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Heil f\u00fcr den, der dies verlangt,", "tokens": ["Und", "Heil", "f\u00fcr", "den", ",", "der", "dies", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "$,", "PRELS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text"