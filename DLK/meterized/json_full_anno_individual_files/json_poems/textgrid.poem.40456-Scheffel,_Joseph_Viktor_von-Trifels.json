{"textgrid.poem.40456": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Trifels", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch schwellt kein Gr\u00fcn der Buchen Kronen,", "tokens": ["Noch", "schwellt", "kein", "Gr\u00fcn", "der", "Bu\u00b7chen", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch singt die Drossel schon vom Ast", "tokens": ["Doch", "singt", "die", "Dros\u00b7sel", "schon", "vom", "Ast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit dem Wei\u00df der Anemonen", "tokens": ["Und", "mit", "dem", "Wei\u00df", "der", "A\u00b7ne\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mischt sich der Primel gelber Glast;", "tokens": ["Mischt", "sich", "der", "Pri\u00b7mel", "gel\u00b7ber", "Glast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Annweilers Berge seh' ich wieder", "tokens": ["Ann\u00b7wei\u00b7lers", "Ber\u00b7ge", "seh'", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihre Burgdreifaltigkeit,", "tokens": ["Und", "ih\u00b7re", "Burg\u00b7drei\u00b7fal\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Ehren alt, vernarbt und bieder,", "tokens": ["In", "Eh\u00b7ren", "alt", ",", "ver\u00b7narbt", "und", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kriegszeugen deutscher Kaiserzeit.", "tokens": ["Kriegs\u00b7zeu\u00b7gen", "deut\u00b7scher", "Kai\u00b7ser\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort ", "tokens": ["Dort"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Vor ihr der Felsklotz ", "tokens": ["Vor", "ihr", "der", "Fels\u00b7klotz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und hier als dritter im Vereine", "tokens": ["Und", "hier", "als", "drit\u00b7ter", "im", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "ADJA", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Reichspfalz ", "tokens": ["Der", "Reich\u00b7spfalz"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ihr Turm mit der Kapelle Erker,", "tokens": ["Ihr", "Turm", "mit", "der", "Ka\u00b7pel\u00b7le", "Er\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der einst die Reichskleinodien barg,", "tokens": ["Der", "einst", "die", "Reichs\u00b7klein\u00b7o\u00b7di\u00b7en", "barg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Des L\u00f6wenherzen Richard Kerker", "tokens": ["Des", "L\u00f6\u00b7wen\u00b7her\u00b7zen", "Ric\u00b7hard", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00e4chst m\u00e4chtig aus des Felsens Mark.", "tokens": ["W\u00e4chst", "m\u00e4ch\u00b7tig", "aus", "des", "Fel\u00b7sens", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo einst in zierem Pfauentritt", "tokens": ["Wo", "einst", "in", "zie\u00b7rem", "Pfau\u00b7en\u00b7tritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Harfenschall und Minneweisen", "tokens": ["Bei", "Har\u00b7fen\u00b7schall", "und", "Min\u00b7ne\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Kaiserhofes Reigen schritt.", "tokens": ["Des", "Kai\u00b7ser\u00b7ho\u00b7fes", "Rei\u00b7gen", "schritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ahi! wie sah man T\u00fccher winken,", "tokens": ["A\u00b7hi", "!", "wie", "sah", "man", "T\u00fc\u00b7cher", "win\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als hier am zw\u00f6lften Maientag", "tokens": ["Als", "hier", "am", "zw\u00f6lf\u00b7ten", "Mai\u00b7en\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei vieler tausend Helme Blinken", "tokens": ["Bei", "vie\u00b7ler", "tau\u00b7send", "Hel\u00b7me", "Blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "CARD", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der sechste ", "tokens": ["Der", "sechs\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Im ernsten Auge spr\u00fcht' ein Feuer,", "tokens": ["Im", "erns\u00b7ten", "Au\u00b7ge", "spr\u00fcht'", "ein", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als klirre schon der Speere Krach:", "tokens": ["Als", "klir\u00b7re", "schon", "der", "Spee\u00b7re", "Krach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.4": {"text": "Bald r\u00e4chen wir Salernos Schmach;", "tokens": ["Bald", "r\u00e4\u00b7chen", "wir", "Sa\u00b7ler\u00b7nos", "Schmach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eh' sich die W\u00e4lder herbstlich f\u00e4rben,", "tokens": ["Eh'", "sich", "die", "W\u00e4l\u00b7der", "herbst\u00b7lich", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die heute diese Fahnen sehn,", "tokens": ["Die", "heu\u00b7te", "die\u00b7se", "Fah\u00b7nen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Soll siegreich uns und unsern Erben", "tokens": ["Soll", "sieg\u00b7reich", "uns", "und", "un\u00b7sern", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Reichspanier am \u00c4tna wehn!\u00ab", "tokens": ["Das", "Reich\u00b7spa\u00b7nier", "am", "\u00c4t\u00b7na", "wehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Als ihres Kaisers Heergeleite", "tokens": ["Als", "ih\u00b7res", "Kai\u00b7sers", "Heer\u00b7ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ritt eine stolze F\u00fcrstenschaft", "tokens": ["Ritt", "ei\u00b7ne", "stol\u00b7ze", "F\u00fcrs\u00b7ten\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seinem Bruder treu zur Seite", "tokens": ["Und", "sei\u00b7nem", "Bru\u00b7der", "treu", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch zog des Rotbarts blondem Kinde", "tokens": ["Noch", "zog", "des", "Rot\u00b7barts", "blon\u00b7dem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Fr\u00fchlingsahnen durch den Sinn,", "tokens": ["Kein", "Fr\u00fch\u00b7lin\u00b7gsah\u00b7nen", "durch", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er die Braut ", "tokens": ["Da\u00df", "er", "die", "Braut"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Als dieser Maifahrt Beut'gewinn.", "tokens": ["Als", "die\u00b7ser", "Mai\u00b7fahrt", "Beut'\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gleich einer ehernen Schlange wanden", "tokens": ["Gleich", "ei\u00b7ner", "e\u00b7her\u00b7nen", "Schlan\u00b7ge", "wan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Helme sich den Wald hindurch", "tokens": ["Die", "Hel\u00b7me", "sich", "den", "Wald", "hin\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Heerdrommeter sandten", "tokens": ["Und", "al\u00b7le", "Heer\u00b7drom\u00b7me\u00b7ter", "sand\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Abschiedsgru\u00df das Lied zur Burg:", "tokens": ["Als", "Ab\u00b7schieds\u00b7gru\u00df", "das", "Lied", "zur", "Burg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbihr frische Rosen, sanfte Lilien,", "tokens": ["\u00bb", "ihr", "fri\u00b7sche", "Ro\u00b7sen", ",", "sanf\u00b7te", "Li\u00b7li\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Lebt wohl und bl\u00fcht in Gottes Hut;", "tokens": ["Lebt", "wohl", "und", "bl\u00fcht", "in", "Got\u00b7tes", "Hut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Adlers Flug geht nach Sizilien,", "tokens": ["Des", "Ad\u00b7lers", "Flug", "geht", "nach", "Si\u00b7zi\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihn d\u00fcrstet nach Normannenblut!\u00ab", "tokens": ["Ihn", "d\u00fcrs\u00b7tet", "nach", "Nor\u00b7man\u00b7nen\u00b7blut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Wer wei\u00df noch von den Rittern allen", "tokens": ["Wer", "wei\u00df", "noch", "von", "den", "Rit\u