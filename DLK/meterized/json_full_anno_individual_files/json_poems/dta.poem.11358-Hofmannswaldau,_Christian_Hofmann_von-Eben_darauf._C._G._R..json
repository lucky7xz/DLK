{"dta.poem.11358": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eben darauf.  \n C. G. R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sind, gelehrter mann! vier winter fast begraben,", "tokens": ["Es", "sind", ",", "ge\u00b7lehr\u00b7ter", "mann", "!", "vier", "win\u00b7ter", "fast", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "$.", "CARD", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seit dem ich dich gekannt, geh\u00f6rt und hochgesch\u00e4tzt:", "tokens": ["Seit", "dem", "ich", "dich", "ge\u00b7kannt", ",", "ge\u00b7h\u00f6rt", "und", "hoch\u00b7ge\u00b7sch\u00e4tzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darum ich offt gew\u00fcnscht, gelegenheit zu haben,", "tokens": ["Da\u00b7rum", "ich", "offt", "ge\u00b7w\u00fcnscht", ",", "ge\u00b7le\u00b7gen\u00b7heit", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVPP", "$,", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu zeigen, was ein kiel verbundner diener setzt;", "tokens": ["Zu", "zei\u00b7gen", ",", "was", "ein", "kiel", "ver\u00b7bund\u00b7ner", "die\u00b7ner", "setzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat sich aber nichts dergleichen f\u00fcgen wollen,", "tokens": ["Es", "hat", "sich", "a\u00b7ber", "nichts", "derg\u00b7lei\u00b7chen", "f\u00fc\u00b7gen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "PIS", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bi\u00df deines kindes tod mich itzt zum tichter macht.", "tokens": ["Bi\u00df", "dei\u00b7nes", "kin\u00b7des", "tod", "mich", "itzt", "zum", "tich\u00b7ter", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach! h\u00e4tt ich doch ein lied von freude schreiben sollen!", "tokens": ["Ach", "!", "h\u00e4tt", "ich", "doch", "ein", "lied", "von", "freu\u00b7de", "schrei\u00b7ben", "sol\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach! w\u00fcste doch dein tag von keiner trauer-nacht!", "tokens": ["Ach", "!", "w\u00fcs\u00b7te", "doch", "dein", "tag", "von", "kei\u00b7ner", "trau\u00b7e\u00b7rnacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein kind, das liebste kind, die zarte Leonore", "tokens": ["Dein", "kind", ",", "das", "liebs\u00b7te", "kind", ",", "die", "zar\u00b7te", "Le\u00b7o\u00b7no\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vergeht als wie ein licht, das kaum entglommen ist;", "tokens": ["Ver\u00b7geht", "als", "wie", "ein", "licht", ",", "das", "kaum", "ent\u00b7glom\u00b7men", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "KOKOM", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Drum deckt dein antlitz sich mit einem duncklen flore,", "tokens": ["Drum", "deckt", "dein", "ant\u00b7litz", "sich", "mit", "ei\u00b7nem", "dunck\u00b7len", "flo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In deinem zimmer wird nur ach! und boy erkiest.", "tokens": ["In", "dei\u00b7nem", "zim\u00b7mer", "wird", "nur", "ach", "!", "und", "boy", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$.", "KON", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man h\u00f6ret nun nicht mehr das angeuehme lallen,", "tokens": ["Man", "h\u00f6\u00b7ret", "nun", "nicht", "mehr", "das", "an\u00b7ge\u00b7u\u00b7eh\u00b7me", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Damit ihr kleiner mund der eltern hertz erfreut:", "tokens": ["Da\u00b7mit", "ihr", "klei\u00b7ner", "mund", "der", "el\u00b7tern", "hertz", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jhr unschuld-voller schertz, der allen wohl gefallen,", "tokens": ["Ihr", "un\u00b7schuld\u00b7vol\u00b7ler", "schertz", ",", "der", "al\u00b7len", "wohl", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und viel verdru\u00df vers\u00fc\u00dft, kehrt sich in herbes leid.", "tokens": ["Und", "viel", "ver\u00b7dru\u00df", "ver\u00b7s\u00fc\u00dft", ",", "kehrt", "sich", "in", "her\u00b7bes", "leid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,", "VVFIN", "PRF", "APPR", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der muntre Friedrich sucht die schwester zwar zu wecken;", "tokens": ["Der", "mun\u00b7tre", "Fried\u00b7rich", "sucht", "die", "schwes\u00b7ter", "zwar", "zu", "we\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ART", "ADJA", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Allein sie schl\u00e4ft den schlaf, den niemand st\u00f6hren kan;", "tokens": ["Al\u00b7lein", "sie", "schl\u00e4ft", "den", "schlaf", ",", "den", "nie\u00b7mand", "st\u00f6h\u00b7ren", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Drum steht er gantz best\u00fcrtzt, es will ihm nichts mehr schmecken,", "tokens": ["Drum", "steht", "er", "gantz", "be\u00b7st\u00fcrtzt", ",", "es", "will", "ihm", "nichts", "mehr", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVPP", "$,", "PPER", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und giebt mit tr\u00fcbem ach! sein liebstes spielwerck an.", "tokens": ["Und", "giebt", "mit", "tr\u00fc\u00b7bem", "ach", "!", "sein", "liebs\u00b7tes", "spiel\u00b7werck", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "ITJ", "$.", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hoch-werthester patron! es schiessen die cypressen", "tokens": ["Hoch\u00b7wert\u00b7hes\u00b7ter", "pat\u00b7ron", "!", "es", "schies\u00b7sen", "die", "cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Dir selber allzu fr\u00fch in deinem garten auf.", "tokens": ["Dir", "sel\u00b7ber", "all\u00b7zu", "fr\u00fch", "in", "dei\u00b7nem", "gar\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der hoffnungs-bau verf\u00e4llt, man kan es leicht ermessen,", "tokens": ["Der", "hoff\u00b7nungs\u00b7bau", "ver\u00b7f\u00e4llt", ",", "man", "kan", "es", "leicht", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PIS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dein sch\u00f6ner vorsatz hemmt den angefangnen lauff.", "tokens": ["Dein", "sch\u00f6\u00b7ner", "vor\u00b7satz", "hemmt", "den", "an\u00b7ge\u00b7fang\u00b7nen", "lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der himmel hatte dich zwey kinder k\u00fcssen lassen,", "tokens": ["Der", "him\u00b7mel", "hat\u00b7te", "dich", "zwey", "kin\u00b7der", "k\u00fcs\u00b7sen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "CARD", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es lie\u00df uns die vernunfft was seltnes propheceyn:", "tokens": ["Es", "lie\u00df", "uns", "die", "ver\u00b7nunfft", "was", "selt\u00b7nes", "pro\u00b7phe\u00b7ceyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PWS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die tochter wolt in sich der mutter tugend fassen,", "tokens": ["Die", "toch\u00b7ter", "wolt", "in", "sich", "der", "mut\u00b7ter", "tu\u00b7gend", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der sohn ein contrefait des klugen vaters seyn;", "tokens": ["Der", "sohn", "ein", "con\u00b7tre\u00b7fait", "des", "klu\u00b7gen", "va\u00b7ters", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Allein wie irren doch die allerbesten schl\u00fcsse,", "tokens": ["Al\u00b7lein", "wie", "ir\u00b7ren", "doch", "die", "al\u00b7ler\u00b7bes\u00b7ten", "schl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn GOtt uns einen strich in unsre rechnung macht!", "tokens": ["Wenn", "Gott", "uns", "ei\u00b7nen", "strich", "in", "uns\u00b7re", "rech\u00b7nung", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dein vorsatz war gerecht, die hoffnung zucker-s\u00fcsse;", "tokens": ["Dein", "vor\u00b7satz", "war", "ge\u00b7recht", ",", "die", "hoff\u00b7nung", "zu\u00b7cke\u00b7rs\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der heyland aber hat ein ander ziel erdacht.", "tokens": ["Der", "hey\u00b7land", "a\u00b7ber", "hat", "ein", "an\u00b7der", "ziel", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Eleonor erbleicht, die weichen glieder starren,", "tokens": ["E\u00b7leo\u00b7nor", "er\u00b7bleicht", ",", "die", "wei\u00b7chen", "glie\u00b7der", "star\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Die augen sincken hin, ihr blick verliehret sich,", "tokens": ["Die", "au\u00b7gen", "sin\u00b7cken", "hin", ",", "ihr", "blick", "ver\u00b7lieh\u00b7ret", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Jhr geist darff l\u00e4nger nicht in dieser welt verharren,", "tokens": ["Ihr", "geist", "darff", "l\u00e4n\u00b7ger", "nicht", "in", "die\u00b7ser", "welt", "ver\u00b7har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und dieses scheiden ist vor dich ein hertzens-stich.", "tokens": ["Und", "die\u00b7ses", "schei\u00b7den", "ist", "vor", "dich", "ein", "hert\u00b7zens\u00b7stich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "VAFIN", "APPR", "PRF", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Gott aber meint es gut, ja besser, als wir dencken,", "tokens": ["Gott", "a\u00b7ber", "meint", "es", "gut", ",", "ja", "bes\u00b7ser", ",", "als", "wir", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ob seine wege gleich verborgne r\u00e4thsel sind.", "tokens": ["Ob", "sei\u00b7ne", "we\u00b7ge", "gleich", "ver\u00b7borg\u00b7ne", "r\u00e4th\u00b7sel", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wer wei\u00df, ob uns die grufft, die itzt von thr\u00e4nen-tr\u00e4ncken", "tokens": ["Wer", "wei\u00df", ",", "ob", "uns", "die", "grufft", ",", "die", "itzt", "von", "thr\u00e4\u00b7nen\u00b7tr\u00e4n\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und todten-wasser quillt, nicht trost und leben rinnt.", "tokens": ["Und", "tod\u00b7ten\u00b7was\u00b7ser", "quillt", ",", "nicht", "trost", "und", "le\u00b7ben", "rinnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PTKNEG", "ADJD", "KON", 