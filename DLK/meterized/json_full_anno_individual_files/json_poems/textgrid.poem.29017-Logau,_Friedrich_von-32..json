{"textgrid.poem.29017": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein K\u00fcnstler war nechst hier, der suff nur Wasser ein,", "tokens": ["Ein", "K\u00fcnst\u00b7ler", "war", "nechst", "hier", ",", "der", "suff", "nur", "Was\u00b7ser", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "PRELS", "ADV", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gab wieder doch herau\u00df gebrant- und rothen Wein", "tokens": ["Gab", "wie\u00b7der", "doch", "her\u00b7au\u00df", "ge\u00b7brant", "und", "ro\u00b7then", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Wasser von Anis, von Ziemet und Violen,", "tokens": ["Und", "Was\u00b7ser", "von", "A\u00b7nis", ",", "von", "Zie\u00b7met", "und", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von Rosen, andrem mehr, gantz frey und unverholen.", "tokens": ["Von", "Ro\u00b7sen", ",", "an\u00b7drem", "mehr", ",", "gantz", "frey", "und", "un\u00b7ver\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PIS", "ADV", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nat\u00fcrlich war es nur; es war nicht Zauberey,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "war", "es", "nur", ";", "es", "war", "nicht", "Zau\u00b7be\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es blieb doch Wasser nur; List, Kunst war blo\u00df dabey.", "tokens": ["Es", "blieb", "doch", "Was\u00b7ser", "nur", ";", "List", ",", "Kunst", "war", "blo\u00df", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV", "$.", "NE", "$,", "NN", "VAFIN", "ADV", "PAV", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Also sind derer mehr, die zwar die Warheit nennen,", "tokens": ["Al\u00b7so", "sind", "de\u00b7rer", "mehr", ",", "die", "zwar", "die", "War\u00b7heit", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ADV", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Befinden und verstehn, gar selten doch bekennen,", "tokens": ["Be\u00b7fin\u00b7den", "und", "ver\u00b7stehn", ",", "gar", "sel\u00b7ten", "doch", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,", "ADV", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo was Verlust dabey. Um Nutz, um Ehr, um Gunst", "tokens": ["Wo", "was", "Ver\u00b7lust", "da\u00b7bey", ".", "Um", "Nutz", ",", "um", "