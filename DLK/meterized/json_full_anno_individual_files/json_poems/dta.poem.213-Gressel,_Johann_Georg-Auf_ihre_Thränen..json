{"dta.poem.213": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre Thr\u00e4nen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Thr\u00e4nen/ Sch\u00f6nste/ sind ein s\u00fcsser Tranck den Seelen/", "tokens": ["Die", "Thr\u00e4\u00b7nen", "/", "Sch\u00f6ns\u00b7te", "/", "sind", "ein", "s\u00fcs\u00b7ser", "Tranck", "den", "See\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Davon ein Tropffen gleich berauscht und truncken", "tokens": ["Da\u00b7von", "ein", "Tropf\u00b7fen", "gleich", "be\u00b7rauscht", "und", "trun\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADV", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "macht/", "tokens": ["macht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Tyrannen werden weich/ sie h\u00f6ren auf zu qu\u00e4hlen/", "tokens": ["Ty\u00b7ran\u00b7nen", "wer\u00b7den", "weich", "/", "sie", "h\u00f6\u00b7ren", "auf", "zu", "qu\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$(", "PPER", "VVFIN", "APPR", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So bald ein Opffer wird der Thr\u00e4nen dargebracht.", "tokens": ["So", "bald", "ein", "Opf\u00b7fer", "wird", "der", "Thr\u00e4\u00b7nen", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Araspes muste gleich die Panthea erw\u00e4hlen/", "tokens": ["Ar\u00b7as\u00b7pes", "mus\u00b7te", "gleich", "die", "Pan\u00b7thea", "er\u00b7w\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+----+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Als ihre Thr\u00e4nen ihm gezeigt der Sch\u00f6nheits-Pracht.", "tokens": ["Als", "ih\u00b7re", "Thr\u00e4\u00b7nen", "ihm", "ge\u00b7zeigt", "der", "Sch\u00f6n\u00b7heits\u00b7Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Seelen k\u00f6nnen sich dennoch mit Lust verm\u00e4hlen", "tokens": ["Die", "See\u00b7len", "k\u00f6n\u00b7nen", "sich", "den\u00b7noch", "mit", "Lust", "ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ob schon das Auge weint/ wenn nur das Hertze lacht.", "tokens": ["Ob", "schon", "das", "Au\u00b7ge", "weint", "/", "wenn", "nur", "das", "Hert\u00b7ze", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KOUS", "ADV", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aus deinen Augen l\u00e4\u00dft du Liebes-Z\u00e4hren fliessen/", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", "l\u00e4\u00dft", "du", "Lie\u00b7bes\u00b7Z\u00e4h\u00b7ren", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Damit du meine Seel in der Betr\u00fcbni\u00df nehrst/", "tokens": ["Da\u00b7mit", "du", "mei\u00b7ne", "Seel", "in", "der", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "nehrst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich werde noch mehr Lust aus dieser Fluht geniessen/", "tokens": ["Ich", "wer\u00b7de", "noch", "mehr", "Lust", "aus", "die\u00b7ser", "Fluht", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn deiner Sch\u00f6nheit-Zier du durch dieselben mehrst;", "tokens": ["Wenn", "dei\u00b7ner", "Sch\u00f6n\u00b7heit\u00b7Zier", "du", "durch", "die\u00b7sel\u00b7ben", "mehrst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "PDAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ach la\u00df nur/ sch\u00f6nstes Kind/ verliebte Thr\u00e4nen rinnen/", "tokens": ["Ach", "la\u00df", "nur", "/", "sch\u00f6ns\u00b7tes", "Kind", "/", "ver\u00b7lieb\u00b7te", "Thr\u00e4\u00b7nen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "ADV", "$(", "ADJA", "NN", "$(", "VVFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein Hertze solt du bald durch eine Z\u00e4hr\u2019 gewinnen.", "tokens": ["Mein", "Hert\u00b7ze", "solt", "du", "bald", "durch", "ei\u00b7ne", "Z\u00e4hr'", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}