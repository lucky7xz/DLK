{"textgrid.poem.60096": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht lobensw\u00fcrdig ist der Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht lobensw\u00fcrdig ist der Mann,", "tokens": ["Nicht", "lo\u00b7bens\u00b7w\u00fcr\u00b7dig", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch mir des Neides werth,", "tokens": ["Noch", "mir", "des", "Nei\u00b7des", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der nur mit prunkendem Gespann", "tokens": ["Der", "nur", "mit", "prun\u00b7ken\u00b7dem", "Ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um seine G\u00e4rten f\u00e4hrt;", "tokens": ["Um", "sei\u00b7ne", "G\u00e4r\u00b7ten", "f\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An jedem Baum vor\u00fcber zieht,", "tokens": ["An", "je\u00b7dem", "Baum", "vor\u00b7\u00fc\u00b7ber", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4r es sein Pallast \u2013", "tokens": ["Als", "w\u00e4r", "es", "sein", "Pal\u00b7last", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So stolz und kalt \u2013 nicht aufw\u00e4rts sieht", "tokens": ["So", "stolz", "und", "kalt", "\u2013", "nicht", "auf\u00b7w\u00e4rts", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$(", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum fruchtbeladnen Ast;", "tokens": ["Zum", "frucht\u00b7be\u00b7lad\u00b7nen", "Ast", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Schoo\u00df der Buhlen, o Natur,", "tokens": ["Im", "Schoo\u00df", "der", "Buh\u00b7len", ",", "o", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich ohne Lust erblickt;", "tokens": ["Dich", "oh\u00b7ne", "Lust", "er\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu deinem Mutter-Feste nur", "tokens": ["Zu", "dei\u00b7nem", "Mut\u00b7ter\u00b7Fes\u00b7te", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tagel\u00f6hner schickt.", "tokens": ["Die", "Ta\u00b7ge\u00b7l\u00f6h\u00b7ner", "schickt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dagegen halt' ich neidenswerth,", "tokens": ["Da\u00b7ge\u00b7gen", "halt'", "ich", "nei\u00b7dens\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lobe mir den Mann,", "tokens": ["Und", "lo\u00b7be", "mir", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sich von seinen Fr\u00fcchten n\u00e4hrt,", "tokens": ["Der", "sich", "von", "sei\u00b7nen", "Fr\u00fcch\u00b7ten", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und de\u00df sich freuen kann;", "tokens": ["Und", "de\u00df", "sich", "freu\u00b7en", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der unter seinen Blumen wohnt;", "tokens": ["Der", "un\u00b7ter", "sei\u00b7nen", "Blu\u00b7men", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie anzuschauen ging,", "tokens": ["Sie", "an\u00b7zu\u00b7schau\u00b7en", "ging", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bevor der Zweig im Fr\u00fchlingsmond", "tokens": ["Be\u00b7vor", "der", "Zweig", "im", "Fr\u00fch\u00b7lings\u00b7mond"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die erste Bl\u00fcth' empfing;", "tokens": ["Die", "ers\u00b7te", "Bl\u00fcth'", "emp\u00b7fing", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bey Regen und bey Sonnenstrahl", "tokens": ["Bey", "Re\u00b7gen", "und", "bey", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in bereifter Nacht,", "tokens": ["Und", "in", "be\u00b7reif\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Liebes-Sorge jedes Mahl", "tokens": ["Mit", "Lie\u00b7bes\u00b7Sor\u00b7ge", "je\u00b7des", "Mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An seine B\u00e4ume dacht',", "tokens": ["An", "sei\u00b7ne", "B\u00e4u\u00b7me", "dacht'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und so die Fr\u00fcchte wachsen sah,", "tokens": ["Und", "so", "die", "Fr\u00fcch\u00b7te", "wach\u00b7sen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von s\u00fc\u00dfer Hoffnung voll;", "tokens": ["Von", "s\u00fc\u00b7\u00dfer", "Hoff\u00b7nung", "voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nun, der reichen Ernte nah,", "tokens": ["Und", "nun", ",", "der", "rei\u00b7chen", "Ern\u00b7te", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie alle brechen soll.", "tokens": ["Sie", "al\u00b7le", "bre\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihn preis' ich, der die B\u00e4ume gro\u00df", "tokens": ["Ihn", "preis'", "ich", ",", "der", "die", "B\u00e4u\u00b7me", "gro\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebethet und gepflegt;", "tokens": ["Ge\u00b7be\u00b7thet", "und", "ge\u00b7pflegt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Birn mit Lachen in den Schoo\u00df", "tokens": ["Die", "Birn", "mit", "La\u00b7chen", "in", "den", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des treuen Weibes legt.", "tokens": ["Des", "treu\u00b7en", "Wei\u00b7bes", "legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihn preis' ich, wenn um seinen Baum", "tokens": ["Ihn", "preis'", "ich", ",", "wenn", "um", "sei\u00b7nen", "Baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein H\u00e4ufchen Kinder singt,", "tokens": ["Ein", "H\u00e4uf\u00b7chen", "Kin\u00b7der", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Backen frisch und roth, da\u00df kaum", "tokens": ["Mit", "Ba\u00b7cken", "frisch", "und", "roth", ",", "da\u00df", "kaum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Apfel r\u00f6ther blinkt.", "tokens": ["Der", "Ap\u00b7fel", "r\u00f6\u00b7ther", "blinkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da lehnt an seine Garten-Th\u00fcr", "tokens": ["Da", "lehnt", "an", "sei\u00b7ne", "Gar\u00b7ten\u00b7T\u00b7h\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wittwe sich, und blickt", "tokens": ["Die", "Witt\u00b7we", "sich", ",", "und", "blickt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PRF", "$,", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aufs arme Waislein neben ihr,", "tokens": ["Aufs", "ar\u00b7me", "Wais\u00b7lein", "ne\u00b7ben", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem keiner Fr\u00fcchte pfl\u00fcckt.", "tokens": ["Dem", "kei\u00b7ner", "Fr\u00fcch\u00b7te", "pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Weil er die Wittwe tr\u00f6sten kann", "tokens": ["Weil", "er", "die", "Witt\u00b7we", "tr\u00f6s\u00b7ten", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem, was Gott beschert,", "tokens": ["Mit", "dem", ",", "was", "Gott", "be\u00b7schert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Deswegen lob' ich mir den Mann,", "tokens": ["Des\u00b7we\u00b7gen", "lob'", "ich", "mir", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und halt' ihn neidenswerth.", "tokens": ["Und", "halt'", "ihn", "nei\u00b7dens\u00b7werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Nicht lobensw\u00fcrdig ist der Mann,", "tokens": ["Nicht", "lo\u00b7bens\u00b7w\u00fcr\u00b7dig", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch mir des Neides werth,", "tokens": ["Noch", "mir", "des", "Nei\u00b7des", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der nur mit prunkendem Gespann", "tokens": ["Der", "nur", "mit", "prun\u00b7ken\u00b7dem", "Ge\u00b7spann"], "token_info": ["word", "word", "wo