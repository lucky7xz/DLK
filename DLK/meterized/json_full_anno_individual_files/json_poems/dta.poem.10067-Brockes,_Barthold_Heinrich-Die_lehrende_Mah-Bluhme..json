{"dta.poem.10067": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrende Mah-Bluhme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ich gestern tausend Bluhmen von dem allersch\u00f6n-\nsten Mah", "tokens": ["Wo", "ich", "ge\u00b7stern", "tau\u00b7send", "Bluh\u00b7men", "von", "dem", "al\u00b7ler\u00b7sch\u00f6n", "sten", "Mah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "CARD", "NN", "APPR", "ART", "TRUNC", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "In den reinsten Farben bl\u00fchen,", "tokens": ["In", "den", "reins\u00b7ten", "Far\u00b7ben", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja im bunten Feuer gl\u00fchen,", "tokens": ["Ja", "im", "bun\u00b7ten", "Feu\u00b7er", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4ntzen, prangen, funckeln sah;", "tokens": ["Gl\u00e4nt\u00b7zen", ",", "pran\u00b7gen", ",", "fun\u00b7ckeln", "sah", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eben da", "tokens": ["E\u00b7ben", "da"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Seh ich nichts als ihre K\u00f6pfe,", "tokens": ["Seh", "ich", "nichts", "als", "ih\u00b7re", "K\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von der bunten Pracht beraubt,", "tokens": ["Von", "der", "bun\u00b7ten", "Pracht", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Von den Bl\u00e4ttern gantz entlaubt,", "tokens": ["Von", "den", "Bl\u00e4t\u00b7tern", "gantz", "ent\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Als so viele Todten-T\u00f6pfe.", "tokens": ["Als", "so", "vie\u00b7le", "Tod\u00b7ten\u00b7T\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fast durch nichts wird auf der Welt", "tokens": ["Fast", "durch", "nichts", "wird", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIS", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Dinge dieser Zeit", "tokens": ["Al\u00b7ler", "Din\u00b7ge", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00fcchtige Beschaffenheit", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ge", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So nat\u00fcrlich vorgestellt.", "tokens": ["So", "na\u00b7t\u00fcr\u00b7lich", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber in dem fl\u00fcchtgen Wesen", "tokens": ["A\u00b7ber", "in", "dem", "fl\u00fccht\u00b7gen", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann man, da\u00df sie doch bestehn,", "tokens": ["Kann", "man", ",", "da\u00df", "sie", "doch", "be\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nicht, wie es scheint, vergehn,", "tokens": ["Und", "nicht", ",", "wie", "es", "scheint", ",", "ver\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eben an den Kn\u00f6pfen lesen.", "tokens": ["E\u00b7ben", "an", "den", "Kn\u00f6p\u00b7fen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da sie, recht als Samen-T\u00f6pfe,", "tokens": ["Da", "sie", ",", "recht", "als", "Sa\u00b7men\u00b7T\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen Geist in sich enthalten,", "tokens": ["Ei\u00b7nen", "Geist", "in", "sich", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der sie nimmer l\u00e4sst veralten,", "tokens": ["Der", "sie", "nim\u00b7mer", "l\u00e4sst", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht verkommen, nicht verderben,", "tokens": ["Nicht", "ver\u00b7kom\u00b7men", ",", "nicht", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nicht vernichtigt seyn, nicht sterben:", "tokens": ["Nicht", "ver\u00b7nich\u00b7tigt", "seyn", ",", "nicht", "ster\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sondern dessen Eigenschaft,", "tokens": ["Son\u00b7dern", "des\u00b7sen", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Durch uns unbekannte Krafft,", "tokens": ["Durch", "uns", "un\u00b7be\u00b7kann\u00b7te", "Krafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Also sich in sich verj\u00fcnget,", "tokens": ["Al\u00b7so", "sich", "in", "sich", "ver\u00b7j\u00fcn\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und sich selber wiederbringet.", "tokens": ["Und", "sich", "sel\u00b7ber", "wie\u00b7der\u00b7brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch hab ich an diesen Bluhmen, wann die Knospen\noffen gehn,", "tokens": ["Noch", "hab", "ich", "an", "die\u00b7sen", "Bluh\u00b7men", ",", "wann", "die", "Knos\u00b7pen", "of\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Da sie krumm gebogen stehn,", "tokens": ["Da", "sie", "krumm", "ge\u00b7bo\u00b7gen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die meisten Erd-w\u00e4rts sehn,", "tokens": ["Da\u00df", "die", "meis\u00b7ten", "Erd\u00b7w\u00e4rts", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr als einmahl selbst betrachtet, auch wohl sonst davon", "tokens": ["Mehr", "als", "ein\u00b7mahl", "selbst", "be\u00b7trach\u00b7tet", ",", "auch", "wohl", "sonst", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "ADV", "PAV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Aber eh\u2019 und wann sie sterben,", "tokens": ["A\u00b7ber", "eh'", "und", "wann", "sie", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Bl\u00e4tter sich entf\u00e4rben,", "tokens": ["Und", "die", "Bl\u00e4t\u00b7ter", "sich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sind sie Himmel-w\u00e4rts gekehrt.", "tokens": ["Sind", "sie", "Him\u00b7mel\u00b7w\u00e4rts", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dadurch werd auch ich belehrt,", "tokens": ["Da\u00b7durch", "werd", "auch", "ich", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ie \u00e4lter ich hier werde,", "tokens": ["Da\u00df", "ie", "\u00e4l\u00b7ter", "ich", "hier", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich auch billig Seel und Sinn", "tokens": ["Ich", "auch", "bil\u00b7lig", "Seel", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den Tieffen dieser Erde", "tokens": ["Von", "den", "Tief\u00b7fen", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wende nach dem Himmel hin.", "tokens": ["Wen\u00b7de", "nach", "dem", "Him\u00b7mel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So wird, wie ein\u2019 irdsche Crone", "tokens": ["So", "wird", ",", "wie", "ein'", "ird\u00b7sche", "Cro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Diesen Bluhmen, also mir,", "tokens": ["Die\u00b7sen", "Bluh\u00b7men", ",", "al\u00b7so", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KOUS", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In nie welcker Pracht und Zier,", "tokens": ["In", "nie", "wel\u00b7cker", "Pracht", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PWAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Eine himmlische zum Lohne.", "tokens": ["Ei\u00b7ne", "himm\u00b7li\u00b7sche", "zum", "Loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch kann dieser Bluhmen Flor", "tokens": ["Noch", "kann", "die\u00b7ser", "Bluh\u00b7men", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch den Grossen dieser Erden", "tokens": ["Auch", "den", "Gros\u00b7sen", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein belehrend Sinn-Bild werden.", "tokens": ["Ein", "be\u00b7leh\u00b7rend", "Sinn\u00b7Bild", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenigstens kommt mir es vor,", "tokens": ["We\u00b7nigs\u00b7tens", "kommt", "mir", "es", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Recht, als sagte die\u00df Gesch\u00f6pfe,", "tokens": ["Recht", ",", "als", "sag\u00b7te", "die\u00df", "Ge\u00b7sch\u00f6p\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "VVFIN", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das gekr\u00f6nt so wol als sie:", "tokens": ["Das", "ge\u00b7kr\u00f6nt", "so", "wol", "als", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trotzt nicht auf die Herrlichkeit.", "tokens": ["Trotzt", "nicht", "auf", "die", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Merckt es! auch gekr\u00f6nte K\u00f6pfe", "tokens": ["Merckt", "es", "!", "auch", "ge\u00b7kr\u00f6n\u00b7te", "K\u00f6p\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sind ein Raub der schnellen Zeit.", "tokens": ["Sind", "ein", "Raub", "der", "schnel\u00b7len", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}