{"textgrid.poem.26499": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weit gedehnte, \u00f6de Strecken,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit gedehnte, \u00f6de Strecken,", "tokens": ["Weit", "ge\u00b7dehn\u00b7te", ",", "\u00f6\u00b7de", "Stre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmutzig-gelbe Wassert\u00fcmpel;", "tokens": ["Schmut\u00b7zig\u00b7gel\u00b7be", "Was\u00b7ser\u00b7t\u00fcm\u00b7pel", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einsam ragt der Schlot des Ofens", "tokens": ["Ein\u00b7sam", "ragt", "der", "Schlot", "des", "O\u00b7fens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber morsche Bretterschuppen.", "tokens": ["Ue\u00b7ber", "mor\u00b7sche", "Bret\u00b7ter\u00b7schup\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fahle Menschen, wie geknetet", "tokens": ["Fah\u00b7le", "Men\u00b7schen", ",", "wie", "ge\u00b7kne\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem fahlen Lehm des Bodens,", "tokens": ["Aus", "dem", "fah\u00b7len", "Lehm", "des", "Bo\u00b7dens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART"