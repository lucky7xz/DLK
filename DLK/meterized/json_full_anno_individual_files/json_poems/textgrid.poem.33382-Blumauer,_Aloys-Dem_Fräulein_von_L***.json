{"textgrid.poem.33382": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Dem Fr\u00e4ulein von L***", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So weih' ich denn mit Lust und Freude", "tokens": ["So", "weih'", "ich", "denn", "mit", "Lust", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch dir die Kinder meiner Phantasie,", "tokens": ["Auch", "dir", "die", "Kin\u00b7der", "mei\u00b7ner", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ohne Schmuck, pranglosem Kleide", "tokens": ["Der", "oh\u00b7ne", "Schmuck", ",", "prang\u00b7lo\u00b7sem", "Klei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur karg die Muse ihre Farben lieh;", "tokens": ["Nur", "karg", "die", "Mu\u00b7se", "ih\u00b7re", "Far\u00b7ben", "lieh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn ewig wollen sie nicht leben,", "tokens": ["Denn", "e\u00b7wig", "wol\u00b7len", "sie", "nicht", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch auf des Ruhmes schl\u00fcpferiger Bahn", "tokens": ["Noch", "auf", "des", "Ruh\u00b7mes", "schl\u00fcp\u00b7fe\u00b7ri\u00b7ger", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Lorbeerkranz sich nimmer weben,", "tokens": ["Den", "Lor\u00b7beer\u00b7kranz", "sich", "nim\u00b7mer", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch gl\u00e4nzen je an seinem fernen Plan.", "tokens": ["Noch", "gl\u00e4n\u00b7zen", "je", "an", "sei\u00b7nem", "fer\u00b7nen", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nur nach der Freunde Beifall streben,", "tokens": ["Nur", "nach", "der", "Freun\u00b7de", "Bei\u00b7fall", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dies ist ihr h\u00f6chster Wunsch, allein ihr Sinn,", "tokens": ["Dies", "ist", "ihr", "h\u00f6chs\u00b7ter", "Wunsch", ",", "al\u00b7lein", "ihr", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und wenn mir ihn nun diese geben,", "tokens": ["Und", "wenn", "mir", "ihn", "nun", "die\u00b7se", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "PDAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sag' an, ob ich nicht reich belohnet bin?", "tokens": ["Sag'", "an", ",", "ob", "ich", "nicht", "reich", "be\u00b7loh\u00b7net", "bin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und hab' ich erst von dir ihn auch errungen,", "tokens": ["Und", "hab'", "ich", "erst", "von", "dir", "ihn", "auch", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ja dann, dann ist mein sch\u00f6nster Zweck gelungen.", "tokens": ["Ja", "dann", ",", "dann", "ist", "mein", "sch\u00f6ns\u00b7ter", "Zweck", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So weih' ich denn mit Lust und Freude", "tokens": ["So", "weih'", "ich", "denn", "mit", "Lust", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch dir die Kinder meiner Phantasie,", "tokens": ["Auch", "dir", "die", "Kin\u00b7der", "mei\u00b7ner", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3"