, "sie", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mit Hand und Fu\u00df", "tokens": ["Mit", "Hand", "und", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Schn\u00fcrt sie der Scherg an ohne Wank,", "tokens": ["Schn\u00fcrt", "sie", "der", "Scherg", "an", "oh\u00b7ne", "Wank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und nun beginnt das Schneiden.", "tokens": ["Und", "nun", "be\u00b7ginnt", "das", "Schnei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da wird der Zwerg so ausgereckt,", "tokens": ["Da", "wird", "der", "Zwerg", "so", "aus\u00b7ge\u00b7reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er wird d\u00fcnn wie Spinnen;", "tokens": ["Da\u00df", "er", "wird", "d\u00fcnn", "wie", "Spin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da wird die Riesin so gezeckt,", "tokens": ["Da", "wird", "die", "Rie\u00b7sin", "so", "ge\u00b7zeckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihr vergehn die Sinnen.", "tokens": ["Da\u00df", "ihr", "ver\u00b7gehn", "die", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Schreiner schnitzt,", "tokens": ["Der", "Schrei\u00b7ner", "schnitzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bis alles schlitzt;", "tokens": ["Bis", "al\u00b7les", "schlitzt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie nun die beiden sind verreckt,", "tokens": ["Wie", "nun", "die", "bei\u00b7den", "sind", "ver\u00b7reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "PIAT", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird der Verstand es innen.", "tokens": ["Wird", "der", "Ver\u00b7stand", "es", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbes war f\u00fcr sie zu schwer die Prob',", "tokens": ["\u00bb", "es", "war", "f\u00fcr", "sie", "zu", "schwer", "die", "Prob'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sie sind dran verschieden;", "tokens": ["Und", "sie", "sind", "dran", "ver\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun werden sie mich doch, Gottlob,", "tokens": ["Nun", "wer\u00b7den", "sie", "mich", "doch", ",", "Gott\u00b7lob", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht \u00e4rgern mehr hienieden.", "tokens": ["Nicht", "\u00e4r\u00b7gern", "mehr", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nehmt sie hier ab,", "tokens": ["Nehmt", "sie", "hier", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Schnell in ein Grab!", "tokens": ["Schnell", "in", "ein", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ihr zwei Gesellen kraus und grob,", "tokens": ["Ihr", "zwei", "Ge\u00b7sel\u00b7len", "kraus", "und", "grob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich w\u00fcnsch' euch ewigen Frieden.\u00ab", "tokens": ["Ich", "w\u00fcn\u00b7sch'", "euch", "e\u00b7wi\u00b7gen", "Frie\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}