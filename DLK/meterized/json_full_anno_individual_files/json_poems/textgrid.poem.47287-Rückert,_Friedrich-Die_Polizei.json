Al\u00b7lein", "es", "ist", "doch", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie sitzen drau\u00dfen auf den Bergen.", "tokens": ["Sie", "sit\u00b7zen", "drau\u00b7\u00dfen", "auf", "den", "Ber\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geht der Verstand und geht der Scherg,", "tokens": ["Geht", "der", "Ver\u00b7stand", "und", "geht", "der", "Scherg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit ihnen geht noch einer,", "tokens": ["Mit", "ih\u00b7nen", "geht", "noch", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der die zwei drau\u00dfen auf dem Berg", "tokens": ["Der", "die", "zwei", "drau\u00b7\u00dfen", "auf", "dem", "Berg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "CARD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Abhobeln soll, ein Schreiner.", "tokens": ["Ab\u00b7ho\u00b7beln", "soll", ",", "ein", "Schrei\u00b7ner", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wenn sein wollt ihr", "tokens": ["Wenn", "sein", "wollt", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAINF", "VMFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Geduldet hier,", "tokens": ["Ge\u00b7dul\u00b7det", "hier", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So mu\u00dft du gr\u00f6\u00dfer werden, Zwerg,", "tokens": ["So", "mu\u00dft", "du", "gr\u00f6\u00b7\u00dfer", "wer\u00b7den", ",", "Zwerg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und, Riesin, du viel kleiner.", "tokens": ["Und", ",", "Rie\u00b7sin", ",", "du", "viel", "klei\u00b7ner", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wird aufgespannt die Hobelbank,", "tokens": ["Wird", "auf\u00b7ge\u00b7spannt", "die", "Ho\u00b7bel\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darauf gespannt die beiden;", "tokens": ["Da\u00b7rauf", "ge\u00b7spannt", "die", "bei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "ART", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Zwerg lacht sich vor Lust halb krank,", "tokens": ["Der", "Zwerg", "lacht", "sich", "vor", "Lust", "halb", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Riesin will's nicht leiden;", "tokens": ["Die", "Rie\u00b7sin", "will's", "nicht", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein sie mu\u00df.", "tokens": ["Al\u00b7lein", "sie", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mit Hand und Fu\u00df", "tokens": ["Mit", "Hand", "und", "Fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Schn\u00fcrt sie der Scherg an ohne Wank,", "tokens": ["Schn\u00fcrt", "sie", "der", "Scherg", "an", "oh\u00b7ne", "Wank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und nun beginnt das Schneiden.", "tokens": ["Und", "nun", "be\u00b7ginnt", "das", "Schnei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da wird der Zwerg so ausgereckt,", "tokens": ["Da", "wird", "der", "Zwerg", "so", "aus\u00b7ge\u00b7reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er wird d\u00fcnn wie Spinnen;", "tokens": ["Da\u00df", "er", "wird", "d\u00fcnn", "wie", "Spin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da wird die Riesin so gezeckt,", "tokens": ["Da", "wird", "die", "Rie\u00b7sin", "so", "ge\u00b7zeckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihr vergehn die Sinnen.", "tokens": ["Da\u00df", "ihr", "ver\u00b7gehn", "die", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Schreiner schnitzt,", "tokens": ["Der", "Schrei\u00b7ner", "schnitzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bis alles schlitzt;", "tokens": ["Bis", "al\u00b7les", "schlitzt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie nun die beiden sind verreckt,", "tokens": ["Wie", "nun", "die", "bei\u00b7den", "sind", "ver\u00b7reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "PIAT", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird der Verstand es innen.", "tokens": ["Wird", "der", "Ver\u00b7stand", "es", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbes war f\u00fcr sie zu schwer die Prob',", "tokens": ["\u00bb", "es", "war", "f\u00fcr", "sie", "zu", "schwer", "die", "Prob'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sie si