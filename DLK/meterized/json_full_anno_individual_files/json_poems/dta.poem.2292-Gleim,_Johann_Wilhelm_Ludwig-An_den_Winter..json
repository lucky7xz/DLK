{"dta.poem.2292": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An den Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Winter mit dem grauen Barte,", "tokens": ["Win\u00b7ter", "mit", "dem", "grau\u00b7en", "Bar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den angefrornen Lokken,", "tokens": ["Mit", "den", "an\u00b7ge\u00b7fror\u00b7nen", "Lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wilst du denn nicht einmal lachen?", "tokens": ["Wilst", "du", "denn", "nicht", "ein\u00b7mal", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die Lippen zugefroren?", "tokens": ["Sind", "die", "Lip\u00b7pen", "zu\u00b7ge\u00b7fro\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Komm herein, was stehst du draussen?", "tokens": ["Komm", "her\u00b7ein", ",", "was", "stehst", "du", "draus\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Komm herein, du solst schon thauen.", "tokens": ["Komm", "her\u00b7ein", ",", "du", "solst", "schon", "thau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich! wie st\u00f6rrisch sind die Minen.", "tokens": ["Sich", "!", "wie", "st\u00f6r\u00b7risch", "sind", "die", "Mi\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$.", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bist du denn ein Feind der Freude?", "tokens": ["Bist", "du", "denn", "ein", "Feind", "der", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wilst du meine Lust verdammen?", "tokens": ["Wilst", "du", "mei\u00b7ne", "Lust", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gut! so will ich dich nicht bitten.", "tokens": ["Gut", "!", "so", "will", "ich", "dich", "nicht", "bit\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber sei nur immer st\u00f6rrisch,", "tokens": ["A\u00b7ber", "sei", "nur", "im\u00b7mer", "st\u00f6r\u00b7risch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mache Felder, mache Fluren,", "tokens": ["Ma\u00b7che", "Fel\u00b7der", ",", "ma\u00b7che", "Flu\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mache Berg und Th\u00e4ler traurig,", "tokens": ["Ma\u00b7che", "Berg", "und", "Th\u00e4\u00b7ler", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mich solst du nicht traurig machen.", "tokens": ["Mich", "solst", "du", "nicht", "trau\u00b7rig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "T\u00f6dte diese frische Lilgen,", "tokens": ["T\u00f6d\u00b7te", "die\u00b7se", "fri\u00b7sche", "Lil\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "T\u00f6dte diese iunge Rosen,", "tokens": ["T\u00f6d\u00b7te", "die\u00b7se", "i\u00b7un\u00b7ge", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Auf den iugendlichen Wangen,", "tokens": ["Auf", "den", "i\u00b7u\u00b7gend\u00b7li\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "T\u00f6dte sie einmal zum Scherze;", "tokens": ["T\u00f6d\u00b7te", "sie", "ein\u00b7mal", "zum", "Scher\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.19": {"text": "Aber la\u00df mir nur die Rosen,", "tokens": ["A\u00b7ber", "la\u00df", "mir", "nur", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Auf den Wangen, auf dem Busen", "tokens": ["Auf", "den", "Wan\u00b7gen", ",", "auf", "dem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Meiner braunen Doris bl\u00fchend:", "tokens": ["Mei\u00b7ner", "brau\u00b7nen", "Do\u00b7ris", "bl\u00fc\u00b7hend", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Dann so soll sie dich besch\u00e4men,", "tokens": ["Dann", "so", "soll", "sie", "dich", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Dann soll sie mit einem Kusse", "tokens": ["Dann", "soll", "sie", "mit", "ei\u00b7nem", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Meinen halberstorbnen Wangen,", "tokens": ["Mei\u00b7nen", "hal\u00b7ber\u00b7storb\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Alle Rosen wieder geben;", "tokens": ["Al\u00b7le", "Ro\u00b7sen", "wie\u00b7der", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Dann soll sie mit ihren Lippen", "tokens": ["Dann", "soll", "sie", "mit", "ih\u00b7ren", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Meine Lippen sch\u00f6ner f\u00e4rben.", "tokens": ["Mei\u00b7ne", "Lip\u00b7pen", "sch\u00f6\u00b7ner", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Alter! wilst du\u2019s selbst versuchen?", "tokens": ["Al\u00b7ter", "!", "wilst", "du's", "selbst", "ver\u00b7su\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Komm! sie soll dich einmal k\u00fcssen;", "tokens": ["Komm", "!", "sie", "soll", "dich", "ein\u00b7mal", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Dann solst du, wir wollen wetten,", "tokens": ["Dann", "solst", "du", ",", "wir", "wol\u00b7len", "wet\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Bald dein Pelzwerk von dir werfen.", "tokens": ["Bald", "dein", "Pelz\u00b7werk", "von", "dir", "wer\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Dann solst du f\u00fcr Hitze dursten.", "tokens": ["Dann", "solst", "du", "f\u00fcr", "Hit\u00b7ze", "durs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Komm! hier ist schon was zu trinken.", "tokens": ["Komm", "!", "hier", "ist", "schon", "was", "zu", "trin\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}