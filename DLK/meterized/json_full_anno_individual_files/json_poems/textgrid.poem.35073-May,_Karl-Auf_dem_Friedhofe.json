{"textgrid.poem.35073": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Friedhofe", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm her; komm her, du fremder Wandersmann;", "tokens": ["Komm", "her", ";", "komm", "her", ",", "du", "frem\u00b7der", "Wan\u00b7ders\u00b7mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geh nicht vorbei an unbekanntem Grabe.", "tokens": ["Geh", "nicht", "vor\u00b7bei", "an", "un\u00b7be\u00b7kann\u00b7tem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6r mich, ja auch um deinetwillen, an,", "tokens": ["H\u00f6r", "mich", ",", "ja", "auch", "um", "dei\u00b7net\u00b7wil\u00b7len", ",", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "ADV", "APPR", "ADV", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und glaube, was ich dir zu sagen habe!", "tokens": ["Und", "glau\u00b7be", ",", "was", "ich", "dir", "zu", "sa\u00b7gen", "ha\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein jeder Mensch, der nach dem Himmel strebt,", "tokens": ["Ein", "je\u00b7der", "Mensch", ",", "der", "nach", "dem", "Him\u00b7mel", "strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soll hier ein liebes, gutes W\u00f6rtlein sagen;", "tokens": ["Soll", "hier", "ein", "lie\u00b7bes", ",", "gu\u00b7tes", "W\u00f6rt\u00b7lein", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wird der Seele, die da oben lebt,", "tokens": ["Es", "wird", "der", "See\u00b7le", ",", "die", "da", "o\u00b7ben", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf H\u00e4nden des Gebets emporgetragen.", "tokens": ["Auf", "H\u00e4n\u00b7den", "des", "Ge\u00b7bets", "em\u00b7por\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort nimmt sie es mit Freuden in Empfang", "tokens": ["Dort", "nimmt", "sie", "es", "mit", "Freu\u00b7den", "in", "Emp\u00b7fang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4chelt dankbar auf den Spender nieder,", "tokens": ["Und", "l\u00e4\u00b7chelt", "dank\u00b7bar", "auf", "den", "Spen\u00b7der", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dieses L\u00e4cheln strahlt ihm lebenslang", "tokens": ["Und", "die\u00b7ses", "L\u00e4\u00b7cheln", "strahlt", "ihm", "le\u00b7bens\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das, was er gab, mit tausend Zinsen wieder.", "tokens": ["Das", ",", "was", "er", "gab", ",", "mit", "tau\u00b7send", "Zin\u00b7sen", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Komm her; komm her, du fremder Wandersmann;", "tokens": ["Komm", "her", ";", "komm", "her", ",", "du", "frem\u00b7der", "Wan\u00b7ders\u00b7mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geh nicht vorbei an unbekanntem Grabe.", "tokens": ["Geh", "nicht", "vor\u00b7bei", "an", "un\u00b7be\u00b7kann\u00b7tem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6r mich, ja auch um deinetwillen, an,", "tokens": ["H\u00f6r", "mich", ",", "ja", "auch", "um", "dei\u00b7net\u00b7wil\u00b7len", ",", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "ADV", "APPR", "ADV", "$,