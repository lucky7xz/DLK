{"textgrid.poem.50201": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "33. Auf den um Raht fragenden Crato", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ersuchet, nach ", "tokens": ["Er\u00b7su\u00b7chet", ",", "nach"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So merck' ich seinen Anschlag woll;", "tokens": ["So", "merck", "ich", "sei\u00b7nen", "An\u00b7schlag", "woll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er will dass ich ihn ", "tokens": ["Er", "will", "dass", "ich", "ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KOUS", "PPER", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Gleich sprech' ich ihm von Klugheit vor,", "tokens": ["Gleich", "sprech'", "ich", "ihm", "von", "Klug\u00b7heit", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kitzel' ihm sein willig Ohr;", "tokens": ["Und", "kit\u00b7zel'", "ihm", "sein", "wil\u00b7lig", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Biss dass ich meinen Schluss gemacht,", "tokens": ["Biss", "dass", "ich", "mei\u00b7nen", "Schluss