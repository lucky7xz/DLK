{"textgrid.poem.41574": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Allerseelentage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Stunde k\u00f6mmt, wo Tote gehn,", "tokens": ["Die", "Stun\u00b7de", "k\u00f6mmt", ",", "wo", "To\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo l\u00e4ngst vermorschte Augen sehn.", "tokens": ["Wo", "l\u00e4ngst", "ver\u00b7morschte", "Au\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O Stunde! Stunde! gr\u00f6\u00dfte aller Stunden,", "tokens": ["O", "Stun\u00b7de", "!", "Stun\u00b7de", "!", "gr\u00f6\u00df\u00b7te", "al\u00b7ler", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du bist bei mir und l\u00e4\u00dft mich nicht,", "tokens": ["Du", "bist", "bei", "mir", "und", "l\u00e4\u00dft", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin bei dir in strenger Pflicht,", "tokens": ["Ich", "bin", "bei", "dir", "in", "stren\u00b7ger", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir atm' ich auf, dir bluten meine Wunden!", "tokens": ["Dir", "atm'", "ich", "auf", ",", "dir", "blu\u00b7ten", "mei\u00b7ne", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entsetzlich bist du, und doch wert,", "tokens": ["Ent\u00b7setz\u00b7lich", "bist", "du", ",", "und", "doch", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja meine ganze Seele kehrt", "tokens": ["Ja", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "kehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dir sich, in des Lebens Nacht und Irren", "tokens": ["Zu", "dir", "sich", ",", "in", "des", "Le\u00b7bens", "Nacht", "und", "Ir\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "$,", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein fest Asyl, mein Herzgebl\u00fct,", "tokens": ["Mein", "fest", "A\u00b7syl", ",", "mein", "Herz\u00b7ge\u00b7bl\u00fct", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu dem die z\u00e4he Hoffnung flieht,", "tokens": ["Zu", "dem", "die", "z\u00e4\u00b7he", "Hoff\u00b7nung", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Angst und Gr\u00fcbeln wie Gespenster irren.", "tokens": ["Wenn", "Angst", "und", "Gr\u00fc\u00b7beln", "wie", "Ge\u00b7spens\u00b7ter", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "W\u00fc\u00dft' ich es nicht, da\u00df du gewi\u00df", "tokens": ["W\u00fc\u00dft'", "ich", "es", "nicht", ",", "da\u00df", "du", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jener R\u00e4ume Finsternis", "tokens": ["In", "je\u00b7ner", "R\u00e4u\u00b7me", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegst schlummernd wie ein Embrio verborgen:", "tokens": ["Liegst", "schlum\u00b7mernd", "wie", "ein", "Emb\u00b7rio", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann m\u00f6cht' ich schaudernd mein Gesicht", "tokens": ["Dann", "m\u00f6cht'", "ich", "schau\u00b7dernd", "mein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verbergen vor der Sonne Licht,", "tokens": ["Ver\u00b7ber\u00b7gen", "vor", "der", "Son\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergehn wie Regenlache vor dem Morgen.", "tokens": ["Ver\u00b7gehn", "wie", "Re\u00b7gen\u00b7la\u00b7che", "vor", "dem", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verkennung nicht treibt mich zu dir,", "tokens": ["Ver\u00b7ken\u00b7nung", "nicht", "treibt", "mich", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mild ist die strengste Stimme mir,", "tokens": ["Mild", "ist", "die", "strengs\u00b7te", "Stim\u00b7me", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimmt meine Heller und gibt Millionen.", "tokens": ["Nimmt", "mei\u00b7ne", "Hel\u00b7ler", "und", "gibt", "Mil\u00b7lion\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nein, wo mir Unrecht je geschehn,", "tokens": ["Nein", ",", "wo", "mir", "Un\u00b7recht", "je", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ward mir wohl, da f\u00fchlt' ich wehn", "tokens": ["Da", "ward", "mir", "wohl", ",", "da", "f\u00fchlt'", "ich", "wehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein leises Atmen durch der Zeit \u00c4onen.", "tokens": ["Dein", "lei\u00b7ses", "At\u00b7men", "durch", "der", "Zeit", "\u00c4\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch Liebe, Ehre treibt mich fort", "tokens": ["Doch", "Lie\u00b7be", ",", "Eh\u00b7re", "treibt", "mich", "fort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dir als meinem letzten Port,", "tokens": ["Zu", "dir", "als", "mei\u00b7nem", "letz\u00b7ten", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo klar mein Grabesinnre wird erscheinen.", "tokens": ["Wo", "klar", "mein", "Gra\u00b7be\u00b7sinn\u00b7re", "wird", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann auf der rechten Waage mag", "tokens": ["Dann", "auf", "der", "rech\u00b7ten", "Waa\u00b7ge", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich t\u00fcrmen meine Schuld und Schmach,", "tokens": ["Sich", "t\u00fcr\u00b7men", "mei\u00b7ne", "Schuld", "und", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zitternd nahn mein K\u00e4mpfen und mein Weinen.", "tokens": ["Und", "zit\u00b7ternd", "nahn", "mein", "K\u00e4mp\u00b7fen", "und", "mein", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vor dir ich sollte Trostes bar", "tokens": ["Vor", "dir", "ich", "soll\u00b7te", "Tros\u00b7tes", "bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PPER", "VMFIN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zergehen wie ein Schatten gar;", "tokens": ["Zer\u00b7ge\u00b7hen", "wie", "ein", "Schat\u00b7ten", "gar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch anders ist es ohne mein Verschulden:", "tokens": ["Doch", "an\u00b7ders", "ist", "es", "oh\u00b7ne", "mein", "Ver\u00b7schul\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu dir als zu dem h\u00f6chsten Gl\u00fcck", "tokens": ["Zu", "dir", "als", "zu", "dem", "h\u00f6chs\u00b7ten", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KOKOM", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie unbeweglich starrt der Blick,", "tokens": ["Wie", "un\u00b7be\u00b7weg\u00b7lich", "starrt", "der", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kaum, kaum mag die Z\u00f6gerung ich dulden.", "tokens": ["Und", "kaum", ",", "kaum", "mag", "die", "Z\u00f6\u00b7ge\u00b7rung", "ich", "dul\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch da sich einmal Hoffnung regt,", "tokens": ["Doch", "da", "sich", "ein\u00b7mal", "Hoff\u00b7nung", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wird die Hand, die sie gelegt", "tokens": ["So", "wird", "die", "Hand", ",", "die", "sie", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dieses Busens fabelgleichen Boden,", "tokens": ["In", "die\u00b7ses", "Bu\u00b7sens", "fa\u00b7bel\u00b7glei\u00b7chen", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie wird den Keim, der willenlos", "tokens": ["Sie", "wird", "den", "Keim", ",", "der", "wil\u00b7len\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und keinem \u00dcbermut entspro\u00df,", "tokens": ["Und", "kei\u00b7nem", "\u00dc\u00b7ber\u00b7mut", "ent\u00b7spro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht wie ein Unkraut aus dem Grunde roden.", "tokens": ["Nicht", "wie", "ein", "Un\u00b7kraut", "aus", "dem", "Grun\u00b7de", "ro\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wenn k\u00f6mmt die Zeit, wenn niederf\u00e4llt", "tokens": ["Wenn", "k\u00f6mmt", "die", "Zeit", ",", "wenn", "nie\u00b7der\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "$,", "KOUS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Flitter, den gelegt die Welt,", "tokens": ["Der", "Flit\u00b7ter", ",", "den", "ge\u00b7legt", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Talent und Gl\u00fcck, ums hagere Gerippe:", "tokens": ["Ta\u00b7lent", "und", "Gl\u00fcck", ",", "ums", "ha\u00b7ge\u00b7re", "Ge\u00b7rip\u00b7pe", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da steht der Bettler, schaut ihn an!", "tokens": ["Da", "steht", "der", "Bett\u00b7ler", ",", "schaut", "ihn", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann ist die Zeit, um Gnade dann", "tokens": ["Dann", "ist", "die", "Zeit", ",", "um", "Gna\u00b7de", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KOUI", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf zitternd flehen des Verarmten Lippe.", "tokens": ["Darf", "zit\u00b7ternd", "fle\u00b7hen", "des", "Ver\u00b7arm\u00b7ten", "Lip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Dann macht nicht schamrot mich ein Tand,", "tokens": ["Dann", "macht", "nicht", "scham\u00b7rot", "mich", "ein", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann hat gestellt die rechte Hand", "tokens": ["Dann", "hat", "ge\u00b7stellt", "die", "rech\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich tief und \u00e4rmlich, wie ich es verdienet,", "tokens": ["Mich", "tief", "und", "\u00e4rm\u00b7lich", ",", "wie", "ich", "es", "ver\u00b7die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann trifft mich wie ein Dolchsto\u00df nicht", "tokens": ["Dann", "trifft", "mich", "wie", "ein", "Dolch\u00b7sto\u00df", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinfort ein Aug' voll Liebeslicht:", "tokens": ["Hin\u00b7fort", "ein", "Aug'", "voll", "Lie\u00b7bes\u00b7licht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin erniedriget und bin ges\u00fchnet.", "tokens": ["Ich", "bin", "er\u00b7nied\u00b7ri\u00b7get", "und", "bin", "ge\u00b7s\u00fch\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die Stunde k\u00f6mmt, wo Tote gehn,", "tokens": ["Die", "Stun\u00b7de", "k\u00f6mmt", ",", "wo", "To\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo l\u00e4ngst vermorschte Augen sehn.", "tokens": ["Wo", "l\u00e4ngst", "ver\u00b7morschte", "Au\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O Stunde! Stunde! gr\u00f6\u00dfte aller Stunden,", "tokens": ["O", "Stun\u00b7de", "!", "Stun\u00b7de", "!", "gr\u00f6\u00df\u00b7te", "al\u00b7ler", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du bist bei mir und l\u00e4\u00dft mich nicht,", "tokens": ["Du", "bist", "bei", "mir", "und", "l\u00e4\u00dft", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin bei dir in strenger Pflicht,", "tokens": ["Ich", "bin", "bei", "dir", "in", "stren\u00b7ger", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir atm' ich auf, dir bluten meine Wunden!", "tokens": ["Dir", "atm'", "ich", "auf", ",", "dir", "blu\u00b7ten", "mei\u00b7ne", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Entsetzlich bist du, und doch wert,", "tokens": ["Ent\u00b7setz\u00b7lich", "bist", "du", ",", "und", "doch", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja meine ganze Seele kehrt", "tokens": ["Ja", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "kehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dir sich, in des Lebens Nacht und Irren", "tokens": ["Zu", "dir", "sich", ",", "in", "des", "Le\u00b7bens", "Nacht", "und", "Ir\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "$,", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein fest Asyl, mein Herzgebl\u00fct,", "tokens": ["Mein", "fest", "A\u00b7syl", ",", "mein", "Herz\u00b7ge\u00b7bl\u00fct", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu dem die z\u00e4he Hoffnung flieht,", "tokens": ["Zu", "dem", "die", "z\u00e4\u00b7he", "Hoff\u00b7nung", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Angst und Gr\u00fcbeln wie Gespenster irren.", "tokens": ["Wenn", "Angst", "und", "Gr\u00fc\u00b7beln", "wie", "Ge\u00b7spens\u00b7ter", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "W\u00fc\u00dft' ich es nicht, da\u00df du gewi\u00df", "tokens": ["W\u00fc\u00dft'", "ich", "es", "nicht", ",", "da\u00df", "du", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jener R\u00e4ume Finsternis", "tokens": ["In", "je\u00b7ner", "R\u00e4u\u00b7me", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegst schlummernd wie ein Embrio verborgen:", "tokens": ["Liegst", "schlum\u00b7mernd", "wie", "ein", "Emb\u00b7rio", "ver\u00b7bor\u00b7gen", ":"], "