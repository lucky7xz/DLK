{"textgrid.poem.54497": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn nicht das Weitzen-Korn/ ins Grab der Erden f\u00e4llt/", "tokens": ["Wenn", "nicht", "das", "Weit\u00b7zen\u00b7Korn", "/", "ins", "Grab", "der", "Er\u00b7den", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "$(", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd sich den schnellen Zahn der F\u00e4ule l\u00e4st verzehren.", "tokens": ["Vnd", "sich", "den", "schnel\u00b7len", "Zahn", "der", "F\u00e4u\u00b7le", "l\u00e4st", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kan es keine Bl\u00fct'/ auch keine Frucht gewehren:", "tokens": ["So", "kan", "es", "kei\u00b7ne", "Bl\u00fct'", "/", "auch", "kei\u00b7ne", "Frucht", "ge\u00b7weh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "$(", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob es schon Safft vnd Krafft vnd Saamen in sich h\u00e4lt/", "tokens": ["Ob", "es", "schon", "Safft", "vnd", "Krafft", "vnd", "Saa\u00b7men", "in", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PP