{"dta.poem.19274": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einer H\u00f6le hochgew\u00f6lbt und tiefgegraben", "tokens": ["In", "ei\u00b7ner", "H\u00f6\u00b7le", "hoch\u00b7ge\u00b7w\u00f6lbt", "und", "tief\u00b7ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind tr\u00e4ge Wohner, die dort feste Sitze haben.", "tokens": ["Sind", "tr\u00e4\u00b7ge", "Woh\u00b7ner", ",", "die", "dort", "fes\u00b7te", "Sit\u00b7ze", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie angefesselt sind sie an dem Sitz von Stein,", "tokens": ["Wie", "an\u00b7ge\u00b7fes\u00b7selt", "sind", "sie", "an", "dem", "Sitz", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sitzen auswerts nicht gewendet, sondern ein.", "tokens": ["Und", "sit\u00b7zen", "aus\u00b7werts", "nicht", "ge\u00b7wen\u00b7det", ",", "son\u00b7dern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "VVPP", "$,", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In ihrem R\u00fccken ist von oben eine Kluft", "tokens": ["In", "ih\u00b7rem", "R\u00fc\u00b7cken", "ist", "von", "o\u00b7ben", "ei\u00b7ne", "Kluft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "APPR", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesprengt, durch welche dringt des Himmels Licht und Luft.", "tokens": ["Ge\u00b7sprengt", ",", "durch", "wel\u00b7che", "dringt", "des", "Him\u00b7mels", "Licht", "und", "Luft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "PRELS", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vor ihrem Angesicht der H\u00f6le finstre Wand", "tokens": ["Vor", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", "der", "H\u00f6\u00b7le", "finst\u00b7re", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dient ihrem Augenmerk zum einz'gen Gegenstand.", "tokens": ["Dient", "ih\u00b7rem", "Au\u00b7gen\u00b7merk", "zum", "einz'\u00b7gen", "Ge\u00b7gen\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie halten zugewandt den R\u00fccken jenem Licht,", "tokens": ["Sie", "hal\u00b7ten", "zu\u00b7ge\u00b7wandt", "den", "R\u00fc\u00b7cken", "je\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur auf diese Wand gewendet ihr Gesicht.", "tokens": ["Und", "nur", "auf", "die\u00b7se", "Wand", "ge\u00b7wen\u00b7det", "ihr", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was werden sie da sehn? die Schatten, die entstehn", "tokens": ["Was", "wer\u00b7den", "sie", "da", "sehn", "?", "die", "Schat\u00b7ten", ",", "die", "ent\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVINF", "$.", "ART", "NN", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Dinge, die vorbei in ihrem R\u00fccken gehn;", "tokens": ["Der", "Din\u00b7ge", ",", "die", "vor\u00b7bei", "in", "ih\u00b7rem", "R\u00fc\u00b7cken", "gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Schatten, welche wirft der Sonne Glanz vom R\u00fccken,", "tokens": ["Die", "Schat\u00b7ten", ",", "wel\u00b7che", "wirft", "der", "Son\u00b7ne", "Glanz", "vom", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um auch mit einem Bild das dunkle Haus zu schm\u00fccken.", "tokens": ["Um", "auch", "mit", "ei\u00b7nem", "Bild", "das", "dunk\u00b7le", "Haus", "zu", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Leute drinnen sehn die Dinge nicht, und halten", "tokens": ["Die", "Leu\u00b7te", "drin\u00b7nen", "sehn", "die", "Din\u00b7ge", "nicht", ",", "und", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Schattenbild davon f\u00fcr wirkliche Gestalten.", "tokens": ["Das", "Schat\u00b7ten\u00b7bild", "da\u00b7von", "f\u00fcr", "wirk\u00b7li\u00b7che", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Sie freuen m\u00e4\u00dfig sich am bunten Schattenspiel,", "tokens": ["Sie", "freu\u00b7en", "m\u00e4\u00b7\u00dfig", "sich", "am", "bun\u00b7ten", "Schat\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wissen doch davon den Grund nicht noch das Ziel.", "tokens": ["Und", "wis\u00b7sen", "doch", "da\u00b7von", "den", "Grund", "nicht", "noch", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Nun aber ist ein Geist zu einem hergekommen,", "tokens": ["Nun", "a\u00b7ber", "ist", "ein", "Geist", "zu", "ei\u00b7nem", "her\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPR", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hat die Fesseln ihm, die Tr\u00e4gheit abgenommen.", "tokens": ["Der", "hat", "die", "Fes\u00b7seln", "ihm", ",", "die", "Tr\u00e4g\u00b7heit", "ab\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Geblieben sind geschn\u00fcrt die andern unber\u00fchrt,", "tokens": ["Ge\u00b7blie\u00b7ben", "sind", "ge\u00b7schn\u00fcrt", "die", "an\u00b7dern", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "VVPP", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn aber hat der Geist befreiet und entf\u00fchrt.", "tokens": ["Ihn", "a\u00b7ber", "hat", "der", "Geist", "be\u00b7frei\u00b7et", "und", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Sein Angesicht zum Licht wandt' er mit schneller Wendung,", "tokens": ["Sein", "An\u00b7ge\u00b7sicht", "zum", "Licht", "wandt'", "er", "mit", "schnel\u00b7ler", "Wen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da traf sein Angesicht vom Licht zuerst die Blendung.", "tokens": ["Da", "traf", "sein", "An\u00b7ge\u00b7sicht", "vom", "Licht", "zu\u00b7erst", "die", "Blen\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Doch aufwerts zog er ihn die hehre schwere Kluft,", "tokens": ["Doch", "auf\u00b7werts", "zog", "er", "ihn", "die", "heh\u00b7re", "schwe\u00b7re", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihm entgegen kam zur St\u00e4rkung Himmelsluft.", "tokens": ["Und", "ihm", "ent\u00b7ge\u00b7gen", "kam", "zur", "St\u00e4r\u00b7kung", "Him\u00b7mel\u00b7sluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Und als er drau\u00dfen war, erstaunt' er nicht geringe,", "tokens": ["Und", "als", "er", "drau\u00b7\u00dfen", "war", ",", "er\u00b7staunt'", "er", "nicht", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er nun offenbar statt Schatten sah die Dinge.", "tokens": ["Da\u00df", "er", "nun", "of\u00b7fen\u00b7bar", "statt", "Schat\u00b7ten", "sah", "die", "Din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Sein Auge war noch schwach f\u00fcr die Gewalt des Sch\u00f6nen,", "tokens": ["Sein", "Au\u00b7ge", "war", "noch", "schwach", "f\u00fcr", "die", "Ge\u00b7walt", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er mu\u00dfte nach und nach sich an den Glanz gew\u00f6hnen.", "tokens": ["Er", "mu\u00df\u00b7te", "nach", "und", "nach", "sich", "an", "den", "Glanz", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "KON", "APPR", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Er sah der Sonne Bild zuerst im Spiegelteich;", "tokens": ["Er", "sah", "der", "Son\u00b7ne", "Bild", "zu\u00b7erst", "im", "Spie\u00b7gel\u00b7teich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie war noch nicht sie selbst, doch schon sich selber gleich.", "tokens": ["Sie", "war", "noch", "nicht", "sie", "selbst", ",", "doch", "schon", "sich", "sel\u00b7ber", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "PPER", "ADV", "$,", "ADV", "ADV", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Dann aber konnt' er ihr ins Auge blicken frei,", "tokens": ["Dann", "a\u00b7ber", "konnt'", "er", "ihr", "ins", "Au\u00b7ge", "bli\u00b7cken", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beseligt, da\u00df ihr Blick in seinem Auge sei.", "tokens": ["Be\u00b7se\u00b7ligt", ",", "da\u00df", "ihr", "Blick", "in", "sei\u00b7nem", "Au\u00b7ge", "sei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Nun aber durchs Geschick ist er zur\u00fcckgekommen", "tokens": ["Nun", "a\u00b7ber", "durchs", "Ge\u00b7schick", "ist", "er", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur H\u00f6l', und hat den Sitz dort wieder eingenommen.", "tokens": ["Zur", "H\u00f6l'", ",", "und", "hat", "den", "Sitz", "dort", "wie\u00b7der", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Dort sitzen noch, die sich am Schattenbild erbaun,", "tokens": ["Dort", "sit\u00b7zen", "noch", ",", "die", "sich", "am", "Schat\u00b7ten\u00b7bild", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PRELS", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denselben wollt' er nun, was er geschaut, vertraun.", "tokens": ["Den\u00b7sel\u00b7ben", "wollt'", "er", "nun", ",", "was", "er", "ge\u00b7schaut", ",", "ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Viel M\u00fche gab er sich, in Bildern zu erkl\u00e4ren,", "tokens": ["Viel", "M\u00fc\u00b7he", "gab", "er", "sich", ",", "in", "Bil\u00b7dern", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PRF", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dis die Bilder nur, und nicht die Dinge w\u00e4ren.", "tokens": ["Da\u00df", "dis", "die", "Bil\u00b7der", "nur", ",", "und", "nicht", "die", "Din\u00b7ge", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "ADV", "$,", "KON", "PTKNEG", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.21": {"line.1": {"text": "Doch sie verstanden's nicht