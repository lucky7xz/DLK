{"textgrid.poem.66557": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr scheltet auf die Verselei", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr scheltet auf die Verselei", "tokens": ["Ihr", "schel\u00b7tet", "auf", "die", "Ver\u00b7se\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und werft das Buch bei Seite,", "tokens": ["Und", "werft", "das", "Buch", "bei", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr f\u00fchlt euch innen und au\u00dfen frei", "tokens": ["Ihr", "f\u00fchlt", "euch", "in\u00b7nen", "und", "au\u00b7\u00dfen", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von jedem Widerstreite.", "tokens": ["Von", "je\u00b7dem", "Wi\u00b7der\u00b7strei\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da kommt in unruhvoller Stund", "tokens": ["Da", "kommt", "in", "un\u00b7ruh\u00b7vol\u00b7ler", "Stund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Buch euch in die H\u00e4nde,", "tokens": ["Das", "Buch", "euch", "in", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr les't, und les't euch gar gesund", "tokens": ["Ihr", "les't", ",", "und", "les't", "euch", "gar", "ge\u00b7sund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An der verschm\u00e4hten Spende.", "tokens": ["An", "der", "ver\u00b7schm\u00e4h\u00b7ten", "Spen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn endlich er gefangen nahm", "tokens": ["Wenn", "end\u00b7lich", "er", "ge\u00b7fan\u00b7gen", "nahm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Gegner oder Richter,", "tokens": ["Den", "Geg\u00b7ner", "o\u00b7der", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was hat f\u00fcr seinen Versekram", "tokens": ["Was", "hat", "f\u00fcr", "sei\u00b7nen", "Ver\u00b7se\u00b7kram"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu w\u00fcnschen noch der Dichter?", "tokens": ["Zu", "w\u00fcn\u00b7schen", "noch", "der", "Dich\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr scheltet auf die Verselei", "tokens": ["Ihr", "schel\u00b7tet", "auf", "die", "Ver\u00b7se\u00b7lei"], "token_