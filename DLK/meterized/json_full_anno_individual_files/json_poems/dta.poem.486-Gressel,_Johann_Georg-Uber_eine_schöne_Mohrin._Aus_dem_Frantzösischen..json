{"dta.poem.486": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber eine sch\u00f6ne Mohrin.  \n Aus dem Frantz\u00f6sischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Scheint gleich kein weisser Glantz von deinem Angesicht/", "tokens": ["Scheint", "gleich", "kein", "weis\u00b7ser", "Glantz", "von", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So l\u00f6scht die Schw\u00e4rtze doch der Liebe-Fackel nicht.", "tokens": ["So", "l\u00f6scht", "die", "Schw\u00e4rt\u00b7ze", "doch", "der", "Lie\u00b7be\u00b7Fa\u00b7ckel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Schwartzen sind auch sch\u00f6n/ ja sch\u00f6ner als die Weissen/", "tokens": ["Die", "Schwart\u00b7zen", "sind", "auch", "sch\u00f6n", "/", "ja", "sch\u00f6\u00b7ner", "als", "die", "Weis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wie der Alabast vor schwartzen ", "tokens": ["So", "wie", "der", "A\u00b7la\u00b7bast", "vor", "schwart\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie die Tanne nicht dem Eben-Holtze gleicht/", "tokens": ["Und", "wie", "die", "Tan\u00b7ne", "nicht", "dem", "E\u00b7ben\u00b7Holt\u00b7ze", "gleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So must du pr\u00e4chtiger als weisse Jungfern heissen.", "tokens": ["So", "must", "du", "pr\u00e4ch\u00b7ti\u00b7ger", "als", "weis\u00b7se", "Jung\u00b7fern", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}