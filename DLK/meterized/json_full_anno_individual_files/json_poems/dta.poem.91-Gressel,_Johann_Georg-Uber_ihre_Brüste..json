{"dta.poem.91": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber ihre Br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Besch\u00e4mte Rosen flieht/ weicht Liljen und Narcissen:", "tokens": ["Be\u00b7sch\u00e4m\u00b7te", "Ro\u00b7sen", "flieht", "/", "weicht", "Lil\u00b7jen", "und", "Nar\u00b7cis\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$(", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Semirens Busen l\u00e4st mich be\u00dfre Blumen k\u00fcssen/", "tokens": ["Se\u00b7mi\u00b7rens", "Bu\u00b7sen", "l\u00e4st", "mich", "be\u00df\u00b7re", "Blu\u00b7men", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Den Rosen/ so ihr seht auf ihren H\u00fcgeln bl\u00fchn/", "tokens": ["Den", "Ro\u00b7sen", "/", "so", "ihr", "seht", "auf", "ih\u00b7ren", "H\u00fc\u00b7geln", "bl\u00fchn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat ", "tokens": ["Hat"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Den Liljen/ die so sch\u00f6n sich auf zu bl\u00e4hen wissen/", "tokens": ["Den", "Lil\u00b7jen", "/", "die", "so", "sch\u00f6n", "sich", "auf", "zu", "bl\u00e4\u00b7hen", "wis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADJD", "PRF", "APPR", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat selbst der ", "tokens": ["Hat", "selbst", "der"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Die Wollust wil mit Macht sich recht darauf bem\u00fchn/", "tokens": ["Die", "Wol\u00b7lust", "wil", "mit", "Macht", "sich", "recht", "da\u00b7rauf", "be\u00b7m\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "PRF", "ADJD", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df dieser Wunder-Platz den Lippen vorzuziehn.", "tokens": ["Da\u00df", "die\u00b7ser", "Wun\u00b7der\u00b7Platz", "den", "Lip\u00b7pen", "vor\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier kan ein k\u00fcssend Mund mehr S\u00fcssigkeiten haben/", "tokens": ["Hier", "kan", "ein", "k\u00fcs\u00b7send", "Mund", "mehr", "S\u00fcs\u00b7sig\u00b7kei\u00b7ten", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJD", "NN", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als Sternen an der Burg des blauen Himmels stehn/", "tokens": ["Als", "Ster\u00b7nen", "an", "der", "Burg", "des", "blau\u00b7en", "Him\u00b7mels", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hymettens Bienen-Safft ein Honig von Athen", "tokens": ["Hy\u00b7met\u00b7tens", "Bie\u00b7nen\u00b7\u00b7S\u00b7afft", "ein", "Ho\u00b7nig", "von", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "APPR", "NE"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Verkehrt sich in Allaun bey diesen sch\u00f6nen Gaben.", "tokens": ["Ver\u00b7kehrt", "sich", "in", "Al\u00b7laun", "bey", "die\u00b7sen", "sch\u00f6\u00b7nen", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Seelen essen hier den Zucker wahrer Lust/", "tokens": ["Die", "See\u00b7len", "es\u00b7sen", "hier", "den", "Zu\u00b7cker", "wah\u00b7rer", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und sch\u00e4tzen sich begl\u00fcckt bey einer sch\u00f6nen Brust.", "tokens": ["Und", "sch\u00e4t\u00b7zen", "sich", "be\u00b7gl\u00fcckt", "bey", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}