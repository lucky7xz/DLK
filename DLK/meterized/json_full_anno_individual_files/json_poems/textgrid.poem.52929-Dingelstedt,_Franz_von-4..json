{"textgrid.poem.52929": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dft, was das bedeuten will? Du wirst sie mir nicht streichen?", "tokens": ["Du", "wei\u00dft", ",", "was", "das", "be\u00b7deu\u00b7ten", "will", "?", "Du", "wirst", "sie", "mir", "nicht", "strei\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PDS", "VVINF", "VMFIN", "$.", "PPER", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Es sind ja nur unschuldige \u2013 vier kleine Fragezeichen.", "tokens": ["Es", "sind", "ja", "nur", "un\u00b7schul\u00b7di\u00b7ge", "\u2013", "vier", "klei\u00b7ne", "Fra\u00b7ge\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "$(", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die wurzeln tief, die ragen hoch; wie die ger\u00fchmten Eichen", "tokens": ["Die", "wur\u00b7zeln", "tief", ",", "die", "ra\u00b7gen", "hoch", ";", "wie", "die", "ge\u00b7r\u00fchm\u00b7ten", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "VVFIN", "ADJD", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Des freien deutschen Volkes stehn vier kleine Fragezeichen.", "tokens": ["Des", "frei\u00b7en", "deut\u00b7schen", "Vol\u00b7kes", "stehn", "vier", "klei\u00b7ne", "Fra\u00b7ge\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Du wolltest sie zwar nimmer sehn in deinen weiten Reichen,", "tokens": ["Du", "woll\u00b7test", "sie", "zwar", "nim\u00b7mer", "sehn", "in", "dei\u00b7nen", "wei\u00b7ten", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Doch dr\u00e4ngen sie sich immer auf, vier kleine Fragezeichen.", "tokens": ["Doch", "dr\u00e4n\u00b7gen", "sie", "sich", "im\u00b7mer", "auf", ",", "vier", "klei\u00b7ne", "Fra\u00b7ge\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Wer wird denn so erschrocken sein und scheu vom Wege weichen,", "tokens": ["Wer", "wird", "denn", "so", "er\u00b7schro\u00b7cken", "sein", "und", "scheu", "vom", "We\u00b7ge", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "VVPP", "VAINF", "KON", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Wenn einem nichts begegnet als vier kleine Fragezeichen?", "tokens": ["Wenn", "ei\u00b7nem", "nichts", "be\u00b7geg\u00b7net", "als", "vier", "klei\u00b7ne", "Fra\u00b7ge\u00b7zei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "VVFIN", "KOKOM", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Gekr\u00fcmmt, gebeugt erscheinen sie, Hofr\u00e4ten zu vergleichen,", "tokens": ["Ge\u00b7kr\u00fcmmt", ",", "ge\u00b7beugt", "er\u00b7schei\u00b7nen", "sie", ",", "Hof\u00b7r\u00e4\u00b7ten", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VVFIN", "PPER", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Im S\u00e4cklein eine Handsupplik, vier kleine Fragezeichen;", "tokens": ["Im", "S\u00e4ck\u00b7lein", "ei\u00b7ne", "Han\u00b7dsupp\u00b7lik", ",", "vier", "klei\u00b7ne", "Fra\u00b7ge\u00b7zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Du wiesest sie hinweg von dir, nun schl\u00fcpfen sie und schleichen", "tokens": ["Du", "wie\u00b7sest", "sie", "hin\u00b7weg", "von", "dir", ",", "nun", "schl\u00fcp\u00b7fen", "sie", "und", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Umher im Volk und murmeln leis, vier kleine Fragezeichen.", "tokens": ["Um\u00b7her", "im", "Volk", "und", "mur\u00b7meln", "leis", ",", "vier", "klei\u00b7ne", "Fra\u00b7ge\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "VVFIN", "ADJD", "$,", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Zwar was sie wollen werden sie wohl nimmermehr erreichen,", "tokens": ["Zwar", "was", "sie", "wol\u00b7len", "wer\u00b7den", "sie", "wohl", "nim\u00b7mer\u00b7mehr", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VMFIN", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Allein sie bleiben, was sie sind, vier kleine Fragezeichen.", "tokens": ["Al\u00b7lein", "sie", "blei\u00b7ben", ",", "was", "sie", "sind", ",", "vier", "klei\u00b7ne", "Fra\u00b7ge\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$,", "PRELS", "PPER", "VAFIN", "$,", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Und einst, wann sie gestorben sind, erscheinen sie als Leichen", "tokens": ["Und", "einst", ",", "wann", "sie", "ge\u00b7stor\u00b7ben", "sind", ",", "er\u00b7schei\u00b7nen", "sie", "als", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Dir nachts im Traum und \u00e4rgern dich, vier kleine Fragezeichen,", "tokens": ["Dir", "nachts", "im", "Traum", "und", "\u00e4r\u00b7gern", "dich", ",", "vier", "klei\u00b7ne", "Fra\u00b7ge\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "KON", "VVFIN", "PPER", "$,", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+