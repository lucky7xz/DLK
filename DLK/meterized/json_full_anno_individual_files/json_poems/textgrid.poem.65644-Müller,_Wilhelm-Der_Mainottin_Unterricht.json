{"textgrid.poem.65644": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Mainottin Unterricht", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viele wei\u00dfe Schw\u00e4ne schwimmen still auf des Eurotas Wogen,", "tokens": ["Vie\u00b7le", "wei\u00b7\u00dfe", "Schw\u00e4\u00b7ne", "schwim\u00b7men", "still", "auf", "des", "Eu\u00b7ro\u00b7tas", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Viele schwarze Raben kommen kreischend durch die Luft gezogen.", "tokens": ["Vie\u00b7le", "schwar\u00b7ze", "Ra\u00b7ben", "kom\u00b7men", "krei\u00b7schend", "durch", "die", "Luft", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wei\u00dfe Schw\u00e4ne, woher schwimmt ihr? Wi\u00dft ihr Kunde nicht zu sagen,", "tokens": ["Wei\u00b7\u00dfe", "Schw\u00e4\u00b7ne", ",", "wo\u00b7her", "schwimmt", "ihr", "?", "Wi\u00dft", "ihr", "Kun\u00b7de", "nicht", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "$.", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ob mein Sohn sich wie ein Sparter in dem flachen Land geschlagen?", "tokens": ["Ob", "mein", "Sohn", "sich", "wie", "ein", "Spar\u00b7ter", "in", "dem", "fla\u00b7chen", "Land", "ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Schwarze Raben, woher fliegt ihr? \u2013 Saht ihr nicht auf euren Z\u00fcgen", "tokens": ["Schwar\u00b7ze", "Ra\u00b7ben", ",", "wo\u00b7her", "fliegt", "ihr", "?", "\u2013", "Saht", "ihr", "nicht", "auf", "eu\u00b7ren", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Viele blut'ge T\u00fcrkensch\u00e4del in den Siegesfeldern liegen?", "tokens": ["Vie\u00b7le", "blut'\u00b7ge", "T\u00fcr\u00b7ken\u00b7sch\u00e4\u00b7del", "in", "den", "Sie\u00b7ges\u00b7fel\u00b7dern", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "In den gr\u00fcnen Lorbeerstr\u00e4uchen, die zum Flusse niederschauen,", "tokens": ["In", "den", "gr\u00fc\u00b7nen", "Lor\u00b7beer\u00b7str\u00e4u\u00b7chen", ",", "die", "zum", "Flus\u00b7se", "nie\u00b7der\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Wo die Schw\u00e4ne ihre Nester unter dichtem Laube bauen,", "tokens": ["Wo", "die", "Schw\u00e4\u00b7ne", "ih\u00b7re", "Nes\u00b7ter", "un\u00b7ter", "dich\u00b7tem", "Lau\u00b7be", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "H\u00e4ngen viele wei\u00dfe Federn, die will ich zusammenraffen,", "tokens": ["H\u00e4n\u00b7gen", "vie\u00b7le", "wei\u00b7\u00dfe", "Fe\u00b7dern", ",", "die", "will", "ich", "zu\u00b7sam\u00b7men\u00b7raf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und daraus f\u00fcr meinen Knaben schneiden spitze K\u00f6cherwaffen;", "tokens": ["Und", "da\u00b7raus", "f\u00fcr", "mei\u00b7nen", "Kna\u00b7ben", "schnei\u00b7den", "spit\u00b7ze", "K\u00f6\u00b7cher\u00b7waf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "PPOSAT", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Will dann oben in den L\u00fcften zeigen ihm die schwarzen Raben,", "tokens": ["Will", "dann", "o\u00b7ben", "in", "den", "L\u00fcf\u00b7ten", "zei\u00b7gen", "ihm", "die", "schwar\u00b7zen", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Sag' ihm: Das sind ", "tokens": ["Sag'", "ihm", ":", "Das", "sind"], "token_info": ["word", "word", "punct", "word", "word"], "pos":