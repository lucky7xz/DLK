{"dta.poem.15591": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du ein Ungl\u00fcck ob dem N\u00e4chsten siehst verhangen,", "tokens": ["Wenn", "du", "ein", "Un\u00b7gl\u00fcck", "ob", "dem", "N\u00e4chs\u00b7ten", "siehst", "ver\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOUS", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hoffst du, weil ihn es traf, sei dirs vorbei gegangen.", "tokens": ["Hoffst", "du", ",", "weil", "ihn", "es", "traf", ",", "sei", "dirs", "vor\u00b7bei", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und f\u00fchlst du menschlicher, so dauert dich der Mann;", "tokens": ["Und", "f\u00fchlst", "du", "menschli\u00b7cher", ",", "so", "dau\u00b7ert", "dich", "der", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Warum? weil was ihn traf, auch dich betreffen kann.", "tokens": ["Wa\u00b7rum", "?", "weil", "was", "ihn", "traf", ",", "auch", "dich", "be\u00b7tref\u00b7fen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PWS", "PPER", "VVFIN", "$,", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was tr\u00e4gt es aus, ob warm du's aufnimmst oder k\u00fchl?", "tokens": ["Was", "tr\u00e4gt", "es", "aus", ",", "ob", "warm", "du's", "auf\u00b7nimmst", "o\u00b7der", "k\u00fchl", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADJD", "PIS", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So eigens\u00fcchtig ist Gef\u00fchl wie Ungef\u00fchl.", "tokens": ["So", "ei\u00b7gen\u00b7s\u00fcch\u00b7tig", "ist", "Ge\u00b7f\u00fchl", "wie", "Un\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}