{"dta.poem.11300": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Marcolphus verse machen wolte.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Marcolphus solt' unl\u00e4ngst begr\u00e4bni\u00df-verse schreiben.", "tokens": ["Mar\u00b7col\u00b7phus", "solt'", "un\u00b7l\u00e4ngst", "be\u00b7gr\u00e4b\u00b7ni\u00df\u00b7ver\u00b7se", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADJA", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Weil er nun gerne wolt\u2019 ein guter tichter seyn,", "tokens": ["Weil", "er", "nun", "ger\u00b7ne", "wolt'", "ein", "gu\u00b7ter", "tich\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und starcke tr\u00e4ncke sonst geschickte reime treiben;", "tokens": ["Und", "star\u00b7cke", "tr\u00e4n\u00b7cke", "sonst", "ge\u00b7schick\u00b7te", "rei\u00b7me", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ADV", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erkaufft er sich zuvor ein maas zwey-groschen-wein.", "tokens": ["Er\u00b7kaufft", "er", "sich", "zu\u00b7vor", "ein", "maas", "zwey\u00b7gro\u00b7schen\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wolt\u2019 er noch darauf zur tichter queck-brunn hincken;", "tokens": ["Denn", "wolt'", "er", "noch", "da\u00b7rauf", "zur", "tich\u00b7ter", "queck\u00b7brunn", "hin\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PAV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein er war berauscht, drum fehlt er in dem trincken,", "tokens": ["Al\u00b7lein", "er", "war", "be\u00b7rauscht", ",", "drum", "fehlt", "er", "in", "dem", "trin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$,", "PAV", "VVFIN", "PPER", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und traff ich wei\u00df nicht was vor eine mist-pf\u00fctz an;", "tokens": ["Und", "traff", "ich", "wei\u00df", "nicht", "was", "vor", "ei\u00b7ne", "mist\u00b7pf\u00fctz", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKNEG", "PWS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was wunder, wann er nun, wie fr\u00f6sche, fingen kan?", "tokens": ["Was", "wun\u00b7der", ",", "wann", "er", "nun", ",", "wie", "fr\u00f6\u00b7sche", ",", "fin\u00b7gen", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PWAV", "PPER", "ADV", "$,", "PWAV", "ADJA", "$,", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}