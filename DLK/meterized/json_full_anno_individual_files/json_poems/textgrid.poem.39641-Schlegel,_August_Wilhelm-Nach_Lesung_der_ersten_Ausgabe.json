{"textgrid.poem.39641": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Nach Lesung der ersten Ausgabe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich achte sehr die Curien,", "tokens": ["Ich", "ach\u00b7te", "sehr", "die", "Cu\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tribus, die Centurien,", "tokens": ["Die", "Tri\u00b7bus", ",", "die", "Cen\u00b7tu\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auspicien, Augurien,", "tokens": ["Aus\u00b7pi\u00b7ci\u00b7en", ",", "Au\u00b7gu\u00b7ri\u00b7en", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahrsager aus Etrurien,", "tokens": ["Wahr\u00b7sa\u00b7ger", "aus", "E\u00b7tru\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und n\u00e4chtliche Lemurien.", "tokens": ["Und", "n\u00e4cht\u00b7li\u00b7che", "Le\u00b7mu\u00b7ri\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch schw\u00f6r' ich bei Veturien,", "tokens": ["Doch", "schw\u00f6r'", "ich", "bei", "Ve\u00b7tu\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich wollte, da\u00df die Furien", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "die", "Fu\u00b7ri\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verschl\u00e4ngen die Centurien", "tokens": ["Ver\u00b7schl\u00e4n\u00b7gen", "die", "Cen\u00b7tu\u00b7ri\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sammt allen drei\u00dfig Curien", "tokens": ["Sammt", "al\u00b7len", "drei\u00b7\u00dfig", "Cu\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und tausend Sammelsurien,", "tokens": ["Und", "tau\u00b7send", "Sam\u00b7mel\u00b7su\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von Zankle bis Ligurien,", "tokens": ["Von", "Zank\u00b7le", "bis", "Li\u00b7gu\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Im neuesten Niebuhrien.", "tokens": ["Im", "neu\u00b7es\u00b7ten", "Nie\u00b7buh\u00b7ri\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+---", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich achte sehr die Curien,", "tokens": ["Ich", "ach\u00b7te", "sehr", "die", "Cu\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tribus, die Centurien,", "tokens"