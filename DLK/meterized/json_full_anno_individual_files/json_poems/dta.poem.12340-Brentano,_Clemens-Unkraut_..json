{"dta.poem.12340": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Unkraut .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unkraut. Wie kommt's, da\u00df du so traurig bist,               ", "tokens": ["Un\u00b7kraut", ".", "Wie", "kommt's", ",", "da\u00df", "du", "so", "trau\u00b7rig", "bist", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und gar nicht einmahl lachst?", "tokens": ["Und", "gar", "nicht", "ein\u00b7mahl", "lachst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh dir's an den Augen an,", "tokens": ["Ich", "seh", "dir's", "an", "den", "Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du geweinet hast.", "tokens": ["Da\u00df", "du", "ge\u00b7wei\u00b7net", "hast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "G\u00e4rtner. Und wer ein'n stein'gen Acker hat,               ", "tokens": ["G\u00e4rt\u00b7ner", ".", "Und", "wer", "ein'n", "stein'\u00b7gen", "A\u00b7cker", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PWS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dazu 'nen stumpfen Pflug,", "tokens": ["Da\u00b7zu", "'nen", "stump\u00b7fen", "Pflug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dessen Schatz zum Schelmen wird,", "tokens": ["Und", "des\u00b7sen", "Schatz", "zum", "Schel\u00b7men", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat der nicht Kreutz genug?", "tokens": ["Hat", "der", "nicht", "Kreutz", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PTKNEG", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Unkraut. Doch wer mit Katzen ackern will,               ", "tokens": ["Un\u00b7kraut", ".", "Doch", "wer", "mit", "Kat\u00b7zen", "ac\u00b7kern", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PWS", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der spann die M\u00e4us voraus,", "tokens": ["Der", "spann", "die", "M\u00e4us", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So geht es alles wie ein Wind,", "tokens": ["So", "geht", "es", "al\u00b7les", "wie", "ein", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So f\u00e4ngt die Katz die Maus.", "tokens": ["So", "f\u00e4ngt", "die", "Katz", "die", "Maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hab all mein Tag kein Gut gethan,", "tokens": ["Hab", "all", "mein", "Tag", "kein", "Gut", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab's auch noch nicht im Sinn;", "tokens": ["Hab's", "auch", "noch", "nicht", "im", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ganze Freundschaft wei\u00df es ja,", "tokens": ["Die", "gan\u00b7ze", "Freund\u00b7schaft", "wei\u00df", "es", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich ein Unkraut bin.", "tokens": ["Da\u00df", "ich", "ein", "Un\u00b7kraut", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}