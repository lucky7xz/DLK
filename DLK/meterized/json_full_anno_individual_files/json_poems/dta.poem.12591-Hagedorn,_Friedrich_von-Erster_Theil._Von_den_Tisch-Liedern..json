{"dta.poem.12591": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du, die du bey dem Flu\u00dfe Triton", "tokens": ["Du", ",", "die", "du", "bey", "dem", "Flu\u00b7\u00dfe", "Tri\u00b7ton"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Welt zuerst erschienen bist,", "tokens": ["Der", "Welt", "zu\u00b7erst", "er\u00b7schie\u00b7nen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Pallas, K\u00f6niginn Athens!", "tokens": ["O", "Pal\u00b7las", ",", "K\u00f6\u00b7ni\u00b7ginn", "A\u00b7thens", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O sch\u00fctz\u2019 Athen und seine B\u00fcrger", "tokens": ["O", "sch\u00fctz'", "A\u00b7then", "und", "sei\u00b7ne", "B\u00fcr\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "KON", "PPOSAT", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Ungl\u00fcck, Aufruhr, fr\u00fchem Sterben.", "tokens": ["Vor", "Un\u00b7gl\u00fcck", ",", "Auf\u00b7ruhr", ",", "fr\u00fc\u00b7hem", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sch\u00fctze du sie auch, o Vater dieser G\u00f6ttinn!", "tokens": ["Und", "sch\u00fct\u00b7ze", "du", "sie", "auch", ",", "o", "Va\u00b7ter", "die\u00b7ser", "G\u00f6t\u00b7tinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "$,", "FM", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jetzt, da wir bekr\u00e4nzet sind,", "tokens": ["Jetzt", ",", "da", "wir", "be\u00b7kr\u00e4n\u00b7zet", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will ich dir, o Mutter Plutons!", "tokens": ["Will", "ich", "dir", ",", "o", "Mut\u00b7ter", "Plu\u00b7tons", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "$,", "FM", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir, o Ceres! will ich singen.", "tokens": ["Dir", ",", "o", "Ce\u00b7res", "!", "will", "ich", "sin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NE", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey gegr\u00fcsset, grosse Ceres!", "tokens": ["Sey", "ge\u00b7gr\u00fcs\u00b7set", ",", "gros\u00b7se", "Ce\u00b7res", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du, Tochter Jupiters,", "tokens": ["Und", "du", ",", "Toch\u00b7ter", "Ju\u00b7pi\u00b7ters", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Proserpine, sey gegr\u00fcsset!", "tokens": ["Pro\u00b7ser\u00b7pi\u00b7ne", ",", "sey", "ge\u00b7gr\u00fcs\u00b7set", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00fctzet beyde diese Stadt!", "tokens": ["Sch\u00fct\u00b7zet", "bey\u00b7de", "die\u00b7se", "Stadt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In Delus bracht\u2019 einst die Latone", "tokens": ["In", "De\u00b7lus", "bracht'", "einst", "die", "La\u00b7to\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwey Kinder auf die Welt;", "tokens": ["Zwey", "Kin\u00b7der", "auf", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Ph\u00f6bus mit den g\u00fcldnen Haaren,", "tokens": ["Den", "Ph\u00f6\u00b7bus", "mit", "den", "g\u00fcld\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Diane, so das Jagen liebt,", "tokens": ["Di\u00b7a\u00b7ne", ",", "so", "das", "Ja\u00b7gen", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die keinen Pfeil vergebens schiesset,", "tokens": ["Die", "kei\u00b7nen", "Pfeil", "ver\u00b7ge\u00b7bens", "schies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcber alle Weiber herrscht.", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7le", "Wei\u00b7ber", "herrscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Schutzgott der Arcadier,", "tokens": ["O", "Schutz\u00b7gott", "der", "Ar\u00b7ca\u00b7dier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O Pan! du so ber\u00fchmter T\u00e4nzer,", "tokens": ["O", "Pan", "!", "du", "so", "be\u00b7r\u00fchm\u00b7ter", "T\u00e4n\u00b7zer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der du den muntern Nymphen nachl\u00e4ufst,", "tokens": ["Der", "du", "den", "mun\u00b7tern", "Nym\u00b7phen", "nach\u00b7l\u00e4ufst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die lachend sich vor dir verstecken.", "tokens": ["Die", "la\u00b7chend", "sich", "vor", "dir", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erscheine doch bey unsern Freuden,", "tokens": ["Er\u00b7schei\u00b7ne", "doch", "bey", "un\u00b7sern", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Pan! erschein\u2019 in unsern Liedern", "tokens": ["O", "Pan", "!", "er\u00b7schein'", "in", "un\u00b7sern", "Lie\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Stets munter und wohlaufger\u00e4umt.", "tokens": ["Stets", "mun\u00b7ter", "und", "wohl\u00b7auf\u00b7ge\u00b7r\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun haben wir den Feind geschlagen,", "tokens": ["Nun", "ha\u00b7ben", "wir", "den", "Feind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und so, wie wir gew\u00fcnscht, den Sieg davon getragen;", "tokens": ["Und", "so", ",", "wie", "wir", "ge\u00b7w\u00fcnscht", ",", "den", "Sieg", "da\u00b7von", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die G\u00f6tter haben ihn verliehn.", "tokens": ["Die", "G\u00f6t\u00b7ter", "ha\u00b7ben", "ihn", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, ja, die G\u00f6tter haben ihn", "tokens": ["Ja", ",", "ja", ",", "die", "G\u00f6t\u00b7ter", "ha\u00b7ben", "ihn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "ART", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir, o Athen, Pandrosens Vaterland!", "tokens": ["Dir", ",", "o", "A\u00b7then", ",", "Pand\u00b7ro\u00b7sens", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NE", "$,", "NN", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Das ihnen lieb ist, zugewandt.", "tokens": ["Das", "ih\u00b7nen", "lieb", "ist", ",", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}