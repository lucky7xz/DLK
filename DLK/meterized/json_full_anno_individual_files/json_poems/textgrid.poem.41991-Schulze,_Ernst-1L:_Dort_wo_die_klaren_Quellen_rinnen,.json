{"textgrid.poem.41991": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort wo die klaren Quellen rinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort wo die klaren Quellen rinnen,", "tokens": ["Dort", "wo", "die", "kla\u00b7ren", "Quel\u00b7len", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seht ihr das H\u00fcttchen dort wohl stehn?", "tokens": ["Seht", "ihr", "das", "H\u00fctt\u00b7chen", "dort", "wohl", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort wohnt von allen Sch\u00e4ferinnen", "tokens": ["Dort", "wohnt", "von", "al\u00b7len", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sch\u00f6nste, die ich je gesehn.", "tokens": ["Die", "Sch\u00f6ns\u00b7te", ",", "die", "ich", "je", "ge\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und b\u00f6te man mir Gold und Kronen,", "tokens": ["Und", "b\u00f6\u00b7te", "man", "mir", "Gold", "und", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So d\u00e4cht' ich doch in meinem Sinn:", "tokens": ["So", "d\u00e4cht'", "ich", "doch", "in", "mei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im H\u00fcttchen m\u00f6cht' ich lieber wohnen", "tokens": ["Im", "H\u00fctt\u00b7chen", "m\u00f6cht'", "ich", "lie\u00b7ber", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bei der sch\u00f6nen Sch\u00e4ferin.", "tokens": ["Und", "bei", "der", "sch\u00f6\u00b7nen", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort durch die dichten Bl\u00fcthenb\u00e4ume", "tokens": ["Dort", "durch", "die", "dich\u00b7ten", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kannst du ihr Fensterlein ersp\u00e4hn,", "tokens": ["Kannst", "du", "ihr", "Fens\u00b7ter\u00b7lein", "er\u00b7sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich wache, wenn ich tr\u00e4ume,", "tokens": ["Und", "wenn", "ich", "wa\u00b7che", ",", "wenn", "ich", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Fenster mu\u00df ich immer sehn;", "tokens": ["Das", "Fens\u00b7ter", "mu\u00df", "ich", "im\u00b7mer", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn durch die gr\u00fcnumrankten Scheiben", "tokens": ["Denn", "durch", "die", "gr\u00fc\u00b7num\u00b7rank\u00b7ten", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schaut sie mich gar zu freundlich an,", "tokens": ["Schaut", "sie", "mich", "gar", "zu", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKA", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So freundlich, da\u00df ich's nicht beschreiben", "tokens": ["So", "freund\u00b7lich", ",", "da\u00df", "ich's", "nicht", "be\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PIS", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und da\u00df ich's kaum ertragen kann.", "tokens": ["Und", "da\u00df", "ich's", "kaum", "er\u00b7tra\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Treibt sie im fr\u00fchen Morgenschimmer", "tokens": ["Treibt", "sie", "im", "fr\u00fc\u00b7hen", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die zarten L\u00e4mmchen in den Hain,", "tokens": ["Die", "zar\u00b7ten", "L\u00e4mm\u00b7chen", "in", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann sagt mein armes Herz mir immer:", "tokens": ["Dann", "sagt", "mein", "ar\u00b7mes", "Herz", "mir", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ach, m\u00f6chtest du ihr Sch\u00e4fer seyn!", "tokens": ["Ach", ",", "m\u00f6ch\u00b7test", "du", "ihr", "Sch\u00e4\u00b7fer", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und heimlich nehm' ich meine Fl\u00f6te", "tokens": ["Und", "heim\u00b7lich", "nehm'", "ich", "mei\u00b7ne", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schleich' um ihre Triften her,", "tokens": ["Und", "schleich'", "um", "ih\u00b7re", "Trif\u00b7ten", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn man mir den Himmel b\u00f6te,", "tokens": ["Und", "wenn", "man", "mir", "den", "Him\u00b7mel", "b\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Hause kehrt' ich nimmermehr.", "tokens": ["Zu", "Hau\u00b7se", "kehrt'", "ich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wenn die goldnen Sterne blinken,", "tokens": ["Und", "wenn", "die", "gold\u00b7nen", "Ster\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeder Laut im Haine ruht,", "tokens": ["Und", "je\u00b7der", "Laut", "im", "Hai\u00b7ne", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann l\u00e4\u00dft sie ihren Schleier sinken,", "tokens": ["Dann", "l\u00e4\u00dft", "sie", "ih\u00b7ren", "Schlei\u00b7er", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und taucht sich in die klare Fluth.", "tokens": ["Und", "taucht", "sich", "in", "die", "kla\u00b7re", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie gern w\u00fcrd' ich sie dann belauschen:", "tokens": ["Wie", "gern", "w\u00fcrd'", "ich", "sie", "dann", "be\u00b7lau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wag' ich's nicht hinzuzugehn,", "tokens": ["Doch", "wag'", "ich's", "nicht", "hin\u00b7zu\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn ach, es k\u00f6nnt ein Bl\u00e4ttchen rauschen,", "tokens": ["Denn", "ach", ",", "es", "k\u00f6nnt", "ein", "Bl\u00e4tt\u00b7chen", "rau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nie d\u00fcrft' ich sie wiedersehn.", "tokens": ["Und", "nie", "d\u00fcrft'", "ich", "sie", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auch wenn des Nachts die Elfen weben,", "tokens": ["Auch", "wenn", "des", "Nachts", "die", "El\u00b7fen", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlich' ich mich gern zum Fensterlein,", "tokens": ["Schlich'", "ich", "mich", "gern", "zum", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fl\u00fcsterte: Mach auf, mein Leben,", "tokens": ["Und", "fl\u00fcs\u00b7ter\u00b7te", ":", "Mach", "auf", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und la\u00df in's H\u00fcttchen mich hinein.", "tokens": ["Und", "la\u00df", "in's", "H\u00fctt\u00b7chen", "mich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch m\u00f6chte sie mich kommen h\u00f6ren;", "tokens": ["Doch", "m\u00f6ch\u00b7te", "sie", "mich", "kom\u00b7men", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein leises Klopfen an der Th\u00fcr", "tokens": ["Mein", "lei\u00b7ses", "Klop\u00b7fen", "an", "der", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es k\u00f6nnte sie im Schlummer st\u00f6ren,", "tokens": ["Es", "k\u00f6nn\u00b7te", "sie", "im", "Schlum\u00b7mer", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Drum bleib' ich lieber einsam hier.", "tokens": ["Drum", "bleib'", "ich", "lie\u00b7ber", "ein\u00b7sam", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Oft wollt' es mir im Traume scheinen,", "tokens": ["Oft", "wollt'", "es", "mir", "im", "Trau\u00b7me", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als wandelten wir Hand in Hand,", "tokens": ["Als", "wan\u00b7del\u00b7ten", "wir", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer mu\u00dft' ich herzlich weinen,", "tokens": ["Und", "im\u00b7mer", "mu\u00dft'", "ich", "herz\u00b7lich", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich beim Wachen sie nicht fand.", "tokens": ["Wenn", "ich", "beim", "Wa\u00b7chen", "sie", "nicht", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "D\u00fcrft' ich mein Leiden ihr nur klagen,", "tokens": ["D\u00fcrft'", "ich", "mein", "Lei\u00b7den", "ihr", "nur", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gewi\u00df, sie sagte mir alsdann,", "tokens": ["Ge\u00b7wi\u00df", ",", "sie", "sag\u00b7te", "mir", "als\u00b7dann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Warum mein Herz f\u00fcr sie nur schlagen,", "tokens": ["Wa\u00b7rum", "mein", 