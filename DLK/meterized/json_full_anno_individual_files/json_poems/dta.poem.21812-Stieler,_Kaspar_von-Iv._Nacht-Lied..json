{"dta.poem.21812": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Nacht-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Du blasser Mohn/", "tokens": ["Du", "blas\u00b7ser", "Mohn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "weistu auch was darvon/", "tokens": ["weis\u00b7tu", "auch", "was", "dar\u00b7von", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PWS", "PAV", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "da\u00df ich hieunten klage?", "tokens": ["da\u00df", "ich", "hie\u00b7un\u00b7ten", "kla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du silber-heer/", "tokens": ["Du", "sil\u00b7ber\u00b7heer", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "der Tr\u00e4hnen-Plage?", "tokens": ["der", "Tr\u00e4h\u00b7nen\u00b7Pla\u00b7ge", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das weisse Licht", "tokens": ["Das", "weis\u00b7se", "Licht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "der Sternen achtet nicht", "tokens": ["der", "Ster\u00b7nen", "ach\u00b7tet", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mein unertr\u00e4glichs Leiden:", "tokens": ["mein", "un\u00b7er\u00b7tr\u00e4g\u00b7lichs", "Lei\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "verwandeln diesen Flu\u00df", "tokens": ["ver\u00b7wan\u00b7deln", "die\u00b7sen", "Flu\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "in Lust und Freuden.", "tokens": ["in", "Lust", "und", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wie offters trug", "tokens": ["Wie", "off\u00b7ters", "trug"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "der tr\u00fcben Wolken-zug", "tokens": ["der", "tr\u00fc\u00b7ben", "Wol\u00b7ken\u00b7zug"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erbarmn\u00fc\u00df mit mir Schwachen!", "tokens": ["Er\u00b7barm\u00b7n\u00fc\u00df", "mit", "mir", "Schwa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Schmerzen-Lied", "tokens": ["Mein", "Schmer\u00b7zen\u00b7Lied"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "K\u00fcnt\u2019 ihr bewegt Gem\u00fcht\u2019", "tokens": ["K\u00fcnt'", "ihr", "be\u00b7wegt", "Ge\u00b7m\u00fcht'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "auch weinend machen.", "tokens": ["auch", "wei\u00b7nend", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Himmel ri\u00df\u2019", "tokens": ["Der", "Him\u00b7mel", "ri\u00df'"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "auff mein Bek\u00fcmmern\u00fc\u00df", "tokens": ["auff", "mein", "Be\u00b7k\u00fcm\u00b7mer\u00b7n\u00fc\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit Hagel und mit Schlossen/", "tokens": ["mit", "Ha\u00b7gel", "und", "mit", "Schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "weil meine Brunst", "tokens": ["weil", "mei\u00b7ne", "Brunst"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "von der Geliebten Gunst", "tokens": ["von", "der", "Ge\u00b7lieb\u00b7ten", "Gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "wurd\u2019 au\u00dfgestossen.", "tokens": ["wurd'", "au\u00df\u00b7ge\u00b7stos\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Latern und Licht/", "tokens": ["La\u00b7tern", "und", "Licht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "entdekket mich nur nicht!", "tokens": ["ent\u00b7dek\u00b7ket", "mich", "nur", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "kehrt ab das Judas-Feuer.", "tokens": ["kehrt", "ab", "das", "Ju\u00b7das\u00b7Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schaut mir nicht nach/", "tokens": ["Schaut", "mir", "nicht", "nach", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "ihr Leute/ was ich mach\u2019", "tokens": ["ihr", "Leu\u00b7te", "/", "was", "ich", "mach'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "ich armer Freyer.", "tokens": ["ich", "ar\u00b7mer", "Frey\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Geht mich vorbey", "tokens": ["Geht", "mich", "vor\u00b7bey"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "und fraget nicht/ wer ich sey/", "tokens": ["und", "fra\u00b7get", "nicht", "/", "wer", "ich", "sey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "doch/ wird mich wer erkennen:", "tokens": ["doch", "/", "wird", "mich", "wer", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VAFIN", "PPER", "PWS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der werde stumm.", "tokens": ["Der", "wer\u00b7de", "stumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "ich wil mich hier kurz um", "tokens": ["ich", "wil", "mich", "hier", "kurz", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "nicht lassen nennen.", "tokens": ["nicht", "las\u00b7sen", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Schweert und beteurt", "tokens": ["Schweert", "und", "be\u00b7teurt"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "bey Ammon der da feurt", "tokens": ["bey", "Am\u00b7mon", "der", "da", "feurt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit Blizz und Donner-schl\u00e4gen:", "tokens": ["mit", "Blizz", "und", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "es sey niemand/", "tokens": ["es", "sey", "nie\u00b7mand", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "als der euch unbekant", "tokens": ["als", "der", "euch", "un\u00b7be\u00b7kant"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "gewest zugegen.", "tokens": ["ge\u00b7west", "zu\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "So w\u00fcntsch\u2019 ich euch/", "tokens": ["So", "w\u00fcnt\u00b7sch'", "ich", "euch", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "da\u00df ihr in Amors Reich\u2019", "tokens": ["da\u00df", "ihr", "in", "A\u00b7mors", "Reich'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "erfreuet m\u00f6get wohnen.", "tokens": ["er\u00b7freu\u00b7et", "m\u00f6\u00b7get", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es fall\u2019 euch Ruh/", "tokens": ["Es", "fall'", "euch", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Lust und Vergn\u00fcgen zu", "tokens": ["Lust", "und", "Ver\u00b7gn\u00fc\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PTKZU"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "bey der Dionen!", "tokens": ["bey", "der", "Di\u00b7o\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}