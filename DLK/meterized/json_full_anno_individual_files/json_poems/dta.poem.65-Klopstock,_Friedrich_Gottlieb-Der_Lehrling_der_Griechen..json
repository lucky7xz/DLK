{"dta.poem.65": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der Lehrling der Griechen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1771", "urn": "urn:nbn:de:kobv:b4-200905199486", "language": ["de:0.99"], "booktitle": "[Klopstock, Friedrich Gottlieb]: Oden. Hamburg, 1771."}, "poem": {"stanza.1": {"line.1": {"text": "Wen des Genius Blick, als er gebohren ward,", "tokens": ["Wen", "des", "Ge\u00b7nius", "Blick", ",", "als", "er", "ge\u00b7boh\u00b7ren", "ward", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mit einweichendem L\u00e4cheln sah,", "tokens": ["Mit", "ein\u00b7wei\u00b7chen\u00b7dem", "L\u00e4\u00b7cheln", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wen, als Knaben, ihr einst Smintheus Anakreons", "tokens": ["Wen", ",", "als", "Kna\u00b7ben", ",", "ihr", "einst", "Smin\u00b7theus", "A\u00b7nak\u00b7re\u00b7ons"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "$,", "KOUS", "NN", "$,", "PPER", "ADV", "NE", "NE"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Fabelhafte Gespielinnen,", "tokens": ["Fa\u00b7bel\u00b7haf\u00b7te", "Ge\u00b7spie\u00b7lin\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dichtrische Tauben umflogt, und sein m\u00e4onisch Ohr", "tokens": ["Dich\u00b7tri\u00b7sche", "Tau\u00b7ben", "um\u00b7flogt", ",", "und", "sein", "m\u00e4o\u00b7nisch", "Ohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "KON", "PPOSAT", "ADJD", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vor dem L\u00e4rme der Scholien", "tokens": ["Vor", "dem", "L\u00e4r\u00b7me", "der", "Scho\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Sanft zugirrtet, und ihm, da\u00df er das Alterthum", "tokens": ["Sanft", "zu\u00b7girr\u00b7tet", ",", "und", "ihm", ",", "da\u00df", "er", "das", "Al\u00b7ter\u00b7thum"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "KON", "PPER", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Ihrer faltigen Stirn nicht sah,", "tokens": ["Ih\u00b7rer", "fal\u00b7ti\u00b7gen", "Stirn", "nicht", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Eure Fittige lieht, und ihn umschattetet,", "tokens": ["Eu\u00b7re", "Fit\u00b7ti\u00b7ge", "lieht", ",", "und", "ihn", "um\u00b7schat\u00b7te\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Den ruft, stolz auf den Lorberkranz,", "tokens": ["Den", "ruft", ",", "stolz", "auf", "den", "Lor\u00b7ber\u00b7kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Welcher vom Fluche des Volks welkt, der Eroberer", "tokens": ["Wel\u00b7cher", "vom", "Flu\u00b7che", "des", "Volks", "welkt", ",", "der", "Er\u00b7o\u00b7be\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "APPRART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.12": {"text": "In das eiserne Feld umsonst,", "tokens": ["In", "das", "ei\u00b7ser\u00b7ne", "Feld", "um\u00b7sonst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Wo kein m\u00fctterlich Ach b\u00e4nger beym Scheideku\u00df,", "tokens": ["Wo", "kein", "m\u00fct\u00b7ter\u00b7lich", "Ach", "b\u00e4n\u00b7ger", "beym", "Schei\u00b7de\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJD", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und aus blutender Brust geseufzt,", "tokens": ["Und", "aus", "blu\u00b7ten\u00b7der", "Brust", "ge\u00b7seufzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Ihren sterbenden Sohn dir, unerbittlicher", "tokens": ["Ih\u00b7ren", "ster\u00b7ben\u00b7den", "Sohn", "dir", ",", "un\u00b7er\u00b7bitt\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$,", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Hundertarmiger Tod, entrei\u00dft!", "tokens": ["Hun\u00b7der\u00b7tar\u00b7mi\u00b7ger", "Tod", ",", "ent\u00b7rei\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Wenn das Schicksal ihn ja K\u00f6nigen zugesellt,", "tokens": ["Wenn", "das", "Schick\u00b7sal", "ihn", "ja", "K\u00f6\u00b7ni\u00b7gen", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Umgew\u00f6hnt zu dem Waffenklang,", "tokens": ["Um\u00b7ge\u00b7w\u00f6hnt", "zu", "dem", "Waf\u00b7fen\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "Sieht er, von richtendem Ernst schauernd, die Leich-", "tokens": ["Sieht", "er", ",", "von", "rich\u00b7ten\u00b7dem", "Ernst", "schau\u00b7ernd", ",", "die", "Leich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,", "ART", "TRUNC"], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Stumm und seelenlos ausgestreckt,", "tokens": ["Stumm", "und", "see\u00b7len\u00b7los", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Segnet dem fliehenden Geist in die Gefilde nach,", "tokens": ["Seg\u00b7net", "dem", "flie\u00b7hen\u00b7den", "Geist", "in", "die", "Ge\u00b7fil\u00b7de", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.22": {"text": "Wo kein t\u00f6dtender Held mehr siegt.", "tokens": ["Wo", "kein", "t\u00f6d\u00b7ten\u00b7der", "Held", "mehr", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Ihn l\u00e4\u00dft g\u00fctiges Lob, oder Unsterblichkeit", "tokens": ["Ihn", "l\u00e4\u00dft", "g\u00fc\u00b7ti\u00b7ges", "Lob", ",", "o\u00b7der", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "KON", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "De\u00df, der Ehre vergeudet, kalt!", "tokens": ["De\u00df", ",", "der", "Eh\u00b7re", "ver\u00b7geu\u00b7det", ",", "kalt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "VVFIN", "$,", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "Kalt der wartende Thor, welcher bewundernsvoll", "tokens": ["Kalt", "der", "war\u00b7ten\u00b7de", "Thor", ",", "wel\u00b7cher", "be\u00b7wun\u00b7derns\u00b7voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.26": {"text": "Ihn gro\u00df\u00e4ugigten Freunden zeigt,", "tokens": ["Ihn", "gro\u00df\u00b7\u00e4u\u00b7gig\u00b7ten", "Freun\u00b7den", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.27": {"text": "Und der l\u00e4chelnde Blick einer nur sch\u00f6nen Frau,", "tokens": ["Und", "der", "l\u00e4\u00b7cheln\u00b7de", "Blick", "ei\u00b7ner", "nur", "sch\u00f6\u00b7nen", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "Der zu dunkel die Singer ist.", "tokens": ["Der", "zu", "dun\u00b7kel", "die", "Sin\u00b7ger", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.29": {"text": "Thr\u00e4nen nach besserem Ruhm werden Unsterblichen,", "tokens": ["Thr\u00e4\u00b7nen", "nach", "bes\u00b7se\u00b7rem", "Ruhm", "wer\u00b7den", "U\u00b7nsterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "+--+--+--+---", "measure": "dactylic.tetra.plus"}, "line.30": {"text": "Jenen alten Unsterblichen,", "tokens": ["Je\u00b7nen", "al\u00b7ten", "U\u00b7nsterb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.31": {"text": "Deren daurender Werth, wachsenden Str\u00f6men gleich,", "tokens": ["De\u00b7ren", "dau\u00b7ren\u00b7der", "Werth", ",", "wach\u00b7sen\u00b7den", "Str\u00f6\u00b7men", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Jedes lange Jahrhundert f\u00fcllt,", "tokens": ["Je\u00b7des", "lan\u00b7ge", "Jahr\u00b7hun\u00b7dert", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.33": {"text": "Ihn gesellen, und ihn jenen Belohnungen,", "tokens": ["Ihn", "ge\u00b7sel\u00b7len", ",", "und", "ihn", "je\u00b7nen", "Be\u00b7loh\u00b7nun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "KON", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Die der Stolze nur tr\u00e4umte, weihn!", "tokens": ["Die", "der", "Stol\u00b7ze", "nur", "tr\u00e4um\u00b7te", ",", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "ADJA", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Ihm ist, wenn ihm das Gl\u00fcck, was es so selten that,", "tokens": ["Ihm", "ist", ",", "wenn", "ihm", "das", "Gl\u00fcck", ",", "was", "es", "so", "sel\u00b7ten", "that", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Eine denkende Freundin giebt,", "tokens": ["Ei\u00b7ne", "den\u00b7ken\u00b7de", "Freun\u00b7din", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.37": {"text": "Jede Z\u00e4hre von ihr, die ihr sein Werk entlockt,", "tokens": ["Je\u00b7de", "Z\u00e4h\u00b7re", "von", "ihr", ",", "die", "ihr", "sein", "Werk", "ent\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "K\u00fcnftiger Z\u00e4hren Verk\u00fcnderin!", "tokens": ["K\u00fcnf\u00b7ti\u00b7ger", "Z\u00e4h\u00b7ren", "Ver\u00b7k\u00fcn\u00b7de\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}