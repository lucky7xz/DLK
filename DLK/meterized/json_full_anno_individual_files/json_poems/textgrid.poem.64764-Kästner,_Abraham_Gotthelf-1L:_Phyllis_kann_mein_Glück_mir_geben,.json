{"textgrid.poem.64764": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Phyllis kann mein Gl\u00fcck mir geben,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Phyllis kann mein Gl\u00fcck mir geben,", "tokens": ["Phyl\u00b7lis", "kann", "mein", "Gl\u00fcck", "mir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch sie macht mir keine Pein;", "tokens": ["Doch", "sie", "macht", "mir", "kei\u00b7ne", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterthan will ich ihr leben,", "tokens": ["Un\u00b7ter\u00b7than", "will", "ich", "ihr", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber nicht ihr Sklave seyn:", "tokens": ["A\u00b7ber", "nicht", "ihr", "Skla\u00b7ve", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So soll sie kein Herz verehren,", "tokens": ["So", "soll", "sie", "kein", "Herz", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie mein Herze sie verehrt;", "tokens": ["Wie", "mein", "Her\u00b7ze", "sie", "ver\u0