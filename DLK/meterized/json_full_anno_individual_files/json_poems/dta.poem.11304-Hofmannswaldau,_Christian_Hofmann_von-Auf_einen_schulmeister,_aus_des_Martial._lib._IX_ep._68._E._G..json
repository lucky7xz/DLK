{"dta.poem.11304": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen schulmeister, aus des  Martial.  \n lib. IX ep. 68.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch unstern hat euch, Mops! in unsre gegend bracht?", "tokens": ["Welch", "uns\u00b7tern", "hat", "euch", ",", "Mops", "!", "in", "uns\u00b7re", "ge\u00b7gend", "bracht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "NN", "$.", "APPR", "PPOSAT", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch, den das kleine volck bald f\u00fcrchtet, bald verlacht!", "tokens": ["Euch", ",", "den", "das", "klei\u00b7ne", "volck", "bald", "f\u00fcrch\u00b7tet", ",", "bald", "ver\u00b7lacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Euch, der ihr, eh der hahn sein Danck dir herre schreyt,", "tokens": ["Euch", ",", "der", "ihr", ",", "eh", "der", "hahn", "sein", "Danck", "dir", "her\u00b7re", "schreyt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schon mit bewehrter faust der jungen \u00e4rse bleut!", "tokens": ["Schon", "mit", "be\u00b7wehr\u00b7ter", "faust", "der", "jun\u00b7gen", "\u00e4r\u00b7se", "bleut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Euch, der ihr, eh der schmidt auf stahl und eisen schl\u00e4gt,", "tokens": ["Euch", ",", "der", "ihr", ",", "eh", "der", "schmidt", "auf", "stahl", "und", "ei\u00b7sen", "schl\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "$,", "KOUS", "ART", "ADJD", "APPR", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, eh man feuer macht, der m\u00e4gdgen essen fegt!", "tokens": ["Und", ",", "eh", "man", "feu\u00b7er", "macht", ",", "der", "m\u00e4gd\u00b7gen", "es\u00b7sen", "fegt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "NN", "VVFIN", "$,", "ART", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Glaubt, wenn in Spanien ein stier und ritter ficht,", "tokens": ["Glaubt", ",", "wenn", "in", "Spa\u00b7ni\u00b7en", "ein", "stier", "und", "rit\u00b7ter", "ficht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "NE", "ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So h\u00f6ret man f\u00fcrwahr ein solch turnieren nicht.", "tokens": ["So", "h\u00f6\u00b7ret", "man", "f\u00fcr\u00b7wahr", "ein", "solch", "tur\u00b7nie\u00b7ren", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum w\u00fcnscht der nachbar, der vor euch nicht schlafen kan,", "tokens": ["Drum", "w\u00fcnscht", "der", "nach\u00b7bar", ",", "der", "vor", "euch", "nicht", "schla\u00b7fen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJD", "$,", "PRELS", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Wenn gleich sonst alles schl\u00e4ft, euch alle teufel an.", "tokens": ["Wenn", "gleich", "sonst", "al\u00b7les", "schl\u00e4ft", ",", "euch", "al\u00b7le", "teu\u00b7fel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIS", "VVFIN", "$,", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja, was die gantze stadt vor euer schreyen giebt,", "tokens": ["Ja", ",", "was", "die", "gant\u00b7ze", "stadt", "vor", "eu\u00b7er", "schre\u00b7yen", "giebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das zahlt\u2019 er gern allein, wann ihr nur stille bliebt.", "tokens": ["Das", "zahlt'", "er", "gern", "al\u00b7lein", ",", "wann", "ihr", "nur", "stil\u00b7le", "bliebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}