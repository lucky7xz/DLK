{"dta.poem.18175": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n  \u00fcber ein fliegend W\u00fcrmchen  \n Ephemeris .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein fliegend W\u00fcrmchen, das Ephemeris", "tokens": ["Ein", "flie\u00b7gend", "W\u00fcrm\u00b7chen", ",", "das", "E\u00b7phe\u00b7me\u00b7ris"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "NE"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "genant,", "tokens": ["ge\u00b7nant", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ist uns aus der Geschicht des Thier-", "tokens": ["Ist", "uns", "aus", "der", "Ge\u00b7schicht", "des", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "reichs gnug bekant;", "tokens": ["reichs", "gnug", "be\u00b7kant", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "---+", "measure": "unknown.measure.single"}, "line.5": {"text": "Weil es denselben Tag, woran es erst", "tokens": ["Weil", "es", "den\u00b7sel\u00b7ben", "Tag", ",", "wo\u00b7ran", "es", "erst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "entstehet,", "tokens": ["ent\u00b7ste\u00b7het", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Auch wiederum verstirbt und schleunig untergehet.", "tokens": ["Auch", "wie\u00b7de\u00b7rum", "ver\u00b7stirbt", "und", "schleu\u00b7nig", "un\u00b7ter\u00b7ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich wunderte mich drob, da\u00df der Geburts Tag", "tokens": ["Ich", "wun\u00b7der\u00b7te", "mich", "drob", ",", "da\u00df", "der", "Ge\u00b7burts", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "auch,", "tokens": ["auch", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Sein Todestag schon sei, und da\u00df sein Lebenshauch", "tokens": ["Sein", "To\u00b7des\u00b7tag", "schon", "sei", ",", "und", "da\u00df", "sein", "Le\u00b7bens\u00b7hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "$,", "KON", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der doch vor kurzer Frist, sich erstlich angefangen,", "tokens": ["Der", "doch", "vor", "kur\u00b7zer", "Frist", ",", "sich", "erst\u00b7lich", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "$,", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "gangen.", "tokens": ["gan\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Doch fiel mir dabei ein; wie mancher Mensch er-", "tokens": ["Doch", "fiel", "mir", "da\u00b7bei", "ein", ";", "wie", "man\u00b7cher", "Mensch", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "PTKVZ", "$.", "KOKOM", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "bla\u00dft", "tokens": ["bla\u00dft"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Wenn er dem Othem kaum, zum Leben aufgefa\u00dft!", "tokens": ["Wenn", "er", "dem", "O\u00b7them", "kaum", ",", "zum", "Le\u00b7ben", "auf\u00b7ge\u00b7fa\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja! als ich weiter noch, der Menschen Ziel be-", "tokens": ["Ja", "!", "als", "ich", "wei\u00b7ter", "noch", ",", "der", "Men\u00b7schen", "Ziel", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "PPER", "ADV", "ADV", "$,", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "dachte,", "tokens": ["dach\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Und Zeit und Ewigkeit in ein Verh\u00e4ltnis brachte:", "tokens": ["Und", "Zeit", "und", "E\u00b7wig\u00b7keit", "in", "ein", "Ver\u00b7h\u00e4lt\u00b7nis", "brach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So sah ich, da\u00df man sich nicht drob verwundern", "tokens": ["So", "sah", "ich", ",", "da\u00df", "man", "sich", "nicht", "drob", "ver\u00b7wun\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PRF", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "kann,", "tokens": ["kann", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Beim Leben f\u00e4ngt sich schon, der Menschen Ster-", "tokens": ["Beim", "Le\u00b7ben", "f\u00e4ngt", "sich", "schon", ",", "der", "Men\u00b7schen", "Ster"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ADV", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "ben an:", "tokens": ["ben", "an", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Wenn er gebohren wird, das ist des Lebens Morgen,", "tokens": ["Wenn", "er", "ge\u00b7boh\u00b7ren", "wird", ",", "das", "ist", "des", "Le\u00b7bens", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wird sein erbla\u00dfter Leib, ins dunkle Grab ver-", "tokens": ["Wird", "sein", "er\u00b7bla\u00df\u00b7ter", "Leib", ",", "ins", "dunk\u00b7le", "Grab", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "borgen;", "tokens": ["bor\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "So ist der Abend da. Die ganze Lebens Zeit,", "tokens": ["So", "ist", "der", "A\u00b7bend", "da", ".", "Die", "gan\u00b7ze", "Le\u00b7bens", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Daurt sie gleich noch so lang, ist doch ein fl\u00fcchtig", "tokens": ["Daurt", "sie", "gleich", "noch", "so", "lang", ",", "ist", "doch", "ein", "fl\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ART", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Heut.", "tokens": ["Heut", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Doch dieser Umstand macht, bei unsern schnell Er-", "tokens": ["Doch", "die\u00b7ser", "Um\u00b7stand", "macht", ",", "bei", "un\u00b7sern", "schnell", "Er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "bleichen,", "tokens": ["blei\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Da\u00df das Ephemeris, mit uns nicht zu vergleichen.", "tokens": ["Da\u00df", "das", "E\u00b7phe\u00b7me\u00b7ris", ",", "mit", "uns", "nicht", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "$,", "APPR", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Sein Leben, ist sein Todt; der Anfang ist der", "tokens": ["Sein", "Le\u00b7ben", ",", "ist", "sein", "Todt", ";", "der", "An\u00b7fang", "ist", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Schlu\u00df:", "tokens": ["Schlu\u00df", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Da erst auf unsern Todt, das Leben folgen mu\u00df;", "tokens": ["Da", "erst", "auf", "un\u00b7sern", "Todt", ",", "das", "Le\u00b7ben", "fol\u00b7gen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bei unsern End mu\u00df sich zu einen ewgen Leben,", "tokens": ["Bei", "un\u00b7sern", "End", "mu\u00df", "sich", "zu", "ei\u00b7nen", "ew\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein Anfang ohne End, in Ewigkeit anheben.", "tokens": ["Ein", "An\u00b7fang", "oh\u00b7ne", "End", ",", "in", "E\u00b7wig\u00b7keit", "an\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}