{"dta.poem.8145": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "An den  \n  Erl\u00f6ser .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hofft' es zu dir! und ich habe gesungen,", "tokens": ["Ich", "hofft'", "es", "zu", "dir", "!", "und", "ich", "ha\u00b7be", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$.", "KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Vers\u00f6ner Gottes, von dir das heilige Lied!", "tokens": ["Ver\u00b7s\u00f6\u00b7ner", "Got\u00b7tes", ",", "von", "dir", "das", "hei\u00b7li\u00b7ge", "Lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durchlaufen bin ich die furchtbare Laufbahn;", "tokens": ["Durch\u00b7lau\u00b7fen", "bin", "ich", "die", "furcht\u00b7ba\u00b7re", "Lauf\u00b7bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und du hast mir mein Straucheln verziehn!", "tokens": ["Und", "du", "hast", "mir", "mein", "Strau\u00b7cheln", "ver\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Beginn den ersten Harfenlaut,", "tokens": ["Be\u00b7ginn", "den", "ers\u00b7ten", "Har\u00b7fen\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heisser, gefl\u00fcgelter, ewiger Dank!", "tokens": ["Heis\u00b7ser", ",", "ge\u00b7fl\u00fc\u00b7gel\u00b7ter", ",", "e\u00b7wi\u00b7ger", "Dank", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Beginn, beginn, mir str\u00f6met das Herz!", "tokens": ["Be\u00b7ginn", ",", "be\u00b7ginn", ",", "mir", "str\u00f6\u00b7met", "das", "Herz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und ich weine vor Wonne!", "tokens": ["Und", "ich", "wei\u00b7ne", "vor", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich fleh\u2019 um keinen Lohn; ich bin schon belohnt,", "tokens": ["Ich", "fleh'", "um", "kei\u00b7nen", "Lohn", ";", "ich", "bin", "schon", "be\u00b7lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch Engelfreuden, wenn ich dich sang!", "tokens": ["Durch", "En\u00b7gel\u00b7freu\u00b7den", ",", "wenn", "ich", "dich", "sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Der ganzen Seele Bewegung", "tokens": ["Der", "gan\u00b7zen", "See\u00b7le", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis hin in die Tiefen ihrer ersten Kraft,", "tokens": ["Bis", "hin", "in", "die", "Tie\u00b7fen", "ih\u00b7rer", "ers\u00b7ten", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Ersch\u00fcttrung des Innersten, da\u00df Himmel", "tokens": ["Er\u00b7sch\u00fct\u00b7trung", "des", "In\u00b7ners\u00b7ten", ",", "da\u00df", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und Erde mir schwanden!", "tokens": ["Und", "Er\u00b7de", "mir", "schwan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und flogen die Fl\u00fcge nicht mehr des Sturms; durch sanftes Gef\u00fchl,", "tokens": ["Und", "flo\u00b7gen", "die", "Fl\u00fc\u00b7ge", "nicht", "mehr", "des", "Sturms", ";", "durch", "sanf\u00b7tes", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das, wie des Lenztags Fr\u00fche, Leben s\u00e4uselte.", "tokens": ["Das", ",", "wie", "des", "Lenz\u00b7tags", "Fr\u00fc\u00b7he", ",", "Le\u00b7ben", "s\u00e4u\u00b7sel\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "NN", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Der kennt nicht meinen ganzen Dank,", "tokens": ["Der", "kennt", "nicht", "mei\u00b7nen", "gan\u00b7zen", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem es da noch d\u00e4mmert,", "tokens": ["Dem", "es", "da", "noch", "d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df, wenn in ihrer vollen Empfindung", "tokens": ["Da\u00df", ",", "wenn", "in", "ih\u00b7rer", "vol\u00b7len", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Seele sich ergeu\u00dft, nur stammeln die Sprache kann.", "tokens": ["Die", "See\u00b7le", "sich", "er\u00b7geu\u00dft", ",", "nur", "stam\u00b7meln", "die", "Spra\u00b7che", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Belohnt bin ich, belohnt! Ich habe gesehen", "tokens": ["Be\u00b7lohnt", "bin", "ich", ",", "be\u00b7lohnt", "!", "Ich", "ha\u00b7be", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "VVPP", "$.", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Thr\u00e4ne des Christen rinnen:", "tokens": ["Die", "Thr\u00e4\u00b7ne", "des", "Chris\u00b7ten", "rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und darf hinaus in die Zukunft", "tokens": ["Und", "darf", "hin\u00b7aus", "in", "die", "Zu\u00b7kunft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APZR", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach der himlischen Thr\u00e4ne blicken!", "tokens": ["Nach", "der", "him\u00b7li\u00b7schen", "Thr\u00e4\u00b7ne", "bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Durch Menschenfreuden auch. Umsonst verb\u00fcrg\u2019 ich vor dir", "tokens": ["Durch", "Men\u00b7schen\u00b7freu\u00b7den", "auch", ".", "Um\u00b7sonst", "ver\u00b7b\u00fcr\u00b7g'", "ich", "vor", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Mein Herz der Ehrbegierde voll.", "tokens": ["Mein", "Herz", "der", "Ehr\u00b7be\u00b7gier\u00b7de", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem J\u00fcnglinge schlug es laut empor; dem Manne", "tokens": ["Dem", "J\u00fcng\u00b7lin\u00b7ge", "schlug", "es", "laut", "em\u00b7por", ";", "dem", "Man\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ART", "NN"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Hat es stets, gehaltner nur, geschlagen.", "tokens": ["Hat", "es", "stets", ",", "ge\u00b7halt\u00b7ner", "nur", ",", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "ADJA", "ADV", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ist etwa ein Lob, ist etwa eine Tugend,", "tokens": ["Ist", "et\u00b7wa", "ein", "Lob", ",", "ist", "et\u00b7wa", "ei\u00b7ne", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dem trachtet nach!.. Die Flamm\u2019 erkohr ich zur Leiterin mir!", "tokens": ["Dem", "trach\u00b7tet", "nach", "!", "..", "Die", "Flamm'", "er\u00b7kohr", "ich", "zur", "Lei\u00b7te\u00b7rin", "mir", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PPER", "$."], "meter": "-+-+-+-+--+---", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hoch weht die heilige Flamme voran, und weiset", "tokens": ["Hoch", "weht", "die", "hei\u00b7li\u00b7ge", "Flam\u00b7me", "vo\u00b7ran", ",", "und", "wei\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Dem Ehrbegierigen besseren Pfad!", "tokens": ["Dem", "Ehr\u00b7be\u00b7gie\u00b7ri\u00b7gen", "bes\u00b7se\u00b7ren", "Pfad", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie war es, sie that\u2019s, da\u00df die Menschenfreuden", "tokens": ["Sie", "war", "es", ",", "sie", "that's", ",", "da\u00df", "die", "Men\u00b7schen\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit ihrem Zauber mich nicht einschl\u00e4ferten;", "tokens": ["Mit", "ih\u00b7rem", "Zau\u00b7ber", "mich", "nicht", "ein\u00b7schl\u00e4\u00b7fer\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wekte mich oft der Wiederkehr", "tokens": ["Sie", "wek\u00b7te", "mich", "oft", "der", "Wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu den Engelfreuden!", "tokens": ["Zu", "den", "En\u00b7gel\u00b7freu\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Sie wekten mich auch, mit lautem, durchdringenden Silberton,", "tokens": ["Sie", "wek\u00b7ten", "mich", "auch", ",", "mit", "lau\u00b7tem", ",", "durch\u00b7drin\u00b7gen\u00b7den", "Sil\u00b7ber\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit trunkner Erinnrung an die Stunden der Weihe,", "tokens": ["Mit", "trunk\u00b7ner", "E\u00b7rinn\u00b7rung", "an", "die", "Stun\u00b7den", "der", "Wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+----+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie selber, sie selber die Engelfreuden,", "tokens": ["Sie", "sel\u00b7ber", ",", "sie", "sel\u00b7ber", "die", "En\u00b7gel\u00b7freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit Harf\u2019, und Posaune, mit Donnerruf!", "tokens": ["Mit", "Ha\u00b7rf'", ",", "und", "Po\u00b7sau\u00b7ne", ",", "mit", "Don\u00b7ner\u00b7ruf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KON", "NN", "$,", "APPR", "NN", "$."], "meter": "-++--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Ich bin an dem Ziel, an dem Ziel! und f\u00fchle, wo ich bin,", "tokens": ["Ich", "bin", "an", "dem", "Ziel", ",", "an", "dem", "Ziel", "!", "und", "f\u00fch\u00b7le", ",", "wo", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$.", "KON", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es in der ganzen Seele beben! So wird es, (ich rede", "tokens": ["Es", "in", "der", "gan\u00b7zen", "See\u00b7le", "be\u00b7ben", "!", "So", "wird", "es", ",", "(", "ich", "re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$.", "ADV", "VAFIN", "PPER", "$,", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Menschlich von g\u00f6ttlichen Dingen) uns einst, ihr Br\u00fcder de\u00df,", "tokens": ["Menschlich", "von", "g\u00f6tt\u00b7li\u00b7chen", "Din\u00b7gen", ")", "uns", "einst", ",", "ihr", "Br\u00fc\u00b7der", "de\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$(", "PPER", "ADV", "$,", "PPOSAT", "NN", "ART", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Der starb! und erstand! bey der Ankunft im Himmel seyn!", "tokens": ["Der", "starb", "!", "und", "er\u00b7stand", "!", "bey", "der", "An\u00b7kunft", "im", "Him\u00b7mel", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "KON", "VVFIN", "$.", "APPR", "ART", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.12": {"line.1": {"text": "Zu diesem Ziel hinauf hast du,", "tokens": ["Zu", "die\u00b7sem", "Ziel", "hin\u00b7auf", "hast", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herr! und mein Gott!", "tokens": ["Mein", "Herr", "!", "und", "mein", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Bey mehr als Einem Grabe mich,", "tokens": ["Bey", "mehr", "als", "Ei\u00b7nem", "Gra\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit m\u00e4chtigem Arme, vor\u00fcbergef\u00fchrt!", "tokens": ["Mit", "m\u00e4ch\u00b7ti\u00b7gem", "Ar\u00b7me", ",", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.13": {"line.1": {"text": "Genesung gabst du mir! gabst Mut und Entschlu\u00df", "tokens": ["Ge\u00b7ne\u00b7sung", "gabst", "du", "mir", "!", "gabst", "Mut", "und", "Ent\u00b7schlu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "NN", "KON", "NN"], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In Gefahren des nahen Todes!", "tokens": ["In", "Ge\u00b7fah\u00b7ren", "des", "na\u00b7hen", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und sah ich sie etwa die schreklichen unbekannten,", "tokens": ["Und", "sah", "ich", "sie", "et\u00b7wa", "die", "schrek\u00b7li\u00b7chen", "un\u00b7be\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Die weichen mu\u00dften, weil du der Schirmende warst?", "tokens": ["Die", "wei\u00b7chen", "mu\u00df\u00b7ten", ",", "weil", "du", "der", "Schir\u00b7men\u00b7de", "warst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}}, "stanza.14": {"line.1": {"text": "Sie flohen davon! und ich habe gesungen,", "tokens": ["Sie", "flo\u00b7hen", "da\u00b7von", "!", "und", "ich", "ha\u00b7be", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$.", "KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Vers\u00f6ner Gottes, von dir das heilige Lied!", "tokens": ["Ver\u00b7s\u00f6\u00b7ner", "Got\u00b7tes", ",", "von", "dir", "das", "hei\u00b7li\u00b7ge", "Lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durchlaufen bin ich die furchtbare Laufbahn!", "tokens": ["Durch\u00b7lau\u00b7fen", "bin", "ich", "die", "furcht\u00b7ba\u00b7re", "Lauf\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich hofft\u2019 es zu dir!", "tokens": ["Ich", "hofft'", "es", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}