{"textgrid.poem.62796": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Kater sah bey einem Schmau\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kater sah bey einem Schmau\u00df", "tokens": ["Ein", "Ka\u00b7ter", "sah", "bey", "ei\u00b7nem", "Schmau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die goldgef\u00fcllten R\u00f6mer blinken;", "tokens": ["Die", "gold\u00b7ge\u00b7f\u00fcll\u00b7ten", "R\u00f6\u00b7mer", "blin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er sah die G\u00e4ste wacker trinken", "tokens": ["Er", "sah", "die", "G\u00e4s\u00b7te", "wa\u00b7cker", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und rief in vollem Eifer aus:", "tokens": ["Und", "rief", "in", "vol\u00b7lem", "Ei\u00b7fer", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Himmel, welch ein toller Haufen!", "tokens": ["O", "Him\u00b7mel", ",", "welch", "ein", "tol\u00b7ler", "Hau\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sch\u00e4ndlich ist es Wein zu saufen,", "tokens": ["Wie", "sch\u00e4nd\u00b7lich", "ist", "es", "Wein", "zu", "sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Uns Katzen ekelt vor dem Wein.", "tokens": ["Uns", "Kat\u00b7zen", "e\u00b7kelt", "vor", "dem", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur bey den Menschen giebt es Prasser;", "tokens": ["Nur", "bey", "den", "Men\u00b7schen", "giebt", "es", "Pras\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wir l\u00f6schen unsern Durst mit Wasser,", "tokens": ["Wir", "l\u00f6\u00b7schen", "un\u00b7sern", "Durst", "mit", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "O lernt von Katzen weise seyn!", "tokens": ["O", "lernt", "von", "Kat\u00b7zen", "wei\u00b7se", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Herr Murner, nur nicht so vermessen,", "tokens": ["Herr", "Mur\u00b7ner", ",", "nur", "nicht", "so", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Rief ihm ein Gast mit Lachen zu:", "token