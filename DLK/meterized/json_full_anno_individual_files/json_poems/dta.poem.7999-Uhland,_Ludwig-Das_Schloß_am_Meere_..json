{"dta.poem.7999": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Schlo\u00df am Meere .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Hast du das Schlo\u00df gesehen,", "tokens": ["Hast", "du", "das", "Schlo\u00df", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das hohe Schlo\u00df am Meer?", "tokens": ["Das", "ho\u00b7he", "Schlo\u00df", "am", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Golden und rosig wehen", "tokens": ["Gol\u00b7den", "und", "ro\u00b7sig", "we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Wolken dr\u00fcber her.", "tokens": ["Die", "Wol\u00b7ken", "dr\u00fc\u00b7ber", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es m\u00f6chte sich niederneigen", "tokens": ["Es", "m\u00f6ch\u00b7te", "sich", "nie\u00b7der\u00b7nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In die spiegelklare Flut;", "tokens": ["In", "die", "spie\u00b7gel\u00b7kla\u00b7re", "Flut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es m\u00f6chte streben und steigen", "tokens": ["Es", "m\u00f6ch\u00b7te", "stre\u00b7ben", "und", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In der Abendwolken Glut.", "tokens": ["In", "der", "Ab\u00b7end\u00b7wol\u00b7ken", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ewohl hab\u2019 ich es gesehen,", "tokens": ["\u201e", "wohl", "hab'", "ich", "es", "ge\u00b7se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das hohe Schlo\u00df am Meer,", "tokens": ["Das", "ho\u00b7he", "Schlo\u00df", "am", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und den Mond dar\u00fcber stehen,", "tokens": ["Und", "den", "Mond", "da\u00b7r\u00fc\u00b7ber", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Nebel weit umher.\u201c", "tokens": ["Und", "Ne\u00b7bel", "weit", "um\u00b7her", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Wind und des Meeres Wallen", "tokens": ["Der", "Wind", "und", "des", "Mee\u00b7res", "Wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gaben sie frischen Klang?", "tokens": ["Ga\u00b7ben", "sie", "fri\u00b7schen", "Klang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vernahmst du aus hohen Hallen", "tokens": ["Ver\u00b7nahmst", "du", "aus", "ho\u00b7hen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Saiten und Festgesang?", "tokens": ["Sai\u00b7ten", "und", "Fest\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "\u201edie Winde, die Wogen alle", "tokens": ["\u201e", "die", "Win\u00b7de", ",", "die", "Wo\u00b7gen", "al\u00b7le"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lagen in tiefer Ruh,", "tokens": ["La\u00b7gen", "in", "tie\u00b7fer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Einem Klagelied aus der Halle", "tokens": ["Ei\u00b7nem", "Kla\u00b7ge\u00b7lied", "aus", "der", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "H\u00f6rt\u2019 ich mit Thr\u00e4nen zu.\u201c", "tokens": ["H\u00f6rt'", "ich", "mit", "Thr\u00e4\u00b7nen", "zu", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sahest du oben gehen", "tokens": ["Sa\u00b7hest", "du", "o\u00b7ben", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Den K\u00f6nig und sein Gemahl?", "tokens": ["Den", "K\u00f6\u00b7nig", "und", "sein", "Ge\u00b7mahl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der rothen M\u00e4ntel Wehen?", "tokens": ["Der", "ro\u00b7then", "M\u00e4n\u00b7tel", "We\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der goldnen Kronen Stral?", "tokens": ["Der", "gold\u00b7nen", "Kro\u00b7nen", "Stral", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "F\u00fchrten sie nicht mit Wonne", "tokens": ["F\u00fchr\u00b7ten", "sie", "nicht", "mit", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Eine sch\u00f6ne Jungfrau dar,", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Jung\u00b7frau", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrlich wie eine Sonne,", "tokens": ["Herr\u00b7lich", "wie", "ei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Stralend im goldnen Haar?", "tokens": ["Stra\u00b7lend", "im", "gold\u00b7nen", "Haar", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "\u201ewohl sah ich die Eltern beide,", "tokens": ["\u201e", "wohl", "sah", "ich", "die", "El\u00b7tern", "bei\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne der Kronen Licht,", "tokens": ["Oh\u00b7ne", "der", "Kro\u00b7nen", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im schwarzen Trauerkleide;", "tokens": ["Im", "schwar\u00b7zen", "Trau\u00b7er\u00b7klei\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Jungfrau sah ich nicht.\u201c", "tokens": ["Die", "Jung\u00b7frau", "sah", "ich", "nicht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}