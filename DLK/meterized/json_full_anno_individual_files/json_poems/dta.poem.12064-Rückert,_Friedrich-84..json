{"dta.poem.12064": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "84.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du dem Gegner ab Vernunft sprichst und Verstand,", "tokens": ["Wenn", "du", "dem", "Geg\u00b7ner", "ab", "Ver\u00b7nunft", "sprichst", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ists ja kein Gro\u00dfes da\u00df dein Geist ihn \u00fcberwand.", "tokens": ["Ists", "ja", "kein", "Gro\u00b7\u00dfes", "da\u00df", "dein", "Geist", "ihn", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hingegen, wirst du ihn mit starken Waffen r\u00fcsten,", "tokens": ["Hin\u00b7ge\u00b7gen", ",", "wirst", "du", "ihn", "mit", "star\u00b7ken", "Waf\u00b7fen", "r\u00fcs\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ihn schlagend, willst du nur damit dich selber br\u00fcsten.", "tokens": ["Ihn", "schla\u00b7gend", ",", "willst", "du", "nur", "da\u00b7mit", "dich", "sel\u00b7ber", "br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "VMFIN", "PPER", "ADV", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geh deinen Weg und la\u00df den Gegner seinen gehn;", "tokens": ["Geh", "dei\u00b7nen", "Weg", "und", "la\u00df", "den", "Geg\u00b7ner", "sei\u00b7nen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "VVIMP", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer zum Ziel gelangt, das werden wir ja sehn.", "tokens": ["Und", "wer", "zum", "Ziel", "ge\u00b7langt", ",", "das", "wer\u00b7den", "wir", "ja", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVPP", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}