ter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf dem Grund, der sie erfand?", "tokens": ["Auf", "dem", "Grund", ",", "der", "sie", "er\u00b7fand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohlan denn, schmettern mu\u00df das Horn,", "tokens": ["Wo\u00b7hlan", "denn", ",", "schmet\u00b7tern", "mu\u00df", "das", "Horn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fchlen mu\u00df das Ro\u00df den Sporn!", "tokens": ["F\u00fch\u00b7len", "mu\u00df", "das", "Ro\u00df", "den", "Sporn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ernst herab auf ihr Geschlecht", "tokens": ["Ernst", "her\u00b7ab", "auf", "ihr", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht der V\u00e4ter stolze Reih',", "tokens": ["Sieht", "der", "V\u00e4\u00b7ter", "stol\u00b7ze", "Reih'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ruft und winkt euch: Ins Gefecht!", "tokens": ["Ruft", "und", "winkt", "euch", ":", "Ins", "Ge\u00b7fecht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$.", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Werdet f