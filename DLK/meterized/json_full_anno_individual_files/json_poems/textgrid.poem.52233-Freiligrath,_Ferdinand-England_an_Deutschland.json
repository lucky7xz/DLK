meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dreimal geheiligt sei", "tokens": ["Und", "drei\u00b7mal", "ge\u00b7hei\u00b7ligt", "sei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Unsrer Herzen heilig Band,", "tokens": ["Uns\u00b7rer", "Her\u00b7zen", "hei\u00b7lig", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn uns zujauchzt endlich frei", "tokens": ["Wenn", "uns", "zu\u00b7jauchzt", "end\u00b7lich", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.10": {"text": "Euer Land \u2013 euer Land!", "tokens": ["Eu\u00b7er", "Land", "\u2013", "eu\u00b7er", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Britannia durch die Meere", "tokens": ["Bri\u00b7tan\u00b7nia", "durch", "die", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwingt der Freiheit Banner hoch:", "tokens": ["Schwingt", "der", "Frei\u00b7heit", "Ban\u00b7ner", "hoch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euer 'breiter Stein der Ehre'", "tokens": ["Eu\u00b7er", "'", "brei\u00b7ter", "Stein", "der", "Eh\u00b7re", "'"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Sklavenzwinger noch!", "tokens": ["Ist", "ein", "Skla\u00b7ven\u00b7zwin\u00b7ger", "noch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O Schmach! des alten Ruhms gedacht!", "tokens": ["O", "Schmach", "!", "des", "al\u00b7ten", "Ruhms", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Alemannen, auf, erwacht!", "tokens": ["A\u00b7le\u00b7man\u00b7nen", ",", "auf", ",", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die jetzt euch fesselt: \u2013 bleich", "tokens": ["Und", "die", "jetzt", "euch", "fes\u00b7selt", ":", "\u2013", "bleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "ART", "ADV", "PPER", "ADJD", "$.", "$(", "ADJD"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Fl\u00fcchten wird die Tyrannei,", "tokens": ["Fl\u00fcch\u00b7ten", "wird", "die", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn sich aufrafft euer Reich", "tokens": ["Wenn", "sich", "auf\u00b7rafft", "eu\u00b7er", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Gro\u00df und frei \u2013 gro\u00df und frei.", "tokens": ["Gro\u00df", "und", "frei", "\u2013", "gro\u00df", "und", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$(", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Dem Mars habt ihr erfunden", "tokens": ["Dem", "Mars", "habt", "ihr", "er\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Donerkeil der Schlacht,", "tokens": ["Den", "Do\u00b7ner\u00b7keil", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch die Kett' um eure Wunden", "tokens": ["Doch", "die", "Kett'", "um", "eu\u00b7re", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat kein Donner noch zerkracht!", "tokens": ["Hat", "kein", "Don\u00b7ner", "noch", "zer\u00b7kracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Land des Gedankens! soll dein Herz", "tokens": ["Land", "des", "Ge\u00b7dan\u00b7kens", "!", "soll", "dein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "VMFIN", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Reiben stets der Fessel Erz?", "tokens": ["Rei\u00b7ben", "stets", "der", "Fes\u00b7sel", "Erz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein, die Schlaguhr, hell von Schall,",