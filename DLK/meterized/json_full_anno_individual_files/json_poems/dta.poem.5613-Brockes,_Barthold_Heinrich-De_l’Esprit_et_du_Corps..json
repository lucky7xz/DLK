{"dta.poem.5613": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De l\u2019Esprit et du Corps.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So manche Schw\u00fcrigkeit, wovon sich unser Geist,", "tokens": ["So", "man\u00b7che", "Schw\u00fc\u00b7rig\u00b7keit", ",", "wo\u00b7von", "sich", "un\u00b7ser", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PWAV", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da er so eingeschr\u00e4nckt und mangelhafft,", "tokens": ["Da", "er", "so", "ein\u00b7ge\u00b7schr\u00e4nckt", "und", "man\u00b7gel\u00b7hafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df \u00fcberwinden lassen,", "tokens": ["Mu\u00df", "\u00fc\u00b7berw\u00b7in\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entsteht daher, weil wir der Gottheit Eigenschafft", "tokens": ["Ent\u00b7steht", "da\u00b7her", ",", "weil", "wir", "der", "Got\u00b7theit", "Ei\u00b7gen\u00b7schafft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht fassen.", "tokens": ["Nicht", "fas\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wofern man aber nur ein Wesen zugestehet,", "tokens": ["Wo\u00b7fern", "man", "a\u00b7ber", "nur", "ein", "We\u00b7sen", "zu\u00b7ge\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das ewig, ob man es so, gleich nicht ganz erh\u00f6het;", "tokens": ["Das", "e\u00b7wig", ",", "ob", "man", "es", "so", ",", "gleich", "nicht", "ganz", "er\u00b7h\u00f6\u00b7het", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "KOUS", "PIS", "PPER", "ADV", "$,", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verschwindet alsobald der Zweisel und verfleucht,", "tokens": ["Ver\u00b7schwin\u00b7det", "al\u00b7so\u00b7bald", "der", "Zwei\u00b7sel", "und", "ver\u00b7fleucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von den Gesch\u00f6pffen ist zu Jhm der Fortgang leicht.", "tokens": ["Von", "den", "Ge\u00b7sch\u00f6pf\u00b7fen", "ist", "zu", "Jhm", "der", "Fort\u00b7gang", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}