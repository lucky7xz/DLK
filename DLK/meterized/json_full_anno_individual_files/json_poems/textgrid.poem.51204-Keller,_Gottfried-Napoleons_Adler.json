{"textgrid.poem.51204": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Napoleons Adler", "genre": "verse", "period": "N.A.", "pub_year": 1880, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4hrend des Gefallnen Flamme", "tokens": ["W\u00e4h\u00b7rend", "des", "Ge\u00b7fall\u00b7nen", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Ozeanos verzischt,", "tokens": ["Im", "O\u00b7zea\u00b7nos", "ver\u00b7zischt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Auf des h\u00f6chsten Berges Kamme", "tokens": ["Auf", "des", "h\u00f6chs\u00b7ten", "Ber\u00b7ges", "Kam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich sein Aar das Aug erfrischt;", "tokens": ["Sich", "sein", "Aar", "das", "Aug", "er\u00b7frischt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem ew'gen Schnee umschauert,", "tokens": ["Von", "dem", "ew'\u00b7gen", "Schnee", "um\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den die Gemse nie betrat,", "tokens": ["Den", "die", "Gem\u00b7se", "nie", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der zerzauste Adler kauert", "tokens": ["Der", "zer\u00b7zaus\u00b7te", "Ad\u00b7ler", "kau\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einsam auf beeistem Grat:", "tokens": ["Ein\u00b7sam", "auf", "be\u00b7eis\u00b7tem", "Grat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbda\u00df mir Kiel und Federn stoben,", "tokens": ["\u00bb", "da\u00df", "mir", "Kiel", "und", "Fe\u00b7dern", "sto\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teufel, dort bei Waterloo!", "tokens": ["Teu\u00b7fel", ",", "dort", "bei", "Wa\u00b7ter\u00b7loo", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie hab ich mich schwer erhoben,", "tokens": ["Wie", "hab", "ich", "mich", "schwer", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als ich jenen St\u00fcmper floh!", "tokens": ["Als", "ich", "je\u00b7nen", "St\u00fcm\u00b7per", "floh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Glaubte fast nicht zu erreichen", "tokens": ["Glaub\u00b7te", "fast", "nicht", "zu", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier des alten Berges Bann \u2013", "tokens": ["Hier", "des", "al\u00b7ten", "Ber\u00b7ges", "Bann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wieviel Sonnen werden bleichen,", "tokens": ["Wie\u00b7viel", "Son\u00b7nen", "wer\u00b7den", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis ich wieder steigen kann?\u00ab", "tokens": ["Bis", "ich", "wie\u00b7der", "stei\u00b7gen", "kann", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und er duckt ins Eis sich nieder,", "tokens": ["Und", "er", "duckt", "ins", "Eis", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rmt das Haupt im matten Strahl,", "tokens": ["W\u00e4rmt", "das", "Haupt", "im", "mat\u00b7ten", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reckt und dehnt das Schwunggefieder", "tokens": ["Reckt", "und", "dehnt", "das", "Schwung\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einmal und das andre Mal,", "tokens": ["Ein\u00b7mal", "und", "das", "and\u00b7re", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schl\u00e4ft dann, bis ein schrilles Pfeifen,", "tokens": ["Schl\u00e4ft", "dann", ",", "bis", "ein", "schril\u00b7les", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohlbekannt, ihn jach erweckt \u2013", "tokens": ["Wohl\u00b7be\u00b7kannt", ",", "ihn", "jach", "er\u00b7weckt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die gro\u00dfen H\u00fcgel greifen", "tokens": ["Und", "die", "gro\u00b7\u00dfen", "H\u00fc\u00b7gel", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schon die Luft, lang hingestreckt.", "tokens": ["Schon", "die", "Luft", ",", "lang", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald das alte Nest gefunden", "tokens": ["Bald", "das", "al\u00b7te", "Nest", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat er in der Stadt Paris;", "tokens": ["Hat", "er", "in", "der", "Stadt", "Pa\u00b7ris", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jahre wieder sind verschwunden,", "tokens": ["Jah\u00b7re", "wie\u00b7der", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seit er dort sich niederlie\u00df;", "tokens": ["Seit", "er", "dort", "sich", "nie\u00b7der\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Weib kraut ihm 's Gefieder,", "tokens": ["Und", "ein", "Weib", "kraut", "ihm", "'s", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und es geht dem Vogel gut;", "tokens": ["Und", "es", "geht", "dem", "Vo\u00b7gel", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Glatt und gl\u00e4nzend wird er wieder", "tokens": ["Glatt", "und", "gl\u00e4n\u00b7zend", "wird", "er", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem Zuckerbrot in Blut.", "tokens": ["Von", "dem", "Zu\u00b7cker\u00b7brot", "in", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie der Abt im Nonnenkloster", "tokens": ["Wie", "der", "Abt", "im", "Non\u00b7nen\u00b7klos\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt und treibt er mancherlei;", "tokens": ["Spielt", "und", "treibt", "er", "man\u00b7cher\u00b7lei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Klau ein Paternoster,", "tokens": ["In", "der", "Klau", "ein", "Pa\u00b7ter\u00b7nos\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kost er wie ein Papagei.", "tokens": ["Kost", "er", "wie", "ein", "Pa\u00b7pa\u00b7gei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So auf goldner Stange sitzet", "tokens": ["So", "auf", "gold\u00b7ner", "Stan\u00b7ge", "sit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter Zofen fromm der Aar;", "tokens": ["Un\u00b7ter", "Zo\u00b7fen", "fromm", "der", "Aar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch sein funkelnd Auge blitzet", "tokens": ["Doch", "sein", "fun\u00b7kelnd", "Au\u00b7ge", "blit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Quer durch aller Schranzen Schar.", "tokens": ["Quer", "durch", "al\u00b7ler", "Schran\u00b7zen", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00f6rt! Da donnert's tief im \u00c4ther,", "tokens": ["H\u00f6rt", "!", "Da", "don\u00b7nert's", "tief", "im", "\u00c4\u00b7ther", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Lockruf dumpf ert\u00f6nt,", "tokens": ["Wie", "ein", "Lock\u00b7ruf", "dumpf", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus Schutt und Weh und Zeter", "tokens": ["Und", "aus", "Schutt", "und", "Weh", "und", "Ze\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwingt er sich, des Flugs gew\u00f6hnt;", "tokens": ["Schwingt", "er", "sich", ",", "des", "Flugs", "ge\u00b7w\u00f6hnt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "R\u00fcckw\u00e4rts lassend Staub und Tr\u00fcmmer,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "las\u00b7send", "Staub", "und", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwindet er im Abendgold:", "tokens": ["Schwin\u00b7det", "er", "im", "A\u00b7bend\u00b7gold", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeus, der Vater, hat f\u00fcr immer", "tokens": ["Zeus", ",", "der", "Va\u00b7ter", ",", "hat", "f\u00fcr", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "VAFIN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen Adler heimgeholt!", "tokens": ["Sei\u00b7nen", "Ad\u00b7ler", "heim\u00b7ge\u00b7holt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "W\u00e4hrend des Gefallnen Flamme", "tokens": ["W\u00e4h\u00b7rend", "des", "Ge\u00b7fall\u00b7nen", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Ozeanos verzischt,", "tokens": ["Im", "O\u00b7zea\u00b7nos", "ver\u00b7zischt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Auf des h\u00f6chsten Berges Kamme", "tokens": ["Auf", "des", "h\u00f6chs\u00b7ten", "Ber\u00b7ges", "Kam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich sein Aar das Aug erfrischt;", "tokens": ["Sich", "sein", "Aar", "das", "Aug", "er\u00b7frischt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem ew'gen Schnee umschauert,", "tokens": ["Von", "dem", "ew'\u00b7gen", "Schnee", "um\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den die Gemse nie betrat,", "tokens": ["Den", "die", "Gem\u00b7se", "nie", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der zerzauste Adler kauert", "tokens": ["Der", "zer\u00b7zaus\u00b7te", "Ad\u00b7ler", "kau\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einsam auf beeistem Grat:", "tokens": ["Ein\u00b7sam", "auf", "be\u00b7eis\u00b7tem", "Grat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbda\u00df mir Kiel und Federn stoben,", "tokens": ["\u00bb", "da\u00df", "mir", "Kiel", "und", "Fe\u00b7dern", "sto\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teufel, dort bei Waterloo!", "tokens": ["Teu\u00b7fel", ",", "dort", "bei", "Wa\u00b7ter\u00b7loo", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie hab ich mich schwer erhoben,", "tokens": ["Wie", "hab", "ich", "mich", "schwer", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als ich jenen St\u00fcmper floh!", "tokens": ["Als", "ich", "je\u00b7nen", "St\u00fcm\u00b7per", "floh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Glaubte fast nicht zu erreichen", "tokens": ["Glaub\u00b7te", "fast", "nicht", "zu", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier des alten Berges Bann \u2013", "tokens": ["Hier", "des", "al\u00b7ten", "Ber\u00b7ges", "Bann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wieviel Sonnen werden bleichen,", "tokens": ["Wie\u00b7viel", "Son\u00b7nen", "wer\u00b7den", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis ich wieder steigen kann?\u00ab", "tokens": ["Bis", "ich", "wie\u00b7der", "stei\u00b7gen", "kann", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und er duckt ins Eis sich nieder,", "tokens": ["Und", "er", "duckt", "ins", "Eis", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4rmt das Haupt im matten Strahl,", "tokens": ["W\u00e4rmt", "das", "Haupt", "im", "mat\u00b7ten", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reckt und dehnt das Schwunggefieder", "tokens": ["Reckt", "und", "dehnt", "das", "Schwung\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einmal und das andre Mal,", "tokens": ["Ein\u00b7mal", "und", "das", "and\u00b7re", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schl\u00e4ft dann, bis ein schrilles Pfeifen,", "tokens": ["Schl\u00e4ft", "dann", ",", "bis", "ein", "schril\u00b7les", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohlbekannt, ihn jach erweckt \u2013", "tokens": ["Wohl\u00b7be\u00b7kannt", ",", "ihn", "jach", "er\u00b7weckt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die gro\u00dfen H\u00fcgel greifen", "tokens": ["Und", "die", "gro\u00b7\u00dfen", "H\u00fc\u00b7gel", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schon die Luft, lang hingestreckt.", "tokens": ["Schon", "die", "Luft", ",", "lang", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Bald das alte Nest gefunden", "tokens": ["Bald", "das", "al\u00b7te", "Nest", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat er in der Stadt Paris;", "tokens": ["Hat", "er", "in", "der", "Stadt", "Pa\u00b7ris", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jahre wieder sind verschwunden,", "tokens": ["Jah\u00b7re", "wie\u00b7der", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seit er dort sich niederlie\u00df;", "tokens": ["Seit", "er", "dort", "sich", "nie\u00b7der\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Weib kraut ihm 's Gefieder,", "tokens": ["Und", "ein", "Weib", "kraut", "ihm", "'s", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und es geht dem Vogel gut;", "tokens": ["Und", "es", "geht", "dem", "Vo\u00b7gel", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NE", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Glatt und gl\u00e4nzend wird er wieder", "tokens": ["Glatt", "und", "gl\u00e4n\u00b7zend", "wird", "er", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von dem Zuckerbrot in Blut.", "tokens": ["Von", "dem", "Zu\u00b7cker\u00b7brot", "in", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie der Abt im Nonnenkloster", "tokens": ["Wie", "der", "Abt", "im", "Non\u00b7nen\u00b7klos\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt und treibt er mancherlei;", "tokens": ["Spielt", "und", "treibt", "er", "man\u00b7cher\u00b7lei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Klau ein Paternoster,", "tokens": ["In", "der", "Klau", "ein", "Pa\u00b7ter\u00b7nos\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kost er wie ein Papagei.", "tokens": ["Kost", "er", "wie", "ein", "Pa\u00b7pa\u00b7gei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So auf goldner Stange sitzet", "tokens": ["So", "auf", "gold\u00b7ner", "Stan\u00b7ge", "sit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter Zofen fromm der Aar;", "tokens": ["Un\u00b7ter", "Zo\u00b7fen", "fromm", "der", "Aar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch sein funkelnd Auge blitzet", "tokens": ["Doch", "sein", "fun\u00b7kelnd", "Au\u00b7ge", "blit\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Quer durch aller Schranzen Schar.", "tokens": ["Quer", "durch", "al\u00b7ler", "Schran\u00b7zen", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "H\u00f6rt! Da donnert's tief im \u00c4ther,", "tokens": ["H\u00f6rt", "!", "Da", "don\u00b7nert's", "tief", "im", "\u00c4\u00b7ther", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Lockruf dumpf ert\u00f6nt,", "tokens": ["Wie", "ein", "Lock\u00b7ruf", "dumpf", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus Schutt und Weh und Zeter", "tokens": ["Und", "aus", "Schutt", "und", "Weh", "und", "Ze\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwingt er sich, des Flugs gew\u00f6hnt;", "tokens": ["Schwingt", "er", "sich", ",", "des", "Flugs", "ge\u00b7w\u00f6hnt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "R\u00fcckw\u00e4rts lassend Staub und Tr\u00fcmmer,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "las\u00b7send", "Staub", "und", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwindet er im Abendgold:", "tokens": ["Schwin\u00b7det", "er", "im", "A\u00b7bend\u00b7gold", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeus, der Vater, hat f\u00fcr immer", "tokens": ["Zeus", ",", "der", "Va\u00b7ter", ",", "hat", "f\u00fcr", "im\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "VAFIN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen Adler heimgeholt!", "tokens": ["Sei\u00b7nen", "Ad\u00b7ler", "heim\u00b7ge\u00b7holt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}