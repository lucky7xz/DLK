{"textgrid.poem.37813": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ueberdru\u00df der Gelahrtheit", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich empfinde fast ein Grauen,", "tokens": ["Ich", "emp\u00b7fin\u00b7de", "fast", "ein", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich, Plato, f\u00fcr und f\u00fcr", "tokens": ["Da\u00df", "ich", ",", "Pla\u00b7to", ",", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin gesessen \u00fcber dir;", "tokens": ["Bin", "ge\u00b7ses\u00b7sen", "\u00fc\u00b7ber", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist Zeit hinaus zu schauen,", "tokens": ["Es", "ist", "Zeit", "hin\u00b7aus", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sich bey den frischen Quellen", "tokens": ["Und", "sich", "bey", "den", "fri\u00b7schen", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Gr\u00fcnen zu ergehn,", "tokens": ["In", "dem", "Gr\u00fc\u00b7nen", "zu", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo die sch\u00f6nen Blumen stehn,", "tokens": ["Wo", "die", "sch\u00f6\u00b7nen", "Blu\u00b7men", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Fischer Netze stellen.", "tokens": ["Und", "die", "Fi\u00b7scher", "Net\u00b7ze", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wozu dienet das Studieren?", "tokens": ["Wo\u00b7zu", "die\u00b7net", "das", "Stu\u00b7die\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Als zu lauter Ungemach?", "tokens": ["Als", "zu", "lau\u00b7ter", "Un\u00b7ge\u00b7mach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterdessen l\u00e4uft der Bach", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "l\u00e4uft", "der", "Bach"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsers Lebens, uns zu f\u00fchren,", "tokens": ["Un\u00b7sers", "Le\u00b7bens", ",", "uns", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ehe wir es inne werden,", "tokens": ["E\u00b7he", "wir", "es", "in\u00b7ne", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKVZ", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf sein leztes Ende hin,", "tokens": ["Auf", "sein", "lez\u00b7tes", "En\u00b7de", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann k\u00f6mmt ohne Geist und Sinn", "tokens": ["Dann", "k\u00f6mmt", "oh\u00b7ne", "Geist", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieses alles in die Erden.", "tokens": ["Die\u00b7ses", "al\u00b7les", "in", "die", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hola, Junge geh und frage,", "tokens": ["Ho\u00b7la", ",", "Jun\u00b7ge", "geh", "und", "fra\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der be\u00dfte Trunk mag seyn,", "tokens": ["Wo", "der", "be\u00df\u00b7te", "Trunk", "mag", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm den Krug, und f\u00fclle Wein.", "tokens": ["Nimm", "den", "Krug", ",", "und", "f\u00fcl\u00b7le", "Wein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Trauren, Leid und Klage", "tokens": ["Al\u00b7les", "Trau\u00b7ren", ",", "Leid", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie wir Menschen t\u00e4glich haben,", "tokens": ["Wie", "wir", "Men\u00b7schen", "t\u00e4g\u00b7lich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' der Strom uns fortgerafft,", "tokens": ["Eh'", "der", "Strom", "uns", "fort\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will ich in den s\u00fc\u00dfen Saft", "tokens": ["Will", "ich", "in", "den", "s\u00fc\u00b7\u00dfen", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Den die Traube gibt, vergraben.", "tokens": ["Den", "die", "Trau\u00b7be", "gibt", ",", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kaufe gleichfalls auch Melonen,", "tokens": ["Kau\u00b7fe", "gleich\u00b7falls", "auch", "Me\u00b7lo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vergi\u00df des Zuckers nicht;", "tokens": ["Und", "ver\u00b7gi\u00df", "des", "Zu\u00b7ckers", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaue nur da\u00df nichts gebricht.", "tokens": ["Schau\u00b7e", "nur", "da\u00df", "nichts", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener mag der Heller schonen,", "tokens": ["Je\u00b7ner", "mag", "der", "Hel\u00b7ler", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der bey seinem Gold und Sch\u00e4tzen", "tokens": ["Der", "bey", "sei\u00b7nem", "Gold", "und", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tolle sich zu kr\u00e4nken pflegt,", "tokens": ["Tol\u00b7le", "sich", "zu", "kr\u00e4n\u00b7ken", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und nicht satt zu Bette legt:", "tokens": ["Und", "nicht", "satt", "zu", "Bet\u00b7te", "legt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich will, weil ich kann, mich letzen.", "tokens": ["Ich", "will", ",", "weil", "ich", "kann", ",", "mich", "let\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "PPER", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Bitte meine guten Br\u00fcder", "tokens": ["Bit\u00b7te", "mei\u00b7ne", "gu\u00b7ten", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Musik und auf ein Glas:", "tokens": ["Auf", "Mu\u00b7sik", "und", "auf", "ein", "Glas", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kein Ding schickt sich, d\u00fcnkt mich, ba\u00df,", "tokens": ["Kein", "Ding", "schickt", "sich", ",", "d\u00fcnkt", "mich", ",", "ba\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,", "VVFIN", "PPER", "$,", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein Trunk und gute Lieder.", "tokens": ["Als", "ein", "Trunk", "und", "gu\u00b7te", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df' ich schon nicht viel zu erben,", "tokens": ["La\u00df'", "ich", "schon", "nicht", "viel", "zu", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ey so hab ich edlen Wein,", "tokens": ["Ey", "so", "hab", "ich", "ed\u00b7len", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will mit andern lustig seyn,", "tokens": ["Will", "mit", "an\u00b7dern", "lus\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann ich gleich allein mu\u00df sterben.", "tokens": ["Wann", "ich", "gleich", "al\u00b7lein", "mu\u00df", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich empfinde fast ein Grauen,", "tokens": ["Ich", "emp\u00b7fin\u00b7de", "fast", "ein", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich, Plato, f\u00fcr und f\u00fcr", "tokens": ["Da\u00df", "ich", ",", "Pla\u00b7to", ",", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin gesessen \u00fcber dir;", "tokens": ["Bin", "ge\u00b7ses\u00b7sen", "\u00fc\u00b7ber", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist Zeit hinaus zu schauen,", "tokens": ["Es", "ist", "Zeit", "hin\u00b7aus", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sich bey den frischen Quellen", "token