}, "line.3": {"text": "Mitten in den Reihen schlie\u00dfen", "tokens": ["Mit\u00b7ten", "in", "den", "Rei\u00b7hen", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie die sch\u00f6ne Lalage.", "tokens": ["Sie", "die", "sch\u00f6\u00b7ne", "La\u00b7la\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Langsam steigt ihr Busen; leise", "tokens": ["Lang\u00b7sam", "steigt", "ihr", "Bu\u00b7sen", ";", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$.", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnschet sie, und wei\u00df nicht was.", "tokens": ["W\u00fcn\u00b7schet", "sie", ",", "und", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht doch, neben ihr im Kreise", "tokens": ["Seht", "doch", ",", "ne\u00b7ben", "ihr", "im", "Krei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PPOSAT", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht der junge Lycidas.", "tokens": ["Steht", "der", "jun\u00b7ge", "Ly\u00b7ci\u00b7das", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Ihm entfliehen will die Spr\u00f6de,", "tokens": ["Ihm", "ent\u00b7flie\u00b7hen", "will", "die", "Spr\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn verachten soll ihr Blick;", "tokens": ["Ihn", "ver\u00b7ach\u00b7ten", "soll", "ihr", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der J\u00fcngling, nicht mehr bl\u00f6de,", "tokens": ["Doch", "der", "J\u00fcng\u00b7ling", ",", "nicht", "mehr", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt die Sch\u00e4ferin zur\u00fcck.", "tokens": ["H\u00e4lt", "die", "Sch\u00e4\u00b7fe\u00b7rin", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Fliehen kann sie nicht; es haben", "tokens": ["Flie\u00b7hen", "kann", "sie", "nicht", ";", "es", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Bogen, aufgespannt,", "tokens": ["Ih\u00b7ren", "Bo\u00b7gen", ",", "auf\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings um sie die G\u00f6tterknaben", "tokens": ["Rings", "um", "sie", "die", "G\u00f6t\u00b7ter\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der r\u00e4cherischen Hand.", "tokens": ["In", "der", "r\u00e4\u00b7che\u00b7ri\u00b7schen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.31": {"line.1": {"text": "K\u00fcssen mu\u00df sie nun den Hirten,", "tokens": ["K\u00fcs\u00b7sen", "mu\u00df", "sie", "nun", "den", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein wollustvolles Ach!", "tokens": ["Und", "ein", "wol\u00b7lust\u00b7vol\u00b7les", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter sanftbewegten Myrthen", "tokens": ["Un\u00b7ter", "sanft\u00b7be\u00b7weg\u00b7ten", "Myr\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seufzet Philomele nach.", "tokens": ["Seuf\u00b7zet", "Phi\u00b7lo\u00b7me\u00b7le", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.32": {"line.1": {"text": "Im Triumphe weggeflogen", "tokens": ["Im", "Tri\u00b7um\u00b7phe", "weg\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die G\u00f6tter, ohne Streit.", "tokens": ["Sind", "die", "G\u00f6t\u00b7ter", ",", "oh\u00b7ne", "Streit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+", 