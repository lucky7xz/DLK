{"textgrid.poem.60022": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lalage, die kleine Spr\u00f6de,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lalage, die kleine Spr\u00f6de,", "tokens": ["La\u00b7la\u00b7ge", ",", "die", "klei\u00b7ne", "Spr\u00f6\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Floh den jungen Lycidas;", "tokens": ["Floh", "den", "jun\u00b7gen", "Ly\u00b7ci\u00b7das", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bittrer Spott war ihre Rede,", "tokens": ["Bit\u00b7trer", "Spott", "war", "ih\u00b7re", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Blicke lauter Ha\u00df.", "tokens": ["Und", "die", "Bli\u00b7cke", "lau\u00b7ter", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In das Thal, zu jener Quelle", "tokens": ["In", "das", "Thal", ",", "zu", "je\u00b7ner", "Quel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockte sie Dianens Schein;", "tokens": ["Lock\u00b7te", "sie", "Di\u00b7a\u00b7nens", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fernher murmelte die Welle,", "tokens": ["Fern\u00b7her", "mur\u00b7mel\u00b7te", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Leise lispelte der Hain.", "tokens": ["Lei\u00b7se", "lis\u00b7pel\u00b7te", "der", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Sanfter wurden ihre Triebe,", "tokens": ["Sanf\u00b7ter", "wur\u00b7den", "ih\u00b7re", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friede ward ihr Herz und Ruh,", "tokens": ["Frie\u00b7de", "ward", "ihr", "Herz", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ein kleines Wort von Liebe", "tokens": ["Denn", "ein", "klei\u00b7nes", "Wort", "von", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rief ihr jedes B\u00fcschchen zu.", "tokens": ["Rief", "ihr", "je\u00b7des", "B\u00fc\u00b7schchen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Liebe sprach die junge Rose,", "tokens": ["Lie\u00b7be", "sprach", "die", "jun\u00b7ge", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach der Quelle gr\u00fcner Rand \u2013", "tokens": ["Sprach", "der", "Quel\u00b7le", "gr\u00fc\u00b7ner", "Rand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als das M\u00e4dchen auf dem Moose", "tokens": ["Als", "das", "M\u00e4d\u00b7chen", "auf", "dem", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlafend einen Knaben fand.", "tokens": ["Schla\u00b7fend", "ei\u00b7nen", "Kna\u00b7ben", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Von dem Monde halb bestralet,", "tokens": ["Von", "dem", "Mon\u00b7de", "halb", "be\u00b7stra\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb in Schatten eingeh\u00fcllt,", "tokens": ["Halb", "in", "Schat\u00b7ten", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lag er im Geb\u00fcsch. Es malet", "tokens": ["Lag", "er", "im", "Ge\u00b7b\u00fcsch", ".", "Es", "ma\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur Albano dieses Bild.", "tokens": ["Nur", "Al\u00b7ba\u00b7no", "die\u00b7ses", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PDAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Seine Miene sagt im Traume,", "tokens": ["Sei\u00b7ne", "Mie\u00b7ne", "sagt", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Liebe wachend denkt.", "tokens": ["Was", "die", "Lie\u00b7be", "wa\u00b7chend", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem n\u00e4chsten Myrthenbaume", "tokens": ["An", "dem", "n\u00e4chs\u00b7ten", "Myr\u00b7then\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein K\u00f6cher aufgeh\u00e4ngt.", "tokens": ["Ist", "ein", "K\u00f6\u00b7cher", "auf\u00b7ge\u00b7h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihm zur Seite gl\u00e4nzt ein Bogen;", "tokens": ["Ihm", "zur", "Sei\u00b7te", "gl\u00e4nzt", "ein", "Bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00e4her geht das M\u00e4dchen hin,", "tokens": ["N\u00e4\u00b7her", "geht", "das", "M\u00e4d\u00b7chen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und allm\u00e4hlig ihm gewogen", "tokens": ["Und", "all\u00b7m\u00e4h\u00b7lig", "ihm", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wird die gute Sch\u00e4ferin.", "tokens": ["Wird", "die", "gu\u00b7te", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Siehst Du nicht auf jenem H\u00fcgel,", "tokens": ["Siehst", "Du", "nicht", "auf", "je\u00b7nem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lalage! die ganze Schaar?", "tokens": ["La\u00b7la\u00b7ge", "!", "die", "gan\u00b7ze", "Schaar", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allerliebste kleine Fl\u00fcgel", "tokens": ["Al\u00b7ler\u00b7liebs\u00b7te", "klei\u00b7ne", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben sie, und goldnes Haar.", "tokens": ["Ha\u00b7ben", "sie", ",", "und", "gold\u00b7nes", "Haar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Schnell bewegen sie die Schwingen;", "tokens": ["Schnell", "be\u00b7we\u00b7gen", "sie", "die", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Quelle sind sie schon,", "tokens": ["An", "der", "Quel\u00b7le", "sind", "sie", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tanzen um das Kind, und singen", "tokens": ["Tan\u00b7zen", "um", "das", "Kind", ",", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieder von Anakreon.", "tokens": ["Lie\u00b7der", "von", "A\u00b7nak\u00b7re\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Aufgewecket durch die Lieder,", "tokens": ["Auf\u00b7ge\u00b7we\u00b7cket", "durch", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht der kleine Gott umher;", "tokens": ["Sieht", "der", "klei\u00b7ne", "Gott", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mischt sich unter seine Br\u00fcder,", "tokens": ["Mischt", "sich", "un\u00b7ter", "sei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Hirtin l\u00e4chelt er.", "tokens": ["Und", "der", "Hir\u00b7tin", "l\u00e4\u00b7chelt", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Tausend neue Bl\u00fcmchen sprie\u00dfen,", "tokens": ["Tau\u00b7send", "neu\u00b7e", "Bl\u00fcm\u00b7chen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sie tanzen, aus dem Klee;", "tokens": ["Wo", "sie", "tan\u00b7zen", ",", "aus", "dem", "Klee", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mitten in den Reihen schlie\u00dfen", "tokens": ["Mit\u00b7ten", "in", "den", "Rei\u00b7hen", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie die sch\u00f6ne Lalage.", "tokens": ["Sie", "die", "sch\u00f6\u00b7ne", "La\u00b7la\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Langsam steigt ihr Busen; leise", "tokens": ["Lang\u00b7sam", "steigt", "ihr", "Bu\u00b7sen", ";", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$.", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcnschet sie, und wei\u00df nicht was.", "tokens": ["W\u00fcn\u00b7schet", "sie", ",", "und", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht doch, neben ihr im Kreise", "tokens": ["Seht", "doch", ",", "ne\u00b7ben", "ihr", "im", "Krei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PPOSAT", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht der junge Lycidas.", "tokens": ["Steht", "der", "jun\u00b7ge", "Ly\u00b7ci\u00b7das", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ihm entfliehen will die Spr\u00f6de,", "tokens": ["Ihm", "ent\u00b7flie\u00b7hen", "will", "die", "Spr\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn verachten soll ihr Blick;", "tokens": ["Ihn", "ver\u00b7ach\u00b7ten", "soll", "ihr", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der J\u00fcngling, nicht mehr bl\u00f6de,", "tokens": ["Doch", "der", "J\u00fcng\u00b7ling", ",", "nicht", "mehr", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt die Sch\u00e4ferin zur\u00fcck.", "tokens": ["H\u00e4lt", "die", "Sch\u00e4\u00b7fe\u00b7rin", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Fliehen kann sie nicht; es haben", "tokens": ["Flie\u00b7hen", "kann", "sie", "nicht", ";", "es", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Bogen, aufgespannt,", "tokens": ["Ih\u00b7ren", "Bo\u00b7gen", ",", "auf\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings um sie die G\u00f6tterknaben", "tokens": ["Rings", "um", "sie", "die", "G\u00f6t\u00b7ter\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der r\u00e4cherischen Hand.", "tokens": ["In", "der", "r\u00e4\u00b7che\u00b7ri\u00b7schen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "K\u00fcssen mu\u00df sie nun den Hirten,", "tokens": ["K\u00fcs\u00b7sen", "mu\u00df", "sie", "nun", "den", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein wollustvolles Ach!", "tokens": ["Und", "ein", "wol\u00b7lust\u00b7vol\u00b7les", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter sanftbewegten Myrthen", "tokens": ["Un\u00b7ter", "sanft\u00b7be\u00b7weg\u00b7ten", "Myr\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seufzet Philomele nach.", "tokens": ["Seuf\u00b7zet", "Phi\u00b7lo\u00b7me\u00b7le", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Im Triumphe weggeflogen", "tokens": ["Im", "Tri\u00b7um\u00b7phe", "weg\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die G\u00f6tter, ohne Streit.", "tokens": ["Sind", "die", "G\u00f6t\u00b7ter", ",", "oh\u00b7ne", "Streit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4chtiger als Amors Bogen", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", "als", "A\u00b7mors", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Ku\u00df der Z\u00e4rtlichkeit.", "tokens": ["Ist", "ein", "Ku\u00df", "der", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Lalage, die kleine Spr\u00f6de,", "tokens": ["La\u00b7la\u00b7ge", ",", "die", "klei\u00b7ne", "Spr\u00f6\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Floh den jungen Lycidas;", "tokens": ["Floh", "den", "jun\u00b7gen", "Ly\u00b7ci\u00b7das", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bittrer Spott war ihre Rede,", "tokens": ["Bit\u00b7trer", "Spott", "war", "ih\u00b7re", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Blicke lauter Ha\u00df.", "tokens": ["Und", "die", "Bli\u00b7cke", "lau\u00b7ter", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "In das Thal, zu jener Quelle", "tokens": ["In", "das", "Thal", ",", "zu", "je\u00b7ner", "Quel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockte sie Dianens Schein;", "tokens": ["Lock\u00b7te", "sie", "Di\u00b7a\u00b7nens", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fernher murmelte die Welle,", "tokens": ["Fern\u00b7her", "mur\u00b7mel\u00b7te", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Leise lispelte der Hain.", "tokens": ["Lei\u00b7se", "lis\u00b7pel\u00b7te", "der", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.19": {"line.1": {"text": "Sanfter wurden ihre Triebe,", "tokens": ["Sanf\u00b7ter", "wur\u00b7den", "ih\u00b7re", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friede ward ihr Herz und Ruh,", "tokens": ["Frie\u00b7de", "ward", "ihr", "Herz", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ein kleines Wort von Liebe", "tokens": ["Denn", "ein", "klei\u00b7nes", "Wort", "von", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rief ihr jedes B\u00fcschchen zu.", "tokens": ["Rief", "ihr", "je\u00b7des", "B\u00fc\u00b7schchen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Liebe sprach die junge Rose,", "tokens": ["Lie\u00b7be", "sprach", "die", "jun\u00b7ge", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach der Quelle gr\u00fcner Rand \u2013", "tokens": ["Sprach", "der", "Quel\u00b7le", "gr\u00fc\u00b7ner", "Rand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als das M\u00e4dchen auf dem Moose", "tokens": ["Als", "das", "M\u00e4d\u00b7chen", "auf", "dem", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlafend einen Knaben fand.", "tokens": ["Schla\u00b7fend", "ei\u00b7nen", "Kna\u00b7ben", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Von dem Monde halb bestralet,", "tokens": ["Von", "dem", "Mon\u00b7de", "halb", "be\u00b7stra\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb in Schatten eingeh\u00fcllt,", "tokens": ["Halb", "in", "Schat\u00b7ten", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lag er im Geb\u00fcsch. Es malet", "tokens": ["Lag", "er", "im", "Ge\u00b7b\u00fcsch", ".", "Es", "ma\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur Albano dieses Bild.", "tokens": ["Nur", "Al\u00b7ba\u00b7no", "die\u00b7ses", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PDAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.22": {"line.1": {"text": "Seine Miene sagt im Traume,", "tokens": ["Sei\u00b7ne", "Mie\u00b7ne", "sagt", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Liebe wachend denkt.", "tokens": ["Was", "die", "Lie\u00b7be", "wa\u00b7chend", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"te