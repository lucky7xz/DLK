{"dta.poem.20705": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den ber\u00fchmten Chodowiecky.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Du mein Auge gut getroffen,                 ", "tokens": ["Der", "Du", "mein", "Au\u00b7ge", "gut", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So, da\u00df Dirs meine Muse dankt,", "tokens": ["So", ",", "da\u00df", "Dirs", "mei\u00b7ne", "Mu\u00b7se", "dankt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Da\u00df sich des Dichters Kopf nicht mit dem Kopfe zankt,", "tokens": ["Da\u00df", "sich", "des", "Dich\u00b7ters", "Kopf", "nicht", "mit", "dem", "Kop\u00b7fe", "zankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der mit ihm in Gesellschaft reiset,", "tokens": ["Der", "mit", "ihm", "in", "Ge\u00b7sell\u00b7schaft", "rei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist so gro\u00df \u2014 ich bin so klein,", "tokens": ["Er", "ist", "so", "gro\u00df", "ich", "bin", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ich sing\u2019 ein Lied, das nichts beweiset,", "tokens": ["Ich", "sing'", "ein", "Lied", ",", "das", "nichts", "be\u00b7wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er singt, um ewig hier zu seyn,", "tokens": ["Er", "singt", ",", "um", "e\u00b7wig", "hier", "zu", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ADJD", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und jede Welt zu \u00fcberf\u00fchren,", "tokens": ["Und", "je\u00b7de", "Welt", "zu", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df Ihn Apollo Sohn genannt;", "tokens": ["Da\u00df", "Ihn", "A\u00b7pol\u00b7lo", "Sohn", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mein Bischen Ruhm wird sich verlieren,", "tokens": ["Mein", "Bi\u00b7schen", "Ruhm", "wird", "sich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn ich ins Geistervaterland", "tokens": ["Wenn", "ich", "ins", "Geis\u00b7ter\u00b7va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hinweggeflattert bin \u2014 und Seiner wird bestehen.", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7flat\u00b7tert", "bin", "und", "Sei\u00b7ner", "wird", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$(", "KON", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Ich bin ein Weib, Er ist ein Mann,", "tokens": ["Ich", "bin", "ein", "Weib", ",", "Er", "ist", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mein Verschen wei\u00df nur deutsch zu gehen,", "tokens": ["Mein", "Ver\u00b7schen", "wei\u00df", "nur", "deutsch", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wenn Sein Vers nach dem Takt des R\u00f6mers tan-", "tokens": ["Wenn", "Sein", "Vers", "nach", "dem", "Takt", "des", "R\u00f6\u00b7mers", "tan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Der f\u00fcr die Jugend Roms ein Jubellied gedichtet,", "tokens": ["Der", "f\u00fcr", "die", "Ju\u00b7gend", "Roms", "ein", "Ju\u00b7bel\u00b7lied", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und f\u00fcr den C\u00e4sar und f\u00fcr sich", "tokens": ["Und", "f\u00fcr", "den", "C\u00e4\u00b7sar", "und", "f\u00fcr", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NE", "KON", "APPR", "PRF"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Ein dauernd Denkmahl aufgerichtet;", "tokens": ["Ein", "dau\u00b7ernd", "Denk\u00b7mahl", "auf\u00b7ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Auch Du errichtest eins f\u00fcr Dich", "tokens": ["Auch", "Du", "er\u00b7rich\u00b7test", "eins", "f\u00fcr", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PIS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "In den Figuren, die mit Blicken,", "tokens": ["In", "den", "Fi\u00b7gu\u00b7ren", ",", "die", "mit", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und mit Geberden sprechend sind.", "tokens": ["Und", "mit", "Ge\u00b7ber\u00b7den", "spre\u00b7chend", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.23": {"text": "Der Pastor Sebald", "tokens": ["Der", "Pas\u00b7tor", "Se\u00b7bald"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Sein todtes Weib, sein starres Kind,", "tokens": ["Sein", "tod\u00b7tes", "Weib", ",", "sein", "star\u00b7res", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und seine Tochter Mariane,", "tokens": ["Und", "sei\u00b7ne", "Toch\u00b7ter", "Ma\u00b7ri\u00b7a\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der Bauer und sein Freund, der von der Reisebahne", "tokens": ["Der", "Bau\u00b7er", "und", "sein", "Freund", ",", "der", "von", "der", "Rei\u00b7se\u00b7bah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Befl\u00fcgelt ihm zu tr\u00f6sten kam,", "tokens": ["Be\u00b7fl\u00fc\u00b7gelt", "ihm", "zu", "tr\u00f6s\u00b7ten", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sind insgesammt so wahr gezeichnet, da\u00df der Gram", "tokens": ["Sind", "ins\u00b7ge\u00b7sammt", "so", "wahr", "ge\u00b7zeich\u00b7net", ",", "da\u00df", "der", "Gram"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mich selbst ergreift, wenn ich sie sehe \u2014", "tokens": ["Mich", "selbst", "er\u00b7greift", ",", "wenn", "ich", "sie", "se\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und wann die Frau von Hohenauf", "tokens": ["Und", "wann", "die", "Frau", "von", "Ho\u00b7hen\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Das arme M\u00e4dchen schreckt \u2014 dann f\u00fchl\u2019 ich", "tokens": ["Das", "ar\u00b7me", "M\u00e4d\u00b7chen", "schreckt", "dann", "f\u00fchl'", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Vom Haupte bis zum Fu\u00df, und flehe", "tokens": ["Vom", "Haup\u00b7te", "bis", "zum", "Fu\u00df", ",", "und", "fle\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die G\u00f6tter f\u00fcr das junge Paar,", "tokens": ["Die", "G\u00f6t\u00b7ter", "f\u00fcr", "das", "jun\u00b7ge", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Das in der sch\u00f6nen Sommerlaube", "tokens": ["Das", "in", "der", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "So keusch verliebt, so z\u00e4rtlich war;", "tokens": ["So", "keusch", "ver\u00b7liebt", ",", "so", "z\u00e4rt\u00b7lich", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin get\u00e4uscht, ich denk und glaube,", "tokens": ["Ich", "bin", "ge\u00b7t\u00e4uscht", ",", "ich", "denk", "und", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df alles wirklich vor mir sey,", "tokens": ["Da\u00df", "al\u00b7les", "wirk\u00b7lich", "vor", "mir", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der J\u00fcngling und die b\u00f6se Tante;", "tokens": ["Der", "J\u00fcng\u00b7ling", "und", "die", "b\u00f6\u00b7se", "Tan\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir d\u00fcnkt, mein Ohr h\u00f6rt ihr Geschrey,", "tokens": ["Mir", "d\u00fcnkt", ",", "mein", "Ohr", "h\u00f6rt", "ihr", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und jedes Afterwort der Zunge, die da brannte", "tokens": ["Und", "je\u00b7des", "Af\u00b7ter\u00b7wort", "der", "Zun\u00b7ge", ",", "die", "da", "brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vom Grimm, der aus dem Herzen fuhr,", "tokens": ["Vom", "Grimm", ",", "der", "aus", "dem", "Her\u00b7zen", "fuhr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Flammen aus dem H\u00f6llenschlunde;", "tokens": ["Wie", "Flam\u00b7men", "aus", "dem", "H\u00f6l\u00b7len\u00b7schlun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich hasse diese Kreatur,", "tokens": ["Ich", "has\u00b7se", "die\u00b7se", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit ihrem Mops-gestalten Munde,", "tokens": ["Mit", "ih\u00b7rem", "Mops\u00b7ge\u00b7stal\u00b7ten", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und f\u00fchle ganz des S\u00e4uglings Schmerz,", "tokens": ["Und", "f\u00fch\u00b7le", "ganz", "des", "S\u00e4ug\u00b7lings", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der fast versteinert ist im Starren,", "tokens": ["Der", "fast", "ver\u00b7stei\u00b7nert", "ist", "im", "Star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Stirn herunter h\u00e4ngt aufs Herz,", "tokens": ["Die", "Stirn", "her\u00b7un\u00b7ter", "h\u00e4ngt", "aufs", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den Handschuh fallen lie\u00df, weil er vor einen Narren", "tokens": ["Den", "Hand\u00b7schuh", "fal\u00b7len", "lie\u00df", ",", "weil", "er", "vor", "ei\u00b7nen", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In Folio gescholten ward,", "tokens": ["In", "Fo\u00b7lio", "ge\u00b7schol\u00b7ten", "ward", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Vor einen blinden, dummen Knaben,", "tokens": ["Vor", "ei\u00b7nen", "blin\u00b7den", ",", "dum\u00b7men", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der sichs im Kopf gesetzt, ein M\u00e4dchen solcher Art,", "tokens": ["Der", "sichs", "im", "Kopf", "ge\u00b7setzt", ",", "ein", "M\u00e4d\u00b7chen", "sol\u00b7cher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVPP", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein staubgebornes Kind, zur \u00e4chten Frau zu haben \u2014", "tokens": ["Ein", "staub\u00b7ge\u00b7b