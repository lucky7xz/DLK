{"textgrid.poem.32975": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "Lumpenlied", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein armer Be-Bi-Ba-", "tokens": ["Ich", "bin", "ein", "ar\u00b7mer", "Be\u00b7Bi\u00b7Ba"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bo-Bettelmusikant,", "tokens": ["Bo\u00b7Bet\u00b7tel\u00b7mu\u00b7si\u00b7kant", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "doch kreuzfidel stets pe-pi-pa-", "tokens": ["doch", "kreuz\u00b7fi\u00b7del", "stets", "pe\u00b7pi\u00b7pa"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "TRUNC"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "po-pump ich mich durchs Land;", "tokens": ["po\u00b7pump", "ich", "mich", "durchs", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "zu spielen gibt's all\u00fcberall,", "tokens": ["zu", "spie\u00b7len", "gibt's", "al\u00b7l\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bar Geld nur leider keins,", "tokens": ["bar", "Geld", "nur", "lei\u00b7der", "keins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "ADV", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "und dennoch bleib ich, was ich bin", "tokens": ["und", "den\u00b7noch", "bleib", "ich", ",", "was", "ich", "bin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und pfi-pfa-pfeif mir eins!", "tokens": ["und", "pfi\u00b7pfa\u00b7pfeif", "mir", "eins", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ob hier, ob dort, was verfa-fe-", "tokens": ["Ob", "hier", ",", "ob", "dort", ",", "was", "ver\u00b7fa\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "$,", "KOUS", "ADV", "$,", "PWS", "TRUNC"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "was verfo-fu-versicht's?!", "tokens": ["was", "ver\u00b7fo\u00b7fu\u00b7ver\u00b7sicht's", "?!"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "ein K\u00fcnstler kam sein La-Li-Le-", "tokens": ["ein", "K\u00fcnst\u00b7ler", "kam", "sein", "La\u00b7Li\u00b7Le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lo-Lebtag noch zu nichts!", "tokens": ["Lo\u00b7Leb\u00b7tag", "noch", "zu", "nichts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und da dies mal jedweder Kunst", "tokens": ["und", "da", "dies", "mal", "jed\u00b7we\u00b7der", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PDS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "betr\u00fcbter Erdenlauf,", "tokens": ["be\u00b7tr\u00fcb\u00b7ter", "Er\u00b7den\u00b7lauf", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "so plag dich nicht umsi-sa-sunst", "tokens": ["so", "plag", "dich", "nicht", "um\u00b7si\u00b7sa\u00b7sunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und pfi-pfa-pfeif darauf!", "tokens": ["und", "pfi\u00b7pfa\u00b7pfeif", "da\u00b7rauf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch ich hab einst von Ra-Re-Ri-", "tokens": ["Auch", "ich", "hab", "einst", "von", "Ra\u00b7Re\u00b7Ri"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "APPR", "TRUNC"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "von Ri-Ro-Ruhm getr\u00e4umt", "tokens": ["von", "Ri\u00b7Ro\u00b7Ruhm", "ge\u00b7tr\u00e4umt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und hab damit mich ma-me-mi-", "tokens": ["und", "hab", "da\u00b7mit", "mich", "ma\u00b7me\u00b7mi"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "PPER", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mu-m\u00e4chtiglich geleimt!", "tokens": ["mu\u00b7m\u00e4ch\u00b7tig\u00b7lich", "ge\u00b7leimt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum nahm ich einen Nagel und \u2013", "tokens": ["Drum", "nahm", "ich", "ei\u00b7nen", "Na\u00b7gel", "und", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NE", "KON", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und hing den Kram dran auf", "tokens": ["und", "hing", "den", "Kram", "dran", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "und wurde Vi-Va-Vagabund", "tokens": ["und", "wur\u00b7de", "Vi\u00b7Va\u00b7Va\u00b7ga\u00b7bund"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und pfi-pfa-pfoff darauf!", "tokens": ["und", "pfi\u00b7pfa\u00b7pfoff", "da\u00b7rauf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Bettelmusike-ki-ko-", "tokens": ["Ein", "Bet\u00b7tel\u00b7mu\u00b7si\u00b7ke\u00b7ki\u00b7ko"], "token_info": ["word", "word"], "pos": ["ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ku-kant ist auch nicht schlecht,", "tokens": ["ku\u00b7kant", "ist", "auch", "nicht", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "und wer einmal ein Le-Li-Lo-", "tokens": ["und", "wer", "ein\u00b7mal", "ein", "Le\u00b7Li\u00b7Lo"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La-Lump ist, sei's auch recht!", "tokens": ["La\u00b7Lump", "ist", ",", "sei's", "auch", "recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum Mi-Ma-Millio-n\u00f6-n\u00fc-n\u00e4r", "tokens": ["Zum", "Mi\u00b7Ma\u00b7Mil\u00b7lio\u00b7n\u00f6\u00b7n\u00fc\u00b7n\u00e4r"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bringt doch von uns es keins,", "tokens": ["bringt", "doch", "von", "uns", "es", "keins", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PPER", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "drum bleib ich, was ich bi-ba-bin", "tokens": ["drum", "bleib", "ich", ",", "was", "ich", "bi\u00b7ba\u00b7bin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWS", "PPER", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und pfi-pfa-pfeif mir eins!", "tokens": ["und", "pfi\u00b7pfa\u00b7pfeif", "mir", "eins", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir machen unsern Di-Da-Du-", "tokens": ["Wir", "ma\u00b7chen", "un\u00b7sern", "Di\u00b7Da\u00b7Du"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Do-Dank dem Publiko:", "tokens": ["Do\u00b7Dank", "dem", "Pub\u00b7li\u00b7ko", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "es bleib wie wir stets kri-kra-kru-", "tokens": ["es", "bleib", "wie", "wir", "stets", "kri\u00b7kra\u00b7kru"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "kro-kreuzfidel und froh!", "tokens": ["kro\u00b7kreuz\u00b7fi\u00b7del", "und", "froh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ein Mensch, der keinen Spa\u00df versteht,", "tokens": ["Ein", "Mensch", ",", "der", "kei\u00b7nen", "Spa\u00df", "ver\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "merkt euch zum Schli-Schla-Schlu\u00df,", "tokens": ["merkt", "euch", "zum", "Schli\u00b7Schla\u00b7Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "bleibt ewiglich ein Rha-Rhe-Rhi-", "tokens": ["bleibt", "e\u00b7wig\u00b7lich", "ein", "Rha\u00b7R\u00b7he\u00b7R\u00b7hi"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Rho-Rhu-Rhinozerus!", "tokens": ["Rho\u00b7R\u00b7hu\u00b7R\u00b7hi\u00b7no\u00b7ze\u00b7rus", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ich bin ein armer Be-Bi-Ba-", "tokens": ["Ich", "bin", "ein", "ar\u00b7mer", "Be\u00b7Bi\u00b7Ba"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bo-Bettelmusikant,", "tokens": ["Bo\u00b7Bet\u00b7tel\u00b7mu\u00b7si\u00b7kant", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "doch kreuzfidel stets pe-pi-pa-", "tokens": ["doch", "kreuz\u00b7fi\u00b7del", "stets", "pe\u00b7pi\u00b7pa"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", 