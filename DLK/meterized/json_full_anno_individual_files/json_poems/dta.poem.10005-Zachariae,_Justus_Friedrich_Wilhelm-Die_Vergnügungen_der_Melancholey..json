{"dta.poem.10005": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aber wenn ietzo die Welt in der Mitternacht Ra-", "tokens": ["A\u00b7ber", "wenn", "iet\u00b7zo", "die", "Welt", "in", "der", "Mit\u00b7ter\u00b7nacht", "Ra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.2": {"text": "Eingekleidet, dann la\u00df mich auch oft im hallenden", "tokens": ["Ein\u00b7ge\u00b7klei\u00b7det", ",", "dann", "la\u00df", "mich", "auch", "oft", "im", "hal\u00b7len\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "VVIMP", "PPER", "ADV", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Jene zitternden Flammen erblicken, die \u00fcber die Haufen", "tokens": ["Je\u00b7ne", "zit\u00b7tern\u00b7den", "Flam\u00b7men", "er\u00b7bli\u00b7cken", ",", "die", "\u00fc\u00b7ber", "die", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVINF", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "D\u00fcrrer Knochen und Sch\u00e4del mit blassem Schimmer", "tokens": ["D\u00fcr\u00b7rer", "Kno\u00b7chen", "und", "Sch\u00e4\u00b7del", "mit", "blas\u00b7sem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Da indes die Mauer hinab \u00e4therische Stimmen", "tokens": ["Da", "in\u00b7des", "die", "Mau\u00b7er", "hin\u00b7ab", "\u00e4t\u00b7he\u00b7ri\u00b7sche", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "In den Kirchhof ert\u00f6nen, und Geistergestalten von ferne,", "tokens": ["In", "den", "Kirch\u00b7hof", "er\u00b7t\u00f6\u00b7nen", ",", "und", "Geis\u00b7ter\u00b7ge\u00b7stal\u00b7ten", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "KON", "NN", "APPR", "ADJA", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Durch die langen gekr\u00fcmmten Gew\u00f6lbe, die einsamen", "tokens": ["Durch", "die", "lan\u00b7gen", "ge\u00b7kr\u00fcmm\u00b7ten", "Ge\u00b7w\u00f6l\u00b7be", ",", "die", "ein\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Zu sich winken. \u2014 Voll Anmuth ist auch der Mitter-", "tokens": ["Zu", "sich", "win\u00b7ken", ".", "Voll", "An\u00b7muth", "ist", "auch", "der", "Mit\u00b7ter"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "VVINF", "$.", "$(", "ADJD", "NN", "VAFIN", "ADV", "ART", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.9": {"text": "Wenn ich pl\u00f6tzlich erwacht mich von dem Lager erhebe.", "tokens": ["Wenn", "ich", "pl\u00f6tz\u00b7lich", "er\u00b7wacht", "mich", "von", "dem", "La\u00b7ger", "er\u00b7he\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Siehe! wie todt ist alles um mich! Die ruhigen Winde", "tokens": ["Sie\u00b7he", "!", "wie", "todt", "ist", "al\u00b7les", "um", "mich", "!", "Die", "ru\u00b7hi\u00b7gen", "Win\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "PWAV", "ADJD", "VAFIN", "PIS", "APPR", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Brausen ietzt nicht; die S\u00f6hne der Menschen, und alle", "tokens": ["Brau\u00b7sen", "ietzt", "nicht", ";", "die", "S\u00f6h\u00b7ne", "der", "Men\u00b7schen", ",", "und", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "$.", "ART", "NN", "ART", "NN", "$,", "KON", "PIAT"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Liegen in tiefer Vergessenheit da; die ganze Natur ist", "tokens": ["Lie\u00b7gen", "in", "tie\u00b7fer", "Ver\u00b7ges\u00b7sen\u00b7heit", "da", ";", "die", "gan\u00b7ze", "Na\u00b7tur", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VAFIN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.13": {"text": "In den tiefesten Schlaf, in die tiefeste Stille, gewickelt.", "tokens": ["In", "den", "tie\u00b7fes\u00b7ten", "Schlaf", ",", "in", "die", "tie\u00b7fes\u00b7te", "Stil\u00b7le", ",", "ge\u00b7wi\u00b7ckelt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "O wie grausend ist dann der Gedanke, da\u00df au\u00dfer mir,", "tokens": ["O", "wie", "grau\u00b7send", "ist", "dann", "der", "Ge\u00b7dan\u00b7ke", ",", "da\u00df", "au\u00b7\u00dfer", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "KOUS", "APPR", "PPER", "$,"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Auf der ver\u00f6deten Erde noch wacht! Bis mit dem Ge-", "tokens": ["Auf", "der", "ver\u00b7\u00f6\u00b7de\u00b7ten", "Er\u00b7de", "noch", "wacht", "!", "Bis", "mit", "dem", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVFIN", "$.", "APPR", "APPR", "ART", "TRUNC"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.16": {"text": "Mein hinsinkendes Haupt der schleichende Schlummer", "tokens": ["Mein", "hin\u00b7sin\u00b7ken\u00b7des", "Haupt", "der", "schlei\u00b7chen\u00b7de", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Dann auch m\u00fcsse kein Traum, von fr\u00f6hlicher Thorheit", "tokens": ["Dann", "auch", "m\u00fcs\u00b7se", "kein", "Traum", ",", "von", "fr\u00f6h\u00b7li\u00b7cher", "Thor\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PIAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Mich zur blumichten Au der gauckelnden Freude ver-", "tokens": ["Mich", "zur", "blu\u00b7mich\u00b7ten", "Au", "der", "gau\u00b7ckeln\u00b7den", "Freu\u00b7de", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Sondern mir sende der Schutzgeist der Nacht so my-", "tokens": ["Son\u00b7dern", "mir", "sen\u00b7de", "der", "Schutz\u00b7geist", "der", "Nacht", "so", "my"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.20": {"text": "So erhabne Gesichter, wie ehmals Spenser gesehen,", "tokens": ["So", "er\u00b7hab\u00b7ne", "Ge\u00b7sich\u00b7ter", ",", "wie", "eh\u00b7mals", "Spen\u00b7ser", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PWAV", "ADV", "NN", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Wenn er v\u00f6llig vertieft in Phantaseyen der Dichtkunst,", "tokens": ["Wenn", "er", "v\u00f6l\u00b7lig", "ver\u00b7tieft", "in", "Phan\u00b7ta\u00b7se\u00b7yen", "der", "Dicht\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Zu des Busirans schwarzen Palast den Britomart f\u00fchrte:", "tokens": ["Zu", "des", "Bu\u00b7si\u00b7rans", "schwar\u00b7zen", "Pa\u00b7last", "den", "Bri\u00b7to\u00b7mart", "f\u00fchr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.23": {"text": "Oder als Milton gesehn, wenn er in hoher Begeistrung,", "tokens": ["O\u00b7der", "als", "Mil\u00b7ton", "ge\u00b7sehn", ",", "wenn", "er", "in", "ho\u00b7her", "Be\u00b7geis\u00b7trung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "VVPP", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Jm Tumulte des Kriegs, den ganzen Himmel sich dachte,", "tokens": ["Jm", "Tu\u00b7mul\u00b7te", "des", "Kriegs", ",", "den", "gan\u00b7zen", "Him\u00b7mel", "sich", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "Und in seinen entz\u00fcckten Gedanken der Seraphim", "tokens": ["Und", "in", "sei\u00b7nen", "ent\u00b7z\u00fcck\u00b7ten", "Ge\u00b7dan\u00b7ken", "der", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Vor ihm sich th\u00fcrmten, mit Waffen bedeckt von De-", "tokens": ["Vor", "ihm", "sich", "th\u00fcrm\u00b7ten", ",", "mit", "Waf\u00b7fen", "be\u00b7deckt", "von", "De"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PRF", "VVFIN", "$,", "APPR", "NN", "VVPP", "APPR", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}