{"textgrid.poem.41579": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am ersten Sonntage im Advent", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist so mild,", "tokens": ["Du", "bist", "so", "mild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So reich an Duldung, liebster Hort,", "tokens": ["So", "reich", "an", "Dul\u00b7dung", ",", "liebs\u00b7ter", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mu\u00dft so wilde Streiter haben;", "tokens": ["Und", "mu\u00dft", "so", "wil\u00b7de", "Strei\u00b7ter", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein heilig Bild", "tokens": ["Dein", "hei\u00b7lig", "Bild"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ragt \u00fcberm stolzen Banner fort,", "tokens": ["Ragt", "\u00fc\u00b7berm", "stol\u00b7zen", "Ban\u00b7ner", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und deine Zeichen will man graben", "tokens": ["Und", "dei\u00b7ne", "Zei\u00b7chen", "will", "man", "gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In Speer und funkenspr\u00fchnden Schild.", "tokens": ["In", "Speer", "und", "fun\u00b7ken\u00b7spr\u00fchn\u00b7den", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit Spott und Hohn", "tokens": ["Mit", "Spott", "und", "Hohn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gewaffnet hat Parteienwut,", "tokens": ["Ge\u00b7waff\u00b7net", "hat", "Par\u00b7tei\u00b7en\u00b7wut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was deinen sanften Namen tr\u00e4get,", "tokens": ["Was", "dei\u00b7nen", "sanf\u00b7ten", "Na\u00b7men", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und klirrend schon", "tokens": ["Und", "klir\u00b7rend", "schon"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hat in des frommsten Lammes Blut", "tokens": ["Hat", "in", "des", "fromms\u00b7ten", "Lam\u00b7mes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Fehdehandschuh man geleget,", "tokens": ["Den", "Feh\u00b7de\u00b7hand\u00b7schuh", "man", "ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Zepter an die Dornenkron'.", "tokens": ["Den", "Zep\u00b7ter", "an", "die", "Dor\u00b7nen\u00b7kron'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So bleibt es wahr,", "tokens": ["So", "bleibt", "es", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was wandelt durch des Volkes Mund,", "tokens": ["Was", "wan\u00b7delt", "durch", "des", "Vol\u00b7kes", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wo man deinen Tempel schauet", "tokens": ["Da\u00df", "wo", "man", "dei\u00b7nen", "Tem\u00b7pel", "schau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "PIS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mild und klar,", "tokens": ["So", "mild", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dicht neben den geweihten Grund", "tokens": ["Dicht", "ne\u00b7ben", "den", "ge\u00b7weih\u00b7ten", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Der Teufel seine Zelle bauet,", "tokens": ["Der", "Teu\u00b7fel", "sei\u00b7ne", "Zel\u00b7le", "bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich w\u00e4rmt die Schlange am Altar.", "tokens": ["Sich", "w\u00e4rmt", "die", "Schlan\u00b7ge", "am", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn Stirn an Stirn", "tokens": ["Wenn", "Stirn", "an", "Stirn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Sich dr\u00e4ngen mit verwirrtem Schrei", "tokens": ["Sich", "dr\u00e4n\u00b7gen", "mit", "ver\u00b7wirr\u00b7tem", "Schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die K\u00e4mpfer um geweihte Sache,", "tokens": ["Die", "K\u00e4mp\u00b7fer", "um", "ge\u00b7weih\u00b7te", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn in dem Hirn", "tokens": ["Wenn", "in", "dem", "Hirn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mehr schwindelt von der Welt Geb\u00e4u,", "tokens": ["Mehr", "schwin\u00b7delt", "von", "der", "Welt", "Ge\u00b7b\u00e4u", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Siegesjubel, Ehr' und Rache", "tokens": ["Von", "Sie\u00b7ges\u00b7ju\u00b7bel", ",", "Ehr'", "und", "Ra\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mehr z\u00e4he Mottenf\u00e4den schwirrn", "tokens": ["Mehr", "z\u00e4\u00b7he", "Mot\u00b7ten\u00b7f\u00e4\u00b7den", "schwirrn"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Als stark und rein", "tokens": ["Als", "stark", "und", "rein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Treue Nothemd weben sich", "tokens": ["Der", "Treu\u00b7e", "No\u00b7themd", "we\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt', von des Herzens Schlag ger\u00f6tet:", "tokens": ["Sollt'", ",", "von", "des", "Her\u00b7zens", "Schlag", "ge\u00b7r\u00f6\u00b7tet", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer denkt der Pein", "tokens": ["Wer", "denkt", "der", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durchzuckend wie mit Messern dich,", "tokens": ["Durch\u00b7zu\u00b7ckend", "wie", "mit", "Mes\u00b7sern", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als f\u00fcr die Kreuz'ger du gebetet!", "tokens": ["Als", "f\u00fcr", "die", "Kreuz'\u00b7ger", "du", "ge\u00b7be\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O Herr sind dies die Diener dein?", "tokens": ["O", "Herr", "sind", "dies", "die", "Die\u00b7ner", "dein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PDS", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie liegt der Fluch", "tokens": ["Wie", "liegt", "der", "Fluch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Doch \u00fcber allen, deren Hand", "tokens": ["Doch", "\u00fc\u00b7ber", "al\u00b7len", ",", "de\u00b7ren", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PIS", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch r\u00fchrt die S\u00fcndenmutter Erde!", "tokens": ["Noch", "r\u00fchrt", "die", "S\u00fcn\u00b7den\u00b7mut\u00b7ter", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist's nicht genug,", "tokens": ["Ist's", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da\u00df sich der Fl\u00fcchtling w\u00e4rmt am Brand", "tokens": ["Da\u00df", "sich", "der", "Fl\u00fccht\u00b7ling", "w\u00e4rmt", "am", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der H\u00fctte? mu\u00df auf deinem Herde", "tokens": ["Der", "H\u00fct\u00b7te", "?", "mu\u00df", "auf", "dei\u00b7nem", "Her\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Flamme sch\u00fcrn unsel'ger Trug?", "tokens": ["Die", "Flam\u00b7me", "sch\u00fcrn", "un\u00b7sel'\u00b7ger", "Trug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Wer um ein Gut", "tokens": ["Wer", "um", "ein", "Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Welt die Sehnsucht sich verdarb,", "tokens": ["Der", "Welt", "die", "Sehn\u00b7sucht", "sich", "ver\u00b7darb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den mu\u00df der finstre Geist umfahren:", "tokens": ["Den", "mu\u00df", "der", "finst\u00b7re", "Geist", "um\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch was dein Blut,", "tokens": ["Doch", "was", "dein", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dein heilig Dulden uns erwarb:", "tokens": ["Dein", "hei\u00b7lig", "Dul\u00b7den", "uns", "er\u00b7warb", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das sollten knieend wir bewahren", "tokens": ["Das", "soll\u00b7ten", "kni\u00b7e\u00b7end", "wir", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit starkem aber reinem Mut.", "tokens": ["Mit", "star\u00b7kem", "a\u00b7ber", "rei\u00b7nem", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Allm\u00e4cht'ger du!", "tokens": ["All\u00b7m\u00e4cht'\u00b7ger", "du", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In dieser Zeit, wo dringend not", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "wo", "drin\u00b7gend", "not"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df rein dein Heiligtum sich zeige,", "tokens": ["Da\u00df", "rein", "dein", "Hei\u00b7lig\u00b7tum", "sich", "zei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O la\u00df nicht zu", "tokens": ["O", "la\u00df", "nicht", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "PTKZU"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da\u00df L\u00e4sterung, die lauernd droht,", "tokens": ["Da\u00df", "L\u00e4s\u00b7te\u00b7rung", ",", "die", "lau\u00b7ernd", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Versch\u00fctten darf des Hefens Neige", "tokens": ["Ver\u00b7sch\u00fct\u00b7ten", "darf", "des", "He\u00b7fens", "Nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und, ach, den klaren Trank dazu!", "tokens": ["Und", ",", "ach", ",", "den", "kla\u00b7ren", "Trank", "da\u00b7zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df alle Treu'", "tokens": ["La\u00df", "al\u00b7le", "Treu'"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und allen standhaft echten Mut", "tokens": ["Und", "al\u00b7len", "stand\u00b7haft", "ech\u00b7ten", "Mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufflammen, immer licht und lichter;", "tokens": ["Auf\u00b7flam\u00b7men", ",", "im\u00b7mer", "licht", "und", "lich\u00b7ter", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Opfer sei", "tokens": ["Kein", "Op\u00b7fer", "sei"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zu gro\u00df f\u00fcr ein unsch\u00e4tzbar Gut", "tokens": ["Zu", "gro\u00df", "f\u00fcr", "ein", "un\u00b7sch\u00e4tz\u00b7bar", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "APPR", "ART", "ADJD", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Und deine Scharen m\u00f6gen dichter", "tokens": ["Und", "dei\u00b7ne", "Scha\u00b7ren", 