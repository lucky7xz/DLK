{"dta.poem.13245": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201emir scheints, wir haben schon die Lorber in der Hand.", "tokens": ["\u201e", "mir", "scheints", ",", "wir", "ha\u00b7ben", "schon", "die", "Lor\u00b7ber", "in", "der", "Hand", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eder Thron ist voller Sieg, der Feind verl\u00e4\u00dft das Land!", "tokens": ["\u201e", "der", "Thron", "ist", "vol\u00b7ler", "Sieg", ",", "der", "Feind", "ver\u00b7l\u00e4\u00dft", "das", "Land", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201everzeih, ", "tokens": ["\u201e", "ver\u00b7zeih", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVIMP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "\u201ewo man auf GOtt vertraut, ist Zweifeln ein Verbrechen.", "tokens": ["\u201e", "wo", "man", "auf", "Gott", "ver\u00b7traut", ",", "ist", "Zwei\u00b7feln", "ein", "Ver\u00b7bre\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "APPR", "NN", "ADJD", "$,", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambi