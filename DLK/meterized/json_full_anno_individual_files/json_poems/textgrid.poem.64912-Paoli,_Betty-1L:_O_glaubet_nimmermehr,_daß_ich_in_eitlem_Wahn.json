{"textgrid.poem.64912": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O glaubet nimmermehr, da\u00df ich in eitlem Wahn", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O glaubet nimmermehr, da\u00df ich in eitlem Wahn", "tokens": ["O", "glau\u00b7bet", "nim\u00b7mer\u00b7mehr", ",", "da\u00df", "ich", "in", "eit\u00b7lem", "Wahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch hier belehren will! das ma\u00df' ich mir nicht an.", "tokens": ["Euch", "hier", "be\u00b7leh\u00b7ren", "will", "!", "das", "ma\u00df'", "ich", "mir", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$.", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ausdeuten will ich nicht des Lebens Zauberstrahlen,", "tokens": ["Aus\u00b7deu\u00b7ten", "will", "ich", "nicht", "des", "Le\u00b7bens", "Zau\u00b7ber\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur schildern, wie sie sich in meinem Geiste malen.", "tokens": ["Nur", "schil\u00b7dern", ",", "wie", "sie", "sich", "in", "mei\u00b7nem", "Geis\u00b7te", "ma\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PWAV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bestimmen will ich nicht, was Euch zu thun zukommt,", "tokens": ["Be\u00b7stim\u00b7men", "will", "ich", "nicht", ",", "was", "Euch", "zu", "thun", "zu\u00b7kommt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur k\u00fcnden treu und wahr, was meinem Sein gefrommt.", "tokens": ["Nur", "k\u00fcn\u00b7den", "treu", "und", "wahr", ",", "was", "mei\u00b7nem", "Sein", "ge\u00b7frommt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht will zu Euerm Kampf die Waffen ich erlesen,", "tokens": ["Nicht", "will", "zu", "Eu\u00b7erm", "Kampf", "die", "Waf\u00b7fen", "ich", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur sagen, welche mir am dienlichsten gewesen.", "tokens": ["Nur", "sa\u00b7gen", ",", "wel\u00b7che", "mir", "am", "dien\u00b7lichs\u00b7ten", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PRELS", "PPER", "APPRART", "ADJA", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und f\u00fchlet Euer Herz, da\u00df es dem meinen gleicht,", "tokens": ["Und", "f\u00fch\u00b7let", "Eu\u00b7er", "Herz", ",", "da\u00df", "es", "dem", "mei\u00b7nen", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann thut, was ich gethan, bis Gott den Sieg Euch reicht.", "tokens": ["Dann", "thut", ",", "was", "ich", "ge\u00b7than", ",", "bis", "Gott", "den", "Sieg", "Euch", "reicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$,", "KOUS", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn nicht, so kann's Euch doch in keinem Falle schaden,", "tokens": ["Wenn", "nicht", ",", "so", "kann's", "Euch", "doch", "in", "kei\u00b7nem", "Fal\u00b7le", "scha\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertraut zu werden auch mit fremden Lebenspfaden.", "tokens": ["Ver\u00b7traut", "zu", "wer\u00b7den", "auch", "mit", "frem\u00b7den", "Le\u00b7ben\u00b7spfa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Denn lehren wird es Euch, da\u00df man auf jeder Bahn,", "tokens": ["Denn", "leh\u00b7ren", "wird", "es", "Euch", ",", "da\u00df", "man", "auf