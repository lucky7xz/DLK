{"textgrid.poem.26853": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Perlen baut sich ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Perlen baut sich eine Br\u00fccke", "tokens": ["Von", "Per\u00b7len", "baut", "sich", "ei\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch \u00fcber einen grauen See,", "tokens": ["Hoch", "\u00fc\u00b7ber", "ei\u00b7nen", "grau\u00b7en", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie baut sich auf im Augenblicke,", "tokens": ["Sie", "baut", "sich", "auf", "im", "Au\u00b7gen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwindelnd steigt sie in die H\u00f6h.", "tokens": ["Und", "schwin\u00b7delnd", "steigt", "sie", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der h\u00f6chsten Schiffe h\u00f6chste Masten", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Schif\u00b7fe", "h\u00f6chs\u00b7te", "Mas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ziehn unter ihrem Bogen hin,", "tokens": ["Ziehn", "un\u00b7ter", "ih\u00b7rem", "Bo\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie selber trug noch keine Lasten", "tokens": ["Sie", "sel\u00b7ber", "trug", "noch", "kei\u00b7ne", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und scheint, wenn du ihr nahst, zu fliehn.", "tokens": ["Und", "scheint", ",", "wenn", "du", "ihr", "nahst", ",", "zu", "fliehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie ", "tokens": ["Sie"], "token_info": ["word"], "pos": ["PPER"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Sowie des Wassers Flut versiegt.", "tokens": ["So\u00b7wie", "des", "Was\u00b7sers", "Flut", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sprich, ", "tokens": ["So", "sprich", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und wer sie k\u00fcnstlich hat gef\u00fcgt?", "tokens": ["Und", "wer", "sie", "k\u00fcnst\u00b7lich", "hat", "ge\u00b7f\u00fcgt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Diese Br\u00fccke, die von Perlen sich erbaut,", "tokens": ["Die\u00b7se", "Br\u00fc\u00b7cke", ",", "die", "von", "Per\u00b7len", "sich", "er\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sich gl\u00e4nzend hebt und in die L\u00fcfte gr\u00fcndet,", "tokens": ["Sich", "gl\u00e4n\u00b7zend", "hebt", "und", "in", "die", "L\u00fcf\u00b7te", "gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die mit dem Strom erst wird und mit dem Strome schwindet", "tokens": ["Die", "mit", "dem", "Strom", "erst", "wird", "und", "mit", "dem", "Stro\u00b7me", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VAFIN", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und \u00fcber die kein Wandrer noch gezogen,", "tokens": ["Und", "\u00fc\u00b7ber", "die", "kein", "Wand\u00b7rer", "noch", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am Himmel siehst du sie, sie hei\u00dft \u2013 ", "tokens": ["Am", "Him\u00b7mel", "siehst", "du", "sie", ",", "sie", "hei\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Von Perlen baut sich eine Br