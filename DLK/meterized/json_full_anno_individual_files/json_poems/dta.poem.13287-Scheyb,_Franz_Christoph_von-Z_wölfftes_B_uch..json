{"dta.poem.13287": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie h\u00e4tte mehr geredt: allein es kam ein Mann", "tokens": ["Sie", "h\u00e4t\u00b7te", "mehr", "ge\u00b7redt", ":", "al\u00b7lein", "es", "kam", "ein", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$.", "ADV", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ehrw\u00fcrdiger Gestallt und frommer Mienen an;", "tokens": ["Ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "Ge\u00b7stallt", "und", "from\u00b7mer", "Mie\u00b7nen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er gr\u00fc\u00dfte mich. Mein Puls begunnte sehr zu schlagen;", "tokens": ["Er", "gr\u00fc\u00df\u00b7te", "mich", ".", "Mein", "Puls", "be\u00b7gunn\u00b7te", "sehr", "zu", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sp\u00fchrte Furcht und Trost; mein Herz fieng an zu sagen:", "tokens": ["Ich", "sp\u00fchr\u00b7te", "Furcht", "und", "Trost", ";", "mein", "Herz", "fi\u00b7eng", "an", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Es sey der Mann, von dem die K\u00f6niginn erz\u00e4hlt,", "tokens": ["Es", "sey", "der", "Mann", ",", "von", "dem", "die", "K\u00f6\u00b7ni\u00b7ginn", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "50Wie sie desselben Hilff und Rath in Ehren h\u00e4lt.", "tokens": ["sie", "des\u00b7sel\u00b7ben", "Hilff", "und", "Rath", "in", "Eh\u00b7ren", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}