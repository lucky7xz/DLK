{"dta.poem.14141": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Klage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Liebchen wu\u00dft's nicht zu deuten", "tokens": ["Lieb\u00b7chen", "wu\u00dft's", "nicht", "zu", "deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und lacht mir in's Gesicht,", "tokens": ["Und", "lacht", "mir", "in's", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dreht sich zu andern Leuten", "tokens": ["Dreht", "sich", "zu", "an\u00b7dern", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und achtet's weiter nicht.", "tokens": ["Und", "ach\u00b7tet's", "wei\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}