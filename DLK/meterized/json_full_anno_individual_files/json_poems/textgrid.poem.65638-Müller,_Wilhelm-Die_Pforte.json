{"textgrid.poem.65638": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Pforte", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hohe Pforte, hohe Pforte! Zu dem Schatten deiner Gnade", "tokens": ["Ho\u00b7he", "Pfor\u00b7te", ",", "ho\u00b7he", "Pfor\u00b7te", "!", "Zu", "dem", "Schat\u00b7ten", "dei\u00b7ner", "Gna\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$.", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Rufst zur\u00fcck du die Verirrten von der Freiheit wildem Pfade.", "tokens": ["Rufst", "zu\u00b7r\u00fcck", "du", "die", "Ver\u00b7irr\u00b7ten", "von", "der", "Frei\u00b7heit", "wil\u00b7dem", "Pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Heil den Griechen! Heil den Christen! Wirf nur einen", "tokens": ["Heil", "den", "Grie\u00b7chen", "!", "Heil", "den", "Chris\u00b7ten", "!", "Wirf", "nur", "ei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "NN", "ART", "NN", "$.", "NE", "ADV", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "\u00dcber nackte Tr\u00fcmmerfelder, \u00fcber blutgetr\u00e4nkte Matten,", "tokens": ["\u00dc\u00b7ber", "nack\u00b7te", "Tr\u00fcm\u00b7mer\u00b7fel\u00b7der", ",", "\u00fc\u00b7ber", "blut\u00b7ge\u00b7tr\u00e4nk\u00b7te", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Da\u00df wir alle Platz gewinnen in dem sch\u00f6nen Zufluchtsorte,", "tokens": ["Da\u00df", "wir", "al\u00b7le", "Platz", "ge\u00b7win\u00b7nen", "in", "dem", "sch\u00f6\u00b7nen", "Zu\u00b7fluchts\u00b7or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "In dem k\u00fchlen Abendschatten deiner Gnade, hohe Pforte!", "tokens": ["In", "dem", "k\u00fch\u00b7len", "A\u00b7bend\u00b7schat\u00b7ten", "dei\u00b7ner", "Gna\u00b7de", ",", "ho\u00b7he", "Pfor\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Unsrer Br\u00fcder rothe H\u00e4upter, aufgesteckt auf deine Zinnen,", "tokens": ["Uns\u00b7rer", "Br\u00fc\u00b7der", "ro\u00b7the", "H\u00e4up\u00b7ter", ",", "auf\u00b7ge\u00b7steckt", "auf", "dei\u00b7ne", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Rufen laut mit dir vereinigt: Eilt, den Schatten zu gewinnen!", "tokens": ["Ru\u00b7fen", "laut", "mit", "dir", "ver\u00b7ei\u00b7nigt", ":", "Eilt", ",", "den", "Schat\u00b7ten", "zu", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPER", "VVPP", "$.", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Hohe Pforte, hohe Pforte! Rufe nur und schmiede Ketten,", "tokens": ["Ho\u00b7he", "Pfor\u00b7te", ",", "ho\u00b7he", "Pfor\u00b7te", "!", "Ru\u00b7fe", "nur", "und", "schmie\u00b7de", "Ket\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$.", "VVFIN", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Schicht' empor die Scheiterhaufen, deiner Gnade warme Betten,", "tokens": ["Schicht'", "em\u00b7por", "die", "Schei\u00b7ter\u00b7hau\u00b7fen", ",", "dei\u00b7ner", "Gna\u00b7de", "war\u00b7me", "Bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "F\u00fcr die Armen, Nackten, M\u00fcden, die in deinen Schatten fliehen,", "tokens": ["F\u00fcr", "die", "Ar\u00b7men", ",", "Nack\u00b7ten", ",", "M\u00fc\u00b7den", ",", "die", "in", "dei\u00b7nen", "Schat\u00b7ten", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Flehend, in dem Sklavenjoche wieder friedlich hinzuziehen!", "tokens": ["Fle\u00b7hend", ",", "in", "dem", "Skla\u00b7ven\u00b7jo\u00b7che", "wie\u00b7der", "fried\u00b7lich", "hin\u00b7zu\u00b7zie\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Rufe nur \u2013 zur Antwort schlagen unsre Waffen wir zusammen,", "tokens": ["Ru\u00b7fe", "nur", "\u2013", "zur", "Ant\u00b7wort", "schla\u00b7gen", "uns\u00b7re", "Waf\u00b7fen", "wir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Lassen unsre Kreuzfahne blitzend durch die L\u00fcfte flammen!", "tokens": ["Las\u00b7sen", "uns\u00b7re", "Kreuz\u00b7fah\u00b7ne", "blit\u00b7zend", "durch", "die", "L\u00fcf\u00b7te", "flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Gott mit uns! auf unsrer Fahne \u2013 Gott mit uns! in unsrem Herzen.", "tokens": ["Gott", "mit", "uns", "!", "auf", "uns\u00b7rer", "Fah\u00b7ne", "\u2013", "Gott", "mit", "uns", "!", "in", "uns\u00b7rem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "APPR", "PPOSAT", "NN", "$(", "NN", "APPR", "PPER", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Wir mit Gott in Siegesjubel \u2013 wir mit Gott in Todesschmerzen!", "tokens": ["Wir", "mit", "Gott", "in", "Sie\u00b7ges\u00b7ju\u00b7bel", "\u2013", "wir", "mit", "Gott", "in", "To\u00b7des\u00b7schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "NE", "$(", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Selig, die mit Gott gefallen! Zu der Pforte seiner Gnade", "tokens": ["Se\u00b7lig", ",", "die", "mit", "Gott", "ge\u00b7fal\u00b7len", "!", "Zu", "der", "Pfor\u00b7te", "sei\u00b7ner", "Gna\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "NN", "VVPP", "$.", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Ruft er heim die m\u00fcden Streiter von des Lebens wirrem Pfade:", "tokens": ["Ruft", "er", "heim", "die", "m\u00fc\u00b7den", "Strei\u00b7ter", "von", "des", "Le\u00b7bens", "wir\u00b7rem", "Pfa\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.19": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Auf dem Dornenbett der S\u00fcnder, und in Blumen der Gerechte.", "tokens": ["Auf", "dem", "Dor\u00b7nen\u00b7bett", "der", "S\u00fcn\u00b7der", ",", "und", "in", "Blu\u00b7men", "der", "Ge\u00b7rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Br\u00fcder, nach ", "tokens": ["Br\u00fc\u00b7der", ",", "nach"], "token_info": ["word", "punct", "word"], "pos": ["N