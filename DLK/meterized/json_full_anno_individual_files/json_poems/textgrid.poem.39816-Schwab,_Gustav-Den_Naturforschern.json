{"textgrid.poem.39816": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Den Naturforschern", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr fragt, warum die Sonn' erschien", "tokens": ["Ihr", "fragt", ",", "wa\u00b7rum", "die", "Sonn'", "er\u00b7schien"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einem goldnern Wagen,", "tokens": ["Auf", "ei\u00b7nem", "gold\u00b7nern", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sich den Wolkenhermelin", "tokens": ["Und", "sich", "den", "Wol\u00b7ken\u00b7her\u00b7me\u00b7lin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herbst nicht umgeschlagen?", "tokens": ["Der", "Herbst", "nicht", "um\u00b7ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht hat vergessen die Natur,", "tokens": ["Nicht", "hat", "ver\u00b7ges\u00b7sen", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ihre Freunde kommen;", "tokens": ["Da\u00df", "ih\u00b7re", "Freun\u00b7de", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat ihr Festkleid von Azur", "tokens": ["Sie", "hat", "ihr", "Fest\u00b7kleid", "von", "A\u00b7zur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "L\u00e4ngst freudig umgenommen.", "tokens": ["L\u00e4ngst", "freu\u00b7dig", "um\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Durch unsre G\u00e4rten wogt ein Licht", "tokens": ["Durch", "uns\u00b7re", "G\u00e4r\u00b7ten", "wogt", "ein", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit \u00fcberird'schen Fl\u00fcgeln,", "tokens": ["Mit", "\u00fc\u00b7ber\u00b7ird'\u00b7schen", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ein geheimes Feuer bricht", "tokens": ["Und", "ein", "ge\u00b7hei\u00b7mes", "Feu\u00b7er", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus unsern Rebenh\u00fcgeln.", "tokens": ["Aus", "un\u00b7sern", "Re\u00b7ben\u00b7h\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Traube dieses Jahres quoll", "tokens": ["Die", "Trau\u00b7be", "die\u00b7ses", "Jah\u00b7res", "quoll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Ruhm der Wissenschaften,", "tokens": ["Zum", "Ruhm", "der", "Wis\u00b7sen\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und unsrer G\u00e4ste Name soll", "tokens": ["Und", "uns\u00b7rer", "G\u00e4s\u00b7te", "Na\u00b7me", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An diesem Weine haften.", "tokens": ["An", "die\u00b7sem", "Wei\u00b7ne", "haf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn er als J\u00fcngling g\u00e4rend braust,", "tokens": ["Wenn", "er", "als", "J\u00fcng\u00b7ling", "g\u00e4\u00b7rend", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschieht's zu ihrem Preise,", "tokens": ["Ge\u00b7schieht's", "zu", "ih\u00b7rem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn als Mann im Keller haust,", "tokens": ["Und", "wenn", "als", "Mann", "im", "Kel\u00b7ler", "haust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "KOUS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn als Greis labt Greise.", "tokens": ["Und", "wenn", "als", "Greis", "labt", "Grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "KOUS", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ja, bricht des Lebens Nacht herein,", "tokens": ["Ja", ",", "bricht", "des", "Le\u00b7bens", "Nacht", "her\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird unsre H\u00fctte morscher:", "tokens": ["Wird", "uns\u00b7re", "H\u00fct\u00b7te", "mor\u00b7scher", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So schenkt uns noch ein Enkel ein", "tokens": ["So", "schenkt", "uns", "noch", "ein", "En\u00b7kel", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom starken Wein ", "tokens": ["Vom", "star\u00b7ken", "Wein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Doch in den ", "tokens": ["Doch", "in", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Gelangen ganz zu Ehren,", "tokens": ["Ge\u00b7lan\u00b7gen", "ganz", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sich in ", "tokens": ["Und", "sich", "in"], "token_info": ["word", "word", "word"], "pos": ["KON", "PRF", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Zum Zauberwein verkl\u00e4ren.", "tokens": ["Zum", "Zau\u00b7ber\u00b7wein", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dort wirkt er mit Erweckungen", "tokens": ["Dort", "wirkt", "er", "mit", "Er\u00b7we\u00b7ckun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lang als Gedankenzunder,", "tokens": ["Lang", "als", "Ge\u00b7dan\u00b7ken\u00b7zun\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dort schafft er in Entdeckungen", "tokens": ["Dort", "schafft", "er", "in", "Ent\u00b7de\u00b7ckun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gar manches neue Wunder.", "tokens": ["Gar", "man\u00b7ches", "neu\u00b7e", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch jetzo seht ihn schlummerstill", "tokens": ["Doch", "jet\u00b7zo", "seht", "ihn", "schlum\u00b7mer\u00b7still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch hinter Bl\u00e4ttern tr\u00e4umen,", "tokens": ["Noch", "hin\u00b7ter", "Bl\u00e4t\u00b7tern", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer den Knaben k\u00fcssen will,", "tokens": ["Und", "wer", "den", "Kna\u00b7ben", "k\u0