{"textgrid.poem.66853": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, mich bezwingen soll die F\u00e4ulnis nicht", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, mich bezwingen soll die F\u00e4ulnis nicht", "tokens": ["Nein", ",", "mich", "be\u00b7zwin\u00b7gen", "soll", "die", "F\u00e4ul\u00b7nis", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVINF", "VMFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der kranken Welt, die Kraft und Mut verloren ...", "tokens": ["Der", "kran\u00b7ken", "Welt", ",", "die", "Kraft", "und", "Mut", "ver\u00b7lo\u00b7ren", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ob sie rings umher die Besten bricht,", "tokens": ["Und", "ob", "sie", "rings", "um\u00b7her", "die", "Bes\u00b7ten", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKVZ", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich biete Trotz, ob auch mit ihr geboren.", "tokens": ["Ich", "bie\u00b7te", "Trotz", ",", "ob", "auch", "mit", "ihr", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auch mir, auch mir verf\u00fchrerischen Blicks", "tokens": ["Auch", "mir", ",", "auch", "mir", "ver\u00b7f\u00fch\u00b7re\u00b7ri\u00b7schen", "Blicks"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Will sich die \u00bbm\u00fcde Seele\u00ab gern verm\u00e4hlen,", "tokens": ["Will", "sich", "die", "\u00bb", "m\u00fc\u00b7de", "See\u00b7le", "\u00ab", "gern", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "$(", "ADJD", "NN", "$(", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Scharf parf\u00fcmiert, raubl\u00fcstern hinterr\u00fccks", "tokens": ["Scharf", "par\u00b7f\u00fc\u00b7miert", ",", "rau\u00b7bl\u00fcs\u00b7tern", "hin\u00b7ter\u00b7r\u00fccks"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Will sie das Mark mir aus den Knochen sch\u00e4len ...", "tokens": ["Will", "sie", "das", "Mark", "mir", "aus", "den", "Kno\u00b7chen", "sch\u00e4\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich aber ", "tokens": ["Ich", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Das heute j\u00e4h der sch\u00f6nsten Kraft bereitet,", "tokens": ["Das", "heu\u00b7te", "j\u00e4h", "der", "sch\u00f6ns\u00b7ten", "Kraft", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich stemme mich auf meinen Wanderstab", "tokens": ["Ich", "stem\u00b7me", "mich", "auf", "mei\u00b7nen", "Wan\u00b7der\u00b7stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sehe schaudernd, was hinuntergleitet.", "tokens": ["Und", "se\u00b7he", "schau\u00b7dernd", ",", "was", "hin\u00b7un\u00b7ter\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht an Erkenntnis will ich untergehn,", "tokens": ["Nicht", "an", "Er\u00b7kennt\u00b7nis", "will", "ich", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die aus geschw\u00e4chten Nerven schon geboren,", "tokens": ["Die", "aus", "ge\u00b7schw\u00e4ch\u00b7ten", "Ner\u00b7ven", "schon", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich will dem Fluch der Zeit ins Auge sehn", "tokens": ["Ich", "will", "dem", "Fluch", "der", "Zeit", "ins", "Au\u00b7ge", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und trotzen ihm, ob auch mit ihm geboren.", "tokens": ["Und", "trot\u00b7zen", "ihm", ",", "ob", "auch", "mit", "ihm", "ge\u00b7bo\u00b7ren", "."], "token_info": ["