{"textgrid.poem.47090": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben wohl indes daheim vergessen,", "tokens": ["Sie", "ha\u00b7ben", "wohl", "in\u00b7des", "da\u00b7heim", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sie gewohnt sonst waren, Blumengabe", "tokens": ["Was", "sie", "ge\u00b7wohnt", "sonst", "wa\u00b7ren", ",", "Blu\u00b7men\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VAFIN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu bringen, s\u00fc\u00dfe Blume, deinem Grabe,", "tokens": ["Zu", "brin\u00b7gen", ",", "s\u00fc\u00b7\u00dfe", "Blu\u00b7me", ",", "dei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seit ihren Lauf der Jahre vier durchmessen.", "tokens": ["Seit", "ih\u00b7ren", "Lauf", "der", "Jah\u00b7re", "vier", "durch\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "CARD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und selber, ach, verga\u00df ich hier indessen,", "tokens": ["Und", "sel\u00b7ber", ",", "ach", ",", "ver\u00b7ga\u00df", "ich", "hier", "in\u00b7des\u00b7sen", ","], "token_info": ["word", "word", 