{"dta.poem.21869": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Mich tr\u00e4umt\u2019 als Rosilis auff meine Lippen", "tokens": ["Mich", "tr\u00e4umt'", "als", "Ro\u00b7si\u00b7lis", "auff", "mei\u00b7ne", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jhr s\u00fcsser Zukkermund gab mir so man-", "tokens": ["Ihr", "s\u00fcs\u00b7ser", "Zuk\u00b7ker\u00b7mund", "gab", "mir", "so", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Seel\u2019 erhube sich ob dem beliebten Spiele.", "tokens": ["die", "Seel'", "er\u00b7hu\u00b7be", "sich", "ob", "dem", "be\u00b7lieb\u00b7ten", "Spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wacht\u2019: Indehm entwich der bunte", "tokens": ["Ich", "wacht'", ":", "In\u00b7dehm", "ent\u00b7wich", "der", "bun\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da stunde Rosilis f\u00fcr mir mit Leib und Leben.", "tokens": ["Da", "stun\u00b7de", "Ro\u00b7si\u00b7lis", "f\u00fcr", "mir", "mit", "Leib", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat Morfeus nu den Ku\u00df mir/ oder Sie ge-", "tokens": ["Hat", "Mor\u00b7feus", "nu", "den", "Ku\u00df", "mir", "/", "o\u00b7der", "Sie", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NE", "ADV", "ART", "NN", "PPER", "$(", "KON", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}