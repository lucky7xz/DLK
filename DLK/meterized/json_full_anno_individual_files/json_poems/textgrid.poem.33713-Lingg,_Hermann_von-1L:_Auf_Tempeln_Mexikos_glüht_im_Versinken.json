{"textgrid.poem.33713": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Tempeln Mexikos gl\u00fcht im Versinken", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Tempeln Mexikos gl\u00fcht im Versinken", "tokens": ["Auf", "Tem\u00b7peln", "Me\u00b7xi\u00b7kos", "gl\u00fcht", "im", "Ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Sonne noch, was zaudert sie so lange?", "tokens": ["Die", "Son\u00b7ne", "noch", ",", "was", "zau\u00b7dert", "sie", "so", "lan\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie lauscht der Priester blutigem Gesange,", "tokens": ["Sie", "lauscht", "der", "Pries\u00b7ter", "blu\u00b7ti\u00b7gem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Opferfest beim Schall der hellen Zinken.", "tokens": ["Zum", "Op\u00b7fer\u00b7fest", "beim", "Schall", "der", "hel\u00b7len", "Zin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf die Gefangnen scheint sie. Federn winken", "tokens": ["Auf", "die", "Ge\u00b7fang\u00b7nen", "scheint", "sie", ".", "Fe\u00b7dern", "win\u00b7ken"], "token_in