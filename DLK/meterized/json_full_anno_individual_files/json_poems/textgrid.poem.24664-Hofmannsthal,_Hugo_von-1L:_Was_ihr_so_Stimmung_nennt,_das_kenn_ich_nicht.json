"word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit List, in Arbeit, Qualen, Leidenschaft.", "tokens": ["Mit", "List", ",", "in", "Ar\u00b7beit", ",", "Qua\u00b7len", ",", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und wie von einem Geisterblitz erhellt,", "tokens": ["Und", "wie", "von", "ei\u00b7nem", "Geis\u00b7ter\u00b7blitz", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich ein reich Gedr\u00e4nge, eine Welt.", "tokens": ["Sah", "ich", "ein", "reich", "Ge\u00b7dr\u00e4n\u00b7ge", ",", "ei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Kristallklar lag der Menschen Sein vor mir,", "tokens": ["Kris\u00b7tall\u00b7klar", "lag", "der", "Men\u00b7schen", "Sein", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "APPR", "PPER", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Ich sah das Zauberreich, des Pforte f\u00e4llt", "tokens": ["Ich", "sah", "das", "Zau\u00b7ber\u00b7reich", ",", "des", "Pfor\u00b7te", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFI