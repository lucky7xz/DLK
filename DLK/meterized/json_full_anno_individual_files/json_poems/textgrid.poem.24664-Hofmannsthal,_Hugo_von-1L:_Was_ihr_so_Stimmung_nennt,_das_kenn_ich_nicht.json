{"textgrid.poem.24664": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ihr so Stimmung nennt, das kenn ich nicht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ihr so Stimmung nennt, das kenn ich nicht", "tokens": ["Was", "ihr", "so", "Stim\u00b7mung", "nennt", ",", "das", "kenn", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schweige still, wenn einer davon spricht.", "tokens": ["Und", "schwei\u00b7ge", "still", ",", "wenn", "ei\u00b7ner", "da\u00b7von", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann sein, da\u00df es ein Fr\u00fchlingswogen gibt,", "tokens": ["Kann", "sein", ",", "da\u00df", "es", "ein", "Fr\u00fch\u00b7lings\u00b7wo\u00b7gen", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo Vers an Vers und Bild an Bild sich flicht,", "tokens": ["Wo", "Vers", "an", "Vers", "und", "Bild", "an", "Bild", "sich", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenns tief im Herzen gl\u00fcht und sch\u00e4umt und liebt ...", "tokens": ["Wenns", "tief", "im", "Her\u00b7zen", "gl\u00fcht", "und", "sch\u00e4umt", "und", "liebt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mir ward es nie so gut. Wie Schaum zerstiebt", "tokens": ["Mir", "ward", "es", "nie", "so", "gut", ".", "Wie", "Schaum", "zer\u00b7stiebt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "PWAV", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Sonnenlicht mir jede Traumgestalt,", "tokens": ["Im", "Son\u00b7nen\u00b7licht", "mir", "je\u00b7de", "Traum\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein dumpfes Beben bleibt von der Gewalt", "tokens": ["Ein", "dum\u00b7pfes", "Be\u00b7ben", "bleibt", "von", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Melodie, die ich im Traum geh\u00f6rt;", "tokens": ["Der", "Me\u00b7lo\u00b7die", ",", "die", "ich", "im", "Traum", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie selber ist verloren und verhallt,", "tokens": ["Sie", "sel\u00b7ber", "ist", "ver\u00b7lo\u00b7ren", "und", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Duft verweht, der Farbenschmelz zerst\u00f6rt,", "tokens": ["Der", "Duft", "ver\u00b7weht", ",", "der", "Far\u00b7ben\u00b7schmelz", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ich vom Suchen matt, entt\u00e4uscht, verst\u00f6rt.", "tokens": ["Und", "ich", "vom", "Su\u00b7chen", "matt", ",", "ent\u00b7t\u00e4uscht", ",", "ver\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "PTKVZ", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch manchmal, ohne Wunsch, Gedanke, Ziel,", "tokens": ["Doch", "manch\u00b7mal", ",", "oh\u00b7ne", "Wunsch", ",", "Ge\u00b7dan\u00b7ke", ",", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Alltagstreiben, mitten im Gew\u00fchl", "tokens": ["Im", "All\u00b7tags\u00b7trei\u00b7ben", ",", "mit\u00b7ten", "im", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Gro\u00dfstadt, aus dem tausendstimmgen Chor,", "tokens": ["Der", "Gro\u00df\u00b7stadt", ",", "aus", "dem", "tau\u00b7sends\u00b7timm\u00b7gen", "Chor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem wirren Chaos, schl\u00e4gt es an mein Ohr", "tokens": ["Dem", "wir\u00b7ren", "Chaos", ",", "schl\u00e4gt", "es", "an", "mein", "Ohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie M\u00e4rchenklang, waldduftig, n\u00e4chtigk\u00fchl,", "tokens": ["Wie", "M\u00e4r\u00b7chen\u00b7klang", ",", "wald\u00b7duf\u00b7tig", ",", "n\u00e4ch\u00b7tig\u00b7k\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJD", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Bilder seh ich, nie geahnt zuvor.", "tokens": ["Und", "Bil\u00b7der", "seh", "ich", ",", "nie", "ge\u00b7ahnt", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "ADV", "VVPP", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Nichts, der Klang, der Duft, er wird zum Keim,", "tokens": ["Das", "Nichts", ",", "der", "Klang", ",", "der", "Duft", ",", "er", "wird", "zum", "Keim", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Lied, geziert mit flimmernd buntem Reim,", "tokens": ["Zum", "Lied", ",", "ge\u00b7ziert", "mit", "flim\u00b7mernd", "bun\u00b7tem", "Reim", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVPP", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ein paar Tage im Ged\u00e4chtnis gl\u00fcht ...", "tokens": ["Das", "ein", "paar", "Ta\u00b7ge", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "gl\u00fcht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem Strau\u00df am Fenstersims verbl\u00fcht", "tokens": ["Mit", "ei\u00b7nem", "Strau\u00df", "am", "Fens\u00b7ter\u00b7sims", "ver\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In meines M\u00e4dchens duftig engem Heim ...", "tokens": ["In", "mei\u00b7nes", "M\u00e4d\u00b7chens", "duf\u00b7tig", "en\u00b7gem", "Heim", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Beim Wein in einem Trinkspruch fl\u00fcchtig spr\u00fcht ...", "tokens": ["Beim", "Wein", "in", "ei\u00b7nem", "Trinks\u00b7pruch", "fl\u00fcch\u00b7tig", "spr\u00fcht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So fa\u00df ich der Begeistrung scheues Pfand", "tokens": ["So", "fa\u00df", "ich", "der", "Be\u00b7geis\u00b7trung", "scheu\u00b7es", "Pfand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und halt es fest, zuweilen bunten Tand,", "tokens": ["Und", "halt", "es", "fest", ",", "zu\u00b7wei\u00b7len", "bun\u00b7ten", "Tand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein wertlos Spielzeug, manchmal \u2013 selten \u2013 mehr,", "tokens": ["Ein", "wert\u00b7los", "Spiel\u00b7zeug", ",", "manch\u00b7mal", "\u2013", "sel\u00b7ten", "\u2013", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADV", "$(", "ADJD", "$(", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schreibs, wo immer, an der Zeitung Rand,", "tokens": ["Und", "schreibs", ",", "wo", "im\u00b7mer", ",", "an", "der", "Zei\u00b7tung", "Rand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf eine leere Seite im Homer,", "tokens": ["Auf", "ei\u00b7ne", "lee\u00b7re", "Sei\u00b7te", "im", "Ho\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In einen Brief \u2013 (es wiegt ja selten schwer) ...", "tokens": ["In", "ei\u00b7nen", "Brief", "\u2013", "(", "es", "wiegt", "ja", "sel\u00b7ten", "schwer", ")", "..."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$(", "$(", "PPER", "VVFIN", "ADV", "ADJD", "ADJD", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich schrieb auch schon auf eine Gartenbank,", "tokens": ["Ich", "schrieb", "auch", "schon", "auf", "ei\u00b7ne", "Gar\u00b7ten\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf einen Stein am Quell, daraus sie trank,", "tokens": ["Auf", "ei\u00b7nen", "Stein", "am", "Quell", ",", "da\u00b7raus", "sie", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf bunte Schleifen buntre Verse schier,", "tokens": ["Auf", "bun\u00b7te", "Schlei\u00b7fen", "bunt\u00b7re", "Ver\u00b7se", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf einer Birke Stamm, wei\u00dfschimmernd, blank,", "tokens": ["Auf", "ei\u00b7ner", "Bir\u00b7ke", "Stamm", ",", "wei\u00df\u00b7schim\u00b7mernd", ",", "blank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVPP", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und j\u00fcngst auf ein zerknittert St\u00fcck Papier", "tokens": ["Und", "j\u00fcngst", "auf", "ein", "zer\u00b7knit\u00b7tert", "St\u00fcck", "Pa\u00b7pier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "VVPP", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit trockner Inschrift, krauser Schn\u00f6rkelzier:", "tokens": ["Mit", "trock\u00b7ner", "In\u00b