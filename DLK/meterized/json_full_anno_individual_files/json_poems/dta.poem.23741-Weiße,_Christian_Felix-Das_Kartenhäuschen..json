{"dta.poem.23741": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das Kartenh\u00e4uschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Lacht nur, guten Leute, lacht,", "tokens": ["Lacht", "nur", ",", "gu\u00b7ten", "Leu\u00b7te", ",", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mein Haus, das ich gemacht,", "tokens": ["Da\u00df", "mein", "Haus", ",", "das", "ich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine leichte Luft zerst\u00f6rt.", "tokens": ["Ei\u00b7ne", "leich\u00b7te", "Luft", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die\u00df lachens werth?", "tokens": ["Ist", "die\u00df", "la\u00b7chens", "werth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "O! ihr baut auch oft in Wind!", "tokens": ["O", "!", "ihr", "baut", "auch", "oft", "in", "Wind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt, was eure Schl\u00f6sser sind,", "tokens": ["Sagt", ",", "was", "eu\u00b7re", "Schl\u00f6s\u00b7ser", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr euch so hoch erbaut,", "tokens": ["Die", "ihr", "euch", "so", "hoch", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Stolz beschaut?", "tokens": ["Und", "mit", "Stolz", "be\u00b7schaut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Werden sie noch morgen stehn?", "tokens": ["Wer\u00b7den", "sie", "noch", "mor\u00b7gen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ja \u2014 vielleicht \u2014 wir wollen sehn.", "tokens": ["Ja", "viel\u00b7leicht", "wir", "wol\u00b7len", "sehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ADV", "$(", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "St\u00f6rt nicht oft ein Augenblick", "tokens": ["St\u00f6rt", "nicht", "oft", "ein", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser ganzes Gl\u00fcck?", "tokens": ["Un\u00b7ser", "gan\u00b7zes", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}