{"textgrid.poem.39616": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Philosophische Lection", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Herrn, damit Sie's wi\u00dfen", "tokens": ["Mei\u00b7ne", "Herrn", ",", "da\u00b7mit", "Sie's", "wi\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche Wi\u00dfenschaft ich lehre:", "tokens": ["Wel\u00b7che", "Wi\u00b7\u00dfen\u00b7schaft", "ich", "leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es ist die Wi\u00dfenschaftslehre,", "tokens": ["Es", "ist", "die", "Wi\u00b7\u00dfen\u00b7schafts\u00b7leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das hei\u00dft Wi\u00dfen von dem Wi\u00dfen.", "tokens": ["Das", "hei\u00dft", "Wi\u00b7\u00dfen", "von", "dem", "Wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die\u00df mu\u00df ich nun deutlich machen.", "tokens": ["Die\u00df", "mu\u00df", "ich", "nun", "deut\u00b7lich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnen Sie es nicht verdauen,", "tokens": ["K\u00f6n\u00b7nen", "Sie", "es", "nicht", "ver\u00b7dau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich's Ihnen erstlich kauen,", "tokens": ["Will", "ich's", "Ih\u00b7nen", "erst\u00b7lich", "kau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dann schieben in den Rachen.", "tokens": ["Und", "dann", "schie\u00b7ben", "in", "den", "Ra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwar Ihr Kopf wird etwas krachen,", "tokens": ["Zwar", "Ihr", "Kopf", "wird", "et\u00b7was", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil mit unn\u00fctz schlechtem Wi\u00dfen", "tokens": ["Weil", "mit", "un\u00b7n\u00fctz", "schlech\u00b7tem", "Wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie ihn ohne Noth verschli\u00dfen;", "tokens": ["Sie", "ihn", "oh\u00b7ne", "Noth", "ver\u00b7schli\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Darum will ich's unverholen", "tokens": ["Da\u00b7rum", "will", "ich's", "un\u00b7ver\u00b7ho\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihnen sagen, wiederholen,", "tokens": ["Ih\u00b7nen", "sa\u00b7gen", ",", "wie\u00b7der\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meine Herrn, damit Sie's wi\u00dfen.", "tokens": ["Mei\u00b7ne", "Herrn", ",", "da\u00b7mit", "Sie's", "wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kreide nehm' ich in die Rechte.", "tokens": ["Krei\u00b7de", "nehm'", "ich", "in", "die", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will man Wahrheit recht erwerben,", "tokens": ["Will", "man", "Wahr\u00b7heit", "recht", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df man auf ein Brett sie kerben,", "tokens": ["Mu\u00df", "man", "auf", "ein", "Brett", "sie", "ker\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie im Wirthshaus, was man zechte.", "tokens": ["Wie", "im", "Wirths\u00b7haus", ",", "was", "man", "zech\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich tapfer sie verfechte,", "tokens": ["Da\u00df", "ich", "tap\u00b7fer", "sie", "ver\u00b7fech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Komme nach der Kinderlehre", "tokens": ["Kom\u00b7me", "nach", "der", "Kin\u00b7der\u00b7leh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeder, den ich nicht bekehre,", "tokens": ["Je\u00b7der", ",", "den", "ich", "nicht", "be\u00b7keh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zweifle, frage, fordre Proben,", "tokens": ["Zweif\u00b7le", ",", "fra\u00b7ge", ",", "ford\u00b7re", "Pro\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und er wird dereinst noch loben,", "tokens": ["Und", "er", "wird", "de\u00b7reinst", "noch", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Welche Wi\u00dfenschaft ich lehre.", "tokens": ["Wel\u00b7che", "Wi\u00b7\u00dfen\u00b7schaft", "ich", "leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wi\u00dfen Sie nicht irgend was?", "tokens": ["Wi\u00b7\u00dfen", "Sie", "nicht", "ir\u00b7gend", "was", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PWS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweimal zwei etwa macht viere.", "tokens": ["Zwei\u00b7mal", "zwei", "et\u00b7wa", "macht", "vie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich Sie nicht prostituiere,", "tokens": ["Da\u00df", "ich", "Sie", "nicht", "pros\u00b7ti\u00b7tu\u00b7ie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nehm' ich an, Sie w\u00fc\u00dften das.", "tokens": ["Nehm'", "ich", "an", ",", "Sie", "w\u00fc\u00df\u00b7ten", "das", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Wi\u00dfen ist nur cra\u00df.", "tokens": ["Die\u00b7ses", "Wi\u00b7\u00dfen", "ist", "nur", "cra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis ich's h\u00f6her erst verkl\u00e4re", "tokens": ["Bis", "ich's", "h\u00f6\u00b7her", "erst", "ver\u00b7kl\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADJD", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Besinnung drein bescheere;", "tokens": ["Und", "Be\u00b7sin\u00b7nung", "drein", "be\u00b7schee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dann wird draus die Wi\u00dfenschaft,", "tokens": ["Dann", "wird", "draus", "die", "Wi\u00b7\u00dfen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die des Wi\u00dfens Wi\u00dfen schafft:", "tokens": ["Die", "des", "Wi\u00b7\u00dfens", "Wi\u00b7\u00dfen", "schafft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Es ist die Wi\u00dfenschaftslehre.", "tokens": ["Es", "ist", "die", "Wi\u00b7\u00dfen\u00b7schafts\u00b7leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Diesen derben Stab der Fichte", "tokens": ["Die\u00b7sen", "der\u00b7ben", "Stab", "der", "Fich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Hand, da kann man handeln,", "tokens": ["In", "der", "Hand", ",", "da", "kann", "man", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fest auf Erden stehn und wandeln,", "tokens": ["Fest", "auf", "Er\u00b7den", "stehn", "und", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wird nimmermehr zu nichte.", "tokens": ["Und", "wird", "nim\u00b7mer\u00b7mehr", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKA", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis zum letzten Weltgerichte,", "tokens": ["Bis", "zum", "letz\u00b7ten", "Welt\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn auch alle Str\u00e4nge ri\u00dfen,", "tokens": ["Wenn", "auch", "al\u00b7le", "Str\u00e4n\u00b7ge", "ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bleibt Ihr ruhig im Gewi\u00dfen,", "tokens": ["Bleibt", "Ihr", "ru\u00b7hig", "im", "Ge\u00b7wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn Ihr wi\u00dft gewi\u00df, Ihr wi\u00dft,", "tokens": ["Denn", "Ihr", "wi\u00dft", "ge\u00b7wi\u00df", ",", "Ihr", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und auch was die\u00df Wi\u00dfen ist:", "tokens": ["Und", "auch", "was", "die\u00df", "Wi\u00b7\u00dfen", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "PDS", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Das hei\u00dft Wi\u00dfen von dem Wi\u00dfen.", "tokens": ["Das", "hei\u00dft", "Wi\u00b7\u00dfen", "von", "dem", "Wi\u00b7\u00dfen"