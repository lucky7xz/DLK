{"textgrid.poem.48709": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10. \u00dcber Herrn Martin Opitzen auf Boberfeld sein Ableben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So zeuch auch du denn hin in dein Elyserfeld,", "tokens": ["So", "zeuch", "auch", "du", "denn", "hin", "in", "dein", "E\u00b7ly\u00b7ser\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du Pindar, du Homer, du Maro unsrer Zeiten,", "tokens": ["du", "Pin\u00b7dar", ",", "du", "Ho\u00b7mer", ",", "du", "Ma\u00b7ro", "uns\u00b7rer", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NE", "NE", "$,", "PPER", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "und untermenge dich mit diesen gro\u00dfen Leuten,", "tokens": ["und", "un\u00b7ter\u00b7men\u00b7ge", "dich", "mit", "die\u00b7sen", "gro\u00b7\u00dfen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die ganz in deinen Geist sich hatten hier verstellt.", "tokens": ["die", "ganz", "in", "dei\u00b7nen", "Geist", "sich", "hat\u00b7ten", "hier", "ver\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "PRF", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zeuch jenen Helden zu, du jenen gleicher Held,", "tokens": ["Zeuch", "je\u00b7nen", "Hel\u00b7den", "zu", ",", "du", "je\u00b7nen", "glei\u00b7cher", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PTKVZ", "$,", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der itzt nichts Gleiches hat, du Herzog deutscher Saiten;", "tokens": ["der", "itzt", "nichts", "Glei\u00b7ches", "hat", ",", "du", "Her\u00b7zog", "deut\u00b7scher", "Sai\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "NN", "VAFIN", "$,", "PPER", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o Erbe durch dich selbst der steten Ewigkeiten,", "tokens": ["o", "Er\u00b7be", "durch", "dich", "selbst", "der", "ste\u00b7ten", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "o ewiglicher Schatz und auch Verlust der Welt.", "tokens": ["o", "e\u00b7wig\u00b7li\u00b7cher", "Schatz", "und", "auch", "Ver\u00b7lust", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "KON", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Germanie ist tot, die herrliche, die freie,", "tokens": ["Ger\u00b7ma\u00b7nie", "ist", "tot", ",", "die", "herr\u00b7li\u00b7che", ",", "die", "frei\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "ein Grab verdecket sie und ihre ganze Treue,", "tokens": ["ein", "Grab", "ver\u00b7de\u00b7cket", "sie", "und", "ih\u00b7re", "gan\u00b7ze", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Mutter, die ist hin. Hier liegt nun auch ihr Sohn,", "tokens": ["die", "Mut\u00b7ter", ",", "die", "ist", "hin", ".", "Hier", "liegt", "nun", "auch", "ihr", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "$.", "ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "ihr R\u00e4cher und sein Arm. La\u00dft, la\u00dft nur Alles bleiben", "tokens": ["ihr", "R\u00e4\u00b7cher", "und", "sein", "Arm", ".", "La\u00dft", ",", "la\u00dft", "nur", "Al\u00b7les", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$.", "VVIMP", "$,", "VVIMP", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr, die ihr \u00fcbrig seid, und macht euch nur darvon.", "tokens": ["ihr", ",", "die", "ihr", "\u00fcb\u00b7rig", "seid", ",", "und", "macht", "euch", "nur", "dar\u00b7von", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,