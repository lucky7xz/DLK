{"textgrid.poem.46245": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ihr herz ist gefroren", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie ein armer mensch, aus irdischem verstand,", "tokens": ["Gleich\u00b7wie", "ein", "ar\u00b7mer", "mensch", ",", "aus", "ir\u00b7di\u00b7schem", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "vermeinet, horchend zu des aberglaubens lehren,", "tokens": ["ver\u00b7mei\u00b7net", ",", "hor\u00b7chend", "zu", "des", "a\u00b7berg\u00b7lau\u00b7bens", "leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein sch\u00f6n gemaltes bild, als seines geists heiland,", "tokens": ["ein", "sch\u00f6n", "ge\u00b7mal\u00b7tes", "bild", ",", "als", "sei\u00b7nes", "geists", "hei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "mit bitten, opfern, lob und anderm dienst zu ehren:", "tokens": ["mit", "bit\u00b7ten", ",", "op\u00b7fern", ",", "lob", "und", "an\u00b7derm", "dienst", "zu", "eh\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "VVINF", "$,", "NN", "KON", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Also und mehr fehl ich, witzlos, durch mein begehren,", "tokens": ["Al\u00b7so", "und", "mehr", "fehl", "ich", ",", "witz\u00b7los", ",", "durch", "mein", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PIAT", "NN", "PPER", "$,", "ADJD", "$,", "APPR", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wan ich f\u00fcr euch erheb mein herz, gesicht und hand,", "tokens": ["wan", "ich", "f\u00fcr", "euch", "er\u00b7heb", "mein", "herz", ",", "ge\u00b7sicht", "und", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "wan ich mich darf ab euch beklagen und beschweren,", "tokens": ["wan", "ich", "mich", "darf", "ab", "euch", "be\u00b7kla\u00b7gen", "und", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "da schuldig doch allein mein eigner unverstand.", "tokens": ["da", "schul\u00b7dig", "doch", "al\u00b7lein", "mein", "eig\u00b7ner", "un\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja, g\u00f6ttin, deren gnad mich kont allein erlaben,", "tokens": ["Ja", ",", "g\u00f6t\u00b7tin", ",", "de\u00b7ren", "gnad", "mich", "kont", "al\u00b7lein", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PRELAT", "NN", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "euch klag ich an umsunst, umsunst hoff ich den lust,", "tokens": ["euch", "klag", "ich", "an", "um\u00b7sunst", ",", "um\u00b7sunst", "hoff", "ich", "den", "lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "da\u00df euer herz mit lieb werd meine lieb begaben;", "tokens": ["da\u00df", "eu\u00b7er", "herz", "mit", "lieb", "werd", "mei\u00b7ne", "lieb", "be\u00b7ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJD", "VAFIN", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dan solt ich, als ich sah eurer schneewei\u00dfen brust", "tokens": ["Dan", "solt", "ich", ",", "als", "ich", "sah", "eu\u00b7rer", "schnee\u00b7wei\u00b7\u00dfen", "brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+---+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "bezauberende b\u00fchl, nicht (kl\u00fcger) gedacht haben,", "tokens": ["be\u00b7zau\u00b7be\u00b7ren\u00b7de", "b\u00fchl", ",", "nicht", "(", "kl\u00fc\u00b7ger", ")", "ge\u00b7dacht", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "w