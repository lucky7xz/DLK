{"textgrid.poem.56461": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Hunger", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er fuhr in einen Hund, dem gro\u00df er sperrt", "tokens": ["Er", "fuhr", "in", "ei\u00b7nen", "Hund", ",", "dem", "gro\u00df", "er", "sperrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das rote Maul. Die blaue Zunge wirft", "tokens": ["Das", "ro\u00b7te", "Maul", ".", "Die", "blau\u00b7e", "Zun\u00b7ge", "wirft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich lang heraus. Er w\u00e4lzt im Staub. Er schl\u00fcrft", "tokens": ["Sich", "lang", "he\u00b7raus", ".", "Er", "w\u00e4lzt", "im", "Staub", ".", "Er", "schl\u00fcrft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verwelktes Gras, das er dem Sand entzerrt.", "tokens": ["Ver\u00b7welk\u00b7tes", "Gras", ",", "das", "er", "dem", "Sand", "ent\u00b7zerrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sein leerer Schlund ist wie ein gro\u00dfes Tor,", "tokens": ["Sein", "lee\u00b7rer", "Schlund", "ist", "wie", "ein", "gro\u00b7\u00dfes", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drin Feuer sickert, langsam, tropfenweis,", "tokens": ["Drin", "Feu\u00b7er", "si\u00b7ckert", ",", "lang\u00b7sam", ",", "trop\u00b7fen\u00b7weis", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ihm den Bauch verbrennt. Dann w\u00e4scht mit Eis", "tokens": ["Das", "ihm", "den", "Bauch", "ver\u00b7brennt", ".", "Dann", "w\u00e4scht", "mit", "Eis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihm eine Hand das hei\u00dfe Speiserohr.", "tokens": ["Ihm", "ei\u00b7ne", "Hand", "das", "hei\u00b7\u00dfe", "Spei\u00b7se\u00b7rohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er wankt durch Dampf Die Sonne ist ein Fleck,", "tokens": ["Er", "wankt", "durch", "Dampf", "Die", "Son\u00b7ne", "ist", "ein", "Fleck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein rotes Ofentor. Ein gr\u00fcner Halbmond f\u00fchrt", "tokens": ["Ein", "ro\u00b7tes", "O\u00b7fen\u00b7tor", ".", "Ein", "gr\u00fc\u00b7ner", "Halb\u00b7mond", "f\u00fchrt"], "token_