{"textgrid.poem.37216": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Schuhknecht", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.85", "it:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor allen Dirnen so flink und so glatt", "tokens": ["Vor", "al\u00b7len", "Dir\u00b7nen", "so", "flink", "und", "so", "glatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lacht mir die lachende Lore.", "tokens": ["Lacht", "mir", "die", "la\u00b7chen\u00b7de", "Lo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Vor allen prunkenden Pl\u00e4tzen der Stadt", "tokens": ["Vor", "al\u00b7len", "prun\u00b7ken\u00b7den", "Pl\u00e4t\u00b7zen", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Prunkt mir der Winkel am Thore.", "tokens": ["Prunkt", "mir", "der", "Win\u00b7kel", "am", "Tho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Des Hofes Dame, wie schmuck sie sich macht,", "tokens": ["Des", "Ho\u00b7fes", "Da\u00b7me", ",", "wie", "schmuck", "sie", "sich", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit nichten gleicht sie der Lore.", "tokens": ["Mit", "nich\u00b7ten", "gleicht", "sie", "der", "Lo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bei Tag ist Sie mein Gedank und bei Nacht", "tokens": ["Bei", "Tag", "ist", "Sie", "mein", "Ge\u00b7dank", "und", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wohnt im Winkel am Thore.", "tokens": ["Und", "wohnt", "im", "Win\u00b7kel", "am", "Tho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr Vater hockt in dem St\u00fcbchen und flicht", "tokens": ["Ihr", "Va\u00b7ter", "hockt", "in", "dem", "St\u00fcb\u00b7chen", "und", "flicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus Eggen warme Pantoffeln.", "tokens": ["Aus", "Eg\u00b7gen", "war\u00b7me", "Pan\u00b7tof\u00b7feln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mutter, gibt es Kastanien nicht,", "tokens": ["Die", "Mut\u00b7ter", ",", "gibt", "es", "Kas\u00b7ta\u00b7ni\u00b7en", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verkauft am Markte Kartoffeln.", "tokens": ["Ver\u00b7kauft", "am", "Mark\u00b7te", "Kar\u00b7tof\u00b7feln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So brav erzogen, so eben und sacht,", "tokens": ["So", "brav", "er\u00b7zo\u00b7gen", ",", "so", "e\u00b7ben", "und", "sacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ward nie ein M\u00e4dchen als Lore.", "tokens": ["Ward", "nie", "ein", "M\u00e4d\u00b7chen", "als", "Lo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "KOUS", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bei Tag ist Sie mein Gedank und bei Nacht", "tokens": ["Bei", "Tag", "ist", "Sie", "mein", "Ge\u00b7dank", "und", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wohnt im Winkel am Thore.", "tokens": ["Und", "wohnt", "im", "Win\u00b7kel", "am", "Tho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "K\u00f6mmt sie getrippelt das G\u00e4\u00dfchen herab,", "tokens": ["K\u00f6mmt", "sie", "ge\u00b7trip\u00b7pelt", "das", "G\u00e4\u00df\u00b7chen", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ART", "NN", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Dann wird mirs blind vor den Augen;", "tokens": ["Dann", "wird", "mirs", "blind", "vor", "den", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch schallt im Haus ihr behendes klipp klapp,", "tokens": ["Doch", "schallt", "im", "Haus", "ihr", "be\u00b7hen\u00b7des", "klipp", "klapp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "ITJ", "ITJ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht Stich noch Naht will mir taugen.", "tokens": ["Nicht", "Stich", "noch", "Naht", "will", "mir", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Meister schmunzelt \u2013 doch hab er Verdacht,", "tokens": ["Der", "Meis\u00b7ter", "schmun\u00b7zelt", "\u2013", "doch", "hab", "er", "Ver\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich sei erpicht auf die Lore!", "tokens": ["Ich", "sei", "er\u00b7picht", "auf", "die", "Lo\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bei Tag ist Sie mein Gedank und bei Nacht", "tokens": ["Bei", "Tag", "ist", "Sie", "mein", "Ge\u00b7dank", "und", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wohnt im Winkel am Thore.", "tokens": ["Und", "wohnt", "im", "Win\u00b7kel", "am", "Tho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Vor allen Tagen der Woche behagt", "tokens": ["Vor", "al\u00b7len", "Ta\u00b7gen", "der", "Wo\u00b7che", "be\u00b7hagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Tag behaglicher Ruhe.", "tokens": ["Der", "Tag", "be\u00b7hag\u00b7li\u00b7cher", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da wird ein Sprung in das freie gewagt,", "tokens": ["Da", "wird", "ein", "Sprung", "in", "das", "frei\u00b7e", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da rasten Stiefel und Schuhe.", "tokens": ["Da", "ras\u00b7ten", "Stie\u00b7fel", "und", "Schu\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mit Bursch und M\u00e4dchen in stattlicher Pracht", "tokens": ["Mit", "Bursch", "und", "M\u00e4d\u00b7chen", "in", "statt\u00b7li\u00b7cher", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gehts flink zu Dorf mit der Lore.", "tokens": ["Gehts", "flink", "zu", "Dorf", "mit", "der", "Lo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bei Tag ist Sie mein Gedank und bei Nacht", "tokens": ["Bei", "Tag", "ist", "Sie", "mein", "Ge\u00b7dank", "und", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wohnt im Winkel am Thore.", "tokens": ["Und", "wohnt", "im", "Win\u00b7kel", "am", "Tho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Auch schleppt der ehrbare Meister mich wohl", "tokens": ["Auch", "schleppt", "der", "ehr\u00b7ba\u00b7re", "Meis\u00b7ter", "mich", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Festtag mit in die Predigt,", "tokens": ["Am", "Fest\u00b7tag", "mit", "in", "die", "Pre\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und fegt mich wacker beim dampfenden Kohl,", "tokens": ["Und", "fegt", "mich", "wa\u00b7cker", "beim", "damp\u00b7fen\u00b7den", "Kohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hab ich des Zwangs mich entledigt.", "tokens": ["Hab", "ich", "des", "Zwangs", "mich", "ent\u00b7le\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Doch halt er immer die geistliche Wacht,", "tokens": ["Doch", "halt", "er", "im\u00b7mer", "die", "geist\u00b7li\u00b7che", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich Weltkind schleiche zur Lore!", "tokens": ["Ich", "Welt\u00b7kind", "schlei\u00b7che", "zur", "Lo\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bei Tag ist Sie mein Gedank und bei Nacht", "tokens": ["Bei", "Tag", "ist", "Sie", "mein", "Ge\u00b7dank", "und", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wohnt im Winkel am Thore.", "tokens": ["Und", "wohnt", "im", "Win\u00b7kel", "am", "Tho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Tritt Weihnacht wieder einmal ins Land,", "tokens": ["Tritt", "Weih\u00b7nacht", "wie\u00b7der", "ein\u00b7mal", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann strozt von Geld mir die Ficke,", "tokens": ["Dann", "strozt", "von", "Geld", "mir", "die", "Fi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das mir zum Rocke die Mutter gesandt,", "tokens": ["Das", "mir", "zum", "Ro\u00b7cke", "die", "Mut\u00b7ter", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Ihr ins H\u00e4ndchen ichs dr\u00fccke.", "tokens": ["Und", "Ihr", "ins", "H\u00e4nd\u00b7chen", "ichs", "dr\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ja h\u00f6b ich Sch\u00e4tze vom Satan bewacht,", "tokens": ["Ja", "h\u00f6b", "ich", "Sch\u00e4t\u00b7ze", "vom", "Sa\u00b7tan", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Sch\u00e4tze fl\u00f6gen zur Lore!", "tokens": ["Die", "Sch\u00e4t\u00b7ze", "fl\u00f6\u00b7gen", "zur", "Lo\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bei Tag ist Sie mein Gedank und bei Nacht", "tokens": ["Bei", "Tag", "ist", "Sie", "mein", "Ge\u00b7dank", "und", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und wohnt im Winkel am Thore.", "tokens": ["Und", "wohnt", "im", "Win\u00b7kel", "am", "Tho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Mein St\u00fcndlein k\u00f6mmt da\u00df ich fort in die Welt", "tokens": ["Mein", "St\u00fcnd\u00b7lein", "k\u00f6mmt", "da\u00df", "ich", "fort", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KOUS", "PPER", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach Handwerksordnungen wandre,", "tokens": ["Nach", "Hand\u00b7werk\u00b7sord\u00b7nun\u00b7gen", "wand\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und drauf als redlicher Mann f\u00fcr mein Geld", "tokens": ["Und", "drauf", "als", "red\u00b7li\u00b7cher", "Mann", "f\u00fcr", "mein", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "KOUS", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Herr Meister we