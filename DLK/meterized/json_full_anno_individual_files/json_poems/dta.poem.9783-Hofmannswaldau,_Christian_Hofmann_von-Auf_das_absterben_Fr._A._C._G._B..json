{"dta.poem.9783": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben Fr. A. C. G. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbte/ zieht das saltz der schweren thr\u00e4nen ein/", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "/", "zieht", "das", "saltz", "der", "schwe\u00b7ren", "thr\u00e4\u00b7nen", "ein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schlagt zugleich die last der sorgen in die erden:", "tokens": ["Und", "schlagt", "zu\u00b7gleich", "die", "last", "der", "sor\u00b7gen", "in", "die", "er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn sagt/ wo k\u00f6nte wol der mutter besser seyn/", "tokens": ["Denn", "sagt", "/", "wo", "k\u00f6n\u00b7te", "wol", "der", "mut\u00b7ter", "bes\u00b7ser", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWAV", "VMFIN", "ADV", "ART", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als itzt da mund und hertz zu lauter sternen werden.", "tokens": ["Als", "itzt", "da", "mund", "und", "hertz", "zu", "lau\u00b7ter", "ster\u00b7nen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "KON", "NN", "APPR", "PIAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr wi\u00dft wol da\u00df ein mensch den jahres-zeiten gleicht/", "tokens": ["Ihr", "wi\u00dft", "wol", "da\u00df", "ein", "mensch", "den", "jah\u00b7res\u00b7zei\u00b7ten", "gleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die erstlich nichts als lentz und s\u00fcsse sommer zeigen/", "tokens": ["Die", "erst\u00b7lich", "nichts", "als", "lentz", "und", "s\u00fcs\u00b7se", "som\u00b7mer", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "KOKOM", "ADV", "KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bi\u00df da\u00df der k\u00fchle herbst dem kalten winter weicht/", "tokens": ["Bi\u00df", "da\u00df", "der", "k\u00fch\u00b7le", "herbst", "dem", "kal\u00b7ten", "win\u00b7ter", "weicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und endlich beyde sich zum untergange neigen/", "tokens": ["Und", "end\u00b7lich", "bey\u00b7de", "sich", "zum", "un\u00b7ter\u00b7gan\u00b7ge", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "PRF", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dann f\u00e4ngt der na\u00dfe mertz den fr\u00fchling wieder an/", "tokens": ["Dann", "f\u00e4ngt", "der", "na\u00b7\u00dfe", "mertz", "den", "fr\u00fch\u00b7ling", "wie\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die strenge lufft zertrennt den dicken dunst der erden/", "tokens": ["Die", "stren\u00b7ge", "lufft", "zer\u00b7trennt", "den", "di\u00b7cken", "dunst", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und denn bricht der aprill die bunte blumen-bahn/", "tokens": ["Und", "denn", "bricht", "der", "ap\u00b7rill", "die", "bun\u00b7te", "blu\u00b7men\u00b7bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und l\u00e4st den kalten schnee zu frischen purpur werden:", "tokens": ["Und", "l\u00e4st", "den", "kal\u00b7ten", "schnee", "zu", "fri\u00b7schen", "pur\u00b7pur", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So must es/ traurige/ der mutter auch ergehn;", "tokens": ["So", "must", "es", "/", "trau\u00b7ri\u00b7ge", "/", "der", "mut\u00b7ter", "auch", "er\u00b7gehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "ADJA", "$(", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhr alter war nunmehr bi\u00df in den winter kommen:", "tokens": ["Ihr", "al\u00b7ter", "war", "nun\u00b7mehr", "bi\u00df", "in", "den", "win\u00b7ter", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Weil sie von neuem nun soll wieder aufferstehn/", "tokens": ["Weil", "sie", "von", "neu\u00b7em", "nun", "soll", "wie\u00b7der", "auf\u00b7fer\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADV", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So hat der krancke mertz die k\u00e4lte weggenommen.", "tokens": ["So", "hat", "der", "kran\u00b7cke", "mertz", "die", "k\u00e4l\u00b7te", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir graben nichts als schnee und schwache glieder ein;", "tokens": ["Wir", "gra\u00b7ben", "nichts", "als", "schnee", "und", "schwa\u00b7che", "glie\u00b7der", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "VVFIN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dort aber scheinet schon der fr\u00fchling ihrer seelen/", "tokens": ["Dort", "a\u00b7ber", "schei\u00b7net", "schon", "der", "fr\u00fch\u00b7ling", "ih\u00b7rer", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und schreibt euch insgesamt auf ihren leichen-stein:", "tokens": ["Und", "schreibt", "euch", "ins\u00b7ge\u00b7samt", "auf", "ih\u00b7ren", "lei\u00b7chen\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wer GOtt liebt/ soll sich nicht um fromme seelen qu\u00e4len.", "tokens": ["Wer", "Gott", "liebt", "/", "soll", "sich", "nicht", "um", "from\u00b7me", "see\u00b7len", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$(", "VMFIN", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}