{"textgrid.poem.41005": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: In einer Winternacht tr\u00e4umt' ich ", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Winternacht tr\u00e4umt' ich ", "tokens": ["In", "ei\u00b7ner", "Win\u00b7ter\u00b7nacht", "tr\u00e4umt'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich stand allein in eines Kirchhofs Raum,", "tokens": ["Ich", "stand", "al\u00b7lein", "in", "ei\u00b7nes", "Kirch\u00b7hofs", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hell schien der Mond von blauer Himmelsh\u00f6h',", "tokens": ["Hell", "schien", "der", "Mond", "von", "blau\u00b7er", "Him\u00b7mels\u00b7h\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich spiegelnd in der Gr\u00e4ber Eis und Schnee,", "tokens": ["Sich", "spie\u00b7gelnd", "in", "der", "Gr\u00e4\u00b7ber", "Eis", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kein Laut, als nur der eis'gen Flocken Fall,", "tokens": ["Kein", "Laut", ",", "als", "nur", "der", "eis'\u00b7gen", "Flo\u00b7cken", "Fall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Kreuz und Baum zartklingender Kristall;", "tokens": ["Vom", "Kreuz", "und", "Baum", "zart\u00b7klin\u00b7gen\u00b7der", "Kris\u00b7tall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der weite Garten gl\u00e4nzend wei\u00df und rein.", "tokens": ["Der", "wei\u00b7te", "Gar\u00b7ten", "gl\u00e4n\u00b7zend", "wei\u00df", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Drin fiel mir auf ein H\u00fcgel schmal und klein,", "tokens": ["Drin", "fiel", "mir", "auf", "ein", "H\u00fc\u00b7gel", "schmal", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Weil der vor allen licht und lilienwei\u00df,", "tokens": ["Weil", "der", "vor", "al\u00b7len", "licht", "und", "li\u00b7li\u00b7en\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PIAT", "NN", "KON", "ADJD", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Doch nicht durch Mondesstrahlen auf sein Eis.", "tokens": ["Doch", "nicht", "durch", "Mon\u00b7des\u00b7strah\u00b7len", "auf", "sein", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "ia