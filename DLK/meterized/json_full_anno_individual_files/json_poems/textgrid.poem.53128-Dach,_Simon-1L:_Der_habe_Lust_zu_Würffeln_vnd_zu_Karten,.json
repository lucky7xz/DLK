{"textgrid.poem.53128": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der habe Lust zu W\u00fcrffeln vnd zu Karten,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der habe Lust zu W\u00fcrffeln vnd zu Karten,", "tokens": ["Der", "ha\u00b7be", "Lust", "zu", "W\u00fcrf\u00b7feln", "vnd", "zu", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der zu dem Tantz, vnd der zum k\u00fchlen Wein,", "tokens": ["Der", "zu", "dem", "Tantz", ",", "vnd", "der", "zum", "k\u00fch\u00b7len", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "KON", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich liebe nichts, als was in diesem Garten", "tokens": ["Ich", "lie\u00b7be", "nichts", ",", "als", "was", "in", "die\u00b7sem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PIS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Drangsals-trost vnd KranckheitArtzt kan seyn,", "tokens": ["Mein", "Drang\u00b7sals\u00b7trost", "vnd", "Kran\u00b7ck\u00b7heit", "Artzt", "kan", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ihr gr\u00fcnen B\u00e4ume,", "tokens": ["Ihr", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Du Blumen Zier,", "tokens": ["Du", "Blu\u00b7men", "Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ihr Hau\u00df der Reyme,", "tokens": ["Ihr", "Hau\u00df", "der", "Rey\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ihr zwinget mir", "tokens": ["Ihr", "zwin\u00b7get", "mir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die\u00df Lied herf\u00fcr.", "tokens": ["Die\u00df", "Lied", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mir mangelt nur mein Spiel, die s\u00fcsse Geige,", "tokens": ["Mir", "man\u00b7gelt", "nur", "mein", "Spiel", ",", "die", "s\u00fcs\u00b7se", "Gei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die w\u00fcrdig ist, da\u00df sie mit Macht erschall',", "tokens": ["Die", "w\u00fcr\u00b7dig", "ist", ",", "da\u00df", "sie", "mit", "Macht", "er\u00b7schall'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hie, wo das Laub vnd die begr\u00fcnten Zweige", "tokens": ["Hie", ",", "wo", "das", "Laub", "vnd", "die", "be\u00b7gr\u00fcn\u00b7ten", "Zwei\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Graben mich vmbschatten \u00fcberal,", "tokens": ["Am", "Gra\u00b7ben", "mich", "vmb\u00b7schat\u00b7ten", "\u00fc\u00b7be\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hie wo von weiten", "tokens": ["Hie", "wo", "von", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPR", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Gegend lacht,", "tokens": ["Die", "Ge\u00b7gend", "lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wo an der Seiten", "tokens": ["Wo", "an", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Wiesen Pracht", "tokens": ["Der", "Wie\u00b7sen", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mich fr\u00f6lich macht.", "tokens": ["Mich", "fr\u00f6\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was mir gebricht an Geld vnd grossen Sch\u00e4tzen,", "tokens": ["Was", "mir", "ge\u00b7bricht", "an", "Geld", "vnd", "gros\u00b7sen", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df mein Gem\u00fcht vnd dessen g\u00fcldne Ruh", "tokens": ["Mu\u00df", "mein", "Ge\u00b7m\u00fcht", "vnd", "des\u00b7sen", "g\u00fcld\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch freyes Thun vnd Fr\u00f6lichkeit ersetzen,", "tokens": ["Durch", "frey\u00b7es", "Thun", "vnd", "Fr\u00f6\u00b7lich\u00b7keit", "er\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die schleusst vor mir das Hau\u00df der Sorgen zu;", "tokens": ["Die", "schleusst", "vor", "mir", "das", "Hau\u00df", "der", "Sor\u00b7gen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wil es geben", "tokens": ["Ich", "wil", "es", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Vmb keine Welt,", "tokens": ["Vmb", "kei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df sich mein Leben", "tokens": ["Da\u00df", "sich", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Oft ohne Geld", "tokens": ["Oft", "oh\u00b7ne", "Geld"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So frewdig h\u00e4lt.", "tokens": ["So", "frew\u00b7dig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Gesetzt, da\u00df ich den Erdenkrei\u00df besesse,", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "ich", "den", "Er\u00b7den\u00b7krei\u00df", "be\u00b7ses\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd h\u00e4tte nichts mit guter Lust gemein,", "tokens": ["Vnd", "h\u00e4t\u00b7te", "nichts", "mit", "gu\u00b7ter", "Lust", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann ich der Zeit in Angst vnd Furcht gen\u00f6sse,", "tokens": ["Wann", "ich", "der", "Zeit", "in", "Angst", "vnd", "Furcht", "ge\u00b7n\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was w\u00fcrd' es mir doch f\u00fcr ein Vortheil seyn?", "tokens": ["Was", "w\u00fcrd'", "es", "mir", "doch", "f\u00fcr", "ein", "Vor\u00b7theil", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weg mit dem allen,", "tokens": ["Weg", "mit", "dem", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIAT", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was Vnmuht bringt!", "tokens": ["Was", "Vn\u00b7muht", "bringt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mir sol gefallen", "tokens": ["Mir", "sol", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Was lacht vnd singt", "tokens": ["Was", "lacht", "vnd", "singt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Vnd Frewd' erzwingt.", "tokens": ["Vnd", "Fre\u00b7wd'", "er\u00b7zwingt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ihr alten Baum', vnd jhr noch junge Pflantzen,", "tokens": ["Ihr", "al\u00b7ten", "Baum'", ",", "vnd", "jhr", "noch", "jun\u00b7ge", "Pflant\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rings vmb verwahrt vor aller Winde Sto\u00df,", "tokens": ["Rings", "vmb", "ver\u00b7wahrt", "vor", "al\u00b7ler", "Win\u00b7de", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVFIN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo vmb vnd vmb sich Frewd' vnd Ruh verschantzen,", "tokens": ["Wo", "vmb", "vnd", "vmb", "sich", "Fre\u00b7wd'", "vnd", "Ruh", "ver\u00b7schant\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "KON", "APPR", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Senckt alle Lust herab in meinen Scho\u00df,", "tokens": ["Senckt", "al\u00b7le", "Lust", "her\u00b7ab", "in", "mei\u00b7nen", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr solt imgleichen", "tokens": ["Ihr", "solt", "im\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Durch die\u00df mein Lied", "tokens": ["Durch", "die\u00df", "mein", "Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDS", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Auch nicht verbleichen,", "tokens": ["Auch", "nicht", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "So lang man Bl\u00fcht", "tokens": ["So", "lang", "man", "Bl\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIS", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Auf Erden sieht.", "tokens": ["Auf", "Er\u00b7den", "sieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Der habe Lust zu W\u00fcrffeln vnd zu Karten,", "tokens": ["Der", "ha\u00b7be", "Lust", "zu", "W\u00fcrf\u00b7feln", "vnd", "zu", "Kar\u00b7ten", ","], "token_info": ["word"