{"textgrid.poem.39179": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[es ist das Lied ein reisiges Geschwader]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist das Lied ein reisiges Geschwader", "tokens": ["Es", "ist", "das", "Lied", "ein", "rei\u00b7si\u00b7ges", "Ge\u00b7schwa\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sprengt von dannen im Trompetenschmettern;", "tokens": ["Und", "sprengt", "von", "dan\u00b7nen", "im", "Trom\u00b7pe\u00b7ten\u00b7schmet\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Aufbr\u00fcllt der Schlachtruf zu den ew'gen G\u00f6ttern,", "tokens": ["Auf\u00b7br\u00fcllt", "der", "Schlach\u00b7truf", "zu", "den", "ew'\u00b7gen", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus jedem Verse donnert Grimm und Hader.", "tokens": ["Aus", "je\u00b7dem", "Ver\u00b7se", "don\u00b7nert", "Grimm", "und", "Ha\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aufquillt dem Hengst am Hals die dicke Ader,", "tokens": ["Auf\u00b7quillt", "dem", "Hengst", "am", "Hals", "die", "di\u00b7cke", "A\u00b7der", ","], "token_inf