{"dta.poem.1272": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsame und Verliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbte Nacht/ in der mich Lieb' und Schrecken", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "Nacht", "/", "in", "der", "mich", "Lieb'", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ohn Unterla\u00df von meiner Ruh erwecken/", "tokens": ["Ohn", "Un\u00b7ter\u00b7la\u00df", "von", "mei\u00b7ner", "Ruh", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn k\u00f6mmt einmahl die angenehme Nacht/", "tokens": ["Wenn", "k\u00f6mmt", "ein\u00b7mahl", "die", "an\u00b7ge\u00b7neh\u00b7me", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die meiner Pein ein fr\u00f6lich Ende macht?", "tokens": ["Die", "mei\u00b7ner", "Pein", "ein", "fr\u00f6\u00b7lich", "En\u00b7de", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du gehst dahin/ nicht aber mein Betr\u00fcben/", "tokens": ["Du", "gehst", "da\u00b7hin", "/", "nicht", "a\u00b7ber", "mein", "Be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$(", "PTKNEG", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Morgen k\u00f6mmt/ nicht aber mein Belieben:", "tokens": ["Der", "Mor\u00b7gen", "k\u00f6mmt", "/", "nicht", "a\u00b7ber", "mein", "Be\u00b7lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein frischer Thau erquickt das d\u00fcrre Land;", "tokens": ["Dein", "fri\u00b7scher", "Thau", "er\u00b7quickt", "das", "d\u00fcr\u00b7re", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer k\u00fchlet mir den ungel\u00f6schten Brand?", "tokens": ["Wer", "k\u00fch\u00b7let", "mir", "den", "un\u00b7ge\u00b7l\u00f6schten", "Brand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Der Sterne Glantz erleuchtet deinen Schaten/", "tokens": ["Der", "Ster\u00b7ne", "Glantz", "er\u00b7leuch\u00b7tet", "dei\u00b7nen", "Scha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lehret dich der heissen Sonn entrathen;", "tokens": ["Und", "leh\u00b7ret", "dich", "der", "heis\u00b7sen", "Sonn", "ent\u00b7ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer tr\u00f6stet mich/ wenn dieses Auge weint/", "tokens": ["Wer", "tr\u00f6s\u00b7tet", "mich", "/", "wenn", "die\u00b7ses", "Au\u00b7ge", "weint", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "KOUS", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ihm kein Stern und keine Sonne scheint?", "tokens": ["Da\u00df", "ihm", "kein", "Stern", "und", "kei\u00b7ne", "Son\u00b7ne", "scheint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Philander ruht in s\u00fcssen Schlaff gewieget/", "tokens": ["Phi\u00b7lan\u00b7der", "ruht", "in", "s\u00fcs\u00b7sen", "Schlaff", "ge\u00b7wie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Einsamkeit in meinen Armen lieget:", "tokens": ["Wenn", "Ein\u00b7sam\u00b7keit", "in", "mei\u00b7nen", "Ar\u00b7men", "lie\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die leichte Last der Federn ist zu schwer/", "tokens": ["Die", "leich\u00b7te", "Last", "der", "Fe\u00b7dern", "ist", "zu", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich wende mich vergebens hin und her.", "tokens": ["Ich", "wen\u00b7de", "mich", "ver\u00b7ge\u00b7bens", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Endimion kan mit Dianens K\u00fcssen", "tokens": ["En\u00b7di\u00b7mi\u00b7on", "kan", "mit", "Di\u00b7a\u00b7nens", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "NE", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den Uberdru\u00df der langen Nacht vers\u00fcssen:", "tokens": ["Den", "U\u00b7berd\u00b7ru\u00df", "der", "lan\u00b7gen", "Nacht", "ver\u00b7s\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Hunger w\u00e4chst durch fremden Uberflu\u00df:", "tokens": ["Mein", "Hun\u00b7ger", "w\u00e4chst", "durch", "frem\u00b7den", "U\u00b7ber\u00b7flu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach h\u00e4tt ich nur f\u00fcr tausend einen Ku\u00df!", "tokens": ["Ach", "h\u00e4tt", "ich", "nur", "f\u00fcr", "tau\u00b7send", "ei\u00b7nen", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "APPR", "CARD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der stille Brand verzehret mein Gebl\u00fctte/", "tokens": ["Der", "stil\u00b7le", "Brand", "ver\u00b7zeh\u00b7ret", "mein", "Ge\u00b7bl\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Hertze raucht/ wie Bajens Schwefel-H\u00fctte/", "tokens": ["Mein", "Hert\u00b7ze", "raucht", "/", "wie", "Ba\u00b7jens", "Schwe\u00b7fel\u00b7H\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "$(", "KOKOM", "NE", "NE", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Geister sind bey mir umsonst bem\u00fcht/", "tokens": ["Die", "Geis\u00b7ter", "sind", "bey", "mir", "um\u00b7sonst", "be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "An der man selbst nur d\u00fcrren Schatten sieht.", "tokens": ["An", "der", "man", "selbst", "nur", "d\u00fcr\u00b7ren", "Schat\u00b7ten", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Komm/ Sonne/ komm/ und bringe deinen Morgen/", "tokens": ["Komm", "/", "Son\u00b7ne", "/", "komm", "/", "und", "brin\u00b7ge", "dei\u00b7nen", "Mor\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "$(", "VVFIN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm fr\u00fcher Tag/ du Trost verliebter Sorgen/", "tokens": ["Komm", "fr\u00fc\u00b7her", "Tag", "/", "du", "Trost", "ver\u00b7lieb\u00b7ter", "Sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und la\u00df mich den/ den ich verlange/ sehn/", "tokens": ["Und", "la\u00df", "mich", "den", "/", "den", "ich", "ver\u00b7lan\u00b7ge", "/", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ART", "$(", "PRELS", "PPER", "VVFIN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sonst ists um mich f\u00fcr Abends noch geschehn.", "tokens": ["Sonst", "ists", "um", "mich", "f\u00fcr", "A\u00b7bends", "noch", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PRF", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}