{"dta.poem.16491": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "152.  Du selbst must GOttes L\u00e4m-  \n lein seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df GOtt ein L\u00e4mmlein ist/ das hilfft dich nicht mein", "tokens": ["Da\u00df", "Gott", "ein", "L\u00e4mm\u00b7lein", "ist", "/", "das", "hilfft", "dich", "nicht", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "PPOSAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Christ:", "tokens": ["Christ", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wo du nicht selber auch ein L\u00e4mmlein GOttes bist.", "tokens": ["Wo", "du", "nicht", "sel\u00b7ber", "auch", "ein", "L\u00e4mm\u00b7lein", "Got\u00b7tes", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "ADV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}