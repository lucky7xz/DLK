{"textgrid.poem.50917": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Klage der Magd", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der Lenz gekommen,", "tokens": ["Nun", "ist", "der", "Lenz", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun bl\u00fchen alle Wiesen,", "tokens": ["Nun", "bl\u00fc\u00b7hen", "al\u00b7le", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun herrschen Glanz und Liebe", "tokens": ["Nun", "herr\u00b7schen", "Glanz", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Erden weit und breit;", "tokens": ["Auf", "Er\u00b7den", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur meine b\u00f6se Herrin,", "tokens": ["Nur", "mei\u00b7ne", "b\u00f6\u00b7se", "Her\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Sie keift und zetert immer", "tokens": ["Sie", "keift", "und", "ze\u00b7tert", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Noch, wie in der betr\u00fcbten", "tokens": ["Noch", ",", "wie", "in", "der", "be\u00b7tr\u00fcb\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dunklen Winterzeit!", "tokens": ["Und", "dunk\u00b7len", "Win\u00b7ter\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich am fr\u00fchen Morgen", "tokens": ["Wenn", "ich", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit aufgewachtem Herzen", "tokens": ["Mit", "auf\u00b7ge\u00b7wach\u00b7tem", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Garten schaff und singe,", "tokens": ["Im", "Gar\u00b7ten", "schaff", "und", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt mir freundlich blickt:", "tokens": ["Die", "Welt", "mir", "freund\u00b7lich", "blickt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wirft sie mir aus dem Fenster", "tokens": ["Wirft", "sie", "mir", "aus", "dem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Die ungef\u00fcgen Worte,", "tokens": ["Die", "un\u00b7ge\u00b7f\u00fc\u00b7gen", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df rasch in meiner Kehle", "tokens": ["Da\u00df", "rasch", "in", "mei\u00b7ner", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein jedes Lied erstickt!", "tokens": ["Ein", "je\u00b7des", "Lied", "er\u00b7stickt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn mein Vielgeliebter", "tokens": ["Und", "wenn", "mein", "Viel\u00b7ge\u00b7lieb\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Hag vor\u00fcberwandelt", "tokens": ["Am", "Hag", "vor\u00b7\u00fc\u00b7ber\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ein paar hei\u00dfe Blicke", "tokens": ["Und", "ein", "paar", "hei\u00b7\u00dfe", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir in die Seele warf:", "tokens": ["Mir", "in", "die", "See\u00b7le", "warf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kommt sie und streut mit Schelten", "tokens": ["Kommt", "sie", "und", "streut", "mit", "Schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ausgesuchter Bosheit", "tokens": ["Und", "aus\u00b7ge\u00b7such\u00b7ter", "Bos\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mir in die s\u00fc\u00dfe Wallung", "tokens": ["Mir", "in", "die", "s\u00fc\u00b7\u00dfe", "Wal\u00b7lung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Tod, so eisig scharf!", "tokens": ["Den", "Tod", ",", "so", "ei\u00b7sig", "scharf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn am Mittagsmahle", "tokens": ["Und", "wenn", "am", "Mit\u00b7tags\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich mit gesenkten Augen", "tokens": ["Ich", "mit", "ge\u00b7senk\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am Tische sitz und esse", "tokens": ["Am", "Ti\u00b7sche", "sitz", "und", "es\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "KON", "NN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Und m\u00e4uschenstille bin:", "tokens": ["Und", "m\u00e4u\u00b7schen\u00b7stil\u00b7le", "bin", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zielt sie mit schiefen Augen,", "tokens": ["Zielt", "sie", "mit", "schie\u00b7fen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mit harten, spitzen Reden", "tokens": ["Mit", "har\u00b7ten", ",", "spit\u00b7zen", "Re\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und oft mit groben Scherzen", "tokens": ["Und", "oft", "mit", "gro\u00b7ben", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor allen nach mir hin,", "tokens": ["Vor", "al\u00b7len", "nach", "mir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df hungernd ich, mit Tr\u00e4nen", "tokens": ["Da\u00df", "hun\u00b7gernd", "ich", ",", "mit", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "VVPP", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Essen stehenlassen", "tokens": ["Das", "Es\u00b7sen", "ste\u00b7hen\u00b7las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mich hinweg mu\u00df wenden", "tokens": ["Und", "mich", "hin\u00b7weg", "mu\u00df", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APZR", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Scham und voll Verdru\u00df", "tokens": ["Voll", "Scham", "und", "voll", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und weinend im Verborgnen", "tokens": ["Und", "wei\u00b7nend", "im", "Ver\u00b7borg\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein St\u00fccklein harten Brotes", "tokens": ["Ein", "St\u00fcck\u00b7lein", "har\u00b7ten", "Bro\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit all den harten Reden", "tokens": ["Mit", "all", "den", "har\u00b7ten", "Re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hinunterw\u00fcrgen mu\u00df!", "tokens": ["Hin\u00b7un\u00b7ter\u00b7w\u00fcr\u00b7gen", "mu\u00df", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O lieber Gott im Himmel!", "tokens": ["O", "lie\u00b7ber", "Gott", "im", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du wei\u00dft, wie sehr es schmerzet,", "tokens": ["Du", "wei\u00dft", ",", "wie", "sehr", "es", "schmer\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man just m\u00f6chte weinen", "tokens": ["Wenn", "man", "just", "m\u00f6ch\u00b7te", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dazu essen soll!", "tokens": ["Und", "da\u00b7zu", "es\u00b7sen", "soll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man sch\u00e4mt sich, es zu zeigen,", "tokens": ["Man", "sch\u00e4mt", "sich", ",", "es", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und kann es doch nicht lassen,", "tokens": ["Und", "kann", "es", "doch", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es ist ein Zucken, W\u00fcrgen", "tokens": ["Es", "ist", "ein", "Zu\u00b7cken", ",", "W\u00fcr\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Herzen jammervoll!", "tokens": ["Im", "Her\u00b7zen", "jam\u00b7mer\u00b7voll", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sogar, wenn ich am Sonntag", "tokens": ["So\u00b7gar", ",", "wenn", "ich", "am", "Sonn\u00b7tag"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will in die Kirche gehen", "tokens": ["Will", "in", "die", "Kir\u00b7che", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mir ein armes B\u00e4ndchen", "tokens": ["Und", "mir", "ein", 