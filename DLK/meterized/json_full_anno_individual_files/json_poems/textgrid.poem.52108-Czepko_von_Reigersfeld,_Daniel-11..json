{"textgrid.poem.52108": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott theilt die W\u00fcnsch: Du hoffst die Tochter, ich den Sohn,", "tokens": ["Gott", "theilt", "die", "W\u00fcnsch", ":", "Du", "hoffst", "die", "Toch\u00b7ter", ",", "ich", "den", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir tragen keines so und beydes doch davon:", "tokens": ["Wir", "tra\u00b7gen", "kei\u00b7nes", "so", "und", "bey\u00b7des", "doch", "da\u00b7von", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "KON", "PIS", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Tochter haben dir, den Sohn mir zugedacht,", "tokens": ["Die", "Toch\u00b7ter", "ha\u00b7ben", "dir", ",", "den", "Sohn", "mir", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie selbst, so uns verkehrt zu V\u00e4tern itzt gemacht:", "tokens": ["Sie", "selbst", ",", "so", "uns", "ver\u00b7kehrt", "zu", "V\u00e4\u00b7tern", "itzt", "ge\u00b7macht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "PPER", "VVFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun wohl, der Sohn wird dir, die Tochter mir bescheert:", "tokens": ["Nun", "wohl", ",", "der", "Sohn", "wird", "dir", ",", "die", "Toch\u00b7ter", "mir", "be\u00b7scheert", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "VAFIN", "PPER", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gott mischt die Lust u. hat sie dennoch gantz gew\u00e4hrt:", "tokens": ["Gott", "mischt", "die", "Lust", "u.", "hat", "sie", "den\u00b7noch", "gantz", "ge\u00b7w\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Doch alles ist und bleibt bey Freunden stets gemein,", "tokens": ["Doch", "al\u00b7les", "ist", "und", "bleibt", "bey", "Freun\u00b7den", "stets", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "KON", "VVFIN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Treu wil gantz und gar nicht unterschieden seyn:", "tokens": ["Die", "Treu", "wil", "gantz", "und", "gar", "nicht", "un\u00b7ter\u00b7schie\u00b7den", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "KON", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wolan, was die Natur versagt, la\u00df uns vollziehn,", "tokens": ["Wo\u00b7lan", ",", "was", "die", "Na\u00b7tur", "ver\u00b7sagt", ",", "la\u00df", "uns", "voll\u00b7ziehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So krieget seinen Wunsch von uns ein jeder hin:", "tokens": ["So", "krie\u00b7get", "sei\u00b7nen", "Wunsch", "von", "uns", "ein", "je\u00b7der", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "ART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gib mir den Sohn, ich wil die Tochter geben dir,", "tokens": ["Gib", "mir", "den", "Sohn", ",", "ich", "wil", "die", "Toch\u00b7ter", "ge\u00b7ben", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dem Sohne fehlt ein Weib einmahl, ein Mann auch ihr:", "tokens": ["Dem", "Soh\u00b7ne", "fehlt", "ein", "Weib", "ein\u00b7mahl", ",", "ein", "Mann", "auch", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So kriegst die Tochter du, Herr Schildbach, ich den Sohn,", "tokens": ["So", "kriegst", "die", "Toch\u00b7ter", "du", ",", "Herr", "Schild\u00b7bach", ",", "ich", "den", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word",