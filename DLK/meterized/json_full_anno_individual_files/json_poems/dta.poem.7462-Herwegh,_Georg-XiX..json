{"dta.poem.7462": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Hermelin den Mantel umgeschlagen,", "tokens": ["Von", "Her\u00b7me\u00b7lin", "den", "Man\u00b7tel", "um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das trunkne Haubt weit \u00fcber mir im Blauen,", "tokens": ["Das", "trunk\u00b7ne", "Haubt", "weit", "\u00fc\u00b7ber", "mir", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Alpen \u2014 wie so stolz darein sie schauen,", "tokens": ["Die", "Al\u00b7pen", "wie", "so", "stolz", "da\u00b7rein", "sie", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOKOM", "ADV", "ADJD", "PAV", "PPER", "VVINF", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Als w\u00fc\u00dften sie, da\u00df sie den Himmel tragen!", "tokens": ["Als", "w\u00fc\u00df\u00b7ten", "sie", ",", "da\u00df", "sie", "den", "Him\u00b7mel", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gleich leichtbeschwingten Liebesboten jagen", "tokens": ["Gleich", "leicht\u00b7be\u00b7schwing\u00b7ten", "Lie\u00b7bes\u00b7bo\u00b7ten", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Silberstr\u00f6me hin durch Nacht und Grauen,", "tokens": ["Die", "Sil\u00b7ber\u00b7str\u00f6\u00b7me", "hin", "durch", "Nacht", "und", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Oceane von den hohen Frauen", "tokens": ["Dem", "O\u00b7cea\u00b7ne", "von", "den", "ho\u00b7hen", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Manch einen sehnsuchtsvollen Gru\u00df zu sagen.", "tokens": ["Manch", "ei\u00b7nen", "sehn\u00b7suchts\u00b7vol\u00b7len", "Gru\u00df", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Herden l\u00e4uten und die Adler fliegen,", "tokens": ["Die", "Her\u00b7den", "l\u00e4u\u00b7ten", "und", "die", "Ad\u00b7ler", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist ein ewig Rauschen, ewig Rinnen,", "tokens": ["Das", "ist", "ein", "e\u00b7wig", "Rau\u00b7schen", ",", "e\u00b7wig", "Rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als k\u00f6nnt' das Leben nimmer hier versiegen.", "tokens": ["Als", "k\u00f6nnt'", "das", "Le\u00b7ben", "nim\u00b7mer", "hier", "ver\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "L\u00e4\u00dft sich ein sch\u00f6ner, sch\u00f6ner Bild ersinnen?", "tokens": ["L\u00e4\u00dft", "sich", "ein", "sch\u00f6\u00b7ner", ",", "sch\u00f6\u00b7ner", "Bild", "er\u00b7sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch hab' ich das Sch\u00f6nste noch verschwiegen:", "tokens": ["Und", "doch", "hab'", "ich", "das", "Sch\u00f6ns\u00b7te", "noch", "ver\u00b7schwie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den frommen, stillen Friedhof mitten drinnen!", "tokens": ["Den", "from\u00b7men", ",", "stil\u00b7len", "Fried\u00b7hof", "mit\u00b7ten", "drin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}