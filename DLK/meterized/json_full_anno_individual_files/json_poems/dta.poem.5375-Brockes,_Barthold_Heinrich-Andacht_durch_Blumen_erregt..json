{"dta.poem.5375": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Andacht durch Blumen erregt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bl\u00fchest, wundersch\u00f6ne Blume,", "tokens": ["Du", "bl\u00fc\u00b7hest", ",", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In deiner Bildung, Farben-Pracht,", "tokens": ["In", "dei\u00b7ner", "Bil\u00b7dung", ",", "Fa\u00b7rben\u00b7Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geruch und Sch\u00f6nheit, dem zum Ruhme,", "tokens": ["Ge\u00b7ruch", "und", "Sch\u00f6n\u00b7heit", ",", "dem", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der dich und alle Welt gemacht.", "tokens": ["Der", "dich", "und", "al\u00b7le", "Welt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herr! gieb uns Augen, da\u00df wir sehen,", "tokens": ["Herr", "!", "gieb", "uns", "Au\u00b7gen", ",", "da\u00df", "wir", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie grosse Wunder blo\u00df durch dich,", "tokens": ["Wie", "gros\u00b7se", "Wun\u00b7der", "blo\u00df", "durch", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So \u00fcberall, als sonderlich,", "tokens": ["So", "\u00fc\u00b7be\u00b7rall", ",", "als", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jm bunten Blumen-Heer, geschehen!", "tokens": ["Jm", "bun\u00b7ten", "Blu\u00b7men\u00b7Heer", ",", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "La\u00df uns, in ihrem Schmuck und Schein,", "tokens": ["La\u00df", "uns", ",", "in", "ih\u00b7rem", "Schmuck", "und", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erwegen, da\u00df von dir allein", "tokens": ["Er\u00b7we\u00b7gen", ",", "da\u00df", "von", "dir", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie ihrer Bl\u00e4tter holdes Prangen,", "tokens": ["Sie", "ih\u00b7rer", "Bl\u00e4t\u00b7ter", "hol\u00b7des", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und zwar zu unsrer Lust, empfangen!", "tokens": ["Und", "zwar", "zu", "uns\u00b7rer", "Lust", ",", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "La\u00df uns, an ihren bunten Sch\u00e4tzen,", "tokens": ["La\u00df", "uns", ",", "an", "ih\u00b7ren", "bun\u00b7ten", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In stiller Andacht, uns ergetzen,", "tokens": ["In", "stil\u00b7ler", "An\u00b7dacht", ",", "uns", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und, dir zu Ehren, fr\u00f6lich seyn!", "tokens": ["Und", ",", "dir", "zu", "Eh\u00b7ren", ",", "fr\u00f6\u00b7lich", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "APPR", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}