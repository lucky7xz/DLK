ure": "iambic.tetra"}, "line.3": {"text": "Ins stille Thal, auf deinen H\u00fcgel, nieder,", "tokens": ["Ins", "stil\u00b7le", "Thal", ",", "auf", "dei\u00b7nen", "H\u00fc\u00b7gel", ",", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um den ein Rosenw\u00e4ldchen bl\u00fcht.", "tokens": ["Um", "den", "ein", "Ro\u00b7sen\u00b7w\u00e4ld\u00b7chen", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die schauervolle Stunde kehrt zur\u00fccke,", "tokens": ["Die", "schau\u00b7er\u00b7vol\u00b7le", "Stun\u00b7de", "kehrt", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da du mir matt und sterbend riefst,", "tokens": ["Da", "du", "mir", "matt", "und", "ster\u00b7bend", "riefst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einmal noch mit halberloschnem Blicke", "tokens": ["Und", "ein\u00b7mal", "noch", "mit", "hal\u00b7ber\u00b7lo\u00b7schnem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir l\u00e4cheltest, und dann entschliefst.", "tokens": ["Mir", "l\u00e4\u00b7chel\u00b7test", ",", "und", "dann", "ent\u00b7schliefst", "."], "token_info": ["word", "word", "pun