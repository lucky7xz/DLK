{"textgrid.poem.32663": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mein Bruder warst, als du hienieden", "tokens": ["Der", "du", "mein", "Bru\u00b7der", "warst", ",", "als", "du", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch unter Staubbewohnern gingst,", "tokens": ["Noch", "un\u00b7ter", "Staub\u00b7be\u00b7woh\u00b7nern", "gingst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner Tugend Lohn, des Himmels Frieden,", "tokens": ["Und", "dei\u00b7ner", "Tu\u00b7gend", "Lohn", ",", "des", "Him\u00b7mels", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach fr\u00fchdurchwallter Bahn, empfingst;", "tokens": ["Nach", "fr\u00fch\u00b7durch\u00b7wall\u00b7ter", "Bahn", ",", "emp\u00b7fingst", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vernimm anjetzt mein Lied! Im Staubgewande", "tokens": ["Ver\u00b7nimm", "an\u00b7jetzt", "mein", "Lied", "!", "Im", "Staub\u00b7ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Irr' ich auf dieser Welt umher,", "tokens": ["Irr'", "ich", "auf", "die\u00b7ser", "Welt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sehe, fern vom m\u00fctterlichen Lande,", "tokens": ["Und", "se\u00b7he", ",", "fern", "vom", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Lan\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den H\u00fcgel deiner Ruh nicht mehr.", "tokens": ["Den", "H\u00fc\u00b7gel", "dei\u00b7ner", "Ruh", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch oft schwingt, auf der Mitternacht Gefieder,", "tokens": ["Doch", "oft", "schwingt", ",", "auf", "der", "Mit\u00b7ter\u00b7nacht", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich meine Seel' empor, und sieht", "tokens": ["Sich", "mei\u00b7ne", "Seel'", "em\u00b7por", ",", "und", "sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ins stille Thal, auf deinen H\u00fcgel, nieder,", "tokens": ["Ins", "stil\u00b7le", "Thal", ",", "auf", "dei\u00b7nen", "H\u00fc\u00b7gel", ",", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um den ein Rosenw\u00e4ldchen bl\u00fcht.", "tokens": ["Um", "den", "ein", "Ro\u00b7sen\u00b7w\u00e4ld\u00b7chen", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die schauervolle Stunde kehrt zur\u00fccke,", "tokens": ["Die", "schau\u00b7er\u00b7vol\u00b7le", "Stun\u00b7de", "kehrt", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da du mir matt und sterbend riefst,", "tokens": ["Da", "du", "mir", "matt", "und", "ster\u00b7bend", "riefst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einmal noch mit halberloschnem Blicke", "tokens": ["Und", "ein\u00b7mal", "noch", "mit", "hal\u00b7ber\u00b7lo\u00b7schnem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir l\u00e4cheltest, und dann entschliefst.", "tokens": ["Mir", "l\u00e4\u00b7chel\u00b7test", ",", "und", "dann", "ent\u00b7schliefst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da sah ich auf den blassen Mund hernieder,", "tokens": ["Da", "sah", "ich", "auf", "den", "blas\u00b7sen", "Mund", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und harrte seiner Reden; ach!", "tokens": ["Und", "harr\u00b7te", "sei\u00b7ner", "Re\u00b7den", ";", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "XY", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der holde Mund entschlo\u00df sich nimmer wieder,", "tokens": ["Der", "hol\u00b7de", "Mund", "ent\u00b7schlo\u00df", "sich", "nim\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sonst so s\u00fc\u00dfe Worte sprach!", "tokens": ["Der", "sonst", "so", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", "sprach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du selber pflanztest dir die Maienglocken,", "tokens": ["Du", "sel\u00b7ber", "pflanz\u00b7test", "dir", "die", "Mai\u00b7en\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von denen ich, mit tr\u00e4ger Hand,", "tokens": ["Von", "de\u00b7nen", "ich", ",", "mit", "tr\u00e4\u00b7ger", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir einen Kranz um deine Silberlocken,", "tokens": ["Dir", "ei\u00b7nen", "Kranz", "um", "dei\u00b7ne", "Sil\u00b7berl\u00b7o\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und um die bleiche Schl\u00e4fe wand.", "tokens": ["Und", "um", "die", "blei\u00b7che", "Schl\u00e4\u00b7fe", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und in den frommgefaltnen H\u00e4nden bl\u00fchte", "tokens": ["Und", "in", "den", "fromm\u00b7ge\u00b7falt\u00b7nen", "H\u00e4n\u00b7den", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein R\u00f6schen, ach ein Bild von dir!", "tokens": ["Ein", "R\u00f6\u00b7schen", ",", "ach", "ein", "Bild", "von", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es \u00fcberlebte dich; doch bald verbl\u00fchte", "tokens": ["Es", "\u00fc\u00b7ber\u00b7leb\u00b7te", "dich", ";", "doch", "bald", "ver\u00b7bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch seine jugendliche Zier.", "tokens": ["Auch", "sei\u00b7ne", "ju\u00b7gend\u00b7li\u00b7che", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und so werd' ich verbl\u00fchn! Ach, mit der Jugend", "tokens": ["Und", "so", "werd'", "ich", "ver\u00b7bl\u00fchn", "!", "Ach", ",", "mit", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF", "$.", "ITJ", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eilt schon die Heiterkeit davon!", "tokens": ["Eilt", "schon", "die", "Hei\u00b7ter\u00b7keit", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch winket nicht dem Freunde stiller Tugend", "tokens": ["Doch", "win\u00b7ket", "nicht", "dem", "Freun\u00b7de", "stil\u00b7ler", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch jenseits dieser Welt ein Lohn?", "tokens": ["Noch", "jen\u00b7seits", "die\u00b7ser", "Welt", "ein", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja, Freund! Und darum schw\u00f6r' ich, sie zu lieben,", "tokens": ["Ja", ",", "Freund", "!", "Und", "da\u00b7rum", "schw\u00f6r'", "ich", ",", "sie", "zu", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "KON", "PAV", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr jeden Augenblick zu weihn,", "tokens": ["Ihr", "je\u00b7den", "Au\u00b7gen\u00b7blick", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Pflichten kleinste treulich auszu\u00fcben;", "tokens": ["Der", "Pflich\u00b7ten", "kleins\u00b7te", "treu\u00b7lich", "aus\u00b7zu\u00b7\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit dir des Lohnes wert zu sein!", "tokens": ["Mit", "dir", "des", "Loh\u00b7nes", "wert", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Umschweb' indes mich unsichtbar, und leite", "tokens": ["Um\u00b7schweb'", "in\u00b7des", "mich", "un\u00b7sicht\u00b7bar", ",", "und", "lei\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ADJD", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchs tr\u00fcbe Leben meinen Lauf!", "tokens": ["Durchs", "tr\u00fc\u00b7be", "Le\u00b7ben", "mei\u00b7nen", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wann ich, allzu sicher wandelnd, gleite,", "tokens": ["Und", "wann", "ich", ",", "all\u00b7zu", "si\u00b7cher", "wan\u00b7delnd", ",", "glei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "PTKA", "ADJD", "VVPP", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O dann hilf dem Gefallnen auf!", "tokens": ["O", "dann", "hilf", "dem", "Ge\u00b7fall\u00b7nen", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Der du mein Bruder warst, als du hienieden", "tokens": ["Der", "du", "mein", "Bru\u00b7der", "warst", ",", "als", "du", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch unter Staubbewohnern gingst,", "tokens": ["Noch", "un\u00b7ter", "Staub\u00b7be\u00b7woh\u00b7nern", "gingst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner Tugend Lohn, des Himmels Frieden,", "tokens": ["Und", "dei\u00b7ner", "Tu\u00b7gend", "Lohn", ",", "des", "Him\u00b7mels", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach fr\u00fchdurchwallter Bahn, empfingst;", "tokens": ["Nach", "fr\u00fch\u00b7durch\u00b7wall\u00b7ter", "Bahn", ",", "emp\u00b7fingst", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Vernimm anjetzt mein Lied! Im Staubgewande", "tokens": ["Ver\u00b7nimm", "a