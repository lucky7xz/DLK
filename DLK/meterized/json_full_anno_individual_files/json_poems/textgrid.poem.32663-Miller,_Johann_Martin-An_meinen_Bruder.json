er Hand,", "tokens": ["Von", "de\u00b7nen", "ich", ",", "mit", "tr\u00e4\u00b7ger", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir einen Kranz um deine Silberlocken,", "tokens": ["Dir", "ei\u00b7nen", "Kranz", "um", "dei\u00b7ne", "Sil\u00b7berl\u00b7o\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und um die bleiche Schl\u00e4fe wand.", "tokens": ["Und", "um", "die", "blei\u00b7che", "Schl\u00e4\u00b7fe", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Und in den frommgefaltnen H\u00e4nden bl\u00fchte", "tokens": ["Und", "in", "den", "fromm\u00b7ge\u00b7falt\u00b7nen", "H\u00e4n\u00b7den", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein R\u00f6schen, ach ein Bild von dir!", "tokens": ["Ein", "R\u00f6\u00b7schen", ",", "ach", "ein", "Bild", "von", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es \u00fcberlebte dich; doch bald verbl\u00fchte", "tokens": ["Es", "\u00fc\u00b7ber\u00b7leb\u00b7te", "dich", ";", "doch", "bald", "ver\u00b7bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", 