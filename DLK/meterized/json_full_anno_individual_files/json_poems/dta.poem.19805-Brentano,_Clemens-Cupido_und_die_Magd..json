{"dta.poem.19805": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Cupido und die Magd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cupido. Als ich bei dunkler Nacht               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "Als", "ich", "bei", "dunk\u00b7ler", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "War auf der Liebesjagd,", "tokens": ["War", "auf", "der", "Lie\u00b7bes\u00b7jagd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wollt fangen in der Still", "tokens": ["Wollt", "fan\u00b7gen", "in", "der", "Still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Herzen viel,", "tokens": ["Der", "Her\u00b7zen", "viel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da th\u00e4t sich offerirn", "tokens": ["Da", "th\u00e4t", "sich", "of\u00b7fe\u00b7rirn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein sch\u00f6ne Bauersdirn,", "tokens": ["Ein", "sch\u00f6\u00b7ne", "Bau\u00b7ers\u00b7dirn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als ich sie schlafend fand,", "tokens": ["Als", "ich", "sie", "schla\u00b7fend", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mein Bogen spannt,", "tokens": ["Mein", "Bo\u00b7gen", "spannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und scho\u00df in schneller Eil,", "tokens": ["Und", "scho\u00df", "in", "schnel\u00b7ler", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ihr Herz mit Liebespfeil.", "tokens": ["Ihr", "Herz", "mit", "Lie\u00b7be\u00b7spfeil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Magd. Tausend Sapperlot,               ", "tokens": ["Magd", ".", "Tau\u00b7send", "Sap\u00b7per\u00b7lot", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "I mein gar, mein Jackerl brennt,", "tokens": ["I", "mein", "gar", ",", "mein", "Ja\u00b7ckerl", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "PPOSAT", "ADV", "$,", "PPOSAT", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G'schosse bin i auch,", "tokens": ["G'\u00b7schos\u00b7se", "bin", "i", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "An irgend einem End,", "tokens": ["An", "ir\u00b7gend", "ei\u00b7nem", "End", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "I schmeck schon a Rauch!", "tokens": ["I", "schmeck", "schon", "a", "Rauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Cupido. In Scherz und Liebeslust               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "In", "Scherz", "und", "Lie\u00b7bes\u00b7lust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schie\u00df ich nach deiner Brust.", "tokens": ["Schie\u00df", "ich", "nach", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Magd. Schau, der Narr is g'scheid,               ", "tokens": ["Magd", ".", "Schau", ",", "der", "Narr", "is", "g'\u00b7scheid", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "ART", "NN", "FM", "FM", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schie\u00dft mer dann uff die Leut", "tokens": ["Schie\u00dft", "mer", "dann", "uff", "die", "Leut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So grad f\u00fcr Gespa\u00df,", "tokens": ["So", "grad", "f\u00fcr", "Ge\u00b7spa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Da\u00df Gott erb\u00e4rmel!", "tokens": ["Da\u00df", "Gott", "er\u00b7b\u00e4r\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Schie\u00df mer brav in Ermel,", "tokens": ["Schie\u00df", "mer", "brav", "in", "Er\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Do triffst mi nit uff die Nas.", "tokens": ["Do", "triffst", "mi", "nit", "uff", "die", "Nas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Cupido. M\u00e4gdlein treib du kein Spott,               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "M\u00e4gd\u00b7lein", "treib", "du", "kein", "Spott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich bin der Liebesgott,", "tokens": ["Ich", "bin", "der", "Lie\u00b7bes\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der nach deinem Herzen tracht',", "tokens": ["Der", "nach", "dei\u00b7nem", "Her\u00b7zen", "tracht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich nit veracht,", "tokens": ["Mich", "nit", "ver\u00b7acht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sonst brauch ich mein Gewalt,", "tokens": ["Sonst", "brauch", "ich", "mein", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du wirst's erfahren bald,", "tokens": ["Du", "wir\u00b7st's", "er\u00b7fah\u00b7ren", "bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich auch jedermann", "tokens": ["Da\u00df", "ich", "auch", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bezwingen kann,", "tokens": ["Be\u00b7zwin\u00b7gen", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mit meinen Pfeilen spitz.", "tokens": ["Mit", "mei\u00b7nen", "Pfei\u00b7len", "spitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Magd. Was schert mich dein Bolz,               ", "tokens": ["Magd", ".", "Was", "schert", "mich", "dein", "Bolz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schie\u00df dir im Holz", "tokens": ["Schie\u00df", "dir", "im", "Holz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kleine V\u00f6gle z'sammen,", "tokens": ["Klei\u00b7ne", "V\u00f6g\u00b7le", "z'\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Erdbeer oder Schwammen,", "tokens": ["Erd\u00b7beer", "o\u00b7der", "Schwam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dir zum Futter such.", "tokens": ["Dir", "zum", "Fut\u00b7ter", "such", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Cupido. Ich hab Spei\u00df und Fr\u00fcchten gnug,               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "Ich", "hab", "Spei\u00df", "und", "Fr\u00fcch\u00b7ten", "gnug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dich nur zu lieben such.", "tokens": ["Dich", "nur", "zu", "lie\u00b7ben", "such", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Magd. Ey du kleiner Diab!               ", "tokens": ["Magd", ".", "Ey", "du", "klei\u00b7ner", "Di\u00b7ab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Was verstehst du von der Liab,", "tokens": ["Was", "ver\u00b7stehst", "du", "von", "der", "Li\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bischt hintern Ohren", "tokens": ["Bischt", "hin\u00b7tern", "Oh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Noch nit trucke woren,", "tokens": ["Noch", "nit", "tru\u00b7cke", "wo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Machst noch in die Wiegen.", "tokens": ["Machst", "noch", "in", "die", "Wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Cupido. Weil du mich dann verachst,               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "Weil", "du", "mich", "dann", "ver\u00b7achst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und meiner Worte lachst,", "tokens": ["Und", "mei\u00b7ner", "Wor\u00b7te", "lachst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sollst mit Liebespein", "tokens": ["So", "sollst", "mit", "Lie\u00b7be\u00b7spein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du ganz umgeben seyn.", "tokens": ["Du", "ganz", "um\u00b7ge\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn dein Herz in Flammen brinnt,", "tokens": ["Wenn", "dein", "Herz", "in", "Flam\u00b7men", "brinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denk an das kleine Kind,", "tokens": ["Denk", "an", "das", "klei\u00b7ne", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das dir so zugesetzt,", "tokens": ["Das", "dir", "so", "zu\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So da\u00df die Liebesglut", "tokens": ["So", "da\u00df", "die", "Lie\u00b7bes\u00b7glut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dich schier verzehren thut.", "tokens": ["Dich", "schier", "ver\u00b7zeh\u00b7ren", "thut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Magd. Sollst mirs nur probieren,               ", "tokens": ["Magd", ".", "Sollst", "mirs", "nur", "pro\u00b7bie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich will dirs Fleisch kuriren,", "tokens": ["Ich", "will", "dirs", "Fleisch", "ku\u00b7ri\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will dir dein Spiegellein", "tokens": ["Will", "dir", "dein", "Spie\u00b7gel\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Ruthen kehren rein.", "tokens": ["Mit", "Ru\u00b7then", "keh\u00b7ren", "rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Cupido. Niemand mich fangen kann,               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "Nie\u00b7mand", "mich", "fan\u00b7gen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Weil ich hab Fl\u00fcgel an.", "tokens": ["Weil", "ich", "hab", "Fl\u00fc\u00b7gel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Magd. So kannst Zauberey,               ", "tokens": ["Magd", ".", "So", "kannst", "Zau\u00b7be\u00b7rey", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VMFIN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Fliegst in L\u00fcften frey,", "tokens": ["Fliegst", "in", "L\u00fcf\u00b7ten", "frey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ein geropte Gans?", "tokens": ["Wie", "ein", "ge\u00b7rop\u00b7te", "Gans", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du Spatzenhirn.", "tokens": ["Du", "Spat\u00b7zen\u00b7hirn", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Cupido. Du stolze Bauerndirn!               ", "tokens": ["Cu\u00