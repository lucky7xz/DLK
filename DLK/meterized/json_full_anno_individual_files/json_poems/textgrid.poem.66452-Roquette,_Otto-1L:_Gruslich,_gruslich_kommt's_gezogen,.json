{"textgrid.poem.66452": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gruslich, gruslich kommt's gezogen,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gruslich, gruslich kommt's gezogen,", "tokens": ["Grus\u00b7lich", ",", "grus\u00b7lich", "kommt's", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Fr\u00fchlingswinden,", "tokens": ["Mit", "den", "Fr\u00fch\u00b7lings\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und mit schaurig s\u00fc\u00dfem Wogen", "tokens": ["Und", "mit", "schau\u00b7rig", "s\u00fc\u00b7\u00dfem", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Regt sich's in den Linden.", "tokens": ["Regt", "sich's", "in", "den", "Lin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Soll die Bl\u00fcthe dich erfreun,", "tokens": ["Soll", "die", "Bl\u00fc\u00b7the", "dich", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00dft du nicht das Gruseln scheun,", "tokens": ["Mu\u00dft", "du", "nicht", "das", "Gru\u00b7seln", "scheun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN",