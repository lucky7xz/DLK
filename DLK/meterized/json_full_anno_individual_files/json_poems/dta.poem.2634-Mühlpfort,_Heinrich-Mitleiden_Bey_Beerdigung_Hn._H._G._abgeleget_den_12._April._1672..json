{"dta.poem.2634": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mitleiden  \n Bey Beerdigung Hn. H. G. abgeleget den  \n 12. April. 1672.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So gehst du/ Seeliger/ nach so viel Creutz und Leiden/", "tokens": ["So", "gehst", "du", "/", "See\u00b7li\u00b7ger", "/", "nach", "so", "viel", "Creutz", "und", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "$(", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit dem Gecreutzigten in deines Grabes Nacht!", "tokens": ["Mit", "dem", "Ge\u00b7cr\u00b7eut\u00b7zig\u00b7ten", "in", "dei\u00b7nes", "Gra\u00b7bes", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So kan auch nicht der Tod von deinem GOtt dich scheid\u0113/", "tokens": ["So", "kan", "auch", "nicht", "der", "Tod", "von", "dei\u00b7nem", "Gott", "dich", "schei\u00b7d\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er durch seinen Tod das Leben dir gebracht!", "tokens": ["Weil", "er", "durch", "sei\u00b7nen", "Tod", "das", "Le\u00b7ben", "dir", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So hast du dich gesehnt mit JEsu zu erblassen/", "tokens": ["So", "hast", "du", "dich", "ge\u00b7sehnt", "mit", "Je\u00b7su", "zu", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "APPR", "NE", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit du kanst verklart einst in ihm aufferstehn!", "tokens": ["Da\u00b7mit", "du", "kanst", "ver\u00b7klart", "einst", "in", "ihm", "auf\u00b7fer\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und weil ein finster Grab das Heil der Welt mu\u00df fassen/", "tokens": ["Und", "weil", "ein", "fins\u00b7ter", "Grab", "das", "Heil", "der", "Welt", "mu\u00df", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wilstu auch Glaubens-voll zu deinen V\u00e4tern gehn.", "tokens": ["Wils\u00b7tu", "auch", "Glau\u00b7bens\u00b7voll", "zu", "dei\u00b7nen", "V\u00e4\u00b7tern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Oelberg voller Angst ist nunmehr \u00fcberstiegen/", "tokens": ["Der", "O\u00b7el\u00b7berg", "vol\u00b7ler", "Angst", "ist", "nun\u00b7mehr", "\u00fc\u00b7bers\u00b7tie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Des HErren Leichen-Tuch h\u00fcllt jetzt den C\u00f6rper ein:", "tokens": ["Des", "Her\u00b7ren", "Lei\u00b7chen\u00b7Tuch", "h\u00fcllt", "jetzt", "den", "C\u00f6r\u00b7per", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie der Erl\u00f6ser nicht im Grab ist blieben liegen/", "tokens": ["Wie", "der", "Er\u00b7l\u00f6\u00b7ser", "nicht", "im", "Grab", "ist", "blie\u00b7ben", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "APPRART", "NN", "VAFIN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird auch dir dein Sarch kein ewig Wohn-Haus seyn.", "tokens": ["So", "wird", "auch", "dir", "dein", "Sarch", "kein", "e\u00b7wig", "Wohn\u00b7Haus", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "PPOSAT", "NN", "PIAT", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.13": {"text": "Denn ob uns allbereit die Finstern\u00fc\u00df verschlungen/", "tokens": ["Denn", "ob", "uns", "all\u00b7be\u00b7reit", "die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "ver\u00b7schlun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die S\u00fcnde gantz verstellt/ der Tod ins Garn ger\u00fcckt/", "tokens": ["Die", "S\u00fcn\u00b7de", "gantz", "ver\u00b7stellt", "/", "der", "Tod", "ins", "Garn", "ge\u00b7r\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$(", "ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ist doch die Herrligkeit des GOttes durchgedrungen/", "tokens": ["Ist", "doch", "die", "Herr\u00b7lig\u00b7keit", "des", "Got\u00b7tes", "durch\u00b7ge\u00b7drun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und hat mit neuem Glantz uns wiederumb geschm\u00fcckt.", "tokens": ["Und", "hat", "mit", "neu\u00b7em", "Glantz", "uns", "wie\u00b7de\u00b7rumb", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Den alle Himmel nicht in sich begreiffen k\u00f6nnen", "tokens": ["Den", "al\u00b7le", "Him\u00b7mel", "nicht", "in", "sich", "be\u00b7greif\u00b7fen", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "APPR", "PRF", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Umbfast ein enges Grab zu Trost der Sterbligkeit:", "tokens": ["Umb\u00b7fast", "ein", "en\u00b7ges", "Grab", "zu", "Trost", "der", "Ster\u00b7blig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er selbst das Leben stirbt/ das Leben zu gewinnen/", "tokens": ["Er", "selbst", "das", "Le\u00b7ben", "stirbt", "/", "das", "Le\u00b7ben", "zu", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Geht willig in den Tod/ damit er uns befreyt.", "tokens": ["Geht", "wil\u00b7lig", "in", "den", "Tod", "/", "da\u00b7mit", "er", "uns", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$(", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O unerme\u00dfner Lieb hochwerthe Wunder-Zeichen/", "tokens": ["O", "un\u00b7er\u00b7me\u00df\u00b7ner", "Lieb", "hoch\u00b7wert\u00b7he", "Wun\u00b7der\u00b7Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die recht magnetisch uns mit GOtt verbinden kan!", "tokens": ["Die", "recht", "mag\u00b7ne\u00b7tisch", "uns", "mit", "Gott", "ver\u00b7bin\u00b7den", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der mu\u00df ein Felsen seyn/ der bey des Herren Leichen", "tokens": ["Der", "mu\u00df", "ein", "Fel\u00b7sen", "seyn", "/", "der", "bey", "des", "Her\u00b7ren", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$(", "ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "In Thr \u00e4nen nicht zerrinnt/ und nimmt di\u00df Leiden an.", "tokens": ["In", "Thr", "\u00e4\u00b7nen", "nicht", "zer\u00b7rinnt", "/", "und", "nimmt", "di\u00df", "Lei\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKNEG", "VVPP", "$(", "KON", "VVFIN", "PDS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Ein Heyde kr\u00f6ne frey die Gr\u00e4ber mit Cypressen/", "tokens": ["Ein", "Hey\u00b7de", "kr\u00f6\u00b7ne", "frey", "die", "Gr\u00e4\u00b7ber", "mit", "Cyp\u00b7res\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Und sch\u00fctt\u2019 aus Onychstein den theuren Balsam drauff;", "tokens": ["Und", "sch\u00fctt'", "aus", "O\u00b7nych\u00b7stein", "den", "theu\u00b7ren", "Bal\u00b7sam", "drauff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Pracht/ Bilder/ Th\u00fcrm und Zier hat doch die Zeit gefressen/", "tokens": ["Pracht", "/", "Bil\u00b7der", "/", "Th\u00fcrm", "und", "Zier", "hat", "doch", "die", "Zeit", "ge\u00b7fres\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.28": {"text": "Des Heylands Grabmal weckt die Christen st\u00fcndlich auff.", "tokens": ["Des", "Hey\u00b7lands", "Grab\u00b7mal", "weckt", "die", "Chris\u00b7ten", "st\u00fcnd\u00b7lich", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer woltenicht die Welt und ihre Marter-Wochen", "tokens": ["Wer", "wol\u00b7te\u00b7nicht", "die", "Welt", "und", "ih\u00b7re", "Mar\u00b7ter\u00b7Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Verwechseln mit der Lust der ungest\u00f6rten Ruh\u2019?", "tokens": ["Ver\u00b7wech\u00b7seln", "mit", "der", "Lust", "der", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Ruh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer wil nicht/ wenn ihm schon die Augen halb gebrochen/", "tokens": ["Wer", "wil", "nicht", "/", "wenn", "ihm", "schon", "die", "Au\u00b7gen", "halb", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Auff des Erl\u00f6sers Grab mit Freuden eilen zu?", "tokens": ["Auff", "des", "Er\u00b7l\u00f6\u00b7sers", "Grab", "mit", "Freu\u00b7den", "ei\u00b7len", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und legen willig ab die Last der m\u00fcden Glieder/", "tokens": ["Und", "le\u00b7gen", "wil\u00b7lig", "ab", "die", "Last", "der", "m\u00fc\u00b7den", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die B\u00fcrde so uns hier zu Boden hat gebeugt?", "tokens": ["Die", "B\u00fcr\u00b7de", "so", "uns", "hier", "zu", "Bo\u00b7den", "hat", "ge\u00b7beugt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Erden Angst-Geschrey vertauschen umb die Lieder/", "tokens": ["Der", "Er\u00b7den", "Angst\u00b7Ge\u00b7schrey", "ver\u00b7tau\u00b7schen", "umb", "die", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wormit der Engel-Chor vor GOtt sich freudig zeigt?", "tokens": ["Wor\u00b7mit", "der", "En\u00b7gel\u00b7Chor", "vor", "Gott", "sich", "freu\u00b7dig", "zeigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ju solcher Andachts-Flamm/ und heilger Glut entz\u00fcndet", "tokens": ["Ju", "sol\u00b7cher", "An\u00b7dachts\u00b7Flamm", "/", "und", "heil\u00b7ger", "Glut", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "NN", "$(", "KON", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hast du/ ", "tokens": ["Hast", "du", "/"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.39": {"text": "Jm Glauben wolgemuth/ in Hoffnung stets gegr\u00fcndet", "tokens": ["Jm", "Glau\u00b7ben", "wol\u00b7ge\u00b7muth", "/", "in", "Hoff\u00b7nung", "stets", "ge\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "$(", "APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Verlassen diese Welt an Ehr und Leben sat.", "tokens": ["Ver\u00b7las\u00b7sen", "die\u00b7se", "Welt", "an", "Ehr", "und", "Le\u00b7ben", "sat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du hast erst GOtt geliebt/ und dann ein rein Gewissen/", "tokens": ["Du", "hast", "erst", "Gott", "ge\u00b7liebt", "/", "und", "dann", "ein", "rein", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VVPP", "$(", "KON", "ADV", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "In das die Redligkeit ihr Bildnu\u00df hat gepregt:"