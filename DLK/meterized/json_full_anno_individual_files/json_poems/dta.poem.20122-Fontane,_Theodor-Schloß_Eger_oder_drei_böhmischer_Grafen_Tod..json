{"dta.poem.20122": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Schlo\u00df Eger  \n oder  \n drei b\u00f6hmischer Grafen Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4rmend, im Schlo\u00df zu Eger", "tokens": ["L\u00e4r\u00b7mend", ",", "im", "Schlo\u00df", "zu", "E\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "APPRART", "NN", "APPR", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ueber dem Ungarwein,", "tokens": ["Ue\u00b7ber", "dem", "Un\u00b7gar\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sitzen die W\u00fcrdentr\u00e4ger", "tokens": ["Sit\u00b7zen", "die", "W\u00fcr\u00b7den\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Herzogs Wallenstein:", "tokens": ["Her\u00b7zogs", "Wal\u00b7len\u00b7stein", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Tertschka \u2014 des Feldherrn Schwager,", "tokens": ["Tertschka", "des", "Feld\u00b7herrn", "Schwa\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Illo und Kinsky dazu,", "tokens": ["Il\u00b7lo", "und", "Kins\u00b7ky", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PAV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Ihre Heimath das Lager,", "tokens": ["Ih\u00b7re", "Hei\u00b7math", "das", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und die Schlacht \u2014 ihre Ruh.", "tokens": ["Und", "die", "Schlacht", "ih\u00b7re", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Lustig flackern die Kerzen;", "tokens": ["Lus\u00b7tig", "fla\u00b7ckern", "die", "Ker\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Aber der Tertschka sprischt:", "tokens": ["A\u00b7ber", "der", "Tertschka", "sprischt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "\u201eist mir\u2019s Nacht im Herzen,", "tokens": ["\u201e", "ist", "mir's", "Nacht", "im", "Her\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NE", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Oder vor\u2019m Gesicht?", "tokens": ["O\u00b7der", "vor'm", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Diese Lichter leuchten", "tokens": ["Die\u00b7se", "Lich\u00b7ter", "leuch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie in dunkler Gruft,", "tokens": ["Wie", "in", "dunk\u00b7ler", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und die W\u00e4nde, die feuchten,", "tokens": ["Und", "die", "W\u00e4n\u00b7de", ",", "die", "feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Hauchen Grabesluft.\u201c", "tokens": ["Hau\u00b7chen", "Gra\u00b7bes\u00b7luft", ".", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Feurig funkelt der Unger;", "tokens": ["Feu\u00b7rig", "fun\u00b7kelt", "der", "Un\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Aber der Kinsky spricht:", "tokens": ["A\u00b7ber", "der", "Kins\u00b7ky", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Drau\u00dfen bei Frost und Hunger", "tokens": ["Drau\u00b7\u00dfen", "bei", "Frost", "und", "Hun\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sch\u00fcttelte so mich\u2019s nicht,", "tokens": ["Sch\u00fct\u00b7tel\u00b7te", "so", "mich's", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Hielte lieber bei L\u00fctzen", "tokens": ["Hiel\u00b7te", "lie\u00b7ber", "bei", "L\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Wieder in Qualm und Rauch;", "tokens": ["Wie\u00b7der", "in", "Qualm", "und", "Rauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wolle Gott uns sch\u00fctzen,", "tokens": ["Wol\u00b7le", "Gott", "uns", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Oder \u2014 der Teufel auch.\u201c", "tokens": ["O\u00b7der", "der", "Teu\u00b7fel", "auch", ".", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Illo nur, Herz wie Kehle", "tokens": ["Il\u00b7lo", "nur", ",", "Herz", "wie", "Keh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "NN", "KOKOM", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "H\u00e4lt er bei Laune sich,", "tokens": ["H\u00e4lt", "er", "bei", "Lau\u00b7ne", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dicht ist seine Seele", "tokens": ["Dicht", "ist", "sei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gegen Hieb und Stich,", "tokens": ["Ge\u00b7gen", "Hieb", "und", "Stich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Tr\u00e4gt ein B\u00fcffelkoller", "tokens": ["Tr\u00e4gt", "ein", "B\u00fcf\u00b7fel\u00b7kol\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie sein K\u00f6rper traun, \u2014", "tokens": ["Wie", "sein", "K\u00f6r\u00b7per", "traun", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Lustiger und toller", "tokens": ["Lus\u00b7ti\u00b7ger", "und", "tol\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "War er nie zu schaun.", "tokens": ["War", "er", "nie", "zu", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und vom Trunke heiser", "tokens": ["Und", "vom", "Trun\u00b7ke", "hei\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kreischt er jetzt und lacht:", "tokens": ["Kreischt", "er", "jetzt", "und", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u201edas erst ist der Kaiser,", "tokens": ["\u201e", "das", "erst", "ist", "der", "Kai\u00b7ser", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wer den Kaiser macht;", "tokens": ["Wer", "den", "Kai\u00b7ser", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Eid und Treue brechen", "tokens": ["Eid", "und", "Treu\u00b7e", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schreckt den Feigen allein,", "tokens": ["Schreckt", "den", "Fei\u00b7gen", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Hoch, der K\u00f6nig der Czechen,", "tokens": ["Hoch", ",", "der", "K\u00f6\u00b7nig", "der", "Cze\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Herzog Wallenstein!\u201c", "tokens": ["Her\u00b7zog", "Wal\u00b7len\u00b7stein", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "NE", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Spricht\u2019s. Da neue Bewohner", "tokens": ["Spricht'", "s.", "Da", "neu\u00b7e", "Be\u00b7woh\u00b7ner"], "token_info": ["word", "abbreviation", "word", "word", "word"], "pos": ["VVFIN", "VVIMP", "ADV", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Klirrend in Eisen und Stahl,", "tokens": ["Klir\u00b7rend", "in", "Ei\u00b7sen", "und", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Buttlersche Dragoner", "tokens": ["Butt\u00b7ler\u00b7sche", "Dra\u00b7go\u00b7ner"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dringen in den Saal;", "tokens": ["Drin\u00b7gen", "in", "den", "Saal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Buttler selbst, im Helme,", "tokens": ["Butt\u00b7ler", "selbst", ",", "im", "Hel\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Tritt an den Illo: \u201esprich,", "tokens": ["Tritt", "an", "den", "Il\u00b7lo", ":", "\u201e", "sprich", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NE", "$.", "$(", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seid Ihr Schurken und Schelme,", "tokens": ["Seid", "Ihr", "Schur\u00b7ken", "und", "Schel\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Oder gut kaiserlich?!\u201c", "tokens": ["O\u00b7der", "gut", "kai\u00b7ser\u00b7lich", "?!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "ADJD", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Hei, da fahren die Klingen", "tokens": ["Hei", ",", "da", "fah\u00b7ren", "die", "Klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wie von selber heraus,", "tokens": ["Wie", "von", "sel\u00b7ber", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADV", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Von dem Pfeifen und Schwingen", "tokens": ["Von", "dem", "Pfei\u00b7fen", "und", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "L\u00f6schen die Lichter aus;", "tokens": ["L\u00f6\u00b7schen", "die", "Lich\u00b7ter", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Weiter geht es im Dunkeln,", "tokens": ["Wei\u00b7ter", "geht", "es", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Nein, im Dunkeln nicht:", "tokens": ["Nein", ",", "im", "Dun\u00b7keln", "nicht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPRART", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ihrer Augen Funkeln", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "Fun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Giebt das rechte Licht.", "tokens": ["Giebt", "das", "rech\u00b7te", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Tertschka f\u00e4llt; daneben", "tokens": ["Tertschka", "f\u00e4llt", ";", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "$.", "PAV"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Kinsky mit Fluch und Schwur;", "tokens": ["Kins\u00b7ky", "mit", "Fluch", "und", "Schwur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mehr um Tod wie Leben", "tokens": ["Mehr", "um", "Tod", "wie", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KOKOM", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ficht selbst Illo nur,", "tokens": ["Ficht", "selbst", "Il\u00b7lo", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schl\u00e4gt blindhin in Scherben", "tokens": ["Schl\u00e4gt", "blind\u00b7hin", "in", "Scher\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sch\u00e4del und Flaschen jetzt,", "tokens": ["Sch\u00e4\u00b7del", "und", "Fla\u00b7schen", "jetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wie ein Eber im Sterben", "tokens": ["Wie", "ein", "E\u00b7ber", "im", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Noch die Hauer wetzt.", "tokens": ["Noch", "die", "Hau\u00b7er", "wetzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Licht und Fackel kommen,", "tokens": ["Licht", "und", "Fa\u00b7ckel", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Geben d\u00fcstren Schein:", "tokens": ["Ge\u00b7ben", "d\u00fcst\u00b7ren", "Schein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In einander verschwommen", "tokens": ["In", "ein\u00b7an\u00b7der", "ver\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRF", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Blinken Blut und Wein;", "tokens": ["Blin\u00b7ken", "Blut", "und", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ueberall im Saale", "tokens": ["Ue\u00b7be\u00b7rall", "im", "Saa\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Leichen in buntem Gemisch,", "tokens": ["Lei\u00b7chen", "in", "bun\u00b7tem", "Ge\u00b7misch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Stumm, vor seinem Mahle,", "tokens": ["Stumm", ",", "vor", "sei\u00b7nem", "Mah\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sitzt der Tod am Tisch.", "tokens": ["Sitzt", "der", "Tod", "am", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Buttler aber, wie Wetter", "tokens": ["Butt\u00b7ler", "a\u00b7ber", ",", "wie", "Wet\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Donnert jetzt: \u201ela\u00dft sie ruhn!", "tokens": ["Don\u00b7nert", "jetzt", ":", "\