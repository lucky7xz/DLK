{"textgrid.poem.49300": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Reimen an H. Hieronymum Snitker", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, wenn es m\u00fcglich w\u00e4r', itz v\u00f6llig zu vertreiben", "tokens": ["Herr", ",", "wenn", "es", "m\u00fcg\u00b7lich", "w\u00e4r'", ",", "itz", "v\u00f6l\u00b7lig", "zu", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "die Schmerzen, die so gar eur Vaterherz zerreiben,", "tokens": ["die", "Schmer\u00b7zen", ",", "die", "so", "gar", "eur", "Va\u00b7ter\u00b7herz", "zer\u00b7rei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df kaum mit Worten ist zu z\u00e4hlen eure Pein,", "tokens": ["da\u00df", "kaum", "mit", "Wor\u00b7ten", "ist", "zu", "z\u00e4h\u00b7len", "eu\u00b7re", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VAFIN", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so wolt' ich euch mit Hand und Mund zu Diensten sein:", "tokens": ["so", "wolt'", "ich", "euch", "mit", "Hand", "und", "Mund", "zu", "Diens\u00b7ten", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich aber, der ich selbst, und zwar vor wenig Jahren,", "tokens": ["Ich", "a\u00b7ber", ",", "der", "ich", "selbst", ",", "und", "zwar", "vor", "we\u00b7nig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "$,", "KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "was diese Schmerzen sind, mit Schmerzen hab' erfahren,", "tokens": ["was", "die\u00b7se", "Schmer\u00b7zen", "sind", ",", "mit", "Schmer\u00b7zen", "hab'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "$,", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "verdecke gleich die Not und wil zu dieser Frist", "tokens": ["ver\u00b7de\u00b7cke", "gleich", "die", "Not", "und", "wil", "zu", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "nur k\u00fcrzlich zeichnen an das, was euch tr\u00f6stlich ist.", "tokens": ["nur", "k\u00fcrz\u00b7lich", "zeich\u00b7nen", "an", "das", ",", "was", "euch", "tr\u00f6st\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PDS", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr wisset, werter Freund, da\u00df alles, was wir sehen,", "tokens": ["Ihr", "wis\u00b7set", ",", "wer\u00b7ter", "Freund", ",", "da\u00df", "al\u00b7les", ",", "was", "wir", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "KOUS", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "nachdem es seine Zeit gestanden, mu\u00df vergehen;", "tokens": ["nach\u00b7dem", "es", "sei\u00b7ne", "Zeit", "ge\u00b7stan\u00b7den", ",", "mu\u00df", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "das eine lebt und schwebt, das andre f\u00e4llt und bricht;", "tokens": ["das", "ei\u00b7ne", "lebt", "und", "schwebt", ",", "das", "and\u00b7re", "f\u00e4llt", "und", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die sch\u00f6ne Sonne selbst bleibt ja best\u00e4ndig nicht.", "tokens": ["die", "sch\u00f6\u00b7ne", "Son\u00b7ne", "selbst", "bleibt", "ja", "be\u00b7st\u00e4n\u00b7dig", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Sommer ist dahin, die bunten Blumen sterben;", "tokens": ["Der", "Som\u00b7mer", "ist", "da\u00b7hin", ",", "die", "bun\u00b7ten", "Blu\u00b7men", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wir sehen Kr\u00e4uter, B\u00e4um' und alles Laub verderben,", "tokens": ["wir", "se\u00b7hen", "Kr\u00e4u\u00b7ter", ",", "B\u00e4um'", "und", "al\u00b7les", "Laub", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "ja, was so fr\u00f6lich stund f\u00fcr einer kurzen Weil',", "tokens": ["ja", ",", "was", "so", "fr\u00f6\u00b7lich", "stund", "f\u00fcr", "ei\u00b7ner", "kur\u00b7zen", "Weil'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "erliget itz vom Reif' und zwar in gro\u00dfer Eil.", "tokens": ["er\u00b7li\u00b7get", "itz", "vom", "Reif'", "und", "zwar", "in", "gro\u00b7\u00dfer", "Eil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Inmittelst wei\u00df man doch, da\u00df, was itzund verschwindet,", "tokens": ["In\u00b7mit\u00b7telst", "wei\u00df", "man", "doch", ",", "da\u00df", ",", "was", "it\u00b7zund", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.18": {"text": "zur sch\u00f6nen Fr\u00fchlingszeit sich alles wiedrum findet", "tokens": ["zur", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7zeit", "sich", "al\u00b7les", "wie\u00b7drum", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PRF", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und gleich aufs neue lebt. Die\u00df treibt den Ackersmann,", "tokens": ["und", "gleich", "aufs", "neu\u00b7e", "lebt", ".", "Die\u00df", "treibt", "den", "A\u00b7ckers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "VVFIN", "$.", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "da\u00df er so gro\u00dfe M\u00fch' im Herbst ertragen kan.", "tokens": ["da\u00df", "er", "so", "gro\u00b7\u00dfe", "M\u00fch'", "im", "Herbst", "er\u00b7tra\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da wirft er seine Saat ins feuchte Land mit Freuden,", "tokens": ["Da", "wirft", "er", "sei\u00b7ne", "Saat", "ins", "feuch\u00b7te", "Land", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "er glaubet, wenn die K\u00e4lt' im Lenzen nun mu\u00df scheiden,", "tokens": ["er", "glau\u00b7bet", ",", "wenn", "die", "K\u00e4lt'", "im", "Len\u00b7zen", "nun", "mu\u00df", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "so wachs' und gr\u00fcn' alsdenn sein K\u00f6rnlein frisch daher;", "tokens": ["so", "wachs'", "und", "gr\u00fcn'", "als\u00b7denn", "sein", "K\u00f6rn\u00b7lein", "frisch", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "die\u00df schaffet, da\u00df ihm gar kein Arbeit f\u00e4llt zu schwer:", "tokens": ["die\u00df", "schaf\u00b7fet", ",", "da\u00df", "ihm", "gar", "kein", "Ar\u00b7beit", "f\u00e4llt", "zu", "schwer", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So wird des Menschen Leib, wenn ihn der Tod abmeiet,", "tokens": ["So", "wird", "des", "Men\u00b7schen", "Leib", ",", "wenn", "ihn", "der", "Tod", "ab\u00b7me\u00b7iet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "gleich wie das liebe Korn, in Schwachheit ausgestreuet", "tokens": ["gleich", "wie", "das", "lie\u00b7be", "Korn", ",", "in", "Schwach\u00b7heit", "aus\u00b7ge\u00b7streu\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "und stehet auf in Kraft, in Ehr' und Herlichkeit,", "tokens": ["und", "ste\u00b7het", "auf", "in", "Kraft", ",", "in", "Ehr'", "und", "Her\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "wenn Christus unser Herr zur allerletsten Zeit", "tokens": ["wenn", "Chris\u00b7tus", "un\u00b7ser", "Herr", "zur", "al\u00b7ler\u00b7lets\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sein pr\u00e4chtigs \u00bbStehet auf, die ihr vergraben liget\u00ab", "tokens": ["Sein", "pr\u00e4ch\u00b7tigs", "\u00bb", "Ste\u00b7het", "auf", ",", "die", "ihr", "ver\u00b7gra\u00b7ben", "li\u00b7get", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PTKVZ", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "l\u00e4\u00dft schallen durch die Luft und fein zusammen f\u00fcget", "tokens": ["l\u00e4\u00dft", "schal\u00b7len", "durch", "die", "Luft", "und", "fein", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "APPR", "ART", "NN", "KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "den Geist und seinen Leib, der schon so manches Jahr", "tokens": ["den", "Geist", "und", "sei\u00b7nen", "Leib", ",", "der", "schon", "so", "man\u00b7ches", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "im tiefen Scho\u00df der Erd' als Staub vergraben war...", "tokens": ["im", "tie\u00b7fen", "Scho\u00df", "der", "Erd'", "als", "Staub", "ver\u00b7gra\u00b7ben", "war", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "KOUS", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Herr, wenn es m\u00fcglich w\u00e4r', itz v\u00f6llig zu vertreiben", "tokens": ["Herr", ",", "wenn", "es", "m\u00fcg\u00b7lich", "w\u00e4r'", ",", "itz", "v\u00f6l\u00b7lig", "zu", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "die Schmerzen, die so gar eur Vaterherz zerreiben,", "tokens": ["die", "Schmer\u00b7zen", ",", "die", "so", "gar", "eur", "Va\u00b7ter\u00b7herz", "zer\u00b7rei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df kaum mit Worten ist zu z\u00e4hlen eure Pein,", "tokens": ["da\u00df", "kaum", "mit", "Wor\u00b7ten", "ist", "zu", "z\u00e4h\u00b7len", "eu\u00b7re", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VAFIN", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so wolt' ich euch mit Hand und Mund zu Diensten sein:", "tokens": ["so", "wolt'", "ich", "euch", "mit", "Hand", "und", "Mund", "zu", "Diens\u00b7ten", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich aber, der ich selbst, und zwar vor wenig Jahren,", "tokens": ["Ich", "a\u00b7ber", ",", "der", "ich", "selbst", ",", "und", "zwar", "vor", "we\u00b7nig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "$,", "KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "was diese Schmerzen sind, mit Schmerzen hab' erfahren,", "tokens": ["was", "die\u00b7se", "Schmer\u00b7zen", "sind", ",", "mit", "Schmer\u00b7zen", "hab'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VAFIN", "$,", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "verdecke gleich die Not und wil zu dieser Frist", "tokens": ["ver\u00b7de\u00b7cke", "gleich", "die", "Not", "und", "wil", "zu", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "nur k\u00fcrzlich zeichnen an das, was euch tr\u00f6stlich ist.", "tokens": ["nur", "k\u00fcrz\u00b7lich", "zeich\u00b7nen", "an", "das", ",", "was", "euch", "tr\u00f6st\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PDS", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr wisset, werter Freund, da\u00df alles, was wir sehen,", "tokens": ["Ihr", "wis\u00b7set", ",", "wer\u00b7ter", "Freund", ",", "da\u00df", "al\u00b7les", ",", "was", "wir", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "KOUS", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "nachdem es seine Zeit gestanden, mu\u00df vergehen;", "tokens": ["nach\u00b7dem", "es", "sei\u00b7ne", "Zeit", "ge\u00b7stan\u00b7den", ",", "mu\u00df", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "das eine lebt und schwebt, das andre f\u00e4llt und bricht;", "tokens": ["das", "ei\u00b7ne", "lebt", "und", "schwebt", ",", "das", "and\u00b7re", "f\u00e4llt", "und", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die sch\u00f6ne Sonne selbst bleibt ja best\u00e4ndig nicht.", "tokens": ["die", "sch\u00f6\u00b7ne", "Son\u00b7ne", "selbst", "bleibt", "ja", "be\u00b7st\u00e4n\u00b7dig", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Sommer ist dahin, die bunten Blumen sterben;", "tokens": ["Der", "Som\u00b7mer", "ist", "da\u00b7hin", ",", "die", "bun\u00b7ten", "Blu\u00b7men", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wir sehen Kr\u00e4uter, B\u00e4um' und alles Laub verderben,", "tokens": ["wir", "se\u00b7hen", "Kr\u00e4u\u00b7ter", ",", "B\u00e4um'", "und", "al\u00b7les", "Laub", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "ja, was so fr\u00f6lich stund f\u00fcr einer kurzen Weil',", "tokens": ["ja", ",", "was", "so", "fr\u00f6\u00b7lich", "stund", "f\u00fcr", "ei\u00b7ner", "kur\u00b7zen", "Weil'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "erliget itz vom Reif' und zwar in gro\u00dfer Eil.", "tokens": ["er\u00b7li\u00b7get", "itz", "vom", "Reif'", "und", "zwar", "in", "gro\u00b7\u00dfer", "Eil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Inmittelst wei\u00df man doch, da\u00df, was itzund verschwindet,", "tokens": ["In\u00b7mit\u00b7telst", "wei\u00df", "man", "doch", ",", "da\u00df", ",", "was", "it\u00b7zund", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.18": {"text": "zur sch\u00f6nen Fr\u00fchlingszeit sich alles wiedrum findet", "tokens": ["zur", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7zeit", "sich", "al\u00b7les", "wie\u00b7drum", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PRF", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und gleich aufs neue lebt. Die\u00df treibt den Ackersmann,", "tokens": ["und", "gleich", "aufs", "neu\u00b7e", "lebt", ".", "Die\u00df", "treibt", "den", "A\u00b7ckers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "VVFIN", "$.", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "da\u00df er so gro\u00dfe M\u00fch' im Herbst ertragen kan.", "tokens": ["da\u00df", "er", "so", "gro\u00b7\u00dfe", "M\u00fch'", "im", "Herbst", "er\u00b7tra\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da wirft er seine Saat ins feuchte Land mit Freuden,", "tokens": ["Da", "wirft", "er", "sei\u00b7ne", "Saat", "ins", "feuch\u00b7te", "Land", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "er glaubet, wenn die K\u00e4lt' im Lenzen nun mu\u00df scheiden,", "tokens": ["er", "glau\u00b7bet", ",", "wenn", "die", "K\u00e4lt'", "im", "Len\u00b7zen", "nun", "mu\u00df", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "so wachs' und gr\u00fcn' alsdenn sein K\u00f6rnlein frisch daher;", "tokens": ["so", "wachs'", "und", "gr\u00fcn'", "als\u00b7denn", "sein", "K\u00f6rn\u00b7lein", "frisch", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "die\u00df schaffet, da\u00df ihm gar kein Arbeit f\u00e4llt zu schwer:", "tokens": ["die\u00df", "schaf\u00b7fet", ",", "da\u00df", "ihm", "gar", "kein", "Ar\u00b7beit", "f\u00e4llt", "zu", "schwer", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-