{"textgrid.poem.51032": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, der so lang im Herzen mich geborgen,", "tokens": ["Du", ",", "der", "so", "lang", "im", "Her\u00b7zen", "mich", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit allen meinen gr\u00e4mlichen Gebrechen,", "tokens": ["Mit", "al\u00b7len", "mei\u00b7nen", "gr\u00e4m\u00b7li\u00b7chen", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit meinen hastig immer neuen Schw\u00e4chen,", "tokens": ["Mit", "mei\u00b7nen", "has\u00b7tig", "im\u00b7mer", "neu\u00b7en", "Schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit allen meinen wunderlichen Sorgen;", "tokens": ["Mit", "al\u00b7len", "mei\u00b7nen", "wun\u00b7der\u00b7li\u00b7chen", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Hand vergessend botest jeden Morgen,", "tokens": ["Die", "Hand", "ver\u00b7ges\u00b7send", "bo\u00b7test", "je\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich die Nacht vorher mit blindem Stechen,", "tokens": ["Wenn", "ich", "die", "Nacht", "vor\u00b7her", "mit", "blin\u00b7dem", "Ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit ungerechtem oder bittrem Sprechen", "tokens": ["Mit", "un\u00b7ge\u00b7rech\u00b7tem", "o\u00b7der", "bitt\u00b7rem", "Spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir schnitt ins Herz, so treu und unverborgen;", "tokens": ["Dir", "schnitt", "ins", "Herz", ",", "so", "treu", "und", "un\u00b7ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht um zu sp\u00e4hn nach Tadel oder Lobe,", "tokens": ["Nicht", "um", "zu", "sp\u00e4hn", "nach", "Ta\u00b7del", "o\u00b7der", "Lo\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUI", "PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will ich dir diese Lieder \u00fcbersenden,", "tokens": ["Will", "ich", "dir", "die\u00b7se", "Lie\u00b7der", "\u00fc\u00b7ber\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh u