{"dta.poem.7593": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der F\u00fcrst ritt auf die Jagd, und ward durch ein Gewitter", "tokens": ["Der", "F\u00fcrst", "ritt", "auf", "die", "Jagd", ",", "und", "ward", "durch", "ein", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Getrennt vom stattlichen Geleite seiner Ritter.", "tokens": ["Ge\u00b7trennt", "vom", "statt\u00b7li\u00b7chen", "Ge\u00b7lei\u00b7te", "sei\u00b7ner", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er fand zum erstenmal, woran er nie gedacht,", "tokens": ["Er", "fand", "zum", "ers\u00b7ten\u00b7mal", ",", "wo\u00b7ran", "er", "nie", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADV", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohnm\u00e4chtig selber sich in eines H\u00f6hern Macht.", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "sel\u00b7ber", "sich", "in", "ei\u00b7nes", "H\u00f6\u00b7hern", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihm war nun Heer und Hof und Herrschaft ohne Nutz,", "tokens": ["Ihm", "war", "nun", "Heer", "und", "Hof", "und", "Herr\u00b7schaft", "oh\u00b7ne", "Nutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er suchte gegen Sturm im offnen Felde Schutz.", "tokens": ["Er", "such\u00b7te", "ge\u00b7gen", "Sturm", "im", "off\u00b7nen", "Fel\u00b7de", "Schutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er sp\u00e4hte weit umher, und sah mit halber Freude", "tokens": ["Er", "sp\u00e4h\u00b7te", "weit", "um\u00b7her", ",", "und", "sah", "mit", "hal\u00b7ber", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuletzt ein l\u00e4ndliches unscheinbares Geb\u00e4ude.", "tokens": ["Zu\u00b7letzt", "ein", "l\u00e4nd\u00b7li\u00b7ches", "un\u00b7schein\u00b7ba\u00b7res", "Ge\u00b7b\u00e4u\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.5": {"line.1": {"text": "Mit Unmuth trat er ein ins niedre H\u00fcttendach;", "tokens": ["Mit", "Un\u00b7muth", "trat", "er", "ein", "ins", "nied\u00b7re", "H\u00fct\u00b7ten\u00b7dach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit seiner Tochter sa\u00df ein Vater im Gemach.", "tokens": ["Mit", "sei\u00b7ner", "Toch\u00b7ter", "sa\u00df", "ein", "Va\u00b7ter", "im", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der alte Vater herb, ein Landmann starr und spr\u00f6de,", "tokens": ["Der", "al\u00b7te", "Va\u00b7ter", "herb", ",", "ein", "Land\u00b7mann", "starr", "und", "spr\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die junge Tochter mild, ein Landkind hold und bl\u00f6de;", "tokens": ["Die", "jun\u00b7ge", "Toch\u00b7ter", "mild", ",", "ein", "Land\u00b7kind", "hold", "und", "bl\u00f6\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Alsob ein alter Dorn mit rauhbemoos'tem Nacken", "tokens": ["Al\u00b7sob", "ein", "al\u00b7ter", "Dorn", "mit", "rauh\u00b7be\u00b7moos'\u00b7tem", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sch\u00f6nste Rose tr\u00fcg' als Schmuck an seinen Zacken.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Ro\u00b7se", "tr\u00fcg'", "als", "Schmuck", "an", "sei\u00b7nen", "Za\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der F\u00fcrst gewahrte nicht die Rose duftumschwommen,", "tokens": ["Der", "F\u00fcrst", "ge\u00b7wahr\u00b7te", "nicht", "die", "Ro\u00b7se", "duf\u00b7tum\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00f6rt' es kaum, wie ihn der Vater hie\u00df willkommen.", "tokens": ["Und", "h\u00f6rt'", "es", "kaum", ",", "wie", "ihn", "der", "Va\u00b7ter", "hie\u00df", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der Tochter winkte der, die sich mit Anstand sch\u00fcrzte,", "tokens": ["Der", "Toch\u00b7ter", "wink\u00b7te", "der", ",", "die", "sich", "mit", "An\u00b7stand", "sch\u00fcrz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Gast ein Mahl auftrug, und es mit Anmuth w\u00fcrzte.", "tokens": ["Dem", "Gast", "ein", "Mahl", "auf\u00b7trug", ",", "und", "es", "mit", "An\u00b7muth", "w\u00fcrz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Das Mahl blieb unber\u00fchrt, der Gast stumm und verdrossen,", "tokens": ["Das", "Mahl", "blieb", "un\u00b7be\u00b7r\u00fchrt", ",", "der", "Gast", "stumm", "und", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die W\u00fcrze merkt' er nicht, sonst h\u00e4tt' er es genossen.", "tokens": ["Die", "W\u00fcr\u00b7ze", "merkt'", "er", "nicht", ",", "sonst", "h\u00e4tt'", "er", "es", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Er dacht' im stillen Kreis an seinen lauten Tro\u00df,", "tokens": ["Er", "dacht'", "im", "stil\u00b7len", "Kreis", "an", "sei\u00b7nen", "lau\u00b7ten", "Tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus der nackten H\u00fctt' in sein vergoldet Schlo\u00df.", "tokens": ["Und", "aus", "der", "nack\u00b7ten", "H\u00fctt'", "in", "sein", "ver\u00b7gol\u00b7det", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Da trat am Abend ein des Bauern Knecht, der Hirte,", "tokens": ["Da", "trat", "am", "A\u00b7bend", "ein", "des", "Bau\u00b7ern", "Knecht", ",", "der", "Hir\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ART", "NN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um der Herde Stand ward er befragt vom Wirthe.", "tokens": ["Und", "um", "der", "Her\u00b7de", "Stand", "ward", "er", "be\u00b7fragt", "vom", "Wirt\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Er sprach: die Herde war noch nie in schlimmerm Stande,", "tokens": ["Er", "sprach", ":", "die", "Her\u00b7de", "war", "noch", "nie", "in", "schlim\u00b7merm", "Stan\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Nahrung scheint ihr nicht mehr anzustehn im Lande.", "tokens": ["Die", "Nah\u00b7rung", "scheint", "ihr", "nicht", "mehr", "an\u00b7zu\u00b7stehn", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "VVIZU", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Die Euter alle sind versiegt, es hilft kein F\u00fcttern,", "tokens": ["Die", "Eu\u00b7ter", "al\u00b7le", "sind", "ver\u00b7siegt", ",", "es", "hilft", "kein", "F\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den eignen L\u00e4mmern wird kein Trunk von ihren M\u00fcttern.", "tokens": ["Den", "eig\u00b7nen", "L\u00e4m\u00b7mern", "wird", "kein", "Trunk", "von", "ih\u00b7ren", "M\u00fct\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Der alte Landmann wiegt sein Haupt erstaunt: Versiegt", "tokens": ["Der", "al\u00b7te", "Land\u00b7mann", "wiegt", "sein", "Haupt", "er\u00b7staunt", ":", "Ver\u00b7siegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$.", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Euter auf einmal! Wer sagt, woran das liegt?", "tokens": ["Die", "Eu\u00b7ter", "auf", "ein\u00b7mal", "!", "Wer", "sagt", ",", "wo\u00b7ran", "das", "liegt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "$.", "PWS", "VVFIN", "$,", "PWAV", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Da hebt die Tochter an: Es liegt allein daran,", "tokens": ["Da", "hebt", "die", "Toch\u00b7ter", "an", ":", "Es", "liegt", "al\u00b7lein", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht des F\u00fcrsten Herz dem Land ist zugethan.", "tokens": ["Da\u00df", "nicht", "des", "F\u00fcrs\u00b7ten", "Herz", "dem", "Land", "ist", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Denn wo nicht zugethan der Himmel ist der Erde,", "tokens": ["Denn", "wo", "nicht", "zu\u00b7ge\u00b7than", "der", "Him\u00b7mel", "ist", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKNEG", "ADJD", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alda verschmachten mu\u00df aller Lebend'gen Herde;", "tokens": ["Al\u00b7da", "ver\u00b7schmach\u00b7ten", "mu\u00df", "al\u00b7ler", "Le\u00b7ben\u00b7d'\u00b7gen", "Her\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "PIAT", "NN", "NN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}}, "stanza.18": {"line.1": {"text": "Und also, wo der F\u00fcrst in Liebe nicht dem Land", "tokens": ["Und", "al\u00b7so", ",", "wo", "der", "F\u00fcrst", "in", "Lie\u00b7be", "nicht", "dem", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "NN", "APPR", "NN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist zugethan, das ihm vertraut des Himmels Hand.", "tokens": ["Ist", "zu\u00b7ge\u00b7than", ",", "das", "ihm", "ver\u00b7traut", "des", "Him\u00b7mels", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PRELS", "PPER", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Der Alte sprach: Was bleibt denn \u00fcbrig, als zu wandern", "tokens": ["Der", "Al\u00b7te", "sprach", ":", "Was", "bleibt", "denn", "\u00fcb\u00b7rig", ",", "als", "zu", "wan\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus einem Land, das Gott verlassen hat, zum andern?", "tokens": ["Aus", "ei\u00b7nem", "Land", ",", "das", "Gott", "ver\u00b7las\u00b7sen", "hat", ",", "zum", "an\u00b7dern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVPP", "VAFIN", "$,", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Geh, Hirte, gib dem Vieh hier seine letzte Rast!", "tokens": ["Geh", ",", "Hir\u00b7te", ",", "gib", "dem", "Vieh", "hier", "sei\u00b7ne", "letz\u00b7te", "Rast", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVIMP", "ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und