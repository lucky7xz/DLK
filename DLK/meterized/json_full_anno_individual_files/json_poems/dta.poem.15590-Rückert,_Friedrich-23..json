{"dta.poem.15590": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Froh bin ich, durch zu seyn durch das Gedr\u00e4ng' im Leben,", "tokens": ["Froh", "bin", "ich", ",", "durch", "zu", "seyn", "durch", "das", "Ge\u00b7dr\u00e4ng'", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "APPR", "PTKZU", "VAINF", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und m\u00f6chte nicht hinein mich noch einmal begeben.", "tokens": ["Und", "m\u00f6ch\u00b7te", "nicht", "hin\u00b7ein", "mich", "noch", "ein\u00b7mal", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Noch minder m\u00f6cht' ich, nicht darin gewesen seyn,", "tokens": ["Noch", "min\u00b7der", "m\u00f6cht'", "ich", ",", "nicht", "da\u00b7rin", "ge\u00b7we\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "$,", "PTKNEG", "PAV", "VAPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch einen hindern, der auch einmal will hinein.", "tokens": ["Noch", "ei\u00b7nen", "hin\u00b7dern", ",", "der", "auch", "ein\u00b7mal", "will", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVINF", "$,", "PRELS", "ADV", "ADV", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geh nur hinein, mein Sohn, hilf durch dir, wie du kannst;", "tokens": ["Geh", "nur", "hin\u00b7ein", ",", "mein", "Sohn", ",", "hilf", "durch", "dir", ",", "wie", "du", "kannst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVIMP", "APPR", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn du kommst heraus, la\u00df sehn, was du gewannst.", "tokens": ["Und", "wenn", "du", "kommst", "he\u00b7raus", ",", "la\u00df", "sehn", ",", "was", "du", "ge\u00b7wannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKVZ", "$,", "VVIMP", "VVINF", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}