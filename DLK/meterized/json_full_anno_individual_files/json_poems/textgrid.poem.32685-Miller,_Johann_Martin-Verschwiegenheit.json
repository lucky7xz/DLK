{"textgrid.poem.32685": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Verschwiegenheit", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heimlich nur, doch inniglich", "tokens": ["Heim\u00b7lich", "nur", ",", "doch", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "$,", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieben wir uns beide;", "tokens": ["Lie\u00b7ben", "wir", "uns", "bei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "PRF", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn die Liebe scheuet sich", "tokens": ["Denn", "die", "Lie\u00b7be", "scheu\u00b7et", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weislich vor dem Neide.", "tokens": ["Weis\u00b7lich", "vor", "dem", "Nei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wissen soll die Welt es nie,", "tokens": ["Wis\u00b7sen", "soll", "die", "Welt", "es", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie wir uns verehren;", "tokens": ["Wie", "wir", "uns", "ver\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn in kurzem w\u00fcrde sie", "tokens": ["Denn", "