{"textgrid.poem.48808": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "33. Dreien Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So freundlich, so geneigt, so g\u00fctig an Geberden,", "tokens": ["So", "freund\u00b7lich", ",", "so", "ge\u00b7neigt", ",", "so", "g\u00fc\u00b7tig", "an", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVPP", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so zart, so tugendhaft, so g\u00f6tlich um und an,", "tokens": ["so", "zart", ",", "so", "tu\u00b7gend\u00b7haft", ",", "so", "g\u00f6t\u00b7lich", "um", "und", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als keine G\u00f6ttin nicht gesch\u00e4tzet werden kan,", "tokens": ["als", "kei\u00b7ne", "G\u00f6t\u00b7tin", "nicht", "ge\u00b7sch\u00e4t\u00b7zet", "wer\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKNEG", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so hochbegabt seid ihr, ihr Gratien der Erden,", "tokens": ["so", "hoch\u00b7be\u00b7gabt", "seid", "ihr", ",", "ihr", "Gra\u00b7ti\u00b7en", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "die durch die Himlischen mehr himlisch t\u00e4glich werden,", "tokens": ["die", "durch", "die", "Him\u00b7li\u00b7schen", "mehr", "him\u00b7lisch", "t\u00e4g\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die ihre Schwestern sind. Es glaubt es Iederman,", "tokens": ["die", "ih\u00b7re", "Schwes\u00b7tern", "sind", ".", "Es", "glaubt", "es", "Ie\u00b7der\u00b7man", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df die Vollkommenheit sich ganz in euch vertan", "tokens": ["da\u00df", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "sich", "ganz", "in", "euch", "ver\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und mu\u00df es auch der Neid bekennen ohn' Beschwerden.", "tokens": ["und", "mu\u00df", "es", "auch", "der", "Neid", "be\u00b7ken\u00b7nen", "ohn'", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "von so viel Tugenden macht eine Harmonei", "tokens": ["von", "so", "viel", "Tu\u00b7gen\u00b7den", "macht", "ei\u00b7ne", "Har\u00b7mo\u00b7nei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "mit solcher Treflichkeit in euren dreien Leibern,", "tokens": ["mit", "sol\u00b7cher", "Tref\u00b7lich\u00b7keit", "in", "eu\u00b7ren", "drei\u00b7en", "Lei\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df Orpheus sich befragt bei allen klugen Weibern,", "tokens": ["da\u00df", "Or\u00b7pheus", "sich", "be\u00b7fragt", "bei", "al\u00b7len", "klu\u00b7gen", "Wei\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "VVPP", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ob seiner Harfen Klang in euch verwandelt sei?", "tokens": ["ob", "sei\u00b7ner", "Har\u00b7fen", "Klang", "in", "euch", "ver\u00b7wan\u00b7delt", "sei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So freundlich, so geneigt, so g\u00fctig an Geberden,", "tokens": ["So", "freund\u00b7lich", ",", "so", "ge\u00b7neigt", ",", "so", "g\u00fc\u00b7tig", "an", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVPP", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so zart, so tugendhaft, so g\u00f6tlich um