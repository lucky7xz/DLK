{"dta.poem.8911": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und meine Blicke nur an dich verschwendet!", "tokens": ["Und", "mei\u00b7ne", "Bli\u00b7cke", "nur", "an", "dich", "ver\u00b7schwen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O h\u00e4tt' ich nie ein Wort dir zugewendet,", "tokens": ["O", "h\u00e4tt'", "ich", "nie", "ein", "Wort", "dir", "zu\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00fc\u00dft' ich keinen Kr\u00e4nkungen erliegen!", "tokens": ["So", "m\u00fc\u00dft'", "ich", "kei\u00b7nen", "Kr\u00e4n\u00b7kun\u00b7gen", "er\u00b7lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Doch diese Liebe m\u00f6cht' ich nie besiegen,", "tokens": ["Doch", "die\u00b7se", "Lie\u00b7be", "m\u00f6cht'", "ich", "nie", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weh dem Tag, an dem sie frostig endet!", "tokens": ["Und", "weh", "dem", "Tag", ",", "an", "dem", "sie", "fros\u00b7tig", "en\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie ward aus jenen R\u00e4umen uns gesendet,", "tokens": ["Sie", "ward", "aus", "je\u00b7nen", "R\u00e4u\u00b7men", "uns", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo selig Engel sich an Engel schmiegen.", "tokens": ["Wo", "se\u00b7lig", "En\u00b7gel", "sich", "an", "En\u00b7gel", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "PRF", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "D'rum la\u00df des Wahns mich, da\u00df du liebst, mich freuen,", "tokens": ["D'\u00b7rum", "la\u00df", "des", "Wahns", "mich", ",", "da\u00df", "du", "liebst", ",", "mich", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Damit die Seele nicht mir ganz ver\u00f6de,", "tokens": ["Da\u00b7mit", "die", "See\u00b7le", "nicht", "mir", "ganz", "ver\u00b7\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKNEG", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meinen Glauben m\u00f6ge nichts zerstreuen!", "tokens": ["Und", "mei\u00b7nen", "Glau\u00b7ben", "m\u00f6\u00b7ge", "nichts", "zer\u00b7streu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Gl\u00fcck, verweig're nicht mir allzuschn\u00f6de", "tokens": ["O", "Gl\u00fcck", ",", "ver\u00b7weig'\u00b7re", "nicht", "mir", "all\u00b7zu\u00b7schn\u00f6\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PTKNEG", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Tag, an welchem seinem Vielgetreuen", "tokens": ["Den", "Tag", ",", "an", "wel\u00b7chem", "sei\u00b7nem", "Viel\u00b7ge\u00b7treu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ganze Seele zeigt der sch\u00f6ne Spr\u00f6de!", "tokens": ["Die", "gan\u00b7ze", "See\u00b7le", "zeigt", "der", "sch\u00f6\u00b7ne", "Spr\u00f6\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}