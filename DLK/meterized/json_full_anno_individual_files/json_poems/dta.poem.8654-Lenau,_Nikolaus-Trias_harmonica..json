{"dta.poem.8654": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Trias harmonica.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Seelen hab' ich offenbar&#8218;", "tokens": ["Drei", "See\u00b7len", "hab'", "ich", "of\u00b7fen\u00b7bar", "&#8218;"], "token_info": ["word", "word", "word", "word", "word", "XML_entity"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn eine kann drei Dinge nicht", "tokens": ["Denn", "ei\u00b7ne", "kann", "drei", "Din\u00b7ge", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "CARD", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zugleich vollbringen, wie sogar", "tokens": ["Zu\u00b7gleich", "voll\u00b7brin\u00b7gen", ",", "wie", "so\u00b7gar"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der weise Psychologe spricht.", "tokens": ["Der", "wei\u00b7se", "Psy\u00b7cho\u00b7lo\u00b7ge", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die eine h\u00e4ngt voll Liebesgluth", "tokens": ["Die", "ei\u00b7ne", "h\u00e4ngt", "voll", "Lie\u00b7bes\u00b7gluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An sch\u00f6nen Munds Korallenrand;", "tokens": ["An", "sch\u00f6\u00b7nen", "Munds", "Ko\u00b7ral\u00b7len\u00b7rand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die andre schwimmt auf Weinesfluth", "tokens": ["Die", "and\u00b7re", "schwimmt", "auf", "Wei\u00b7nes\u00b7fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hin\u00fcber an den G\u00f6tterstrand;", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "an", "den", "G\u00f6t\u00b7ter\u00b7strand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die dritt' in freudigem Tumult", "tokens": ["Die", "dritt'", "in", "freu\u00b7di\u00b7gem", "Tu\u00b7mult"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Braust ihre Dithyramben laut,", "tokens": ["Braust", "ih\u00b7re", "Dit\u00b7hy\u00b7ram\u00b7ben", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schleudert ihren Katapult", "tokens": ["Und", "schleu\u00b7dert", "ih\u00b7ren", "Ka\u00b7ta\u00b7pult"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ans kalte Herz, metallverbaut.", "tokens": ["Ans", "kal\u00b7te", "Herz", ",", "me\u00b7tall\u00b7ver\u00b7baut", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So geht's, bis an den Bettelstab", "tokens": ["So", "geht's", ",", "bis", "an", "den", "Bet\u00b7tel\u00b7stab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ihren Wirth, den Leib, gezehrt;", "tokens": ["Sie", "ih\u00b7ren", "Wirth", ",", "den", "Leib", ",", "ge\u00b7zehrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis jubilirend dann hinab", "tokens": ["Bis", "ju\u00b7bi\u00b7li\u00b7rend", "dann", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die tolle Drei zur H\u00f6lle f\u00e4hrt.", "tokens": ["Die", "tol\u00b7le", "Drei", "zur", "H\u00f6l\u00b7le", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}