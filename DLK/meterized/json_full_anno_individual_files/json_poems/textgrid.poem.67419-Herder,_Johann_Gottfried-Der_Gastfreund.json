rd", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "$.", "PPER", "VAFIN", "PPER", "PIS", "VVPP", "$,", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Kein Europ\u00e4er jetzt in meiner H\u00fctte;", "tokens": ["Kein", "Eu\u00b7ro\u00b7p\u00e4\u00b7er", "jetzt", "in", "mei\u00b7ner", "H\u00fct\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mein Gastfreund ist er und ein kranker Mensch.\u00ab", "tokens": ["Mein", "Gast\u00b7freund", "ist", "er", "und", "ein", "kran\u00b7ker", "Mensch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die lauten Haufen trennten murmelnd sich", "tokens": ["Die", "lau\u00b7ten", "Hau\u00b7fen", "trenn\u00b7ten", "mur\u00b7melnd", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und dankten's ihm am Morgen, da\u00df sie gestern,", "tokens": ["Und", "dank\u00b7ten's", "ihm", "am", "Mor\u00b7gen", ",", "da\u00df", "sie", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Von seiner Billigkeit geleitet, sich", "tokens": ["Von", "sei\u00b7ner", "Bil\u00b7lig\u00b7keit", "ge\u00b7lei\u00b7tet", ",", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOS