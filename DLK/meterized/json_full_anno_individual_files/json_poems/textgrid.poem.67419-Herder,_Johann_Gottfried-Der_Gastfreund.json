{"textgrid.poem.67419": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Gastfreund", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei einem Neger in Guinea war", "tokens": ["Bei", "ei\u00b7nem", "Ne\u00b7ger", "in", "Gui\u00b7nea", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Brite krank zur\u00fcckgeblieben; treu,", "tokens": ["Ein", "Bri\u00b7te", "krank", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", ";", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$.", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie seinen Bruder, pflegt' der Neger ihn.", "tokens": ["Wie", "sei\u00b7nen", "Bru\u00b7der", ",", "pflegt'", "der", "Ne\u00b7ger", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da kam ein ander europ\u00e4isch Schiff", "tokens": ["Da", "kam", "ein", "an\u00b7der", "eu\u00b7ro\u00b7p\u00e4\u00b7isch", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und stahl mit T\u00e4nzen, mit Musik und List", "tokens": ["Und", "stahl", "mit", "T\u00e4n\u00b7zen", ",", "mit", "Mu\u00b7sik", "und", "List"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Neger viel' hinweg. Sie jammerten,", "tokens": ["Der", "Ne\u00b7ger", "viel'", "hin\u00b7weg", ".", "Sie", "jam\u00b7mer\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PTKVZ", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Die H\u00e4nde ringend auf dem fliehnden Schiff,", "tokens": ["Die", "H\u00e4n\u00b7de", "rin\u00b7gend", "auf", "dem", "fliehn\u00b7den", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bis sie der dumpfe Boden hart verschlo\u00df.", "tokens": ["Bis", "sie", "der", "dump\u00b7fe", "Bo\u00b7den", "hart", "ver\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Flecken ward voll Aufruhr. V\u00e4ter, M\u00fctter", "tokens": ["Der", "Fle\u00b7cken", "ward", "voll", "Auf\u00b7ruhr", ".", "V\u00e4\u00b7ter", ",", "M\u00fct\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$.", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und Br\u00e4ute, S\u00f6hne, Weiber sammelten", "tokens": ["Und", "Br\u00e4u\u00b7te", ",", "S\u00f6h\u00b7ne", ",", "Wei\u00b7ber", "sam\u00b7mel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sich um die H\u00fctte, wo der Europ\u00e4