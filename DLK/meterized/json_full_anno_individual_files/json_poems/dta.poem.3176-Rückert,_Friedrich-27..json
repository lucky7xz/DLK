{"dta.poem.3176": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht der ist gl\u00fccklich, den ein Ungl\u00fcck nie geschlagen;", "tokens": ["Nicht", "der", "ist", "gl\u00fcck\u00b7lich", ",", "den", "ein", "Un\u00b7gl\u00fcck", "nie", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "VAFIN", "ADJD", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer wei\u00df, wann es ihn trifft, wie er es wird ertragen.", "tokens": ["Wer", "wei\u00df", ",", "wann", "es", "ihn", "trifft", ",", "wie", "er", "es", "wird", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur der ist gl\u00fccklich, der mit Fassung eines trug,", "tokens": ["Nur", "der", "ist", "gl\u00fcck\u00b7lich", ",", "der", "mit", "Fas\u00b7sung", "ei\u00b7nes", "trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VAFIN", "ADJD", "$,", "PRELS", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und noch manch andres ist zu wagen stark genug.", "tokens": ["Und", "noch", "manch", "and\u00b7res", "ist", "zu", "wa\u00b7gen", "stark", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "PIS", "VAFIN", "PTKZU", "VVINF", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn mancher Sturmwind tobt, der unser Schifflein probt,", "tokens": ["Denn", "man\u00b7cher", "Sturm\u00b7wind", "tobt", ",", "der", "un\u00b7ser", "Schif\u00b7flein", "probt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn die Pr\u00fcfung wir bestehn, sei Gott gelobt.", "tokens": ["Und", "wenn", "die", "Pr\u00fc\u00b7fung", "wir", "be\u00b7stehn", ",", "sei", "Gott", "ge\u00b7lobt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVINF", "$,", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}