{"textgrid.poem.61767": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Zur Friedensfeier", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flammt auf von allen Spitzen,", "tokens": ["Flammt", "auf", "von", "al\u00b7len", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Feuer deutscher Lust,", "tokens": ["Ihr", "Feu\u00b7er", "deut\u00b7scher", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weckt mit euren Blitzen", "tokens": ["Und", "weckt", "mit", "eu\u00b7ren", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Danklied jeder Brust!", "tokens": ["Ein", "Dank\u00b7lied", "je\u00b7der", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das grause Spiel der Waffen", "tokens": ["Das", "grau\u00b7se", "Spiel", "der", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Gott ist's abgetan,", "tokens": ["Mit", "Gott", "ist's", "ab\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und, die das Schwert geschaffen,", "tokens": ["Und", ",", "die", "das", "Schwert", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Palmenzeit bricht an.", "tokens": ["Die", "Pal\u00b7men\u00b7zeit", "bricht", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Preis dem Herrn, dem starken Retter,", "tokens": ["Preis", "dem", "Herrn", ",", "dem", "star\u00b7ken", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der nach wunderbarem Rat", "tokens": ["Der", "nach", "wun\u00b7der\u00b7ba\u00b7rem", "Rat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Aus dem Staub uns hob im Wetter", "tokens": ["Aus", "dem", "Staub", "uns", "hob", "im", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und uns heut im S\u00e4useln naht!", "tokens": ["Und", "uns", "heut", "im", "S\u00e4u\u00b7seln", "naht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Nun ward in ", "tokens": ["Nun", "ward", "in"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Was eitel St\u00fcckwerk war,", "tokens": ["Was", "ei\u00b7tel", "St\u00fcck\u00b7werk", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun liegt das Reich umfriedet", "tokens": ["Nun", "liegt", "das", "Reich", "um\u00b7frie\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Arglist und Gefahr.", "tokens": ["Vor", "Arg\u00b7list", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Vom Alpengl\u00fchn zum Meere,", "tokens": ["Vom", "Al\u00b7pen\u00b7gl\u00fchn", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Haff zur Mosel weht", "tokens": ["Vom", "Haff", "zur", "Mo\u00b7sel", "weht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Banner deutscher Ehre", "tokens": ["Das", "Ban\u00b7ner", "deut\u00b7scher", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In junger Majest\u00e4t.", "tokens": ["In", "jun\u00b7ger", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Preis dein Herrn, dem starken Retter,", "tokens": ["Preis", "dein", "Herrn", ",", "dem", "star\u00b7ken", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der nach wunderbarem Rat", "tokens": ["Der", "nach", "wun\u00b7der\u00b7ba\u00b7rem", "Rat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Aus dem Staub uns hob im Wetter", "tokens": ["Aus", "dem", "Staub", "uns", "hob", "im", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und uns heut im S\u00e4useln naht!", "tokens": ["Und", "uns", "heut", "im", "S\u00e4u\u00b7seln", "naht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Wie braust von Stamm zu Stamme", "tokens": ["Wie", "braust", "von", "Stamm", "zu", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Leben reich und stolz,", "tokens": ["Ein", "Le\u00b7ben", "reich", "und", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seit der Begeistrung Flamme,", "tokens": ["Seit", "der", "Be\u00b7geis\u00b7trung", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was starr sich mied, verschmolz,", "tokens": ["Was", "starr", "sich", "mied", ",", "ver\u00b7schmolz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seit am vereinten Werke", "tokens": ["Seit", "am", "ver\u00b7ein\u00b7ten", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des S\u00fcdens Fl\u00fcgelkraft,", "tokens": ["Des", "S\u00fc\u00b7dens", "Fl\u00fc\u00b7gel\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Nordens klare St\u00e4rke", "tokens": ["Des", "Nor\u00b7dens", "kla\u00b7re", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wetteifernd ringt und schafft!", "tokens": ["Wett\u00b7ei\u00b7fernd", "ringt", "und", "schafft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Preis dem Herrn, dem starken Retter,", "tokens": ["Preis", "dem", "Herrn", ",", "dem", "star\u00b7ken", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der nach wunderbarem Rat", "tokens": ["Der", "nach", "wun\u00b7der\u00b7ba\u00b7rem", "Rat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Aus dem Staub uns hob im Wetter", "tokens": ["Aus", "dem", "Staub", "uns", "hob", "im", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und uns heut im S\u00e4useln naht!", "tokens": ["Und", "uns", "heut", "im", "S\u00e4u\u00b7seln", "naht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Der in der Feuerwolke", "tokens": ["Der", "in", "der", "Feu\u00b7er\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voran uns zog im Krieg,", "tokens": ["Vo\u00b7ran", "uns", "zog", "im", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun send' er unsrem Volke", "tokens": ["Nun", "send'", "er", "uns\u00b7rem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Kraft zum letzten Sieg,", "tokens": ["Die", "Kraft", "zum", "letz\u00b7ten", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Kraft, auch aus den Herzen", "tokens": ["Die", "Kraft", ",", "auch", "aus", "den", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der L\u00fcge finstre Saat,", "tokens": ["Der", "L\u00fc\u00b7ge", "finst\u00b7re", "Saat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Welschtum auszumerzen", "tokens": ["Das", "Welschtum", "aus\u00b7zu\u00b7mer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "In Glauben, Wort und Tat.", "tokens": ["In", "Glau\u00b7ben", ",", "Wort", "und", "Tat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Preis dem Herrn, dem starken Retter,", "tokens": ["Preis", "dem", "Herrn", ",", "dem", "star\u00b7ken", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der nach wunderbarem Rat", "tokens": ["Der", "nach", "wun\u00b7der\u00b7ba\u00b7rem", "Rat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Aus dem Staub uns hob im Wetter", "tokens": ["Aus", "dem", "Staub", "uns", "hob", "im", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und uns heut im S\u00e4useln naht!", "tokens": ["Und", "uns", "heut", "im", "S\u00e4u\u00b7seln", "naht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Zieh ein zu allen Toren,", "tokens": ["Zieh", "ein", "zu", "al\u00b7len", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "me