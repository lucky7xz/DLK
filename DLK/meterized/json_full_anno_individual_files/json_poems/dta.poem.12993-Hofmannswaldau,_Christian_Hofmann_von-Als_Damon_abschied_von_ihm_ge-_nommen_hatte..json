{"dta.poem.12993": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Damon abschied von ihm ge-  \n nommen hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist mein vergn\u00fcgen", "tokens": ["Nun", "ist", "mein", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit meinem Damon hin!", "tokens": ["Mit", "mei\u00b7nem", "Da\u00b7mon", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn bey thr\u00e4nen-kr\u00fcgen", "tokens": ["Denn", "bey", "thr\u00e4\u00b7nen\u00b7kr\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Erqvicket sich kein sinn,", "tokens": ["E\u00b7rqvic\u00b7ket", "sich", "kein", "sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ein von uns entfernter freund", "tokens": ["Ein", "von", "uns", "ent\u00b7fern\u00b7ter", "freund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird billich stets beweint.", "tokens": ["Wird", "bil\u00b7lich", "stets", "be\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Alles scheinet tr\u00fcbe,", "tokens": ["Al\u00b7les", "schei\u00b7net", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo uns das licht gebricht:", "tokens": ["Wo", "uns", "das", "licht", "ge\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und entfernte liebe", "tokens": ["Und", "ent\u00b7fern\u00b7te", "lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Vergn\u00fcgt die freunde nicht.", "tokens": ["Ver\u00b7gn\u00fcgt", "die", "freun\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Thyrsis sitzt nun gantz allein,", "tokens": ["Thyr\u00b7sis", "sitzt", "nun", "gantz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie kan er fr\u00f6lich seyn?", "tokens": ["Wie", "kan", "er", "fr\u00f6\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schmeckt auch eine pfeiffe,", "tokens": ["Schmeckt", "auch", "ei\u00b7ne", "pfeif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn man sie einsam raucht?", "tokens": ["Wenn", "man", "sie", "ein\u00b7sam", "raucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich sie ergreiffe,", "tokens": ["Wenn", "ich", "sie", "er\u00b7greif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So wird ja wol geschmaucht;", "tokens": ["So", "wird", "ja", "wol", "ge\u00b7schmaucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch sie ist kaum angebrannt,", "tokens": ["Doch", "sie", "ist", "kaum", "an\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So f\u00e4llt sie aus der hand.", "tokens": ["So", "f\u00e4llt", "sie", "aus", "der", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Holde morgen-stunden!", "tokens": ["Hol\u00b7de", "mor\u00b7gen\u00b7stun\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jhr seyd mir lauter nacht.", "tokens": ["Ihr", "seyd", "mir", "lau\u00b7ter", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damon ist verschwunden,", "tokens": ["Da\u00b7mon", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der euch beliebt gemacht;", "tokens": ["Der", "euch", "be\u00b7liebt", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Damon, der so niedlich fang,", "tokens": ["Da\u00b7mon", ",", "der", "so", "nied\u00b7lich", "fang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn meine zitter klang.", "tokens": ["Wenn", "mei\u00b7ne", "zit\u00b7ter", "klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jhr gew\u00fcnschten n\u00e4chte,", "tokens": ["Ihr", "ge\u00b7w\u00fcnschten", "n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Seyd wie der rauch zerstreut.", "tokens": ["Seyd", "wie", "der", "rauch", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer euch wiederbr\u00e4chte,", "tokens": ["Wer", "euch", "wie\u00b7der\u00b7br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "D\u00e4mpfft\u2019 alle traurigkeit.", "tokens": ["D\u00e4mpfft'", "al\u00b7le", "trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aber dieser hoffnung kind", "tokens": ["A\u00b7ber", "die\u00b7ser", "hoff\u00b7nung", "kind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist lauter dunst und wind.", "tokens": ["Ist", "lau\u00b7ter", "dunst", "und", "wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df doch freunde scheiden,", "tokens": ["Da\u00df", "doch", "freun\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die sich so wohl begehn!", "tokens": ["Die", "sich", "so", "wohl", "be\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darff kein schlo\u00df der freuden", "tokens": ["Darff", "kein", "schlo\u00df", "der", "freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auf festem grunde stehn?", "tokens": ["Auf", "fes\u00b7tem", "grun\u00b7de", "stehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nein! dieweil des himmels rath", "tokens": ["Nein", "!", "die\u00b7weil", "des", "him\u00b7mels", "rath"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Es so beschlossen hat.", "tokens": ["Es", "so", "be\u00b7schlos\u00b7sen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Damons angedencken", "tokens": ["Da\u00b7mons", "an\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word"], "pos": ["NE", "VVINF"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.2": {"text": "Soll noch mein leben seyn.", "tokens": ["Soll", "noch", "mein", "le\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich alles kr\u00e4ncken;", "tokens": ["La\u00df", "mich", "al\u00b7les", "kr\u00e4n\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "F\u00e4llt mir nur Damon ein,", "tokens": ["F\u00e4llt", "mir", "nur", "Da\u00b7mon", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NE", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ist doch noch in der welt", "tokens": ["So", "ist", "doch", "noch", "in", "der", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Ein trost, der mir gef\u00e4llt.", "tokens": ["Ein", "trost", ",", "der", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}