{"textgrid.poem.46937": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Gott! Wenn euch nicht ganz die Sinne blenden,", "tokens": ["Bei", "Gott", "!", "Wenn", "euch", "nicht", "ganz", "die", "Sin\u00b7ne", "blen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Mord und Gier das Aug' euch ganz umfloren;", "tokens": ["Nicht", "Mord", "und", "Gier", "das", "Aug'", "euch", "ganz", "um\u00b7flo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So thut es auf, seht, wo ihr steht, ihr Thoren,", "tokens": ["So", "thut", "es", "auf", ",", "seht", ",", "wo", "ihr", "steht", ",", "ihr", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wendet euch, weil's noch ist Zeit zu wenden.", "tokens": ["Und", "wen\u00b7det", "euch", ",", "weil's", "noch", "ist", "Zeit", "zu", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ADV", "VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Nach wem wollt ihr die gift'gen Pfeile senden?", "tokens": ["Nach", "wem", "wollt", "ihr", "die", "gift'\u00b7gen", "Pfei\u00b7le", "sen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wen wollt ihr mit dem blut'gen Schwert durchbohren?", "tokens": ["Wen", "wollt", "ihr", "mit", "dem", "blut'\u00b7gen", "Schwert", "durch\u00b7boh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Uns! Welche Mutter hat denn uns geboren,", "tokens": ["Uns", "!", "Wel\u00b7che", "Mut\u00b7ter", "hat", "denn", "uns", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PWAT", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und welche trug denn euch in ihren Lenden?", "tokens": ["Und", "wel\u00b7che", "trug", "denn", "euch", "in", "ih\u00b7ren", "Len\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht Eine? Wollt ihr Bruderblut verspritzen?", "tokens": ["Nicht", "Ei\u00b7ne", "?", "Wollt", "ihr", "Bru\u00b7der\u00b7blut", "ver\u00b7sprit\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "$.", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10"