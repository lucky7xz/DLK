{"dta.poem.19562": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Weihnacht in Ajaccio.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Reife Goldorangen fallen sahn wir heute, Myrte bl\u00fchte,", "tokens": ["Rei\u00b7fe", "Gol\u00b7do\u00b7ran\u00b7gen", "fal\u00b7len", "sahn", "wir", "heu\u00b7te", ",", "Myr\u00b7te", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "PPER", "ADV", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eidechs glitt entlang der Mauer, die von Sonne gl\u00fchte.", "tokens": ["Ei\u00b7dechs", "glitt", "ent\u00b7lang", "der", "Mau\u00b7er", ",", "die", "von", "Son\u00b7ne", "gl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Uns zu H\u00e4upten neben einem morschen Laube flog ein Falter \u2014", "tokens": ["Uns", "zu", "H\u00e4up\u00b7ten", "ne\u00b7ben", "ei\u00b7nem", "mor\u00b7schen", "Lau\u00b7be", "flog", "ein", "Fal\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Keine herbe Grenze scheidet Jugend hier und Alter.", "tokens": ["Kei\u00b7ne", "her\u00b7be", "Gren\u00b7ze", "schei\u00b7det", "Ju\u00b7gend", "hier", "und", "Al\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "Eh' das welke Blatt verweht ist, wird die Knospe neu geboren \u2014", "tokens": ["Eh'", "das", "wel\u00b7ke", "Blatt", "ver\u00b7weht", "ist", ",", "wird", "die", "Knos\u00b7pe", "neu", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$,", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eine liebliche Verwirrung, schwebt der Zug der Horen.", "tokens": ["Ei\u00b7ne", "lieb\u00b7li\u00b7che", "Ver\u00b7wir\u00b7rung", ",", "schwebt", "der", "Zug", "der", "Ho\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Sprich, was tr\u00e4umen deine Blicke? Fehlt ein Winter dir, ein", "tokens": ["Sprich", ",", "was", "tr\u00e4u\u00b7men", "dei\u00b7ne", "Bli\u00b7cke", "?", "Fehlt", "ein", "Win\u00b7ter", "dir", ",", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "ART", "NN", "PPER", "$,", "ART"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "bleicher?", "tokens": ["blei\u00b7cher", "?"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Theures Weib, du bist um einen lichten Fr\u00fchling reicher!", "tokens": ["Theu\u00b7res", "Weib", ",", "du", "bist", "um", "ei\u00b7nen", "lich\u00b7ten", "Fr\u00fch\u00b7ling", "rei\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Liebst du doch die langen Sonnen und die Kraft und Gluth", "tokens": ["Liebst", "du", "doch", "die", "lan\u00b7gen", "Son\u00b7nen", "und", "die", "Kraft", "und", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "KON", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "der Farben!", "tokens": ["der", "Far\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und du sehnst dich nach der Heimath, wo sie l\u00e4ngst erstarben?", "tokens": ["Und", "du", "sehnst", "dich", "nach", "der", "Hei\u00b7math", ",", "wo", "sie", "l\u00e4ngst", "er\u00b7star\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.6": {"line.1": {"text": "Horch! Durch paradieseswarme L\u00fcfte t\u00f6nen Weihnachtsglocken!", "tokens": ["Horch", "!", "Durch", "pa\u00b7ra\u00b7die\u00b7ses\u00b7war\u00b7me", "L\u00fcf\u00b7te", "t\u00f6\u00b7nen", "Weih\u00b7nachts\u00b7glo\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sprich, was tr\u00e4umen deine Blicke? Von den wei\u00dfen Flocken?", "tokens": ["Sprich", ",", "was", "tr\u00e4u\u00b7men", "dei\u00b7ne", "Bli\u00b7cke", "?", "Von", "den", "wei\u00b7\u00dfen", "Flo\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}