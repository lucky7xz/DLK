enta"}, "line.9": {"text": "Dich einen Traum des Lebens tr\u00e4umen lassen,", "tokens": ["Dich", "ei\u00b7nen", "Traum", "des", "Le\u00b7bens", "tr\u00e4u\u00b7men", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Drin sich verkn\u00fcpft verzeihend Welterfassen", "tokens": ["Drin", "sich", "ver\u00b7kn\u00fcpft", "ver\u00b7zei\u00b7hend", "Wel\u00b7ter\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVPP", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und keusches, lichtverkl\u00e4rtes Daseinsfreuen ...", "tokens": ["Und", "keu\u00b7sches", ",", "licht\u00b7ver\u00b7kl\u00e4r\u00b7tes", "Da\u00b7se\u00b7ins\u00b7freu\u00b7en", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Dir m\u00f6gen g\u00fct'ge G\u00f6tter Wolken breiten, \u2013", "tokens": ["Dir", "m\u00f6\u00b7gen", "g\u00fct'\u00b7ge", "G\u00f6t\u00b7ter", "Wol\u00b7ken", "brei\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "NN", "ADJA", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "An goldenem Gespinst dich heimgeleiten ...", "tokens": ["An", "gol\u00b7de\u00b7nem", "Ge\u00b7spinst", "dich", "heim\u00b7ge\u00b7lei\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}