{"dta.poem.20946": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n Sc\u00e6vol\u00e6.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Jrrthum und Verlust macht ", "tokens": ["Der", "Jrr\u00b7thum", "und", "Ver\u00b7lust", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "kand/", "tokens": ["kand", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Rom konte nicht so viel/ als die verbrandte Hand.", "tokens": ["Rom", "kon\u00b7te", "nicht", "so", "viel", "/", "als", "die", "ver\u00b7brand\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PTKNEG", "ADV", "ADV", "$(", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Asche liegt alhier/ vermischet mit der Erden/", "tokens": ["Die", "A\u00b7sche", "liegt", "al\u00b7hier", "/", "ver\u00b7mi\u00b7schet", "mit", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch wil aus dieser nicht ein ander ", "tokens": ["Noch", "wil", "aus", "die\u00b7ser", "nicht", "ein", "an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PDS", "PTKNEG", "ART", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}