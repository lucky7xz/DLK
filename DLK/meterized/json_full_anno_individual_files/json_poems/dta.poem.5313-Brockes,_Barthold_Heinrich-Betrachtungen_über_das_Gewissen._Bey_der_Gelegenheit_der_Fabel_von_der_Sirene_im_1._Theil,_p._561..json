{"dta.poem.5313": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gewissen.  \n Bey der Gelegenheit der Fabel von der Sirene  \n im 1. Theil,  p.  561.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da es mehr nun als zu wahr, da\u00df die Dinge die-\nser Welt,", "tokens": ["Da", "es", "mehr", "nun", "als", "zu", "wahr", ",", "da\u00df", "die", "Din\u00b7ge", "die", "ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "KOUS", "PTKA", "ADJD", "$,", "KOUS", "ART", "NN", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie gesagt, zwo Seiten haben, und da\u00df wir sie mei-", "tokens": ["Wie", "ge\u00b7sagt", ",", "zwo", "Sei\u00b7ten", "ha\u00b7ben", ",", "und", "da\u00df", "wir", "sie", "mei"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "CARD", "NN", "VAFIN", "$,", "KON", "KOUS", "PPER", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Nach dem Zustand unsers Wesens, da die Liebe pl\u00f6tz-", "tokens": ["Nach", "dem", "Zu\u00b7stand", "un\u00b7sers", "We\u00b7sens", ",", "da", "die", "Lie\u00b7be", "pl\u00f6tz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Und uns die genossne Sch\u00f6nheit wiedrig deucht, und", "tokens": ["Und", "uns", "die", "ge\u00b7noss\u00b7ne", "Sch\u00f6n\u00b7heit", "wied\u00b7rig", "deucht", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,", "KON"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Minder, durch des Vorwurfs Schuld, welcher ja der-", "tokens": ["Min\u00b7der", ",", "durch", "des", "Vor\u00b7wurfs", "Schuld", ",", "wel\u00b7cher", "ja", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$,", "PRELS", "ADV", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Als durch Abnahm\u2019 unsers Feuers, das vorher in Adern", "tokens": ["Als", "durch", "Ab\u00b7nahm'", "un\u00b7sers", "Feu\u00b7ers", ",", "das", "vor\u00b7her", "in", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ist es eine grosse Frag\u2019, ob, was uns zur Reue treibet,", "tokens": ["Ist", "es", "ei\u00b7ne", "gros\u00b7se", "Frag'", ",", "ob", ",", "was", "uns", "zur", "Reu\u00b7e", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Und was insgemein Gewissen von dem Menschen wird", "tokens": ["Und", "was", "ins\u00b7ge\u00b7mein", "Ge\u00b7wis\u00b7sen", "von", "dem", "Men\u00b7schen", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "NN", "APPR", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Dieses nicht zur Ursach habe? folglich ob\u2019s so f\u00fcrchter-", "tokens": ["Die\u00b7ses", "nicht", "zur", "Ur\u00b7sach", "ha\u00b7be", "?", "folg\u00b7lich", "ob's", "so", "f\u00fcrch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "APPRART", "NN", "VAFIN", "$.", "ADV", "KOUS", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Als man es sonst glaubet, sey? ob mans nicht mit Un-", "tokens": ["Als", "man", "es", "sonst", "glau\u00b7bet", ",", "sey", "?", "ob", "mans", "nicht", "mit", "Un"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "$.", "KOUS", "PIS", "PTKNEG", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So verdammend vorgestellt? ob vielmehr nicht eigent-", "tokens": ["So", "ver\u00b7dam\u00b7mend", "vor\u00b7ge\u00b7stellt", "?", "ob", "viel\u00b7mehr", "nicht", "ei\u00b7gent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$.", "KOUS", "ADV", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Durch des C\u00f6rpers Aenderung, oder durch empfundnes", "tokens": ["Durch", "des", "C\u00f6r\u00b7pers", "A\u00b7en\u00b7de\u00b7rung", ",", "o\u00b7der", "durch", "emp\u00b7fund\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "APPR", "ADJA"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Ueber die gehofte Lust, die Gedancken uns verwirren,", "tokens": ["Ue\u00b7ber", "die", "ge\u00b7hof\u00b7te", "Lust", ",", "die", "Ge\u00b7dan\u00b7cken", "uns", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Und wir durch ein eingebildet, irrig so genannt Gewis-", "tokens": ["Und", "wir", "durch", "ein", "ein\u00b7ge\u00b7bil\u00b7det", ",", "ir\u00b7rig", "so", "ge\u00b7nannt", "Ge\u00b7wis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "VVPP", "$,", "ADJD", "ADV", "VVPP", "TRUNC"], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.15": {"text": "Uns nicht, mehr als n\u00f6htig w\u00e4re, f\u00fcrchten und uns", "tokens": ["Uns", "nicht", ",", "mehr", "als", "n\u00f6h\u00b7tig", "w\u00e4\u00b7re", ",", "f\u00fcrch\u00b7ten", "und", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "$,", "PIAT", "KOKOM", "ADJD", "VAFIN", "$,", "VVFIN", "KON", "PPER"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Eben unsers C\u00f6rpers Zustand, da sich nemlich in der", "tokens": ["E\u00b7ben", "un\u00b7sers", "C\u00f6r\u00b7pers", "Zu\u00b7stand", ",", "da", "sich", "nem\u00b7lich", "in", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$,", "KOUS", "PRF", "ADV", "APPR", "ART"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nach genossner Lust, so bald alle vormahls heisse Triebe", "tokens": ["Nach", "ge\u00b7noss\u00b7ner", "Lust", ",", "so", "bald", "al\u00b7le", "vor\u00b7mahls", "heis\u00b7se", "Trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADV", "PIS", "ADV", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "In dem Augenblick ver\u00e4ndern, da ein Eckel schnell ent-", "tokens": ["In", "dem", "Au\u00b7gen\u00b7blick", "ver\u00b7\u00e4n\u00b7dern", ",", "da", "ein", "E\u00b7ckel", "schnell", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "$,", "KOUS", "ART", "NN", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Und, im schnell-verbrannten Feuer, alle Lust nicht nur", "tokens": ["Und", ",", "im", "schnell\u00b7ver\u00b7brann\u00b7ten", "Feu\u00b7er", ",", "al\u00b7le", "Lust", "nicht", "nur"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPRART", "ADJA", "NN", "$,", "PIAT", "NN", "PTKNEG", "ADV"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Sondern, wie uns in der Bibel Ammons Beyspiel", "tokens": ["Son\u00b7dern", ",", "wie", "uns", "in", "der", "Bi\u00b7bel", "Am\u00b7mons", "Bey\u00b7spiel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "NE", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Sich in bittern Wiederwillen, ja in Ha\u00df und Reu ver-", "tokens": ["Sich", "in", "bit\u00b7tern", "Wie\u00b7der\u00b7wil\u00b7len", ",", "ja", "in", "Ha\u00df", "und", "Reu", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "KON", "NE", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Dieses, sag ich dir, entstehet warlich nicht von unge-", "tokens": ["Die\u00b7ses", ",", "sag", "ich", "dir", ",", "ent\u00b7ste\u00b7het", "war\u00b7lich", "nicht", "von", "un\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "ADV", "PTKNEG", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Stammt aus keinem blinden Zufall, sondern einer", "tokens": ["Stammt", "aus", "kei\u00b7nem", "blin\u00b7den", "Zu\u00b7fall", ",", "son\u00b7dern", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,", "KON", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Die nicht gnugsahm zu bewundern. W\u00e4r\u2019 es anders;", "tokens": ["Die", "nicht", "gnug\u00b7sahm", "zu", "be\u00b7wun\u00b7dern", ".", "W\u00e4r'", "es", "an\u00b7ders", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$.", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Sich gewi\u00df von allem Feuer, das man nicht entbehren", "tokens": ["Sich", "ge\u00b7wi\u00df", "von", "al\u00b7lem", "Feu\u00b7er", ",", "das", "man", "nicht", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "PIS", "NN", "$,", "PRELS", "PIS", "PTKNEG", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Sonder Zweifel, gantz ersch\u00f6pfen. Schaut, wie man", "tokens": ["Son\u00b7der", "Zwei\u00b7fel", ",", "gantz", "er\u00b7sch\u00f6p\u00b7fen", ".", "Schaut", ",", "wie", "man"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "VVPP", "$.", "VVFIN", "$,", "PWAV", "PIS"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Da\u00df nicht minder das Gewissen, als das G\u00f6ttliche Ver-", "tokens": ["Da\u00df", "nicht", "min\u00b7der", "das", "Ge\u00b7wis\u00b7sen", ",", "als", "das", "G\u00f6tt\u00b7li\u00b7che", "Ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "In der Wollust auszuschweiffen, sey in der Natur ge-", "tokens": ["In", "der", "Wol\u00b7lust", "aus\u00b7zu\u00b7schwei\u00b7ffen", ",", "sey", "in", "der", "Na\u00b7tur", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "VAFIN", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "Folglich nicht zu \u00fcbertreten, ja da\u00df beides sich so gar", "tokens": ["Folg\u00b7lich", "nicht", "zu", "\u00fc\u00b7bert\u00b7re\u00b7ten", ",", "ja", "da\u00df", "bei\u00b7des", "sich", "so", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "PTKZU", "VVINF", "$,", "ADV", "KOUS", "PIS", "PRF", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Selbst mit unserer Erhaltung, Wolseyn und Gesund-", "tokens": ["Selbst", "mit", "un\u00b7se\u00b7rer", "Er\u00b7hal\u00b7tung", ",", "Wol\u00b7seyn", "und", "Ge\u00b7sun\u00b7d"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "Welches wenn man es erweget unbegreiflich, wunder-", "tokens": ["Wel\u00b7ches", "wenn", "man", "es", "er\u00b7we\u00b7get", "un\u00b7be\u00b7greif\u00b7lich", ",", "wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "KOUS", "PIS", "PPER", "VVFIN", "ADJD", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}