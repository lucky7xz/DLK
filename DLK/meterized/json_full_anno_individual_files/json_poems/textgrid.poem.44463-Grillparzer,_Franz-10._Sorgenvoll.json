{"textgrid.poem.44463": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "10. Sorgenvoll", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kummer ist mein Eigentum,", "tokens": ["Mein", "Kum\u00b7mer", "ist", "mein", "Ei\u00b7gen\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den geb ich nicht heraus.", "tokens": ["Den", "geb", "ich", "nicht", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was gut wohl sonst an mir und schlimm,", "tokens": ["Was", "gut", "wohl", "sonst", "an", "mir", "und", "schlimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "ADV", "APPR", "PPER", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besitzt und teilt! Das hab und nimm!", "tokens": ["Be\u00b7sitzt", "und", "teilt", "!", "Das", "hab", "und", "nimm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$.", "PDS", "VAFIN", "KON", "VVIMP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit ihm nur halt ich Haus.", "tokens": ["Mit", "ihm", "nur", "halt", "ich", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wie der Geizge seinen Schatz", "tokens": ["Und", "wie", "der", "Geiz\u00b7ge", "sei\u00b7nen", "Schatz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Nachts besieht bei Licht,", "tokens": ["Des", "Nachts", "be\u00b7sieht", "bei", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So z\u00e4hl ich ihn, wenn alles Ruh,", "tokens": ["So", "z\u00e4hl", "ich", "ihn", ",", "wenn", "al\u00b7les", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entsprungne K\u00f6rner leg ich zu", "tokens": ["Ent\u00b7sprung\u00b7ne", "K\u00f6r\u00b7ner", "leg", "ich", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NE", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lausch und atme nicht.", "tokens": ["Und", "lausch", "und", "at\u00b7me", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und kommts zu sterben, leg ich ihn", "tokens": ["Und", "kommts", "zu", "ster\u00b7ben", ",", "leg", "ich", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "$,", "NE", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Obol in den Mund,", "tokens": ["Als", "O\u00b7bol", "in", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.