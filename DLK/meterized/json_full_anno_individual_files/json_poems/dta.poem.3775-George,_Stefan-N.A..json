{"dta.poem.3775": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Um alle giebel erst die sonne wallt", "tokens": ["Um", "al\u00b7le", "gie\u00b7bel", "erst", "die", "son\u00b7ne", "wallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00fchlung noch in h\u00f6fen von basalt", "tokens": ["Und", "k\u00fch\u00b7lung", "noch", "in", "h\u00f6\u00b7fen", "von", "ba\u00b7salt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann warten auf den kaiser seine tauben", "tokens": ["Dann", "war\u00b7ten", "auf", "den", "kai\u00b7ser", "sei\u00b7ne", "tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er tr\u00e4gt ein kleid aus blauer Serer-seide", "tokens": ["Er", "tr\u00e4gt", "ein", "kleid", "aus", "blau\u00b7er", "Se\u00b7rer\u00b7sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit sardern und saffiren \u00fcbers\u00e4t", "tokens": ["Mit", "sar\u00b7dern", "und", "saf\u00b7fi\u00b7ren", "\u00fc\u00b7bers\u00b7\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In silberh\u00fclsen s\u00e4umend aufgen\u00e4ht", "tokens": ["In", "sil\u00b7ber\u00b7h\u00fcl\u00b7sen", "s\u00e4u\u00b7mend", "auf\u00b7ge\u00b7n\u00e4ht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch an den armen hat er kein geschmeide", "tokens": ["Doch", "an", "den", "ar\u00b7men", "hat", "er", "kein", "ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er l\u00e4chelte \u00b7 sein weisser finger schenkte", "tokens": ["Er", "l\u00e4\u00b7chel\u00b7te", "sein", "weis\u00b7ser", "fin\u00b7ger", "schenk\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "XY", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die hirsek\u00f6rner aus dem goldnen trog", "tokens": ["Die", "hir\u00b7se\u00b7k\u00f6r\u00b7ner", "aus", "dem", "gold\u00b7nen", "trog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als leis ein Lyder aus den s\u00e4ulen bog", "tokens": ["Als", "leis", "ein", "Ly\u00b7der", "aus", "den", "s\u00e4u\u00b7len", "bog"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und an des herren fuss die stirne senkte", "tokens": ["Und", "an", "des", "her\u00b7ren", "fuss", "die", "stir\u00b7ne", "senk\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die tauben flattern \u00e4ngstig nach dem dache", "tokens": ["Die", "tau\u00b7ben", "flat\u00b7tern", "\u00e4ngs\u00b7tig", "nach", "dem", "da\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVINF", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2014 Ich sterbe gern weil mein gebieter schrak \u2014", "tokens": ["Ich", "ster\u00b7be", "gern", "weil", "mein", "ge\u00b7bie\u00b7ter", "schrak"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ein breiter dolch ihm schon im busen stak", "tokens": ["Ein", "brei\u00b7ter", "dolch", "ihm", "schon", "im", "bu\u00b7sen", "stak"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit gr\u00fcnem flure spielt die rote lache", "tokens": ["Mit", "gr\u00fc\u00b7nem", "flu\u00b7re", "spielt", "die", "ro\u00b7te", "la\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der kaiser wich mit h\u00f6hnender geberde", "tokens": ["Der", "kai\u00b7ser", "wich", "mit", "h\u00f6h\u00b7nen\u00b7der", "ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Worauf er doch am selben tag befahl", "tokens": ["Wo\u00b7rauf", "er", "doch", "am", "sel\u00b7ben", "tag", "be\u00b7fahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dass in den abendlichen weinpokal", "tokens": ["Dass", "in", "den", "a\u00b7bend\u00b7li\u00b7chen", "wein\u00b7po\u00b7kal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des knechtes name eingegraben werde", "tokens": ["Des", "knech\u00b7tes", "na\u00b7me", "ein\u00b7ge\u00b7gra\u00b7ben", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}