{"dta.poem.8036": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Romanze vom kleinen D\u00e4umling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Kleiner D\u00e4umling! kleiner D\u00e4umling!", "tokens": ["Klei\u00b7ner", "D\u00e4um\u00b7ling", "!", "klei\u00b7ner", "D\u00e4um\u00b7ling", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allw\u00e4rts ist dein Ruhm posaunet.", "tokens": ["All\u00b7w\u00e4rts", "ist", "dein", "Ruhm", "po\u00b7sau\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon die Kindlein in der Wiege", "tokens": ["Schon", "die", "Kin\u00b7dlein", "in", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht man der Geschichte staunen.", "tokens": ["Sieht", "man", "der", "Ge\u00b7schich\u00b7te", "stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welches Auge mu\u00df nicht weinen,", "tokens": ["Wel\u00b7ches", "Au\u00b7ge", "mu\u00df", "nicht", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie du liefst durch Waldes Grausen,", "tokens": ["Wie", "du", "liefst", "durch", "Wal\u00b7des", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als die W\u00f6lfe hungrig heulten", "tokens": ["Als", "die", "W\u00f6l\u00b7fe", "hung\u00b7rig", "heul\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Nachtorkane sausten!", "tokens": ["Und", "die", "Nach\u00b7tor\u00b7ka\u00b7ne", "saus\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welches Herz mu\u00df nicht erzittern,", "tokens": ["Wel\u00b7ches", "Herz", "mu\u00df", "nicht", "er\u00b7zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie du lagst im Riesenhause", "tokens": ["Wie", "du", "lagst", "im", "Rie\u00b7sen\u00b7hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und den Oger h\u00f6rtest nahen,", "tokens": ["Und", "den", "O\u00b7ger", "h\u00f6r\u00b7test", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Der nach deinem Fleisch geschnaubet!", "tokens": ["Der", "nach", "dei\u00b7nem", "Fleisch", "ge\u00b7schnau\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dich und deine sechs Gebr\u00fcder", "tokens": ["Dich", "und", "dei\u00b7ne", "sechs", "Ge\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Hast vom Tode du erkaufet,", "tokens": ["Hast", "vom", "To\u00b7de", "du", "er\u00b7kau\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Listiglich die sieben Kappen", "tokens": ["Lis\u00b7tig\u00b7lich", "die", "sie\u00b7ben", "Kap\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Mit den sieben Kronen tauschend.", "tokens": ["Mit", "den", "sie\u00b7ben", "Kro\u00b7nen", "tau\u00b7schend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Als der Riese lag am Felsen,", "tokens": ["Als", "der", "Rie\u00b7se", "lag", "am", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Schnarchend, da\u00df die W\u00e4lder rauschten,", "tokens": ["Schnar\u00b7chend", ",", "da\u00df", "die", "W\u00e4l\u00b7der", "rauschten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Hast du keck die Meilenstiefel", "tokens": ["Hast", "du", "keck", "die", "Mei\u00b7len\u00b7stie\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Von den F\u00fc\u00dfen ihm gemauset.", "tokens": ["Von", "den", "F\u00fc\u00b7\u00dfen", "ihm", "ge\u00b7mau\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Einem vielbedr\u00e4ngten K\u00f6nig", "tokens": ["Ei\u00b7nem", "viel\u00b7be\u00b7dr\u00e4ng\u00b7ten", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Bist als Bote du gelaufen;", "tokens": ["Bist", "als", "Bo\u00b7te", "du", "ge\u00b7lau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "K\u00f6stlich war dein Botenbrot:", "tokens": ["K\u00f6st\u00b7lich", "war", "dein", "Bo\u00b7ten\u00b7brot", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Eine Braut vom K\u00f6nigshause.", "tokens": ["Ei\u00b7ne", "Braut", "vom", "K\u00f6\u00b7nigs\u00b7hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Kleiner D\u00e4umling! kleiner D\u00e4umling!", "tokens": ["Klei\u00b7ner", "D\u00e4um\u00b7ling", "!", "klei\u00b7ner", "D\u00e4um\u00b7ling", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "M\u00e4chtig ist dein Ruhm erbrauset,", "tokens": ["M\u00e4ch\u00b7tig", "ist", "dein", "Ruhm", "er\u00b7brau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Mit den Siebenmeilenstiefeln", "tokens": ["Mit", "den", "Sie\u00b7ben\u00b7mei\u00b7len\u00b7stie\u00b7feln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Schritt er schon durch manch Jahrtausend.", "tokens": ["Schritt", "er", "schon", "durch", "manch", "Jahr\u00b7tau\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}