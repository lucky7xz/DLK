{"textgrid.poem.63881": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nicht zu ehrlich mit der Welt! Die Welt will ja betrogen sein.", "tokens": ["Sei", "nicht", "zu", "ehr\u00b7lich", "mit", "der", "Welt", "!", "Die", "Welt", "will", "ja", "be\u00b7tro\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKA", "ADJD", "APPR", "ART", "NN", "$.", "ART", "NN", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Nur wer sie dreist zum Narren h\u00e4lt, dem wird sie wohlgewogen sein.", "tokens": ["Nur", "wer", "sie", "dreist", "zum", "Nar\u00b7ren", "h\u00e4lt", ",", "dem", "wird", "sie", "wohl\u00b7ge\u00b7wo\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Der Glaub' an etwas G\u00f6ttliches, w\u00fcrd' es in frommer Demut nur", "tokens": ["Der", "Glaub'", "an", "et\u00b7was", "G\u00f6tt\u00b7li\u00b7ches", ",", "w\u00fcrd'", "es", "in", "from\u00b7mer", "De\u00b7mut", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "ADJA", "$,", "VAFIN", "PPER", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Als ein Geheimnis hingestellt, er w\u00fcrde bald verflogen sein.", "tokens": ["Als", "ein", "Ge\u00b7heim\u00b7nis", "hin\u00b7ge\u00b7stellt", ",", "er", "w\u00fcr\u00b7de", "bald", "ver\u00b7flo\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "PPER", "VAFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Ein Gott nur wird, des wei\u00dfer Bart als eines guten Gro\u00dfpapa's", "tokens": ["Ein", "Gott", "nur", "wird", ",", "des", "wei\u00b7\u00dfer", "Bart", "als", "ei\u00b7nes", "gu\u00b7ten", "Gro\u00df\u00b7pa\u00b7pa's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "ART", "ADJA", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Den gro\u00dfen Kindern wohlgef\u00e4llt, der Gott der Theologen sein.", "tokens": ["Den", "gro\u00b7\u00dfen", "Kin\u00b7dern", "wohl\u00b7ge\u00b7f\u00e4llt", ",", "der", "Gott", "der", "Theo\u00b7lo\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Und sieh gewisse Dichter an! Der Schleier, drin sich r\u00e4tselhaft", "tokens": ["Und", "sieh", "ge\u00b7wis\u00b7se", "Dich\u00b7ter", "an", "!", "Der", "Schlei\u00b7er", ",", "drin", "sich", "r\u00e4t\u00b7sel\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADJA", "NN", "PTKVZ", "$.", "ART", "NN", "$,", "ADV", "PRF", "ADJD"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Drapiert ihr h\u00f6chst trivialer Held, soll niemals fortgezogen sein.", "tokens": ["Dra\u00b7piert", "ihr", "h\u00f6chst", "tri\u00b7vi\u00b7a\u00b7ler", "Held", ",", "soll", "nie\u00b7mals", "fort\u00b7ge\u00b7zo\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Zufrieden ist's das Publikum und will ja im Theater selbst", "tokens": ["Zu\u00b7frie\u00b7den", "ist's", "das", "Pub\u00b7li\u00b7kum", "und", "will", "ja", "im", "The\u00b7a\u00b7ter", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "VMFIN", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Mit Worten um den Sinn geprellt von schlauen Mystagogen sein.", "tokens": ["Mit", "Wor\u00b7ten", "um", "den", "Sinn", "ge\u00b7prellt", "von", "schlau\u00b7en", "Mys\u00b7ta\u00b7go\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Da\u00df hinter Sonnenklarheit just sich birgt das Unergr\u00fcndlichste,", "tokens": ["Da\u00df", "hin\u00b7ter", "Son\u00b7nen\u00b7klar\u00b7heit", "just", "sich", "birgt", "das", "Un\u00b7er\u00b7gr\u00fcnd\u00b7lichs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVFIN", "PRF", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Wird, weil es nicht ins Auge f\u00e4llt, von Wen'gen nur erwogen sein.", "tokens": ["Wi