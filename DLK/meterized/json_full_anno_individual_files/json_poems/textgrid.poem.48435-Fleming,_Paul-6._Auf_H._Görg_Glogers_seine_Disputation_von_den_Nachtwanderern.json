{"textgrid.poem.48435": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf H. G\u00f6rg Glogers seine Disputation von den Nachtwanderern", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vermag denn di\u00df ein Dampf, der uns bei Schlaf' und Nacht", "tokens": ["Ver\u00b7mag", "denn", "di\u00df", "ein", "Dampf", ",", "der", "uns", "bei", "Schlaf'", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PDS", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "umnebelt Seel' und Sinn, der uns zu Schw\u00e4rmern macht,", "tokens": ["um\u00b7ne\u00b7belt", "Seel'", "und", "Sinn", ",", "der", "uns", "zu", "Schw\u00e4r\u00b7mern", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "im Schlafen ohne Schlaf', im Ruhen ohne Rasten?", "tokens": ["im", "Schla\u00b7fen", "oh\u00b7ne", "Schlaf'", ",", "im", "Ru\u00b7hen", "oh\u00b7ne", "Ras\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der klettert hin und her an T\u00fcren, in Palasten,", "tokens": ["Der", "klet\u00b7tert", "hin", "und", "her", "an", "T\u00fc\u00b7ren", ",", "in", "Pa\u00b7las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "ADV", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der will der Lunen nach in unbepf\u00e4hlter Luft,", "tokens": ["der", "will", "der", "Lu\u00b7nen", "nach", "in", "un\u00b7be\u00b7pf\u00e4hl\u00b7ter", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "steigt frisch dem Giebel zu. Der wagt in eine Kluft", "tokens": ["steigt", "frisch", "dem", "Gie\u00b7bel", "zu", ".", "Der", "wagt", "in", "ei\u00b7ne", "Kluft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$.", "ART", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und Brunnen seinen Leib. Der Schmied ergreift den Hammer", "tokens": ["und", "Brun\u00b7nen", "sei\u00b7nen", "Leib", ".", "Der", "Schmied", "er\u00b7greift", "den", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und l\u00e4uft zum Ambos hin, der Zimmerman die Klammer.", "tokens": ["und", "l\u00e4uft", "zum", "Am\u00b7bos", "hin", ",", "der", "Zim\u00b7mer\u00b7man", "die", "Klam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der schwimmet durch den Strom, erleget seinen Feind,", "tokens": ["Der", "schwim\u00b7met", "durch", "den", "Strom", ",", "er\u00b7le\u00b7get", "sei\u00b7nen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der macht sich auf den Weg, eh' Phosphorus noch scheint;", "tokens": ["der", "macht", "sich", "auf", "den", "Weg", ",", "eh'", "Phos\u00b7pho\u00b7rus", "noch", "scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KOUS", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der setzt sich auf das Holz und meinet wegzureiten,", "tokens": ["der", "setzt", "sich", "auf", "das", "Holz", "und", "mei\u00b7net", "weg\u00b7zu\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "giebt frisch der Wand den Sporn. Der f\u00e4nget an zu streiten", "tokens": ["giebt", "frisch", "der", "Wand", "den", "Sporn", ".", "Der", "f\u00e4n\u00b7get", "an", "zu", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$.", "ART", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und brauchet seiner Faust, der zeucht sich auf das Haus", "tokens": ["und", "brau\u00b7chet", "sei\u00b7ner", "Faust", ",", "der", "zeucht", "sich", "auf", "das", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "am Kloben k\u00fchnlich an und nimmt die Elstren aus.", "tokens": ["am", "Klo\u00b7ben", "k\u00fchn\u00b7lich", "an", "und", "nimmt", "die", "E\u00b7lstren", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie auch viel' Andre mehr, die schlafend das beginnen,", "tokens": ["Wie", "auch", "viel'", "And\u00b7re", "mehr", ",", "die", "schla\u00b7fend", "das", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "PIS", "ADV", "$,", "PRELS", "ADJD", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "was niemand wachend kan. Hier sch\u00e4rfet eure Sinnen,", "tokens": ["was", "nie\u00b7mand", "wa\u00b7chend", "kan", ".", "Hier", "sch\u00e4r\u00b7fet", "eu\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VMFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "ihr, die ihr Weisen seid! Hier ist das sch\u00f6ne Ziel.", "tokens": ["ihr", ",", "die", "ihr", "Wei\u00b7sen", "seid", "!", "Hier", "ist", "das", "sch\u00f6\u00b7ne", "Ziel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kunst mu\u00df der setzen auf, der hier gewinnen will.", "tokens": ["Kunst", "mu\u00df", "der", "set\u00b7zen", "auf", ",", "der", "hier", "ge\u00b7win\u00b7nen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Hier ist die g\u00fcldne Frucht. Hier laufet um die Wette!", "tokens": ["Hier", "ist", "die", "g\u00fcld\u00b7ne", "Frucht", ".", "Hier", "lau\u00b7fet", "um", "die", "Wet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sphinx muste doch einmal mit Blute gehn zu Bette,", "tokens": ["Sphinx", "mus\u00b7te", "doch", "ein\u00b7mal", "mit", "Blu\u00b7te", "gehn", "zu", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "weil man ihr R\u00e4tsel traf. Hier rate, wer da kan!", "tokens": ["weil", "man", "ihr", "R\u00e4t\u00b7sel", "traf", ".", "Hier", "ra\u00b7te", ",", "wer", "da", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "VVFIN", "$,", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hier l\u00f6st den Knoten auf, das keiner noch getan,", "tokens": ["Hier", "l\u00f6st", "den", "Kno\u00b7ten", "auf", ",", "das", "kei\u00b7ner", "noch", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "ja, keiner noch getan! Da steht die teure Krone.", "tokens": ["ja", ",", "kei\u00b7ner", "noch", "ge\u00b7tan", "!", "Da", "steht", "die", "teu\u00b7re", "Kro\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "ADV", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Krone, Siegesman, bek\u00f6mmest du zu Lohne!", "tokens": ["Die", "Kro\u00b7ne", ",", "Sie\u00b7ges\u00b7man", ",", "be\u00b7k\u00f6m\u00b7mest", "du", "zu", "Loh\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vermag denn di\u00df ein Dampf, der uns bei Schlaf' und Nacht", "tokens": ["Ver\u00b7mag", "denn", "di\u00df", "ein", "Dampf", ",", "der", "uns", "bei", "Schlaf'", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PDS", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "umnebelt Seel' und Sinn, der uns zu Schw\u00e4rmern macht,", "tokens": ["um\u00b7ne\u00b7belt", "Seel'", "und", "Sinn", ",", "der", "uns", "zu", "Schw\u00e4r\u00b7mer