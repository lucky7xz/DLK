["PWAV", "ADV", "PIAT", "PIS", "ADV", "$,", "PRELS", "ADJD", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "was niemand wachend kan. Hier sch\u00e4rfet eure Sinnen,", "tokens": ["was", "nie\u00b7mand", "wa\u00b7chend", "kan", ".", "Hier", "sch\u00e4r\u00b7fet", "eu\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VMFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "ihr, die ihr Weisen seid! Hier ist das sch\u00f6ne Ziel.", "tokens": ["ihr", ",", "die", "ihr", "Wei\u00b7sen", "seid", "!", "Hier", "ist", "das", "sch\u00f6\u00b7ne", "Ziel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kunst mu\u00df der setzen auf, der hier gewinnen will.", "tokens": ["Kunst", "mu\u00df", "der", "set\u00b7zen", "auf", ",", "der", "hier", "ge\u00b7win\u00b7nen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Hier ist die g\u00fcldne Frucht. Hier laufet um die Wette!", "tokens": ["Hier", "ist", "die", "g\u00fcld\u00b7ne", "Frucht", ".", "Hier", "lau\u00b7fet", "um", "die", "Wet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sphinx muste doch einmal mit Blute gehn zu Bette,", "tokens": ["Sphinx", "mus\u00b7te", "doch", "ein\u00b7mal", "mit", "Blu\u00b7te", "gehn", "zu", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "weil man ihr R\u00e4tsel traf. Hier rate, wer da kan!", "tokens": ["weil", "man", "ihr", "R\u00e4t\u00b7sel", "traf", ".", "Hier", "ra\u00b7te", ",", "wer", "da", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "VVFIN", "$,", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hier l\u00f6st den Knoten auf, das keiner noch getan,", "tokens": ["Hier", "l\u00f6st", "den", "Kno\u00b7ten", "auf", ",", "das", "kei\u00b7ner", "noch", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "ja, keiner noch getan! Da steht die teure Krone.", "tokens": ["ja", ",", "kei\u00b7ner", "noch", "ge\u00b7tan", "!", "Da", "steht", "die", "teu\u00b7re", "Kro\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "ADV", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Krone, Siegesman, bek\u00f6mmest du zu Lohne!", "tokens": ["Die", "Kro\u00b7ne", ",", "Sie\u00b7ges\u00b7man", ",", "be\u00b7k\u00f6m\u00b7mest", "du", "zu", "Loh\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}