{"dta.poem.4350": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der sp\u00e4te Frost im Jahre 1740  \n den 10 April.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, will denn noch von unsern Grenzen", "tokens": ["Ach", ",", "will", "denn", "noch", "von", "un\u00b7sern", "Gren\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der gar zu feste Frost nicht fliehn!", "tokens": ["Der", "gar", "zu", "fes\u00b7te", "Frost", "nicht", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will das so lang\u2019 erseufzte Gr\u00fcn,", "tokens": ["Will", "das", "so", "lang'", "er\u00b7seufz\u00b7te", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jm l\u00e4ngst schon eingetret\u2019nen Lenzen,", "tokens": ["Jm", "l\u00e4ngst", "schon", "ein\u00b7ge\u00b7tret'\u00b7nen", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wiesen nicht mehr \u00fcberziehn,", "tokens": ["Die", "Wie\u00b7sen", "nicht", "mehr", "\u00fc\u00b7ber\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Gras noch Kraut das Feld bekr\u00e4nzen,", "tokens": ["Kein", "Gras", "noch", "Kraut", "das", "Feld", "be\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Bluhmen-Heer in G\u00e4rten bl\u00fchn,", "tokens": ["Kein", "Bluh\u00b7men\u00b7Heer", "in", "G\u00e4r\u00b7ten", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und in gef\u00e4rbter Anmuht gl\u00e4nzen?", "tokens": ["Und", "in", "ge\u00b7f\u00e4rb\u00b7ter", "An\u00b7muht", "gl\u00e4n\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo ist des Fr\u00fchlings Pracht und Schein?", "tokens": ["Wo", "ist", "des", "Fr\u00fch\u00b7lings", "Pracht", "und", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Boden ist noch Felsen-hart,", "tokens": ["Der", "Bo\u00b7den", "ist", "noch", "Fel\u00b7sen\u00b7hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das Feld so fest als Stahl und Stein.", "tokens": ["Das", "Feld", "so", "fest", "als", "Stahl", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auch in der Sonnen Gegenwart", "tokens": ["Auch", "in", "der", "Son\u00b7nen", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Scheint dennoch die Natur erstarrt.", "tokens": ["Scheint", "den\u00b7noch", "die", "Na\u00b7tur", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "An statt des Zephyrs lauen Blasen", "tokens": ["An", "statt", "des", "Ze\u00b7phyrs", "lau\u00b7en", "Bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "H\u00f6rt man noch scharfe Winde rasen.", "tokens": ["H\u00f6rt", "man", "noch", "schar\u00b7fe", "Win\u00b7de", "ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Des reinen Himmels heitres Blau", "tokens": ["Des", "rei\u00b7nen", "Him\u00b7mels", "heit\u00b7res", "Blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Verh\u00fcllt ein kalt- und tr\u00fcbes Grau.", "tokens": ["Ver\u00b7h\u00fcllt", "ein", "kal\u00b7t", "und", "tr\u00fc\u00b7bes", "Grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Es friert nunmehr ein halbes Jahr", "tokens": ["Es", "friert", "nun\u00b7mehr", "ein", "hal\u00b7bes", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Seit dem October und November;", "tokens": ["Seit", "dem", "Oc\u00b7to\u00b7ber", "und", "No\u00b7vem\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Der Martius gleicht dem December,", "tokens": ["Der", "Mar\u00b7tius", "gleicht", "dem", "De\u00b7cem\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Der Majus selbst dem Januar.", "tokens": ["Der", "Ma\u00b7jus", "selbst", "dem", "Ja\u00b7nu\u00b7ar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Es k\u00f6nnen aus den festen Fl\u00e4chen", "tokens": ["Es", "k\u00f6n\u00b7nen", "aus", "den", "fes\u00b7ten", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und denen nicht verhandnen Ritzen", "tokens": ["Und", "de\u00b7nen", "nicht", "ver\u00b7hand\u00b7nen", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der kleinen gr\u00fcnen Pfriemen Spitzen", "tokens": ["Der", "klei\u00b7nen", "gr\u00fc\u00b7nen", "Pfrie\u00b7men", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Von Bluhmen, Kraut und Gras nicht brechen;", "tokens": ["Von", "Bluh\u00b7men", ",", "Kraut", "und", "Gras", "nicht", "bre\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Es ist noch gar kein Gr\u00fcn zu sehn.", "tokens": ["Es", "ist", "noch", "gar", "kein", "Gr\u00fcn", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Auf dem verschlo\u00dfnen Erden-Schoo\u00df", "tokens": ["Auf", "dem", "ver\u00b7schlo\u00df\u00b7nen", "Er\u00b7den\u00b7Schoo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sieht man nur ein erfrornes Moo\u00df,", "tokens": ["Sieht", "man", "nur", "ein", "er\u00b7fror\u00b7nes", "Moo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und zwar nur hin und wieder, stehen.", "tokens": ["Und", "zwar", "nur", "hin", "und", "wie\u00b7der", ",", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKVZ", "KON", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}