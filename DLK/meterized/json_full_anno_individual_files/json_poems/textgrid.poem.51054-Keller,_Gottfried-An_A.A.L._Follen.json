{"textgrid.poem.51054": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An A.A.L. Follen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm diese Lieder, Lobgesang und Klagen,", "tokens": ["Nimm", "die\u00b7se", "Lie\u00b7der", ",", "Lob\u00b7ge\u00b7sang", "und", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie die bunte Jahreszeit gebracht!", "tokens": ["Wie", "sie", "die", "bun\u00b7te", "Jah\u00b7res\u00b7zeit", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie mir der Himmel wechselnd weint' und lacht',", "tokens": ["Wie", "mir", "der", "Him\u00b7mel", "wech\u00b7selnd", "weint'", "und", "lacht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hab ich die Lyra regellos geschlagen.", "tokens": ["Hab", "ich", "die", "Ly\u00b7ra", "re\u00b7gel\u00b7los", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Sande knarrt der Freiheit goldner Wagen,", "tokens": ["Im", "San\u00b7de", "knarrt", "der", "Frei\u00b7heit", "gold\u00b7ner", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist ein m\u00fc\u00dfig Schreien Tag und Nacht;", "tokens": ["Es", "ist", "ein", "m\u00fc\u00b7\u00dfig", "Schrei\u00b7en", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bet\u00e4ubt, verworren von der Zungenschlacht,", "tokens": ["Be\u00b7t\u00e4ubt", ",", "ver\u00b7wor\u00b7ren", "von", "der", "Zun\u00b7gen\u00b7schlacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zeigt sich der Beste schwach in diesen Tagen.", "tokens": ["Zeigt", "sich", "der", "Bes\u00b7te", "schwach", "in", "die\u00b7sen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Uns mangelt des Gef\u00fchles edle Feinheit,", "tokens": ["Uns", "man\u00b7gelt", "des", "Ge\u00b7f\u00fch\u00b7les", "ed\u00b7le", "Fein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So Schwung und Sch\u00e4rfe leiht dem Schwert im Fechten,", "tokens": ["So", "Schwung", "und", "Sch\u00e4r\u00b7fe", "leiht", "dem", "Schwert", "im", "Fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das hohe Wollen und des Herzens Reinheit.", "tokens": ["Das", "ho\u00b7he", "Wol\u00b7len", "und", "des", "Her\u00b7zens", "Rein\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Klar sind sich nur die Schlimmen und die Schlechten,", "tokens": ["Klar", "sind", "sich", "nur", "die", "Schlim\u00b7men", "und", "die", "Schlech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PRF", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sie suchen sich und scharen sich in Einheit,", "tokens": ["Sie", "su\u00b7chen", "sich", "und", "scha\u00b7ren", "sich", "in", "Ein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entsagend dumpf der Ehre und dem Rechten!", "tokens": ["Ent\u00b7sa\u00b7gend", "dumpf", "der", "Eh\u00b7re", "und", "dem", "Rech\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nimm diese Lieder, Lobgesang und Klagen,", "tokens": ["Nimm", "die\u00b7se", "Lie\u00b7der", ",", "Lob\u00b7ge\u00b7sang", "und", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text"