{"textgrid.poem.63647": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe l\u00e4ngst in mir den Wunsch begraben,", "tokens": ["Ich", "ha\u00b7be", "l\u00e4ngst", "in", "mir", "den", "Wunsch", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu schl\u00fcrfen aus des Lebens Freudebronnen;", "tokens": ["Zu", "schl\u00fcr\u00b7fen", "aus", "des", "Le\u00b7bens", "Freu\u00b7de\u00b7bron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Ehrgeiz schwand, mich am Erfolg zu sonnen,", "tokens": ["Der", "Ehr\u00b7geiz", "schwand", ",", "mich", "am", "Er\u00b7folg", "zu", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcber Habsucht f\u00fchl' ich mich erhaben.", "tokens": ["Und", "\u00fc\u00b7ber", "Hab\u00b7sucht", "f\u00fchl'", "ich", "mich", "er\u00b7ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So werd' ich meinen Weg zu Ende traben", "tokens": ["So", "werd'", "ich", "mei\u00b7nen", "Weg", "zu", "En\u00b7de", "tra\u00b7be