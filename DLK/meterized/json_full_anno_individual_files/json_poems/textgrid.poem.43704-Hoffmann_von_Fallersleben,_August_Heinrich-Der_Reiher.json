{"textgrid.poem.43704": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Reiher", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn spazieren geht der Reiher,", "tokens": ["Wenn", "spa\u00b7zie\u00b7ren", "geht", "der", "Rei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denkt er \u00fcber Manches nach:", "tokens": ["Denkt", "er", "\u00fc\u00b7ber", "Man\u00b7ches", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob sich's besser fischt am Weiher", "tokens": ["Ob", "sich's", "bes\u00b7ser", "fischt", "am", "Wei\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder besser noch am Bach.", "tokens": ["O\u00b7der", "bes\u00b7ser", "noch", "am", "Bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Endlich hat er sich entschlossen,", "tokens": ["End\u00b7lich", "hat", "er", "sich", "ent\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht zum Weiher hin und fischt,", "tokens": ["Geht", "zum", "Wei\u00b7her", "hin", "und", "fischt", ","], "token_info": ["word", "word", "word", "