{"textgrid.poem.39321": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Schweizer h\u00f6chste Dapferkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schweizer h\u00f6chste Dapferkeit,", "tokens": ["Der", "Schwei\u00b7zer", "h\u00f6chs\u00b7te", "Dap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die keinem Schmerz entflieht,", "tokens": ["Die", "kei\u00b7nem", "Schmerz", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Besiegt noch k\u00e4mpft, den Tod nicht scheut,", "tokens": ["Be\u00b7siegt", "noch", "k\u00e4mpft", ",", "den", "Tod", "nicht", "scheut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "$,", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdiente die kein Lied?", "tokens": ["Ver\u00b7dien\u00b7te", "die", "kein", "Lied", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, str\u00f6me m\u00e4chtig und ert\u00f6n,", "tokens": ["Ja", ",", "str\u00f6\u00b7me", "m\u00e4ch\u00b7tig", "und", "er\u00b7t\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lied, das unsterblich macht!", "tokens": ["Lied", ",", "das", "uns\u00b7terb\u00b7lich", "macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie trutzten gleich den Alpenh\u00f6hn", "tokens": ["Sie", "trutz\u00b7ten", "gleich", "den", "Al\u00b7pen\u00b7h\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Donner in der Schlacht.", "tokens": ["Dem", "Don\u00b7ner", "in", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sahn den Feind und schlugen ihn", "tokens": ["Sie", "sahn", "den", "Feind", "und", "schlu\u00b7gen", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fck mit kleiner Zahl;", "tokens": ["Zu\u00b7r\u00fck", "mit", "klei\u00b7ner", "Zahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sehn ihn wieder, schlagen k\u00fchn", "tokens": ["Sie", "sehn", "ihn", "wie\u00b7der", ",", "schla\u00b7gen", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn schnell zum zweitenmal.", "tokens": ["Ihn", "schnell", "zum", "zwei\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Verwegen macht der fr\u00fche Sieg", "tokens": ["Ver\u00b7we\u00b7gen", "macht", "der", "fr\u00fc\u00b7he", "Sieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sieger Heldenhand:", "tokens": ["Der", "Sie\u00b7ger", "Hel\u00b7den\u00b7hand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie st\u00fcrzten sich in tiefern Krieg", "tokens": ["Sie", "st\u00fcrz\u00b7ten", "sich", "in", "tie\u00b7fern", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu voll von Vaterland.", "tokens": ["Zu", "voll", "von", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Umsonst Kanonendonner br\u00fcll'", "tokens": ["Um\u00b7sonst", "Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", "br\u00fcll'"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und str\u00f6me Tod auf Tod!", "tokens": ["Und", "str\u00f6\u00b7me", "Tod", "auf", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie dringen ein; Tod ist ihr Spiel,", "tokens": ["Sie", "drin\u00b7gen", "ein", ";", "Tod", "ist", "ihr", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und Feinde Morgenbrod.", "tokens": ["Und", "Fein\u00b7de", "Mor\u00b7gen\u00b7brod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zwar st\u00f6\u00dft das zehnmal gr\u00f6sre Heer", "tokens": ["Zwar", "st\u00f6\u00dft", "das", "zehn\u00b7mal", "gr\u00f6s\u00b7re", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Feinde sie zur\u00fck.", "tokens": ["Der", "Fein\u00b7de", "sie", "zu\u00b7r\u00fck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch zehnfach t\u00f6dtet ihr Gewehr", "tokens": ["Doch", "zehn\u00b7fach", "t\u00f6d\u00b7tet", "ihr", "Ge\u00b7wehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jedem Augenblik.", "tokens": ["Mit", "je\u00b7dem", "Au\u00b7gen\u00b7blik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bey Jacobs Mauren h\u00f6rten sie", "tokens": ["Bey", "Ja\u00b7cobs", "Mau\u00b7ren", "h\u00f6r\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kriegesrosse Trab:", "tokens": ["Der", "Krie\u00b7ges\u00b7ros\u00b7se", "Trab", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbeh unser auch nur einer flieh',", "tokens": ["\u00bb", "eh", "un\u00b7ser", "auch", "nur", "ei\u00b7ner", "flieh'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh find' er hier sein Grab\u00ab!", "tokens": ["Eh", "find'", "er", "hier", "sein", "Grab", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Feind stieg schnaubend von dem Pferd;", "tokens": ["Der", "Feind", "stieg", "schnau\u00b7bend", "von", "dem", "Pferd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbkomm nur, wir bleiben still;", "tokens": ["\u00bb", "komm", "nur", ",", "wir", "blei\u00b7ben", "still", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Maur\u00fcber flamme Schwerd an Schwerd", "tokens": ["Maur\u00b7\u00fc\u00b7ber", "flam\u00b7me", "Schwerd", "an", "Schwerd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Tod; wir bleiben still\u00ab!", "tokens": ["Nach", "Tod", ";", "wir", "blei\u00b7ben", "still", "\u00ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$.", "PPER", "VVINF", "VMFIN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die L\u00f6wen stritten; jeder stand,", "tokens": ["Die", "L\u00f6\u00b7wen", "strit\u00b7ten", ";", "je\u00b7der", "stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wich keines Haares breit;", "tokens": ["Wich", "kei\u00b7nes", "Haa\u00b7res", "breit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die schon zerst\u00fckte Schweizerhand", "tokens": ["Die", "schon", "zer\u00b7st\u00fck\u00b7te", "Schwei\u00b7zer\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War muthig noch im Streit.", "tokens": ["War", "mut\u00b7hig", "noch", "im", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sink immer Glied um Glied zerfezt;", "tokens": ["Sink", "im\u00b7mer", "Glied", "um", "Glied", "zer\u00b7fezt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie k\u00e4mpften tief im Blut.", "tokens": ["Sie", "k\u00e4mpf\u00b7ten", "tief", "im", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer Freyheit mehr als Leben sch\u00e4zt,", "tokens": ["Wer", "Frey\u00b7heit", "mehr", "als", "Le\u00b7ben", "sch\u00e4zt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PIAT", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beh\u00e4lt im Tode Muth.", "tokens": ["Be\u00b7h\u00e4lt", "im", "To\u00b7de", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Feinde ungeheure Zahl", "tokens": ["Der", "Fein\u00b7de", "un\u00b7ge\u00b7heu\u00b7re", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlug, traf, doch siegt' sie nicht;", "tokens": ["Schlug", ",", "traf", ",", "doch", "siegt'", "sie", "nicht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rief: \u00bbStr\u00f6mt, Kanonen, noch einmal", "tokens": ["Rief", ":", "\u00bb", "Str\u00f6mt", ",", "Ka\u00b7no\u00b7nen", ",", "noch", "ein\u00b7mal"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "VVFIN", "$,", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tod in ihr Angesicht\u00ab!", "tokens": ["Tod", "in", "ihr", "An\u00b7ge\u00b7sicht", "\u00ab", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Die Kr\u00e4ffte sanken, nicht ihr Muth;", "tokens": ["Die", "Kr\u00e4ff\u00b7te", "san\u00b7ken", ",", "nicht", "ihr", "Muth", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nein! der sah nie zur\u00fck!", "tokens": ["Nein", "!", "der", "sah", "nie", "zu\u00b7r\u00fck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie rafften sich empor im Blut;", "tokens": ["Sie", "raff\u00b7ten", "sich", "em\u00b7por", "im", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tod war ihr lezter Blik.", "tokens": ["Tod", "war", "ihr", "lez\u00b7ter", "Blik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Wenn Dapferkeit im hei\u00dfen Krieg", "tokens": ["Wenn", "Dap\u00b7fer\u00b7keit", "im", "hei\u00b7\u00dfen", "Krieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht immer siegen mag,", "tokens": ["Nicht", "im\u00b7mer", "sie\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n ist sie doch; dem sch\u00f6nsten Sieg", "tokens": ["Sch\u00f6n", "ist", "sie", "doch", ";", "dem", "sch\u00f6ns\u00b7ten", "Sieg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleicht diese Niederlag!", "tokens": ["Gleicht", "die\u00b7se", "Nie\u00b7der\u00b7lag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.14": {"line.1": {"text": "Erstaunungsvoll sah der Delphin", "tokens": ["Er\u00b7stau\u00b7nungs\u00b7voll", "sah", "der", "Del\u00b7phin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sein bestes Volk im Grab;", "tokens": ["Sein", "bes\u00b7tes", "Volk", "im", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Sieg erf\u00fcllt mit Grauen ihn,", "tokens": ["Sein", "Sieg", "er\u00b7f\u00fcllt", "mit", "Grau\u00b7en", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch staunt er, bebt, zieht ab.", "tokens": ["Noch", "staunt", "er", ",", "bebt", ",", "zieht", "ab", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Der Schweizer h\u00f6chste Dapferkeit,", "tokens": ["Der", "Schwei\u00b7zer", "h\u00f6chs\u00b7te", "Dap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die keinem Schmerz entflieht,", "tokens": ["Die", "kei\u00b7nem", "Schmerz", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Besiegt noch k\u00e4mpft, den Tod nicht scheut,", "tokens": ["Be\u00b7siegt", "noch", "k\u00e4mpft", ",", "den", "Tod", "nicht", "sch