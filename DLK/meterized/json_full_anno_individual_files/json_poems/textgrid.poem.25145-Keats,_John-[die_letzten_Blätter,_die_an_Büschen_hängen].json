{"textgrid.poem.25145": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[die letzten Bl\u00e4tter, die an B\u00fcschen h\u00e4ngen]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die letzten Bl\u00e4tter, die an B\u00fcschen h\u00e4ngen,", "tokens": ["Die", "letz\u00b7ten", "Bl\u00e4t\u00b7ter", ",", "die", "an", "B\u00fc\u00b7schen", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerrauft ein Wind mit monotonem Wort,", "tokens": ["Zer\u00b7rauft", "ein", "Wind", "mit", "mo\u00b7no\u00b7to\u00b7nem", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sterne stehen kalt am Himmel dort,", "tokens": ["Die", "Ster\u00b7ne", "ste\u00b7hen", "kalt", "am", "Him\u00b7mel", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und vor mir liegt ein Weg von Meilesl\u00e4ngen.", "tokens": ["Und", "vor", "mir", "liegt", "ein", "Weg", "von", "Mei\u00b7les\u00b7l\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch kann mich Kampf und K\u00e4lte nicht bedr\u00e4ngen;", "tokens": ["Doch", "kann", "mich", "Kampf", "und", "K\u00e4l\u00b7te", "nicht", "be\u00b7dr\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf toten Bl\u00e4ttern schreit ich heiter fort,", "tokens": ["Auf", "to\u00b7ten", "Bl\u00e4t\u00b7tern", "schreit", "ich", "hei\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Obschon sehr fern der heimatliche Ort", "tokens": ["Ob\u00b7schon", "sehr", "fern", "der", "hei\u00b7mat\u00b7li\u00b7che", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Und kalt herab die Silberlampen h\u00e4ngen.", "tokens": ["Und", "kalt", "her\u00b7ab", "die", "Sil\u00b7ber\u00b7lam\u00b7pen", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn \u00fcbervoll bin ich der Freundlichkeit,", "tokens": ["Denn", "\u00fc\u00b7ber\u00b7voll", "bin", "ich", "der", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die heut in kleiner H\u00fctte zu mir kam,", "tokens": ["Die", "heut", "in", "klei\u00b7ner", "H\u00fct\u00b7te", "zu", "mir", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Miltons Klage, Lycidas geweiht,", "tokens": ["Von", "Mil\u00b7tons", "Kla\u00b7ge", ",", "Ly\u00b7ci\u00b7das", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als Schicksalsh\u00e4rte diesen Freund ihm nahm;", "tokens": ["Als", "Schick\u00b7sals\u00b7h\u00e4r\u00b7te", "die\u00b7sen", "Freund", "ihm", "nahm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Laura, die Petrarca so entz\u00fcckte,", "tokens": ["Von", "Lau\u00b7ra", ",", "die", "Pe\u00b7trar\u00b7ca", "so", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ihn die Krone aller Kronen schm\u00fcckte.", "tokens": ["Da\u00df", "ihn", "die", "Kro\u00b7ne", "al\u00b7ler", "Kro\u00b7nen", "schm\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die letzten Bl\u00e4tter, die an B\u00fcschen h\u00e4ngen,", "tokens": ["Die", "letz\u00b7ten", "Bl\u00e4t\u00b7ter", ",", "die", "an", "B\u00fc\u00b7schen", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text