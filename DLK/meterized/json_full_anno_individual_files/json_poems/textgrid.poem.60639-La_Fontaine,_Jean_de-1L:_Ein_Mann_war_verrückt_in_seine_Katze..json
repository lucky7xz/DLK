{"textgrid.poem.60639": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Mann war verr\u00fcckt in seine Katze.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann war verr\u00fcckt in seine Katze.", "tokens": ["Ein", "Mann", "war", "ver\u00b7r\u00fcckt", "in", "sei\u00b7ne", "Kat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er fand ihr Miauen so zaubervoll,", "tokens": ["Er", "fand", "ihr", "Mi\u00b7au\u00b7en", "so", "zau\u00b7ber\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So reizend ihr K\u00f6pfchen, so z\u00e4rtlich die Tatze \u2013", "tokens": ["So", "rei\u00b7zend", "ihr", "K\u00f6pf\u00b7chen", ",", "so", "z\u00e4rt\u00b7lich", "die", "Tat\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Kurz, er war toller als toll!", "tokens": ["Kurz", ",", "er", "war", "tol\u00b7ler", "als", "toll", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VAFIN", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Was tat er nicht alles an Bittgebeten,", "tokens": ["Was", "tat", "er", "nicht", "al\u00b7les", "an", "Bitt\u00b7ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "PIS", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "An Tr\u00e4nen und Spr\u00fcchen der Zauberkunst,", "tokens": ["An", "Tr\u00e4\u00b7nen", "und", "Spr\u00fc\u00b7chen", "der", "Zau\u00b7ber\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Bis endlich durch eines Gottes Gunst", "tokens": ["Bis", "end\u00b7lich", "durch", "ei\u00b7nes", "Got\u00b7tes", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Das gro\u00dfe Wunder eingetreten:", "tokens": ["Das", "gro\u00b7\u00dfe", "Wun\u00b7der", "ein\u00b7ge\u00b7tre\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als eines Morgens der Narr erwacht,", "tokens": ["Als", "ei\u00b7nes", "Mor\u00b7gens", "der", "Narr", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Da war seine Katze zum Weibe geworden!", "tokens": ["Da", "war", "sei\u00b7ne", "Kat\u00b7ze", "zum", "Wei\u00b7be", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VAPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Er hat sich keine Minute bedacht,", "tokens": ["Er", "hat", "sich", "kei\u00b7ne", "Mi\u00b7nu\u00b7te", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Er trat mit ihr in Hymens Orden", "tokens": ["Er", "trat", "mit", "ihr", "in", "Hy\u00b7mens", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und jubelte in den h\u00f6chsten Akkorden,", "tokens": ["Und", "ju\u00b7bel\u00b7te", "in", "den", "h\u00f6chs\u00b7ten", "Ak\u00b7kor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Als er sie abends zu Bett gebracht.", "tokens": ["Als", "er", "sie", "a\u00b7bends", "zu", "Bett", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Er zeigte sich ebenso \u00fcbertrieben,", "tokens": ["Er", "zeig\u00b7te", "sich", "e\u00b7ben\u00b7so", "\u00fc\u00b7bert\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Wie einst in der Freundschaft, jetzt im Lieben.", "tokens": ["Wie", "einst", "in", "der", "Freund\u00b7schaft", ",", "jetzt", "im", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "$,", "ADV", "APPRART", "ADJA", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "So sehr war er von ihr entz\u00fcckt,", "tokens": ["So", "sehr", "war", "er", "von", "ihr", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.18": {"text": "Wie noch kein Dichter je geschrieben,", "tokens": ["Wie", "noch", "kein", "Dich\u00b7ter", "je", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df eine Sch\u00f6ne den Liebsten begl\u00fcckt.", "tokens": ["Da\u00df", "ei\u00b7ne", "Sch\u00f6\u00b7ne", "den", "Liebs\u00b7ten", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und w\u00e4hrend sie ihm innig schmeichelt,", "tokens": ["Und", "w\u00e4h\u00b7rend", "sie", "ihm", "in\u00b7nig", "schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Liebkost er sie und herzt und streichelt", "tokens": ["Lieb\u00b7kost", "er", "sie", "und", "herzt", "und", "strei\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "KON", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Der jungen Gattin geschmeidigen Leib;", "tokens": ["Der", "jun\u00b7gen", "Gat\u00b7tin", "ge\u00b7schmei\u00b7di\u00b7gen", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Die ganze Katze ist vergessen,", "tokens": ["Die", "gan\u00b7ze", "Kat\u00b7ze", "ist", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und der Begl\u00fcckte glaubt vermessen,", "tokens": ["Und", "der", "Be\u00b7gl\u00fcck\u00b7te", "glaubt", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "In allem sei sie ganz ein Weib.", "tokens": ["In", "al\u00b7lem", "sei", "sie", "ganz", "ein", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Doch j\u00e4hlings wird der Gatte", "tokens": ["Doch", "j\u00e4h\u00b7lings", "wird", "der", "Gat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "In seiner Freude gest\u00f6rt,", "tokens": ["In", "sei\u00b7ner", "Freu\u00b7de", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Dieweil man an der Matte", "tokens": ["Die\u00b7weil", "man", "an", "der", "Mat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Ein M\u00e4uslein knabbern h\u00f6rt.", "tokens": ["Ein", "M\u00e4us\u00b7lein", "knab\u00b7bern", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Das Weib mit einem Satz zum Bett hinaus!", "tokens": ["Das", "Weib", "mit", "ei\u00b7nem", "Satz", "zum", "Bett", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Das M\u00e4uslein fort \u2013 doch kehrt es bald zur\u00fcck \u2013", "tokens": ["Das", "M\u00e4us\u00b7lein", "fort", "\u2013", "doch", "kehrt", "es", "bald", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$(", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Mit diesem M\u00e4uslein war es aus!", "tokens": ["Mit", "die\u00b7sem", "M\u00e4us\u00b7lein", "war", "es", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und immer war sie so auf jede Maus erpicht,", "tokens": ["Und", "im\u00b7mer", "war", "sie", "so", "auf", "je\u00b7de", "Maus", "er\u00b7picht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Getreu dem Wort: Die Katze l\u00e4\u00dft das Mausen nicht!", "tokens": ["Ge\u00b7treu", "dem", "Wort", ":", "Die", "Kat\u00b7ze", "l\u00e4\u00dft", "das", "Mau\u00b7sen", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was nutzte nun die sch\u00f6ne Wandlung der Statur?", "tokens": ["Was", "nutz\u00b7te", "nun", "die", "sch\u00f6\u00b7ne", "Wand\u00b7lung", "der", "Sta\u00b7tur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "St\u00e4rker als alles ist die Stimme der Natur!", "tokens": ["St\u00e4r\u00b7ker", "als", "al\u00b7les", "ist", "die", "Stim\u00b7me", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PIS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Der angeborne Trieb verspottet jeden Zwang,", "tokens": ["Der", "an\u00b7ge\u00b7bor\u00b7ne", "Trieb", "ver\u00b7spot\u00b7tet", "je\u00b7den", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr lehrt ihn nicht mit Sporn noch Peitsche andern Gang.", "tokens": ["Ihr", "lehrt", "ihn", "nicht", "mit", "Sporn", "noch", "Peit\u00b7sche", "an\u00b7dern", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "ADV", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Spart euch die M\u00fch und la\u00dft es bleiben,", "tokens": ["Spart", "euch", "die", "M\u00fch", "und", "la\u00dft", "es", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alte Manieren auszutreiben,", "tokens": ["Al\u00b7te", "Ma\u00b7nie\u00b7ren", "aus\u00b7zu\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Denn alles wird erfolglos sein;", "tokens": ["Denn", "al\u00b7les", "wird", "er\u00b7folg\u00b7los", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Werft ihr sie vorne auch zur T\u00fcr hinaus,", "tokens": ["Werft", "ihr", "sie", "vor\u00b7ne", "auch", "zur", "T\u00fcr", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So schlagen hinten sie ein Fenster ein \u2013", "tokens": ["So", "schla\u00b7gen", "hin\u00b7ten", "sie", "ein", "Fens\u00b7ter", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und sitzen wieder mitten drin im Haus.", "tokens": ["Und", "sit\u00b7zen", "wie\u00b7der", "mit\u00b7ten", "drin", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Mann war verr\u00fcckt in 