{"dta.poem.939": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Im freien viereck mit den gelben steinen               ", "tokens": ["Im", "frei\u00b7en", "vier\u00b7eck", "mit", "den", "gel\u00b7ben", "stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dessen mitte sich die brunnen regen               ", "tokens": ["In", "des\u00b7sen", "mit\u00b7te", "sich", "die", "brun\u00b7nen", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VMFIN", "PRF", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Willst du noch fl&#252;chtig sp&#228;te rede pflegen               ", "tokens": ["Willst", "du", "noch", "fl", "&#252;", "ch\u00b7tig", "sp", "&#228;", "te", "re\u00b7de", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "VVINF"], "meter": "+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Da heut dir hell wie nie die sterne scheinen             ", "tokens": ["Da", "heut", "dir", "hell", "wie", "nie", "die", "ster\u00b7ne", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADJD", "KOKOM", "ADV", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch tritt von dem basaltenen beh&#228;lter               ", "tokens": ["Doch", "tritt", "von", "dem", "ba\u00b7sal\u00b7te\u00b7nen", "beh", "&#228;", "lter"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "VVFIN", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er winkt die toten zweige zu bestatten               ", "tokens": ["Er", "winkt", "die", "to\u00b7ten", "zwei\u00b7ge", "zu", "be\u00b7stat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im vollen mondenlichte weht es k&#228;lter               ", "tokens": ["Im", "vol\u00b7len", "mon\u00b7den\u00b7lich\u00b7te", "weht", "es", "k", "&#228;", "lter"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$(", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Als dr&#252;ben unter jener f\u00f6hren schatten &#183;             ", "tokens": ["Als", "dr", "&#252;", "ben", "un\u00b7ter", "je\u00b7ner", "f\u00f6h\u00b7ren", "schat\u00b7ten", "&#183;"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["KOUS", "NE", "$(", "VVFIN", "APPR", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich lasse meine grosse traurigkeit               ", "tokens": ["Ich", "las\u00b7se", "mei\u00b7ne", "gros\u00b7se", "trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich falsch erraten um dich zu verschonen               ", "tokens": ["Dich", "falsch", "er\u00b7ra\u00b7ten", "um", "dich", "zu", "ver\u00b7scho\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich f&#252;hle hat die zeit uns kaum entzweit               ", "tokens": ["Ich", "f", "&#252;", "hle", "hat", "die", "zeit", "uns", "kaum", "ent\u00b7zweit"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "VAFIN", "ART", "NN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wirst du meinen traum nicht mehr bewohnen             ", "tokens": ["So", "wirst", "du", "mei\u00b7nen", "traum", "nicht", "mehr", "be\u00b7woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wenn erst unterm schnee der park entschlief               ", "tokens": ["Doch", "wenn", "erst", "un\u00b7term", "schnee", "der", "park", "ent\u00b7schlief"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So glaub ich dass noch leiser trost entquille               ", "tokens": ["So", "glaub", "ich", "dass", "noch", "lei\u00b7ser", "trost", "ent\u00b7quil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ADV", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus manchen sch\u00f6nen resten strauss und brief               ", "tokens": ["Aus", "man\u00b7chen", "sch\u00f6\u00b7nen", "res\u00b7ten", "strauss", "und", "brief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In tiefer kalter winterlicher stille.             ", "tokens": ["In", "tie\u00b7fer", "kal\u00b7ter", "win\u00b7ter\u00b7li\u00b7cher", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}