{"textgrid.poem.62934": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Memphis goldner Burg trat vor des K\u00f6nigs Thron", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Memphis goldner Burg trat vor des K\u00f6nigs Thron", "tokens": ["In", "Mem\u00b7phis", "gold\u00b7ner", "Burg", "trat", "vor", "des", "K\u00f6\u00b7nigs", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein edler Greis an seinem Stabe:", "tokens": ["Ein", "ed\u00b7ler", "Greis", "an", "sei\u00b7nem", "Sta\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich diente deinem Vater schon,", "tokens": ["Ich", "dien\u00b7te", "dei\u00b7nem", "Va\u00b7ter", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach er, und wenn ich treu gedienet habe,", "tokens": ["Sprach", "er", ",", "und", "wenn", "ich", "treu", "ge\u00b7die\u00b7net", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "KOUS", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So gieb mir meiner Arbeit Lohn.", "tokens": ["So", "gieb", "mir", "mei\u00b7ner", "Ar\u00b7beit", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der K\u00f6nig kannte l\u00e4ngst des Alten Treue,", "tokens": ["Der", "K\u00f6\u00b7nig", "kann\u00b7te", "l\u00e4ngst", "des", "Al\u00b7ten", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er sah ihn freundlich an: \u00bbWas foderst du", "tokens": ["Er", "sah", "ihn", "freund\u00b7lich", "an", ":", "\u00bb", "Was", "fo\u00b7derst", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$(", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von meiner Dankbarkeit?\u00ab \u2013 \u00bbDie Ruh;", "tokens": ["Von", "mei\u00b7ner", "Dank\u00b7bar\u00b7keit", "?", "\u00ab", "\u2013", "\u00bb", "Die", "Ruh", ";"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "$(", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verg\u00f6nne, da\u00df ich mich mit meinen Enkeln freue,", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", ",", "da\u00df", "ich", "mich", "mit", "mei\u00b7nen", "En\u00b7keln", "freu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Eh mich der Tod von hinnen raft.\u00ab", "tokens": ["Eh", "mich", "der", "Tod", "von", "hin\u00b7nen", "raft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ey, rief der F\u00fcrst, du kannst noch lange n\u00fctzen,", "tokens": ["Ey", ",", "rief", "der", "F\u00fcrst", ",", "du", "kannst", "noch", "lan\u00b7ge", "n\u00fct\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dein Geist beh\u00e4lt noch seine ganze Kraft;", "tokens": ["Dein", "Geist", "be\u00b7h\u00e4lt", "noch", "sei\u00b7ne", "gan\u00b7ze", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Bleib hier: du sollst an meiner Seite sitzen,", "tokens": ["Bleib", "hier", ":", "du", "sollst", "an", "mei\u00b7ner", "Sei\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und wenn der Tod dich von uns ruft,", "tokens": ["Und", "wenn", "der", "Tod", "dich", "von", "uns", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So leg ich dich in meine F\u00fcrstengruft.", "tokens": ["So", "leg", "ich", "dich", "in", "mei\u00b7ne", "F\u00fcrs\u00b7ten\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ein Grab in deinen Pyramiden,", "tokens": ["Ein", "Grab", "in", "dei\u00b7nen", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Versetzt der Greis, ist auch ein Grab:", "tokens": ["Ver\u00b7setzt", "der", "Greis", ",", "ist", "auch", "ein", "Grab", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Das dunkle Thal, das mir mein Daseyn gab,", "tokens": ["Das", "dunk\u00b7le", "Thal", ",", "das", "mir", "mein", "Da\u00b7seyn", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Sey auch mein Ruheplatz hienieden.", "tokens": ["Sey", "auch", "mein", "Ru\u00b7he\u00b7platz", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der K\u00f6nig lie\u00df den Diener ungern ziehn,", "tokens": ["Der", "K\u00f6\u00b7nig", "lie\u00df", "den", "Die\u00b7ner", "un\u00b7gern", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Doch mu\u00dft er ihm zuletzt willfahren.", "tokens": ["Doch", "mu\u00dft", "er", "ihm", "zu\u00b7letzt", "will\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Er dachte schon nicht mehr an ihn,", "tokens": ["Er", "dach\u00b7te", "schon", "nicht", "mehr", "an", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Als er nach zwey verflo\u00dfnen Jahren", "tokens": ["Als", "er", "nach", "zwey", "ver\u00b7flo\u00df\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Einst m\u00fcde von der Jagd auf einen Sandstein sa\u00df,", "tokens": ["Einst", "m\u00fc\u00b7de", "von", "der", "Jagd", "auf", "ei\u00b7nen", "Sand\u00b7stein", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Auf dem er diese Worte las:", "tokens": ["Auf", "dem", "er", "die\u00b7se", "Wor\u00b7te", "las", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "\u00bbdas B\u00fcschchen, das auf diesem H\u00fcgel gr\u00fcnet,", "tokens": ["\u00bb", "das", "B\u00fc\u00b7schchen", ",", "das", "auf", "die\u00b7sem", "H\u00fc\u00b7gel", "gr\u00fc\u00b7net", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Erkohr ein Greis zu seiner Ruhestatt,", "tokens": ["Er\u00b7kohr", "ein", "Greis", "zu", "sei\u00b7ner", "Ru\u00b7he\u00b7statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der f\u00fcnfzig Jahre lang zween K\u00f6nigen gedienet", "tokens": ["Der", "f\u00fcnf\u00b7zig", "Jah\u00b7re", "lang", "zween", "K\u00f6\u00b7ni\u00b7gen", "ge\u00b7die\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "ADJD", "VVFIN", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und nur ein Jahr gelebet hat.\u00ab", "tokens": ["Und", "nur", "ein", "Jahr", "ge\u00b7le\u00b7bet", "hat", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Bruder! wann wirst du den Wunsch erf\u00fcllen,", "tokens": ["O", "Bru\u00b7der", "!", "wann", "wirst", "du", "den", "Wunsch", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den einzigen, f\u00fcr den mein Herz noch brennt,", "tokens": ["Den", "ein\u00b7zi\u00b7gen", ",", "f\u00fcr", "den", "mein", "Herz", "noch", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den s\u00fc\u00dfen Wunsch, der Tage Rest im Stillen,", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Wunsch", ",", "der", "Ta\u00b7ge", "Rest", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns selbst genug und ungetrennt,", "tokens": ["Uns", "selbst", "ge\u00b7nug", "und", "un\u00b7ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von unsrer Lieben Kreis umgeben,", "tokens": ["Von", "uns\u00b7rer", "Lie\u00b7ben", "Kreis", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In heitrer Ruhe durchzuleben?", "tokens": ["In", "hei\u00b7trer", "Ru\u00b7he", "durch\u00b7zu\u00b7le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Sommer schenktest du dem Vaterland,", "tokens": ["Den", "Som\u00b7mer", "schenk\u00b7test", "du", "dem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gieb uns den Herbst. Die strengen Parzen spinnen", "tokens": ["Gieb", "uns", "den", "Herbst", ".", "Die", "stren\u00b7gen", "Par\u00b7zen", "spin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mit schneller niemals m\u00fcder Hand", "tokens": ["Mit", "schnel\u00b7ler", "nie\u00b7mals", "m\u00fc\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Am d\u00fcnnen Rocken fort, und eh wir uns besinnen,", "tokens": ["Am", "d\u00fcn\u00b7nen", "Ro\u00b7cken", "fort", ",", "und", "eh", "wir", "uns", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,", "KON", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Rei\u00dft Atropos den Faden ab.", "tokens": ["Rei\u00dft", "A\u00b7tro\u00b7pos", "den", "Fa\u00b7den", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Drum komm, o komm, befl\u00fcgle deine Schritte!", "tokens": ["Drum", "komm", ",", "o", "komm", ",", "be\u00b7fl\u00fcg\u00b7le", "dei\u00b7ne", "Schrit\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Kein Trianon gleicht unsers Vaters H\u00fctte,", "tokens": ["Kein", "Tri\u00b7a\u00b7non", "gleicht", "un\u00b7sers", "Va\u00b7ters", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Kein Saint-Denys gleicht seinem Grab.", "tokens": ["Kein", "Saint\u00b7De