{"textgrid.poem.53220": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was f\u00fcr Unmut, Pein und Sorg", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was f\u00fcr Unmut, Pein und Sorg", "tokens": ["Was", "f\u00fcr", "Un\u00b7mut", ",", "Pein", "und", "Sorg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat dich, ", "tokens": ["Hat", "dich", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Schaw, dein F\u00fcrsten-Bild ", "tokens": ["Schaw", ",", "dein", "F\u00fcrs\u00b7ten\u00b7Bild"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ach, dein Hertzog, Haupt und Liecht", "tokens": ["Ach", ",", "dein", "Hert\u00b7zog", ",", "Haupt", "und", "Liecht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denckt nun an dich ewig nicht!", "tokens": ["Denckt", "nun", "an", "dich", "e\u00b7wig", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ADJD", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Recht, Begn\u00fcgung, Fried und Lust,", "tokens": ["Recht", ",", "Be\u00b7gn\u00fc\u00b7gung", ",", "Fried", "und", "Lust", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leget in den Staub euch nieder!", "tokens": ["Le\u00b7get", "in", "den", "Staub", "euch", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Thut euch weh, zerreisst die Brust,", "tokens": ["Thut", "euch", "weh", ",", "zer\u00b7reisst", "die", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Raufft das Haar, entbl\u00f6st die Glieder!", "tokens": ["Raufft", "das", "Haar", ",", "ent\u00b7bl\u00f6st", "die", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes f\u00fclle, wie es kan,", "tokens": ["Je\u00b7des", "f\u00fcl\u00b7le", ",", "wie", "es", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit Geschrey den Himmel an!", "tokens": ["Mit", "Ge\u00b7schrey", "den", "Him\u00b7mel", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Des entseelten F\u00fcrsten Rhue", "tokens": ["Des", "ent\u00b7seel\u00b7ten", "F\u00fcrs\u00b7ten", "Rhue"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4sst sich euch zum Beyspiel h\u00f6ren:", "tokens": ["L\u00e4sst", "sich", "euch", "zum", "Bey\u00b7spiel", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht nur, seht ", "tokens": ["Seht", "nur", ",", "seht"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Sie kan recht euch klagen lehren,", "tokens": ["Sie", "kan", "recht", "euch", "kla\u00b7gen", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wil auch, durch nicht schlechte Pein,", "tokens": ["Wil", "auch", ",", "durch", "nicht", "schlech\u00b7te", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "APPR", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcrstin der Betr\u00fcbten seyn.", "tokens": ["F\u00fcrs\u00b7tin", "der", "Be\u00b7tr\u00fcb\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAINF", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Kummer, Einsamkeit und Noht", "tokens": ["Kum\u00b7mer", ",", "Ein\u00b7sam\u00b7keit", "und", "Noht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben bey Ihr Platz gewonnen,", "tokens": ["Ha\u00b7ben", "bey", "Ihr", "Platz", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Umb Sie herbergt lauter Todt,", "tokens": ["Umb", "Sie", "her\u00b7bergt", "lau\u00b7ter", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie entsagt fast Mond und Sonnen,", "tokens": ["Sie", "ent\u00b7sagt", "fast", "Mond", "und", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spricht: was hab ich ihr Gewinn?", "tokens": ["Spricht", ":", "was", "hab", "ich", "ihr", "Ge\u00b7winn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWS", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Hertzens Sonn' ist hin.", "tokens": ["Mei\u00b7nes", "Hert\u00b7zens", "Sonn'", "ist", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darumb deckt Sie umb Sich her", "tokens": ["Da\u00b7rumb", "deckt", "Sie", "umb", "Sich", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PRF", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunckel, Grawen, Ungehewer,", "tokens": ["Dun\u00b7ckel", ",", "Gra\u00b7wen", ",", "Un\u00b7ge\u00b7he\u00b7wer", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Liebt zu Zeiten ohngefehr", "tokens": ["Liebt", "zu", "Zei\u00b7ten", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum ein kleines Schorstein-Fewer,", "tokens": ["Kaum", "ein", "klei\u00b7nes", "Schor\u00b7stein\u00b7Fe\u00b7wer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sagt: die Lebens-Funck in mir", "tokens": ["Sagt", ":", "die", "Le\u00b7bens\u00b7Funck", "in", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ART", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00fchrt, mein Herr, noch auch von Dir.", "tokens": ["R\u00fchrt", ",", "mein", "Herr", ",", "noch", "auch", "von", "Dir", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie bezeuget Erd und See,", "tokens": ["Sie", "be\u00b7zeu\u00b7get", "Erd", "und", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lufft und Himmel aller Enden,", "tokens": ["Lufft", "und", "Him\u00b7mel", "al\u00b7ler", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df Ihr Leid und Seelen-Weh", "tokens": ["Da\u00df", "Ihr", "Leid", "und", "See\u00b7len\u00b7Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich nicht k\u00f6nne lassen wenden,", "tokens": ["Sich", "nicht", "k\u00f6n\u00b7ne", "las\u00b7sen", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df Sie sey von aller Krafft", "tokens": ["Bi\u00df", "Sie", "sey", "von", "al\u00b7ler", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und zu Ihm werd hingerafft.", "tokens": ["Und", "zu", "Ihm", "werd", "hin\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Folgt mit h\u00e4\u00dflichem Geschrey!", "tokens": ["Folgt", "mit", "h\u00e4\u00df\u00b7li\u00b7chem", "Ge\u00b7schrey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heulet, da\u00df das Reich der Sternen", "tokens": ["Heu\u00b7let", ",", "da\u00df", "das", "Reich", "der", "Ster\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uber euch best\u00fcrtzet sey!", "tokens": ["U\u00b7ber", "euch", "be\u00b7st\u00fcrt\u00b7zet", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Merckt das Leid recht zu erlernen,", "tokens": ["Merckt", "das", "Leid", "recht", "zu", "er\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was die Heldin thut und sagt:", "tokens": ["Was", "die", "Hel\u00b7din", "thut", "und", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Er war uns in Sicherheit", "tokens": ["Er", "war", "uns", "in", "Si\u00b7cher\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu verwahren unverdro\u00dfen,", "tokens": ["Zu", "ver\u00b7wah\u00b7ren", "un\u00b7ver\u00b7dro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat verh\u00fctet M\u00fch und Streit,", "tokens": ["Hat", "ver\u00b7h\u00fc\u00b7tet", "M\u00fch", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie der Unschuld Blut vergossen,", "tokens": ["Nie", "der", "Un\u00b7schuld", "Blut", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er war Ursach, wie Ihr wisst,", "tokens": ["Er", "war", "Ur\u00b7sach", ",", "wie", "Ihr", "wisst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df noch Preussen Preussen ist.", "tokens": ["Da\u00df", "noch", "Preus\u00b7sen", "Preus\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun scheint alles die\u00df mit ", "tokens": ["Nun", "scheint", "al\u00b7les", "die\u00df", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PDS", "APPR"], "meter": "-+---+", "measure": "dactylic.init"}, "line.2": {"text": "Schlagt die Brust mit Ungest\u00fcm!", "tokens": ["Schlagt", "die", "Brust", "mit", "Un\u00b7ge\u00b7st\u00fcm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So recht, auch das Haar lasst fliegen,", "tokens": ["So", "recht", ",", "auch", "das", "Haar", "lasst", "flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geht aus Unmuth und Beschwer", "tokens": ["Geht", "aus", "Un\u00b7muth", "und", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Todten-Schatten gleich einher!", "tokens": ["Tod\u00b7ten\u00b7Schat\u00b7ten", "gleich", "ein\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Herr, wir thun uns nicht zu viel,", "tokens": ["Herr", ",", "wir", "thun", "uns", "nicht", "zu", "viel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKA", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher den Fug hat zu klagen", "tokens": ["Wel\u00b7cher", "den", "Fug", "hat", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ART", "NN", "VAFIN", "PTKZU", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Dem wird fast sein Leid ein Spiel,", "tokens": ["Dem", "wird", "fast", "sein", "Leid", "ein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Kan es nicht ohn Vortheil tragen.", "tokens": ["Kan", "es", "nicht", "ohn", "Vor\u00b7theil", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Verlust erheischt doch mehr,", "tokens": ["Dein", "Ver\u00b7lust", "er\u00b7heischt", "doch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kr\u00e4nckte man sich noch so sehr.", "tokens": ["Kr\u00e4nck\u00b7te", "man", "sich", "noch", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Was f\u00fcr Unmut, Pein und Sorg", "tokens": ["Was", "f\u00fcr", "Un\u00b7mut", ",", "Pein", "und", "Sorg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure"