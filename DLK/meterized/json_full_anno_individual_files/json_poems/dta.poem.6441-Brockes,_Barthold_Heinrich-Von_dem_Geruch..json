{"dta.poem.6441": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sp\u00fcret man, da\u00df sich bis ins Gehirne spielen", "tokens": ["So", "sp\u00fc\u00b7ret", "man", ",", "da\u00df", "sich", "bis", "ins", "Ge\u00b7hir\u00b7ne", "spie\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PRF", "APPR", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die leichten C\u00f6rperlein, so durch die L\u00fcffte eilen.", "tokens": ["Die", "leich\u00b7ten", "C\u00f6r\u00b7per\u00b7lein", ",", "so", "durch", "die", "L\u00fcff\u00b7te", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man riecht die S\u00fc\u00dfigkeit der B\u00e4ume, wenn sie bl\u00fchen.", "tokens": ["Man", "riecht", "die", "S\u00fc\u00b7\u00dfig\u00b7keit", "der", "B\u00e4u\u00b7me", ",", "wenn", "sie", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von ihren Fr\u00fcchten auch, die reiff geworden seyn,", "tokens": ["Von", "ih\u00b7ren", "Fr\u00fcch\u00b7ten", "auch", ",", "die", "reiff", "ge\u00b7wor\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "PRELS", "ADJD", "VAPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kan man den s\u00fcssen Dufft mit Anmuth an sich ziehen.", "tokens": ["Kan", "man", "den", "s\u00fcs\u00b7sen", "Dufft", "mit", "An\u00b7muth", "an", "sich", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir finden, da\u00df ein edler Wein,", "tokens": ["Wir", "fin\u00b7den", ",", "da\u00df", "ein", "ed\u00b7ler", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der unsrer Zunge lieblich schmecket,", "tokens": ["Der", "uns\u00b7rer", "Zun\u00b7ge", "lieb\u00b7lich", "schme\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In einem Dufft sich aus dem Glase strecket,", "tokens": ["In", "ei\u00b7nem", "Dufft", "sich", "aus", "dem", "Gla\u00b7se", "stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der unserm Geiste Lust erwecket.", "tokens": ["Der", "un\u00b7serm", "Geis\u00b7te", "Lust", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bevor noch der Geschmack ger\u00fchrt wird, macht ein Dufft,", "tokens": ["Be\u00b7vor", "noch", "der", "Ge\u00b7schmack", "ge\u00b7r\u00fchrt", "wird", ",", "macht", "ein", "Dufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der sehr subtil und zart, da\u00df jedermann", "tokens": ["Der", "sehr", "sub\u00b7til", "und", "zart", ",", "da\u00df", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Schon von desselben G\u00fct\u2019 ein Urtheil f\u00e4llen kan.", "tokens": ["Schon", "von", "des\u00b7sel\u00b7ben", "G\u00fct'", "ein", "Ur\u00b7theil", "f\u00e4l\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}