{"dta.poem.5841": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem wir also nun von der Materie,", "tokens": ["In\u00b7dem", "wir", "al\u00b7so", "nun", "von", "der", "Ma\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die grob und schwer, uns trennen;", "tokens": ["Die", "grob", "und", "schwer", ",", "uns", "tren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So werden wir auch droben in der H\u00f6h\u2019,", "tokens": ["So", "wer\u00b7den", "wir", "auch", "dro\u00b7ben", "in", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Uns andre Wege bahnen k\u00f6nnen.", "tokens": ["Uns", "and\u00b7re", "We\u00b7ge", "bah\u00b7nen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch thut man wol, wenn man die feste Regel lernt:", "tokens": ["Doch", "thut", "man", "wol", ",", "wenn", "man", "die", "fes\u00b7te", "Re\u00b7gel", "lernt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "$,", "KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewer der Natur Geheimni\u00df will ergr\u00fcnden,", "tokens": ["\u201e", "wer", "der", "Na\u00b7tur", "Ge\u00b7heim\u00b7ni\u00df", "will", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "\u201ewird stets die besten Wege finden,", "tokens": ["\u201e", "wird", "stets", "die", "bes\u00b7ten", "We\u00b7ge", "fin\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u201edurch die, so nah, zu denen, die entfernt.", "tokens": ["\u201e", "durch", "die", ",", "so", "nah", ",", "zu", "de\u00b7nen", ",", "die", "ent\u00b7fernt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "$,", "ADV", "ADJD", "$,", "APPR", "PRELS", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "\u201elasst uns ein sicher Urtheil f\u00e4llen", "tokens": ["\u201e", "lasst", "uns", "ein", "si\u00b7cher", "Ur\u00b7theil", "f\u00e4l\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "\u201edurch die Bewegungen, die wir hier unten sehn,", "tokens": ["\u201e", "durch", "die", "Be\u00b7we\u00b7gun\u00b7gen", ",", "die", "wir", "hier", "un\u00b7ten", "sehn", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "\u201evon denen, die im Firmament geschehn.", "tokens": ["\u201e", "von", "de\u00b7nen", ",", "die", "im", "Fir\u00b7ma\u00b7ment", "ge\u00b7schehn", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "$,", "PRELS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u201enach Dingen, die wir sehen, richte man", "tokens": ["\u201e", "nach", "Din\u00b7gen", ",", "die", "wir", "se\u00b7hen", ",", "rich\u00b7te", "man"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "\u201ediejenigen, so man nicht sehen kan.", "tokens": ["\u201e", "die\u00b7je\u00b7ni\u00b7gen", ",", "so", "man", "nicht", "se\u00b7hen", "kan", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "$,", "ADV", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}