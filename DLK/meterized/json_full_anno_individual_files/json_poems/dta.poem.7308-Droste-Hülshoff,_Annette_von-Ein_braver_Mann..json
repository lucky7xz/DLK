{"dta.poem.7308": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ein braver Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Noch lag, ein Wetterbrodem, schwer", "tokens": ["Noch", "lag", ",", "ein", "Wet\u00b7ter\u00b7bro\u00b7dem", ",", "schwer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tyrannei auf Deutschlands Gauen,", "tokens": ["Die", "Ty\u00b7ran\u00b7nei", "auf", "Deutschlands", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Wachen schlichen scheu umher,", "tokens": ["Die", "Wa\u00b7chen", "schli\u00b7chen", "scheu", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menge schlief in dumpfem Grauen;", "tokens": ["Die", "Men\u00b7ge", "schlief", "in", "dum\u00b7pfem", "Grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Seufzer schien der Morgenwind", "tokens": ["Ein", "Seuf\u00b7zer", "schien", "der", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus angstgepre\u00dfter Brust zu brechen;", "tokens": ["Aus", "angst\u00b7ge\u00b7pre\u00df\u00b7ter", "Brust", "zu", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur die Kanone durfte sprechen", "tokens": ["Nur", "die", "Ka\u00b7no\u00b7ne", "durf\u00b7te", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und l\u00e4cheln durfte nur das Kind.", "tokens": ["Und", "l\u00e4\u00b7cheln", "durf\u00b7te", "nur", "das", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da lebt' im Frankenland ein Mann,", "tokens": ["Da", "lebt'", "im", "Fran\u00b7ken\u00b7land", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der bittre Stunden schon getragen,", "tokens": ["Der", "bitt\u00b7re", "Stun\u00b7den", "schon", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dr\u00e4ngenden Geschickes Bann", "tokens": ["In", "dr\u00e4n\u00b7gen\u00b7den", "Ge\u00b7schi\u00b7ckes", "Bann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar manche T\u00e4uschung sonder Klagen;", "tokens": ["Gar", "man\u00b7che", "T\u00e4u\u00b7schung", "son\u00b7der", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm war von seiner Ahnen Flur", "tokens": ["Ihm", "war", "von", "sei\u00b7ner", "Ah\u00b7nen", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der edle Name nur geblieben,", "tokens": ["Der", "ed\u00b7le", "Na\u00b7me", "nur", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von allen, allen Jugendtrieben", "tokens": ["Von", "al\u00b7len", ",", "al\u00b7len", "Ju\u00b7gend\u00b7trie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Herzens warm Gedenken nur.", "tokens": ["Des", "Her\u00b7zens", "warm", "Ge\u00b7den\u00b7ken", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch fr\u00fches Siechthum schwer gebeugt", "tokens": ["Durch", "fr\u00fc\u00b7hes", "Siech\u00b7thum", "schwer", "ge\u00b7beugt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeglichem Beruf verdorben,", "tokens": ["Und", "jeg\u00b7li\u00b7chem", "Be\u00b7ruf", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4tt' oft er gern das Haupt geneigt", "tokens": ["H\u00e4tt'", "oft", "er", "gern", "das", "Haupt", "ge\u00b7neigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4r' in Frieden nur gestorben;", "tokens": ["Und", "w\u00e4r'", "in", "Frie\u00b7den", "nur", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An seinen Schl\u00e4fen lagen schon", "tokens": ["An", "sei\u00b7nen", "Schl\u00e4\u00b7fen", "la\u00b7gen", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit vierzig Jahren wei\u00dfe Garben,", "tokens": ["Mit", "vier\u00b7zig", "Jah\u00b7ren", "wei\u00b7\u00dfe", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und seiner Z\u00fcge tiefe Narben", "tokens": ["Und", "sei\u00b7ner", "Z\u00fc\u00b7ge", "tie\u00b7fe", "Nar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verriethen steter Sorge Frohn.", "tokens": ["Ver\u00b7rie\u00b7then", "ste\u00b7ter", "Sor\u00b7ge", "Frohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch freundlich trug er jeden Dorn,", "tokens": ["Doch", "freund\u00b7lich", "trug", "er", "je\u00b7den", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der auf dem Pfade ihm begegnet,", "tokens": ["Der", "auf", "dem", "Pfa\u00b7de", "ihm", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geschlagen von des Schicksals Zorn,", "tokens": ["Ge\u00b7schla\u00b7gen", "von", "des", "Schick\u00b7sals", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch von der G\u00f6tter Hand gesegnet.", "tokens": ["Doch", "von", "der", "G\u00f6t\u00b7ter", "Hand", "ge\u00b7seg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und eine Kunst war ihm bescheert,", "tokens": ["Und", "ei\u00b7ne", "Kunst", "war", "ihm", "be\u00b7scheert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So mild wie seiner Seele Hauchen,", "tokens": ["So", "mild", "wie", "sei\u00b7ner", "See\u00b7le", "Hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Pinsel lie\u00df die Wiesen rauchen", "tokens": ["Sein", "Pin\u00b7sel", "lie\u00df", "die", "Wie\u00b7sen", "rau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und flammen des Vulkanes Heerd.", "tokens": ["Und", "flam\u00b7men", "des", "Vul\u00b7ka\u00b7nes", "Heerd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Es waren Bilder die mit Lust", "tokens": ["Es", "wa\u00b7ren", "Bil\u00b7der", "die", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "ART", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein unverdorbnes Herz erf\u00fcllen,", "tokens": ["Ein", "un\u00b7ver\u00b7dorb\u00b7nes", "Herz", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie entsteigen warmer Brust", "tokens": ["Wie", "sie", "ent\u00b7stei\u00b7gen", "war\u00b7mer", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und reiner Phantasie entquillen;", "tokens": ["Und", "rei\u00b7ner", "Phan\u00b7ta\u00b7sie", "ent\u00b7quil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch M\u00e4cklern schienen sie zu zart,", "tokens": ["Doch", "M\u00e4ck\u00b7lern", "schie\u00b7nen", "sie", "zu", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Stempel hoher Kunst zu tragen:", "tokens": ["Den", "Stem\u00b7pel", "ho\u00b7her", "Kunst", "zu", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So hat er schwer sich durchgeschlagen", "tokens": ["So", "hat", "er", "schwer", "sich", "durch\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und t\u00e4glich am Bedarf gespart.", "tokens": ["Und", "t\u00e4g\u00b7lich", "am", "Be\u00b7darf", "ge\u00b7spart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da ward in Winterabends Lauf", "tokens": ["Da", "ward", "in", "Win\u00b7ter\u00b7a\u00b7bends", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Brief ihm von der Post gesendet;", "tokens": ["Ein", "Brief", "ihm", "von", "der", "Post", "ge\u00b7sen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er ri\u00df best\u00fcrzt das Siegel auf:", "tokens": ["Er", "ri\u00df", "be\u00b7st\u00fcrzt", "das", "Sie\u00b7gel", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Gott, die Sorgen sind beendet!", "tokens": ["O", "Gott", ",", "die", "Sor\u00b7gen", "sind", "be\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des fernen Vetters Todtenschein", "tokens": ["Des", "fer\u00b7nen", "Vet\u00b7ters", "Tod\u00b7ten\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat als Agnaten ihn berufen,", "tokens": ["Hat", "als", "Ag\u00b7na\u00b7ten", "ihn", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er darf nur treten an die Stufen,", "tokens": ["Er", "darf", "nur", "tre\u00b7ten", "an", "die", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die reichen Lehne harren sein!", "tokens": ["Die", "rei\u00b7chen", "Leh\u00b7ne", "har\u00b7ren", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer denkt es nicht, da\u00df ihm gepre\u00dft", "tokens": ["Wer", "denkt", "es", "nicht", ",", "da\u00df", "ihm", "ge\u00b7pre\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus hei\u00dfer Wimper Thr\u00e4nen flossen!", "tokens": ["Aus", "hei\u00b7\u00dfer", "Wim\u00b7per", "Thr\u00e4\u00b7nen", "flos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann pl\u00f6tzlich steht sein Auge fest,", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", "steht", "sein", "Au\u00b7ge", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Z\u00e4hren Quelle ist geschlossen.", "tokens": ["Der", "Z\u00e4h\u00b7ren", "Quel\u00b7le", "ist", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er liest, er tunkt die Feder ein,", "tokens": ["Er", "liest", ",", "er", "tunkt", "die", "Fe\u00b7der", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat nur Sekunden sich berathen,", "tokens": ["Hat", "nur", "Se\u00b7kun\u00b7den", "sich", "be\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und an den n\u00e4chsten Lehnsagnaten", "tokens": ["Und", "an", "den", "n\u00e4chs\u00b7ten", "Lehn\u00b7sag\u00b7na\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schreibt muthig er beim Lampenschein:", "tokens": ["Schreibt", "mut\u00b7hig", "er", "beim", "Lam\u00b7pen\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201ewohl sagt man, da\u00df Tyrannenmacht", "tokens": ["\u201e", "wohl", "sagt", "man", ",", "da\u00df", "Ty\u00b7ran\u00b7nen\u00b7macht"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PIS", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Eides ", "tokens": ["Nicht", "Ei\u00b7des"], "token_info": ["word", "word"], "pos": ["PTKNEG", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch wo in uns ein Zweifel wacht,", "tokens": ["Doch", "wo", "in", "uns", "ein", "Zwei\u00b7fel", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da m\u00fcssen wir zum Besten ringen.", "tokens": ["Da", "m\u00fcs\u00b7sen", "wir", "zum", "Bes\u00b7ten", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nimm hin der V\u00e4ter liebes Schlo\u00df,", "tokens": ["Nimm", "hin", "der", "V\u00e4\u00b7ter", "lie\u00b7bes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u2014 O w\u00fcrd' ich einstens dort begraben! \u2014", "tokens": ["O", "w\u00fcrd'", "ich", "eins\u00b7tens", "dort", "be\u00b7gra\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ich bin gew\u00f6hnt nicht viel zu haben,", "tokens": ["Ich", "bin", "ge\u00b7w\u00f6hnt", "nicht", "viel", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PTKNEG", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und mein Bed\u00fcrfni\u00df ist nicht gro\u00df.\u201c", "tokens": ["Und", "mein", "Be\u00b7d\u00fcrf\u00b7ni\u00df", "ist", "nicht", "gro\u00df", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer unter Euch von Opfern spricht,", "tokens": ["Wer", "un\u00b7ter", "Euch", "von", "Op\u00b7fern", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von edleren, und M\u00e4rtrerzeichen,", "tokens": ["Von", "ed\u00b7le\u00b7ren", ",", "und", "M\u00e4r\u00b7trer\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der sah gewi\u00df noch Jahre nicht,", "tokens": ["Der", "sah", "ge\u00b7wi\u00df", "noch", "Jah\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht vierzig Jahr in Sorg' entschleichen!", "tokens": ["Nicht", "vier\u00b7zig", "Jahr", "in", "Sor\u00b7g'", "ent\u00b7schlei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "CARD", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ihr die mit St\u00e4rke prunkt und gleich", "tokens": ["Ihr", "die", "mit", "St\u00e4r\u00b7ke", "prunkt", "und", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "APPR", "NN", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch dr\u00e4ngt zu stolzer Thaten Weihe:", "tokens": ["Euch", "dr\u00e4ngt", "zu", "stol\u00b7zer", "Tha\u00b7ten", "Wei\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u2014 Er war ein Mann wie Wachs so weich,", "tokens": ["Er", "war", "ein", "Mann", "wie", "Wachs", "so", "weich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "KOKOM", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur stark in Gott und seiner Treue.", "tokens": ["Nur", "stark", "in", "Gott", "und", "sei\u00b7ner", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und wie es ferner ihm erging?", "tokens": ["Und", "wie", "es", "fer\u00b7ner", "ihm", "er\u00b7ging", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat gemalt bis er gestorben,", "tokens": ["Er", "hat", "ge\u00b7malt", "bis", "er", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zuletzt, in langer Jahre Ring,", "tokens": ["Zu\u00b7letzt", ",", "in", "lan\u00b7ger", "Jah\u00b7re", "Ring", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schmal Verm\u00f6gen sich erworben;", "tokens": ["Ein", "schmal", "Ver\u00b7m\u00f6\u00b7gen", "sich", "er\u00b7wor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nie hat auf der Begeistrung H\u00f6h'", "tokens": ["Nie", "hat", "auf", "der", "Be\u00b7geis\u00b7trung", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Sein schamhaft Schweigen er gebrochen,", "tokens": ["Sein", "scham\u00b7haft", "Schwei\u00b7gen", "er", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und keine Seele hat gesprochen", "tokens": ["Und", "kei\u00b7ne", "See\u00b7le", "hat", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von seinem schweren Opfer je.", "tokens": ["Von", "sei\u00b7nem", "schwe\u00b7ren", "Op\u00b7fer", "je", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Zweimal im Leben gab das Gl\u00fcck", "tokens": ["Zwei\u00b7mal", "im", "Le\u00b7ben", "gab", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vor seinem Antlitz mir zu stehen,", "tokens": ["Vor", "sei\u00b7nem", "Ant\u00b7litz", "mir", "zu", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In seinem mild bescheidnen Blick", "tokens": ["In", "sei\u00b7nem", "mild", "be\u00b7scheid\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Geistes reinen Blitz zu sehen.", "tokens": ["Des", "Geis\u00b7tes", "rei\u00b7nen", "Blitz", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und im December hat man dann", "tokens": ["Und", "im", "De\u00b7cem\u00b7ber", "hat", "man", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Sarges Deckel zugeschlagen", "tokens": ["Des", "Sar\u00b7ges", "De\u00b7ckel", "zu\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und still ihn in die Gruft getragen.", "tokens": ["Und", "still", "ihn", "in", "die", "Gruft", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u2014 Das ist das Lied vom braven Mann.", "tokens": ["Das", "ist", "das", "Lied", "vom", "bra\u00b7ven", "Mann", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}