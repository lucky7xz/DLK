{"dta.poem.18145": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  vier Jahrszeiten,  \n als ein sinnliches Lehrbild, des Lebens,  \n Todes, und der Auferstehung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah im vorgen Jahr, als uns die", "tokens": ["Ich", "sah", "im", "vor\u00b7gen", "Jahr", ",", "als", "uns", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sommerzeit,", "tokens": ["Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mit ihrer schw\u00fclen Hitz, die", "tokens": ["Mit", "ih\u00b7rer", "schw\u00fc\u00b7len", "Hitz", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fr\u00fcchte meist gereifet,", "tokens": ["Fr\u00fcch\u00b7te", "meist", "ge\u00b7rei\u00b7fet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wie schon ein grosses Theil der", "tokens": ["Wie", "schon", "ein", "gros\u00b7ses", "Theil", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "bunten Herrligkeit", "tokens": ["bun\u00b7ten", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Jm Reiche der Natur, die Farben", "tokens": ["Jm", "Rei\u00b7che", "der", "Na\u00b7tur", ",", "die", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NE", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "abgestreifet.", "tokens": ["ab\u00b7ge\u00b7strei\u00b7fet", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Die zarten Kinderchen, die man im G\u00e4rten sicht,", "tokens": ["Die", "zar\u00b7ten", "Kin\u00b7der\u00b7chen", ",", "die", "man", "im", "G\u00e4r\u00b7ten", "sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die Blumen Lillien, die Tulpen, Rosen, Nelken", "tokens": ["Die", "Blu\u00b7men", "Lil\u00b7li\u00b7en", ",", "die", "Tul\u00b7pen", ",", "Ro\u00b7sen", ",", "Nel\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die hatten kurtze Zeit, in ihrer Pracht gebl\u00fcht,", "tokens": ["Die", "hat\u00b7ten", "kurt\u00b7ze", "Zeit", ",", "in", "ih\u00b7rer", "Pracht", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und schienen falb und blas, mit ihren Schmuk zu welken.", "tokens": ["Und", "schie\u00b7nen", "falb", "und", "blas", ",", "mit", "ih\u00b7ren", "Schmuk", "zu", "wel\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich sah ihr Sterben an, das sich dem Augen wies", "tokens": ["Ich", "sah", "ihr", "Ster\u00b7ben", "an", ",", "das", "sich", "dem", "Au\u00b7gen", "wies"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und dachte, wie kan das zu einem Lehrbild dienen?", "tokens": ["Und", "dach\u00b7te", ",", "wie", "kan", "das", "zu", "ei\u00b7nem", "Lehr\u00b7bild", "die\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "VMFIN", "PDS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Garten f\u00fchrte mich aufs Ehe Paradies,", "tokens": ["Der", "Gar\u00b7ten", "f\u00fchr\u00b7te", "mich", "aufs", "E\u00b7he", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Worin die Kinder auch, zur Eltern Freude gr\u00fcnen.", "tokens": ["Wo\u00b7rin", "die", "Kin\u00b7der", "auch", ",", "zur", "El\u00b7tern", "Freu\u00b7de", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "$,", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So wie die Bl\u00fcmelein, offt eh mans meint vergehn,", "tokens": ["So", "wie", "die", "Bl\u00fc\u00b7me\u00b7lein", ",", "offt", "eh", "mans", "meint", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "ADV", "KOUS", "PIS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durch einen Sonnen Stich, in ihrer Bl\u00fct verderben;", "tokens": ["Durch", "ei\u00b7nen", "Son\u00b7nen", "Stich", ",", "in", "ih\u00b7rer", "Bl\u00fct", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So kan man t\u00e4glich auch, im Ehestande sehn;", "tokens": ["So", "kan", "man", "t\u00e4g\u00b7lich", "auch", ",", "im", "E\u00b7hes\u00b7tan\u00b7de", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "ADV", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Eitelkeit der Lust, an vieler Kinder Sterben.", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "der", "Lust", ",", "an", "vie\u00b7ler", "Kin\u00b7der", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dies lehrte mich hernach, ein gantz besamtes Feld,", "tokens": ["Dies", "lehr\u00b7te", "mich", "her\u00b7nach", ",", "ein", "gantz", "be\u00b7sam\u00b7tes", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das ich vor kurtzen noch im gr\u00fcnen Wuchs erblikket,", "tokens": ["Das", "ich", "vor", "kurt\u00b7zen", "noch", "im", "gr\u00fc\u00b7nen", "Wuchs", "er\u00b7blik\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es war die gr\u00fcne Tracht der Halmen schon verstellt,", "tokens": ["Es", "war", "die", "gr\u00fc\u00b7ne", "Tracht", "der", "Hal\u00b7men", "schon", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die bleiche Todtenfarb daran schon abgedr\u00fccket.", "tokens": ["Die", "blei\u00b7che", "Tod\u00b7ten\u00b7farb", "da\u00b7ran", "schon", "ab\u00b7ge\u00b7dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich ging einst wieder hin, da lies ein Schnitter Heer,", "tokens": ["Ich", "ging", "einst", "wie\u00b7der", "hin", ",", "da", "lies", "ein", "Schnit\u00b7ter", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In der bewegten Faust, schon ihre Sensen blinken;", "tokens": ["In", "der", "be\u00b7weg\u00b7ten", "Faust", ",", "schon", "ih\u00b7re", "Sen\u00b7sen", "blin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich sah die schlanke Meug der Halmen mehr und mehr", "tokens": ["Ich", "sah", "die", "schlan\u00b7ke", "Meug", "der", "Hal\u00b7men", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bey wiederhohlten Schlag gest\u00fcrtzt zu Boden sinken.", "tokens": ["Bey", "wie\u00b7der\u00b7hohl\u00b7ten", "Schlag", "ge\u00b7st\u00fcrtzt", "zu", "Bo\u00b7den", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hier dacht ich bey mir selbst: da f\u00e4llt die schlanke", "tokens": ["Hier", "dacht", "ich", "bey", "mir", "selbst", ":", "da", "f\u00e4llt", "die", "schlan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$.", "ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Pracht", "tokens": ["Pracht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Der fetten Akker Frucht mit ihren g\u00fcldnen Aehren;", "tokens": ["Der", "fet\u00b7ten", "Ak\u00b7ker", "Frucht", "mit", "ih\u00b7ren", "g\u00fcld\u00b7nen", "A\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Ein Schlag, ein Schnitt, ein Zug, hats hier schon kahl", "tokens": ["Ein", "Schlag", ",", "ein", "Schnitt", ",", "ein", "Zug", ",", "hats", "hier", "schon", "kahl"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "gemacht", "tokens": ["ge\u00b7macht"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+", "measure": "iambic.single"}, "line.34": {"text": "Dies Sinnbild kan mir auch, der Menschen Zustand", "tokens": ["Dies", "Sinn\u00b7bild", "kan", "mir", "auch", ",", "der", "Men\u00b7schen", "Zu\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "VMFIN", "PPER", "ADV", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "lehren.", "tokens": ["leh\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Wie viele liefert nicht, des Todes Sensenschlag,", "tokens": ["Wie", "vie\u00b7le", "lie\u00b7fert", "nicht", ",", "des", "To\u00b7des", "Sen\u00b7sen\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PTKNEG", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ehs Jahr den Kreis uml\u00e4uft, im Sommer ihrer Jahre", "tokens": ["Ehs", "Jahr", "den", "Kreis", "um\u00b7l\u00e4uft", ",", "im", "Som\u00b7mer", "ih\u00b7rer", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ja! wen der Mars regiert, woll gar auf einem Tag,", "tokens": ["Ja", "!", "wen", "der", "Mars", "re\u00b7giert", ",", "woll", "gar", "auf", "ei\u00b7nem", "Tag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Als unverhofft entseelt, auf ihre Leichenbahre?", "tokens": ["Als", "un\u00b7ver\u00b7hofft", "ent\u00b7seelt", ",", "auf", "ih\u00b7re", "Lei\u00b7chen\u00b7bah\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der Herbst kam endlich an, mit seinem rauhen Nord,", "tokens": ["Der", "Herbst", "kam", "end\u00b7lich", "an", ",", "mit", "sei\u00b7nem", "rau\u00b7hen", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und lies den kalten Hauch auch auf die B\u00e4ume rasen,", "tokens": ["Und", "lies", "den", "kal\u00b7ten", "Hauch", "auch", "auf", "die", "B\u00e4u\u00b7me", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die Fr\u00fcchte fielen hin, die Bl\u00e4tter musten fort,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "fie\u00b7len", "hin", ",", "die", "Bl\u00e4t\u00b7ter", "mus\u00b7ten", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und wurden von dem Wind, verwelket weggeblasen.", "tokens": ["Und", "wur\u00b7den", "von", "dem", "Wind", ",", "ver\u00b7wel\u00b7ket", "weg\u00b7ge\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "$,", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ach! fiel mir dabey ein: das ist ein Sinnenspiel", "tokens": ["Ach", "!", "fiel", "mir", "da\u00b7bey", "ein", ":", "das", "ist", "ein", "Sin\u00b7nen\u00b7spiel"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PAV", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Von denen, welche GOTT zum Alter hat erhalten;", "tokens": ["Von", "de\u00b7nen", ",", "wel\u00b7che", "GoTT", "zum", "Al\u00b7ter", "hat", "er\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "NE", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Der St\u00e4rkste mu\u00df davon, es kommt sein Lebens Ziel", "tokens": ["Der", "St\u00e4rks\u00b7t