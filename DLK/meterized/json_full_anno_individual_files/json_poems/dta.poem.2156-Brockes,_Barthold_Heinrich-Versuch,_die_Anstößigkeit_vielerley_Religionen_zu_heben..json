{"dta.poem.2156": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Versuch, die Anst\u00f6\u00dfigkeit vielerley  \n Religionen zu heben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Denn es gab auch unter ihnen, die es besser nicht ver-", "tokens": ["Denn", "es", "gab", "auch", "un\u00b7ter", "ih\u00b7nen", ",", "die", "es", "bes\u00b7ser", "nicht", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "PRELS", "PPER", "ADJD", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "stunden.", "tokens": ["stun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Haben aber etliche sie, aus Geiz und Stolz, erdacht;", "tokens": ["Ha\u00b7ben", "a\u00b7ber", "et\u00b7li\u00b7che", "sie", ",", "aus", "Geiz", "und", "Stolz", ",", "er\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,", "VVPP", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Haben sie vom wahren Gott das geringste nicht ge-", "tokens": ["Ha\u00b7ben", "sie", "vom", "wah\u00b7ren", "Gott", "das", "ge\u00b7rings\u00b7te", "nicht", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "PTKNEG", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "macht,", "tokens": ["macht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und gar keinen Gott geglaubet: Denn kein Mensch ist so", "tokens": ["Und", "gar", "kei\u00b7nen", "Gott", "ge\u00b7glau\u00b7bet", ":", "Denn", "kein", "Mensch", "ist", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "VVPP", "$.", "KON", "PIAT", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "verwegen,", "tokens": ["ver\u00b7we\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Da\u00df er gegen Gott, den er als Gott kennt, sich sollte", "tokens": ["Da\u00df", "er", "ge\u00b7gen", "Gott", ",", "den", "er", "als", "Gott", "kennt", ",", "sich", "soll\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$,", "PRF", "VMFIN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "legen.", "tokens": ["le\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Da die Menschheit denn hierinn sich aus Bosheit nicht", "tokens": ["Da", "die", "Menschheit", "denn", "hie\u00b7rinn", "sich", "aus", "Bos\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ADV", "PRF", "APPR", "NN", "PTKNEG"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "verschuldet,", "tokens": ["ver\u00b7schul\u00b7det", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Sondern sie, aus Einfalt blo\u00df, Gott so klein sich vor-", "tokens": ["Son\u00b7dern", "sie", ",", "aus", "Ein\u00b7falt", "blo\u00df", ",", "Gott", "so", "klein", "sich", "vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "ADV", "$,", "NN", "ADV", "ADJD", "PRF", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "gestellt;", "tokens": ["ge\u00b7stellt", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Ist vielleicht die\u00df eine Ursach, da\u00df der Sch\u00f6pfer in der", "tokens": ["Ist", "viel\u00b7leicht", "die\u00df", "ei\u00b7ne", "Ur\u00b7sach", ",", "da\u00df", "der", "Sch\u00f6p\u00b7fer", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PDS", "ART", "NN", "$,", "KOUS", "ART", "NN", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Welt", "tokens": ["Welt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Vielerley Religionen leidet, und, aus Langmuth, dul-", "tokens": ["Vie\u00b7ler\u00b7ley", "Re\u00b7li\u00b7gi\u00b7o\u00b7nen", "lei\u00b7det", ",", "und", ",", "aus", "Lang\u00b7muth", ",", "dul"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "$,", "APPR", "NN", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "det.", "tokens": ["det", "."], "token_info": ["word", "punct"], "pos": ["PDS", "$."], "meter": "-", "measure": "single.down"}}}}}