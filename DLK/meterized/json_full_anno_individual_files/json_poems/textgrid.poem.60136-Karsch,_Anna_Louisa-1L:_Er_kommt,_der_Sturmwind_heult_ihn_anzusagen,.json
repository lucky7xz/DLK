{"textgrid.poem.60136": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er kommt, der Sturmwind heult ihn anzusagen,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kommt, der Sturmwind heult ihn anzusagen,", "tokens": ["Er", "kommt", ",", "der", "Sturm\u00b7wind", "heult", "ihn", "an\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verh\u00fcllt in dicker Mitternacht,", "tokens": ["Ver\u00b7h\u00fcllt", "in", "di\u00b7cker", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf dreytausend Feuerwagen", "tokens": ["Und", "auf", "drey\u00b7tau\u00b7send", "Feu\u00b7er\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu uns herabgebracht!", "tokens": ["Zu", "uns", "her\u00b7ab\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Izt ist er da; der Herr des Weltgeb\u00e4udes!", "tokens": ["Izt", "ist", "er", "da", ";", "der", "Herr", "des", "Welt\u00b7ge\u00b7b\u00e4u\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt ihn! sein Donner rollet schwer;", "tokens": ["H\u00f6rt", "ihn", "!", "sein", "Don\u00b7ner", "rol\u00b7let", "schwer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Umfang seines Wolkenkleides", "tokens": ["Der", "Um\u00b7fang", "sei\u00b7nes", "Wol\u00b7ken\u00b7klei\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blizt Schrecken auf uns her.", "tokens": ["Blizt", "Schre\u00b7cken", "auf", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Welch ein Geprassel! kommen seine Krieger", "tokens": ["Welch", "ein", "Ge\u00b7pras\u00b7sel", "!", "kom\u00b7men", "sei\u00b7ne", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihm dahergefahren, so,", "tokens": ["Mit", "ihm", "da\u00b7her\u00b7ge\u00b7fah\u00b7ren", ",", "so", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie zu der Schlacht, da vor dem Sieger", "tokens": ["Wie", "zu", "der", "Schlacht", ",", "da", "vor", "dem", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das H\u00f6llenheer entfloh?", "tokens": ["Das", "H\u00f6l\u00b7len\u00b7heer", "ent\u00b7floh", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Izt st\u00fcrzen ganze Str\u00f6me Kugeln nieder;", "tokens": ["Izt", "st\u00fcr\u00b7zen", "gan\u00b7ze", "Str\u00f6\u00b7me", "Ku\u00b7geln", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gott schl\u00e4gt den Weinstock, schl\u00e4gt die Frucht", "tokens": ["Gott", "schl\u00e4gt", "den", "Wein\u00b7stock", ",", "schl\u00e4gt", "die", "Frucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Baums, der wankend seine Glieder,", "tokens": ["Des", "Baums", ",", "der", "wan\u00b7kend", "sei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerri\u00dfne Aeste, sucht.", "tokens": ["Zer\u00b7ri\u00df\u00b7ne", "A\u00b7es\u00b7te", ",", "sucht", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Der Hagel rauscht und weckt die Trunkenbolde,", "tokens": ["Der", "Ha\u00b7gel", "rauscht", "und", "weckt", "die", "Trun\u00b7ken\u00b7bol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie fahren auf, und stammeln: Gott!", "tokens": ["Sie", "fah\u00b7ren", "auf", ",", "und", "stam\u00b7meln", ":", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wuchrer zittert auf dem Golde;", "tokens": ["Der", "Wuch\u00b7rer", "zit\u00b7tert", "auf", "dem", "Gol\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Freygeist wird sein Spott", "tokens": ["Dem", "Frey\u00b7geist", "wird", "sein", "Spott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Von f\u00fcrchterlichen Rednern wiedersprochen;", "tokens": ["Von", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Red\u00b7nern", "wie\u00b7der\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gott sagt im Donner, wer er sey!", "tokens": ["Gott", "sagt", "im", "Don\u00b7ner", ",", "wer", "er", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00e4hrt an S\u00fcndern, ungerochen,", "tokens": ["Und", "f\u00e4hrt", "an", "S\u00fcn\u00b7dern", ",", "un\u00b7ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Brausen stark vorbey!", "tokens": ["Im", "Brau\u00b7sen", "stark", "vor\u00b7bey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gieb acht, Berlin! sein Zorn sezt, dir zu drohen,", "tokens": ["Gieb", "acht", ",", "Ber\u00b7lin", "!", "sein", "Zorn", "sezt", ",", "dir", "zu", "dro\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "NE", "$.", "PPOSAT", "NN", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein Dorf mit Blitzen in den Brand!", "tokens": ["Ein", "Dorf", "mit", "Blit\u00b7zen", "in", "den", "Brand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Glut warf er nieder; nackend flohen,", "tokens": ["Glut", "warf", "er", "nie\u00b7der", ";", "na\u00b7ckend", "flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ihr Leben in der Hand", "tokens": ["Ihr", "Le\u00b7ben", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Behaltend, aus den H\u00fctten die Bewohner;", "tokens": ["Be\u00b7hal\u00b7tend", ",", "aus", "den", "H\u00fct\u00b7ten", "die", "Be\u00b7woh\u00b7ner", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Kleid, ihr Brod wird aufgezehrt:", "tokens": ["Ihr", "Kleid", ",", "ihr", "Brod", "wird", "auf\u00b7ge\u00b7zehrt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dich, dich findet der Verschoner", "tokens": ["Und", "dich", ",", "dich", "fin\u00b7det", "der", "Ver\u00b7scho\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch seiner Nachsicht werth.", "tokens": ["Noch", "sei\u00b7ner", "Nach\u00b7sicht", "werth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O! unter den von Stroh geflochtnen D\u00e4chern,", "tokens": ["O", "!", "un\u00b7ter", "den", "von", "Stroh", "ge\u00b7flocht\u00b7nen", "D\u00e4\u00b7chern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ART", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohnt minder Bosheit, als in dir!", "tokens": ["Wohnt", "min\u00b7der", "Bos\u00b7heit", ",", "als", "in", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "KOUS", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sagts, ihr Pall\u00e4ste! den Verbrechern:", "tokens": ["Sagts", ",", "ihr", "Pal\u00b7l\u00e4s\u00b7te", "!", "den", "Ver\u00b7bre\u00b7chern", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott war im Wetter hier!", "tokens": ["Gott", "war", "im", "Wet\u00b7ter", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da bebten unsre W\u00e4nde; unsre Riegel,", "tokens": ["Da", "beb\u00b7ten", "uns\u00b7re", "W\u00e4n\u00b7de", ";", "uns\u00b7re", "Rie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Erz gegossen, sprangen lo\u00df;", "tokens": ["Von", "Erz", "ge\u00b7gos\u00b7sen", ",", "spran\u00b7gen", "lo\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sag es, erschrockne Spree, und ihr, ihr H\u00fcgel!", "tokens": ["Sag", "es", ",", "er\u00b7schrock\u00b7ne", "Spree", ",", "und", "ihr", ",", "ihr", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "KON", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf die er Feuer go\u00df.", "tokens": ["Auf", "die", "er", "Feu\u00b7er", "go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sagts, ihr vom Sturm zerri\u00dfne hohe Fichten!", "tokens": ["Sagts", ",", "ihr", "vom", "Sturm", "zer\u00b7ri\u00df\u00b7ne", "ho\u00b7he", "Fich\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "APPRART", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Eichen! sagts der K\u00f6nigs Stadt;", "tokens": ["Ihr", "Ei\u00b7chen", "!", "sagts", "der", "K\u00f6\u00b7nigs", "Stadt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, seinen Willen auszurichten,", "tokens": ["Da\u00df", ",", "sei\u00b7nen", "Wil\u00b7len", "aus\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Blitz Befehle hat.", "tokens": ["Der", "Blitz", "Be\u00b7feh\u00b7le", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Gott zieht die Hand voll Keile schnell zur\u00fccke;", "tokens": ["Gott", "zieht", "die", "Hand", "voll", "Kei\u00b7le", "schnell", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm mu\u00df der Sturm gehorchend stehn;", "tokens": ["Ihm", "mu\u00df", "der", "Sturm", "ge\u00b7hor\u00b7chend", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hei\u00dft den Krieg mit einem Blicke", "tokens": [