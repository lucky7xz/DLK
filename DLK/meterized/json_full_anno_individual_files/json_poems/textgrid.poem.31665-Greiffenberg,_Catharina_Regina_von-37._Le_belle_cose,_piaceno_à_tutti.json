{"textgrid.poem.31665": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "37. Le belle cose, piaceno \u00e0 tutti", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nheit liebet jederman. Wer ist / der da hasst die Sonne?", "tokens": ["Sch\u00f6n\u00b7heit", "lie\u00b7bet", "je\u00b7der\u00b7man", ".", "Wer", "ist", "/", "der", "da", "hasst", "die", "Son\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$.", "PWS", "VAFIN", "$(", "ART", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "warlich Niemand / als der Neid / der ihr ihren Schein nicht gonne.", "tokens": ["war\u00b7lich", "Nie\u00b7mand", "/", "als", "der", "Neid", "/", "der", "ihr", "ih\u00b7ren", "Schein", "nicht", "gon\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$(", "KOUS", "ART", "NN", "$(", "PRELS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "doch versteh' ich nicht die Sch\u00f6nheit / die nur in dem Antlitz glitzt:", "tokens": ["doch", "ver\u00b7steh'", "ich", "nicht", "die", "Sch\u00f6n\u00b7heit", "/", "die", "nur", "in", "dem", "Ant\u00b7litz", "glitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$(", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "sondern wahrer T