{"textgrid.poem.48759": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "46. \u00dcber die Pfirschen daselbst", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den langsamen Verdru\u00df der siebendhalben Wochen", "tokens": ["Den", "lang\u00b7sa\u00b7men", "Ver\u00b7dru\u00df", "der", "sie\u00b7bend\u00b7hal\u00b7ben", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "streicht dieser Morgen hin. Habt wieder einen Mut", "tokens": ["streicht", "die\u00b7ser", "Mor\u00b7gen", "hin", ".", "Habt", "wie\u00b7der", "ei\u00b7nen", "Mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$.", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ihr dapfern ", "tokens": ["ihr", "dap\u00b7fern"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "das nach der ", "tokens": ["das", "nach", "der"], "token_info": ["word", "word", "word"], "pos": ["PDS", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Seht, was euch vor ein Gru\u00df allhier wird zugesprochen,", "tokens": ["Seht", ",", "was", "euch", "vor", "ein", "Gru\u00df", "all\u00b7hier", "wird", "zu\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "schaut, was die fremde Stadt, was ", "tokens": ["schaut", ",", "was", "die", "frem\u00b7de", "Stadt", ",", "was"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "PRELS", "ART", "ADJA", "NN", "$,", "PWS"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das nicht mehr \u00f6de Land empf\u00e4ngt euch durch sein Gut,", "tokens": ["Das", "nicht", "mehr", "\u00f6\u00b7de", "Land", "emp\u00b7f\u00e4ngt", "euch", "durch", "sein", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "ADJD", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das es zu eurer Lust und Ehren hat gebrochen.", "tokens": ["das", "es", "zu", "eu\u00b7rer", "Lust", "und", "Eh\u00b7ren", "hat", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Seid mehr froh, als ihr seid! Nicht Bacchus nur alleine", "tokens": ["Seid", "mehr", "froh", ",", "als", "ihr", "seid", "!", "Nicht", "Bac\u00b7chus", "nur", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADV", "ADJD", "$,", "KOUS", "PPER", "VAFIN", "$.", "PTKNEG", "NE", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bewirbet sich um euch mit seinem braunen Weine;", "tokens": ["be\u00b7wir\u00b7bet", "sich", "um", "euch", "mit", "sei\u00b7nem", "brau\u00b7nen", "Wei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Pomona schickt euch auch aus ihren G\u00e4rten di\u00df.", "tokens": ["Po\u00b7mo\u00b7na", "schickt", "euch", "auch", "aus", "ih\u00b7ren", "G\u00e4r\u00b7ten", "di\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Reist sicher, wie ihr tut. Auch Kaspis k\u00f6mmt geschwommen,", "tokens": ["Reist", "si\u00b7cher", ",", "wie", "ihr", "tut", ".", "Auch", "Kas\u00b7pis", "k\u00f6mmt", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$.", "ADV", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und hei\u00dfet euer Schiff mit sanfter Flut willkommen.", "tokens": ["und", "hei\u00b7\u00dfet", "eu\u00b7er", "Schiff", "mit", "sanf\u00b7ter", "Flut", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die G\u00f6tter machen selbst euch eures Gl\u00fccks gewi\u00df.", "tokens": ["Die", "G\u00f6t\u00b7ter", "ma\u00b7chen", "selbst", "euch", "eu\u00b7res", "Gl\u00fccks", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den 12. September 1636. 70. Werste von d