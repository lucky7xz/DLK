{"dta.poem.5287": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Fr\u00fchlings-Betrach-  \n tungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Freude, welche die Natur, durch die ihr ange-\nschafne Kraft,", "tokens": ["Die", "Freu\u00b7de", ",", "wel\u00b7che", "die", "Na\u00b7tur", ",", "durch", "die", "ihr", "an\u00b7ge", "schaf\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,", "APPR", "PRELS", "PPOSAT", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Da sie sich, uns zum Nutz und Vortheil, zur Fr\u00fchlings-", "tokens": ["Da", "sie", "sich", ",", "uns", "zum", "Nutz", "und", "Vor\u00b7theil", ",", "zur", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "$,", "PPER", "APPRART", "NN", "KON", "NN", "$,", "APPRART", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn man auf ihre Wirckung achtet, so dann in unsrer", "tokens": ["Wenn", "man", "auf", "ih\u00b7re", "Wir\u00b7ckung", "ach\u00b7tet", ",", "so", "dann", "in", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Freude, sag ich, ist unstreitig die alleredelst\u2019 Ei-", "tokens": ["Die", "Freu\u00b7de", ",", "sag", "ich", ",", "ist", "un\u00b7strei\u00b7tig", "die", "al\u00b7le\u00b7re\u00b7delst'", "Ei"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADJD", "ART", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Der unsre Seele f\u00e4hig ist. Da mit der Lust, die man", "tokens": ["Der", "uns\u00b7re", "See\u00b7le", "f\u00e4\u00b7hig", "ist", ".", "Da", "mit", "der", "Lust", ",", "die", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VAFIN", "$.", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Sich ja Bewunderung und Ehrfurcht, Lob, Andacht,", "tokens": ["Sich", "ja", "Be\u00b7wun\u00b7de\u00b7rung", "und", "Ehr\u00b7furcht", ",", "Lob", ",", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die aber, wenn wirs nicht ermessen, in uns erstickt, ver-", "tokens": ["Die", "a\u00b7ber", ",", "wenn", "wirs", "nicht", "er\u00b7mes\u00b7sen", ",", "in", "uns", "er\u00b7stickt", ",", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "$,", "KOUS", "PIS", "PTKNEG", "VVINF", "$,", "APPR", "PPER", "VVPP", "$,", "TRUNC"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.8": {"text": "Wodurch denn unsers Sch\u00f6pfers Absicht, und Lieb\u2019 und", "tokens": ["Wo\u00b7durch", "denn", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Ab\u00b7sicht", ",", "und", "Lieb'", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "NN", "$,", "KON", "NN", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Sammt unsrer Lust und Pflicht zugleich, verhindert und ge-", "tokens": ["Sammt", "uns\u00b7rer", "Lust", "und", "Pflicht", "zu\u00b7gleich", ",", "ver\u00b7hin\u00b7dert", "und", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "$,", "VVPP", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sollt alles dieses uns denn nicht zu GOtt, in seinen Wer-", "tokens": ["Sollt", "al\u00b7les", "die\u00b7ses", "uns", "denn", "nicht", "zu", "Gott", ",", "in", "sei\u00b7nen", "Wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PDAT", "PPER", "ADV", "PTKNEG", "APPR", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Es sehe denn doch jedermann,", "tokens": ["Es", "se\u00b7he", "denn", "doch", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu GOttes Ruhm, und eigner Freude,", "tokens": ["Zu", "Got\u00b7tes", "Ruhm", ",", "und", "eig\u00b7ner", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jm fast verj\u00fcngten Welt-Geb\u00e4ude,", "tokens": ["Jm", "fast", "ver\u00b7j\u00fcng\u00b7ten", "Welt\u00b7Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Creaturen Schmuck, zumahl im Fr\u00fchling, an!", "tokens": ["Der", "Crea\u00b7tu\u00b7ren", "Schmuck", ",", "zu\u00b7mahl", "im", "Fr\u00fch\u00b7ling", ",", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "APPRART", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Seht, wie aus dunckel-braunen Fl\u00e4chen", "tokens": ["Seht", ",", "wie", "aus", "dun\u00b7ckel\u00b7brau\u00b7nen", "Fl\u00e4\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erd\u2019 jetzt \u00fcberall die regen Pflantzen brechen!", "tokens": ["Der", "Erd'", "jetzt", "\u00fc\u00b7be\u00b7rall", "die", "re\u00b7gen", "Pflant\u00b7zen", "bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}