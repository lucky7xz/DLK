{"textgrid.poem.25142": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[wie viele S\u00e4nger schritten durch die Zeit]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie viele S\u00e4nger schritten durch die Zeit", "tokens": ["Wie", "vie\u00b7le", "S\u00e4n\u00b7ger", "schrit\u00b7ten", "durch", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und gaben meiner Seele ein Entz\u00fccken,", "tokens": ["Und", "ga\u00b7ben", "mei\u00b7ner", "See\u00b7le", "ein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn jede Sch\u00f6nheit suchte ich zu pfl\u00fccken,", "tokens": ["Denn", "je\u00b7de", "Sch\u00f6n\u00b7heit", "such\u00b7te", "ich", "zu", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So Erdenklang wie Sang der Ewigkeit!", "tokens": ["So", "Er\u00b7den\u00b7klang", "wie", "Sang", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und oft, wenn mich der Muse Ku\u00df geweiht,", "tokens": ["Und", "oft", ",", "wenn", "mich", "der", "Mu\u00b7se", "Ku\u00df", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schwillt dieses T\u00f6nemeer, mich zu begl\u00fccken.", "tokens": ["Schwillt", "die\u00b7ses", "T\u00f6\u00b7ne\u00b7meer", ",", "mich", "zu", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch sucht kein Klang den andern zu erdr\u00fccken,", "tokens": ["Doch", "sucht", "kein", "Klang", "den", "an\u00b7dern", "zu", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ist kein roher L\u00e4rm, kein wilder Streit.", "tokens": ["Da", "ist", "kein", "ro\u00b7her", "L\u00e4rm", ",", "kein", "wil\u00b7der", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist wie Sang, den uns der Abend bringt:", "tokens": ["Es", "ist", "wie", "Sang", ",", "den", "uns", "der", "A\u00b7bend", "bringt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Quellenrieseln und der Glockenklang,", "tokens": ["Das", "Quel\u00b7len\u00b7rie\u00b7seln", "und", "der", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word",