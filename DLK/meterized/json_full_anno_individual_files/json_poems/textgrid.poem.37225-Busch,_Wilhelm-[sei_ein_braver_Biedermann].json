{"textgrid.poem.37225": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[sei ein braver Biedermann]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei ein braver Biedermann,", "tokens": ["Sei", "ein", "bra\u00b7ver", "Bie\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fange t\u00fcchtig an zu loben!", "tokens": ["Fan\u00b7ge", "t\u00fcch\u00b7tig", "an", "zu", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du wirst von uns sodann", "tokens": ["Und", "du", "wirst", "von", "uns", "so\u00b7dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gerne mit empor gehoben.", "tokens": ["Ger\u00b7ne", "mit", "em\u00b7por", "ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie, du ziehst ein schiefes Maul?", "tokens": ["Wie", ",", "du", "ziehst", "ein", "schie\u00b7fes", "Maul", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst nicht, da\u00df dich andre adeln?", "tokens": ["Willst", "nicht", ",", "da\u00df", "dich", "and\u00b7re", "a\u00b7deln", "?"], "token_info": ["word", "word", "punct", "word", "word