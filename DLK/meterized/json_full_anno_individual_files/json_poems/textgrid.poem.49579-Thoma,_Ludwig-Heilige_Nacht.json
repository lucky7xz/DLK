{"textgrid.poem.49579": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Heilige Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ward der Herr Jesus geboren", "tokens": ["So", "ward", "der", "Herr", "Je\u00b7sus", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Stall bei der kalten Nacht.", "tokens": ["Im", "Stall", "bei", "der", "kal\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Armen, die haben gefroren,", "tokens": ["Die", "Ar\u00b7men", ",", "die", "ha\u00b7ben", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Reichen war's warm gemacht.", "tokens": ["Den", "Rei\u00b7chen", "wa\u00b7r's", "warm", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sein Vater ist Schreiner gewesen,", "tokens": ["Sein", "Va\u00b7ter", "ist", "Schrei\u00b7ner", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "VAPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Mutter war eine Magd.", "tokens": ["Die", "Mut\u00b7ter", "war", "ei\u00b7ne", "Magd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sie haben kein Geld nicht besessen,", "tokens": ["Sie", "ha\u00b7ben", "kein", "Geld", "nicht", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie haben sich wohl geplagt.", "tokens": ["Sie", "ha\u00b7ben", "sich", "wohl", "ge\u00b7plagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Kein Wirt hat ins Haus sie genommen;", "tokens": ["Kein", "Wirt", "hat", "ins", "Haus", "sie", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie waren von Herzen froh,", "tokens": ["Sie", "wa\u00b7ren", "von", "Her\u00b7zen", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df sie noch in Stall sind gekommen.", "tokens": ["Da\u00df", "sie", "noch", "in", "Stall", "sind", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie legten das Kind auf Stroh.", "tokens": ["Sie", "leg\u00b7ten", "das", "Kind", "auf", "Stroh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Engel, die haben gesungen,", "tokens": ["Die", "En\u00b7gel", ",", "die", "ha\u00b7ben", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df wohl ein Wunder geschehn.", "tokens": ["Da\u00df", "wohl", "ein", "Wun\u00b7der", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da kamen die Hirten gesprungen", "tokens": ["Da", "ka\u00b7men", "die", "Hir\u00b7ten", "ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und haben es angesehn.", "tokens": ["Und", "ha\u00b7ben", "es", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Hirten, die will es erbarmen,", "tokens": ["Die", "Hir\u00b7ten", ",", "die", "will", "es", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie elend das Kindlein sei.", "tokens": ["Wie", "e\u00b7lend", "das", "Kin\u00b7dlein", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es ist eine G'schicht' f\u00fcr die Armen,", "tokens": ["Es", "ist", "ei\u00b7ne", "G'\u00b7schicht'", "f\u00fcr", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein Reicher war nicht dabei.", "tokens": ["Kein", "Rei\u00b7cher", "war", "nicht", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKNEG", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "So ward der Herr Jesus geboren", "tokens": ["So", "ward", "der", "Herr", "Je\u00b7sus", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Stall bei der kalten Nacht.", "tokens": ["Im", "Stall", "bei", "der", "kal\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Armen, die haben gefroren,", "tokens": ["Die", "Ar\u00b7men", ",", "die", "ha\u00b7ben", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Reichen war's warm gemacht.", "tokens": ["Den", "Rei\u00b7chen", "wa\u00b7r's", "warm", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sein Vater ist Schreiner gewesen,", "tokens": ["Sein", "Va\u00b7ter", "ist", "Schr