{"textgrid.poem.41698": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Kinderseelen ihr!", "tokens": ["O", "Kin\u00b7der\u00b7see\u00b7len", "ihr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Um nicht das Wichtigste von allem zu vergessen,", "tokens": ["Um", "nicht", "das", "Wich\u00b7tigs\u00b7te", "von", "al\u00b7lem", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "ART", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sahen \u00fcberall, obgleich wir's nie begehrt,", "tokens": ["Wir", "sa\u00b7hen", "\u00fc\u00b7be\u00b7rall", ",", "ob\u00b7gleich", "wir's", "nie", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So oft die Stufen auch der Leiter wir durchmessen,", "tokens": ["So", "oft", "die", "Stu\u00b7fen", "auch", "der", "Lei\u00b7ter", "wir", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den l\u00e4stgen Anblick, den die S\u00fcnde uns gew\u00e4hrt:", "tokens": ["Den", "l\u00e4st\u00b7gen", "An\u00b7blick", ",", "den", "die", "S\u00fcn\u00b7de", "uns", "ge\u00b7w\u00e4hrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Weib, die Sklavin, die ohn' Abscheu, ohne Lachen", "tokens": ["Das", "Weib", ",", "die", "Skla\u00b7vin", ",", "die", "ohn'", "Ab\u00b7scheu", ",", "oh\u00b7ne", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich liebt und tut, was Stolz und Dummheit ihr gebot,", "tokens": ["Sich", "liebt", "und", "tut", ",", "was", "Stolz", "und", "Dumm\u00b7heit", "ihr", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "$,", "PWS", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mann, ein Zwingherr, den Begier und Zorn entfachen,", "tokens": ["Der", "Mann", ",", "ein", "Zwing\u00b7herr", ",", "den", "Be\u00b7gier", "und", "Zorn", "ent\u00b7fa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der Sklavin Sklave und ein Bach in Schmutz und Kot;", "tokens": ["Der", "Skla\u00b7vin", "Skla\u00b7ve", "und", "ein", "Bach", "in", "Schmutz", "und", "Kot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Henker, der sich freut, des Opfers Qual zu sch\u00e4rfen;", "tokens": ["Der", "Hen\u00b7ker", ",", "der", "sich", "freut", ",", "des", "Op\u00b7fers", "Qual", "zu", "sch\u00e4r\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Orgie, der das Blut die rechte W\u00fcrze gibt;", "tokens": ["Die", "Or\u00b7gie", ",", "der", "das", "Blut", "die", "rech\u00b7te", "W\u00fcr\u00b7ze", "gibt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Gift der Herrschgewalt, Despoten zu entnerven,", "tokens": ["Das", "Gift", "der", "Herrschge\u00b7walt", ",", "Des\u00b7po\u00b7ten", "zu", "ent\u00b7ner\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das stumpfe Volk, das in der Peitsche Schlag verliebt;", "tokens": ["Das", "stump\u00b7fe", "Volk", ",", "das", "in", "der", "Peit\u00b7sche", "Schlag", "ver\u00b7liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und Religionen, die der unsren alle gleichen,", "tokens": ["Und", "Re\u00b7li\u00b7gi\u00b7o\u00b7nen", ",", "die", "der", "un\u00b7sren", "al\u00b7le", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "PIAT", "ADJA", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zum Himmel klimmend, stolz auf ihre Heiligkeit,", "tokens": ["Zum", "Him\u00b7mel", "klim\u00b7mend", ",", "stolz", "auf", "ih\u00b7re", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die, wie ein Z\u00e4rtling, der sich w\u00e4lzt im Bett, im weichen,", "tokens": ["Die", ",", "wie", "ein", "Z\u00e4rt\u00b7ling", ",", "der", "sich", "w\u00e4lzt", "im", "Bett", ",", "im", "wei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPRART", "NN", "$,", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich ihre Wollust sucht in Pein und h\u00e4rnem Kleid.", "tokens": ["Sich", "ih\u00b7re", "Wol\u00b7lust", "sucht", "in", "Pein", "und", "h\u00e4r\u00b7nem", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Menschheit redet toll, am eignen Geist sich freuend,", "tokens": ["Die", "Menschheit", "re\u00b7det", "toll", ",", "am", "eig\u00b7nen", "Geist", "sich", "freu\u00b7end", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "APPRART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wie sie immer war, von Wahnsinn heimgesucht,", "tokens": ["Und", "wie", "sie", "im\u00b7mer", "war", ",", "von", "Wahn\u00b7sinn", "heim\u00b7ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VAFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In ihrem Todeskampf zu Gott dem Herren schreiend:", "tokens": ["In", "ih\u00b7rem", "To\u00b7des\u00b7kampf", "zu", "Gott", "dem", "Her\u00b7ren", "schrei\u00b7end", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.4": {"text": "O du mein Ebenbild, mein Meister! Sei verflucht!", "tokens": ["O", "du", "mein", "E\u00b7ben\u00b7bild", ",", "mein", "Meis\u00b7ter", "!", "Sei", "ver\u00b7flucht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die wenigst Dummen noch, die k\u00fchn den Wahnsinn lieben,", "tokens": ["Die", "we\u00b7nigst", "Dum\u00b7men", "noch", ",", "die", "k\u00fchn", "den", "Wahn\u00b7sinn", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "$,", "PRELS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Haufen fliehend, der verschont bleibt vom Gericht,", "tokens": ["Den", "Hau\u00b7fen", "flie\u00b7hend", ",", "der", "ver\u00b7schont", "bleibt", "vom", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ins grenzenlose Reich des Opiums getrieben! \u2013", "tokens": ["Ins", "gren\u00b7zen\u00b7lo\u00b7se", "Reich", "des", "O\u00b7piums", "ge\u00b7trie\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So hei\u00dft des Erdenballs allewiger Bericht.", "tokens": ["So", "hei\u00dft", "des", "Er\u00b7den\u00b7balls", "al\u00b7le\u00b7wi\u00b7ger", "Be\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "O Kinderseelen ihr!", "tokens": ["O", "Kin\u00b7der\u00b7see\u00b7len", "ihr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Um nicht das Wichtigste von allem zu vergessen,", "tokens": ["Um", "nicht", "das", "Wich\u00b7tigs\u00b7te", "von", "al\u00b7lem", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "ART", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"},