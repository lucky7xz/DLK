rd", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.4": {"text": "O du mein Ebenbild, mein Meister! Sei verflucht!", "tokens": ["O", "du", "mein", "E\u00b7ben\u00b7bild", ",", "mein", "Meis\u00b7ter", "!", "Sei", "ver\u00b7flucht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Die wenigst Dummen noch, die k\u00fchn den Wahnsinn lieben,", "tokens": ["Die", "we\u00b7nigst", "Dum\u00b7men", "noch", ",", "die", "k\u00fchn", "den", "Wahn\u00b7sinn", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "$,", "PRELS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Haufen fliehend, der verschont bleibt vom Gericht,", "tokens": ["Den", "Hau\u00b7fen", "flie\u00b7hend", ",", "der", "ver\u00b7schont", "bleibt", "vom", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ins grenzenlose Reich des Opiums getrieben! \u2013", "tokens": ["Ins", "gren\u00b7zen\u00b7lo\u00b7se", "Reich", "des", "O\u00b7piums", "ge\u00b7trie\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4"