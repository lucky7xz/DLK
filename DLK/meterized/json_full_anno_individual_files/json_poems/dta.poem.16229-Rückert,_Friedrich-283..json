{"dta.poem.16229": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "283.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df in der Einsamkeit dir nicht der Reiz gebr\u00e4che", "tokens": ["Da\u00df", "in", "der", "Ein\u00b7sam\u00b7keit", "dir", "nicht", "der", "Reiz", "ge\u00b7br\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "PTKNEG", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unterhaltung, h\u00e4ltst du mit dir Selbstgespr\u00e4che.", "tokens": ["Der", "Un\u00b7ter\u00b7hal\u00b7tung", ",", "h\u00e4ltst", "du", "mit", "dir", "Selbst\u00b7ge\u00b7spr\u00e4\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du hast den Vortheil, dis Gespr\u00e4ch allein zu leiten,", "tokens": ["Du", "hast", "den", "Vor\u00b7theil", ",", "dis", "Ge\u00b7spr\u00e4ch", "al\u00b7lein", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PDS", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4ssest, was du gern nicht h\u00f6rest, leicht beiseiten.", "tokens": ["Und", "l\u00e4s\u00b7sest", ",", "was", "du", "gern", "nicht", "h\u00f6\u00b7rest", ",", "leicht", "bei\u00b7sei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Einseitig ist darum doch nicht die Unterhaltung,", "tokens": ["Ein\u00b7sei\u00b7tig", "ist", "da\u00b7rum", "doch", "nicht", "die", "Un\u00b7ter\u00b7hal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PAV", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist in dir ein Keim unendlicher Entfaltung.", "tokens": ["Es", "ist", "in", "dir", "ein", "Keim", "un\u00b7end\u00b7li\u00b7cher", "Ent\u00b7fal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Viel Unterredner sind in dir, du mu\u00dft nur jeden,", "tokens": ["Viel", "Un\u00b7ter\u00b7red\u00b7ner", "sind", "in", "dir", ",", "du", "mu\u00dft", "nur", "je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "$,", "PPER", "VMFIN", "ADV", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem du lernen willst, nicht hindern auszureden.", "tokens": ["Von", "dem", "du", "ler\u00b7nen", "willst", ",", "nicht", "hin\u00b7dern", "aus\u00b7zu\u00b7re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}