{"dta.poem.15048": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "hin under die getouften schar", "tokens": ["hin", "un\u00b7der", "die", "ge\u00b7touf\u00b7ten", "schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nv si gemischet wurden gar", "tokens": ["nv", "si", "ge\u00b7mi\u00b7schet", "wur\u00b7den", "gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "VVPP", "VAFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "zein ander uf der heide", "tokens": ["zein", "an\u00b7der", "uf", "der", "hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "so daz kein underscheide", "tokens": ["so", "daz", "kein", "un\u00b7der\u00b7schei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PAV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "wart von in gehalten", "tokens": ["wart", "von", "in", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "da wurden si gespalten", "tokens": ["da", "wur\u00b7den", "si", "ge\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "vnd in zwei ganze teil ges ...", "tokens": ["vnd", "in", "zwei", "gan\u00b7ze", "teil", "ges", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "ADJA", "NN", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "der cristen und der heiden w ...", "tokens": ["der", "cris\u00b7ten", "und", "der", "hei\u00b7den", "w", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "beidenthalp geliche vil", "tokens": ["bei\u00b7dent\u00b7halp", "ge\u00b7li\u00b7che", "vil"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "an dem ritterlichen spil", "tokens": ["an", "dem", "rit\u00b7ter\u00b7li\u00b7chen", "spil"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "en weder teil wart minre", "tokens": ["en", "we\u00b7der", "teil", "wart", "min\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "daz uzer und daz inre", "tokens": ["daz", "u\u00b7zer", "und", "daz", "in\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "KON", "PAV", "NE"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.13": {"text": "niht eines mannes hete me", "tokens": ["niht", "ei\u00b7nes", "man\u00b7nes", "he\u00b7te", "me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.14": {"text": "kein tvrnei weder sit noch e", "tokens": ["kein", "tvr\u00b7nei", "we\u00b7der", "sit", "noch", "e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "VVFIN", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "wart als eben uf geleit", "tokens": ["wart", "als", "e\u00b7ben", "uf", "ge\u00b7leit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "das kam do zeiner selekeit", "tokens": ["das", "kam", "do", "zei\u00b7ner", "se\u00b7le\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "in allen und ze heile", "tokens": ["in", "al\u00b7len", "und", "ze", "hei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "KON", "XY", "XY"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "eim ietwedern teile", "tokens": ["eim", "iet\u00b7we\u00b7dern", "tei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "das turnieren da began", "tokens": ["das", "tur\u00b7nie\u00b7ren", "da", "be\u00b7gan"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "wurden zwene houbtman", "tokens": ["wur\u00b7den", "zwe\u00b7ne", "houbt\u00b7man"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "besunder uzbescheiden", "tokens": ["be\u00b7sun\u00b7der", "uz\u00b7be\u00b7schei\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "die cristen unde heiden", "tokens": ["die", "cris\u00b7ten", "un\u00b7de", "hei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "niht streites liezen walten", "tokens": ["niht", "strei\u00b7tes", "lie\u00b7zen", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "den turnei mueste halten", "tokens": ["den", "tur\u00b7nei", "mu\u00b7es\u00b7te", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "der Soldan zeinre siten", "tokens": ["der", "Sol\u00b7dan", "zein\u00b7re", "si\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "unde vvielt sin an den ziten", "tokens": ["un\u00b7de", "vvielt", "sin", "an", "den", "zi\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "mit im ein k&#250;nec uzerkorn", "tokens": ["mit", "im", "ein", "k", "&#250;", "nec", "uz\u00b7er\u00b7korn"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "der waz uz Spangenlant geborn.", "tokens": ["der", "waz", "uz", "Span\u00b7gen\u00b7lant", "ge\u00b7born", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Si zwene mit ir lieb ...", "tokens": ["Si", "zwe\u00b7ne", "mit", "ir", "lieb", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "des males werde cl ...", "tokens": ["des", "ma\u00b7les", "wer\u00b7de", "cl", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "vvurden in die stat ....", "tokens": ["vvur\u00b7den", "in", "die", "stat", "...."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.32": {"text": "diu veste michel und ...", "tokens": ["diu", "ves\u00b7te", "mi\u00b7chel", "und", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}