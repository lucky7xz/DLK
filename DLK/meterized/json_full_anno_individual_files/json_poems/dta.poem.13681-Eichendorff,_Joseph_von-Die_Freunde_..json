{"dta.poem.13681": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "In Stein gehau'n, zwei L\u00f6wen stehen drau\u00dfen,", "tokens": ["In", "Stein", "ge\u00b7hau'n", ",", "zwei", "L\u00f6\u00b7wen", "ste\u00b7hen", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "CARD", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewachen ewig stumm die heil'ge Pforte.", "tokens": ["Be\u00b7wa\u00b7chen", "e\u00b7wig", "stumm", "die", "heil'\u00b7ge", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer sich, die Brust voll Weltlust, naht dem Orte,", "tokens": ["Wer", "sich", ",", "die", "Brust", "voll", "Welt\u00b7lust", ",", "naht", "dem", "Or\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "$,", "ART", "NN", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den f\u00fcllt ihr steinern Blicken bald mit Grausen.", "tokens": ["Den", "f\u00fcllt", "ihr", "stei\u00b7nern", "Bli\u00b7cken", "bald", "mit", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}