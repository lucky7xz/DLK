{"textgrid.poem.43023": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "11. Letzter Trost", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gold'ne Zeit, du Traumbild der Poeten!", "tokens": ["Du", "gold'\u00b7ne", "Zeit", ",", "du", "Traum\u00b7bild", "der", "Po\u00b7et\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann leuchten wieder deine Morgenr\u00f6then?", "tokens": ["Wann", "leuch\u00b7ten", "wie\u00b7der", "dei\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt ist des Menschen Eigenstes geknechtet;", "tokens": ["Jetzt", "ist", "des", "Men\u00b7schen", "Ei\u00b7gens\u00b7tes", "ge\u00b7knech\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer selbst sich lebt, verspottet und ge\u00e4chtet.", "tokens": ["Wer", "selbst", "sich", "lebt", ",", "ver\u00b7spot\u00b7tet", "und", "ge\u00e4ch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VVFIN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ob allem Hohen, Ed'len, h\u00f6hnend prangt", "tokens": ["Ob", "al\u00b7lem", "Ho\u00b7hen", ",", "Ed'\u00b7len", ",", "h\u00f6h\u00b7nend", "prangt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "NN", "$,", "NN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Heuchelei, gleich einer frechen Dirne,", "tokens": ["Die", "Heu\u00b7che\u00b7lei", ",", "gleich", "ei\u00b7ner", "fre\u00b7chen", "Dir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die keck nach Scharlach und nach Purpur langt,", "tokens": ["Die", "keck", "nach", "Schar\u00b7lach", "und", "nach", "Pur\u00b7pur", "langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dem\u00fcth'ge Schaam auf Schl\u00e4fenpaar und Stirne.", "tokens": ["De\u00b7m\u00fcth'\u00b7ge", "Schaam", "auf", "Schl\u00e4\u00b7fen\u00b7paar", "und", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O jetzt zu leben, jetzt!", "tokens": ["O", "jetzt", "zu", "le\u00b7ben", ",", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "PTKZU", "VVINF", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo dem edel'n Gem\u00fcth", "tokens": ["Wo", "dem", "e\u00b7del'n", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "An des eig'nen Gedankens Gescho\u00df,", "tokens": ["An", "des", "eig'\u00b7nen", "Ge\u00b7dan\u00b7kens", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "An der eigenen Seele Gluten,", "tokens": ["An", "der", "ei\u00b7ge\u00b7nen", "See\u00b7le", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wie das ed'le arabische Ro\u00df", "tokens": ["Wie", "das", "ed'\u00b7le", "a\u00b7ra\u00b7bi\u00b7sche", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "An ge\u00f6ffneten Adern, verbluten.", "tokens": ["An", "ge\u00b7\u00f6ff\u00b7ne\u00b7ten", "A\u00b7dern", ",", "ver\u00b7blu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Natur, so heilig in dem stillen Walten!", "tokens": ["Na\u00b7tur", ",", "so", "hei\u00b7lig", "in", "dem", "stil\u00b7len", "Wal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Natur, so keusch im offensten Entfalten!", "tokens": ["Na\u00b7tur", ",", "so", "keusch", "im", "of\u00b7fens\u00b7ten", "Ent\u00b7fal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Frei schm\u00fcckst du dich mit deinen Abendr\u00f6then,", "tokens": ["Frei", "schm\u00fcckst", "du", "dich", "mit", "dei\u00b7nen", "A\u00b7bend\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des neuen Tages flammenden Propheten.", "tokens": ["Des", "neu\u00b7en", "Ta\u00b7ges", "flam\u00b7men\u00b7den", "Pro\u00b7phe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nach eigenem Gesetze still und gro\u00df,", "tokens": ["Nach", "ei\u00b7ge\u00b7nem", "Ge\u00b7set\u00b7ze", "still", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Selbstherrschend wandelst du die Bahn des Lebens.", "tokens": ["Selbst\u00b7herr\u00b7schend", "wan\u00b7delst", "du", "die", "Bahn", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nach strebt dir meine Seele fessellos;", "tokens": ["Nach", "strebt", "dir", "mei\u00b7ne", "See\u00b7le", "fes\u00b7sel\u00b7los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nach deinem Lebensquell schmacht' ich vergebens.", "tokens": ["Nach", "dei\u00b7nem", "Le\u00b7bens\u00b7quell", "schmacht'", "ich", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O jetzt zu leben, jetzt!", "tokens": ["O", "jetzt", "zu", "le\u00b7ben", ",", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "PTKZU", "VVINF", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo dem edel'n Gem\u00fcth", "tokens": ["Wo", "dem", "e\u00b7del'n", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "An des eig'nen Gedankens Gescho\u00df,", "tokens": ["An", "des", "eig'\u00b7nen", "Ge\u00b7dan\u00b7kens", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "An der eigenen Seele Gluten,", "tokens": ["An", "der", "ei\u00b7ge\u00b7nen", "See\u00b7le", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wie das ed'le arabische Ro\u00df", "tokens": ["Wie", "das", "ed'\u00b7le", "a\u00b7ra\u00b7bi\u00b7sche", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "An ge\u00f6ffneten Adern, verbluten.", "tokens": ["An", "ge\u00b7\u00f6ff\u00b7ne\u00b7ten", "A\u00b7dern", ",", "ver\u00b7blu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Du tr\u00e4umerischer D\u00e4nenprinz, erkranken", "tokens": ["Du", "tr\u00e4u\u00b7me\u00b7ri\u00b7scher", "D\u00e4\u00b7nen\u00b7prinz", ",", "er\u00b7kran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df ich, gleich Dir, an bleichen Nachtgedanken!", "tokens": ["Mu\u00df", "ich", ",", "gleich", "Dir", ",", "an", "blei\u00b7chen", "Nacht\u00b7ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Kerker ist die Welt; ich bin gefangen!", "tokens": ["Ein", "Ker\u00b7ker", "ist", "die", "Welt", ";", "ich", "bin", "ge\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gefesselt ist mein gl\u00fchendes Verlangen!", "tokens": ["Ge\u00b7fes\u00b7selt", "ist", "mein", "gl\u00fc\u00b7hen\u00b7des", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In Fesseln seufzt, wie ich, mein ganz' Geschlecht!", "tokens": ["In", "Fes\u00b7seln", "seufzt", ",", "wie", "ich", ",", "mein", "ganz'", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWAV", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Wahn der Menge ist der Kerkermeister,", "tokens": ["Der", "Wahn", "der", "Men\u00b7ge", "ist", "der", "Ker\u00b7ker\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der blinden Satzung treuergebner Knecht,", "tokens": ["Der", "blin\u00b7den", "Sat\u00b7zung", "treu\u00b7er\u00b7geb\u00b7ner", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der nie vers\u00f6hnte Feind der freien Geister!", "tokens": ["Der", "nie", "ver\u00b7s\u00f6hn\u00b7te", "Feind", "der", "frei\u00b7en", "Geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O jetzt zu leben, jetzt!", "tokens": ["O", "jetzt", "zu", "le\u00b7ben", ",", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "PTKZU", "VVINF", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo dem edel'n Gem\u00fcth", "tokens": ["Wo", "dem", "e\u00b7del'n", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "An des eig'nen Gedankens Gescho\u00df,", "tokens": ["An", "des", "eig'\u00b7nen", "Ge\u00b7dan\u00b7kens", "Ge\u00b7scho\u0