{"textgrid.poem.49873": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Lampe", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch unverr\u00fcckt, o sch\u00f6ne Lampe, schm\u00fcckest du,", "tokens": ["Noch", "un\u00b7ver\u00b7r\u00fcckt", ",", "o", "sch\u00f6\u00b7ne", "Lam\u00b7pe", ",", "schm\u00fc\u00b7ckest", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "FM", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An leichten Ketten zierlich aufgehangen hier,", "tokens": ["An", "leich\u00b7ten", "Ket\u00b7ten", "zier\u00b7lich", "auf\u00b7ge\u00b7han\u00b7gen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Decke des nun fast verge\u00dfnen Lustgemachs.", "tokens": ["Die", "De\u00b7cke", "des", "nun", "fast", "ver\u00b7ge\u00df\u00b7nen", "Lust\u00b7ge\u00b7machs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf deiner wei\u00dfen Marmorschale, deren Rand", "tokens": ["Auf", "dei\u00b7ner", "wei\u00b7\u00dfen", "Mar\u00b7mor\u00b7scha\u00b7le", ",", "de\u00b7ren", "Rand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Efeukranz von goldengr\u00fcnem Erz umflicht,", "