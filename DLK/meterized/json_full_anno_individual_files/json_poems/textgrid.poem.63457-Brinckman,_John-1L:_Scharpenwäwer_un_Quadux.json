{"textgrid.poem.63457": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Scharpenw\u00e4wer un Quadux", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Scharpenw\u00e4wer un Quadux", "tokens": ["Schar\u00b7pen\u00b7w\u00e4\u00b7wer", "un", "Qua\u00b7dux"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dr\u00f6pen sick bi'n Graben.", "tokens": ["dr\u00f6\u00b7pen", "sick", "bi'n", "Gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Se hadd s\u00e4ten up ehr B\u00fclt,", "tokens": ["Se", "hadd", "s\u00e4\u00b7ten", "up", "ehr", "B\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "he sin Schuwkor schaben.", "tokens": ["he", "sin", "Schuw\u00b7kor", "scha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "\u00bbnee, wu flitig! Nee, wat'n Flit!", "tokens": ["\u00bb", "nee", ",", "wu", "fli\u00b7tig", "!", "Nee", ",", "wat'n", "Flit", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PWAV", "ADJD", "$.", "NN", "$,", "PWAV", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahwer, so wat l\u00e4wt nich!", "tokens": ["Nah\u00b7wer", ",", "so", "wat", "l\u00e4wt", "nich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dat's nu glik nachtslapen Tit,", "tokens": ["Dat's", "nu", "glik", "nachts\u00b7la\u00b7pen", "Tit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "man Ji \u0153werg\u00e4wt't nich!\u00ab", "tokens": ["man", "Ji", "\u0153wer\u00b7g\u00e4wt't", "nich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "NE", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbnahwersch, je dat's as dit is,", "tokens": ["\u00bb", "nah\u00b7wersch", ",", "je", "dat's", "as", "dit", "is", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADV", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dat s\u00fcnd hochbeent Johren!", "tokens": ["dat", "s\u00fcnd", "hoch\u00b7beent", "Joh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schillingssteg is Dalersweg,", "tokens": ["Schil\u00b7lings\u00b7steg", "is", "Da\u00b7lers\u00b7weg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wen nicks hett, m\u00f6t sporen.\u00ab", "tokens": ["wen", "nicks", "hett", ",", "m\u00f6t", "spo\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "PIS", "VAFIN", "$,", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbwat een meent un wat een seggt,", "tokens": ["\u00bb", "wat", "e\u00b7en", "meent", "un", "wat", "e\u00b7en", "seggt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nahwer, s\u00fcnd twee Saken;", "tokens": ["Nah\u00b7wer", ",", "s\u00fcnd", "twee", "Sa\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ji hewwt got wat f\u00f6r Juch br\u00f6cht,", "tokens": ["Ji", "hewwt", "got", "wat", "f\u00f6r", "Juch", "br\u00f6cht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "brukt nich mihr to raken!\u00ab", "tokens": ["brukt", "nich", "mihr", "to", "ra\u00b7ken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbwat een f\u00f6r sick s\u00fclm nich deit,", "tokens": ["\u00bb", "wat", "e\u00b7en", "f\u00f6r", "sick", "s\u00fclm", "nich", "deit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "deit een f\u00f6r sin G\u00f6ren!\u00ab", "tokens": ["deit", "e\u00b7en", "f\u00f6r", "sin", "G\u00f6\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dormit f\u00fcng he wedder an", "tokens": ["Dor\u00b7mit", "f\u00fcng", "he", "wed\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "as een Kirl to bohren;", "tokens": ["as", "e\u00b7en", "Kirl", "to", "boh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "tr\u00f6ck un sch\u00f6w un sch\u00f6w un tr\u00f6ck,", "tokens": ["tr\u00f6ck", "un", "sch\u00f6w", "un", "sch\u00f6w", "un", "tr\u00f6ck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "suer w\u00fcrr't den Ollen,", "tokens": ["su\u00b7er", "w\u00fcrr't", "den", "Ol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "suer \u2013 bet he vor sin Lock", "tokens": ["su\u00b7er", "\u2013", "bet", "he", "vor", "sin", "Lock"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$(", "VVFIN", "NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mit sin Schuwkor hollen.", "tokens": ["mit", "sin", "Schuw\u00b7kor", "hol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbna, god Nacht ok, Fru Quadux!\u00ab", "tokens": ["\u00bb", "na", ",", "god", "Nacht", "ok", ",", "Fru", "Qua\