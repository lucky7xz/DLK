{"dta.poem.17574": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "196.  GOtt hat alle Nahmen/ und keinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Man kan den h\u00f6chsten GOtt mit allen Nahmen nen\u0304en:", "tokens": ["Man", "kan", "den", "h\u00f6chs\u00b7ten", "Gott", "mit", "al\u00b7len", "Nah\u00b7men", "nen\u0304en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man kan jhm widerumb nicht einen zu erkennen.", "tokens": ["Man", "kan", "jhm", "wi\u00b7de\u00b7rumb", "nicht", "ei\u00b7nen", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}