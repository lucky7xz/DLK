{"textgrid.poem.25141": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r ich von ritterlichem Wuchs, vielleicht", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r ich von ritterlichem Wuchs, vielleicht", "tokens": ["W\u00e4r", "ich", "von", "rit\u00b7ter\u00b7li\u00b7chem", "Wuchs", ",", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4r meinem Weh ein Widerhall erwacht", "tokens": ["W\u00e4r", "mei\u00b7nem", "Weh", "ein", "Wi\u00b7der\u00b7hall", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00e4tte wohl dein Herz in Glut entfacht,", "tokens": ["Und", "h\u00e4t\u00b7te", "wohl", "dein", "Herz", "in", "Glut", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es mir selbst die Waffen \u00fcberreicht.", "tokens": ["Da\u00df", "es", "mir", "selbst", "die", "Waf\u00b7fen", "\u00fc\u00b7berr\u00b7eicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ach, ich bin kein Held, dem alles weicht,", "tokens": ["Doch", "ach", ",", "ich", "bin", "kein", "Held", ",", "dem", "al\u00b7les", "weicht", ","], "token_info": ["word", "word", "pun