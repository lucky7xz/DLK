{"dta.poem.19377": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "110.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Beter hat erz\u00e4hlt: Lang betet' ich, und nickte", "tokens": ["Ein", "Be\u00b7ter", "hat", "er\u00b7z\u00e4hlt", ":", "Lang", "be\u00b7tet'", "ich", ",", "und", "nick\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor Andacht endlich ein, als ich den Traum erblickte:", "tokens": ["Vor", "An\u00b7dacht", "end\u00b7lich", "ein", ",", "als", "ich", "den", "Traum", "er\u00b7blick\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Engel stand vor mir, und hielt in seiner Hand", "tokens": ["Ein", "En\u00b7gel", "stand", "vor", "mir", ",", "und", "hielt", "in", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Blatt, wo jegliches Gebet geschrieben stand;", "tokens": ["Ein", "Blatt", ",", "wo", "jeg\u00b7li\u00b7ches", "Ge\u00b7bet", "ge\u00b7schrie\u00b7ben", "stand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein jegliches, wie ich's der Reihe nach gesprochen;", "tokens": ["Ein", "jeg\u00b7li\u00b7ches", ",", "wie", "ich's", "der", "Rei\u00b7he", "nach", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "PIS", "ART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur eine Zeile war in Mitten abgebrochen.", "tokens": ["Nur", "ei\u00b7ne", "Zei\u00b7le", "war", "in", "Mit\u00b7ten", "ab\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da weint' ich, da\u00df mir die verloren sollte seyn;", "tokens": ["Da", "weint'", "ich", ",", "da\u00df", "mir", "die", "ver\u00b7lo\u00b7ren", "soll\u00b7te", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "VVINF", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum nicht trugest du dis mit dem andern ein?", "tokens": ["Wa\u00b7rum", "nicht", "tru\u00b7gest", "du", "dis", "mit", "dem", "an\u00b7dern", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VVFIN", "PPER", "PDS", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er sprach: Im Beten warst du bis hieher gekommen,", "tokens": ["Er", "sprach", ":", "Im", "Be\u00b7ten", "warst", "du", "bis", "hie\u00b7her", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPRART", "NN", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als beim Vor\u00fcbergehn der Nachbar dich vernommen.", "tokens": ["Als", "beim", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "der", "Nach\u00b7bar", "dich", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du wurdest auch gewahr, da\u00df er vor\u00fcber k\u00e4me,", "tokens": ["Du", "wur\u00b7dest", "auch", "ge\u00b7wahr", ",", "da\u00df", "er", "vor\u00b7\u00fc\u00b7ber", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprachest lauter gar, damit er es vern\u00e4hme.", "tokens": ["Und", "spra\u00b7chest", "lau\u00b7ter", "gar", ",", "da\u00b7mit", "er", "es", "ver\u00b7n\u00e4h\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Stelle des Gebets stahl deines Nachbars Ohr;", "tokens": ["Die", "Stel\u00b7le", "des", "Ge\u00b7bets", "stahl", "dei\u00b7nes", "Nach\u00b7bars", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur was ein Mensch nicht h\u00f6rt, schreib' ich und trag's empor.", "tokens": ["Nur", "was", "ein", "Mensch", "nicht", "h\u00f6rt", ",", "schreib'", "ich", "und", "trag's", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}