{"dta.poem.3356": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1846", "urn": "urn:nbn:de:kobv:b4-200905191464", "language": ["de:0.99"], "booktitle": "Freiligrath, Ferdinand: \u00c7a ira! Herisau, 1846."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Alle, mein\u2019 ich, habt geh\u00f6rt von jenem seltnen", "tokens": ["Ihr", "Al\u00b7le", ",", "mein'", "ich", ",", "habt", "ge\u00b7h\u00f6rt", "von", "je\u00b7nem", "selt\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eispalast!", "tokens": ["Eis\u00b7pa\u00b7last", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Auf der gefrornen Newafluth aufstarrte der gefrorne", "tokens": ["Auf", "der", "ge\u00b7fror\u00b7nen", "Ne\u00b7waf\u00b7luth", "auf\u00b7starr\u00b7te", "der", "ge\u00b7fror\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Glast!", "tokens": ["Glast", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Dem Willen einer Kaiserin, der Laune dienend", "tokens": ["Dem", "Wil\u00b7len", "ei\u00b7ner", "Kai\u00b7se\u00b7rin", ",", "der", "Lau\u00b7ne", "die\u00b7nend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "einer Frau,", "tokens": ["ei\u00b7ner", "Frau", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Scholl\u2019 \u00fcber Scholle stand er da, gediegen Eis der", "tokens": ["Scholl'", "\u00fc\u00b7ber", "Schol\u00b7le", "stand", "er", "da", ",", "ge\u00b7die\u00b7gen", "Eis", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ganze Bau!", "tokens": ["gan\u00b7ze", "Bau", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Um seine blanken Fensterreih\u2019n, um seine Giebel", "tokens": ["Um", "sei\u00b7ne", "blan\u00b7ken", "Fens\u00b7ter\u00b7reih'n", ",", "um", "sei\u00b7ne", "Gie\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,", "KOUI", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "pfiff es kalt:", "tokens": ["pfiff", "es", "kalt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Doch innen hat ihn Fr\u00fchlingsweh\u2019n und hat ihn", "tokens": ["Doch", "in\u00b7nen", "hat", "ihn", "Fr\u00fch\u00b7lings\u00b7weh'n", "und", "hat", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "KON", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Blumenhauch durchwallt!", "tokens": ["Blu\u00b7men\u00b7hauch", "durch\u00b7wallt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "All\u00fcberall, wohin man schritt, Musik und Giran-", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", ",", "wo\u00b7hin", "man", "schritt", ",", "Mu\u00b7sik", "und", "Gi\u00b7ran"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PIS", "VVFIN", "$,", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "dolenglanz,", "tokens": ["do\u00b7len\u00b7glanz", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und durch der S\u00e4le bunte Flucht bewegte wirbelnd", "tokens": ["Und", "durch", "der", "S\u00e4\u00b7le", "bun\u00b7te", "Flucht", "be\u00b7weg\u00b7te", "wir\u00b7belnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "sich der Tanz!", "tokens": ["sich", "der", "Tanz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Also, bis in den M\u00e4rz hinein, war seine Herr-", "tokens": ["Al\u00b7so", ",", "bis", "in", "den", "M\u00e4rz", "hin\u00b7ein", ",", "war", "sei\u00b7ne", "Herr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "NN", "PTKVZ", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "lichkeit zu schau\u2019n;", "tokens": ["lich\u00b7keit", "zu", "schau'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Doch \u2014 auch in Ru\u00dfland kommt der Lenz, und", "tokens": ["Doch", "auch", "in", "Ru\u00df\u00b7land", "kommt", "der", "Lenz", ",", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$(", "ADV", "APPR", "NE", "VVFIN", "ART", "NN", "$,", "KON"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auch der Newa Bl\u00f6cke thau\u2019n!", "tokens": ["auch", "der", "Ne\u00b7wa", "Bl\u00f6\u00b7cke", "thau'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hui, wie bei\u2019m ersten Sturm aus S\u00fcd der ganze", "tokens": ["Hui", ",", "wie", "bei'm", "ers\u00b7ten", "Sturm", "aus", "S\u00fcd", "der", "gan\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "$,", "PWAV", "APPRART", "ADJA", "NN", "APPR", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schimmernde Kolo\u00df", "tokens": ["schim\u00b7mern\u00b7de", "Ko\u00b7lo\u00df"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Hohl in sich selbst zusammen sank, und h\u00e4uptlings", "tokens": ["Hohl", "in", "sich", "selbst", "zu\u00b7sam\u00b7men", "sank", ",", "und", "h\u00e4upt\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "PRF", "ADV", "ADV", "VVFIN", "$,", "KON", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "in die Fluthen scho\u00df!", "tokens": ["in", "die", "Flut\u00b7hen", "scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Die Fluthen aber jauchzten auf! Ja, die der Frost", "tokens": ["Die", "Flut\u00b7hen", "a\u00b7ber", "jauchz\u00b7ten", "auf", "!", "Ja", ",", "die", "der", "Frost"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKVZ", "$.", "PTKANT", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "in Bande schlug,", "tokens": ["in", "Ban\u00b7de", "schlug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die gestern eine Hofburg noch und eines Hofes", "tokens": ["Die", "ge\u00b7stern", "ei\u00b7ne", "Hof\u00b7burg", "noch", "und", "ei\u00b7nes", "Ho\u00b7fes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "KON", "ART", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Unsinn trug,", "tokens": ["Un\u00b7sinn", "trug", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Die es noch gestern schweigend litt, da\u00df man ihr", "tokens": ["Die", "es", "noch", "ge\u00b7stern", "schwei\u00b7gend", "litt", ",", "da\u00df", "man", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,", "KOUS", "PIS", "PPER"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "auflud Pomp und Staat,", "tokens": ["auf\u00b7lud", "Pomp", "und", "Staat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df eine \u00fcpp\u2019ge Kaiserin hoff\u00e4rtig sie mit F\u00fc\u00dfen", "tokens": ["Da\u00df", "ei\u00b7ne", "\u00fcpp'\u00b7ge", "Kai\u00b7se\u00b7rin", "hof\u00b7f\u00e4r\u00b7tig", "sie", "mit", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "trat: \u2014", "tokens": ["trat", ":"], "token_info": ["word", "punct", "punct"], "pos": ["VVFIN", "$.", "$("], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "Dieselbe Newa jauchzt\u2019 empor! Abw\u00e4rts mit brau-", "tokens": ["Die\u00b7sel\u00b7be", "Ne\u00b7wa", "jauchzt'", "em\u00b7por", "!", "Ab\u00b7w\u00e4rts", "mit", "brau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sendem Ergu\u00df,", "tokens": ["sen\u00b7dem", "Er\u00b7gu\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Abw\u00e4rts durch Schnee und Schollenwerk schob sich", "tokens": ["Ab\u00b7w\u00e4rts", "durch", "Schnee", "und", "Schol\u00b7len\u00b7werk", "schob", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN", "PRF"], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}, "line.4": {"text": "und dr\u00e4ngte sich der Flu\u00df!", "tokens": ["und", "dr\u00e4ng\u00b7te", "sich", "der", "Flu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die letzten Spuren seiner Schmach malmt\u2019 er und", "tokens": ["Die", "letz\u00b7ten", "Spu\u00b7ren", "sei\u00b7ner", "Schmach", "malmt'", "er", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "KON"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.6": {"text": "knirscht\u2019 er kurz und klein \u2014", "tokens": ["knirscht'", "er", "kurz", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und str\u00f6mte gro\u00df und ruhig dann in\u2019s ewig freie", "tokens": ["Und", "str\u00f6m\u00b7te", "gro\u00df", "und", "ru\u00b7hig", "dann", "in's", "e\u00b7wig", "frei\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "ADV", "APPRART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Meer hinein!", "tokens": ["Meer", "hin\u00b7ein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}