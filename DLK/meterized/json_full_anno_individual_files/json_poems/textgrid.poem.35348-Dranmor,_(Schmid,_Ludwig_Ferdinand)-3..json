{"textgrid.poem.35348": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O welche Zeit! Wie seltsam und verwirrend", "tokens": ["O", "wel\u00b7che", "Zeit", "!", "Wie", "selt\u00b7sam", "und", "ver\u00b7wir\u00b7rend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PWAT", "NN", "$.", "PWAV", "ADJD", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie, die so wenig Licht und Freude spendet", "tokens": ["Sie", ",", "die", "so", "we\u00b7nig", "Licht", "und", "Freu\u00b7de", "spen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "PIAT", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dennoch, eine weite Bahn durchirrend,", "tokens": ["Und", "den\u00b7noch", ",", "ei\u00b7ne", "wei\u00b7te", "Bahn", "durch\u00b7ir\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Sonnenaufgang ihre Schritte wendet!", "tokens": ["Nach", "Son\u00b7nen\u00b7auf\u00b7gang", "ih\u00b7re", "Schrit\u00b7te", "wen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja, vorw\u00e4rts eilt die Zeit mit Schwert und Wage;", "tokens": ["Ja", ",", "vor\u00b7w\u00e4rts", "eilt", "die", "Zeit", "mit", "Schwert", "und", "Wa\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Uns aber ist ein solcher Trost von n\u00f6ten,", "tokens": ["Uns", "a\u00b7ber", "ist", "ein", "sol\u00b7cher", "Trost", "von", "n\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "PIAT", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn \u00fcber unsrer Herzen Niederlage", "tokens": ["Wenn", "\u00fc\u00b7ber", "uns\u00b7rer", "Her\u00b7zen", "Nie\u00b7der\u00b7la\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wir noch erschrecken, wenn wir noch err\u00f6ten.", "tokens": ["Wir", "noch", "er\u00b7schre\u00b7cken", ",", "wenn", "wir", "noch", "er\u00b7r\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Des Denkers Sch\u00e4tze sind verschm\u00e4hte W\u00e4hrung;", "tokens": ["Des", "Den\u00b7kers", "Sch\u00e4t\u00b7ze", "sind", "ver\u00b7schm\u00e4h\u00b7te", "W\u00e4h\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den Ernst des Weisen trifft des Forums Spott,", "tokens": ["Den", "Ernst", "des", "Wei\u00b7sen", "trifft", "des", "Fo\u00b7rums", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Menge Fluch; denn Mammon hei\u00dft ihr Gott", "tokens": ["Der", "Men\u00b7ge", "Fluch", ";", "denn", "Mam\u00b7mon", "hei\u00dft", "ihr", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "KON", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In diesen Tagen allgemeiner G\u00e4rung.", "tokens": ["In", "die\u00b7sen", "Ta\u00b7gen", "all\u00b7ge\u00b7mei\u00b7ner", "G\u00e4\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und des Gerechten Schmerz, so tief begr\u00fcndet,", "tokens": ["Und", "des", "Ge\u00b7rech\u00b7ten", "Schmerz", ",", "so", "tief", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "In welchen Herzen kann er Wurzeln fassen", "tokens": ["In", "wel\u00b7chen", "Her\u00b7zen", "kann", "er", "Wur\u00b7zeln", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Jetzt, wo des goldnen Kalbes Reich verk\u00fcndet", "tokens": ["Jetzt", ",", "wo", "des", "gold\u00b7nen", "Kal\u00b7bes", "Reich", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Auf allen M\u00e4rkten und in allen Gassen", "tokens": ["Auf", "al\u00b7len", "M\u00e4rk\u00b7ten", "und", "in", "al\u00b7len", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und \u00fcberall der Feind sich eingenistet,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "der", "Feind", "sich", "ein\u00b7ge\u00b7nis\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ein D\u00e4mon, der des Geistes Schwingen l\u00e4hmt,", "tokens": ["Ein", "D\u00e4\u00b7mon", ",", "der", "des", "Geis\u00b7tes", "Schwin\u00b7gen", "l\u00e4hmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doch dessen L\u00e4cheln oft die St\u00e4rksten z\u00e4hmt,", "tokens": ["Doch", "des\u00b7sen", "L\u00e4\u00b7cheln", "oft", "die", "St\u00e4rks\u00b7ten", "z\u00e4hmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Das oft die Besten, Reinsten \u00fcberlistet?", "tokens": ["Das", "oft", "die", "Bes\u00b7ten", ",", "Reins\u00b7ten", "\u00fc\u00b7berl\u00b7is\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Verlockend lautet des Versuchers Lehre", "tokens": ["Ver\u00b7lo\u00b7ckend", "lau\u00b7tet", "des", "Ver\u00b7su\u00b7chers", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und immer gr\u00f6\u00dfer wird der Narren Gilde,", "tokens": ["Und", "im\u00b7mer", "gr\u00f6\u00b7\u00dfer", "wird", "der", "Nar\u00b7ren", "Gil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und in dem Chaos luftiger Gebilde", "tokens": ["Und", "in", "dem", "Chaos", "luf\u00b7ti\u00b7ger", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Versinken Manneswert und Mannesehre.", "tokens": ["Ver\u00b7sin\u00b7ken", "Man\u00b7nes\u00b7wert", "und", "Man\u00b7ne\u00b7seh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Wohin ich blicke: T\u00e4uschung, Selbstbetrug,", "tokens": ["Wo\u00b7hin", "ich", "bli\u00b7cke", ":", "T\u00e4u\u00b7schung", ",", "Selbst\u00b7be\u00b7trug", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Verstellung, Eitelkeit, erborgter Glanz,", "tokens": ["Ver\u00b7stel\u00b7lung", ",", "Ei\u00b7tel\u00b7keit", ",", "er\u00b7borg\u00b7ter", "Glanz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Vergoldeter Gerippe Totentanz,", "tokens": ["Ver\u00b7gol\u00b7de\u00b7ter", "Ge\u00b7rip\u00b7pe", "To\u00b7ten\u00b7tanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Doch auch des Wissens stolzer Adlerflug.", "tokens": ["Doch", "auch", "des", "Wis\u00b7sens", "stol\u00b7zer", "Ad\u00b7ler\u00b7flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Wohin ich fl\u00fcchte: Selbstsucht und Beth\u00f6rung,", "tokens": ["Wo\u00b7hin", "ich", "fl\u00fcch\u00b7te", ":", "Selbst\u00b7sucht", "und", "Be\u00b7th\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Doch auch der Armut z\u00fcrnende Gestalten;", "tokens": ["Doch", "auch", "der", "Ar\u00b7mut", "z\u00fcr\u00b7nen\u00b7de", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und \u00fcberall, wo falsche G\u00f6tter walten,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", ",", "wo", "fal\u00b7sche", "G\u00f6t\u00b7ter", "wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Die Schreckenszeichen nahender Emp\u00f6rung.", "tokens": ["Die", "Schre\u00b7cken\u00b7szei\u00b7chen", "na\u00b7hen\u00b7der", "Em\u00b7p\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Noch immer r\u00e4tselhaft und unverstanden", "tokens": ["Noch", "im\u00b7mer", "r\u00e4t\u00b7sel\u00b7haft", "und", "un\u00b7ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Ist diese Zeit, die Gro\u00dfes schon erstritten,", "tokens": ["Ist", "die\u00b7se", "Zeit", ",", "die", "Gro\u00b7\u00dfes", "schon", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Die nach Erl\u00f6sung seufzt aus alten Banden", "tokens": ["Die", "nach", "Er\u00b7l\u00f6\u00b7sung", "seufzt", "aus", "al\u00b7ten", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Und doch sich fortbewegt mit Riesenschritten", "tokens": ["Und", "doch", "sich", "fort\u00b7be\u00b7wegt", "mit", "Rie\u00b7sen\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PRF", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und vorw\u00e4rts strebt zu dem umw\u00f6lkten Lichte \u2013", "tokens": ["Und", "vor\u00b7w\u00e4rts", "strebt", "zu", "dem", "um\u00b7w\u00f6lk\u00b7ten", "Lich\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Es wird die Erde aus der Knechtschaft Schmach", "tokens": ["Es", "wird", "die", "Er\u00b7de", "aus", "der", "Knecht\u00b7schaf