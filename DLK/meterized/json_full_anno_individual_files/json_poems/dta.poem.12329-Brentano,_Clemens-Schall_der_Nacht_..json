{"dta.poem.12329": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Schall der Nacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm Trost der Nacht, o Nachtigall!               ", "tokens": ["Komm", "Trost", "der", "Nacht", ",", "o", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df deine Stimm mit Freuden-Schall", "tokens": ["La\u00df", "dei\u00b7ne", "Stimm", "mit", "Freu\u00b7den\u00b7Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufs lieblichste erklingen,", "tokens": ["Aufs", "lieb\u00b7lichs\u00b7te", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, komm, und lob den Sch\u00f6pfer dein,", "tokens": ["Komm", ",", "komm", ",", "und", "lob", "den", "Sch\u00f6p\u00b7fer", "dein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KON", "NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil andre V\u00f6gel schlafen seyn,", "tokens": ["Weil", "and\u00b7re", "V\u00f6\u00b7gel", "schla\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht mehr m\u00f6gen singen;", "tokens": ["Und", "nicht", "mehr", "m\u00f6\u00b7gen", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df dein Stimmlein", "tokens": ["La\u00df", "dein", "Stimm\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Laut erschallen, denn vor allen", "tokens": ["Laut", "er\u00b7schal\u00b7len", ",", "denn", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVINF", "$,", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kannst du loben", "tokens": ["Kannst", "du", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Gott im Himmel, hoch dort oben.", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "hoch", "dort", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Obschon ist hin der Sonnenschein,", "tokens": ["Ob\u00b7schon", "ist", "hin", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wir im Finstern m\u00fcssen seyn,", "tokens": ["Und", "wir", "im", "Fins\u00b7tern", "m\u00fcs\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00f6nnen wir doch singen", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "doch", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Gottes G\u00fct und seiner Macht,", "tokens": ["Von", "Got\u00b7tes", "G\u00fct", "und", "sei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil uns kann hindern keine Nacht,", "tokens": ["Weil", "uns", "kann", "hin\u00b7dern", "kei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Loben zu vollbringen.", "tokens": ["Sein", "Lo\u00b7ben", "zu", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum dein Stimmlein", "tokens": ["Drum", "dein", "Stimm\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "La\u00df erschallen, denn vor allen", "tokens": ["La\u00df", "er\u00b7schal\u00b7len", ",", "denn", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kannst du loben", "tokens": ["Kannst", "du", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Gott im Himmel, hoch dort oben.", "tokens": ["Gott", "im", "Him\u00b7mel", ",", "hoch", "dort", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}