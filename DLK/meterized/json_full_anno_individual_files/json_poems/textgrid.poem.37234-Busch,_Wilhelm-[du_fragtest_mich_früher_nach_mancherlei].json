{"textgrid.poem.37234": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[du fragtest mich fr\u00fcher nach mancherlei]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fragtest mich fr\u00fcher nach mancherlei.", "tokens": ["Du", "frag\u00b7test", "mich", "fr\u00fc\u00b7her", "nach", "man\u00b7cher\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PIS", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich sagte dir alles frank und frei.", "tokens": ["Ich", "sag\u00b7te", "dir", "al\u00b7les", "frank", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du fragtest, wann ich zu reisen ged\u00e4chte,", "tokens": ["Du", "frag\u00b7test", ",", "wann", "ich", "zu", "rei\u00b7sen", "ge\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Welch ein Gesch\u00e4ft ich machen m\u00f6chte.", "tokens": ["Welch", "ein", "Ge\u00b7sch\u00e4ft", "ich", "ma\u00b7chen", "m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich sagte dir offen: dann und dann;", "tokens": ["Ich", "sag\u00b7te", "dir", "of\u00b7fen", ":", "dann", "und", "dann", ";"], "token_info": ["word", "word", "w