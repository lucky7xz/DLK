{"textgrid.poem.31609": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das sch\u00f6ne Blumen-Heer / geht wider um zu Feld /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das sch\u00f6ne Blumen-Heer / geht wider um zu Feld /", "tokens": ["Das", "sch\u00f6\u00b7ne", "Blu\u00b7men\u00b7Heer", "/", "geht", "wi\u00b7der", "um", "zu", "Feld", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "APPR", "KOUI", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "um Ruch und Farben-Pracht recht in die Welt zu streiten:", "tokens": ["um", "Ruch", "und", "Fa\u00b7rben\u00b7Pracht", "recht", "in", "die", "Welt", "zu", "strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des Laubes Lorbeer-str\u00e4uch bekr\u00e4nzen's aller seiten.", "tokens": ["des", "Lau\u00b7bes", "Lor\u00b7beer\u00b7str\u00e4uch", "be\u00b7kr\u00e4n\u00b7zen's", "al\u00b7ler", "sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dryaden schlagen auf die k\u00fchlen Schatten-Zelt.", "tokens": ["Dry\u00b7a\u00b7den", "schla\u00b7gen", "auf", "die", "k\u00fch\u00b7len", "Schat\u00b7ten\u00b7Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter":