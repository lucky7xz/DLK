{"textgrid.poem.61117": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Sklav", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O w\u00e4r' ich frei und reich, ein Pascha sondergleichen,", "tokens": ["O", "w\u00e4r'", "ich", "frei", "und", "reich", ",", "ein", "Pa\u00b7scha", "son\u00b7derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie liebt' ich dann dies Land mit seinen Lorbeerstr\u00e4uchen,", "tokens": ["Wie", "liebt'", "ich", "dann", "dies", "Land", "mit", "sei\u00b7nen", "Lor\u00b7beer\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PDS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Korn und Trauben segenschwer,", "tokens": ["Von", "Korn", "und", "Trau\u00b7ben", "se\u00b7gen\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies klare Sonnengold in den kristallnen L\u00fcften,", "tokens": ["Dies", "kla\u00b7re", "Son\u00b7nen\u00b7gold", "in", "den", "kris\u00b7tall\u00b7nen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Diese G\u00e4rten, durchw\u00fcrzt von ew'gen Rosend\u00fcften,", "tokens": ["Die\u00b7se", "G\u00e4r\u00b7ten", ",", "durch\u00b7w\u00fcrzt", "von", "ew'\u00b7gen", "Ro\u00b7sen\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und dieses gl\u00e4nzend blaue Meer!", "tokens": ["Und", "die\u00b7ses", "gl\u00e4n\u00b7zend", "blau\u00b7e", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um Mittag ruht' ich dann auf weichen Purpurdecken", "tokens": ["Um", "Mit\u00b7tag", "ruht'", "ich", "dann", "auf", "wei\u00b7chen", "Pur\u00b7pur\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im luftigen Gemach, wo im marmornen Becken", "tokens": ["Im", "luf\u00b7ti\u00b7gen", "Ge\u00b7mach", ",", "wo", "im", "mar\u00b7mor\u00b7nen", "Be\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Der Springflut Rauschen nie verstummt;", "tokens": ["Der", "Spring\u00b7flut", "Rau\u00b7schen", "nie", "ver\u00b7stummt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wo ein schwarzer Knab', am Nigerstrand geboren,", "tokens": ["Und", "wo", "ein", "schwar\u00b7zer", "Knab'", ",", "am", "Ni\u00b7ger\u00b7strand", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit krausem Wollenhaar, Goldringe in den Ohren,", "tokens": ["Mit", "krau\u00b7sem", "Wol\u00b7len\u00b7haar", ",", "Gold\u00b7rin\u00b7ge", "in", "den", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein Liedchen zur Gitarre summt.", "tokens": ["Sein", "Lied\u00b7chen", "zur", "Gi\u00b7tar\u00b7re", "summt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder auf stolzem Ro\u00df von echt arab'schem Stamme,", "tokens": ["O\u00b7der", "auf", "stol\u00b7zem", "Ro\u00df", "von", "echt", "a\u00b7rab'\u00b7schem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dessen Lauf wie der Wind, des Auge wie die Flamme,", "tokens": ["Des\u00b7sen", "Lauf", "wie", "der", "Wind", ",", "des", "Au\u00b7ge", "wie", "die", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KOKOM", "ART", "NN", "$,", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Fl\u00f6g' ich dahin durch Tal und H\u00f6hn,", "tokens": ["Fl\u00f6g'", "ich", "da\u00b7hin", "durch", "Tal", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die Felder von Mais, beschattet von Platanen,", "tokens": ["Durch", "die", "Fel\u00b7der", "von", "Mais", ",", "be\u00b7schat\u00b7tet", "von", "Pla\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Den pr\u00e4cht'gen Strom entlang, wo stolz wie gr\u00fcne Fahnen", "tokens": ["Den", "pr\u00e4cht'\u00b7gen", "Strom", "ent\u00b7lang", ",", "wo", "stolz", "wie", "gr\u00fc\u00b7ne", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PWAV", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Palmen breite F\u00e4cher wehn.", "tokens": ["Der", "Pal\u00b7men", "brei\u00b7te", "F\u00e4\u00b7cher", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und um die Zeit, wo s\u00fc\u00df die Nachtigallen klagen,", "tokens": ["Und", "um", "die", "Zeit", ",", "wo", "s\u00fc\u00df", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lie\u00df ich ein leicht Gezelt von Seidenstoff mir schlagen", "tokens": ["Lie\u00df", "ich", "ein", "leicht", "Ge\u00b7zelt", "von", "Sei\u00b7den\u00b7stoff", "mir", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Am Berg auf k\u00fchlem Wiesensamt:", "tokens": ["Am", "Berg", "auf", "k\u00fch\u00b7lem", "Wie\u00b7sen\u00b7samt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich s\u00e4he fern das Meer sich dehnen unermessen", "tokens": ["Ich", "s\u00e4\u00b7he", "fern", "das", "Meer", "sich", "deh\u00b7nen", "un\u00b7er\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und an der Bucht die Stadt mit Kuppeln und Zypressen", "tokens": ["Und", "an", "der", "Bucht", "die", "Stadt", "mit", "Kup\u00b7peln", "und", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vom Abendpurpur \u00fcberflammt.", "tokens": ["Vom", "A\u00b7bend\u00b7pur\u00b7pur", "\u00fc\u00b7berf\u00b7lammt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dann die s\u00fc\u00dfe Nacht! Auf schwebender Galeere", "tokens": ["Und", "dann", "die", "s\u00fc\u00b7\u00dfe", "Nacht", "!", "Auf", "schwe\u00b7ben\u00b7der", "Ga\u00b7lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchr' ich bei Fl\u00f6tenschall hinaus zum stillen Meere,", "tokens": ["F\u00fchr'", "ich", "bei", "Fl\u00f6\u00b7ten\u00b7schall", "hin\u00b7aus", "zum", "stil\u00b7len", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "APZR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bei des Halbmonds D\u00e4mmerschein", "tokens": ["Und", "bei", "des", "Halb\u00b7monds", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6b' ich mit leiser Hand der Favorite Schleier", "tokens": ["H\u00f6b'", "ich", "mit", "lei\u00b7ser", "Hand", "der", "Fa\u00b7vo\u00b7ri\u00b7te", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und s\u00e4h' ein dunkles Aug', in dem das tiefe Feuer", "tokens": ["Und", "s\u00e4h'", "ein", "dunk\u00b7les", "Aug'", ",", "in", "dem", "das", "tie\u00b7fe", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verhei\u00dfend spr\u00e4che: \u00bbIch bin dein!\u00ab - -", "tokens": ["Ver\u00b7hei\u00b7\u00dfend", "spr\u00e4\u00b7che", ":", "\u00bb", "Ich", "bin", "dein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVPP", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PPOSAT", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So tr\u00e4umte s\u00fc\u00df der Sklav'. Da klirrte seine Kette,", "tokens": ["So", "tr\u00e4um\u00b7te", "s\u00fc\u00df", "der", "Skla\u00b7v'", ".", "Da", "klirr\u00b7te", "sei\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Er fuhr verst\u00f6rt empor von seiner Lagerst\u00e4tte", "tokens": ["Er", "fuhr", "ver\u00b7st\u00f6rt", "em\u00b7por", "von", "sei\u00b7ner", "La\u00b7ger\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit bangem Blick, mit blassem Mund;", "tokens": ["Mit", "ban\u00b7gem", "Blick", ",", "mit", "blas\u00b7sem", "Mund", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn schon verschwand im Blau der Morgenstern erbleichend,", "tokens": ["Denn", "schon", "ver\u00b7schwand", "im", "Blau", "der", "Mor\u00b7gens\u00b7tern", "er\u00b7blei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und vor ihm stand der Vogt, den krausen Bart sich streichend,", "tokens": ["Und", "vor", "ihm", "stand", "der", "Vogt", ",", "den", "krau\u00b7sen", "Bart", "sich", "strei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und rief: \u00bbZur Arbeit fort, du Hund!\u00ab", "tokens": ["Und", "rief", ":", "\u00bb", "Zur", "Ar\u00b7beit", "fort", ",", "du", "Hund", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "APPRART", "NN", "PTKVZ", "$,", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O w\u00e4r' ich frei und reich, ein Pascha sondergleichen,", "tokens": ["O", "w\u00e4r'", "ich", "frei", "und", "reich", ",", "ein", "Pa\u00b7scha", "son\u00b7derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie liebt' ich dann dies Land mit seinen Lorbeerstr\u00e4uchen,", "tokens": ["Wie", "liebt'", "ich", "dann", "dies", "Land", "mit", "sei\u00b7nen", "Lor\u00b7beer\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PDS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Korn und Trauben segenschwer,", "tokens": ["Von", "Korn", "und", "Trau\u00b7ben", "se\u00b7gen\u00b7s