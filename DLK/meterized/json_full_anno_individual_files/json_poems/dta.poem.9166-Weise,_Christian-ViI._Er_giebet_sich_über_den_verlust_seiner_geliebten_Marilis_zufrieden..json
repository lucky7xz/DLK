{"dta.poem.9166": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n Er giebet sich \u00fcber den verlust seiner geliebten  \n Marilis zufrieden.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.85", "nl:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Was gedenckst du liebes hertze?", "tokens": ["Was", "ge\u00b7denckst", "du", "lie\u00b7bes", "hert\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darffst du nicht zum m\u00e4dgen gehn?", "tokens": ["Darffst", "du", "nicht", "zum", "m\u00e4d\u00b7gen", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df sie deinem freyen schertze", "tokens": ["Mu\u00df", "sie", "dei\u00b7nem", "frey\u00b7en", "schert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halb gezwungen widerstehn?", "tokens": ["Halb", "ge\u00b7zwun\u00b7gen", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib dich nur gedultig drein/", "tokens": ["Gib", "dich", "nur", "ge\u00b7dul\u00b7tig", "drein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du kanst doch wohl lustig seyn.", "tokens": ["Du", "kanst", "doch", "wohl", "lus\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "2. La\u00df den blinden eyfer schelten/", "tokens": ["La\u00df", "den", "blin\u00b7den", "ey\u00b7fer", "schel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verbleibe wohlgefasst/", "tokens": ["Und", "ver\u00b7blei\u00b7be", "wohl\u00b7ge\u00b7fasst", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles doppelt zu entgelten/", "tokens": ["Al\u00b7les", "dop\u00b7pelt", "zu", "ent\u00b7gel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was du nicht verdienet hast:", "tokens": ["Was", "du", "nicht", "ver\u00b7die\u00b7net", "hast", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uberwinde den verdacht/", "tokens": ["U\u00b7ber\u00b7win\u00b7de", "den", "ver\u00b7dacht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welcher dich geh\u00e4ssig macht.", "tokens": ["Wel\u00b7cher", "dich", "ge\u00b7h\u00e4s\u00b7sig", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Sch\u00fctze mir dein gut gewissen", "tokens": ["Sch\u00fct\u00b7ze", "mir", "dein", "gut", "ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der seelen unschuld f\u00fcr/", "tokens": ["Und", "der", "see\u00b7len", "un\u00b7schuld", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirstu was verlieren m\u00fcssen/", "tokens": ["Wirs\u00b7tu", "was", "ver\u00b7lie\u00b7ren", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So gedencke da\u00df du hier", "tokens": ["So", "ge\u00b7den\u00b7cke", "da\u00df", "du", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch auf eine kurtze frist", "tokens": ["Noch", "auf", "ei\u00b7ne", "kurt\u00b7ze", "frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fu\u00df zu halten willens bist.", "tokens": ["Fu\u00df", "zu", "hal\u00b7ten", "wil\u00b7lens", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Wirff die augen auch zur\u00fccke", "tokens": ["Wirff", "die", "au\u00b7gen", "auch", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auff das allererste jahr/", "tokens": ["Auff", "das", "al\u00b7le\u00b7rers\u00b7te", "jahr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als dein eingebildtes gl\u00fccke", "tokens": ["Als", "dein", "ein\u00b7ge\u00b7bild\u00b7tes", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch in weitem felde war/", "tokens": ["Noch", "in", "wei\u00b7tem", "fel\u00b7de", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast du da von keiner lust", "tokens": ["Hast", "du", "da", "von", "kei\u00b7ner", "lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eben als wie itzt gewust.", "tokens": ["E\u00b7ben", "als", "wie", "itzt", "ge\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "KOKOM", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Denn der ungebundne geist", "tokens": ["Denn", "der", "un\u00b7ge\u00b7bund\u00b7ne", "geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird von dir nicht abgeschieden/", "tokens": ["Wird", "von", "dir", "nicht", "ab\u00b7ge\u00b7schie\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bi\u00df der lebens-faden reist/", "tokens": ["Bi\u00df", "der", "le\u00b7bens\u00b7fa\u00b7den", "reist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dessen freyheit stellet dir", "tokens": ["Des\u00b7sen", "frey\u00b7heit", "stel\u00b7let", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeugs genug zum lachen f\u00fcr.", "tokens": ["Zeugs", "ge\u00b7nug", "zum", "la\u00b7chen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "VVINF", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Wann die edle freundlichkeit", "tokens": ["Wann", "die", "ed\u00b7le", "freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die gr\u00fc\u00df und gegengr\u00fcsse", "tokens": ["Durch", "die", "gr\u00fc\u00df", "und", "ge\u00b7gen\u00b7gr\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich von tag zu tag erneut/", "tokens": ["Sich", "von", "tag", "zu", "tag", "er\u00b7neut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "APPR", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann der arm den leib umschlingt/", "tokens": ["Wann", "der", "arm", "den", "leib", "um\u00b7schlingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der mund die seele zwingt.", "tokens": ["Und", "der", "mund", "die", "see\u00b7le", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Ist auch irgend eine freude/", "tokens": ["Ist", "auch", "ir\u00b7gend", "ei\u00b7ne", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die uns also sanffte thut:", "tokens": ["Die", "uns", "al\u00b7so", "sanff\u00b7te", "thut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach wie wallt das eingeweide", "tokens": ["Ach", "wie", "wallt", "das", "ein\u00b7ge\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie erzittert marck und blut/", "tokens": ["Wie", "er\u00b7zit\u00b7tert", "marck", "und", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die liebste sich geneigt/", "tokens": ["Wann", "die", "liebs\u00b7te", "sich", "ge\u00b7neigt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "PRF", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur in blossen minen zeigt.", "tokens": ["Nur", "in", "blos\u00b7sen", "mi\u00b7nen", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Schweig mein hertz/ was wilst du machen/", "tokens": ["Schweig", "mein", "hertz", "/", "was", "wilst", "du", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dencke nur nicht mehr daran:", "tokens": ["Den\u00b7cke", "nur", "nicht", "mehr", "da\u00b7ran", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben dir die s\u00fcssen sachen", "tokens": ["Ha\u00b7ben", "dir", "die", "s\u00fcs\u00b7sen", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja vor diesem wohl gethan", "tokens": ["Ja", "vor", "die\u00b7sem", "wohl", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "PDAT", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey so stelle schertz und pein", "tokens": ["Ey", "so", "stel\u00b7le", "schertz", "und", "pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach des gl\u00fcckes willen ein.", "tokens": ["Nach", "des", "gl\u00fc\u00b7ckes", "wil\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "9. Wilst du dich an eine binden?", "tokens": ["Wilst", "du", "dich", "an", "ei\u00b7ne", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein du wirst die gantze welt/", "tokens": ["Nein", "du", "wirst", "die", "gant\u00b7ze", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Voll verliebter seelen finden/", "tokens": ["Voll", "ver\u00b7lieb\u00b7ter", "see\u00b7len", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo dir ja solch thun gef\u00e4llt:", "tokens": ["Wo", "dir", "ja", "solch", "thun", "ge\u00b7f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "VVINF", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Eine schad f\u00fcrwahr nicht viel/", "tokens": ["Ei\u00b7ne", "schad", "f\u00fcr\u00b7wahr", "nicht", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PTKNEG", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jmmerhin was lauffen wil.", "tokens": ["Jm\u00b7mer\u00b7hin", "was", "lauf\u00b7fen", "wil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "10. Suche deine guten br\u00fcder/", "tokens": ["Su\u00b7che", "dei\u00b7ne", "gu\u00b7ten", "br\u00fc\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm die karten und ein glas/", "tokens": ["Nimm", "die", "kar\u00b7ten", "und", "ein", "glas", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "KON", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe neue possen-lieder/", "tokens": ["Sin\u00b7ge", "neu\u00b7e", "pos\u00b7sen\u00b7lie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seele wie gef\u00e4llt dir das?", "tokens": ["See\u00b7le", "wie", "ge\u00b7f\u00e4llt", "dir", "das", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gelt es geht so lieblich ein", "tokens": ["Gelt", "es", "geht", "so", "lieb\u00b7lich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "ADV", "ADJD", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als wann wir beym m\u00e4dgen seyn?", "tokens": ["Als", "wann", "wir", "beym", "m\u00e4d\u00b7gen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "APPRART", "ADJA", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}