{"dta.poem.3810": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Anderer Abschnitt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Statt einer", "tokens": ["Statt", "ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "PrACTICA", "tokens": ["PrAC\u00b7TI\u00b7CA"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "soll hier ein W\u00fcnschgen", "tokens": ["soll", "hier", "ein", "W\u00fcnschgen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "stehen.", "tokens": ["ste\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Was aber w\u00fcnschen", "tokens": ["Was", "a\u00b7ber", "w\u00fcn\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "wir? Viel tausend", "tokens": ["wir", "?", "Viel", "tau\u00b7send"], "token_info": ["word", "punct", "word", "word"], "pos": ["PPER", "$.", "ADV", "CARD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wohlergehen.", "tokens": ["Woh\u00b7ler\u00b7ge\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Das Gl\u00fcck sey,", "tokens": ["Das", "Gl\u00fcck", "sey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Werthes Paar,", "tokens": ["Wert\u00b7hes", "Paar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "in ieder Jahres-Zeit/", "tokens": ["in", "ie\u00b7der", "Jah\u00b7res\u00b7Zeit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "zu angenehmen Dienst", "tokens": ["zu", "an\u00b7ge\u00b7neh\u00b7men", "Dienst"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "stets gegen Euch bereit/", "tokens": ["stets", "ge\u00b7gen", "Euch", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der Br\u00e4utigam soll", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "soll"], "token_info": ["word", "word", "word"], "pos": ["ART", "NE", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "nichts von Sonnenfin-", "tokens": ["nichts", "von", "Son\u00b7nen\u00b7fin"], "token_info": ["word", "word", "word"], "pos": ["PIS", "APPR", "TRUNC"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "sternissen in seinem E-", "tokens": ["ster\u00b7nis\u00b7sen", "in", "sei\u00b7nem", "E"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "hestand erfahren oder", "tokens": ["hes\u00b7tand", "er\u00b7fah\u00b7ren", "o\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "VVINF", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "wissen. F\u00e4llt ja Ver-", "tokens": ["wis\u00b7sen", ".", "F\u00e4llt", "ja", "Ver"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVINF", "$.", "VVFIN", "ADV", "TRUNC"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "finsterung am lieben", "tokens": ["fins\u00b7te\u00b7rung", "am", "lie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Monden ein, so mu\u00df sie", "tokens": ["Mon\u00b7den", "ein", ",", "so", "mu\u00df", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VMFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "bey der Braut doch gar", "tokens": ["bey", "der", "Braut", "doch", "gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "unsichtbar seyn. Der", "tokens": ["un\u00b7sicht\u00b7bar", "seyn", ".", "Der"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "VAINF", "$.", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Raum erlaubet nicht,", "tokens": ["Raum", "er\u00b7lau\u00b7bet", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "viel Worte herzuschrei-", "tokens": ["viel", "Wor\u00b7te", "her\u00b7zu\u00b7schrei"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "ben. Wir hoffen/ da\u00df", "tokens": ["ben", ".", "Wir", "hof\u00b7fen", "/", "da\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVPP", "$.", "PPER", "VVFIN", "$(", "KOUS"], "meter": "--+-+", "measure": "anapaest.init"}, "line.25": {"text": "bey Euch stets wird gut", "tokens": ["bey", "Euch", "stets", "wird", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.26": {"text": "Wetter bleiben. Des", "tokens": ["Wet\u00b7ter", "blei\u00b7ben", ".", "Des"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVINF", "$.", "ART"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Himmels G\u00fcte nehm", "tokens": ["Him\u00b7mels", "G\u00fc\u00b7te", "nehm"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.28": {"text": "Euch allzeit wohl in", "tokens": ["Euch", "all\u00b7zeit", "wohl", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.29": {"text": "acht, da\u00df Jhr nicht mit", "tokens": ["acht", ",", "da\u00df", "Ihr", "nicht", "mit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "$,", "KOUS", "PPER", "PTKNEG", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.30": {"text": "Verdru\u00df dereinst Ca-", "tokens": ["Ver\u00b7dru\u00df", "de\u00b7reinst", "Ca"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "TRUNC"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "lender macht.", "tokens": ["len\u00b7der", "macht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "--+", "measure": "anapaest.init"}}}}}