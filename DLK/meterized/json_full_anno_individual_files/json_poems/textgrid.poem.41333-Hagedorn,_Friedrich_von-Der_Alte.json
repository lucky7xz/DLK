{"textgrid.poem.41333": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Alte", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der weisheitvolle Greis, der gegenw\u00e4rtge Zeiten", "tokens": ["Der", "weis\u00b7heit\u00b7vol\u00b7le", "Greis", ",", "der", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ge", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hofmeisterlich belehrt, der Freund der Schwierigkeiten,", "tokens": ["Hof\u00b7meis\u00b7ter\u00b7lich", "be\u00b7lehrt", ",", "der", "Freund", "der", "Schwie\u00b7rig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist h\u00e4misch, mi\u00dfvergn\u00fcgt, der Erben Trost und Last,", "tokens": ["Ist", "h\u00e4\u00b7misch", ",", "mi\u00df\u00b7ver\u00b7gn\u00fcgt", ",", "der", "Er\u00b7ben", "Trost", "und", "Last", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VVFIN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hoffet, scherzt und liebt so frostig, als er ha\u00dft:", "tokens": ["Und", "hof\u00b7fet", ",", "scherzt", "und", "liebt", "so", "fros\u00b7tig", ",", "als", "er", "ha\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", 