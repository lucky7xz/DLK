{"textgrid.poem.47574": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[kehr' ich einmal aus der Erde]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kehr' ich einmal aus der Erde", "tokens": ["Kehr'", "ich", "ein\u00b7mal", "aus", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Moderigem Schlunde wieder,", "tokens": ["Mo\u00b7de\u00b7ri\u00b7gem", "Schlun\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Eilig, eilig in die Schenke", "tokens": ["Ei\u00b7lig", ",", "ei\u00b7lig", "in", "die", "Schen\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wander' ich zur Stunde wieder.", "tokens": ["Wan\u00b7der'", "ich", "zur", "Stun\u00b7de", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier die wohlbekannten alten,", "tokens": ["Hier", "die", "wohl\u00b7be\u00b7kann\u00b7ten", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder neue gute Br\u00fcder", "tokens": ["O\u00b7der", "neu\u00b7e", "gu\u00b7te", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treff' ich an und zeche jubelnd", "tokens": ["Treff'", "ich", "an", "und", "ze\u00b7che", "ju\u00b7belnd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In gewohntem Runde wieder.", "tokens": ["In", "ge\u00b7wohn\u00b7tem", "Run\u00b7de", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sind sodann auch noch vorhanden", "tokens": ["Sind", "so\u00b7dann", "auch", "noch", "vor\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebliche Rubinenmunde,", "tokens": ["Lieb\u00b7li\u00b7che", "Ru\u00b7bi\u00b7nen\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O so k\u00fcss' ich ohne Zweifel", "tokens": ["O", "so", "k\u00fcss'", "ich", "oh\u00b7ne", "Zwei\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch dergleichen Munde wieder.", "tokens": ["Auch", "derg\u00b7lei\u00b7chen", "Mun\u00b7de", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sollte noch Kapuz' und Kutte", "tokens": ["Soll\u00b7te", "noch", "Ka\u00b7puz'", "und", "Kut\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die betrogne Welt verd\u00fcstern,", "tokens": ["Die", "be\u00b7trog\u00b7ne", "Welt", "ver\u00b7d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O geha\u00dft werd' ich gewi\u00dflich", "tokens": ["O", "ge\u00b7ha\u00dft", "werd'", "ich", "ge\u00b7wi\u00df\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch von diesem Schunde wieder!", "tokens": ["Auch", "von", "die\u00b7sem", "Schun\u00b7de", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "T\u00f6dtet dich, Hafis, die Liebe,", "tokens": ["T\u00f6d\u00b7tet", "dich", ",", "Ha\u00b7fis", ",", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Richtet dich der Wein zu Grunde,", "tokens": ["Rich\u00b7tet", "dich", "der", "Wein", "zu", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geh', so oft du lebst, durch diese", "tokens": ["Geh'", ",", "so", "oft", "du", "lebst", ",", "durch", "die\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,", "APPR", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfbe