{"dta.poem.7169": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zirckel, den kein Mensch mit Worten,", "tokens": ["Zir\u00b7ckel", ",", "den", "kein", "Mensch", "mit", "Wor\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und kein Geist, durch Dencken, misst,", "tokens": ["Und", "kein", "Geist", ",", "durch", "Den\u00b7cken", ",", "misst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen Mittel aller Orten,", "tokens": ["Des\u00b7sen", "Mit\u00b7tel", "al\u00b7ler", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dessen Umkrei\u00df nirgends ist!", "tokens": ["Des\u00b7sen", "Um\u00b7krei\u00df", "nir\u00b7gends", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geist, der Geistern Geist und Leben", "tokens": ["Geist", ",", "der", "Geis\u00b7tern", "Geist", "und", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Krafft und Herrlichkeit gegeben:", "tokens": ["Krafft", "und", "Herr\u00b7lich\u00b7keit", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die Gr\u00f6sse wird Dein Stand", "tokens": ["Durch", "die", "Gr\u00f6s\u00b7se", "wird", "Dein", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kund zwar, doch auch unbekannt.", "tokens": ["Kund", "zwar", ",", "doch", "auch", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}