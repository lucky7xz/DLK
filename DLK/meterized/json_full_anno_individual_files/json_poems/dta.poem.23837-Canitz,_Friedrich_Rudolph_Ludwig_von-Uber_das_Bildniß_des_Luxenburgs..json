{"dta.poem.23837": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Uber das Bildni\u00df des Luxenburgs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Es soll dir Gl\u00fcck und Sieg seyn immer zugesellt/", "tokens": ["Es", "soll", "dir", "Gl\u00fcck", "und", "Sieg", "seyn", "im\u00b7mer", "zu\u00b7ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "KON", "NN", "PPOSAT", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach Satan/ als ich ihm den krummen Rumpff ver-", "tokens": ["Sprach", "Sa\u00b7tan", "/", "als", "ich", "ihm", "den", "krum\u00b7men", "Rumpff", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$(", "KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da Franckreich ist ersch\u00f6pfft/ holt er mich aus der Welt/", "tokens": ["Da", "Fran\u00b7ck\u00b7reich", "ist", "er\u00b7sch\u00f6pfft", "/", "holt", "er", "mich", "aus", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "VVPP", "$(", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "So da\u00df der schlaue Schelm mir nichts ist schuldig", "tokens": ["So", "da\u00df", "der", "schlau\u00b7e", "Schelm", "mir", "nichts", "ist", "schul\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "PPER", "PIS", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach h\u00e4tte nicht die Noht mein Vaterland gedr\u00fcckt/", "tokens": ["Ach", "h\u00e4t\u00b7te", "nicht", "die", "Noht", "mein", "Va\u00b7ter\u00b7land", "ge\u00b7dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ich nur diesen Punct in den Contract ger\u00fcckt.", "tokens": ["Und", "ich", "nur", "die\u00b7sen", "Punct", "in", "den", "Con\u00b7tract", "ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PDAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}