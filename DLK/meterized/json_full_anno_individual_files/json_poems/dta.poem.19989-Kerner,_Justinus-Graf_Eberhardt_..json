{"dta.poem.19989": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Graf Eberhardt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von W\u00fcrtenberg Graf Eberhardt,", "tokens": ["Von", "W\u00fcr\u00b7ten\u00b7berg", "Graf", "E\u00b7ber\u00b7hardt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Nun alt und la\u00df nach mancher Fahrt,", "tokens": ["Nun", "alt", "und", "la\u00df", "nach", "man\u00b7cher", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVIMP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Legt hin sein rostig Schwerdt von Stahl", "tokens": ["Legt", "hin", "sein", "ros\u00b7tig", "Schwerdt", "von", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und steigt hinab ins stille Thal.", "tokens": ["Und", "steigt", "hin\u00b7ab", "ins", "stil\u00b7le", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort wo in Tiefen wunderbar", "tokens": ["Dort", "wo", "in", "Tie\u00b7fen", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Enzfey schon manch tausend Jahr", "tokens": ["Die", "Enz\u00b7fey", "schon", "manch", "tau\u00b7send", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PIAT", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wasser w\u00e4rmt, den Siechen heilt,", "tokens": ["Die", "Was\u00b7ser", "w\u00e4rmt", ",", "den", "Sie\u00b7chen", "heilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Friedlich der kranke K\u00e4mpe weilt.", "tokens": ["Fried\u00b7lich", "der", "kran\u00b7ke", "K\u00e4m\u00b7pe", "weilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Und wie er ruht in Quellen warm,", "tokens": ["Und", "wie", "er", "ruht", "in", "Quel\u00b7len", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heranst\u00fcrzt neuer Feinde Schwarm,", "tokens": ["Her\u00b7an\u00b7st\u00fcrzt", "neu\u00b7er", "Fein\u00b7de", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auflodert hell das gr\u00fcne Thal:", "tokens": ["Auf\u00b7lo\u00b7dert", "hell", "das", "gr\u00fc\u00b7ne", "Thal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo bist du rostig Schwert von Stahl? \u2014", "tokens": ["Wo", "bist", "du", "ros\u00b7tig", "Schwert", "von", "Stahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu mir, zu mir! alt' K\u00e4mpe traut!", "tokens": ["Zu", "mir", ",", "zu", "mir", "!", "alt'", "K\u00e4m\u00b7pe", "traut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$.", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ert\u00f6nt aus Tiefen s\u00fc\u00dfer Laut;", "tokens": ["Er\u00b7t\u00f6nt", "aus", "Tie\u00b7fen", "s\u00fc\u00b7\u00dfer", "Laut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Graf sinkt in der Quellen Grund,", "tokens": ["Der", "Graf", "sinkt", "in", "der", "Quel\u00b7len", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Feind den Grafen nimmer fund.", "tokens": ["Der", "Feind", "den", "Gra\u00b7fen", "nim\u00b7mer", "fund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu Stuttgart sitzt er beym Pokal,", "tokens": ["Zu", "Stutt\u00b7gart", "sitzt", "er", "beym", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Seit' sein rostig Schwert von Stahl,", "tokens": ["Zur", "Seit'", "sein", "ros\u00b7tig", "Schwert", "von", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Enzfey trinkt er zu mit Macht", "tokens": ["Der", "Enz\u00b7fey", "trinkt", "er", "zu", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00fcrzt verj\u00fcngt sich in die Schlacht.", "tokens": ["Und", "st\u00fcrzt", "ver\u00b7j\u00fcngt", "sich", "in", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}