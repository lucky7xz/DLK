{"textgrid.poem.31531": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Das Erste:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergib' O Vatter / das / was sie an mir verbringen.", "tokens": ["Ver\u00b7gib'", "O", "Vat\u00b7ter", "/", "das", "/", "was", "sie", "an", "mir", "ver\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$(", "ART", "$(", "PWS", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "die That ist b\u00f6\u00df': iedoch mein mild-vergo\u00dfnes Blue", "tokens": ["die", "That", "ist", "b\u00f6\u00df'", ":", "ie\u00b7doch", "mein", "mild\u00b7ver\u00b7go\u00df\u00b7nes", "Blue"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ist f\u00fcr die b\u00f6sen / ja f\u00fcr die Vergiesser / gut.", "tokens": ["ist", "f\u00fcr", "die", "b\u00f6\u00b7sen", "/", "ja", "f\u00fcr", "die", "Ver\u00b7gies\u00b7ser", "/", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "$(", "ADV", "APPR", "ART", "NN", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich la\u00df' es auch f\u00fcr die / so mich verwunden springen;", "tokens": ["Ich", "la\u00df'", "es", "auch", "f\u00fcr", "die", "/", "so", "mich", "ver\u00b7wun\u00b7den", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct",