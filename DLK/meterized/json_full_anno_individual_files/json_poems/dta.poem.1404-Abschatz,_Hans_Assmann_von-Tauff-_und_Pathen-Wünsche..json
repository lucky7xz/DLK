{"dta.poem.1404": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist das zehnde Pfand von GOttes mildem Segen:", "tokens": ["Du", "bist", "das", "zehn\u00b7de", "Pfand", "von", "Got\u00b7tes", "mil\u00b7dem", "Se\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist dir mein treuer Wunsch was kr\u00e4fftig beyzulegen/", "tokens": ["Ist", "dir", "mein", "treu\u00b7er", "Wunsch", "was", "kr\u00e4ff\u00b7tig", "bey\u00b7zu\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "PWS", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So zehlet sich dein Gl\u00fcck und Heyl mit zehn mahl zehn/", "tokens": ["So", "zeh\u00b7let", "sich", "dein", "Gl\u00fcck", "und", "Heyl", "mit", "zehn", "mahl", "zehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "KON", "NN", "APPR", "CARD", "ADV", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wird dein Stamm an dir viel tausend Freude sehn.", "tokens": ["Und", "wird", "dein", "Stamm", "an", "dir", "viel", "tau\u00b7send", "Freu\u00b7de", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}