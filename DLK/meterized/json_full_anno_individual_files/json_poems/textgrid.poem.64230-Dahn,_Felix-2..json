{"textgrid.poem.64230": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn's beschlossen ist da droben, da\u00df unser Reich versink' in Nacht, \u2013", "tokens": ["Und", "wenn's", "be\u00b7schlos\u00b7sen", "ist", "da", "dro\u00b7ben", ",", "da\u00df", "un\u00b7ser", "Reich", "ver\u00b7sink'", "in", "Nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVPP", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Noch einmal soll die Welt erproben des deutschen Schwertes alte Macht:", "tokens": ["Noch", "ein\u00b7mal", "soll", "die", "Welt", "er\u00b7pro\u00b7ben", "des", "deut\u00b7schen", "Schwer\u00b7tes", "al\u00b7te", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Soll nicht mehr deutsches Wort erschallen, nicht deutsche Sitte mehr bestehn,", "tokens": ["Soll", "nicht", "mehr", "deut\u00b7sches", "Wort", "er\u00b7schal\u00b7len", ",", "nicht", "deut\u00b7sche", "Sit\u00b7te", "mehr", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$,", "PTKNEG", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "So la\u00dft uns stolz und herrlich fallen, nicht tatenlos in Schmach vergehn.", "tokens": ["So", "la\u00dft", "uns", "stolz", "und", "herr\u00b7lich", "fal\u00b7len", ",", "nicht", "ta\u00b7ten\u00b7los", "in", "Schmach", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,", "PTKNEG", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Zieht einst ein Tag die Schuld der Ahnen, die eigne Schuld vors Weltgericht:", "tokens": ["Zieht", "einst", "ein", "Tag", "die", "Schuld", "der", "Ah\u00b7nen", ",", "die", "eig\u00b7ne", "Schuld", "vors", "Welt\u00b7ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ihr seid die Schergen, ihr Romanen und Slawen, doch die Richter nicht!", "tokens": ["Ihr", "seid", "die", "Scher\u00b7gen", ",", "ihr", "Ro\u00b7ma\u00b7nen", "und", "Sla\u00b7wen", ",", "doch", "die", "Rich\u00b7ter", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Wir beugen uns den Schicksalsm\u00e4chten: sie strafen furchtbar und gerecht:", "tokens": ["Wir", "beu\u00b7gen", "uns", "den", "Schick\u00b7sals\u00b7m\u00e4ch\u00b7ten", ":", "sie", "stra\u00b7fen", "furcht\u00b7bar", "und", "ge\u00b7recht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Ihr aber seid, mit uns zu rechten, kein ebenb\u00fcrtiges Geschlecht!", "tokens": ["Ihr", "a\u00b7ber", "seid", ",", "mit", "uns", "zu", "rech\u00b7ten", ",", "kein", "e\u00b7ben\u00b7b\u00fcr\u00b7ti\u00b7ges", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Den Schlag der deutschen B\u00e4renpfote ihr kennt ihn, ihr Romanen, wohl,", "tokens": ["Den", "Schlag", "der", "deut\u00b7schen", "B\u00e4\u00b7ren\u00b7pfo\u00b7te", "ihr", "kennt", "ihn", ",", "ihr", "Ro\u00b7ma\u00b7nen", ",", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Seit Alarich, der junge Gote, das Tor zerschlug am Kapitol,", "tokens": ["Seit", "A\u00b7la\u00b7rich", ",", "der", "jun\u00b7ge", "Go\u00b7te", ",", "das", "Tor", "zer\u00b7schlug", "am", "Ka\u00b7pi\u00b7tol", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und euch, ihr Slawen und Polacken, ist deutsche Kraft bekannt seit lang,", "tokens": ["Und", "euch", ",", "ihr", "Sla\u00b7wen", "und", "Po\u00b7la\u00b7cken", ",", "ist", "deut\u00b7sche", "Kraft", "be\u00b7kannt", "seit", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "VAFIN", "ADJA", "NN", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Seit dr\u00f6hnend trat auf eure Nacken der Heineriche Siegergang.", "tokens": ["Seit", "dr\u00f6h\u00b7nend", "trat", "auf", "eu\u00b7re", "Na\u00b7cken", "der", "Hei\u00b7ne\u00b7ri\u00b7che", "Sie\u00b7ger\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Nein, eh' ihr herrscht in diesen Landen, draus oft euch wilde Flucht entrollt,", "tokens": ["Nein", ",", "eh'", "ihr", "herrscht", "in", "die\u00b7sen", "Lan\u00b7den", ",", "draus", "oft", "euch", "wil\u00b7de", "Flucht", "ent\u00b7rollt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,", "PAV", "ADV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Sei noch einmal ein Kampf bestanden, des ewig ihr gedenken sollt:", "tokens": ["Sei", "noch", "ein\u00b7mal", "ein", "Kampf", "be\u00b7stan\u00b7den", ",", "des", "e\u00b7wig", "ihr", "ge\u00b7den\u00b7ken", "sollt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,", "ART", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-++--+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und wimmeln zahllos eure Horden, erf\u00fcllt von tausendj\u00e4hrgem Neid: \u2013", "tokens": ["Und", "wim\u00b7meln", "zahl\u00b7los", "eu\u00b7re", "Hor\u00b7den", ",", "er\u00b7f\u00fcllt", "von", "tau\u00b7send\u00b7j\u00e4hr\u00b7gem", "Neid", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Erst gilt es noch ein furchtbar Morden, eh' ihr die Herrn der Erde seid.", "tokens": ["Erst", "gilt", "es", "noch", "ein", "furcht\u00b7bar", "Mor\u00b7den", ",", "eh'", "ihr", "die", "Herrn", "der", "Er\u00b7de", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Schon einmal ward so stolz gerungen von deutschen Helden, k\u00fchn im Tod:", "tokens": ["Schon", "ein\u00b7mal", "ward", "so", "stolz", "ge\u00b7run\u00b7gen", "von", "deut\u00b7schen", "Hel\u00b7den", ",", "k\u00fchn", "im", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ein zweiter Kampf der Nibelungen sei unsern Feinden angedroht:", "tokens": ["Ein", "zwei\u00b7ter", "Kampf", "der", "Ni\u00b7be\u00b7lun\u00b7gen", "sei", "un\u00b7sern", "Fein\u00b7den", "an\u00b7ge\u00b7droht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Prophetisch war die alte Sage und grauenhaft wird sie erf\u00fcllt,", "tokens": ["Pro\u00b7phe\u00b7tisch", "war", "die", "al\u00b7te", "Sa\u00b7ge", "und", "grau\u00b7en\u00b7haft", "wird", "sie", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+--+--+--+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Wenn an dem letzten deutschen Tage der Schlachtruf dreier V\u00f6lker br\u00fcllt.", "tokens": ["Wenn", "an", "dem", "letz\u00b7ten", "deut\u00b7schen", "Ta\u00b7ge", "der", "Schlach\u00b7truf", "drei\u00b7er", "V\u00f6l\u00b7ker", "br\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADJA", "NN", "ART", "NN", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Von Blute sch\u00e4umend ziehn mit St\u00f6hnen emp\u00f6rt die Donau und der Rhein:", "tokens": ["Von", "Blu\u00b7te", "sch\u00e4u\u00b7mend", "ziehn", "mit", "St\u00f6h\u00b7nen", "em\u00b7p\u00f6rt", "die", "Do\u00b7nau", "und", "der", "Rhein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "APPR", "NN", "VVFIN", "ART", "NE", "KON", "ART", "NE", "$."], "meter": "-+-+-+-+--+--+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Es wollen brausend ihren S\u00f6hnen die deutschen Str\u00f6me Helfer sein;", "tokens": ["Es", "wol\u00b7len", "brau\u00b7send", "ih\u00b7ren", "S\u00f6h\u00b7nen", "die", "deut\u00b7schen", "Str\u00f6\u00b7me", "Hel\u00b7fer", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPOSAT", "NN", "ART", "ADJA", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Auf! Schleudert Feuer in die Felder, von jedem Berg werft Glut ins Land,", "tokens": ["Auf", "!", "Schleu\u00b7dert", "Feu\u00b7er", "in", "die", "Fel\u00b7der", ",", "von", "je\u00b7dem", "Berg", "werft", "Glut", "ins", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "NN", "APPR", "ART", "NN", "$,", "APPR", "PIAT", "NN", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Entflammt die alten Eichenw\u00e4lder zum ungeheuren Leichenbrand.", "tokens": ["Ent\u00b7flammt", "die", "al\u00b7ten", "Ei\u00b7chen\u00b7w\u00e4l\u00b7der", "zum", "un\u00b7ge\u00b7heu\u00b7ren", "Lei\u00b7chen\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Dann siegt der Feind: \u2013 doch mit Entsetzen, und triumphieren soll er nicht!", "tokens": ["Dann", "siegt", "der", "Feind", ":", "\u2013", "doch", "mit", "Ent\u00b7set\u00b7zen", ",", "und", "tri\u00b7um\u00b7phie\u00b7ren", "soll", "er", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "APPR", "NN", "$,", "KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "K\u00e4mpft bis die letzte Fahn' in Fetzen, k\u00e4mpft bis die letzte Klinge bricht,", "tokens": ["K\u00e4mpft", "bis", "die", "letz\u00b7te", "Fahn'", "in", "Fet\u00b7zen", ",", "k\u00e4mpft", "bis", "die", "letz\u00b7te", "Klin\u00b7ge", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+----+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "K\u00e4mpft bis der letzte Streich geschlagen ins letzte deutsche Herzblut rot,", "tokens": ["K\u00e4mpft", "bis", "der", "letz\u00b7te", "Streich", "ge\u00b7schla\u00b7gen", "ins", "letz\u00b7te", "deut\u00b7sche", "Herz\u00b7blut", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "APPRART", "ADJA", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Und lachend, wie der grimme Hagen, springt in die Schwerter und den Tod.", "tokens": ["Und", "la\u00b7chend", ",", "wie", "der", "grim\u00b7me", "Ha\u00b7gen", ",", "springt", "in", "die", "Schwer\u00b7ter", "und", "den", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Wir stiegen auf in Kampfgewittern, der Heldentod ist unser Recht:", "tokens": ["Wir", "stie\u00b7gen", "auf", "in", "Kampf\u00b7ge\u00b7wit\u00b7tern", ",", "der", "Hel\u00b7den\u00b7tod", "ist", "un\u00b7ser", "Recht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Die Erde soll im Kern erzittern, wann f\u00e4llt ihr tapferstes Geschlecht:", "tokens": ["Die", "Er\u00b7de", "soll", "im", "Kern", "er\u00b7zit\u00b7tern", ",", "wann", "f\u00e4llt", "ihr", "tap\u00b7fers\u00b7tes", "Ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Brach Etzels Haus in Glut zusammen, als er die Nibelungen zwang,", "tokens": ["Brach", "Et\u00b7zels", "Haus", "in", "Glut", "zu\u00b7sam\u00b7men", ",", "als", "er", "die", "Ni\u00b7be\u00b7lun\u00b7gen", "zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPR", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "So soll Europa stehn in Flammen bei der Germanen Untergang!", "tokens": ["So", "soll", "Eu\u00b7ro\u00b7pa", "stehn", "in", "Flam\u00b7men", "bei", "der", "Ger\u00b7ma\u00b7nen", "Un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Und wenn's beschlossen ist da droben, da\u00df unser Reich versink' in Nacht, \u2013", "tokens": ["Und", "wenn's", "be\u00b7schlos\u00b7sen", "ist", "da", "dro\u00b7ben", ",", "da\u00df", "un\u00b7ser", "Reich", "ver\u00b7sink'", "in", "Nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVPP", "VAFIN", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Noch einmal soll die Welt erproben des deutschen Schwertes alte Macht:", "tokens": ["Noch", "ein\u00b7mal", "soll", "die", "Welt", "er\u00b7pro\u00b7ben", "des", "deut\u00b7schen", "Schwer\u00b7tes", "al\u00b7te", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Soll nicht mehr deutsches Wort erschallen, nicht deutsche Sitte mehr bestehn,", "tokens": ["Soll", "nicht", "mehr", "deut\u00b7sches", "Wort", "er\u00b7schal\u00b7len", ",", "nicht", "deut\u00b7sche", "Sit\u00b7te", "mehr", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "ADJA", "NN", "VVINF", "$,", "PTKNEG", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "So la\u00dft uns stolz und herrlich fallen, nicht tatenlos in Schmach vergehn.", "tokens": ["So", "la\u00dft", "uns", "stolz", "und", "herr\u00b7lich", "fal\u00b7len", ",", "nicht", "ta\u00b7ten\u00b7los", "in", "Schmach", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,", "PTKNEG", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Zieht einst ein Tag die Schuld der Ahnen, die eigne Schuld vors Weltgericht:", "tokens": ["Zieht", "einst", "ein", "Tag", "die", "Schuld", "der", "Ah\u00b7nen", ",", "die", "eig\u00b7ne", "Schuld", "vors", "Welt\u00b7ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ihr seid die Schergen, ihr Romanen und Slawen, doch die Richter nicht!", "tokens": ["Ihr", "seid", "die", "Scher\u00b7gen", ",", "ihr", "Ro\u00b7ma\u00b7nen", "und", "Sla\u00b7wen", ",", "doch", "die", "Rich\u00b7ter", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Wir beugen uns den Schicksalsm\u00e4chten: sie strafen furchtbar und gerecht:", "tokens": ["Wir", "beu\u00b7gen", "uns", "den", "Schick\u00b7sals\u00b7m\u00e4ch\u00b7ten", ":", "sie", "stra\u00b7fen", "furcht\u00b7bar", "und", "ge\u00b7recht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Ihr aber seid, mit uns zu rechten, kein ebenb\u00fcrtiges Geschlecht!", "tokens": ["Ihr", "a\u00b7ber", "seid", ",", "mit", "uns", "zu", "rech\u00b7ten", ",", "kein", "e\u00b7ben\u00b7b\u00fcr\u00b7ti\u00b7ges", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Den Schlag der deutschen B\u00e4renpfote ihr kennt ihn, ihr Romanen, wohl,", "tokens": ["Den", "Schlag", "der", "deut\u00b7schen", "B\u00e4\u00b7ren\u00b7pfo\u00b7te", "ihr", "kennt", "ihn", ",", "ihr", "Ro\u00b7ma\u00b7nen", ",", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Seit Alarich, der junge Gote, das Tor zerschlug am Kapitol,", "tokens": ["Seit", "A\u00b7la\u00b7rich", ",", "der", "jun\u00b7ge", "Go\u00b7te", ",", "das", "Tor", "zer\u00b7schlug", "am", "Ka\u00b7pi\u00b7tol", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und euch, ihr Slawen und Polacken, ist deutsche Kraft bekannt seit lang,", "tokens": ["Und", "euch", ",", "ihr", "Sla\u00b7wen", "und", "Po\u00b7la\u00b7cken", ",", "ist", "deut\u00b7sche", "Kraft", "be\u00b7kannt", "seit", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "VAFIN", "ADJA", "NN", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Seit dr\u00f6hnend trat auf eure Nacken der Heineriche Siegergang.", "tokens": ["Seit", "dr\u00f6h\u00b7nend", "trat", "auf", "eu\u00b7re", "Na\u00b7cken", "der", "Hei\u00b7ne\u00b7ri\u00b7che", "Sie\u00b7ger\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "Nein, eh' ihr herrscht in diesen Landen, draus oft euch wilde Flucht entrollt,", "tokens": ["Nein", ",", "eh'", "ihr", "herrscht", "in", "die\u00b7sen", "Lan\u00b7den", ",", "draus", "oft", "euch", "wil\u00b7de", "Flucht", "ent\u00b7rollt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,", "PAV", "ADV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Sei noch einmal ein Kampf bestanden, des ewig ihr gedenken sollt:", "tokens": ["Sei", "noch", "ein\u00b7mal", "ein", "Kampf", "be\u00b7stan\u00b7den", ",", "des", "e\u00b7wig", "ihr", "ge\u00b7den\u00b7ken", "sollt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,", "ART", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-++--+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und wimmeln zahllos eure Horden, erf\u00fcllt von tausendj\u00e4hrgem Neid: \u2013", "tokens": ["Und", "wim\u00b7meln", "zahl\u00b7los", "eu\u00b7re", "Hor\u00b7den", ",", "er\u00b7f\u00fcllt", "von", "tau\u00b7send\u00b7j\u00e4hr\u00b7gem", "Neid", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Erst gilt es noch ein furchtbar Morden, eh' ihr die Herrn der Erde seid.", "tokens": ["Erst", "gilt", "es", "noch", "ein", "furcht\u00b7bar", "Mor\u00b7den", ",", "eh'", "ihr", "die", "Herrn", "der", "Er\u00b7de", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "Schon einmal ward so stolz gerungen von deutschen Helden, k\u00fchn im Tod:", "tokens": ["Schon", "ein\u00b7mal", "ward", "so", "stolz", "ge\u00b7run\u00b7gen", "von", "deut\u00b7schen", "Hel\u00b7den", ",", "k\u00fchn", "im", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ein zweiter Kampf der Nibelungen sei unsern Feinden angedroht:", "t