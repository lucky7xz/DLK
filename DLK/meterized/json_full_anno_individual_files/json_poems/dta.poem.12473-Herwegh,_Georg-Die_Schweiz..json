{"dta.poem.12473": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Schweiz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Land der Sehnsucht, drin die Berge", "tokens": ["Land", "der", "Sehn\u00b7sucht", ",", "drin", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie der Freiheit Prachtstat\u00fcen,", "tokens": ["wie", "der", "Frei\u00b7heit", "Pracht\u00b7sta\u00b7t\u00fcen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie aus blankem Gold und Silber", "tokens": ["Wie", "aus", "blan\u00b7kem", "Gold", "und", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "von dem Herrn gegossen, gl\u00fchen;", "tokens": ["von", "dem", "Herrn", "ge\u00b7gos\u00b7sen", ",", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Berge, die er seinem Himmel", "tokens": ["Ber\u00b7ge", ",", "die", "er", "sei\u00b7nem", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "als die letzten S\u00e4ulen gab,", "tokens": ["als", "die", "letz\u00b7ten", "S\u00e4u\u00b7len", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wiege seiner Wetterwolken,", "tokens": ["Wie\u00b7ge", "sei\u00b7ner", "Wet\u00b7ter\u00b7wol\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "seiner Adler einsam Grab!", "tokens": ["sei\u00b7ner", "Ad\u00b7ler", "ein\u00b7sam", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Land der Sehnsucht, drin die Str\u00f6me", "tokens": ["Land", "der", "Sehn\u00b7sucht", ",", "drin", "die", "Str\u00f6\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sich wie muthige Rebellen", "tokens": ["sich", "wie", "mut\u00b7hi\u00b7ge", "Re\u00b7bel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "In die Ebne niederst\u00fcrzen,", "tokens": ["In", "die", "Eb\u00b7ne", "nie\u00b7ders\u00b7t\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auch der Rhein mit seinen Wellen,", "tokens": ["auch", "der", "Rhein", "mit", "sei\u00b7nen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch der Rhein mit seinen Wellen,", "tokens": ["Auch", "der", "Rhein", "mit", "sei\u00b7nen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "der die vielen Worte h\u00f6rt \u2014", "tokens": ["der", "die", "vie\u00b7len", "Wor\u00b7te", "h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob's die deutschen F\u00fcrsten ahnen,", "tokens": ["Ob's", "die", "deut\u00b7schen", "F\u00fcrs\u00b7ten", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "da\u00df sich auch der Rhein emp\u00f6rt?", "tokens": ["da\u00df", "sich", "auch", "der", "Rhein", "em\u00b7p\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df er hier sich nicht um Klippen,", "tokens": ["Da\u00df", "er", "hier", "sich", "nicht", "um", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nicht um deutsche Lieder k\u00fcmmert,", "tokens": ["nicht", "um", "deut\u00b7sche", "Lie\u00b7der", "k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den eignen Friedensbogen", "tokens": ["Und", "den", "eig\u00b7nen", "Frie\u00b7dens\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "tausendfach im Sturz zertr\u00fcmmert?", "tokens": ["tau\u00b7send\u00b7fach", "im", "Sturz", "zer\u00b7tr\u00fcm\u00b7mert", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ihr auch so voll des Lobes,", "tokens": ["Ob", "ihr", "auch", "so", "voll", "des", "Lo\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "deutsche S\u00e4nger, hier erschient,", "tokens": ["deut\u00b7sche", "S\u00e4n\u00b7ger", ",", "hier", "er\u00b7schient", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo er donnernd schon als S\u00e4ugling", "tokens": ["Wo", "er", "don\u00b7nernd", "schon", "als", "S\u00e4ug\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "seine Sporen sich verdient?", "tokens": ["sei\u00b7ne", "Spo\u00b7ren", "sich", "ver\u00b7dient", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo die ersten Sch\u00f6pfungsworte", "tokens": ["Wo", "die", "ers\u00b7ten", "Sch\u00f6p\u00b7fungs\u00b7wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "laut noch durch die L\u00fcfte klingen:", "tokens": ["laut", "noch", "durch", "die", "L\u00fcf\u00b7te", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Land der Dichter! das emporsteigt,", "tokens": ["Land", "der", "Dich\u00b7ter", "!", "das", "em\u00b7por\u00b7steigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "adlergleich, auf Felsenschwingen;", "tokens": ["ad\u00b7lerg\u00b7leich", ",", "auf", "Fel\u00b7sen\u00b7schwin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Erde hei\u00dfverlangend", "tokens": ["Wo", "die", "Er\u00b7de", "hei\u00df\u00b7ver\u00b7lan\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nach dem Kranz der Sterne fa\u00dft,", "tokens": ["nach", "dem", "Kranz", "der", "Ster\u00b7ne", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis sie vor der eignen Gr\u00f6\u00dfe", "tokens": ["Bis", "sie", "vor", "der", "eig\u00b7nen", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "tief erschaudert und erbla\u00dft:", "tokens": ["tief", "er\u00b7schau\u00b7dert", "und", "er\u00b7bla\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wieder bin ich dein geworden!", "tokens": ["Wie\u00b7der", "bin", "ich", "dein", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wieder gl\u00e4nzt ihr, stolze Firnen,", "tokens": ["wie\u00b7der", "gl\u00e4nzt", "ihr", ",", "stol\u00b7ze", "Fir\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeden Abend, jeden Morgen", "tokens": ["Je\u00b7den", "A\u00b7bend", ",", "je\u00b7den", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "frische Rosen um die Stirnen;", "tokens": ["fri\u00b7sche", "Ro\u00b7sen", "um", "die", "Stir\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Land der Sehnsucht, ob auch eitel", "tokens": ["Land", "der", "Sehn\u00b7sucht", ",", "ob", "auch", "ei\u00b7tel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "manch ein Sklave mit dir prahlt,", "tokens": ["manch", "ein", "Skla\u00b7ve", "mit", "dir", "prahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bleibst du doch der treuste Spiegel,", "tokens": ["Bleibst", "du", "doch", "der", "treus\u00b7te", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "der die Freiheit widerstrahlt!", "tokens": ["der", "die", "Frei\u00b7heit", "wi\u00b7der\u00b7strahlt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Einstens, h\u00f6rt' ich, ging ein Engel", "tokens": ["Eins\u00b7tens", ",", "h\u00f6rt'", "ich", ",", "ging", "ein", "En\u00b7gel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "durch der Herren L\u00e4nder fragen,", "tokens": ["durch", "der", "Her\u00b7ren", "L\u00e4n\u00b7der", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ihr Boden nicht den Samen", "tokens": ["Ob", "ihr", "Bo\u00b7den", "nicht", "den", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auch der Freiheit m\u00f6chte tragen?", "tokens": ["auch", "der", "Frei\u00b7heit", "m\u00f6ch\u00b7te", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er bat um wenig Erde", "tokens": ["Und", "er", "bat", "um", "we\u00b7nig", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und er bat um wenig Raum,", "tokens": ["und", "er", "bat", "um", "we\u00b7nig", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenig Raum und wenig Erde", "tokens": ["We\u00b7nig", "Raum", "und", "we\u00b7nig", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "braucht ein solcher Freiheitsbaum.", "tokens": ["braucht", "ein", "sol\u00b7cher", "Frei\u00b7heits\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch sie riefen ihre Schergen", "tokens": ["Doch", "sie", "rie\u00b7fen", "ih\u00b7re", "Scher\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in die Th\u00e4ler, auf die H\u00fcgel,", "tokens": ["in", "die", "Th\u00e4\u00b7ler", ",", "auf", "die", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Engel nahm den Samen", "tokens": ["Und", "der", "En\u00b7gel", "nahm", "den", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wieder unter seine Fl\u00fcgel,", "tokens": ["wie\u00b7der", "un\u00b7ter", "sei\u00b7ne", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trug ihn aus dem finstern Lande", "tokens": ["Trug", "ihn", "aus", "dem", "fins\u00b7tern", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "in der Berge Purpurschein,", "tokens": ["in", "der", "Ber\u00b7ge", "Pur\u00b7pur\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Senkt' ihn statt in lockrer Erde,", "tokens": ["Senkt'", "ihn", "statt", "in", "lock\u00b7rer", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "in den Schoo\u00df der Felsen ein.", "tokens": ["in", "den", "Schoo\u00df", "der", "Fel\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Also mu\u00dft' er seine Wurzeln", "tokens": ["Al\u00b7so", "mu\u00dft'", "er", "sei\u00b7ne", "Wur\u00b7zeln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie die junge Tanne treiben:", "tokens": ["wie", "die", "jun\u00b7ge", "Tan\u00b7ne", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"t