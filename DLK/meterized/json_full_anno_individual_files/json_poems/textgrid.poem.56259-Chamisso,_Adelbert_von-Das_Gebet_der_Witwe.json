{"textgrid.poem.56259": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gebet der Witwe", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Alte wacht und betet allein", "tokens": ["Die", "Al\u00b7te", "wacht", "und", "be\u00b7tet", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In sp\u00e4ter Nacht bei der Lampe Schein:", "tokens": ["In", "sp\u00e4\u00b7ter", "Nacht", "bei", "der", "Lam\u00b7pe", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbla\u00df unsern gn\u00e4digen Herrn, o Herr!", "tokens": ["\u00bb", "la\u00df", "un\u00b7sern", "gn\u00e4\u00b7di\u00b7gen", "Herrn", ",", "o", "Herr", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Recht lange leben, ich bitte dich sehr.", "tokens": ["Recht", "lan\u00b7ge", "le\u00b7ben", ",", "ich", "bit\u00b7te", "dich", "sehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Not lehrt beten.\u00ab", "tokens": ["Die", "Not", "lehrt", "be\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-