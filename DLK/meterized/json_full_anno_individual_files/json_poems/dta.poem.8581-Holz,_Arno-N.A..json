{"dta.poem.8581": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das alte Nest! Die alten D\u00e4cher!", "tokens": ["Das", "al\u00b7te", "Nest", "!", "Die", "al\u00b7ten", "D\u00e4\u00b7cher", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus dunklen Linden dort", "tokens": ["Aus", "dunk\u00b7len", "Lin\u00b7den", "dort"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NE", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der Turm!", "tokens": ["der", "Turm", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Wie klangen Sonntags seine Glocken,", "tokens": ["Wie", "klan\u00b7gen", "Sonn\u00b7tags", "sei\u00b7ne", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "draussen, fern, wo der Kukuk rief . . .", "tokens": ["draus\u00b7sen", ",", "fern", ",", "wo", "der", "Ku\u00b7kuk", "rief", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Da wars so still.", "tokens": ["Da", "wars", "so", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wir pfl\u00fcckten Blumen,", "tokens": ["Wir", "pfl\u00fcck\u00b7ten", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "sangen", "tokens": ["san\u00b7gen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "und horchten, wie's im Bach kluckerte.", "tokens": ["und", "horch\u00b7ten", ",", "wie's", "im", "Bach", "kluc\u00b7ker\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zwanzig Jahre dr\u00fcberhin!", "tokens": ["Zwan\u00b7zig", "Jah\u00b7re", "dr\u00fc\u00b7berh\u00b7in", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Noch einmal jung sein! Mit neuen Augen in die Welt sehn!", "tokens": ["Noch", "ein\u00b7mal", "jung", "sein", "!", "Mit", "neu\u00b7en", "Au\u00b7gen", "in", "die", "Welt", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$.", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ach, wer das k\u00f6nnte!", "tokens": ["Ach", ",", "wer", "das", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PDS", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}