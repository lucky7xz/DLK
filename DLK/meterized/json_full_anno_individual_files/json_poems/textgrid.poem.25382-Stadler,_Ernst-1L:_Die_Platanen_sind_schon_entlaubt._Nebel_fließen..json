{"textgrid.poem.25382": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Platanen sind schon entlaubt. Nebel flie\u00dfen.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Platanen sind schon entlaubt. Nebel flie\u00dfen.", "tokens": ["Die", "Pla\u00b7ta\u00b7nen", "sind", "schon", "ent\u00b7laubt", ".", "Ne\u00b7bel", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$.", "NN", "VVINF", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wenn die Sonne einmal durch den Panzer", "tokens": ["Wenn", "die", "Son\u00b7ne", "ein\u00b7mal", "durch", "den", "Pan\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "grauer Wolken sticht,", "tokens": ["grau\u00b7er", "Wol\u00b7ken", "sticht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Spiegeln ihr die tausend Pf\u00fctzen", "tokens": ["Spie\u00b7geln", "ihr", "die", "tau\u00b7send", "Pf\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ein gebleichtes runzliges Gesicht.", "tokens": ["ein", "ge\u00b7bleich\u00b7tes", "runz\u00b7li\u00b7ges", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Alle Ger\u00e4usche sind sch\u00e4rfer. Den ganzen Tag \u00fcber", "tokens": ["Al\u00b7le", "Ge\u00b7r\u00e4u\u00b7sche", "sind", "sch\u00e4r\u00b7fer", ".", "Den", "gan\u00b7zen", "Tag", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN", "APPR"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.7": {"text": "h\u00f6rt man in den Fabriken die Maschinen gehn \u2013", "tokens": ["h\u00f6rt", "man", "in", "den", "Fab\u00b7ri\u00b7ken", "die", "Ma\u00b7schi\u00b7nen", "gehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "So t\u00f6nt durch die Ebenen der langen Stunden", "tokens": ["So", "t\u00f6nt", "durch", "die", "E\u00b7be\u00b7nen", "der", "lan\u00b7gen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+---+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "mein Herz und mag nicht stille stehn", "tokens": ["mein", "Herz", "und", "mag", "nicht", "stil\u00b7le", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "VMFIN", "PTKNEG", "VVFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und treibt die Gedanken", "tokens": ["Und", "treibt", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "wie surrende R\u00e4der hin und her,", "tokens": ["wie", "sur\u00b7ren\u00b7de", "R\u00e4\u00b7der", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und ist wie eine M\u00fchle mit windgedrehten Fl\u00fcgeln,", "tokens": ["Und", "ist", "wie", "ei\u00b7ne", "M\u00fch\u00b7le", "mit", "wind\u00b7ge\u00b7dreh\u00b7ten", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "aber ihre Kammern sind leer:", "tokens": ["a\u00b7ber", "ih\u00b7re", "Kam\u00b7mern", "sind", "leer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Sie redet irre Worte in den Abend", "tokens": ["Sie", "re\u00b7det", "ir\u00b7re", "Wor\u00b7te", "in", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "und schl\u00e4gt das Kreuz. Schon schlafen die Winde ein.", "tokens": ["und", "schl\u00e4gt", "das", "Kreuz", ".", "Schon", "schla\u00b7fen", "die", "Win\u00b7de", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Bald wird es schnei'n,", "tokens": ["Bald", "wird", "es", "schnei'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Dann f\u00e4llt wie Sternenregen wei\u00dfer Friede", "tokens": ["Dann", "f\u00e4llt", "wie", "Ster\u0