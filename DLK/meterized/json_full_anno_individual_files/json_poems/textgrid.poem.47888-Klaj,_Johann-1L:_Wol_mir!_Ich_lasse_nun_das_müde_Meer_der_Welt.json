{"textgrid.poem.47888": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wol mir! Ich lasse nun das m\u00fcde Meer der Welt/", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol mir! Ich lasse nun das m\u00fcde Meer der Welt/", "tokens": ["Wol", "mir", "!", "Ich", "las\u00b7se", "nun", "das", "m\u00fc\u00b7de", "Meer", "der", "Welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Den wilden Wellenweg/ das schwanke Segelzelt/", "tokens": ["Den", "wil\u00b7den", "Wel\u00b7len\u00b7weg", "/", "das", "schwan\u00b7ke", "Se\u00b7gel\u00b7zelt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Winde St\u00fcrmerstimm/ der Silberfluten Brausen/", "tokens": ["Der", "Win\u00b7de", "St\u00fcr\u00b7mers\u00b7timm", "/", "der", "Sil\u00b7ber\u00b7flu\u00b7ten", "Brau\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Ekkel-ungemach/ der Eitelkeiten Grausen.", "tokens": ["Das", "Ek\u00b7kel\u00b7un\u00b7ge\u00b7mach", "/", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Grau\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie lang? Ach Herr/ wie lang beklagt ich ohne Ma\u00df", "tokens": ["Wie", "lang", "?", "Ach", "Herr", "/", "wie", "lang", "be\u00b7klagt", "ich", "oh\u00b7ne", "Ma\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$.", "NN", "NN", "$(", "PWAV", "ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die pfellgeschwinde Wind/ als schl\u00e4ffrich/ tr\u00e4g und la\u00df?", "tokens": ["Die", "pfell\u00b7ge\u00b7schwin\u00b7de", "Wind", "/", "als", "schl\u00e4f\u00b7frich", "/", "tr\u00e4g", "und", "la\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "ADJD", "$(", "ADJD", "KON", "VVIMP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Threnen leschten nicht mein br\u00fcnstiges Verlangen:", "tokens": ["Die", "Thre\u00b7nen", "leschten", "nicht", "mein", "br\u00fcns\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Mein Seufftzen war zuvor befesselt und gefangen;", "tokens": ["Mein", "Seufft\u00b7zen", "war", "zu\u00b7vor", "be\u00b7fes\u00b7selt", "und", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So gar da\u00df ich gewillt/ aus dollem Frevelmuht/", "tokens": ["So", "gar", "da\u00df", "ich", "ge\u00b7willt", "/", "aus", "dol\u00b7lem", "Fre\u00b7vel\u00b7muht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "VVPP", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nach langverlangtem Port zu schwimmen durch die Flut.", "tokens": ["Nach", "lang\u00b7ver\u00b7lang\u00b7tem", "Port", "zu", "schwim\u00b7men", "durch", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wol mir/ ich bin am Strand! Sorg/ Jammer/ Angst und Leiden/", "tokens": ["Wol", "mir", "/", "ich", "bin", "am", "Strand", "!", "Sorg", "/", "Jam\u00b7mer", "/", "Angst", "und", "Lei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "PPER", "VAFIN", "APPRART", "NN", "$.", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Hat mit mir abgesteurt. Hier ist das Reich der Freuden.", "tokens": ["Hat", "mit", "mir", "ab\u00b7ge\u00b7steurt", ".", "Hier", "ist", "das", "Reich", "der", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$.", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hertzliebes Vaterland!", "tokens": ["Hertz\u00b7lie\u00b7bes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Sey tausendmal gegr\u00fcsst.", "tokens": ["Sey", "tau\u00b7send\u00b7mal", "ge\u00b7gr\u00fcs\u00b7st", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Du sichrer Vferstrand", "tokens": ["Du", "sich\u00b7rer", "Vfer\u00b7strand"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Sey tausendmal gek\u00fcsst.", "tokens": ["Sey", "tau\u00b7send\u00b7mal", "ge\u00b7k\u00fcsst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Gegr\u00fcsset solst du seyn/", "tokens": ["Ge\u00b7gr\u00fcs\u00b7set", "solst", "du", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Mit diesem Liebesku\u00df:", "tokens": ["Mit", "die\u00b7sem", "Lie\u00b7bes\u00b7ku\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Gek\u00fcsst dein Kies und Stein", "tokens": ["Ge\u00b7k\u00fcsst", "dein", "Kies", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Mit meiner Lippengru\u00df.", "tokens": ["Mit", "mei\u00b7ner", "Lip\u00b7pen\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Wol mir! und aber wol/ hier find ich aufgestellt/", "tokens": ["Wol", "mir", "!", "und", "a\u00b7ber", "wol", "/", "hier", "find", "ich", "auf\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "KON", "ADV", "ADV", "$(", "ADV", "VVFIN", "PPER", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Des starken AnkersCreutz/ das mir den Rukken h\u00e4lt:", "tokens": ["Des", "star\u00b7ken", "An\u00b7kers", "Creutz", "/", "das", "mir", "den", "Ruk\u00b7ken", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da mich der Hoffnungstrost mit wahrer Ruh begattet/", "tokens": ["Da", "mich", "der", "Hoff\u00b7nungs\u00b7trost", "mit", "wah\u00b7rer", "Ruh", "be\u00b7gat\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da mich der Lorberkrantz der Ewigkeit beschattet.", "tokens": ["Da", "mich", "der", "Lor\u00b7ber\u00b7krantz", "der", "E\u00b7wig\u00b7keit", "be\u00b7schat\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dir d\u00fcsterwilde Welt sag ich nun gute Nacht;", "tokens": ["Dir", "d\u00fcs\u00b7ter\u00b7wil\u00b7de", "Welt", "sag", "ich", "nun", "gu\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mich hat der Todesschlaff an Heiligland gebracht.", "tokens": ["Mich", "hat", "der", "To\u00b7des\u00b7schlaff", "an", "Hei\u00b7lig\u00b7land", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ach schlaff/ ach sanffter Schlaff; mein Wunsch und mein Verlangen!", "tokens": ["Ach", "schlaff", "/", "ach", "sanff\u00b7ter", "Schlaff", ";", "mein", "Wunsch", "und", "mein", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "XY", "ADJA", "NN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo find ich Hertzenswort dich lieben zu \u00fcmfangen?", "tokens": ["Wo", "find", "ich", "Hert\u00b7zens\u00b7wort", "dich", "lie\u00b7ben", "zu", "\u00fcm\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "PPER", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein stetes Wollustbett ist ei\u00dfl- und eisenkalt/", "tokens": ["Mein", "ste\u00b7tes", "Wol\u00b7lust\u00b7bett", "ist", "ei\u00df\u00b7l", "und", "ei\u00b7sen\u00b7kalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "TRUNC", "KON", "ADJD", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "In welchem mich erkennt die sch\u00f6ne Todsgestalt/", "tokens": ["In", "wel\u00b7chem", "mich", "er\u00b7kennt", "die", "sch\u00f6\u00b7ne", "Tods\u00b7ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was nie kein Aug geschaut/ was niemand kan verj\u00e4hen/", "tokens": ["Was", "nie", "kein", "Aug", "ge\u00b7schaut", "/", "was", "nie\u00b7mand", "kan", "ver\u00b7j\u00e4\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "VVPP", "$(", "PWS", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hat mein entzukkter Geist im Hoffnungstraum gesehen.", "tokens": ["Hat", "mein", "ent\u00b7zukk\u00b7ter", "Geist", "im", "Hoff\u00b7nungs\u00b7traum", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol mir! Ich lasse nun das m\u00fcde Meer der Welt/", "tokens": ["Wol", "mir", "!", "Ich", "las\u00b7se", "nun", "das", "m\u00fc\u00b7de", "Meer", "der", "Welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Den wilden Wellenweg/ das schwanke Segelzelt/", "tokens": ["Den", "wil\u00b7den", "Wel\u00b7len\u00b7weg", "/", "das", "schwan\u00b7ke", "Se\u00b7gel\u00b7zelt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Winde St\u00fcrmerstimm/ der Silberfluten Brausen/", "tokens": ["Der", "Win\u00b7de", "St\u00fcr\u00b7mers\u00b7timm", "/", "der", "Sil\u00b7ber\u00b7flu\u00b7ten", "Brau\u00b7sen", "/