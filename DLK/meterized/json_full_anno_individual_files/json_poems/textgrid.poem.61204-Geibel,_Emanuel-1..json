{"textgrid.poem.61204": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutschland, die Wittib, sa\u00df im Trauerkleide,", "tokens": ["Deutschland", ",", "die", "Wit\u00b7tib", ",", "sa\u00df", "im", "Trau\u00b7er\u00b7klei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und ihre Stimme war von St\u00f6hnen heiser,", "tokens": ["Und", "ih\u00b7re", "Stim\u00b7me", "war", "von", "St\u00f6h\u00b7nen", "hei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da man sie schied von ihrem Herrn und Kaiser,", "tokens": ["Da", "man", "sie", "schied", "von", "ih\u00b7rem", "Herrn", "und", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem sie verschworen war mit teurem Eide.", "tokens": ["Dem", "sie", "ver\u00b7schwo\u00b7ren", "war", "mit", "teu\u00b7rem", "Ei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ist ein Tr\u00f6ster kommen ihrem Leide:", "tokens": ["Doch", "ist", "ein", "Tr\u00f6s\u00b7ter", "kom\u00b7men", "ih\u00b7rem", "Lei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Geist der Eintracht, welcher nun mit leiser", "tokens": ["Der", "Geist", "der", "Ein\u00b7tracht", ",", "wel\u00b7cher", "nun", "mit", "lei\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gewalt um ihre Stirn die Eichenreiser", "tokens": ["Ge\u00b7walt", "um", "ih\u00b7re", "Stirn", "die", "Ei\u00b7chen\u00b7rei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zusammenh\u00e4lt, da\u00df keins vom Kranze scheide.", "tokens": ["Zu\u00b7sam\u00b7men\u00b7h\u00e4lt", ",", "da\u00df", "keins", "vom", "Kran\u00b7ze", "schei\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PIAT", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Kaisererbe, Geist voll Kraft und Milde,", "tokens": ["O", "Kai\u00b7ser\u00b7er\u00b7be", ",", "Geist", "voll", "Kraft", "und", "Mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Stunde schlug, der Welt an allen Enden", "tokens": ["Die", "Stun\u00b7de", "schlug", ",", "der", "Welt", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART"