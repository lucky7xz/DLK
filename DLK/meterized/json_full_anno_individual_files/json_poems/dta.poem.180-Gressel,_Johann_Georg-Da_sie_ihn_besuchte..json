{"dta.poem.180": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Da sie ihn besuchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mich hub des Gl\u00fcckes-Gunst zu den gestirnten H\u00f6hen/", "tokens": ["Mich", "hub", "des", "Gl\u00fc\u00b7ckes\u00b7Gunst", "zu", "den", "ge\u00b7stirn\u00b7ten", "H\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ich die ", "tokens": ["Als", "ich", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Es lachte mich ihr Mund mit holden Blicken an/", "tokens": ["Es", "lach\u00b7te", "mich", "ihr", "Mund", "mit", "hol\u00b7den", "Bli\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Augen waren mir mit Anmuth zugethan", "tokens": ["Die", "Au\u00b7gen", "wa\u00b7ren", "mir", "mit", "An\u00b7muth", "zu\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein Blitz mag nicht so bald durch schwartze Wolcken spielen/", "tokens": ["Kein", "Blitz", "mag", "nicht", "so", "bald", "durch", "schwart\u00b7ze", "Wol\u00b7cken", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PTKNEG", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als sie der Augen-Strahl lie\u00df auf mein Hertze zielen/", "tokens": ["Als", "sie", "der", "Au\u00b7gen\u00b7Strahl", "lie\u00df", "auf", "mein", "Hert\u00b7ze", "zie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der den Crystallen-Schein des Mondes \u00fcbersteigt/", "tokens": ["Der", "den", "Cry\u00b7stal\u00b7len\u00b7Schein", "des", "Mon\u00b7des", "\u00fc\u00b7bers\u00b7teigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Wenn er sich bey der Nacht in vollem Wachsthum zeigt.", "tokens": ["Wenn", "er", "sich", "bey", "der", "Nacht", "in", "vol\u00b7lem", "Wach\u00b7sthum", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wie kein Demant kan der Sonnen-Glantz verdringen/", "tokens": ["So", "wie", "kein", "De\u00b7mant", "kan", "der", "Son\u00b7nen\u00b7Glantz", "ver\u00b7drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Noch ein begraut ", "tokens": ["Noch", "ein", "be\u00b7graut"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "So geht der Wangen-Pracht des Himmels Golde f\u00fcr/", "tokens": ["So", "geht", "der", "Wan\u00b7gen\u00b7Pracht", "des", "Him\u00b7mels", "Gol\u00b7de", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn sich ", "tokens": ["Wenn", "sich"], "token_info": ["word", "word"], "pos": ["KOUS", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Die Lilje kr\u00f6hnt das Haupt/ die Rose deckt die Wangen/", "tokens": ["Die", "Lil\u00b7je", "kr\u00b7\u00f6hnt", "das", "Haupt", "/", "die", "Ro\u00b7se", "deckt", "die", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Die Lippen mit Rubin wie Morgen-Rosen prangen/", "tokens": ["Die", "Lip\u00b7pen", "mit", "Ru\u00b7bin", "wie", "Mor\u00b7gen\u00b7Ro\u00b7sen", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Vor ihrer Augen-Glantz der Sternen Printz entweicht/", "tokens": ["Vor", "ih\u00b7rer", "Au\u00b7gen\u00b7Glantz", "der", "Ster\u00b7nen", "Printz", "ent\u00b7weicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und vor der Br\u00fcste-Schnee des Himmels-Schnee erbleicht.", "tokens": ["Und", "vor", "der", "Br\u00fcs\u00b7te\u00b7Schnee", "des", "Him\u00b7mels\u00b7Schnee", "er\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vor Hitze h\u00e4tte ich wie Wachs zerschmeltzen m\u00fcssen/", "tokens": ["Vor", "Hit\u00b7ze", "h\u00e4t\u00b7te", "ich", "wie", "Wachs", "zer\u00b7schmelt\u00b7zen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "KOKOM", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als ich dis sch\u00f6ne Kind so nahe muste wissen/", "tokens": ["Als", "ich", "dis", "sch\u00f6\u00b7ne", "Kind", "so", "na\u00b7he", "mus\u00b7te", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADJA", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn dessen Freundlichkeit mir nicht den Lippen Safft", "tokens": ["Wenn", "des\u00b7sen", "Freund\u00b7lich\u00b7keit", "mir", "nicht", "den", "Lip\u00b7pen", "Safft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "NN", "PPER", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In diesem Liebes-Brand zur K\u00fchlung angeschafft.", "tokens": ["In", "die\u00b7sem", "Lie\u00b7bes\u00b7Brand", "zur", "K\u00fch\u00b7lung", "an\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich k\u00fchlte meine Hand bey ihren sch\u00f6nen Br\u00fcsten/", "tokens": ["Ich", "k\u00fchl\u00b7te", "mei\u00b7ne", "Hand", "bey", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Br\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die wegen ihrer Pracht den Engeln selbst gel\u00fcsten/", "tokens": ["Die", "we\u00b7gen", "ih\u00b7rer", "Pracht", "den", "En\u00b7geln", "selbst", "ge\u00b7l\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie war recht wunder-sch\u00f6n/ und freundlich mit dabey/", "tokens": ["Sie", "war", "recht", "wun\u00b7der\u00b7sch\u00f6n", "/", "und", "freund\u00b7lich", "mit", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "KON", "ADJD", "APPR", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ach Himmel gib/ da\u00df sie so stets gesinnet sey.", "tokens": ["Ach", "Him\u00b7mel", "gib", "/", "da\u00df", "sie", "so", "stets", "ge\u00b7sin\u00b7net", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVIMP", "$(", "KOUS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}