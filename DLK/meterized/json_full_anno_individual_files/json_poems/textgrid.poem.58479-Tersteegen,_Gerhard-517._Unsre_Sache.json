{"textgrid.poem.58479": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "517. Unsre Sache", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Liebe zu dem h\u00f6chsten Gut", "tokens": ["Aus", "Lie\u00b7be", "zu", "dem", "h\u00f6chs\u00b7ten", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir selbst, der Lust, dem Will'n absagen", "tokens": ["Dir", "selbst", ",", "der", "Lust", ",", "dem", "Will'n", "ab\u00b7sa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dann mit stillem, sanftem Mut", "tokens": ["Und", "dann", "mit", "stil\u00b7lem", ",", "sanf\u00b7tem", "Mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei dies dein Werk und s\u00fc\u00df Behagen,", "tokens": ["Sei", "dies", "dein", "Werk", "und", "s\u00fc\u00df", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu wandeln stets nach Kinderart", "tokens": ["Zu", "wan\u00b7deln", "stets", "nach", "Kin\u00b7der\u00b7art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Gottes Vatersgegenwart.", "tokens": ["In", "Got\u00b