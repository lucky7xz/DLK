["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der f\u00fcr uns starb am Kreuzesstamm,", "tokens": ["Der", "f\u00fcr", "uns", "starb", "am", "Kreu\u00b7zes\u00b7stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der nahm dem Tode seinen Schmerz.", "tokens": ["Der", "nahm", "dem", "To\u00b7de", "sei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du gingst, als gingest Du zum Tanz,", "tokens": ["Du", "gingst", ",", "als", "gin\u00b7gest", "Du", "zum", "Tanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit klarem, freudigem Gem\u00fcth", "tokens": ["Mit", "kla\u00b7rem", ",", "freu\u00b7di\u00b7gem", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wandest sch\u00f6n den Flechtenkranz", "tokens": ["Und", "wan\u00b7dest", "sch\u00f6n", "den", "Flech\u00b7ten\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sangst ein hoch begeistert Lied.", "tokens": ["Und", "sangst", "ein", "hoch", "be\u00b7geis\u00b7tert", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du dr\u00fcckst noch einmal an die Brust,", "tokens": ["Du", "dr\u00fcckst", "noch", "ein\u00b7mal", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Du gebarst, das zarte Kind,", "tokens": ["Das", "Du", "ge\u00b7barst", ",", "das", "zar\u00b7te", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hebst es empor mit Opferlust", "tokens": ["Hebst", "es", "em\u00b7por", "mit", "Op\u00b7fer\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und giebst es dann zur\u00fcck geschwind.", "tokens": ["Und", "giebst", "es", "dann", "zu\u00b7r\u00fcck", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da rei\u00dft von Deinem keuschen Leib", "tokens": ["Da", "rei\u00dft", "von", "Dei\u00b7nem", "keu\u00b7schen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kleider eine freche Hand:", "tokens": ["Die", "Klei\u00b7der", "ei\u00b7ne", "fre\u00b7che", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist nicht nackt, Du edles Weib,", "tokens": ["Du", "bist", "nicht", "nackt", ",", "Du", "ed\u00b7les", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist geh\u00fcllt in Lichtgewand.", "tokens": ["Du", "bist", "ge\u00b7h\u00fcllt", "in", "Licht\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Man hetzt auf Dich das wilde Thier;", "tokens": ["Man", "hetzt", "auf", "Dich", "das", "wil\u00b7de", "Thier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du siehst es an und wehrst Dich nicht", "tokens": ["Du", "siehst", "es", "an", "und", "wehrst", "Dich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprichst: \u00bbIhr Lieben, seht an mir,", "tokens": ["Und", "sprichst", ":", "\u00bb", "Ihr", "Lie\u00b7ben", ",", "seht", "an", "mir", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sanft ein Christenherz zerbricht.\u00ab", "tokens": ["Wie", "sanft", "ein", "Chris\u00b7ten\u00b7herz", "zer\u00b7bricht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie war Dein ganzer Leib so wund,", "tokens": ["Wie", "war", "Dein", "gan\u00b7zer", "Leib", "so", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie reichlich flo\u00df Dein reines Blut!", "tokens": ["Wie", "reich\u00b7lich", "flo\u00df", "Dein", "rei\u00b7nes", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ward es manchem Herzen kund,", "tokens": ["Da", "ward", "es", "man\u00b7chem", "Her\u00b7zen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Woher Dein freudenvoller Muth.", "tokens": ["Wo\u00b7her", "Dein", "freu\u00b7den\u00b7vol\u00b7ler", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das hingew\u00fcrgte Gotteslamm,", "tokens": ["Das", "hin\u00b7ge\u00b7w\u00fcrg\u00b7te", "Got\u00b7tes\u00b7lamm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schmiegte sanft sich an Dein Herz;", "tokens": ["Das", "schmieg\u00b7te", "sanft", "sich", "an", "Dein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der f\u00fcr uns starb am Kreuzesstamm,", "tokens": ["Der", "f\u00fcr", "uns", "starb", "am", "Kreu\u00b7zes\u00b7stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der nahm dem Tode seinen Schmerz.", "tokens": ["Der", "nahm", "dem", "To\u00b7de", "sei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Du auf dieser dunkeln Erd',", "tokens": ["Die", "Du", "auf", "die\u00b7ser", "dun\u00b7keln", "Erd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Qual und Kampf und Tod bedr\u00e4ngt,", "tokens": ["Von", "Qual", "und", "Kampf", "und", "Tod", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Glauben Viele hast bekehrt,", "tokens": ["Zum", "Glau\u00b7ben", "Vie\u00b7le", "hast", "be\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So Viele zu dem Heil gelenkt:", "tokens": ["So", "Vie\u00b7le", "zu", "dem", "Heil", "ge\u00b7lenkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O solltest Du im Himmel nun", "tokens": ["O", "soll\u00b7test", "Du", "im", "Him\u00b7mel", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu F\u00fc\u00dfen unsers Jesu Christ", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "un\u00b7sers", "Je\u00b7su", "Christ"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr uns nicht noch ein Gleiches thun?", "tokens": ["F\u00fcr", "uns", "nicht", "noch", "ein", "Glei\u00b7ches", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O, bitt' Ihn, da\u00df Er gn\u00e4dig ist.", "tokens": ["O", ",", "bitt'", "Ihn", ",", "da\u00df", "Er", "gn\u00e4\u00b7dig", "ist", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O, bitt' Ihn, da\u00df Er uns vereint", "tokens": ["O", ",", "bitt'", "Ihn", ",", "da\u00df", "Er", "uns", "ver\u00b7eint"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVPP"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In Seiner Kirche allzumal,", "tokens": ["In", "Sei\u00b7ner", "Kir\u00b7che", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+