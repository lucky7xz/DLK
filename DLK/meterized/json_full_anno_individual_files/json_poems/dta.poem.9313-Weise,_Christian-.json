{"dta.poem.9313": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lobe die tugend/ und hasse das gl\u00fccke:", "tokens": ["Ich", "lo\u00b7be", "die", "tu\u00b7gend", "/", "und", "has\u00b7se", "das", "gl\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dieweil die tugend blo\u00df", "tokens": ["Die\u00b7weil", "die", "tu\u00b7gend", "blo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In frommen hertzen wohnt/", "tokens": ["In", "from\u00b7men", "hert\u00b7zen", "wohnt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das gl\u00fcck hingegen auch", "tokens": ["Das", "gl\u00fcck", "hin\u00b7ge\u00b7gen", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die b\u00f6sen wohl belohnt.", "tokens": ["Die", "b\u00f6\u00b7sen", "wohl", "be\u00b7lohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die tugend stehet fest: das gl\u00fcck ist wie ein rauch", "tokens": ["Die", "tu\u00b7gend", "ste\u00b7het", "fest", ":", "das", "gl\u00fcck", "ist", "wie", "ein", "rauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der in der lufft vergeht;", "tokens": ["Der", "in", "der", "lufft", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Drum selig ist der mann/", "tokens": ["Drum", "se\u00b7lig", "ist", "der", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Welcher auff der tugend steht/", "tokens": ["Wel\u00b7cher", "auff", "der", "tu\u00b7gend", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und in der warheit sprechen kan:", "tokens": ["Und", "in", "der", "war\u00b7heit", "spre\u00b7chen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich lobe die tugend und hasse das gl\u00fccke.", "tokens": ["Ich", "lo\u00b7be", "die", "tu\u00b7gend", "und", "has\u00b7se", "das", "gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}