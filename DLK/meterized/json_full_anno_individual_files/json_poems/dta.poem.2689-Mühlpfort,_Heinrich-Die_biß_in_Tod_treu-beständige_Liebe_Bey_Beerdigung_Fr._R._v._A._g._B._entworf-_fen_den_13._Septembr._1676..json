{"dta.poem.2689": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die bi\u00df in Tod treu-best\u00e4ndige Liebe/  \n  Bey Beerdigung Fr. R. v. A. g. B. entworf-  \n fen den 13. Septembr. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Seneca das Licht und Wunder kluger Sinnen/", "tokens": ["Als", "Se\u00b7ne\u00b7ca", "das", "Licht", "und", "Wun\u00b7der", "klu\u00b7ger", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "KON", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wei\u00dfheit Inbegrieff/ der Wissenschafften Ziel/", "tokens": ["Der", "Wei\u00df\u00b7heit", "In\u00b7be\u00b7grieff", "/", "der", "Wis\u00b7sen\u00b7schaff\u00b7ten", "Ziel", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nachdem der Blut-Tyrann nicht \u00e4rger w\u00fcten k\u00f6nnen/", "tokens": ["Nach\u00b7dem", "der", "Blut\u00b7Ty\u00b7rann", "nicht", "\u00e4r\u00b7ger", "w\u00fc\u00b7ten", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch in sein\u2019 Ungenad und Halsgerichte fiel/", "tokens": ["Auch", "in", "sein'", "Un\u00b7ge\u00b7nad", "und", "Hals\u00b7ge\u00b7rich\u00b7te", "fiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und must auffs K\u00e4isers Wort die Adern ihm zerschneiden/", "tokens": ["Und", "must", "auffs", "K\u00e4i\u00b7sers", "Wort", "die", "A\u00b7dern", "ihm", "zer\u00b7schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Lie\u00df die Paulina da ein herrlich Beyspiel schaun.", "tokens": ["Lie\u00df", "die", "Pau\u00b7li\u00b7na", "da", "ein", "herr\u00b7lich", "Bey\u00b7spiel", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "ADV", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Ach! sprach sie/ mein Gemahl/ uns kan der Tod nicht scheiden/", "tokens": ["Ach", "!", "sprach", "sie", "/", "mein", "Ge\u00b7mahl", "/", "uns", "kan", "der", "Tod", "nicht", "schei\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mir sol noch f\u00fcr dem Stahl/ noch f\u00fcr dem sterben graun.", "tokens": ["Mir", "sol", "noch", "f\u00fcr", "dem", "Stahl", "/", "noch", "f\u00fcr", "dem", "ster\u00b7ben", "graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "$(", "ADV", "APPR", "ART", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verg\u00f6nne mir den Ruhm zugleich mit zuverblassen/", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", "den", "Ruhm", "zu\u00b7gleich", "mit", "zu\u00b7ver\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Getreue Liebe ist viel st\u00e4rcker als der Tod.", "tokens": ["Ge\u00b7treu\u00b7e", "Lie\u00b7be", "ist", "viel", "st\u00e4r\u00b7cker", "als", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich wil/ ein R\u00f6misch Weib die Nachwelt wissen lassen/", "tokens": ["Ich", "wil", "/", "ein", "R\u00f6\u00b7misch", "Weib", "die", "Nach\u00b7welt", "wis\u00b7sen", "las\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ART", "NN", "NN", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df meine treue Gluth besiegt die letzte Noth.", "tokens": ["Da\u00df", "mei\u00b7ne", "treu\u00b7e", "Gluth", "be\u00b7siegt", "die", "letz\u00b7te", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie schlo\u00df und hie\u00df das Blut aus beyden Armen springen/", "tokens": ["Sie", "schlo\u00df", "und", "hie\u00df", "das", "Blut", "aus", "bey\u00b7den", "Ar\u00b7men", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bi\u00df Nero sie durch Zwang zum Leben wieder rieff.", "tokens": ["Bi\u00df", "Ne\u00b7ro", "sie", "durch", "Zwang", "zum", "Le\u00b7ben", "wie\u00b7der", "rieff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "APPR", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allein ihr blass Gesicht entdeckt\u2019 in allen Dingen", "tokens": ["Al\u00b7lein", "ihr", "blass", "Ge\u00b7sicht", "ent\u00b7deckt'", "in", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "(da\u00df keine R\u00f6the mehr wie vormahls \u00fcberlieff/)", "tokens": ["(", "da\u00df", "kei\u00b7ne", "R\u00f6\u00b7the", "mehr", "wie", "vor\u00b7mahls", "\u00fc\u00b7berl\u00b7ieff", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PIAT", "NN", "ADV", "KOKOM", "ADV", "VVFIN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es steck\u2019 ihr Senecens Ged\u00e4chtn\u00fc\u00df noch im Hertzen/", "tokens": ["Es", "steck'", "ihr", "Se\u00b7ne\u00b7cens", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "noch", "im", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mit ihm sey auch zugleich die Lust des Lebens hin.", "tokens": ["Mit", "ihm", "sey", "auch", "zu\u00b7gleich", "die", "Lust", "des", "Le\u00b7bens", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Der l\u00e4ngere Verzug erwei\u00df\u2019 ihr nichts als Schmertzen/", "tokens": ["Der", "l\u00e4n\u00b7ge\u00b7re", "Ver\u00b7zug", "er\u00b7wei\u00df'", "ihr", "nichts", "als", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und ein geschwinder Tod w\u00e4r\u2019 eintzig ihr Gewinn.", "tokens": ["Und", "ein", "ge\u00b7schwin\u00b7der", "Tod", "w\u00e4r'", "eint\u00b7zig", "ihr", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gar in weit anderm Fall/ und seeligerm Vergn\u00fcgen/", "tokens": ["Gar", "in", "weit", "an\u00b7derm", "Fall", "/", "und", "see\u00b7li\u00b7germ", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch nicht in mindrer Treu/ und reiner Liebe Prei\u00df", "tokens": ["Doch", "nicht", "in", "mind\u00b7rer", "Treu", "/", "und", "rei\u00b7ner", "Lie\u00b7be", "Prei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wil Frau von Assigin sich itzt zur Ruh verf\u00fcgen/", "tokens": ["Wil", "Frau", "von", "As\u00b7si\u00b7gin", "sich", "itzt", "zur", "Ruh", "ver\u00b7f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "NN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und ihren Fu\u00df entziehn der Eitelkeiten Krei\u00df.", "tokens": ["Und", "ih\u00b7ren", "Fu\u00df", "ent\u00b7ziehn", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Krei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Als unser Senecn/ ihr Eh-Schatz/ ist entwichen/", "tokens": ["Als", "un\u00b7ser", "Sen\u00b7ecn", "/", "ihr", "Eh\u00b7Schatz", "/", "ist", "ent\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "VAFIN", "VVPP", "$("], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Und dieser Sterbligkeit gegeben gute Nacht/", "tokens": ["Und", "die\u00b7ser", "Ster\u00b7blig\u00b7keit", "ge\u00b7ge\u00b7ben", "gu\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVPP", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da ist ihr bestes Theil des Lebens mit verblichen/", "tokens": ["Da", "ist", "ihr", "bes\u00b7tes", "Theil", "des", "Le\u00b7bens", "mit", "ver\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sein Grab hat ihr die Bahn bald nach zugehn gemacht.", "tokens": ["Sein", "Grab", "hat", "ihr", "die", "Bahn", "bald", "nach", "zu\u00b7gehn", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Eh\u2019 noch zum f\u00fcnfften mahl das Silber ihrer Wangen/", "tokens": ["Eh'", "noch", "zum", "f\u00fcnff\u00b7ten", "mahl", "das", "Sil\u00b7ber", "ih\u00b7rer", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "ADJA", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der N\u00e4chte Trost und Zier die Cynthia entdeckt/", "tokens": ["Der", "N\u00e4ch\u00b7te", "Trost", "und", "Zier", "die", "Cyn\u00b7thia", "ent\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "So mu\u00df ein gleiches Grab di\u00df edle Paar umbfangen/", "tokens": ["So", "mu\u00df", "ein", "glei\u00b7ches", "Grab", "di\u00df", "ed\u00b7le", "Paar", "umb\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PDS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und beyder Leichen sind in einen Sand gestreckt.", "tokens": ["Und", "bey\u00b7der", "Lei\u00b7chen", "sind", "in", "ei\u00b7nen", "Sand", "ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So w\u00fcnschet ihm Pontan/ der Ruhm gelehrter Zeiten/", "tokens": ["So", "w\u00fcn\u00b7schet", "ihm", "Pon\u00b7tan", "/", "der", "Ruhm", "ge\u00b7lehr\u00b7ter", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "So bald er nur entseelt bey seinem Schatz zu seyn/", "tokens": ["So", "bald", "er", "nur", "ent\u00b7seelt", "bey", "sei\u00b7nem", "Schatz", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er wil sonst nirgend ruhn/ als nur an ihrer Seiten/", "tokens": ["Er", "wil", "sonst", "nir\u00b7gend", "ruhn", "/", "als", "nur", "an", "ih\u00b7rer", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$(", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Und baut bey Leben noch ihm Grufft und Grabe-Stein.", "tokens": ["Und", "baut", "bey", "Le\u00b7ben", "noch", "ihm", "Grufft", "und", "Gra\u00b7be\u00b7Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "In Meinung/ da\u00df sich auch die leichten Schatten k\u00fcssen/", "tokens": ["In", "Mei\u00b7nung", "/", "da\u00df", "sich", "auch", "die", "leich\u00b7ten", "Schat\u00b7ten", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KOUS", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und die Vereinigung der Asche wie verneut\u2019", "tokens": ["Und", "die", "Ver\u00b7ei\u00b7ni\u00b7gung", "der", "A\u00b7sche", "wie", "ver\u00b7neut'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "KOKOM", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df sich die Geister noch wie in die Armen schliessen/", "tokens": ["Da\u00df", "sich", "die", "Geis\u00b7ter", "noch", "wie", "in", "die", "Ar\u00b7men", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "KOKOM", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und solche Gegenwart sie beyderseits erfreut.", "tokens": ["Und", "sol\u00b7che", "Ge\u00b7gen\u00b7wart", "sie", "bey\u00b7der\u00b7seits", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Petrarcha wil auch so bey seiner Laura sterben/", "tokens": ["Pe\u00b7trar\u00b7cha", "wil", "auch", "so", "bey", "sei\u00b7ner", "Lau\u00b7ra", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Und Abelardens Leib Helissens Nachbar seyn.", "tokens": ["Und", "A\u00b7be\u00b7lar\u00b7dens", "Leib", "He\u00b7lis\u00b7sens", "Nach\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "NE", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Es kan die Panthea kein gr\u00f6sser Lob erwerben/", "tokens": ["Es", "kan", "die", "Pan\u00b7thea", "kein", "gr\u00f6s\u00b7ser", "Lob", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NE", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Als wenn ihr eigner Tod salbt Abradaten ein.", "tokens": ["Als", "wenn", "ihr", "eig\u00b7ner", "Tod", "salbt", "Ab\u00b7ra\u00b7da\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Philemon wei\u00df nichts mehr von G\u00f6ttern auszubitten/", "tokens": ["Phi\u00b7le\u00b7mon", "wei\u00df", "nichts", "mehr", "von", "G\u00f6t\u00b7tern", "aus\u00b7zu\u00b7bit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Nachdem der Jahre Schnee ihn und die Baucis dr\u00fcckt/", "tokens": ["Nach\u00b7dem", "der", "Jah\u00b7re", "Schnee", "ihn", "und", "die", "Bau\u00b7cis", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Als da\u00df ihr Faden nur zugleich sey abgeschnitten/", "tokens": ["Als", "da\u00df", "ihr", "Fa\u00b7den", "nur", "zu\u00b7gleich", "sey", "ab\u00b7ge\u00b7schnit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und eine Stunde sie au\u00df diesem Leben schickt.", "tokens": ["Und", "ei\u00b7ne", "Stun\u00b7de", "sie", "au\u00df", "die\u00b7sem", "Le\u00b7ben", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ja meint man/ da\u00df nicht auch bey seinem Schmertz und Jammer/", "tokens": ["Ja", "meint", "man", "/", "da\u00df", "nicht", "auch", "bey", "sei\u00b7nem", "Schmertz", "und", "Jam\u00b7mer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PIS", "$(", "KOUS", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Als Herr von Assig satt de\u00df eiteln Lebens war/", "tokens": ["Als", "Herr", "von", "As\u00b7sig", "satt", "de\u00df", "ei\u00b7teln", "Le\u00b7bens", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NE", "APPR", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Er seinem Schatz gerufft: Fleuch Freundin in die Cammer/", "tokens": ["Er", "sei\u00b7nem", "Schatz", "ge\u00b7rufft", ":", "Fleuch", "Freun\u00b7din", "in", "die", "Cam\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$.", "NN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Verbirg dich/ Freundin fleuch/ hier ist nichts als Gefahr.", "tokens": ["Ver\u00b7birg", "dich", "/", "Freun\u00b7din", "fleuch", "/", "hier", "ist", "nichts", "als", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "NN", "NN", "$(", "ADV", "VAFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Die beste Ruhestatt ist nur im Sarg zu finden/", "tokens": ["Die", "bes\u00b7te", "Ru\u00b7he\u00b7statt", "ist", "nur", "im", "Sarg", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ein Haus voll Sicherheit bereitet uns das Grab.", "tokens": ["Ein", "Haus", "voll", "Si\u00b7cher\u00b7heit", "be\u00b7rei\u00b7tet", "uns", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Beseufftze nicht so sehr mein seeliges Entbinden/", "tokens": ["Be\u00b7seufft\u00b7ze", "nicht", "so", "sehr", "mein", "see\u00b7li\u00b7ges", "Ent\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Ich w\u00fcnsche/ da\u00df ich dich bald zur Gefertin hab.", "tokens": ["Ich", "w\u00fcn\u00b7sche", "/", "da\u00df", "ich", "dich", "bald", "zur", "Ge\u00b7fer\u00b7tin", "hab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.57": {"text": "Und wohl! es ist geschehn. Die ewig-treuen Flammen/", "tokens": ["Und", "wohl", "!", "es", "ist", "ge\u00b7schehn", ".", "Die", "e\u00b7wig\u00b7treu\u00b7en", "Flam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "In welchen jederzeit der Seel\u2019gen Hertz gebrannt/", "tokens": ["In", "wel\u00b7chen", "je\u00b7der\u00b7zeit", "der", "Seel'\u00b7gen", "Hertz", "ge\u00b7brannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Verleschet nicht der Tod er bringt sie mehr zusammen/", "tokens": ["Ver\u00b7le\u00b7schet", "nicht", "der", "Tod", "er", "bringt", "sie", "mehr", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Da\u00df ihrer Liebe Licht werd\u2019 auch im ", "tokens": ["Da\u00df", "ih\u00b7rer", "Lie\u00b7be", "Licht", "werd'", "auch", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VAFIN", "ADV", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "Die unverruckte Treu/ das Tugend-volle Leben/", "tokens": ["Die", "un\u00b7ver\u00b7ruck\u00b7te", "Treu", "/", "das", "Tu\u00b7gen\u00b7dvol\u00b7le", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Der Sitten Freundligkeit entfernt von Glei\u00dfnerey/", "tokens": ["Der", "Sit\u00b7ten", "Freund\u00b7lig\u00b7keit", "ent\u00b7fernt", "von", "Glei\u00df\u00b7ne\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Die haben ihrer Eh\u2019 das Zeugn\u00fc\u00df l\u00e4ngst gegeben/", "tokens": ["Die", "ha\u00b7ben", "ih\u00b7rer", "Eh'", "das", "Zeug\u00b7n\u00fc\u00df", "l\u00e4ngst", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Wie sie ein Paradie\u00df vollkommner Liebe sey.", "tokens": ["Wie", "sie", "ein", "Pa\u00b7ra\u00b7die\u00df", "voll\u00b7komm\u00b7ner", "Lie\