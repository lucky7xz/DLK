{"dta.poem.1167": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "53. Die Sch\u00f6ne im Grabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Das Feuer w\u00e4rmt mich noch/ du Ausbund sch\u00f6ner Leichen/ ", "tokens": ["Das", "Feu\u00b7er", "w\u00e4rmt", "mich", "noch", "/", "du", "Aus\u00b7bund", "sch\u00f6\u00b7ner", "Lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So unter deiner Asch itzund begraben liegt:", "tokens": ["So", "un\u00b7ter", "dei\u00b7ner", "Asch", "it\u00b7zund", "be\u00b7gra\u00b7ben", "liegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Bildni\u00df deines Geists/ wohin dein Staub verfliegt/", "tokens": ["Das", "Bild\u00b7ni\u00df", "dei\u00b7nes", "Geists", "/", "wo\u00b7hin", "dein", "Staub", "ver\u00b7fliegt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "PWAV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan weder Zeit noch Ort aus meinem Hertzen streichen.", "tokens": ["Kan", "we\u00b7der", "Zeit", "noch", "Ort", "aus", "mei\u00b7nem", "Hert\u00b7zen", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "NN", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Flamme vorger Zeit mu\u00df bey der Glutt erbleichen/", "tokens": ["Die", "Flam\u00b7me", "vor\u00b7ger", "Zeit", "mu\u00df", "bey", "der", "Glutt", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die mir/ nachdem du kalt/ die hei\u00dfe Brust bekriegt.", "tokens": ["Die", "mir", "/", "nach\u00b7dem", "du", "kalt", "/", "die", "hei\u00b7\u00dfe", "Brust", "be\u00b7kriegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "KOUS", "PPER", "ADJD", "$(", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Hertze/ das so wohl mit deinem war vergn\u00fcgt/", "tokens": ["Mein", "Hert\u00b7ze", "/", "das", "so", "wohl", "mit", "dei\u00b7nem", "war", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$(", "PDS", "ADV", "ADV", "APPR", "PPOSAT", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "W\u00fcnscht desto mehr/ was ihm so zeitlich mu\u00df entweichen.", "tokens": ["W\u00fcnscht", "des\u00b7to", "mehr", "/", "was", "ihm", "so", "zeit\u00b7lich", "mu\u00df", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$(", "PWS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O wunder-grosse Macht der unbesiegten Liebe/", "tokens": ["O", "wun\u00b7der\u00b7gros\u00b7se", "Macht", "der", "un\u00b7be\u00b7sieg\u00b7ten", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die noch den todten Leib beseelt mit Anmutts-Triebe/", "tokens": ["Die", "noch", "den", "tod\u00b7ten", "Leib", "be\u00b7seelt", "mit", "An\u00b7mutts\u00b7Trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Aus starrer Glieder Ey\u00df die alte Brunst erzwingt.", "tokens": ["Aus", "star\u00b7rer", "Glie\u00b7der", "Ey\u00df", "die", "al\u00b7te", "Brunst", "er\u00b7zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie wird doch nicht ers\u00e4ufft von meinem strengen Weinen/", "tokens": ["Sie", "wird", "doch", "nicht", "er\u00b7s\u00e4ufft", "von", "mei\u00b7nem", "stren\u00b7gen", "Wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Weil meine Flammen zwar durch diesen Marmor scheinen/", "tokens": ["Weil", "mei\u00b7ne", "Flam\u00b7men", "zwar", "durch", "die\u00b7sen", "Mar\u00b7mor", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch keine Thr\u00e4nen-Bach den harten Stein durchdringt.", "tokens": ["Doch", "kei\u00b7ne", "Thr\u00e4\u00b7nen\u00b7Bach", "den", "har\u00b7ten", "Stein", "durch\u00b7dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}