{"textgrid.poem.52144": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Imme, die zu Rom im heilgen Korbe sitzt,", "tokens": ["Die", "Im\u00b7me", ",", "die", "zu", "Rom", "im", "heil\u00b7gen", "Kor\u00b7be", "sitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gt Honig ein und hat den Stachel auch gespitzt:", "tokens": ["Tr\u00e4gt", "Ho\u00b7nig", "ein", "und", "hat", "den", "Sta\u00b7chel", "auch", "ge\u00b7spitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie zeigt (so siehet man die D\u00f6rner bey den Rosen)", "tokens": ["Sie", "zeigt", "(", "so", "sie\u00b7het", "man", "die", "D\u00f6r\u00b7ner", "bey", "den", "Ro\u00b7sen", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "VVFIN", "PIS", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Stachel Spanien, das Honig den Frantzosen.", "tokens": ["Den", "Sta\u00b7chel", "Spa\u00b7ni\u00b7en", ",", "das", "Ho\u00b7nig", "den", "Frant\u00b7zo\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer meint, da\u00df solche Gall' in diesem C\u00f6rper sey,", "tokens": ["Wer", "meint", ",", "da\u00df", "sol\u00b7che", "Gall'", "in", "die\u00b7sem", "C\u00f6r\u00b7per", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PIAT", "NN", "APPR", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lilie kommt sonst ja nicht den Immen bey:", "tokens": ["Die", "Li\u00b7lie", "kommt", "sonst", "ja", "nicht", "den", "Im\u00b7men", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gib Honig, wem du wilt, doch wirst du zu sehr stechen,", "tokens": ["Gib", "Ho\u00b7nig", ",", "wem", "du", "wilt", ",", "doch", "wirst", "du", "zu", "sehr", "ste\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PWS", "PPER", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "PTKA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird mit dem Stachel dir das Honig gantz gebrechen.", "tokens": ["Wird", "mit", "dem", "Sta\u00b7chel", "dir", "das", "Ho\u00b7nig", "gantz", "ge\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Imme, die zu Rom im heilgen Korbe sitzt,", "tokens": ["Die", "Im\u00