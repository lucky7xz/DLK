{"textgrid.poem.24218": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der begossene Pudel", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n sind Sie, mein Fr\u00e4ulein, und ich k\u00f6nnte", "tokens": ["Sch\u00f6n", "sind", "Sie", ",", "mein", "Fr\u00e4u\u00b7lein", ",", "und", "ich", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "PPER", "VMFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stundenlang in Ihre Augen schauen,", "tokens": ["Stun\u00b7den\u00b7lang", "in", "Ih\u00b7re", "Au\u00b7gen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dr\u00fcber sich die sch\u00f6nsten Brauen bauen,", "tokens": ["Dr\u00fc\u00b7ber", "sich", "die", "sch\u00f6ns\u00b7ten", "Brau\u00b7en", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenns das b\u00f6se Schicksal mir verg\u00f6nnte.", "tokens": ["Wenns", "das", "b\u00f6\u00b7se", "Schick\u00b7sal", "mir", "ver\u00b7g\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber ach, aus Amors Gnaden bin ich", "tokens": ["A\u00b7ber", "ach", ",", "aus", "A\u00b7mors", "Gna\u00b7den", "bin", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "APPR", "NE", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4ngst gefallen; seine holden Gaben", "tokens": ["L\u00e4ngst", "ge\u00b7fal\u00b7len", ";", "sei\u00b7ne", "hol\u00b7den", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "G\u00f6nnt er jungen tanzbeflissenen Knaben,", "tokens": ["G\u00f6nnt", "er", "jun\u00b7gen", "tanz\u00b7be\u00b7flis\u00b7se\u00b7nen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und im Winkel Tr\u00fcbsalsverse spinn ich.", "tokens": ["Und", "im", "Win\u00b7kel", "Tr\u00fcb\u00b7sals\u00b7ver\u00b7se", "spinn", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+--++-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ihre sch\u00f6nen Augen wolln nicht sehen,", "tokens": ["Ih\u00b7re", "sch\u00f6\u00b7nen", "Au\u00b7gen", "wolln", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ich Armer mich um Sie verzehre, \u2013", "tokens": ["Wie", "ich", "Ar\u00b7mer", "mich", "um", "Sie", "ver\u00b7zeh\u00b7re", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ich 