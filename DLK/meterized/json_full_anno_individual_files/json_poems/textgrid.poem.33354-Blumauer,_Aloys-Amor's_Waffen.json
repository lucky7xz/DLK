{"textgrid.poem.33354": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Amor's Waffen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traut, M\u00e4dchen, Amor'n nicht, er zieht", "tokens": ["Traut", ",", "M\u00e4d\u00b7chen", ",", "A\u00b7mor'n", "nicht", ",", "er", "zieht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "NE", "PTKNEG", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar auf als wie ein Krieger;", "tokens": ["Zwar", "auf", "als", "wie", "ein", "Krie\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wenn man n\u00e4her ihn besieht,", "tokens": ["Doch", "wenn", "man", "n\u00e4\u00b7her", "ihn", "be\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er nur ein Betr\u00fcger.", "tokens": ["Ist", "er", "nur", "ein", "Be\u00b7tr\u00fc\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An seinen Waffen, die er f\u00fchrt,", "tokens": ["An", "sei\u00b7nen", "Waf\u00b7fen", ",", "die", "er", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat manche sich betrogen:", "tokens": ["Hat", "man\u00b7che", "sich", "be\u00b7tro\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu einer Angelruthe wird", "tokens": ["Zu", "ei\u00b7ner", "An\u00b7gel\u00b7ru\u00b7the", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur allzuleicht sein Bogen.", "tokens": ["Nur", "all\u00b7zu\u00b7leicht", "sein", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6cher, den ihr gern begafft,", "tokens": ["Der", "K\u00f6\u00b7cher", ",", "den", "ihr", "gern", "be\u00b7gafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nichts als eine Falle", "tokens": ["Ist", "nichts", "als", "ei\u00b7ne", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr's liebe M\u00e4uschen Jungfrauschaft,", "tokens": ["F\u00fcr's", "lie\u00b7be", "M\u00e4u\u00b7schen", "Jung\u00b7frausc\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darin f\u00e4ngt er euch alle.", "tokens": ["Da\u00b7rin", "f\u00e4ngt", "er", "euch", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "PIS", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"lin