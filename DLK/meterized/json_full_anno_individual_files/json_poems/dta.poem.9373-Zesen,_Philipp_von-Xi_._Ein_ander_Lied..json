{"dta.poem.9373": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Xi .  \n Ein ander Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So geh\u2019 ich also nun in Einsamkeit alleine/", "tokens": ["So", "geh'", "ich", "al\u00b7so", "nun", "in", "Ein\u00b7sam\u00b7keit", "al\u00b7lei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd niemand h\u00f6ret zu wie ich so sehnlich weine/", "tokens": ["Vnd", "nie\u00b7mand", "h\u00f6\u00b7ret", "zu", "wie", "ich", "so", "sehn\u00b7lich", "wei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein\u2019 jhr G\u00f6ttinnen/ die jhr noch seyd betr\u00fcbe", "tokens": ["Al\u00b7lein'", "jhr", "G\u00f6t\u00b7tin\u00b7nen", "/", "die", "jhr", "noch", "seyd", "be\u00b7tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$(", "PRELS", "PPER", "ADV", "VAFIN", "ADJA"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u00fcmb des Narcissus Fall/ in welchen jhr verliebt/", "tokens": ["\u00fcmb", "des", "Nar\u00b7cis\u00b7sus", "Fall", "/", "in", "wel\u00b7chen", "jhr", "ver\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$(", "APPR", "PWAT", "PPOSAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als er (", "tokens": ["Als", "er", "("], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPER", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Floh' in den wald/ auf da\u00df er nicht w\u00fcrd' au\u00dfgesp\u00e4het/", "tokens": ["Floh'", "in", "den", "wald", "/", "auf", "da\u00df", "er", "nicht", "w\u00fcrd'", "au\u00df\u00b7ge\u00b7sp\u00e4\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$(", "APPR", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da dann sein sch\u00f6ner Leib/ den Echo hat begehrr/", "tokens": ["Da", "dann", "sein", "sch\u00f6\u00b7ner", "Leib", "/", "den", "E\u00b7cho", "hat", "be\u00b7gehrr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In eine Wiesenblum' erb\u00e4rmlich ward verkehrt.", "tokens": ["In", "ei\u00b7ne", "Wie\u00b7sen\u00b7blum'", "er\u00b7b\u00e4rm\u00b7lich", "ward", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich seufftz'/ O Echo/ noch \u00fcmb deiner Liebe willen;", "tokens": ["Ich", "seufft\u00b7z'", "/", "O", "E\u00b7cho", "/", "noch", "\u00fcmb", "dei\u00b7ner", "Lie\u00b7be", "wil\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NE", "NN", "$(", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Drumb hilffstu mir jetzt auch mein Klagelied erf\u00fcllen/", "tokens": ["Drumb", "hilffs\u00b7tu", "mir", "jetzt", "auch", "mein", "Kla\u00b7ge\u00b7lied", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df ich mein' helle Stimm' erheben kan so sehr/", "tokens": ["Da\u00df", "ich", "mein'", "hel\u00b7le", "Stimm'", "er\u00b7he\u00b7ben", "kan", "so", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "VMFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df da\u00df der Himmel auch mein trauren selbey", "tokens": ["Bi\u00df", "da\u00df", "der", "Him\u00b7mel", "auch", "mein", "trau\u00b7ren", "sel\u00b7bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "ADV", "PPOSAT", "VVINF", "VAFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "h\u00f6r. etc.", "tokens": ["h\u00f6r", ".", "et\u00b7c."], "token_info": ["word", "punct", "abbreviation"], "pos": ["VVFIN", "$.", "ADV"], "meter": "+", "measure": "single.up"}}}}}