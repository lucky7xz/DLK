{"dta.poem.8720": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  Der Ring.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Jubelnd ist der Tag erschienen,", "tokens": ["Ju\u00b7belnd", "ist", "der", "Tag", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwingt den Goldpokal der Sonne,", "tokens": ["Schwingt", "den", "Gold\u00b7po\u00b7kal", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gie\u00dft auf Berg und Thal berauschend", "tokens": ["Gie\u00dft", "auf", "Berg", "und", "Thal", "be\u00b7rau\u00b7schend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder seine Strahlenwonne.", "tokens": ["Nie\u00b7der", "sei\u00b7ne", "Strah\u00b7len\u00b7won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den L\u00fcften aufzutauchen", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "auf\u00b7zu\u00b7tau\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darf kein W\u00f6lkchen sich getrauen,", "tokens": ["Darf", "kein", "W\u00f6lk\u00b7chen", "sich", "ge\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf das Gl\u00fcck der treuen Liebe", "tokens": ["Auf", "das", "Gl\u00fcck", "der", "treu\u00b7en", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will der ", "tokens": ["Will", "der"], "token_info": ["word", "word"], "pos": ["VMFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Nur die Lerchen, Freude singend,", "tokens": ["Nur", "die", "Ler\u00b7chen", ",", "Freu\u00b7de", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigen auf im Morgenglanze,", "tokens": ["Stei\u00b7gen", "auf", "im", "Mor\u00b7gen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trunken von den Strahleng\u00fcssen,", "tokens": ["Trun\u00b7ken", "von", "den", "Strah\u00b7len\u00b7g\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzt die Welle der Duranze. \u2014", "tokens": ["Jauchzt", "die", "Wel\u00b7le", "der", "Du\u00b7ran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In dem Garten, wo vor Jahren", "tokens": ["In", "dem", "Gar\u00b7ten", ",", "wo", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gingen in der Schattenk\u00fchle", "tokens": ["Gin\u00b7gen", "in", "der", "Schat\u00b7ten\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klara Hebert und Johannes", "tokens": ["Kla\u00b7ra", "He\u00b7bert", "und", "Jo\u00b7han\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit verschwiegenem Gef\u00fchle,", "tokens": ["Mit", "ver\u00b7schwie\u00b7ge\u00b7nem", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo die lauten Nachtigallen", "tokens": ["Wo", "die", "lau\u00b7ten", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00df verr\u00e4therische Lieder", "tokens": ["S\u00fc\u00df", "ver\u00b7r\u00e4\u00b7the\u00b7ri\u00b7sche", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sangen auf den gr\u00fcnen Zweigen: \u2014", "tokens": ["San\u00b7gen", "auf", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandeln sie auch heute wieder.", "tokens": ["Wan\u00b7deln", "sie", "auch", "heu\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und in seliger Verschlingung", "tokens": ["Und", "in", "se\u00b7li\u00b7ger", "Ver\u00b7schlin\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehren sie zum trauten Orte,", "tokens": ["Keh\u00b7ren", "sie", "zum", "trau\u00b7ten", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo vor Jahren ihre Liebe", "tokens": ["Wo", "vor", "Jah\u00b7ren", "ih\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fand die ersten, leisen Worte.", "tokens": ["Fand", "die", "ers\u00b7ten", ",", "lei\u00b7sen", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Klara bl\u00fcht in neuer Sch\u00f6ne,", "tokens": ["Kla\u00b7ra", "bl\u00fcht", "in", "neu\u00b7er", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosen, Fremdlinge seit lange,", "tokens": ["Ro\u00b7sen", ",", "Fremd\u00b7lin\u00b7ge", "seit", "lan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Kehren sch\u00fcchtern heute wieder", "tokens": ["Keh\u00b7ren", "sch\u00fcch\u00b7tern", "heu\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die freudenhelle Wange. \u2014", "tokens": ["Auf", "die", "freu\u00b7den\u00b7hel\u00b7le", "Wan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nach dem hohen Felsenhause,", "tokens": ["Nach", "dem", "ho\u00b7hen", "Fel\u00b7sen\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das nun wieder w\u00fcst und einsam,", "tokens": ["Das", "nun", "wie\u00b7der", "w\u00fcst", "und", "ein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandeln Klara, ihre Mutter,", "tokens": ["Wan\u00b7deln", "Kla\u00b7ra", ",", "ih\u00b7re", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Johannes froh gemeinsam.", "tokens": ["Und", "Jo\u00b7han\u00b7nes", "froh", "ge\u00b7mein\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Selbst die rauhen, \u00f6den Klippen", "tokens": ["Selbst", "die", "rau\u00b7hen", ",", "\u00f6\u00b7den", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt die Freude jezt umschlungen;", "tokens": ["H\u00e4lt", "die", "Freu\u00b7de", "jezt", "um\u00b7schlun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur wie leichte Nebel schleichen", "tokens": ["Nur", "wie", "leich\u00b7te", "Ne\u00b7bel", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch's Gestein Erinnerungen.", "tokens": ["Durch's", "Ge\u00b7stein", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Als sie treten in das d\u00fcstre,", "tokens": ["Als", "sie", "tre\u00b7ten", "in", "das", "d\u00fcst\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verh\u00e4ngni\u00dfvolle Zimmer,", "tokens": ["Und", "ver\u00b7h\u00e4ng\u00b7ni\u00df\u00b7vol\u00b7le", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sehen die erstaunten Frauen", "tokens": ["Se\u00b7hen", "die", "er\u00b7staun\u00b7ten", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Krucifix und Kerzenschimmer.", "tokens": ["Kru\u00b7ci\u00b7fix", "und", "Ker\u00b7zen\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und dem Priester, der sie gr\u00fc\u00dfet,", "tokens": ["Und", "dem", "Pries\u00b7ter", ",", "der", "sie", "gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harrt am Munde schon der Segen,", "tokens": ["Harrt", "am", "Mun\u00b7de", "schon", "der", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch der alte treue Marko", "tokens": ["Auch", "der", "al\u00b7te", "treu\u00b7e", "Mar\u00b7ko"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NE"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.4": {"text": "Eilt der Jungfrau froh entgegen. \u2014", "tokens": ["Eilt", "der", "Jung\u00b7frau", "froh", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Klara trug das goldne Ringlein,", "tokens": ["Kla\u00b7ra", "trug", "das", "gold\u00b7ne", "Rin\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der stillen Herzenswunde,", "tokens": ["Auf", "der", "stil\u00b7len", "Her\u00b7zens\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ihr scheidend einst gegeben", "tokens": ["Das", "ihr", "schei\u00b7dend", "einst", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADJD", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Johann in der bangen Stunde.", "tokens": ["Jo\u00b7hann", "in", "der", "ban\u00b7gen", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Den Smaragd am Ringe damals", "tokens": ["Den", "Sma\u00b7ragd", "am", "Rin\u00b7ge", "da\u00b7mals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sah das Volk gar hell ergl\u00e4nzen,", "tokens": ["Sah", "das", "Volk", "gar", "hell", "er\u00b7gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit profetischem Gemahnen", "tokens": ["Mit", "pro\u00b7fe\u00b7ti\u00b7schem", "Ge\u00b7mah\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An das Gr\u00fcn von Myrtenkr\u00e4nzen.", "tokens": ["An", "das", "Gr\u00fcn", "von", "Myr\u00b7ten\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}