{"dta.poem.3450": {"metadata": {"author": {"name": "N.A.", "birth": "N.A.", "death": "N.A."}, "title": "Das\u00a0:xxi:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1497", "urn": "urn:nbn:de:kobv:b4-30121-8", "language": ["de:0.99"], "booktitle": "Des Edlen Ritter Morgeners Walfart. Erfurt, 1497."}, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00df dich m\u00fclner vnd dein gesynd", "tokens": ["Gr\u00fc\u00df", "dich", "m\u00fcl\u00b7ner", "vnd", "dein", "ge\u00b7synd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "weist auf dem perg icht newe mer", "tokens": ["weist", "auf", "dem", "perg", "icht", "ne\u00b7we", "mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ob ich tugent an dir vind", "tokens": ["ob", "ich", "tu\u00b7gent", "an", "dir", "vind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPR", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "so sag mirs armen petteler", "tokens": ["so", "sag", "mirs", "ar\u00b7men", "pet\u00b7te\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "newer mer der wei\u00df ich vil", "tokens": ["ne\u00b7wer", "mer", "der", "wei\u00df", "ich", "vil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "das meins herren morgners fraw", "tokens": ["das", "meins", "her\u00b7ren", "morg\u00b7ners", "fraw"], "token_info": ["word", "word", "word", "word", "word"], "pos"