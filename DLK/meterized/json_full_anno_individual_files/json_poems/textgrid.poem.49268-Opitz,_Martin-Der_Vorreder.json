, "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was doch blincket f\u00fcr ein Liecht?", "tokens": ["Was", "doch", "blin\u00b7cket", "f\u00fcr", "ein", "Liecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist es mein Augustus nicht?", "tokens": ["Ist", "es", "mein", "Au\u00b7gus\u00b7tus", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich kenne dich, du Blume dieser Zeit,", "tokens": ["Ich", "ken\u00b7ne", "dich", ",", "du", "Blu\u00b7me", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Zier und Spiegel aller Jugendt.", "tokens": ["Du", "Zier", "und", "Spie\u00b7gel", "al\u00b7ler", "Ju\u00b7gendt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Rautenkrantz, die Freundlichkeit", "tokens": ["Der", "Rau\u00b7ten\u00b7krantz", ",", "die", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verrh\u00e4tet dich, o Glantz der Tugendt;", "tokens": ["Ver\u00b7rh\u00e4\u00b7tet", "dich", ",", "o", "Glantz", "der", "Tu\u00b7gendt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER"