{"textgrid.poem.49268": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Vorreder", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sterblichs Volck, der ich nit sterblich bin,", "tokens": ["Ihr", "sterb\u00b7lichs", "Volck", ",", "der", "ich", "nit", "sterb\u00b7lich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Komm jetzt zu euch von den Elyser-Feldern,", "tokens": ["Komm", "jetzt", "zu", "euch", "von", "den", "E\u00b7ly\u00b7ser\u00b7Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo unsre Geister ziehen hin", "tokens": ["Wo", "uns\u00b7re", "Geis\u00b7ter", "zie\u00b7hen", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und letzen sich in gr\u00fcnen W\u00e4ldern;", "tokens": ["Und", "let\u00b7zen", "sich", "in", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch de\u00df bleichen Charons Meer", "tokens": ["Durch", "de\u00df", "blei\u00b7chen", "Cha\u00b7rons", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Komm' ich, o ihr Menschen, her.", "tokens": ["Komm'", "ich", ",", "o", "ihr", "Men\u00b7schen", ",", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin der Mann, der ich so r\u00fchmlich sang", "tokens": ["Ich", "bin", "der", "Mann", ",", "der", "ich", "so", "r\u00fchm\u00b7lich", "sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In meine Harpff' und die beruff'nen Seiten.", "tokens": ["In", "mei\u00b7ne", "Ha\u00b7rpf\u00b7f'", "und", "die", "be\u00b7ruff'\u00b7nen", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie Amors Macht und harter Zwang", "tokens": ["Wie", "A\u00b7mors", "Macht", "und", "har\u00b7ter", "Zwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Himmlischen vor alten Zeiten", "tokens": ["Den", "Himm\u00b7li\u00b7schen", "vor", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat verwandelt die Gestalt", "tokens": ["Hat", "ver\u00b7wan\u00b7delt", "die", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In Gefl\u00fcgel, Wildt und Waldt.", "tokens": ["In", "Ge\u00b7fl\u00fc\u00b7gel", ",", "Wildt", "und", "Waldt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich habe mich die schwere Liebes-Kunst,", "tokens": ["Ich", "ha\u00b7be", "mich", "die", "schwe\u00b7re", "Lie\u00b7bes\u00b7Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O dich, mein Rom, zu lehren unternommen;", "tokens": ["O", "dich", ",", "mein", "Rom", ",", "zu", "leh\u00b7ren", "un\u00b7ter\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PPOSAT", "NE", "$,", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hab' auch gezeigt, wie solcher Brunst", "tokens": ["Hab'", "auch", "ge\u00b7zeigt", ",", "wie", "sol\u00b7cher", "Brunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Hertze wider ab soll kommen.", "tokens": ["Ein", "Hert\u00b7ze", "wi\u00b7der", "ab", "soll", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "PTKVZ", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df man recht liebt, k\u00f6mpt durch mich,", "tokens": ["Da\u00df", "man", "recht", "liebt", ",", "k\u00f6mpt", "durch", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df man nicht liebt, thu' auch ich.", "tokens": ["Da\u00df", "man", "nicht", "liebt", ",", "thu'", "auch", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVFIN", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Schau' aber zu, was f\u00fcr ein heller Schein", "tokens": ["Schau'", "a\u00b7ber", "zu", ",", "was", "f\u00fcr", "ein", "hel\u00b7ler", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umgibt mich doch, und wessen werd' ich innen?", "tokens": ["Um\u00b7gibt", "mich", "doch", ",", "und", "wes\u00b7sen", "werd'", "ich", "in\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VVINF", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Majest\u00e4t mu\u00df dieses seyn,", "tokens": ["Was", "Ma\u00b7jes\u00b7t\u00e4t", "mu\u00df", "die\u00b7ses", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "PDAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir bescheint Gesicht' und Sinnen?", "tokens": ["Die", "mir", "be\u00b7scheint", "Ge\u00b7sicht'", "und", "Sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was doch blincket f\u00fcr ein Liecht?", "tokens": ["Was", "doch", "blin\u00b7cket", "f\u00fcr", "ein", "Liecht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist es mein Augustus nicht?", "tokens": ["Ist", "es", "mein", "Au\u00b7gus\u00b7tus", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich kenne dich, du Blume dieser Zeit,", "tokens": ["Ich", "ken\u00b7ne", "dich", ",", "du", "Blu\u00b7me", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Zier und Spiegel aller Jugendt.", "tokens": ["Du", "Zier", "und", "Spie\u00b7gel", "al\u00b7ler", "Ju\u00b7gendt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Rautenkrantz, die Freundlichkeit", "tokens": ["Der", "Rau\u00b7ten\u00b7krantz", ",", "die", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verrh\u00e4tet dich, o Glantz der Tugendt;", "tokens": ["Ver\u00b7rh\u00e4\u00b7tet", "dich", ",", "o", "Glantz", "der", "Tu\u00b7gendt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Alle Menschen loben dich,", "tokens": ["Al\u00b7le", "Men\u00b7schen", "lo\u00b7ben", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Elbe neiget sich.", "tokens": ["Und", "die", "El\u00b7be", "nei\u00b7get", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du edle Braut, wol deiner Lieb' und dir;", "tokens": ["Du", "ed\u00b7le", "Braut", ",", "wol", "dei\u00b7ner", "Lieb'", "und", "dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich aber will jetzt wie vorweilen singen,", "tokens": ["Ich", "a\u00b7ber", "will", "jetzt", "wie", "vor\u00b7wei\u00b7len", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "KOKOM", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In was f\u00fcr Noth ein Cavallier", "tokens": ["In", "was", "f\u00fcr", "Noth", "ein", "Ca\u00b7val\u00b7lier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eine Dame sich kan bringen,", "tokens": ["Und", "ei\u00b7ne", "Da\u00b7me", "sich", "kan", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die nicht nach der Liebe fragt,", "tokens": ["Die", "nicht", "nach", "der", "Lie\u00b7be", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nur thut, was ihr behagt.", "tokens": ["Und", "nur", "thut", ",", "was", "ihr", "be\u00b7hagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr werdet sehn f\u00fcr schwerer Liebes-Pein", "tokens": ["Ihr", "wer\u00b7det", "sehn", "f\u00fcr", "schwe\u00b7rer", "Lie\u00b7bes\u00b7Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denselben Gott mit nassen S\u00e4ufftzen klagen,", "tokens": ["Den\u00b7sel\u00b7ben", "Gott", "mit", "nas\u00b7sen", "S\u00e4ufft\u00b7zen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der uns den sch\u00f6nen Tage-Schein", "tokens": ["Der", "uns", "den", "sch\u00f6\u00b7nen", "Ta\u00b7ge\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herumb f\u00fchrt auff dem g\u00fcldnen Wagen,", "tokens": ["He\u00b7rumb", "f\u00fchrt", "auff", "dem", "g\u00fcld\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der uns Allen gibt das Liecht,", "tokens": ["Der", "uns", "Al\u00b7len", "gibt", "das", "Liecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht f\u00fcr Liebe selber nicht.", "tokens": ["Sieht", "f\u00fcr", "Lie\u00b7be", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr sterblichs Volck, der ich nit sterblich bin,", "tokens": ["Ihr", "sterb\u00b7lichs", "Volck", ",", "der", "ich", "nit", "sterb\u00b7lich", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Komm jetzt zu euch von den Elyser-Feldern,", "tokens": ["Komm", "jetzt", "zu", "euch", "von", "den", "E\u00b7ly\u00b7ser\u00b7Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo unsre Geister ziehen hin", "tokens": ["Wo", "uns\u00b7re", "Geis\u00b7ter", "zie\u00b7hen", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und letzen sich in gr\u00fcnen W\u00e4ldern;", "tokens": ["Und", "let\u00b7zen", "sich", "in", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch de\u00df bleichen Charons Meer", "tokens": ["Durch", "de\u00df", "blei\u00b7chen", "Cha\u0