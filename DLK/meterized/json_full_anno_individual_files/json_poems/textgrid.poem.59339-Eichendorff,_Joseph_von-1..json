{"textgrid.poem.59339": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In s\u00fc\u00dfen Spielen unter nun gegangen", "tokens": ["In", "s\u00fc\u00b7\u00dfen", "Spie\u00b7len", "un\u00b7ter", "nun", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Liebchens Augen, und sie atmet linde,", "tokens": ["Sind", "Lieb\u00b7chens", "Au\u00b7gen", ",", "und", "sie", "at\u00b7met", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "$,", "KON", "PPER", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stillauschend sitz ich bei dem holden Kinde,", "tokens": ["Stil\u00b7lau\u00b7schend", "sitz", "ich", "bei", "dem", "hol\u00b7den", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Locken streichelnd ihr von Stirn und Wangen.", "tokens": ["Die", "Lo\u00b7cken", "strei\u00b7chelnd", "ihr", "von", "Stirn", "und", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach! Lust und Mond und Sterne sind vergangen,", "tokens": ["Ach", "!", "Lust", "und", "Mond", "und", "Ster\u00b7ne", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "KON", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Fenster mahnen schon die Morgenwinde:", "tokens": ["Am", "Fens\u00b7ter", "mah\u00b7nen", "schon", "die", "Mor\u00b7gen\u00b7win\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich vom Nacken leis die Arme winde,", "tokens": ["Da\u00df", "ich", "vom", "Na\u00b7cken", "leis", "die", "Ar\u00b7me", "win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die noch im Schlummer lieblich mich umfangen.", "tokens": ["Die", "noch", "im", "Schlum\u00b7mer", "lieb\u00b7lich", "mich", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O \u00f6ffne nicht der Augen s\u00fc\u00dfe Strahle!", "tokens": ["O", "\u00f6ff\u00b7ne", "nicht", "der", "Au\u00b7gen", "s\u00fc\u00b7\u00dfe", "Strah\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur einen Ku\u00df noch \u2013 und zum letzten Male", "tokens": ["Nur", "ei\u00b7nen", "Ku\u00df", "noch", "\u2013", "und", "zum", "letz\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "p