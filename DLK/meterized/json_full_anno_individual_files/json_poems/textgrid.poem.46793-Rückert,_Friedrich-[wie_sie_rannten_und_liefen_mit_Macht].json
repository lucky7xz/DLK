{"textgrid.poem.46793": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie sie rannten und liefen mit Macht]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sie rannten und liefen mit Macht, mit Macht,", "tokens": ["Wie", "sie", "rann\u00b7ten", "und", "lie\u00b7fen", "mit", "Macht", ",", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Das Herz mir im Leib hat gelacht, gelacht;", "tokens": ["Das", "Herz", "mir", "im", "Leib", "hat", "ge\u00b7lacht", ",", "ge\u00b7lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der erste voran, der zweite sodann,", "tokens": ["Der", "ers\u00b7te", "vo\u00b7ran", ",", "der", "zwei\u00b7te", "so\u00b7dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der dritte, der vierte, der f\u00fcnfte Mann,", "tokens": ["Der", "drit\u00b7te", ",", "der", "vier\u00b7te", ",", "der", "f\u00fcnf\u00b7te", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und endlich hinter dem Haufen", "tokens": ["Und", "end\u00b7lich", "hin\u00b7ter", "dem", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das sechste, das M\u00e4dchen, mit Schnaufen.", "tokens": ["Das", "sechs\u00b7te", ",", "das", "M\u00e4d\u00b7chen", ",", "mit", "Schnau\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Sie rannten nach einem Ziel im Lauf,", "tokens": ["Sie", "rann\u00b7ten", "nach", "ei\u00b7nem", "Ziel", "im", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und jedes, wo es fiel, stand auf.", "tokens": ["Und", "je\u00b7des", ",", "wo", "es", "fiel", ",", "stand", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer hat nun den Kranz erlaufen?", "tokens": ["Wer", "hat", "nun", "den", "Kranz", "er\u00b7lau\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Das ist wohl zu errathen leicht:", "tokens": ["Das", "ist", "wohl", "zu", "er\u00b7ra\u00b7then", "leicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Je \u00e4