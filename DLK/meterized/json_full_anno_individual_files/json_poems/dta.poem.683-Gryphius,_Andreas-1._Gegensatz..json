{"dta.poem.683": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Gjb dich zu ruh' vnd las dein sorgen fahren. ", "tokens": ["Gjb", "dich", "zu", "ruh'", "vnd", "las", "dein", "sor\u00b7gen", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKZU", "VVINF", "KON", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch so viel noth best\u00fcrmbter ", "tokens": ["Durch", "so", "viel", "noth", "be\u00b7st\u00fcrmb\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die wehmut/ die dich dr\u00fccket/", "tokens": ["Die", "weh\u00b7mut", "/", "die", "dich", "dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die geissel die dich schmeist vnd beist ", "tokens": ["Die", "geis\u00b7sel", "die", "dich", "schmeist", "vnd", "beist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat GOTT nun wegger\u00fccket:", "tokens": ["Hat", "GoTT", "nun", "weg\u00b7ge\u00b7r\u00fc\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gott der dich offt gerissen von der bahren ", "tokens": ["Gott", "der", "dich", "offt", "ge\u00b7ris\u00b7sen", "von", "der", "bah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "PPER", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Hat dich mit h\u00fclff erquicket. ", "tokens": ["Hat", "dich", "mit", "h\u00fclff", "er\u00b7quic\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd nach den tr\u00fcben/ rawen donner jahren", "tokens": ["Vnd", "nach", "den", "tr\u00fc\u00b7ben", "/", "ra\u00b7wen", "don\u00b7ner", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$(", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dir gnad' vnd trew/ vnd gunst erweist. ", "tokens": ["Dir", "gnad'", "vnd", "trew", "/", "vnd", "gunst", "er\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$(", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}