word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Bef\u00f6rdre meine M\u00fch/ la\u00df sie ihr Ziel erlangen/", "tokens": ["Be\u00b7f\u00f6rd\u00b7re", "mei\u00b7ne", "M\u00fch", "/", "la\u00df", "sie", "ihr", "Ziel", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und d\u00e4mpffe dessen List/ der m\u00e4chtig uns zu fangen/", "tokens": ["Und", "d\u00e4mpf\u00b7fe", "des\u00b7sen", "List", "/", "der", "m\u00e4ch\u00b7tig", "uns", "zu", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$(", "ART", "ADJD", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "So sollt du fort f\u00fcr fort/ O grosser GOtt allein", "tokens": ["So", "sollt", "du", "fort", "f\u00fcr", "fort", "/", "O", "gros\u00b7ser", "Gott", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "APPR", "PTKVZ", "$(", "NE", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Mein erst\u2019 und letztes Lied/ Anfang und Ende seyn.", "tokens": ["Mein", "er\u00b7st'", "und", "letz\u00b7tes", "Lied", "/", "An\u00b7fang", "und", "En\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "KON", "ADJA", "NN", "$(", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+-++--+-+",