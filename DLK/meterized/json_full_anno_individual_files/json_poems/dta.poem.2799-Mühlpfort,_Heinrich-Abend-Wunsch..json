{"dta.poem.2799": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sonnen-Licht ist nun in seine See gesenckt/", "tokens": ["Der", "Son\u00b7nen\u00b7Licht", "ist", "nun", "in", "sei\u00b7ne", "See", "ge\u00b7senckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmel hat den Schmuck der Sternen aus ge-", "tokens": ["Der", "Him\u00b7mel", "hat", "den", "Schmuck", "der", "Ster\u00b7nen", "aus", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4st sie uns zu Trost in grosser Anzahl brennen/", "tokens": ["Und", "l\u00e4st", "sie", "uns", "zu", "Trost", "in", "gros\u00b7ser", "An\u00b7zahl", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer wolte GOttes Macht und Wei\u00dfheit nicht erkennen?", "tokens": ["Wer", "wol\u00b7te", "Got\u00b7tes", "Macht", "und", "Wei\u00df\u00b7heit", "nicht", "er\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die unerforschlich ist/ und die kein Mensch nicht kan/", "tokens": ["Die", "un\u00b7er\u00b7for\u00b7schlich", "ist", "/", "und", "die", "kein", "Mensch", "nicht", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$(", "KON", "ART", "PIAT", "NN", "PTKNEG", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie klug er immer ist/ begreiffen inn\u2019 und an.", "tokens": ["Wie", "klug", "er", "im\u00b7mer", "ist", "/", "be\u00b7greif\u00b7fen", "inn'", "und", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$(", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dir sag\u2019 ich grosser GOtt/ von gantzem Hertzen Danck", "tokens": ["Dir", "sag'", "ich", "gros\u00b7ser", "Gott", "/", "von", "gant\u00b7zem", "Hert\u00b7zen", "Danck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und r\u00fchme deine G\u00fct\u2019 ohn\u2019 End\u2019 und Lebens-lang.", "tokens": ["Und", "r\u00fch\u00b7me", "dei\u00b7ne", "G\u00fct'", "ohn'", "End'", "und", "Le\u00b7bens\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dir singet itzt mein Mund/ und opffert meine Seele", "tokens": ["Dir", "sin\u00b7get", "itzt", "mein", "Mund", "/", "und", "opf\u00b7fert", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Weyhrauch wahrer Reu. Verschm\u00e4he nicht die H\u00f6le/", "tokens": ["Den", "Wey\u00b7hrauch", "wah\u00b7rer", "Reu", ".", "Ver\u00b7schm\u00e4\u00b7he", "nicht", "die", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NE", "$.", "NN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Aus der es zu dir steigt. Ich wei\u00df ja mehr als wohl/", "tokens": ["Aus", "der", "es", "zu", "dir", "steigt", ".", "Ich", "wei\u00df", "ja", "mehr", "als", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PIAT", "KOKOM", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df man ein reines Hertz dem HErren bringen soll.", "tokens": ["Da\u00df", "man", "ein", "rei\u00b7nes", "Hertz", "dem", "Her\u00b7ren", "brin\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier lig ich seufzende und beichte meine Schuld/", "tokens": ["Hier", "lig", "ich", "seuf\u00b7zen\u00b7de", "und", "beich\u00b7te", "mei\u00b7ne", "Schuld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "O HErr der Ewigkeit/ ach habe doch Geduld/", "tokens": ["O", "Herr", "der", "E\u00b7wig\u00b7keit", "/", "ach", "ha\u00b7be", "doch", "Ge\u00b7duld", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$(", "XY", "VAFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und geh nicht ins Gericht mit deinem armen Knechte.", "tokens": ["Und", "geh", "nicht", "ins", "Ge\u00b7richt", "mit", "dei\u00b7nem", "ar\u00b7men", "Knech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wer wolte doch bestehn f\u00fcr deinem strengen Rechte?", "tokens": ["Wer", "wol\u00b7te", "doch", "be\u00b7stehn", "f\u00fcr", "dei\u00b7nem", "stren\u00b7gen", "Rech\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein Mensch ist ohne Fleck und niemand ohne S\u00fcnd/", "tokens": ["Kein", "Mensch", "ist", "oh\u00b7ne", "Fleck", "und", "nie\u00b7mand", "oh\u00b7ne", "S\u00fcnd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN", "KON", "PIS", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So viel man auf der Welt der gross- und weiten findt.", "tokens": ["So", "viel", "man", "auf", "der", "Welt", "der", "gross", "und", "wei\u00b7ten", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPR", "ART", "NN", "ART", "TRUNC", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie oft ich dich zur Ruh und Zorn mein GOtt gebracht/", "tokens": ["Wie", "oft", "ich", "dich", "zur", "Ruh", "und", "Zorn", "mein", "Gott", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "APPRART", "NN", "KON", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das ist mir wol bewust: Kein Tag und Nacht", "tokens": ["Das", "ist", "mir", "wol", "be\u00b7wust", ":", "Kein", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVFIN", "$.", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Geht ohne Frevel weg. Ich habe dich verlassen!", "tokens": ["Geht", "oh\u00b7ne", "Fre\u00b7vel", "weg", ".", "Ich", "ha\u00b7be", "dich", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nun mu\u00df ich selber mich und meine Thorheit hassen.", "tokens": ["Nun", "mu\u00df", "ich", "sel\u00b7ber", "mich", "und", "mei\u00b7ne", "Thor\u00b7heit", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer hilfft mir auf der Welt? die vor die beste war/", "tokens": ["Wer", "hilfft", "mir", "auf", "der", "Welt", "?", "die", "vor", "die", "bes\u00b7te", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ART", "APPR", "ART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nu meine Seele schmacht in Zittern und Gefahr.", "tokens": ["Nu", "mei\u00b7ne", "See\u00b7le", "schmacht", "in", "Zit\u00b7tern", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mir hat vor Gottesfurcht der Jugend Lust beliebt:", "tokens": ["Mir", "hat", "vor", "Got\u00b7tes\u00b7furcht", "der", "Ju\u00b7gend", "Lust", "be\u00b7liebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nu aber seh\u2019 ich auch/ was es vor Nutzen giebt.", "tokens": ["Nu", "a\u00b7ber", "seh'", "ich", "auch", "/", "was", "es", "vor", "Nut\u00b7zen", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$(", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mich hat der B\u00f6sen Schaar mehr als dein Volck erquicket/", "tokens": ["Mich", "hat", "der", "B\u00f6\u00b7sen", "Schaar", "mehr", "als", "dein", "Volck", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "PIAT", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mein eigen Fleisch und Blut das hat mich so bestricket/", "tokens": ["Mein", "ei\u00b7gen", "Fleisch", "und", "Blut", "das", "hat", "mich", "so", "be\u00b7stri\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "PDS", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df ich die Uppigkeit und b\u00f6se L\u00fcste mehr", "tokens": ["Da\u00df", "ich", "die", "Up\u00b7pig\u00b7keit", "und", "b\u00f6\u00b7se", "L\u00fcs\u00b7te", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Gesch\u00e4tzet als dein Wort und deines Ruhmes Ehr.", "tokens": ["Ge\u00b7sch\u00e4t\u00b7zet", "als", "dein", "Wort", "und", "dei\u00b7nes", "Ruh\u00b7mes", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich war in Eitelkeit ersoffen gantz und gar/", "tokens": ["Ich", "war", "in", "Ei\u00b7tel\u00b7keit", "er\u00b7sof\u00b7fen", "gantz", "und", "gar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVFIN", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mich konte weiter nichts als Jrdisches ergetzen/", "tokens": ["Mich", "kon\u00b7te", "wei\u00b7ter", "nichts", "als", "Jr\u00b7di\u00b7sches", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "KOKOM", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich spottete dein Wort/ und half die jenen hetzen/", "tokens": ["Ich", "spot\u00b7te\u00b7te", "dein", "Wort", "/", "und", "half", "die", "je\u00b7nen", "het\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "ART", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So deiner Herrlichkeit erban\u2019ten ein Altar.", "tokens": ["So", "dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "e\u00b7rban'\u00b7ten", "ein", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja ich blieb so vertiest in meiner Eitelkeit", "tokens": ["Ja", "ich", "blieb", "so", "ver\u00b7tiest", "in", "mei\u00b7ner", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Und trotzte keck und k\u00fchn durch meine Lebens-Zeit.", "tokens": ["Und", "trotz\u00b7te", "keck", "und", "k\u00fchn", "durch", "mei\u00b7ne", "Le\u00b7bens\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nu klagt mich eigne Schuld und mein Gewissen an/", "tokens": ["Nu", "klagt", "mich", "eig\u00b7ne", "Schuld", "und", "mein", "Ge\u00b7wis\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie ich verderbter Mensch nichts Gutes je gethan.", "tokens": ["Wie", "ich", "ver\u00b7derb\u00b7ter", "Mensch", "nichts", "Gu\u00b7tes", "je", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich scheu und sch\u00e4me mich f\u00fcr dich O GOtt zu treten/", "tokens": ["Ich", "scheu", "und", "sch\u00e4\u00b7me", "mich", "f\u00fcr", "dich", "O", "Gott", "zu", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "PRF", "APPR", "PPER", "NE", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Erh\u00f6re nur mein Flehn und mein inbr\u00fcnstig Beten.", "tokens": ["Er\u00b7h\u00f6\u00b7re", "nur", "mein", "Flehn", "und", "mein", "in\u00b7br\u00fcns\u00b7tig", "Be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach wasch! ach wasche mich von meinen S\u00fcnden rein", "tokens": ["Ach", "wasch", "!", "ach", "wa\u00b7sche", "mich", "von", "mei\u00b7nen", "S\u00fcn\u00b7den", "rein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$.", "ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und la\u00df mich endlich noch zur Gnaden-Th\u00fcren ein.", "tokens": ["Und", "la\u00df", "mich", "end\u00b7lich", "noch", "zur", "Gna\u00b7den\u00b7T\u00b7h\u00fc\u00b7ren", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Vergib absonderlich/ was ich auch diesen Tag", "tokens": ["Ver\u00b7gib", "ab\u00b7son\u00b7der\u00b7lich", "/", "was", "ich", "auch", "die\u00b7sen", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "$(", "PWS", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Vor Laster/ die ich ja kaum nennen kan und mag/", "tokens": ["Vor", "Las\u00b7ter", "/", "die", "ich", "ja", "kaum", "nen\u00b7nen", "kan", "und", "mag", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "KON", "VMFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Mein HErr und GOtt/ ver\u00fcbt. Bewahre meine Glieder", "tokens": ["Mein", "Herr", "und", "Gott", "/", "ver\u00b7\u00fcbt", ".", "Be\u00b7wah\u00b7re", "mei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "$(", "VVPP", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und gib mir Schlaf und Ruh/ da\u00df ich dich