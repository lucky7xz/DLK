{"textgrid.poem.59971": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Verrat!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verrat \u2013 ihr habt's gesprochen,", "tokens": ["Ver\u00b7rat", "\u2013", "ihr", "habt's", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verrat \u2013 ihr habt's erkannt.", "tokens": ["Ver\u00b7rat", "\u2013", "ihr", "habt's", "er\u00b7kannt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sei mit euch gebrochen;", "tokens": ["Es", "sei", "mit", "euch", "ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Br\u00fccken sind verbrannt.", "tokens": ["Die", "Br\u00fc\u00b7cken", "sind", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch habt ihr selbst vergessen,", "tokens": ["Doch", "habt", "ihr", "selbst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Um Kronen habt gerauft?", "tokens": ["Um", "Kro\u00b7nen", "habt", "ger\u00b7auft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erst lief er vor dem Berge,", "tokens": ["Erst", "lief", "er", "vor", "dem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der deutsche Sumpf, davon,", "tokens": ["Der", "deut\u00b7sche", "Sumpf", ",", "da\u00b7von", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann h\u00f6hnten sie, die Zwerge,", "tokens": ["Dann", "h\u00f6hn\u00b7ten", "sie", ",", "die", "Zwer\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Revolution,", "tokens": ["Die", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die N\u00fcchternen den Zecher,", "tokens": ["Die", "N\u00fcch\u00b7ter\u00b7nen", "den", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der endlich niedersank,", "tokens": ["Der", "end\u00b7lich", "nie\u00b7der\u00b7sank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil er den Freiheitsbecher", "tokens": ["Weil", "er", "den", "Frei\u00b7heits\u00b7be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis auf die Hefen trank.", "tokens": ["Bis", "auf", "die", "He\u00b7fen", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6nredner, mit der Urne", "tokens": ["Sch\u00f6n\u00b7red\u00b7ner", ",", "mit", "der", "Ur\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der toten Herrlichkeit,", "tokens": ["Der", "to\u00b7ten", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beschritten im Kothurne", "tokens": ["Be\u00b7schrit\u00b7ten", "im", "Ko\u00b7thur\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die B\u00fchne unsrer Zeit;", "tokens": ["Die", "B\u00fch\u00b7ne", "uns\u00b7rer", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben in dem Schutte", "tokens": ["Sie", "ha\u00b7ben", "in", "dem", "Schut\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Unrat aufger\u00fchrt,", "tokens": ["Den", "Un\u00b7rat", "auf\u00b7ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Ge\u00dflerhut, die Kutte", "tokens": ["Den", "Ge\u00df\u00b7ler\u00b7hut", ",", "die", "Kut\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Frankreich eingef\u00fchrt.", "tokens": ["In", "Fran\u00b7kreich", "ein\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf hei\u00dfer Opferst\u00e4tte", "tokens": ["Auf", "hei\u00b7\u00dfer", "Op\u00b7fer\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Habt ihr, nach deutscher Art,", "tokens": ["Habt", "ihr", ",", "nach", "deut\u00b7scher", "Art", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergoldet unsre Kette", "tokens": ["Ver\u00b7gol\u00b7det", "uns\u00b7re", "Ket\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und \u2013 vor dem Rost bewahrt.", "tokens": ["Und", "\u2013", "vor", "dem", "Rost", "be\u00b7wahrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schlepptr\u00e4ger der Bourbonen \u2013", "tokens": ["Schlepp\u00b7tr\u00e4\u00b7ger", "der", "Bour\u00b7bo\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O pfui, ein garstig Lied!", "tokens": ["O", "pfui", ",", "ein", "gars\u00b7tig", "Lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo sind die Nationen,", "tokens": ["Wo", "sind", "die", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Deutschland nicht verriet!", "tokens": ["Die", "Deutschland", "nicht", "ver\u00b7riet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Zu Zeugen ruf ich Polen,", "tokens": ["Zu", "Zeu\u00b7gen", "ruf", "ich", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Heldenvolk, herbei,", "tokens": ["Das", "Hel\u00b7den\u00b7volk", ",", "her\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das dreimal ward bestohlen", "tokens": ["Das", "drei\u00b7mal", "ward", "be\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In schn\u00f6der R\u00e4uberei;", "tokens": ["In", "schn\u00f6\u00b7der", "R\u00e4u\u00b7be\u00b7rei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu Zeugen jene tote", "tokens": ["Zu", "Zeu\u00b7gen", "je\u00b7ne", "to\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Italische Republik, \u2013", "tokens": ["I\u00b7ta\u00b7li\u00b7sche", "Re\u00b7pub\u00b7lik", ",", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "$("], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Fluch euch, Ischariote", "tokens": ["Fluch", "euch", ",", "I\u00b7scha\u00b7rio\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Der deutschen Politik!", "tokens": ["Der", "deut\u00b7schen", "Po\u00b7li\u00b7tik", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir wollen's auch verraten,", "tokens": ["Wir", "wol\u00b7len's", "auch", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das schlechte Vaterland", "tokens": ["Das", "schlech\u00b7te", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der vierzig Potentaten", "tokens": ["Der", "vier\u00b7zig", "Po\u00b7ten\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deinen Unverstand,", "tokens": ["Und", "dei\u00b7nen", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie du in grauer Ferne,", "tokens": ["Wie", "du", "in", "grau\u00b7er", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Volk, dein Heil erschaust", "tokens": ["O", "Volk", ",", "dein", "Heil", "er\u00b7schaust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und lieber auf die Sterne", "tokens": ["Und", "lie\u00b7ber", "auf", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als auf dich selbst vertraust.", "tokens": ["Als", "auf", "dich", "selbst", "ver\u00b7traust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir wollen es verk\u00fcnden,", "tokens": ["Wir", "wol\u00b7len", "es", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verraten laut und dreist,", "tokens": ["Ver\u00b7ra\u00b7ten", "laut", "und", "dreist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ihr f\u00fcr \u00bbBurgen gr\u00fcnden\u00ab", "tokens": ["Was", "ihr", "f\u00fcr", "\u00bb", "Bur\u00b7gen", "gr\u00fcn\u00b7den", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "$(", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wollt unserm deutschen Geist;", "tokens": ["Wollt", "un\u00b7serm", "deut\u00b7schen", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verraten, welche Schelle", "tokens": ["Ver\u00b7ra\u00b7ten", ",", "wel\u00b7che", "Schel\u00b7le"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu deutschen Ohren klingt", "tokens": ["Zu", "deut\u00b7schen", "Oh\u00b7ren", "klingt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und welche tr\u00fcbe Quelle", "tokens": ["Und", "wel\u00b7che", "tr\u00fc\u00b7be", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im deutschen Sande springt.", "tokens": ["Im", "deut\u00b7schen", "San\u00b7de", "springt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie du das Wort beschnitten,", "tokens": ["Wie", "du", "das", "Wort", "be\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eunuchen-Regiment,", "tokens": ["Eu\u00b7nu\u00b7chen\u00b7Re\u00b7gi\u00b7ment", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie feige wir's gelitten", "tokens": ["Wie", "fei\u00b7ge", "wir's", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was man Freiheit nennt,", "tokens": ["Und", "was", "man", "Frei\u00b7heit", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Freiheit f\u00fcr \u00bbdas erstarkte", "tokens": ["Frei\u00b7heit", "f\u00fcr", "\u00bb", "das", "er\u00b7stark\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "$(", "ART", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Germanische Geschlecht\u00ab:", "tokens": ["Ger\u00b7ma\u00b7ni\u00b7sche", "Ge\u00b7schlecht", "\u00ab", ":"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$(", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Stock auf offnem Markte", "tokens": ["Den", "Stock", "auf", "off\u00b7nem", "Mark\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und das geheime Recht!", "tokens": ["Und", "das", "ge\u00b7hei\u00b7me", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie ihr in blindem Schnauben", "tokens": ["Wie", "ihr", "in", "blin\u00b7dem", "Schnau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das letzte Licht erstickt", "tokens": ["Das", "letz\u00b7te", "Licht", "er\u00b7stickt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und euren alten Glauben", "tokens": ["Und", "eu\u00b7ren", "al\u00b7ten", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit neuen Lappen flickt", "tokens": ["Mit", "neu\u00b7en", "Lap\u00b7pen", "fl