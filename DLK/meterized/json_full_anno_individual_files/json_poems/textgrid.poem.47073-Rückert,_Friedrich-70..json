{"textgrid.poem.47073": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist sie denn, die ich mit Blicken suche,", "tokens": ["Wo", "ist", "sie", "denn", ",", "die", "ich", "mit", "Bli\u00b7cken", "su\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit des Herzens Schl\u00e4gen, den geschwinden,", "tokens": ["Und", "mit", "des", "Her\u00b7zens", "Schl\u00e4\u00b7gen", ",", "den", "ge\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich unterhaltend, weil sie nicht zu finden,", "tokens": ["Mich", "un\u00b7ter\u00b7hal\u00b7tend", ",", "weil", "sie", "nicht", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "KOUS", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Vogel, Spitzenkraus' und Liederbuche?", "tokens": ["Mit", "Vo\u00b7gel", ",", "Spit\u00b7zen\u00b7kraus'", "und", "Lie\u00b7der\u00b7bu\u00b7che", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo ist sie denn, da\u00df sie mit einem Spruche", "tokens": ["Wo", "ist", "sie", "denn", ",", "da\u00df", "sie", "mit", "ei\u00b7nem", "Spru\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach ihrer Art nach meinem Wohlbefinden", "tokens": ["Nach", "ih\u00b7rer", "Art", "nach", "mei\u00b7nem", "Wohl\u00b7be\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Frag' und sich M\u00fche gebe, zu verwinden", "tokens": ["Frag'", "und", "sich", "M\u00fc\u00b7he", "ge\u00b7be", ",", "zu", "ver\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KON", "PRF", "NN", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Freud' an ihres spr\u00f6den Gasts Besuche?", "tokens": ["Die", "Freud'", "an", "ih\u00b7res", "spr\u00f6\u00b7den", "Gasts", "Be\u00b7su\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo ist sie denn? Ach, aus der Kammer dorten", "tokens": ["Wo", "ist", "sie", "denn", "?", "Ach", ",", "aus", "der", "Kam\u00b7mer", "dor\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$.", "ITJ", "$,", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tritt sie so strahlend, als ob meine Musen", "tokens": ["Tritt", "sie", "so", "strah\u00b7lend", ",", "als", "ob", "mei\u00b7ne", "Mu\u00b7sen"], "token_info": ["word", "wo