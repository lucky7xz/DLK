tetra"}, "line.3": {"text": "F\u00fcr ihre g\u00f6ttlichen Gedanken", "tokens": ["F\u00fcr", "ih\u00b7re", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erzwingt die Kunst Unsterblichkeit.", "tokens": ["Er\u00b7zwingt", "die", "Kunst", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.16": {"line.1": {"text": "Und darum Heil den Weihestunden", "tokens": ["Und", "da\u00b7rum", "Heil", "den", "Wei\u00b7hes\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Offenbarung ihrer Macht:", "tokens": ["Der", "Of\u00b7fen\u00b7ba\u00b7rung", "ih\u00b7rer", "Macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mensch, der so den Gott gefunden,", "tokens": ["Der", "Mensch", ",", "der", "so", "den", "Gott", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat die Erl\u00f6sung mit vollbracht.", "tokens": ["Hat", "die", "Er\u00b7l\u00f6\u00b7sung", "mit", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}