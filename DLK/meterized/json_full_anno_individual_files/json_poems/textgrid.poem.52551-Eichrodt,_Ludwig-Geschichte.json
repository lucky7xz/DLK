{"textgrid.poem.52551": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Geschichte", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren einmal zwei Br\u00fcder,", "tokens": ["Es", "wa\u00b7ren", "ein\u00b7mal", "zwei", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar treu zusammengesellt,", "tokens": ["Gar", "treu", "zu\u00b7sam\u00b7men\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Keine treuere gibt wieder,", "tokens": ["Kei\u00b7ne", "treu\u00b7e\u00b7re", "gibt", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So lang besteht die Welt.", "tokens": ["So", "lang", "be\u00b7steht", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie liebten sich so innig,", "tokens": ["Sie", "lieb\u00b7ten", "sich", "so", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer h\u00e4tt es einst gedacht,", "tokens": ["Wer", "h\u00e4tt", "es", "einst", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Einer doch abtr\u00fcnnig", "tokens": ["Da\u00df", "Ei\u00b7ner", "doch", "ab\u00b7tr\u00fcn\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADJD"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein sch\u00f6nes Weib gemacht?", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Weib", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ist auch \u2013 eine alte Geschichte,", "tokens": ["Ist", "auch", "\u2013", "ei\u00b7ne", "al\u00b7te", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$(", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die nur zu oft geschieht,", "tokens": ["Die", "nur", "zu", "oft", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und leider nie so schlichte", "tokens": ["Und", "lei\u00b7der", "nie", "so", "schlich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wie in diesem Lied.", "tokens": ["Als", "wie", "in", "die\u00b7sem", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es waren einmal zwei Br\u00fcder,", "tokens": ["Es", "wa\u00b7ren", "ein\u00b7mal", "zwei", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-