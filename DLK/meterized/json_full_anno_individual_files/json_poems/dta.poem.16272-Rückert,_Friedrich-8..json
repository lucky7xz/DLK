{"dta.poem.16272": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Ort, wo du einmal entgiengst des Tigers Krallen,", "tokens": ["Am", "Ort", ",", "wo", "du", "ein\u00b7mal", "ent\u00b7giengst", "des", "Ti\u00b7gers", "Kral\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst ohne Wallung du nicht leicht vor\u00fcber wallen.", "tokens": ["Wirst", "oh\u00b7ne", "Wal\u00b7lung", "du", "nicht", "leicht", "vor\u00b7\u00fc\u00b7ber", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPER", "PTKNEG", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So seh' ich nahn den Tag nicht ohne Herzenspochen,", "tokens": ["So", "seh'", "ich", "nahn", "den", "Tag", "nicht", "oh\u00b7ne", "Her\u00b7zen\u00b7spo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem vor Jahren mir das Ungl\u00fcck eingebrochen.", "tokens": ["An", "dem", "vor", "Jah\u00b7ren", "mir", "das", "Un\u00b7gl\u00fcck", "ein\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vor\u00fcber kann ich nicht ihm kommen ohne Schauern,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "kann", "ich", "nicht", "ihm", "kom\u00b7men", "oh\u00b7ne", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es m\u00f6cht' im Hinterhalt ein Ungl\u00fcck wieder lauern.", "tokens": ["Es", "m\u00f6cht'", "im", "Hin\u00b7ter\u00b7halt", "ein", "Un\u00b7gl\u00fcck", "wie\u00b7der", "lau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}