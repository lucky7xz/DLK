{"dta.poem.8385": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df sindt die federn hier so ich zue diesem wesen/", "tokens": ["Di\u00df", "sindt", "die", "fe\u00b7dern", "hier", "so", "ich", "zue", "die\u00b7sem", "we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADV", "PPER", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Au\u00df dreyen nestern zwar/ vmb mitternacht erlesen", "tokens": ["Au\u00df", "drey\u00b7en", "nes\u00b7tern", "zwar", "/", "vmb", "mit\u00b7ter\u00b7nacht", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "CARD", "VVFIN", "ADV", "$(", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vom vogel den ich wei\u00df; di\u00df ist sein eignes haar", "tokens": ["Vom", "vo\u00b7gel", "den", "ich", "wei\u00df", ";", "di\u00df", "ist", "sein", "eig\u00b7nes", "haar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "PPER", "VVFIN", "$.", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das bey dem lincken ohr ein falsches zeichen war", "tokens": ["Das", "bey", "dem", "lin\u00b7cken", "ohr", "ein", "fal\u00b7sches", "zei\u00b7chen", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der liebe die er fleucht: die feder le\u00dft das fliegen;", "tokens": ["Der", "lie\u00b7be", "die", "er", "fleucht", ":", "die", "fe\u00b7der", "le\u00dft", "das", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRELS", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein haar wirdt jetzt ein bandt; er soll mir auch erliegen.", "tokens": ["Sein", "haar", "wirdt", "jetzt", "ein", "bandt", ";", "er", "soll", "mir", "auch", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADJD", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}