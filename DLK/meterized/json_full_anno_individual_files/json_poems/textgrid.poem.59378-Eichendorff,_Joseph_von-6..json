{"textgrid.poem.59378": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann frisch die buntgewirkten Schleier wallen,", "tokens": ["Wann", "frisch", "die", "bunt\u00b7ge\u00b7wirk\u00b7ten", "Schlei\u00b7er", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weit in das Land die Lerchen mich verf\u00fchren,", "tokens": ["Weit", "in", "das", "Land", "die", "Ler\u00b7chen", "mich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da kann ich's tief im Herzen wieder sp\u00fcren,", "tokens": ["Da", "kann", "ich's", "tief", "im", "Her\u00b7zen", "wie\u00b7der", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie mich die Eine liebt und ruft vor allen.", "tokens": ["Wie", "mich", "die", "Ei\u00b7ne", "liebt", "und", "ruft", "vor", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn Nachtigalln aus gr\u00fcnen Hallen schallen,", "tokens": ["Wenn", "Nach\u00b7ti\u00b7galln", "aus", "gr\u00fc\u00b7nen", "Hal\u00b7len", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wen m\u00f6chten nicht die tiefen T\u00f6ne r\u00fchren;", "tokens": ["Wen", "m\u00f6ch\u00b7ten", "nicht", "die", "tie\u00b7fen", "T\u00f6\u00b7ne", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wen nicht das s\u00fc\u00dfe Herzeleid verf\u00fchren,", "tokens": ["Wen", "nicht", "das", "s\u00fc\u00b7\u00dfe", "Her\u00b7ze\u00b7leid", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Liebesschlagen tot vom Baum zu fallen? \u2013", "tokens": ["Im", "Lie\u00b7bes\u00b7schla\u00b7gen", "tot", "vom", "Baum", "zu", "fal\u00b7len", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sag auch ich bei jedem Fr\u00fchlingsglanze:", "tokens": ["So", "sag", "auch", "ich", "bei", "je\u00b7dem", "Fr\u00fch\u00b7lings\u00b7glan\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du s\u00fc\u00dfe Laute! la\u00df uns beide sterben,", "tokens": ["Du", "s\u00fc\u00b7\u00dfe", "Lau\u00b7te", "!", "la\u00df", "uns", "bei\u00b7de", "ster\u00b7ben",