{"dta.poem.5886": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch ihn kan unser Geist bis an den Himmel fliegen;", "tokens": ["Durch", "ihn", "kan", "un\u00b7ser", "Geist", "bis", "an", "den", "Him\u00b7mel", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sehn durch ihn das Kunst-Werck und die Spur", "tokens": ["Wir", "sehn", "durch", "ihn", "das", "Kunst\u00b7\u00b7Werck", "und", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der sich verh\u00fcllenden Natur,", "tokens": ["Der", "sich", "ver\u00b7h\u00fcl\u00b7len\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJA", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man kan in ihrem Schoo\u00df von den geheimen Wesen", "tokens": ["Man", "kan", "in", "ih\u00b7rem", "Schoo\u00df", "von", "den", "ge\u00b7hei\u00b7men", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die z\u00e4rtsten Z\u00fcge deutlich lesen.", "tokens": ["Die", "z\u00e4rts\u00b7ten", "Z\u00fc\u00b7ge", "deut\u00b7lich", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}