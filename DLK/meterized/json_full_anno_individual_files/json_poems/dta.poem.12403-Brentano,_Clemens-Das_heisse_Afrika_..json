{"dta.poem.12403": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das heisse Afrika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf! ihr Br\u00fcder und seyd stark!               ", "tokens": ["Auf", ",", "auf", "!", "ihr", "Br\u00fc\u00b7der", "und", "seyd", "stark", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$.", "PPOSAT", "NN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Abschiedstag ist da,", "tokens": ["Der", "Ab\u00b7schieds\u00b7tag", "ist", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwer liegt er auf der Seele, schwer!", "tokens": ["Schwer", "liegt", "er", "auf", "der", "See\u00b7le", ",", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sollen \u00fcber Land und Meer,", "tokens": ["Wir", "sol\u00b7len", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ins hei\u00dfe Afrika. :,:", "tokens": ["Ins", "hei\u00b7\u00dfe", "Af\u00b7ri\u00b7ka", ".", ":", ",", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPRART", "ADJA", "NE", "$.", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein dichter Kreis von Lieben steht,", "tokens": ["Ein", "dich\u00b7ter", "Kreis", "von", "Lie\u00b7ben", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O, Br\u00fcder! um uns her.", "tokens": ["O", ",", "Br\u00fc\u00b7der", "!", "um", "uns", "her", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns kn\u00fcpft so manches theure Band,", "tokens": ["Uns", "kn\u00fcpft", "so", "man\u00b7ches", "theu\u00b7re", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An unser teutsches Vaterland,", "tokens": ["An", "un\u00b7ser", "teut\u00b7sches", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum f\u00e4llt der Abschied schwer. :,:", "tokens": ["Drum", "f\u00e4llt", "der", "Ab\u00b7schied", "schwer", ".", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADJD", "$.", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem bieten graue Eltern noch,", "tokens": ["Dem", "bie\u00b7ten", "grau\u00b7e", "El\u00b7tern", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum leztenmal die Hand,", "tokens": ["Zum", "lez\u00b7ten\u00b7mal", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den kosen Br\u00fcder, Schwester, Freund,", "tokens": ["Den", "ko\u00b7sen", "Br\u00fc\u00b7der", ",", "Schwes\u00b7ter", ",", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles schweigt, und alles weint,", "tokens": ["Und", "al\u00b7les", "schweigt", ",", "und", "al\u00b7les", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Todtbla\u00df von uns gewandt. :,:", "tokens": ["Todt\u00b7bla\u00df", "von", "uns", "ge\u00b7wandt", ".", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$.", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}