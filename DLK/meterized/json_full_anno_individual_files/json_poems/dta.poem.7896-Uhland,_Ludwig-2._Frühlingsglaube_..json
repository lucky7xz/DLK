{"dta.poem.7896": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Fr\u00fchlingsglaube .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Die linden L\u00fcfte sind erwacht,", "tokens": ["Die", "lin\u00b7den", "L\u00fcf\u00b7te", "sind", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie s\u00e4useln und weben Tag und Nacht,", "tokens": ["Sie", "s\u00e4u\u00b7seln", "und", "we\u00b7ben", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie schaffen an allen Enden.", "tokens": ["Sie", "schaf\u00b7fen", "an", "al\u00b7len", "En\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O frischer Duft, o neuer Klang!", "tokens": ["O", "fri\u00b7scher", "Duft", ",", "o", "neu\u00b7er", "Klang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun, armes Herze, sey nicht bang!", "tokens": ["Nun", ",", "ar\u00b7mes", "Her\u00b7ze", ",", "sey", "nicht", "bang", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "VVFIN", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun mu\u00df sich Alles, Alles wenden.", "tokens": ["Nun", "mu\u00df", "sich", "Al\u00b7les", ",", "Al\u00b7les", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIS", "$,", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Welt wird sch\u00f6ner mit jedem Tag,", "tokens": ["Die", "Welt", "wird", "sch\u00f6\u00b7ner", "mit", "je\u00b7dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Man wei\u00df nicht, was noch werden mag,", "tokens": ["Man", "wei\u00df", "nicht", ",", "was", "noch", "wer\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "PRELS", "ADV", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Bl\u00fchen will nicht enden.", "tokens": ["Das", "Bl\u00fc\u00b7hen", "will", "nicht", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es bl\u00fcht das fernste, tiefste Thal.", "tokens": ["Es", "bl\u00fcht", "das", "ferns\u00b7te", ",", "tiefs\u00b7te", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun, armes Herz, vergi\u00df der Qual!", "tokens": ["Nun", ",", "ar\u00b7mes", "Herz", ",", "ver\u00b7gi\u00df", "der", "Qual", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun mu\u00df sich Alles, Alles wenden.", "tokens": ["Nun", "mu\u00df", "sich", "Al\u00b7les", ",", "Al\u00b7les", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIS", "$,", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}