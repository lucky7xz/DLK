": ["Auf\u00b7su\u00b7chend", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sonne T\u00f6chter, die blanken Maide,", "tokens": ["Der", "Son\u00b7ne", "T\u00f6ch\u00b7ter", ",", "die", "blan\u00b7ken", "Mai\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und bieten ihnen Herz und Hand.", "tokens": ["Und", "bie\u00b7ten", "ih\u00b7nen", "Herz", "und", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6r' im Himmel Rosse traben;", "tokens": ["Ich", "h\u00f6r'", "im", "Him\u00b7mel", "Ros\u00b7se", "tra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drauf sitzen Gottes sch\u00f6ne Knaben,", "tokens": ["Drauf", "sit\u00b7zen", "Got\u00b7tes", "sch\u00f6\u00b7ne", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die keine Rast und Ruhe haben", "tokens": ["Die", "kei\u00b7ne", "Rast", "und", "Ru\u00b7he", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer Minne hei\u00dfem Brand.", "tokens": ["In", "ih\u00b7rer", "Min\u00b7ne", 