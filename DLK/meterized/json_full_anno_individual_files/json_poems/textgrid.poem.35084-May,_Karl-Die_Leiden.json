{"textgrid.poem.35084": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Die Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein Schwert durch meine Seele,", "tokens": ["Es", "ging", "ein", "Schwert", "durch", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie es einst durch Maria ging.", "tokens": ["Wie", "es", "einst", "durch", "Ma\u00b7ria", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ichs gesteh, ob ichs verhehle,", "tokens": ["Ob", "ichs", "ge\u00b7steh", ",", "ob", "ichs", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich zu sehr am Irdschen hing,", "tokens": ["Da\u00df", "ich", "zu", "sehr", "am", "Ird\u00b7schen", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ward durch dieses Schwert getroffen,", "tokens": ["Es", "ward", "durch", "die\u00b7ses", "Schwert", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich, ich la\u00df die Wunde offen.", "tokens": ["Und", "ich", ",", "ich", "la\u00df", "die", "Wun\u00b7de", "of\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun wird es langsam sich verbluten.", "tokens": ["Nun", "wird", "es", "lang\u00b7sam", "sich", "ver\u00b7blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar ists mein eignes Blut, das flie\u00dft,", "tokens": ["Zwar", "ists", "mein", "eig\u00b7nes", "Blut", ",", "das", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch auch die Gegnerschaft des Guten,", "tokens": ["Doch", "auch", "die", "Geg\u00b7ner\u00b7schaft", "des", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die aus der Wunde sich ergie\u00dft.", "tokens": ["Die", "aus", "der", "Wun\u00b7de", "sich", "er\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich la\u00df das alte Leben rinnen,", "tokens": ["Ich", "la\u00df", "das", "al\u00b7te", "Le\u00b7ben", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein neues, bess'res zu gewinnen.", "tokens": ["Ein", "neu\u00b7es", ",", "bess'\u00b7res", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist der gro\u00dfe Zweck der Leiden,", "tokens": ["Es", "ist", "der", "gro\u00b7\u00dfe", "Zweck", "der", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der durch die ganze Sch\u00f6pfung geht:", "tokens": ["Der", "durch", "die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie nahen nur, um auszuscheiden,", "tokens": ["Sie", "na\u00b7hen", "nur", ",", "um", "aus\u00b7zu\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADV", "$,", "KOUI", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Gottes Rathschlu\u00df widersteht.", "tokens": ["Was", "Got\u00b7tes", "Rath\u00b7schlu\u00df", "wi\u00b7der\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will im Leid, das mir geschehen,", "tokens": ["Ich", "will", "im", "Leid", ",", "das", "mir", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "$,", "PRELS", "PPER