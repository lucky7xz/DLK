{"textgrid.poem.44122": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du ungeschminckter Freund bedarfst der W\u00fcntsche nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ungeschminckter Freund bedarfst der W\u00fcntsche nicht,", "tokens": ["Du", "un\u00b7ge\u00b7schminck\u00b7ter", "Freund", "be\u00b7darfst", "der", "W\u00fcnt\u00b7sche", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schein und Heucheley an Ort und Tage binden;", "tokens": ["Die", "Schein", "und", "Heu\u00b7che\u00b7ley", "an", "Ort", "und", "Ta\u00b7ge", "bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Tugend weis vor sich mehr Gl\u00fcck und Heil zu finden,", "tokens": ["Die", "Tu\u00b7gend", "weis", "vor", "sich", "mehr", "Gl\u00fcck", "und", "Heil", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "PRF", "PIAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als immermehr ein Kiel erlogner Treu verspricht.", "tokens": ["Als", "im\u00b7mer\u00b7mehr", "ein", "Kiel", "er\u00b7log\u00b7ner", "Treu", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NE", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wahr du Warheit liebst, so wahr verflucht die Pflicht", "tokens": ["So", "wahr", "du", "War\u00b7heit", "liebst", ",", "so", "wahr", "ver\u00b7flucht", "die", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "NN", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von meiner Redligkeit dergleichen Modes\u00fcnden.", "tokens": ["Von", "mei\u00b7ner", "Red\u00b7lig\u00b7keit", "derg\u00b7lei\u00b7chen", "Mo\u00b7de\u00b7s\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Reime, so sich jezt nur dir zu Dienste winden,", "tokens": ["Die", "Rei\u00b7me", ",", "so", "sich", "jezt", "nur", "dir", "zu", "Diens\u00b7te", "win\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "ADV", "ADV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bedencken blos das Band, woran die Freundschaft flicht.", "tokens": ["Be\u00b7den\u00b7cken", "blos", "das", "Band", ",", "wo\u00b7ran", "die", "Freund\u00b7schaft", "flicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Geist erlangt bereits das h\u00f6chste Gut auf Erden;", "tokens": ["Dein", "Geist", "er\u00b7langt", "be\u00b7reits", "das", "h\u00f6chs\u00b7te", "Gut", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihn vergn\u00fcgen kan, das steht in deiner Macht.", "tokens": ["Was", "ihn", "ver\u00b7gn\u00fc\u00b7gen", "kan", ",", "das", "steht", "in", "dei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch da wir auf die Welt auch Leiber mitgebracht,", "tokens": ["Doch", "da", "wir", "auf", "die", "Welt", "auch", "Lei\u00b7ber", "mit\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sonder unsre Schuld gar leicht getrofen werden,", "tokens": ["Die", "son\u00b7der", "uns\u00b7re", "Schuld", "gar", "leicht", "ge\u00b7tro\u00b7fen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So glaub ich, (pflegt ein Gott die Inbrunst einzusehn,)", "tokens": ["So", "glaub", "ich", ",", "(", "pflegt", "ein", "Gott", "die", "In\u00b7brunst", "ein\u00b7zu\u00b7sehn", ",", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ART", "NN", "AR