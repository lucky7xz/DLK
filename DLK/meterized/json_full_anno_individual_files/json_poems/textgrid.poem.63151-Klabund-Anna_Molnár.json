{"textgrid.poem.63151": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Anna Moln\u00e1r", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stieg aufs Ross Martin Aig\u00f3.", "tokens": ["Es", "stieg", "aufs", "Ross", "Mar\u00b7tin", "Aig\u00f3", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NE", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Steppe ruft. Die Ferne lockt.", "tokens": ["Die", "Step\u00b7pe", "ruft", ".", "Die", "Fer\u00b7ne", "lockt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er traf am Weg Anna Moln\u00e1r.", "tokens": ["Er", "traf", "am", "Weg", "An\u00b7na", "Moln\u00e1r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NE", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "\u00bbkomm mit mir, sch\u00f6ne Anna Moln\u00e1r.", "tokens": ["\u00bb", "komm", "mit", "mir", ",", "sch\u00f6\u00b7ne", "An\u00b7na", "Moln\u00e1r", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "$,", "ADJA", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Steppe ruft. Die Ferne lockt.\u00ab", "tokens": ["Die", "Step\u00b7pe", "ruft", ".", "Die", "Fer\u00b7ne", "lockt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbsie lockt mich nicht, Martin Aig\u00f3.", "tokens": ["\u00bb", "sie", "lockt", "mich", "nicht", ",", "Mar\u00b7tin", "Aig\u00f3", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Zu Haus erwartet mich mein Mann,", "tokens": ["Zu", "Haus", "er\u00b7war\u00b7tet", "mich", "mein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im stillen Haus ein frommer Mann.", "tokens": ["Im", "stil\u00b7len", "Haus", "ein", "from\u00b7mer", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Kindlein hat er auf dem Knie.\u00ab", "tokens": ["Mein", "Kin\u00b7dlein", "hat", "er", "auf", "dem", "Knie", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er fleht. Sie geht. Er raubte sie.", "tokens": ["Er", "fleht", ".", "Sie", "geht", ".", "Er", "raub\u00b7te", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie nahmen ihren Weg zu zwein.", "tokens": ["Sie", "nah\u00b7men", "ih\u00b7ren", "Weg", "zu", "zwein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Steppe ruft. Die Ferne lockt.", "tokens": ["Die", "Step\u00b7pe", "ruft", ".", "Die", "Fer\u00b7ne", "lockt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ruhten unter einem Baum,", "tokens": ["Sie", "ruh\u00b7ten", "un\u00b7ter", "ei\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schatten fiel in ihren Traum.", "tokens": ["Und", "Schat\u00b7ten", "fiel", "in", "ih\u00b7ren", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbsieh mir ins Aug, Anna Moln\u00e1r!\u00ab", "tokens": ["\u00bb", "sieh", "mir", "ins", "Aug", ",", "An\u00b7na", "Moln\u00e1r", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPRART", "NN", "$,", "NE", "NE", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Sie hebt den Blick. Ihr Aug ist nass.", "tokens": ["Sie", "hebt", "den", "Blick", ".", "Ihr", "Aug", "ist", "nass", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbwas weinst du, sch\u00f6ne Anna Moln\u00e1r?\u00ab", "tokens": ["\u00bb", "was", "weinst", "du", ",", "sch\u00f6\u00b7ne", "An\u00b7na", "Moln\u00e1r", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "ADJA", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbich weine nicht, Martin Aig\u00f3.", "tokens": ["\u00bb", "ich", "wei\u00b7ne", "nicht", ",", "Mar\u00b7tin", "Aig\u00f3", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Mein Auge ist von Tau so nass,", "tokens": ["Mein", "Au\u00b7ge", "ist", "von", "Tau", "so", "nass", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der vom Gezweig des Baumes tropft!\u00ab", "tokens": ["Der", "vom", "Ge\u00b7zweig", "des", "Bau\u00b7mes", "tropft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbes tropft kein Tau. Die Sonn steht hoch.\u00ab", "tokens": ["\u00bb", "es", "tropft", "kein", "Tau", ".", "Die", "Sonn", "steht", "hoch", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "$.", "ART", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Martin Aig\u00f3 stieg auf den Baum.", "tokens": ["Mar\u00b7tin", "Aig\u00f3", "stieg", "auf", "den", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es bog den Ast die starke Last.", "tokens": ["Es", "bog", "den", "Ast", "die", "star\u00b7ke", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fiel sein Pallasch ihm herab.", "tokens": ["Da", "fiel", "sein", "Pal\u00b7lasch", "ihm", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbgib mir den Pallasch, Anna Moln\u00e1r.\u00ab", "tokens": ["\u00bb", "gib", "mir", "den", "Pal\u00b7lasch", ",", "An\u00b7na", "Moln\u00e1r", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "NN", "$,", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie warf den Pallasch ihm empor,", "tokens": ["Sie", "warf", "den", "Pal\u00b7lasch", "ihm", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dass er ihm in die Seite drang", "tokens": ["Dass", "er", "ihm", "in", "die", "Sei\u00b7te", "drang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Blut aus allen Zweigen sprang.", "tokens": ["Und", "Blut", "aus", "al\u00b7len", "Zwei\u00b7gen", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie zog sich seine R\u00fcstung an,", "tokens": ["Sie", "zog", "sich", "sei\u00b7ne", "R\u00fcs\u00b7tung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bestieg das Ross und ritt nach Haus", "tokens": ["Be\u00b7stieg", "das", "Ross", "und", "ritt", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und z\u00fcgelte das Ross am Haus.", "tokens": ["Und", "z\u00fc\u00b7gel\u00b7te", "das", "Ross", "am", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbdu frommer Mann, so h\u00f6r mich an,", "tokens": ["\u00bb", "du", "from\u00b7mer", "Mann", ",", "so", "h\u00f6r", "mich", "an", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du Quartier f\u00fcr eine Nacht?\u00ab", "tokens": ["Hast", "du", "Quar\u00b7tier", "f\u00fcr", "ei\u00b7ne", "Nacht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbhab kein Quartier f\u00fcr Euch, mein Herr.", "tokens": ["\u00bb", "hab", "kein", "Quar\u00b7tier", "f\u00fcr", "Euch", ",", "mein", "Herr", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIAT", "NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mein kleiner Knabe weint so sehr.\u00ab", "tokens": ["Mein", "klei\u00b7ner", "Kna\u00b7be", "weint", "so", "sehr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie fleht. Er steht. Er willigt ein.", "tokens": ["Sie", "fleht", ".", "Er", "steht", ".", "Er", "wil\u00b7ligt", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbdu frommer Mann, so h\u00f6r mich an,", "tokens": ["\u00bb", "du", "from\u00b7mer", "Mann", ",", "so", "h\u00f6r", "mich", "an", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gibt es im Dorfe guten Wein,", "tokens": ["Gibt", "es", "im", "Dor\u00b7fe", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bring mir einen Humpen voll!\u00ab", "tokens": ["So", "bring", "mir", "ei\u00b7nen", "Hum\u00b7pen", "voll", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er geht