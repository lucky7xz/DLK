{"textgrid.poem.37036": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Acktaeon", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem alten Rittersitz,", "tokens": ["Auf", "ei\u00b7nem", "al\u00b7ten", "Rit\u00b7ter\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den seine Ahnen sich erlasen,", "tokens": ["Den", "sei\u00b7ne", "Ah\u00b7nen", "sich", "er\u00b7la\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Regierte einst Herr Acktaeon,", "tokens": ["Re\u00b7gier\u00b7te", "einst", "Herr", "Ack\u00b7ta\u00b7e\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "NE", "$,"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ein W\u00fctrich gegen Hasen.", "tokens": ["Ein", "W\u00fct\u00b7rich", "ge\u00b7gen", "Ha\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erstaune Nachwelt, welch ein Geist", "tokens": ["Er\u00b7stau\u00b7ne", "Nach\u00b7welt", ",", "welch", "ein", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Acktaeon gewesen!", "tokens": ["Herr", "Ack\u00b7ta\u00b7e\u00b7on", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er konnte schon im zw\u00f6lften Jahr", "tokens": ["Er", "konn\u00b7te", "schon", "im", "zw\u00f6lf\u00b7ten", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Abendseegen lesen.", "tokens": ["Den", "A\u00b7ben\u00b7dsee\u00b7gen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mama zerflo\u00df in Freude schier,", "tokens": ["Ma\u00b7ma", "zer\u00b7flo\u00df", "in", "Freu\u00b7de", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ihm von seinem Bogen", "tokens": ["Als", "ihm", "von", "sei\u00b7nem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tief in des sch\u00f6nsten Fuchses Herz", "tokens": ["Tief", "in", "des", "sch\u00f6ns\u00b7ten", "Fuch\u00b7ses", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ersten Pfeile flogen.", "tokens": ["Die", "ers\u00b7ten", "Pfei\u00b7le", "flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Papachen lobte sein Talent,", "tokens": ["Pa\u00b7pa\u00b7chen", "lob\u00b7te", "sein", "Ta\u00b7lent", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und pflegte oft zu sagen,", "tokens": ["Und", "pfleg\u00b7te", "oft", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dies S\u00f6hnchen sey ihm sicherlich", "tokens": ["Dies", "S\u00f6hn\u00b7chen", "sey", "ihm", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht aus der Art geschlagen.", "tokens": ["Nicht", "aus", "der", "Art", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er sollte Fr\u00e4ulein Adelheid", "tokens": ["Er", "soll\u00b7te", "Fr\u00e4u\u00b7lein", "A\u00b7del\u00b7heid"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In wenig Wochen freyen.", "tokens": ["In", "we\u00b7nig", "Wo\u00b7chen", "frey\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An ihrem Busen dacht er sich", "tokens": ["An", "ih\u00b7rem", "Bu\u00b7sen", "dacht", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Liebe recht zu weyhen.", "tokens": ["Der", "Lie\u00b7be", "recht", "zu", "wey\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du armer Junker Acktaeon!", "tokens": ["Du", "ar\u00b7mer", "Jun\u00b7ker", "Ack\u00b7ta\u00b7e\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Grausamkeit der G\u00f6tter", "tokens": ["Die", "Grau\u00b7sam\u00b7keit", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Versagt dir ihren Necktarku\u00df,", "tokens": ["Ver\u00b7sagt", "dir", "ih\u00b7ren", "Neck\u00b7tar\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Hymens Myrthenbl\u00e4tter.", "tokens": ["Und", "Hy\u00b7mens", "Myr\u00b7then\u00b7bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihm winkte einst ein Silberbach,", "tokens": ["Ihm", "wink\u00b7te", "einst", "ein", "Sil\u00b7ber\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der durch ein W\u00e4ldchen h\u00fcpfte,", "tokens": ["Der", "durch", "ein", "W\u00e4ld\u00b7chen", "h\u00fcpf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als er ermattet von der Jagd", "tokens": ["Als", "er", "er\u00b7mat\u00b7tet", "von", "der", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In k\u00fchle Schatten schl\u00fcpfte.", "tokens": ["In", "k\u00fch\u00b7le", "Schat\u00b7ten", "schl\u00fcpf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er trippelt hin, und staunt zur\u00fcck \u2013", "tokens": ["Er", "trip\u00b7pelt", "hin", ",", "und", "staunt", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Napaeen, Oreaden,", "tokens": ["Na\u00b7pae\u00b7en", ",", "O\u00b7rea\u00b7den", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und selbst Dianen sah er sich", "tokens": ["Und", "selbst", "Di\u00b7a\u00b7nen", "sah", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NE", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In diesem Bache baden.", "tokens": ["In", "die\u00b7sem", "Ba\u00b7che", "ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Damen wurden feuerroth,", "tokens": ["Die", "Da\u00b7men", "wur\u00b7den", "feu\u00b7er\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sanken rauschend nieder.", "tokens": ["Und", "san\u00b7ken", "rau\u00b7schend", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit beiden H\u00e4nden tappten sie", "tokens": ["Mit", "bei\u00b7den", "H\u00e4n\u00b7den", "tapp\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach ihrem Rock und Mieder.", "tokens": ["Nach", "ih\u00b7rem", "Rock", "und", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Diana aber, Wuth im Blick,", "tokens": ["Di\u00b7a\u00b7na", "a\u00b7ber", ",", "Wuth", "im", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nahm Wa\u00dfer, und bespr\u00fctzte", "tokens": ["Nahm", "Wa\u00b7\u00dfer", ",", "und", "be\u00b7spr\u00fctz\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Junker, dem die L\u00fcsternheit", "tokens": ["Den", "Jun\u00b7ker", ",", "dem", "die", "L\u00fcs\u00b7tern\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus beiden Augen blitzte.", "tokens": ["Aus", "bei\u00b7den", "Au\u00b7gen", "blitz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Man seh einmahl! Ein Hirschgeweyh", "tokens": ["Man", "seh", "ein\u00b7mahl", "!", "Ein", "Hirschge\u00b7weyh"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$.", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Von mehr als sechzehn Enden", "tokens": ["Von", "mehr", "als", "sech\u00b7zehn", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bekr\u00f6nt sein Haupt; ein braunes Fell", "tokens": ["Be\u00b7kr\u00f6nt", "sein", "Haupt", ";", "ein", "brau\u00b7nes", "Fell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umh\u00fcllet seine Lenden.", "tokens": ["Um\u00b7h\u00fcl\u00b7let", "sei\u00b7ne", "Len\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mit langen Beinen setzet er", "tokens": ["Mit", "lan\u00b7gen", "Bei\u00b7nen", "set\u00b7zet", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch B\u00fcsche und durch Hecken,", "tokens": ["Durch", "B\u00fc\u00b7sche", "und", "durch", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gafft furchtsam um sich her, und will", "tokens": ["Gafft", "furcht\u00b7sam", "um", "sich", "her", ",", "und", "will"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PRF", "PTKVZ", "$,", "KON", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Walde sich verstecken.", "tokens": ["Im", "Wal\u00b7de", "sich", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er tanzet seufzend durch den Hayn;", "tokens": ["Er", "tan\u00b7zet", "seuf\u00b7zend", "durch", "den", "Hayn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier liegen seine Hunde;", "tokens": ["Hier", "lie\u00b7gen", "sei\u00b7ne", "Hun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die springen zu, und t\u00f6dten ihn", "tokens": ["Die", "sprin\u00b7gen", "zu", ",", "und", "t\u00f6d\u00b7ten", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch manche tiefe Wunde.", "tokens": ["Durch", "man\u00b7che", "tie\u00b7fe", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.14": {"line.1": {"text": "Auf einem alten Rittersitz,", "tokens": ["Auf", "ei\u00b7nem", "al\u00b7ten", "Rit\u00b7ter\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den seine Ahnen sich erlasen,", "tokens": ["Den", "sei\u00b7ne", "Ah\u00b7nen", "sich", "er\u00b7la\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", 