{"dta.poem.12718": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die promotion   M.  Siebenhaars.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das einmal-eins, und was wir drinne zehlen,", "tokens": ["Das", "ein\u00b7ma\u00b7leins", ",", "und", "was", "wir", "drin\u00b7ne", "zeh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "PWS", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kan keinen reicher machen.", "tokens": ["Kan", "kei\u00b7nen", "rei\u00b7cher", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer w\u00fcrde nicht viel tausend sich erwehlen,", "tokens": ["Wer", "w\u00fcr\u00b7de", "nicht", "viel", "tau\u00b7send", "sich", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "ADV", "CARD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Croesus selber seyn,", "tokens": ["Und", "Cro\u00b7e\u00b7sus", "sel\u00b7ber", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn nach der zahl die g\u00fcter tr\u00e4ffen ein!", "tokens": ["Wenn", "nach", "der", "zahl", "die", "g\u00fc\u00b7ter", "tr\u00e4f\u00b7fen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die sache thut das beste hier,", "tokens": ["Die", "sa\u00b7che", "thut", "das", "bes\u00b7te", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die mit der zahl sich n\u00fctzlich einverleibet,", "tokens": ["Die", "mit", "der", "zahl", "sich", "n\u00fctz\u00b7lich", "ein\u00b7ver\u00b7lei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dieselbe blickt bey deiner sieben f\u00fcr.", "tokens": ["Die\u00b7sel\u00b7be", "blickt", "bey", "dei\u00b7ner", "sie\u00b7ben", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "APPR", "PPOSAT", "CARD", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und diese sieben sachen", "tokens": ["Und", "die\u00b7se", "sie\u00b7ben", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wirst du noch besser machen.", "tokens": ["Wirst", "du", "noch", "bes\u00b7ser", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}