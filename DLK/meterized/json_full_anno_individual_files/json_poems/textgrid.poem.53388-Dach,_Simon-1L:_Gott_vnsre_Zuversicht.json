","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df deines Wortes Stern", "tokens": ["La\u00df", "dei\u00b7nes", "Wor\u00b7tes", "Stern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In vnsern Hertzen blincken,", "tokens": ["In", "vn\u00b7sern", "Hert\u00b7zen", "blin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst m\u00f6chten wir versincken,", "tokens": ["Sonst", "m\u00f6ch\u00b7ten", "wir", "ver\u00b7sin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun deines Eifers Wellen", "tokens": ["Nun", "dei\u00b7nes", "Ei\u00b7fers", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich vns entgegen stellen.", "tokens": ["Sich", "vns", "ent\u00b7ge\u00b7gen", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des Wassers Ubermuth", "tokens": ["Des", "Was\u00b7sers", "U\u00b7ber\u00b7muth"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist vns mit stoltzer Fluth", "tokens": ["Ist", "vns", "mit", "stolt\u00b7zer", "Fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon Seelen-an gedrungen.", "tokens": ["Schon", "See\u00b7len\u00b7an", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt werden wir verschlungen,", "tokens": ["Jetzt", "wer\u00b7den", "wir", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn wirst du doch erwachen,", "tokens": ["Wenn", "wirst", "du", "doch", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du Haffen vnsrer Sachen?", "tokens": ["Du", "Haf\u00b7fen", "vns\u00b7rer", "Sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Steh auff von deiner Rhu,", "tokens": ["Steh", "auff", "von", "dei\u00b7ner", "Rhu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schrey Wind' und Wellen zu,", "tokens": ["Schrey", "Wind'", "und", "Wel\u00b7len", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du weist das Meer zu stillen", "tokens": ["Du", "weist", "das", "Meer", "zu", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit blossem Winck vnd Willen,", "tokens": ["Mit", "blos\u00b7sem", "Win\u00b7ck", "vnd", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn alles was verhanden", "tokens": ["Denn", "al\u00b7les", "was", "ver\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "PWS", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist durch dein Wort entstanden.", "tokens": ["Ist", "durch", "dein", "Wort", "ent\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nimm ja in der Gefahr", "tokens": ["Nimm", "ja", "in", "der", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht unsers Kleinmuhts war,", "tokens": ["Nicht", "un\u00b7sers", "Klein\u00b7muhts", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sich bey vns mu\u00df regen", "tokens": ["Der", "sich", "bey", "vns", "mu\u00df", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PPER", "VMFIN", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ach vnsrer S\u00fcnden wegen,", "tokens": ["Ach", "vns\u00b7rer", "S\u00fcn\u00b7den", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn des Gewissens Plagen", "tokens": ["Denn", "des", "Ge\u00b7wis\u00b7sens", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erwecken Furcht und Zagen.", "tokens": ["Er\u00b7we\u00b7cken", "Furcht", "und", "Za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00df vnsre Hertzen rein", "tokens": ["La\u00df", "vns\u00b7re", "Hert\u00b7zen", "rein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Durch Christi Blutbad seyn,", "tokens": ["Durch", "Chris\u00b7ti", "Blut\u00b7bad", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Angst so er empfunden", "tokens": ["Die", "Angst", "so", "er", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Halt' vns der Furcht entbunden,", "tokens": ["Halt'", "vns", "der", "Furcht", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und seines Sieges Wercke", "tokens": ["Und", "sei\u00b7nes", "Sie\u00b7ges", "Wer\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Seyn unsers Glaubens St\u00e4rcke", "tokens": ["Seyn", "un\u00b7sers", "Glau\u00b7bens", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vnd wahre Freudigheit", "tokens": ["Vnd", "wah\u00b7re", "Freu\u00b7dig\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bey dieser schweren Zeit,", "tokens": ["Bey", "die\u00b7ser", "schwe\u00b7ren", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wieder vns der Hellen", "tokens": ["Da", "wie\u00b7der", "vns", "der", "Hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ges\u00fcmpff sich auff- wil schwellen,", "tokens": ["Ge\u00b7s\u00fcmpff", "sich", "auf\u00b7f", "wil", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "TRUNC", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lehr wider alles Grawen", "tokens": ["Lehr", "wi\u00b7der", "al\u00b7les", "Gra\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vns kindlich dir vertrawen.", "tokens": ["Vns", "kind\u00b7lich", "dir", "ver\u00b7tra\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du bist ja fort vnd fort", "tokens": ["Du", "bist", "ja", "fort", "vnd", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Israels Schutz vnd Hort", "tokens": ["Is\u00b7raels", "Schutz", "vnd", "Hort"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie du es warst vor Zeiten,", "tokens": ["Wie", "du", "es", "warst", "vor", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd stehest jetzt von weitem?", "tokens": ["Vnd", "ste\u00b7hest", "jetzt", "von", "wei\u00b7tem", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bist einem Riesen eben", "tokens": ["Bist", "ei\u00b7nem", "Rie\u00b7sen", "e\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von dem kein Schutz zu heben?", "tokens": ["Von", "dem", "kein", "Schutz", "zu", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Leichen Staub vnd Stanck", "tokens": ["Der", "Lei\u00b7chen", "Staub", "vnd", "Stanck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erzeigt er dir auch Danck?", "tokens": ["Er\u00b7zeigt", "er", "dir", "auch", "Danck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wirst du von allem B\u00f6sen", "tokens": ["Wirst", "du", "von", "al\u00b7lem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER",