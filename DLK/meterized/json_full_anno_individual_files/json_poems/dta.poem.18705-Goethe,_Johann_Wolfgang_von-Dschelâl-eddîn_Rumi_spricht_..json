{"dta.poem.18705": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Dschel\u00e2l-edd\u00een Rumi  \n spricht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Verweilst du in der Welt, sie fleieht als Traum,", "tokens": ["Ver\u00b7weilst", "du", "in", "der", "Welt", ",", "sie", "flei\u00b7eht", "als", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du reisest, ein Geschick bestimmt den Raum,", "tokens": ["Du", "rei\u00b7sest", ",", "ein", "Ge\u00b7schick", "be\u00b7stimmt", "den", "Raum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Hitze, K\u00e4lte nicht vermagst du fest zu halten,", "tokens": ["Nicht", "Hit\u00b7ze", ",", "K\u00e4l\u00b7te", "nicht", "ver\u00b7magst", "du", "fest", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "PTKNEG", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was dir bl\u00fcht, sogleich wird es veralten.", "tokens": ["Und", "was", "dir", "bl\u00fcht", ",", "sog\u00b7leich", "wird", "es", "ver\u00b7al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}}}}