{"textgrid.poem.59415": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck auf", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gar viel hab ich versucht, gek\u00e4mpft, ertragen;", "tokens": ["Gar", "viel", "hab", "ich", "ver\u00b7sucht", ",", "ge\u00b7k\u00e4mpft", ",", "er\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist der tiefen Sehnsucht Lebenslauf,", "tokens": ["Das", "ist", "der", "tie\u00b7fen", "Sehn\u00b7sucht", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df br\u00fcnstig sie an jeden Fels mu\u00df schlagen,", "tokens": ["Da\u00df", "br\u00fcns\u00b7tig", "sie", "an", "je\u00b7den", "Fels", "mu\u00df", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob sich des Lichtes Gnadent\u00fcr t\u00e4t auf,", "tokens": ["Ob", "sich", "des", "Lich\u00b7tes", "Gna\u00b7den\u00b7t\u00fcr", "t\u00e4t", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie ein versch\u00fctt'ter Bergmann in den Kl\u00fcften", "tokens": ["Wie", "ein", "ver\u00b7sch\u00fctt'\u00b7ter", "Berg\u00b7mann", "in", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Heraus sich hauet zu den heitern L\u00fcften.", "tokens": ["He\u00b7raus", "sich", "hau\u00b7et", "zu", "den", "hei\u00b7tern", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch ich gelang einst zu dem stillen Gipfel,", "tokens": ["Auch", "ich", "ge\u00b7lang", "einst", "zu", "dem", "stil\u00b7len", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor dem mich schaudert in geheimer Lust.", "tokens": ["Vor", "dem", "mich", "schau\u00b7dert", "in", "ge\u00b7hei\u00b7mer", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tief unten rauschen da des Lebens Wipfel", "tokens": ["Tief", "un\u00b7ten", "rau\u00b7schen", "da", "des", "Le\u00b7bens", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch einmal dunkelr\u00fchrend an die Brust,", "tokens": ["Noch", "ein\u00b7mal", "dun\u00b7kel\u00b7r\u00fch\u00b7rend", "an", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR"