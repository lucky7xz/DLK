{"dta.poem.7352": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Mittwoch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Begleitest du sie gern", "tokens": ["Be\u00b7glei\u00b7test", "du", "sie", "gern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des Pfarrers Lust und Plagen:", "tokens": ["Des", "Pfar\u00b7rers", "Lust", "und", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich gleich an allen Tagen", "tokens": ["Sich", "gleich", "an", "al\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Triffst du den frommen Herrn.", "tokens": ["Triffst", "du", "den", "from\u00b7men", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der gute Seelenhirt!", "tokens": ["Der", "gu\u00b7te", "See\u00b7len\u00b7hirt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tritt \u00fcber seine Schwelle;", "tokens": ["Tritt", "\u00fc\u00b7ber", "sei\u00b7ne", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da ist er schon zur Stelle", "tokens": ["Da", "ist", "er", "schon", "zur", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als des Kollegen Wirth.", "tokens": ["Als", "des", "Kol\u00b7le\u00b7gen", "Wirth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "In wohlgemeinten Sorgen,", "tokens": ["In", "wohl\u00b7ge\u00b7mein\u00b7ten", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie er gesch\u00e4ftig thut!", "tokens": ["Wie", "er", "ge\u00b7sch\u00e4f\u00b7tig", "thut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch d\u00e4mmert kaum der Morgen,", "tokens": ["Doch", "d\u00e4m\u00b7mert", "kaum", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dies eben d\u00fcnkt ihm gut.", "tokens": ["Dies", "e\u00b7ben", "d\u00fcnkt", "ihm", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am Abend kam der Freund", "tokens": ["Am", "A\u00b7bend", "kam", "der", "Freund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ersch\u00f6pft nach Art der G\u00e4ste;", "tokens": ["Er\u00b7sch\u00f6pft", "nach", "Art", "der", "G\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun s\u00e4ubre man auf's Beste,", "tokens": ["Nun", "s\u00e4u\u00b7bre", "man", "auf's", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df alles nett erscheint.", "tokens": ["Da\u00df", "al\u00b7les", "nett", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schon strahlt die gro\u00dfe Kanne,", "tokens": ["Schon", "strahlt", "die", "gro\u00b7\u00dfe", "Kan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Teller blitzen auf;", "tokens": ["Die", "Tel\u00b7ler", "blit\u00b7zen", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch scheuert Jungfer Anne,", "tokens": ["Noch", "scheu\u00b7ert", "Jung\u00b7fer", "An\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und horcht mitunter auf.", "tokens": ["Und", "horcht", "mi\u00b7tun\u00b7ter", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, sollte sie der Gast", "tokens": ["Ach", ",", "soll\u00b7te", "sie", "der", "Gast"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Im alten J\u00e4ckchen finden:", "tokens": ["Im", "al\u00b7ten", "J\u00e4ck\u00b7chen", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie m\u00fc\u00dfte ganz verschwinden", "tokens": ["Sie", "m\u00fc\u00df\u00b7te", "ganz", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor dieser Schande Last.", "tokens": ["Vor", "die\u00b7ser", "Schan\u00b7de", "Last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und was zur Hand thut stehen,", "tokens": ["Und", "was", "zur", "Hand", "thut", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das reizt den Pfarrer sehr,", "tokens": ["Das", "reizt", "den", "Pfar\u00b7rer", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfer wird's nicht sehen,", "tokens": ["Die", "Jung\u00b7fer", "wird's", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er macht sich dr\u00fcber her;", "tokens": ["Er", "macht", "sich", "dr\u00fc\u00b7ber", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Schlaguhr greift er an", "tokens": ["Die", "Schla\u00b7guhr", "greift", "er", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit ungeschickten H\u00e4nden,", "tokens": ["Mit", "un\u00b7ge\u00b7schick\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und sucht sie sacht zu wenden;", "tokens": ["Und", "sucht", "sie", "sacht", "zu", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der \u00fcberm\u00fcth'ge Mann!", "tokens": ["Der", "\u00fc\u00b7ber\u00b7m\u00fcth'\u00b7ge", "Mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schleppt Foliantenb\u00fcrde,", "tokens": ["Schleppt", "Fo\u00b7li\u00b7an\u00b7ten\u00b7b\u00fcr\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Putzt Fensterglas und Tisch;", "tokens": ["Putzt", "Fens\u00b7ter\u00b7glas", "und", "Tisch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcrwahr mit vieler W\u00fcrde", "tokens": ["F\u00fcr\u00b7wahr", "mit", "vie\u00b7ler", "W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchrt er den Flederwisch.", "tokens": ["F\u00fchrt", "er", "den", "Fle\u00b7der\u00b7wisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Am Paradiesesbaum", "tokens": ["Am", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7baum"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Bl\u00e4tter zart aus Knochen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "zart", "aus", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Eins hat er schon zerbrochen,", "tokens": ["Eins", "hat", "er", "schon", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Jedoch man sieht es kaum.", "tokens": ["Je\u00b7doch", "man", "sieht", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als er just in Schatten", "tokens": ["Und", "als", "er", "just", "in", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die alte Klingel stellt \u2014", "tokens": ["Die", "al\u00b7te", "Klin\u00b7gel", "stellt"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es k\u00f6mmt ihm wohl zu statten \u2014", "tokens": ["Es", "k\u00f6mmt", "ihm", "wohl", "zu", "stat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da rauscht es drau\u00dfen, gelt!", "tokens": ["Da", "rauscht", "es", "drau\u00b7\u00dfen", ",", "gelt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fidel schl\u00e4gt an in Hast,", "tokens": ["Fi\u00b7del", "schl\u00e4gt", "an", "in", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "APPR", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die Jungfer ist gefl\u00fcchtet,", "tokens": ["Die", "Jung\u00b7fer", "ist", "ge\u00b7fl\u00fcch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und stattlich aufgerichtet", "tokens": ["Und", "statt\u00b7lich", "auf\u00b7ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Begr\u00fc\u00dft der Pfarr' den Gast.", "tokens": ["Be\u00b7gr\u00fc\u00dft", "der", "Pfarr'", "den", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie dem so wohl gefallen", "tokens": ["Wie", "dem", "so", "wohl", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Aussicht und das Haus,", "tokens": ["Die", "Aus\u00b7sicht", "und", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie der entz\u00fcckt von allen,", "tokens": ["Wie", "der", "ent\u00b7z\u00fcckt", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVFIN", "APPR", "PIAT", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nicht Worte dr\u00fccken's aus!", "tokens": ["Nicht", "Wor\u00b7te", "dr\u00fc\u00b7cken's", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich sag' es ungenirt,", "tokens": ["Ich", "sag'", "es", "un\u00b7ge\u00b7nirt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie kamen aus den Gleisen,", "tokens": ["Sie", "ka\u00b7men", "aus", "den", "Glei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich Ehre zu erweisen,", "tokens": ["Sich", "Eh\u00b7re", "zu", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Gast und auch der Wirth.", "tokens": ["Der", "Gast", "und", "auch", "der", "Wirth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und bei dem Mittagessen,", "tokens": ["Und", "bei", "dem", "Mit\u00b7ta\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das man vortrefflich fand,", "tokens": ["Das", "man", "vor\u00b7treff\u00b7lich", "fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ward auch nicht vergessen", "tokens": ["Da", "ward", "auch", "nicht", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Lehr- und Ehrenstand.", "tokens": ["Der", "Lehr", "und", "Eh\u00b7ren\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich habe viel geh\u00f6rt,", "tokens": ["Ich", "ha\u00b7be", "viel", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch nichts davon getragen,", "tokens": ["Doch", "nichts", "da\u00b7von", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur dieses mag ich sagen,", "tokens": ["Nur", "die\u00b7ses", "mag", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie sprachen sehr gelehrt.", "tokens": ["Sie", "spra\u00b7chen", "sehr", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und sieh nur! dr\u00fcben schreitet", "tokens": ["Und", "sieh", "nur", "!", "dr\u00fc\u00b7ben", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der gute Pfarrer just,", "tokens": ["Der", "gu\u00b7te", "Pfar\u00b7rer", "just", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat den Gast geleitet", "tokens": ["Er", "hat", "den", "Gast", "ge\u00b7lei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und spricht aus voller Brust:", "tokens": ["Und", "spricht", "aus", "vol\u00b7ler", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ees ist doch wahr! mein Haus,", "tokens": ["\u201e", "es", "ist", "doch", "wahr", "!", "mein", "Haus", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So nett und blank da droben,", "tokens": ["So", "nett", "und", "blank", "da", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich mu\u00df es selber loben,", "tokens": ["Ich", "mu\u00df", "es", "sel\u00b7ber", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": 