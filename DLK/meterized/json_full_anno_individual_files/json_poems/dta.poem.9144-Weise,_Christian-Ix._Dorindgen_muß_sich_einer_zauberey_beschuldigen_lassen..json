{"dta.poem.9144": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n Dorindgen mu\u00df sich einer zauberey beschuldigen lassen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.85", "da:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Dorindgen darff ichs sagen/", "tokens": ["Do\u00b7rind\u00b7gen", "darff", "ichs", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und darff ich ohne scheu", "tokens": ["Und", "darff", "ich", "oh\u00b7ne", "scheu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich \u00f6ffentlich verklagen/", "tokens": ["Dich", "\u00f6f\u00b7fent\u00b7lich", "ver\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit deiner zauberey?", "tokens": ["Mit", "dei\u00b7ner", "zau\u00b7be\u00b7rey", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach du verwandelst meinen sinn/", "tokens": ["Ach", "du", "ver\u00b7wan\u00b7delst", "mei\u00b7nen", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ch mir selbst nicht \u00e4hnlich bin.", "tokens": ["Da\u00df", "ch", "mir", "selbst", "nicht", "\u00e4hn\u00b7lich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Sonst ist mein junges hertz/", "tokens": ["Sonst", "ist", "mein", "jun\u00b7ges", "hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bey allen m\u00e4dgen kalt/", "tokens": ["Bey", "al\u00b7len", "m\u00e4d\u00b7gen", "kalt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gibt der liebes-kertz", "tokens": ["Und", "gibt", "der", "lie\u00b7bes\u00b7kertz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gar schlechten auffenthalt/", "tokens": ["Gar", "schlech\u00b7ten", "auf\u00b7fent\u00b7halt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch du verwandelst meinen sinn/", "tokens": ["Doch", "du", "ver\u00b7wan\u00b7delst", "mei\u00b7nen", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich bey dir verliebet bin.", "tokens": ["Da\u00df", "ich", "bey", "dir", "ver\u00b7lie\u00b7bet", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Verlier ich ja bey andern", "tokens": ["Ver\u00b7lier", "ich", "ja", "bey", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Manchmal ein gutes wort/", "tokens": ["Manch\u00b7mal", "ein", "gu\u00b7tes", "wort", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So bin ich nun von Flandern/", "tokens": ["So", "bin", "ich", "nun", "von", "Flan\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und geh bey zeiten fort/", "tokens": ["Und", "geh", "bey", "zei\u00b7ten", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch du verwandelst meinen sinn", "tokens": ["Doch", "du", "ver\u00b7wan\u00b7delst", "mei\u00b7nen", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich bey dir best\u00e4ndig bin.", "tokens": ["Da\u00df", "ich", "bey", "dir", "be\u00b7st\u00e4n\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ich habe meine br\u00fcder", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7ne", "br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn allen schein geliebt/", "tokens": ["Ohn", "al\u00b7len", "schein", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie haben mich auch wieder", "tokens": ["Sie", "ha\u00b7ben", "mich", "auch", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit willen nicht betr\u00fcbt/", "tokens": ["Mit", "wil\u00b7len", "nicht", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch du verwandelst meinen sinn/", "tokens": ["Doch", "du", "ver\u00b7wan\u00b7delst", "mei\u00b7nen", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich den freunden untreu bin.", "tokens": ["Da\u00df", "ich", "den", "freun\u00b7den", "un\u00b7treu", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ich bin zur lust gebohren/", "tokens": ["Ich", "bin", "zur", "lust", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die h\u00e4ngt mir allzeit an/", "tokens": ["Die", "h\u00e4ngt", "mir", "all\u00b7zeit", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gibt mir nicht verlohren", "tokens": ["Und", "gibt", "mir", "nicht", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lang ich lispeln kan:", "tokens": ["So", "lang", "ich", "lis\u00b7peln", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch du verwandelst meinen sinn", "tokens": ["Doch", "du", "ver\u00b7wan\u00b7delst", "mei\u00b7nen", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich bi\u00dfweiten traurig bin.", "tokens": ["Da\u00df", "ich", "bi\u00df\u00b7wei\u00b7ten", "trau\u00b7rig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ich kan an andern orten", "tokens": ["Ich", "kan", "an", "an\u00b7dern", "or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vortreflich lose seyn/", "tokens": ["Vor\u00b7tre\u00b7flich", "lo\u00b7se", "seyn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAINF", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und lasse mich mit worten", "tokens": ["Und", "las\u00b7se", "mich", "mit", "wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In manche kurtzweil ein/", "tokens": ["In", "man\u00b7che", "kurt\u00b7zweil", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch du verwandelst meinen sinn", "tokens": ["Doch", "du", "ver\u00b7wan\u00b7delst", "mei\u00b7nen", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich gar eingezogen bin.", "tokens": ["Da\u00df", "ich", "gar", "ein\u00b7ge\u00b7zo\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Sonst gieng ich bald zu bette", "tokens": ["Sonst", "gieng", "ich", "bald", "zu", "bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann nun der abend kam/", "tokens": ["Wann", "nun", "der", "a\u00b7bend", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alles um die wette", "tokens": ["Und", "al\u00b7les", "um", "die", "wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit sich zur ruhe nahm:", "tokens": ["Mit", "sich", "zur", "ru\u00b7he", "nahm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch du verwandelst meinen sinn", "tokens": ["Doch", "du", "ver\u00b7wan\u00b7delst", "mei\u00b7nen", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich des abends munter bin.", "tokens": ["Da\u00df", "ich", "des", "a\u00b7bends", "mun\u00b7ter", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Wie schlimm hast du gehandelt/", "tokens": ["Wie", "schlimm", "hast", "du", "ge\u00b7han\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich kenne deine list/", "tokens": ["Ich", "ken\u00b7ne", "dei\u00b7ne", "list", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich werde so verwandelt/", "tokens": ["Ich", "wer\u00b7de", "so", "ver\u00b7wan\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bleibest wie du bist:", "tokens": ["Du", "blei\u00b7best", "wie", "du", "bist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach kind/ verwandle deinen sinn/", "tokens": ["Ach", "kind", "/", "ver\u00b7wand\u00b7le", "dei\u00b7nen", "sinn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ich bey dir verwandelt bin.", "tokens": ["Wie", "ich", "bey", "dir", "ver\u00b7wan\u00b7delt", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}