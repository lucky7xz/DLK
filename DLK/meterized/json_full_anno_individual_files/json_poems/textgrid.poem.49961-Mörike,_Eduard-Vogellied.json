{"textgrid.poem.49961": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Vogellied", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist zwar sonsten nicht der Brauch,", "tokens": ["Es", "ist", "zwar", "sons\u00b7ten", "nicht", "der", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man 's Nestchen baut,", "tokens": ["Da\u00df", "man", "'s", "Nest\u00b7chen", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bevor man erst ein Weiblein auch", "tokens": ["Be\u00b7vor", "man", "erst", "ein", "Weib\u00b7lein", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich angetraut:", "tokens": ["Sich", "an\u00b7ge\u00b7traut", ":"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zirri Zirrli!", "tokens": ["Zir\u00b7ri", "Zirr\u00b7li", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.6": {"text": "Erst ein Sch\u00e4tzchen,", "tokens": ["Erst", "ein", "Sch\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dann ein Pl\u00e4tzchen,", "tokens": ["Dann", "ein", "Pl\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Zirri!", "tokens": ["Zir\u00b7ri", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.9": {"text": "Am Birnbaum oder am Haselstrauch.", "tokens": ["Am", "Birn\u00b7baum", "o\u00b7der", "am", "Ha\u00b7sel\u00b7strauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Allein ich dacht, du baust einmal", "tokens": ["Al\u00b7lein", "ich", "dacht", ",", "du", "baust", "ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf gut Gl\u00fcck.", "tokens": ["Auf", "gut", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schaden kann es auf keinen Fall;", "tokens": ["Scha\u00b7den", "kann", "es", "auf", "kei\u00b7nen", "Fall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Zirrwick Zirrliwick!", "tokens": ["Zirr\u00b7wick", "Zirr\u00b7li\u00b7wick", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Gef\u00e4llt's Ihr nicht, meine Jungfer Braut,", "tokens": ["Ge\u00b7f\u00e4llt's", "Ihr", "nicht", ",", "mei\u00b7ne", "Jung\u00b7fer", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iam