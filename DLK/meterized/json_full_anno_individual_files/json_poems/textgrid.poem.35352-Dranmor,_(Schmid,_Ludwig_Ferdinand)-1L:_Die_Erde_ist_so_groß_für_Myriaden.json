{"textgrid.poem.35352": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Erde ist so gro\u00df f\u00fcr Myriaden", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde ist so gro\u00df f\u00fcr Myriaden", "tokens": ["Die", "Er\u00b7de", "ist", "so", "gro\u00df", "f\u00fcr", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Belebter Wesen und f\u00fcr jede Zeit,", "tokens": ["Be\u00b7leb\u00b7ter", "We\u00b7sen", "und", "f\u00fcr", "je\u00b7de", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr jeden Nachwuchs bleibt sie fruchtbeladen", "tokens": ["F\u00fcr", "je\u00b7den", "Nach\u00b7wuchs", "bleibt", "sie", "frucht\u00b7be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wird in ihrer ganzen Herrlichkeit", "tokens": ["Und", "wird", "in", "ih\u00b7rer", "gan\u00b7zen", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verschm\u00e4ht von denen, die nicht sehen wollen,", "tokens": ["Ver\u00b7schm\u00e4ht", "von", "de\u00b7nen", ",", "die", "nicht", "se\u00b7hen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PRELS", "$,", "PRELS", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die furchtsam ihren Horizont verschleiern", "tokens": ["Die", "furcht\u00b7sam", "ih\u00b7ren", "Ho\u00b7ri\u00b7zont", "ver\u00b7schlei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und dann, bei Lampenlicht, in Kinderrollen", "tokens": ["Und", "dann", ",", "bei", "Lam\u00b7pen\u00b7licht", ",", "in", "Kin\u00b7der\u00b7rol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mit Pygm\u00e4en-Stolz Triumphe feiern!", "tokens": ["Und", "mit", "Pyg\u00b7m\u00e4\u00b7en\u00b7Stolz", "Tri\u00b7um\u00b7phe", "fei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Erde ist so gro\u00df f\u00fcr Myriaden", "tokens": ["Die", "Er\u00b7de", "ist", "so", "gro\u00df", "f\u00fcr", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Belebter Wesen und f\u00fcr jede Zeit,", "tokens": ["Be\u00b7leb\u00b7ter", "We\u00b7sen", "und", "f\u00fcr", "je\u00b7de", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr jeden Nachwuchs bleibt sie fruchtbeladen", "tokens": ["F\u00fcr", "je\u00b7den", "Nach\u00b