{"dta.poem.8724": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Fischerknabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00e4nzt den Saum der Fluth,", "tokens": ["Be\u00b7gl\u00e4nzt", "den", "Saum", "der", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Knabe zieht den Kahn herein,", "tokens": ["Der", "Kna\u00b7be", "zieht", "den", "Kahn", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der still im Hafen ruht.", "tokens": ["Der", "still", "im", "Ha\u00b7fen", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201emein Tagewerk ist treu vollbracht,", "tokens": ["\u201e", "mein", "Ta\u00b7ge\u00b7werk", "ist", "treu", "voll\u00b7bracht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch, liebe Seele, sprich,", "tokens": ["Doch", ",", "lie\u00b7be", "See\u00b7le", ",", "sprich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O sprich, wie soll die lange Nacht", "tokens": ["O", "sprich", ",", "wie", "soll", "die", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PWAV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergeh'n mir ohne dich?\u201c", "tokens": ["Ver\u00b7geh'n", "mir", "oh\u00b7ne", "dich", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am Ufer steht ein Weidenbaum,", "tokens": ["Am", "U\u00b7fer", "steht", "ein", "Wei\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dran gelehnt ein Stein,", "tokens": ["Und", "dran", "ge\u00b7lehnt", "ein", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und drunter liegt im schmalen Raum", "tokens": ["Und", "drun\u00b7ter", "liegt", "im", "schma\u00b7len", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr kaltes Todtenbein.", "tokens": ["Ihr", "kal\u00b7tes", "Tod\u00b7ten\u00b7bein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}