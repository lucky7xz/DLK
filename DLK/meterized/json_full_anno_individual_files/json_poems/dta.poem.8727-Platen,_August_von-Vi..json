{"dta.poem.8727": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wo die Seele sonst Entschl\u00fcsse spr\u00fcht,", "tokens": ["Wo", "die", "See\u00b7le", "sonst", "Ent\u00b7schl\u00fcs\u00b7se", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fchl' ich in der W\u00e4rme meines Strebens,", "tokens": ["F\u00fchl'", "ich", "in", "der", "W\u00e4r\u00b7me", "mei\u00b7nes", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie mein Lebenselement vergl\u00fcht.", "tokens": ["Wie", "mein", "Le\u00b7ben\u00b7se\u00b7le\u00b7ment", "ver\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht ein Windsto\u00df, ein belebend warmer,", "tokens": ["Nicht", "ein", "Wind\u00b7sto\u00df", ",", "ein", "be\u00b7le\u00b7bend", "war\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ART", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meine Haare kr\u00e4uselnd, weht mich an,", "tokens": ["Mei\u00b7ne", "Haa\u00b7re", "kr\u00e4u\u00b7selnd", ",", "weht", "mich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Leer und tr\u00e4ge schifft ein Thatenarmer", "tokens": ["Leer", "und", "tr\u00e4\u00b7ge", "schifft", "ein", "Tha\u00b7ten\u00b7ar\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ueber'n stillen Vater Ocean.", "tokens": ["Ue\u00b7ber'n", "stil\u00b7len", "Va\u00b7ter", "O\u00b7cean", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ich soll? Wer l\u00f6s't mir je die Frage?", "tokens": ["Was", "ich", "soll", "?", "Wer", "l\u00f6s't", "mir", "je", "die", "Fra\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$.", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was ich kann? Wer g\u00f6nnt mir den Versuch?", "tokens": ["Was", "ich", "kann", "?", "Wer", "g\u00f6nnt", "mir", "den", "Ver\u00b7such", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$.", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was ich mu\u00df? Vermag ich's ohne Klage?", "tokens": ["Was", "ich", "mu\u00df", "?", "Ver\u00b7mag", "ich's", "oh\u00b7ne", "Kla\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$.", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "So viel Arbeit um ein Leichentuch?", "tokens": ["So", "viel", "Ar\u00b7beit", "um", "ein", "Lei\u00b7chen\u00b7tuch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Kommt und lispelt Muth in's Herz mir, zarte", "tokens": ["Kommt", "und", "lis\u00b7pelt", "Muth", "in's", "Herz", "mir", ",", "zar\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "KON", "VVFIN", "NN", "APPRART", "NN", "PPER", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liederstimmen, die ihr lange schlieft,", "tokens": ["Lie\u00b7ders\u00b7tim\u00b7men", ",", "die", "ihr", "lan\u00b7ge", "schlieft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich, wie ein Tr\u00e4umer, nicht entarte,", "tokens": ["Da\u00df", "ich", ",", "wie", "ein", "Tr\u00e4u\u00b7mer", ",", "nicht", "ent\u00b7ar\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In verlorne Neigungen vertieft.", "tokens": ["In", "ver\u00b7lor\u00b7ne", "Nei\u00b7gun\u00b7gen", "ver\u00b7tieft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}