{"textgrid.poem.66554": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jahrestag du meiner Wonnen,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahrestag du meiner Wonnen,", "tokens": ["Jah\u00b7res\u00b7tag", "du", "mei\u00b7ner", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich welken Blumen, lang gehegten,", "tokens": ["Gleich", "wel\u00b7ken", "Blu\u00b7men", ",", "lang", "ge\u00b7heg\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Giebst du dahin, besch\u00e4mt und klar,", "tokens": ["Giebst", "du", "da\u00b7hin", ",", "be\u00b7sch\u00e4mt", "und", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Wahn und Irrthum dich bewegten?", "tokens": ["Da\u00df", "Wahn", "und", "Irr\u00b7thum", "dich", "be\u00b7weg\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, da\u00df du dich nicht selbst beraubst!", "tokens": ["Sieh", ",", "da\u00df", "du", "dich", "nicht", "selbst", "be\u00b7raubst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was heut verj\u00e4hrt dir und veraltet,", "tokens": ["Was", "heut", "ver\u00b7j\u00e4hrt", "dir", "und", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat in dir mehr als du glaubst", "tokens": ["Es", "hat", "in", "dir", "mehr", "als", "du", "glaubst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geweckt, gewandelt und gestaltet.", "tokens": ["Ge\u00b7weckt", ",", "ge\u00b7wan\u00b7delt", "und", "ge\u00b7stal\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jahrestag du meiner Wonnen,", "tokens": ["Jah\u00b7res\u00b7tag", "du", "mei\u00b7ner", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich welken Blumen, lang gehegten,", "tokens": ["Gleich", "wel\u00b7ken", "Blu\u00b7men", ",", "lang", "ge\u00b7heg\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Giebst du dahin, besch\u00e4mt und klar,", "tokens": ["Giebst", "du", "da\u00b7hin", ",", "be\u00b7sch\u00e4mt", "und", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text":