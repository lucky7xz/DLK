{"textgrid.poem.34350": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komme nicht dir vorzuklagen,", "tokens": ["Ich", "kom\u00b7me", "nicht", "dir", "vor\u00b7zu\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin zu gl\u00fccklich durch dein Wohl", "tokens": ["Ich", "bin", "zu", "gl\u00fcck\u00b7lich", "durch", "dein", "Wohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als da\u00df dirs Seufzer kosten soll;", "tokens": ["Als", "da\u00df", "dirs", "Seuf\u00b7zer", "kos\u00b7ten", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich komme dir Valet zu sagen.", "tokens": ["Ich", "kom\u00b7me", "dir", "Va\u00b7let", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ein fremder Himmel wartet mein", "tokens": ["Ein", "frem\u00b7der", "Him\u00b7mel", "war\u00b7tet", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du wirst immer gl\u00fccklich seyn.", "tokens": ["Und", "du", "wirst", "im\u00b7mer", "gl\u00fcck\u0