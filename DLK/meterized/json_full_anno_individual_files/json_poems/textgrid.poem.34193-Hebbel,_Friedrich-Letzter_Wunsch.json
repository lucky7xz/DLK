{"textgrid.poem.34193": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mancherlei W\u00fcnsche hatt' ich und mancherlei hab' ich f\u00fcr's Leben,", "tokens": ["Man\u00b7cher\u00b7lei", "W\u00fcn\u00b7sche", "hatt'", "ich", "und", "man\u00b7cher\u00b7lei", "hab'", "ich", "f\u00fcr's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "KON", "PIS", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Einen einzigen nur spar' ich mir auf f\u00fcr den Tod:", "tokens": ["Ei\u00b7nen", "ein\u00b7zi\u00b7gen", "nur", "spa\u00b7r'", "ich", "mir", "auf", "f\u00fcr", "den", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PPER", "PRF", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Da\u00df sich in Flammen mein Geist entbinden m\u00f6ge, noch gl\u00fchend", "tokens": ["Da\u00df", "sich", "in", "Flam\u00b7men", "mein", "Geist", "ent\u00b7bin\u00b7den", "m\u00f6\u00b7ge", ",", "noch", "gl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "NN", "PPOSAT", "NN", "VVINF", "VMFIN", "$,", "ADV", "ADJD"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von dem letzten Gedicht, da\u00df sich in Flammen mein Leib", "tokens": ["Von", "dem", "letz\u00b7ten", "Ge\u00b7dicht", ",", "da\u00df", "sich", "in", "Flam\u00b7men", "mein", "Leib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PRF", "APPR", "NN", "PPOSAT", "NN"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wandeln d\u00fcrfe in Asche, bevor noch v\u00f6llig das Antlitz", "tokens": ["Wan\u00b7deln", "d\u00fcr\u00b7fe", "in", "A\u00b7sche", ",", "be\u00b7vor", "noch", "v\u00f6l\u00b7lig", "das", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "NN", "$,", "KOUS", "ADV", "ADJD", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Sich zur Larve verstellt, das der Geliebten gefiel!", "tokens": ["Sich", "zur", "Lar\u00b7ve", "ver\u00b7stellt", ",", "das", "der", "Ge\u00b7lieb\u00b7ten", "ge\u00b7fiel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Jenes geb' ich den G\u00f6ttern anheim und dieses den Freunden,", "tokens": ["Je\u00b7nes", "geb'", "ich", "den", "G\u00f6t\u00b7tern", "an\u00b7heim", "und", "die\u00b7ses", "den", "Freun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "NE", "KON", "PDS", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Die es wissen, wie sehr stets vor Gew\u00fcrm ich gebebt;", "tokens": ["Die", "es", "wis\u00b7sen", ",", "wie", "sehr", "stets", "vor", "Ge\u00b7w\u00fcrm", "ich", "ge\u00b7bebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "PWAV", "ADV", "ADV", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "M\u00f6gen sie still mir den Holzsto\u00df errichten und rasch ihn entz\u00fcnden,", "tokens": ["M\u00f6\u00b7gen", "sie", "still", "mir", "den", "Holz\u00b7sto\u00df", "er\u00b7rich\u00b7ten", "und", "rasch", "ihn", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "ART", "NN", "VVINF", "KON", "ADJD", "PPER", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Ein gef\u00e4lliger Wind bl\u00e4set wohl freundlich hinein!", "tokens": ["Ein", "ge\u00b7f\u00e4l\u00b7li\u00b7ger", "Wind", "bl\u00e4\u00b7set", "wohl", "freund\u00b7lich", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mancherlei W\u00fcnsche hatt' ich und mancherlei hab' ich f\u00fcr's Leben,", "tokens": ["Man\u00b7cher\u00b7lei", "W\u00fcn\u00b7sche", "hatt'", "ich", "und", "man\u00b7cher\u00b7lei", "hab'", "ich", "f\u00fcr's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "KON", "PIS", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Einen einzigen nur spar' ich mir auf f\u00fcr den Tod:", "tokens": ["Ei\u00b7nen", "ein\u00b7zi\u00b7gen", "nur", "spa\u00b7r'", "ich", "mir", "auf", "f\u00fcr", "den", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PPER", "PRF", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--