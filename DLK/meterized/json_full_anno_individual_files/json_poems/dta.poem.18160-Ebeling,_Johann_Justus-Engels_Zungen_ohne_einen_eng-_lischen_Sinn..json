{"dta.poem.18160": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Engels Zungen ohne einen eng-  \n lischen Sinn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich nur mit Engels-Zungen, ohne", "tokens": ["Wenn", "ich", "nur", "mit", "En\u00b7gels\u00b7Zun\u00b7gen", ",", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "$,", "KOUI"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "einen Engels Sinn,", "tokens": ["ei\u00b7nen", "En\u00b7gels", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Als ein Lehrer reich von Gaben ohne", "tokens": ["Als", "ein", "Leh\u00b7rer", "reich", "von", "Ga\u00b7ben", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "APPR", "NN", "APPR"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "alle Liebe bin;", "tokens": ["al\u00b7le", "Lie\u00b7be", "bin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "So bin ich denselben gleich, die wie die bewegten", "tokens": ["So", "bin", "ich", "den\u00b7sel\u00b7ben", "gleich", ",", "die", "wie", "die", "be\u00b7weg\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "ADV", "$,", "PRELS", "KOKOM", "ART", "ADJA"], "meter": "-+--+----+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Glokken", "tokens": ["Glok\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Andre in das Heiligthum zu des H\u00f6chsten Dienste", "tokens": ["And\u00b7re", "in", "das", "Hei\u00b7lig\u00b7thum", "zu", "des", "H\u00f6chs\u00b7ten", "Diens\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "lokken,", "tokens": ["lok\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Aber selbsten nicht hingehen: O! das h\u00fclfe mir ja", "tokens": ["A\u00b7ber", "selbs\u00b7ten", "nicht", "hin\u00b7ge\u00b7hen", ":", "O", "!", "das", "h\u00fcl\u00b7fe", "mir", "ja"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "$.", "NE", "$.", "PDS", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.10": {"text": "nicht", "tokens": ["nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Herr! wenn ich dereinsten komme vor dein helles", "tokens": ["Herr", "!", "wenn", "ich", "de\u00b7reins\u00b7ten", "kom\u00b7me", "vor", "dein", "hel\u00b7les"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPER", "PDS", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Angesicht", "tokens": ["An\u00b7ge\u00b7sicht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Rufst du mich nach Zion hin, ach! so gieb da\u00df ich", "tokens": ["Rufst", "du", "mich", "nach", "Zi\u00b7on", "hin", ",", "ach", "!", "so", "gieb", "da\u00df", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "PTKVZ", "$,", "ITJ", "$.", "ADV", "VVIMP", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "nicht stehe,", "tokens": ["nicht", "ste\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Als ein Weiser der nur zeigt, sondern auch den Weg", "tokens": ["Als", "ein", "Wei\u00b7ser", "der", "nur", "zeigt", ",", "son\u00b7dern", "auch", "den", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADV", "VVFIN", "$,", "KON", "ADV", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "selbst gehe.", "tokens": ["selbst", "ge\u00b7he", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "F\u00fclle mich mit wahrer Liebe gegen meinen N\u00e4chsten", "tokens": ["F\u00fcl\u00b7le", "mich", "mit", "wah\u00b7rer", "Lie\u00b7be", "ge\u00b7gen", "mei\u00b7nen", "N\u00e4chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "an,", "tokens": ["an", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Und wenn ich denn dieses thue, was ich schuldig", "tokens": ["Und", "wenn", "ich", "denn", "die\u00b7ses", "thue", ",", "was", "ich", "schul\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PDS", "VVFIN", "$,", "PWS", "PPER", "ADJD"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "bin und kann.", "tokens": ["bin", "und", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "So gieb mir den reinen Sinn, da\u00df ich nimmer mit", "tokens": ["So", "gieb", "mir", "den", "rei\u00b7nen", "Sinn", ",", "da\u00df", "ich", "nim\u00b7mer", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "APPR"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "dir rechte,", "tokens": ["dir", "rech\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Sondern denk: ", "tokens": ["Son\u00b7dern", "denk", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "n\u00fctzen Knechte.", "tokens": ["n\u00fct\u00b7zen", "Knech\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}