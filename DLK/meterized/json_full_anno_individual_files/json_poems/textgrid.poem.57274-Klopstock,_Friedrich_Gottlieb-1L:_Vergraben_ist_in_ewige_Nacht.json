{"textgrid.poem.57274": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergraben ist in ewige Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergraben ist in ewige Nacht", "tokens": ["Ver\u00b7gra\u00b7ben", "ist", "in", "e\u00b7wi\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Erfinder grosser Name zu oft!", "tokens": ["Der", "Er\u00b7fin\u00b7der", "gros\u00b7ser", "Na\u00b7me", "zu", "oft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Was ihr Geist gr\u00fcbelnd entdeckt, nutzen wir;", "tokens": ["Was", "ihr", "Geist", "gr\u00fc\u00b7belnd", "ent\u00b7deckt", ",", "nut\u00b7zen", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VVPP", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Aber belohnt Ehre sie auch?", "tokens": ["A\u00b7ber", "be\u00b7lohnt", "Eh\u00b7re", "sie", "auch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "NN", "PPER", "ADV", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wer nannte dir den k\u00fchneren Mann,", "tokens": ["Wer", "nann\u00b7te", "dir", "den", "k\u00fch\u00b7ne\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der zuerst am Maste Segel erhob?", "tokens": ["Der", "zu\u00b7erst", "am", "Mas\u00b7te", "Se\u00b7gel", "er\u00b7hob", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ach verging selber der Ruhm dessen nicht,", "tokens": ["Ach", "ver\u00b7ging", "sel\u00b7ber", "der", "Ruhm", "des\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "NN", "PDS", "PTKNEG", "$,"], "meter": "+-+---++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Welcher dem Fuss Fl\u00fcgel erfand!", "tokens": ["Wel\u00b7cher", "dem", "Fuss", "Fl\u00fc\u00b7gel", "er\u00b7fand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und sollte der unsterblich nicht seyn,", "tokens": ["Und", "soll\u00b7te", "der", "uns\u00b7terb\u00b7lich", "nicht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJD", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Gesundheit uns und Freuden erfand,", "tokens": ["Der", "Ge\u00b7sund\u00b7heit", "uns", "und", "Freu\u00b7den", "er\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die das Ross muthig im Lauf niemals gab,", "tokens": ["Die", "das", "Ross", "mut\u00b7hig", "im", "Lauf", "nie\u00b7mals", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Welche der Reihn selber nicht hat?", "tokens": ["Wel\u00b7che", "der", "Reihn", "sel\u00b7ber", "nicht", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Unsterblich ist mein Name dereinst!", "tokens": ["U\u00b7nsterb\u00b7lich", "ist", "mein", "Na\u00b7me", "de\u00b7reinst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+--++-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ich erfinde noch dem schl\u00fcpfenden Stahl", "tokens": ["Ich", "er\u00b7fin\u00b7de", "noch", "dem", "schl\u00fcp\u00b7fen\u00b7den", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Seinen Tanz! Leichteres Schwungs fliegt er hin,", "tokens": ["Sei\u00b7nen", "Tanz", "!", "Leich\u00b7te\u00b7res", "Schwungs", "fliegt", "er", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Kreiset umher, sch\u00f6ner zu sehn.", "tokens": ["Krei\u00b7set", "um\u00b7her", ",", "sch\u00f6\u00b7ner", "zu", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Du kennest jeden reizenden Ton", "tokens": ["Du", "ken\u00b7nest", "je\u00b7den", "rei\u00b7zen\u00b7den", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Musik, drum gieb dem Tanz Melodie!", "tokens": ["Der", "Mu\u00b7sik", ",", "drum", "gieb", "dem", "Tanz", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mond, und Wald h\u00f6re den Schall ihres Horns,", "tokens": ["Mond", ",", "und", "Wald", "h\u00f6\u00b7re", "den", "Schall", "ih\u00b7res", "Horns", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wenn sie des Flugs Eile gebeut,", "tokens": ["Wenn", "sie", "des", "Flugs", "Ei\u00b7le", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "O J\u00fcngling, der den Wasserkothurn", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "der", "den", "Was\u00b7ser\u00b7ko\u00b7thurn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu beseelen weiss, und fl\u00fcchtiger tanzt,", "tokens": ["Zu", "be\u00b7see\u00b7len", "weiss", ",", "und", "fl\u00fcch\u00b7ti\u00b7ger", "tanzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$,", "KON", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Lass der Stadt ihren Kamin! Kom mit mir,", "tokens": ["Lass", "der", "Stadt", "ih\u00b7ren", "Ka\u00b7min", "!", "Kom", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "$.", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wo des Krystalls Ebne dir winkt!", "tokens": ["Wo", "des", "Krys\u00b7talls", "Eb\u00b7ne", "dir", "winkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Sein Licht hat er in D\u00fcfte geh\u00fcllt,", "tokens": ["Sein", "Licht", "hat", "er", "in", "D\u00fcf\u00b7te", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wie erhellt des Winters werdender Tag", "tokens": ["Wie", "er\u00b7hellt", "des", "Win\u00b7ters", "wer\u00b7den\u00b7der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sanft den See! Gl\u00e4nzenden Reif, Sternen gleich,", "tokens": ["Sanft", "den", "See", "!", "Gl\u00e4n\u00b7zen\u00b7den", "Reif", ",", "Ster\u00b7nen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "NN", "$,", "NN", "ADV", "$,"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Streute die Nacht \u00fcber ihn aus!", "tokens": ["Streu\u00b7te", "die", "Nacht", "\u00fc\u00b7ber", "ihn", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Wie schweigt um uns das weisse Gefild!", "tokens": ["Wie", "schweigt", "um", "uns", "das", "weis\u00b7se", "Ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie ert\u00f6nt vom jungen Froste die Bahn!", "tokens": ["Wie", "er\u00b7t\u00f6nt", "vom", "jun\u00b7gen", "Fros\u00b7te", "die", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Fern verr\u00e4th deines Kothurns Schall dich mir,", "tokens": ["Fern", "ver\u00b7r\u00e4\u00b7th", "dei\u00b7nes", "Ko\u00b7thurns", "Schall", "dich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "PPER", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wenn du dem Blick, Fl\u00fcchtling, enteilst.", "tokens": ["Wenn", "du", "dem", "Blick", ",", "Fl\u00fccht\u00b7ling", ",", "ent\u00b7eilst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Wir haben doch zum Schmause genung", "tokens": ["Wir", "ha\u00b7ben", "doch", "zum", "Schmau\u00b7se", "ge\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von des Hahnes Frucht? und Freuden des Weins?", "tokens": ["Von", "des", "Hah\u00b7nes", "Frucht", "?", "und", "Freu\u00b7den", "des", "Weins", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "KON", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Winterluft reizt die Begier nach dem Mahl;", "tokens": ["Win\u00b7ter\u00b7luft", "reizt", "die", "Be\u00b7gier", "nach", "dem", "Mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Fl\u00fcgel am Fuss reizen sie mehr!", "tokens": ["Fl\u00fc\u00b7gel", "am", "Fuss", "rei\u00b7zen", "sie", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Zur Linken wende du dich, ich will", "tokens": ["Zur", "Lin\u00b7ken", "wen\u00b7de", "du", "dich", ",", "ich", "will"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "$,", "PPER", "VMFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu der Rechten hin halbkreisend mich drehn;", "tokens": ["Zu", "der", "Rech\u00b7ten", "hin", "halb\u00b7krei\u00b7send", "mich", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nim den Schwung, wie du mich ihn nehmen siehst:", "tokens": ["Nim", "den", "Schwung", ",", "wie", "du", "mich", "ihn", "neh\u00b7men", "siehst", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Also! nun fleug schnell mir vorbey!", "tokens": ["Al\u00b7so", "!", "nun", "fleug", "schnell", "mir", "vor\u00b7bey", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "So gehen wir den schl\u00e4ngelnden Gang", "tokens": ["So", "ge\u00b7hen", "wir", "den", "schl\u00e4n\u00b7geln\u00b7den", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "An dem langen Ufer schwebend hinab.", "tokens": ["An", "dem", "lan\u00b7gen", "U\u00b7fer", "schwe\u00b7bend", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "K\u00fcnstle nicht! Stellung, wie die, lieb' ich nicht,", "tokens": ["K\u00fcnst\u00b7le", "nicht", "!", "Stel\u00b7lung", ",", "wie", "die", ",", "lieb'", "ich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "NN", "$,", "PWAV", "ART", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zeichnet dir auch Preisler nicht nach.", "tokens": ["Zeich\u00b7net", "dir", "auch", "Preis\u00b7ler", "nicht", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "----+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Was horchst du nach der Insel hinauf?", "tokens": ["Was", "horchst", "du", "nach", "der", "In\u00b7sel", "hin\u00b7auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unerfahrne L\u00e4ufer t\u00f6nen dort her!", "tokens": ["Un\u00b7er\u00b7fahr\u00b7ne", "L\u00e4u\u00b7fer", "t\u00f6\u00b7nen", "dort", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Huf und Last gingen noch nicht \u00fcbers Eis,", "tokens": ["Huf", "und", "Last", "gin\u00b7gen", "noch", "nicht", "\u00fc\u00b7bers", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Netze noch nicht unter ihm fort.", "tokens": ["Net\u00b7ze", "noch", "nicht", "un\u00b7ter", "ihm", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "Sonst sp\u00e4ht dein Ohr ja alles; vernim,", "tokens": ["Sonst", "sp\u00e4ht", "dein", "Ohr", "ja", "al\u00b7les", ";", "ver\u00b7nim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PIS", "$.", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie der Todeston wehklagt auf der Flut!", "tokens": ["Wie", "der", "T