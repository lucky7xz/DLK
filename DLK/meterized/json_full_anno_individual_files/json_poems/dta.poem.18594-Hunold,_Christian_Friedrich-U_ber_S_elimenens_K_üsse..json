{"dta.poem.18594": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber  S elimenens  K \u00fcsse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Nun wohl zu guter Nacht/ zur guter Nacht/ ihr Sch\u00f6nen!", "tokens": ["Nun", "wohl", "zu", "gu\u00b7ter", "Nacht", "/", "zur", "gu\u00b7ter", "Nacht", "/", "ihr", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$(", "APPRART", "ADJA", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn mein sonst treuer Sinn nun eure Pracht vergist/", "tokens": ["Wenn", "mein", "sonst", "treu\u00b7er", "Sinn", "nun", "eu\u00b7re", "Pracht", "ver\u00b7gist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich die Lippen nicht nach euren K\u00fcssen sehnen/", "tokens": ["Und", "sich", "die", "Lip\u00b7pen", "nicht", "nach", "eu\u00b7ren", "K\u00fcs\u00b7sen", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So dencket/ da\u00df mich itzt die andre Venus k\u00fc\u00dft.", "tokens": ["So", "den\u00b7cket", "/", "da\u00df", "mich", "itzt", "die", "and\u00b7re", "Ve\u00b7nus", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}