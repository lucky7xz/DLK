{"dta.poem.19577": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Gesang der Parze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In der Wiege schlummert ein sch\u00f6nes R\u00f6merkind,", "tokens": ["In", "der", "Wie\u00b7ge", "schlum\u00b7mert", "ein", "sch\u00f6\u00b7nes", "R\u00f6\u00b7mer\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Parze sitzt daneben und spinnt und spinnt.", "tokens": ["Die", "Par\u00b7ze", "sitzt", "da\u00b7ne\u00b7ben", "und", "spinnt", "und", "spinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sonst schweigt sie streng. Ist die lauschende Mutter fort,", "tokens": ["Sonst", "schweigt", "sie", "streng", ".", "Ist", "die", "lau\u00b7schen\u00b7de", "Mut\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$.", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So singt die Parze murmelnd ein dunkles Wort:", "tokens": ["So", "singt", "die", "Par\u00b7ze", "mur\u00b7melnd", "ein", "dunk\u00b7les", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Jetzt liegst du, Kindlein, noch in der Traumesruh.", "tokens": ["Jetzt", "liegst", "du", ",", "Kin\u00b7dlein", ",", "noch", "in", "der", "Trau\u00b7mes\u00b7ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bald, kleine Claudia, spinnest am Rocken du \u2014", "tokens": ["Bald", ",", "klei\u00b7ne", "Clau\u00b7dia", ",", "spin\u00b7nest", "am", "Ro\u00b7cken", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NE", "$,", "VVFIN", "APPRART", "NN", "PPER", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du wachsest rasch und entw\u00e4chst den Kleidlein bald!", "tokens": ["Du", "wach\u00b7sest", "rasch", "und", "ent\u00b7w\u00e4chst", "den", "Kleid\u00b7lein", "bald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du wachsest schlank! Du wirst eine Wohlgestalt!", "tokens": ["Du", "wach\u00b7sest", "schlank", "!", "Du", "wirst", "ei\u00b7ne", "Wohl\u00b7ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Fackel lodert und wirft einen grellen Schein,", "tokens": ["Die", "Fa\u00b7ckel", "lo\u00b7dert", "und", "wirft", "ei\u00b7nen", "grel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie kleiden dich mit dem Hochzeitsschleier ein!", "tokens": ["Sie", "klei\u00b7den", "dich", "mit", "dem", "Hoch\u00b7zeits\u00b7schlei\u00b7er", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Knaben h\u00fcpfen empor am Festgelag", "tokens": ["Die", "Kna\u00b7ben", "h\u00fcp\u00b7fen", "em\u00b7por", "am", "Fest\u00b7ge\u00b7lag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und scherzen ausgelassen zum ernsten Tag.", "tokens": ["Und", "scher\u00b7zen", "aus\u00b7ge\u00b7las\u00b7sen", "zum", "erns\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Eine Herrin wandelt in ihrem eignen Raum,", "tokens": ["Ei\u00b7ne", "Her\u00b7rin", "wan\u00b7delt", "in", "ih\u00b7rem", "eig\u00b7nen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und ihre M\u00e4gd' und die Sklaven athmen kaum.", "tokens": ["Und", "ih\u00b7re", "M\u00e4gd'", "und", "die", "Skla\u00b7ven", "ath\u00b7men", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr ziemt da\u00df all die H\u00e4nde gefl\u00fcgelt sind.", "tokens": ["Ihr", "ziemt", "da\u00df", "all", "die", "H\u00e4n\u00b7de", "ge\u00b7fl\u00fc\u00b7gelt", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PIAT", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr ziemt da\u00df all die Lippen gez\u00fcgelt sind.", "tokens": ["Ihr", "ziemt", "da\u00df", "all", "die", "Lip\u00b7pen", "ge\u00b7z\u00fc\u00b7gelt", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PIAT", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die bl\u00fchenden Horen schwingen im Reigen sich:", "tokens": ["Die", "bl\u00fc\u00b7hen\u00b7den", "Ho\u00b7ren", "schwin\u00b7gen", "im", "Rei\u00b7gen", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PRF", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir ward ein Knabe, Julier, freue dich!", "tokens": ["Dir", "ward", "ein", "Kna\u00b7be", ",", "Ju\u00b7lier", ",", "freu\u00b7e", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wann die Freude schwebt und die Fl\u00f6te schallt,", "tokens": ["Doch", "wann", "die", "Freu\u00b7de", "schwebt", "und", "die", "Fl\u00f6\u00b7te", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dann \u2014 sagt die Parze \u2014 kommt der Jammer bald.", "tokens": ["Dann", "sagt", "die", "Par\u00b7ze", "kommt", "der", "Jam\u00b7mer", "bald", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "ART", "NN", "$(", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Der Tiber flutet und \u00fcberschwemmt den Strand,", "tokens": ["Der", "Ti\u00b7ber", "flu\u00b7tet", "und", "\u00fc\u00b7bersc\u00b7hwemmt", "den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das bleiche Fieber steigt empor ans Land,", "tokens": ["Das", "blei\u00b7che", "Fie\u00b7ber", "steigt", "em\u00b7por", "ans", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Rufer ruft's und k\u00fcndet's von Haus zu Haus:", "tokens": ["Der", "Ru\u00b7fer", "ruft's", "und", "k\u00fcn\u00b7det's", "von", "Haus", "zu", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u201evernehmt! Den Julier tragen sie heut hinaus!\u201c", "tokens": ["\u201e", "ver\u00b7nehmt", "!", "Den", "Ju\u00b7lier", "tra\u00b7gen", "sie", "heut", "hin\u00b7aus", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Jetzt, kleine Claudia, tr\u00e4gst du untr\u00e4glich Leid!", "tokens": ["Jetzt", ",", "klei\u00b7ne", "Clau\u00b7dia", ",", "tr\u00e4gst", "du", "un\u00b7tr\u00e4g\u00b7lich", "Leid", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NE", "$,", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In strenge Falten legst du dein Wittwenkleid \u2014", "tokens": ["In", "stren\u00b7ge", "Fal\u00b7ten", "legst", "du", "dein", "Witt\u00b7wen\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Dein R\u00f6merknabe springt dir behend vom Schoo\u00df", "tokens": ["Dein", "R\u00f6\u00b7mer\u00b7kna\u00b7be", "springt", "dir", "be\u00b7hend", "vom", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und gr\u00fc\u00dft dich helmumflattert herab vom Ro\u00df ...", "tokens": ["Und", "gr\u00fc\u00dft", "dich", "hel\u00b7mum\u00b7flat\u00b7tert", "her\u00b7ab", "vom", "Ro\u00df", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Tuben rufen Schlacht und sie rufen Sieg ...", "tokens": ["Die", "Tu\u00b7ben", "ru\u00b7fen", "Schlacht", "und", "sie", "ru\u00b7fen", "Sieg", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da naht's. Da kommt's, was empor die Stufen stieg:", "tokens": ["Da", "naht'", "s.", "Da", "kommt's", ",", "was", "em\u00b7por", "die", "Stu\u00b7fen", "stieg", ":"], "token_info": ["word", "word", "abbreviation", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVIMP", "ADV", "VVFIN", "$,", "PWS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vier M\u00e4nner und die Bahre, Claudia, sind's", "tokens": ["Vier", "M\u00e4n\u00b7ner", "und", "die", "Bah\u00b7re", ",", "Clau\u00b7dia", ",", "sin\u00b7d's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["CARD", "NN", "KON", "ART", "NN", "$,", "NE", "$,", "NE"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mit der bekr\u00e4nzten Leiche deines Kinds!", "tokens": ["Mit", "der", "be\u00b7kr\u00e4nz\u00b7ten", "Lei\u00b7che", "dei\u00b7nes", "Kinds", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Jetzt, kleine Claudia, bist du zu Tode wund\u201c \u2014", "tokens": ["Jetzt", ",", "klei\u00b7ne", "Clau\u00b7dia", ",", "bist", "du", "zu", "To\u00b7de", "wund", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADJA", "NE", "$,", "VAFIN", "PPER", "APPR", "NN", "ADJD", "$(", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Kindlein l\u00e4chelt. Es klirrt ein Schl\u00fcsselbund.", "tokens": ["Das", "Kin\u00b7dlein", "l\u00e4\u00b7chelt", ".", "Es", "klirrt", "ein", "Schl\u00fcs\u00b7sel\u00b7bund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Mutter tritt besorgt in die Kammer ein", "tokens": ["Die", "Mut\u00b7ter", "tritt", "be\u00b7sorgt", "in", "die", "Kam\u00b7mer", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und die Parze bleicht im goldenen Morgenschein.", "tokens": ["Und", "die", "Par\u00b7ze", "bleicht", "im", "gol\u00b7de\u00b7nen", "Mor\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}