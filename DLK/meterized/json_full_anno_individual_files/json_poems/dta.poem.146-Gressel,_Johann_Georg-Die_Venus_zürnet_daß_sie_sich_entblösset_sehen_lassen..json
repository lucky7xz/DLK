{"dta.poem.146": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die  Venus  z\u00fcrnet/ da\u00df sie sich entbl\u00f6sset  \n sehen lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wirfft man so/ loses Kind/ der Br\u00fcste Rosen hin?", "tokens": ["Wirfft", "man", "so", "/", "lo\u00b7ses", "Kind", "/", "der", "Br\u00fcs\u00b7te", "Ro\u00b7sen", "hin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$(", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verachtest du mein Blut/ das deine Pracht bedecket?", "tokens": ["Ver\u00b7ach\u00b7test", "du", "mein", "Blut", "/", "das", "dei\u00b7ne", "Pracht", "be\u00b7de\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Perlen sind umsonst daselbst nicht ausgehecket/", "tokens": ["Die", "Per\u00b7len", "sind", "um\u00b7sonst", "da\u00b7selbst", "nicht", "aus\u00b7ge\u00b7he\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergebens ziert dich nicht Zinnober und Rubin.", "tokens": ["Ver\u00b7ge\u00b7bens", "ziert", "dich", "nicht", "Zin\u00b7no\u00b7ber", "und", "Ru\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Du l\u00e4ssest dich entbl\u00f6\u00dft gleich als ein Schau-Spiel sehn/", "tokens": ["Du", "l\u00e4s\u00b7sest", "dich", "ent\u00b7bl\u00f6\u00dft", "gleich", "als", "ein", "Schau\u00b7Spiel", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die zarten Liljen auf den holden Rosen-B\u00fchnen/", "tokens": ["Die", "zar\u00b7ten", "Lil\u00b7jen", "auf", "den", "hol\u00b7den", "Ro\u00b7sen\u00b7B\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die m\u00fcssen jedermann zur Augen-Weide dienen/", "tokens": ["Die", "m\u00fcs\u00b7sen", "je\u00b7der\u00b7mann", "zur", "Au\u00b7gen\u00b7Wei\u00b7de", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Damit man sehen mag wie sie so sch\u00f6ne stehn.", "tokens": ["Da\u00b7mit", "man", "se\u00b7hen", "mag", "wie", "sie", "so", "sch\u00f6\u00b7ne", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "KOKOM", "PPER", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein! bedencke doch/ wie du den Leib entweyht/", "tokens": ["Al\u00b7lein", "!", "be\u00b7den\u00b7cke", "doch", "/", "wie", "du", "den", "Leib", "ent\u00b7weyht", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ADV", "$(", "PWAV", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An dem Vollkommenheit ein Meister-St\u00fcck gemachet/", "tokens": ["An", "dem", "Voll\u00b7kom\u00b7men\u00b7heit", "ein", "Meis\u00b7ter\u00b7St\u00fcck", "ge\u00b7ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Sch\u00f6nheit/ die bey mir und meinen Br\u00fcsten wachet/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "/", "die", "bey", "mir", "und", "mei\u00b7nen", "Br\u00fcs\u00b7ten", "wa\u00b7chet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Hat ihren Schatz bey dir verschwendrisch ausgestreut.", "tokens": ["Hat", "ih\u00b7ren", "Schatz", "bey", "dir", "ver\u00b7schwend\u00b7risch", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Br\u00fcste Perlen-Milch verkehrt sich fast in Blut/", "tokens": ["Der", "Br\u00fcs\u00b7te", "Per\u00b7len\u00b7Milch", "ver\u00b7kehrt", "sich", "fast", "in", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie sind mit Schaam und Furcht deswegen angef\u00fcllet/", "tokens": ["Sie", "sind", "mit", "Schaam", "und", "Furcht", "des\u00b7we\u00b7gen", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kein Seuffzer steigt hervor draus nicht ein neuer quillet/", "tokens": ["Kein", "Seuff\u00b7zer", "steigt", "her\u00b7vor", "draus", "nicht", "ein", "neu\u00b7er", "quil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "PAV", "PTKNEG", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du aber siehst es nicht aus blossem Ubermuth.", "tokens": ["Du", "a\u00b7ber", "siehst", "es", "nicht", "aus", "blos\u00b7sem", "U\u00b7ber\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du denckest du bist sch\u00f6n/ du pochst auf deine Macht", "tokens": ["Du", "den\u00b7ckest", "du", "bist", "sch\u00f6n", "/", "du", "pochst", "auf", "dei\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADJD", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie vor den Gliedern weicht der Schmuck der weissen", "tokens": ["Wie", "vor", "den", "Glie\u00b7dern", "weicht", "der", "Schmuck", "der", "weis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Schwaanen/", "tokens": ["Schwaa\u00b7nen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Und suchest dir dadurch den Wohlfahrts-Weg zu bahnen/", "tokens": ["Und", "su\u00b7chest", "dir", "da\u00b7durch", "den", "Wohl\u00b7fahrts\u00b7Weg", "zu", "bah\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und also werde ich fast nichts von dir geacht.", "tokens": ["Und", "al\u00b7so", "wer\u00b7de", "ich", "fast", "nichts", "von", "dir", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich mu\u00df es zwar gestehn/ da\u00df du recht sch\u00f6ne bist/", "tokens": ["Ich", "mu\u00df", "es", "zwar", "ge\u00b7stehn", "/", "da\u00df", "du", "recht", "sch\u00f6\u00b7ne", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "PPER", "ADJD", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du bist ein sch\u00f6nes Kind/ und hei\u00dft mit rechte nette/", "tokens": ["Du", "bist", "ein", "sch\u00f6\u00b7nes", "Kind", "/", "und", "hei\u00dft", "mit", "rech\u00b7te", "net\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Wollust machet sich bey dir ein Rosen-Bette;", "tokens": ["Die", "Wol\u00b7lust", "ma\u00b7chet", "sich", "bey", "dir", "ein", "Ro\u00b7sen\u00b7Bet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Wie/ aber/ geht es dem/ der ungehorsahm ist?", "tokens": ["Wie", "/", "a\u00b7ber", "/", "geht", "es", "dem", "/", "der", "un\u00b7ge\u00b7hor\u00b7sahm", "ist", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "ADV", "$(", "VVFIN", "PPER", "ART", "$(", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du weist/ da\u00df ", "tokens": ["Du", "weist", "/", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Und wie so K\u00f6niglich gantz ", "tokens": ["Und", "wie", "so", "K\u00f6\u00b7nig\u00b7lich", "gantz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.28": {"text": "Den g\u00fcldnen Apffel hat mir ", "tokens": ["Den", "g\u00fcld\u00b7nen", "Apf\u00b7fel", "hat", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Er wurde mir mit recht vor andern zuerkennt.", "tokens": ["Er", "wur\u00b7de", "mir", "mit", "recht", "vor", "an\u00b7dern", "zu\u00b7er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJD", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dort in ", "tokens": ["Dort", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.31": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.32": {"text": "Die holden ", "tokens": ["Die", "hol\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Zur Liebes-G\u00f6ttin hat mein ", "tokens": ["Zur", "Lie\u00b7bes\u00b7G\u00f6t\u00b7tin", "hat", "mein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Ich bin es/ der der Prei\u00df vor aller Welt geb\u00fchret/", "tokens": ["Ich", "bin", "es", "/", "der", "der", "Prei\u00df", "vor", "al\u00b7ler", "Welt", "ge\u00b7b\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "ART", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wo von den Schwaanen hin mein Wage wird gef\u00fchret/", "tokens": ["Wo", "von", "den", "Schwaa\u00b7nen", "hin", "mein", "Wa\u00b7ge", "wird", "ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da werde ich geliebt/ und als ein Gott verehrt.", "tokens": ["Da", "wer\u00b7de", "ich", "ge\u00b7liebt", "/", "und", "als", "ein", "Gott", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$(", "KON", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich poch\u2019 auf meine Macht/ der nichts zu m\u00e4chtig ist/", "tokens": ["Ich", "poch'", "auf", "mei\u00b7ne", "Macht", "/", "der", "nichts", "zu", "m\u00e4ch\u00b7tig", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "ART", "PIS", "PTKA", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie hat den ", "tokens": ["Sie", "hat", "den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Durch sie wird ", "tokens": ["Durch", "sie", "wird"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Sie machts/ da\u00df ", "tokens": ["Sie", "machts", "/", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.43": {"text": "Mein sch\u00f6nes Haupt/ das tr\u00e4gt von Perlen seine Kronen/", "tokens": ["Mein", "sch\u00f6\u00b7nes", "Haupt", "/", "das", "tr\u00e4gt", "von", "Per\u00b7len", "sei\u00b7ne", "Kro\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PDS", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Auf der erh\u00f6hten Stirn sieht man der Anmuth-Thronen/", "tokens": ["Auf", "der", "er\u00b7h\u00f6h\u00b7ten", "Stirn", "sieht", "man", "der", "An\u00b7muth\u00b7Thro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PIS", "ART", "NN", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "Und vor den Augen f\u00e4lt der Sternen Demant hin.", "tokens": ["Und", "vor", "den", "Au\u00b7gen", "f\u00e4lt", "der", "Ster\u00b7nen", "De\u00b7mant", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die Wangen siegen an der ", "tokens": ["Die", "Wan\u00b7gen", "sie\u00b7gen", "an", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Wo sich die Liljen mit sch\u00f6nen Rosen k\u00fcssen/", "tokens": ["Wo", "sich", "die", "Lil\u00b7jen", "mit", "sch\u00f6\u00b7nen", "Ro\u00b7sen", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Und die Corallen/ so die Lippen ziehren m\u00fcssen/", "tokens": ["Und", "die", "Co\u00b7ral\u00b7len", "/", "so", "die", "Lip\u00b7pen", "z