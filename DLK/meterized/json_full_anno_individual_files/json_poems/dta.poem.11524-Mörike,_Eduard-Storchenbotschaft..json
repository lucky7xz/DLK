{"dta.poem.11524": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Storchenbotschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Des Sch\u00e4fers sein Haus und das steht auf zwei Rad,", "tokens": ["Des", "Sch\u00e4\u00b7fers", "sein", "Haus", "und", "das", "steht", "auf", "zwei", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "PDS", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Steht hoch auf der Heiden, so fr\u00fche, wie spat.", "tokens": ["Steht", "hoch", "auf", "der", "Hei\u00b7den", ",", "so", "fr\u00fc\u00b7he", ",", "wie", "spat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "ADV", "ADJA", "$,", "PWAV", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wenn nur ein Mancher so'n Nachtquartier h\u00e4tt'!", "tokens": ["Und", "wenn", "nur", "ein", "Man\u00b7cher", "so'n", "Nacht\u00b7quar\u00b7tier", "h\u00e4tt'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "PIAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ein Sch\u00e4fer tauscht nicht mit dem K\u00f6nig sein Bett.", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "tauscht", "nicht", "mit", "dem", "K\u00f6\u00b7nig", "sein", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Und k\u00e4m' ihm zu Nacht auch was Seltsames vor,", "tokens": ["Und", "k\u00e4m'", "ihm", "zu", "Nacht", "auch", "was", "Selt\u00b7sa\u00b7mes", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ADV", "PWS", "NE", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er betet sein Spr\u00fcchel und legt sich auf's Ohr,", "tokens": ["Er", "be\u00b7tet", "sein", "Spr\u00fc\u00b7chel", "und", "legt", "sich", "auf's", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein Geistlein, ein Hexlein, so lustige Wicht',", "tokens": ["Ein", "Geist\u00b7lein", ",", "ein", "Hex\u00b7lein", ",", "so", "lus\u00b7ti\u00b7ge", "Wicht'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie klopfen ihm wohl, doch er antwortet nicht.", "tokens": ["Sie", "klop\u00b7fen", "ihm", "wohl", ",", "doch", "er", "ant\u00b7wor\u00b7tet", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Einmal doch, da ward es ihm wirklich zu bunt,", "tokens": ["Ein\u00b7mal", "doch", ",", "da", "ward", "es", "ihm", "wirk\u00b7lich", "zu", "bunt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "PTKA", "ADJD", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Es knopert am Laden, es winselt der Hund,", "tokens": ["Es", "kno\u00b7pert", "am", "La\u00b7den", ",", "es", "win\u00b7selt", "der", "Hund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nun ziehet mein Sch\u00e4fer den Riegel \u2014 ei schau!", "tokens": ["Nun", "zie\u00b7het", "mein", "Sch\u00e4\u00b7fer", "den", "Rie\u00b7gel", "ei", "schau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$(", "NE", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da stehen zwei St\u00f6rche, der Mann und die Frau.", "tokens": ["Da", "ste\u00b7hen", "zwei", "St\u00f6r\u00b7che", ",", "der", "Mann", "und", "die", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Das P\u00e4rchen, es machet ein sch\u00f6n Kompliment,", "tokens": ["Das", "P\u00e4r\u00b7chen", ",", "es", "ma\u00b7chet", "ein", "sch\u00f6n", "Kom\u00b7pli\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es m\u00f6chte gern reden, ach, wenn es nur k\u00f6nnt'!", "tokens": ["Es", "m\u00f6ch\u00b7te", "gern", "re\u00b7den", ",", "ach", ",", "wenn", "es", "nur", "k\u00f6nnt'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "ITJ", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was will mir das Ziefer? ist so was erh\u00f6rt?", "tokens": ["Was", "will", "mir", "das", "Zie\u00b7fer", "?", "ist", "so", "was", "er\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "$.", "VAFIN", "ADV", "PWS", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Doch ist mir wohl fr\u00f6hliche Botschaft bescheert.", "tokens": ["Doch", "ist", "mir", "wohl", "fr\u00f6h\u00b7li\u00b7che", "Bot\u00b7schaft", "be\u00b7scheert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ihr seyd wohl dahinten zu Hause am Rhein?", "tokens": ["Ihr", "seyd", "wohl", "da\u00b7hin\u00b7ten", "zu", "Hau\u00b7se", "am", "Rhein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "APPR", "NN", "APPRART", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr habt wohl mein M\u00e4del gebissen in's Bein?", "tokens": ["Ihr", "habt", "wohl", "mein", "M\u00e4\u00b7del", "ge\u00b7bis\u00b7sen", "in's", "Bein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nun weinet das Kind und die Mutter noch mehr,", "tokens": ["Nun", "wei\u00b7net", "das", "Kind", "und", "die", "Mut\u00b7ter", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie w\u00fcnschet den Herzallerliebsten sich her?", "tokens": ["Sie", "w\u00fcn\u00b7schet", "den", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ten", "sich", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und w\u00fcnschet daneben die Taufe bestellt:", "tokens": ["Und", "w\u00fcn\u00b7schet", "da\u00b7ne\u00b7ben", "die", "Tau\u00b7fe", "be\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein L\u00e4mmlein, ein W\u00fcrstlein, ein Beutelein Geld? \u2014", "tokens": ["Ein", "L\u00e4mm\u00b7lein", ",", "ein", "W\u00fcrst\u00b7lein", ",", "ein", "Beu\u00b7tel\u00b7ein", "Geld", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So sagt nur, ich k\u00e4m' in zwei Tag oder drei,", "tokens": ["So", "sagt", "nur", ",", "ich", "k\u00e4m'", "in", "zwei", "Tag", "o\u00b7der", "drei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "APPR", "CARD", "NN", "KON", "CARD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und gr\u00fc\u00dft mir mein B\u00fcbel und r\u00fchrt ihm den Brei!", "tokens": ["Und", "gr\u00fc\u00dft", "mir", "mein", "B\u00fc\u00b7bel", "und", "r\u00fchrt", "ihm", "den", "Brei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Doch halt! warum stellt ihr zu Zweien euch ein?", "tokens": ["Doch", "halt", "!", "wa\u00b7rum", "stellt", "ihr", "zu", "Zwei\u00b7en", "euch", "ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es werden doch, hoff' ich, nicht Zwillinge seyn? \u2014", "tokens": ["Es", "wer\u00b7den", "doch", ",", "hoff'", "ich", ",", "nicht", "Zwil\u00b7lin\u00b7ge", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "VVFIN", "PPER", "$,", "PTKNEG", "NN", "VAINF", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da klappern die St\u00f6rche im lustigsten Ton,", "tokens": ["Da", "klap\u00b7pern", "die", "St\u00f6r\u00b7che", "im", "lus\u00b7tigs\u00b7ten", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie nicken und knixen und fliegen davon.", "tokens": ["Sie", "ni\u00b7cken", "und", "kni\u00b7xen", "und", "flie\u00b7gen", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}