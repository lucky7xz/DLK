{"dta.poem.2086": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Zobel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses ist ein kleines Thier, welches Norden uns nur", "tokens": ["Die\u00b7ses", "ist", "ein", "klei\u00b7nes", "Thier", ",", "wel\u00b7ches", "Nor\u00b7den", "uns", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PWAT", "NN", "PPER", "ADV"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "zollt,", "tokens": ["zollt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Sonderlich Siberien; es ist fast den Martern gleich,", "tokens": ["Son\u00b7der\u00b7lich", "Si\u00b7be\u00b7ri\u00b7en", ";", "es", "ist", "fast", "den", "Mar\u00b7tern", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Doch an einem gl\u00e4nzenden braun- und sch\u00f6nen Haar so reich,", "tokens": ["Doch", "an", "ei\u00b7nem", "gl\u00e4n\u00b7zen\u00b7den", "braun", "und", "sch\u00f6\u00b7nen", "Haar", "so", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "TRUNC", "KON", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Da\u00df es h\u00f6her noch im Preise, als das allerfeinste Gold.", "tokens": ["Da\u00df", "es", "h\u00f6\u00b7her", "noch", "im", "Prei\u00b7se", ",", "als", "das", "al\u00b7ler\u00b7feins\u00b7te", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Nur f\u00fcr Ru\u00dflands Kaiserinn sind sie ein Regal allein,", "tokens": ["Nur", "f\u00fcr", "Ru\u00df\u00b7lands", "Kai\u00b7se\u00b7rinn", "sind", "sie", "ein", "Re\u00b7gal", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "Und sie m\u00fcssen von Gefangnen w\u00f6chentlich geliefert seyn,", "tokens": ["Und", "sie", "m\u00fcs\u00b7sen", "von", "Ge\u00b7fang\u00b7nen", "w\u00f6\u00b7chent\u00b7lich", "ge\u00b7lie\u00b7fert", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "NN", "ADJD", "VVPP", "VAINF", "$,"], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Welche doch, wenn sie die Zahl, die sie schuldig, ein-", "tokens": ["Wel\u00b7che", "doch", ",", "wenn", "sie", "die", "Zahl", ",", "die", "sie", "schul\u00b7dig", ",", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "gebracht,", "tokens": ["ge\u00b7bracht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Und sie etwan mehr bekommen,", "tokens": ["Und", "sie", "et\u00b7wan", "mehr", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jhnen stets f\u00fcr baare Zahlung richtig werden abgenommen.", "tokens": ["Jh\u00b7nen", "stets", "f\u00fcr", "baa\u00b7re", "Zah\u00b7lung", "rich\u00b7tig", "wer\u00b7den", "ab\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Hiermit wird im Orient fast der gr\u00f6\u00dfte Staat gemacht.", "tokens": ["Hier\u00b7mit", "wird", "im", "O\u00b7rient", "fast", "der", "gr\u00f6\u00df\u00b7te", "Staat", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.13": {"text": "Da\u00df die Felle nicht verderben, f\u00e4nget man sie meist in", "tokens": ["Da\u00df", "die", "Fel\u00b7le", "nicht", "ver\u00b7der\u00b7ben", ",", "f\u00e4n\u00b7get", "man", "sie", "meist", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "VVFIN", "PIS", "PPER", "ADV", "APPR"], "meter": "+-+-+-+-+---+-", "measure": "unknown.measure.hexa"}, "line.14": {"text": "Schlingen,", "tokens": ["Schlin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Wo hinein man dieses Thierchen weis mit vieler List", "tokens": ["Wo", "hin\u00b7ein", "man", "die\u00b7ses", "Thier\u00b7chen", "weis", "mit", "vie\u00b7ler", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "PDAT", "NN", "PTKVZ", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "zu bringen;", "tokens": ["zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Auch in Fallen, auch durch Bolzen, vorn mit schwerem", "tokens": ["Auch", "in", "Fal\u00b7len", ",", "auch", "durch", "Bol\u00b7zen", ",", "vorn", "mit", "schwe\u00b7rem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Bley begossen,", "tokens": ["Bley", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Werden Zobel angeschossen.", "tokens": ["Wer\u00b7den", "Zo\u00b7bel", "an\u00b7ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ein ganz ungemeiner Handel wird mit diesem Thier ge-", "tokens": ["Ein", "ganz", "un\u00b7ge\u00b7mei\u00b7ner", "Han\u00b7del", "wird", "mit", "die\u00b7sem", "Thier", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "APPR", "PDAT", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "f\u00fchrt.", "tokens": ["f\u00fchrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Da es manchen sehr bereichert, und auch manchen w\u00e4rmt", "tokens": ["Da", "es", "man\u00b7chen", "sehr", "be\u00b7rei\u00b7chert", ",", "und", "auch", "man\u00b7chen", "w\u00e4rmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "ADV", "VVPP", "$,", "KON", "ADV", "PIS", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "und ziert,", "tokens": ["und", "ziert", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Ist es gleichfalls anzusehn, als ein Theil von vielen Ga-", "tokens": ["Ist", "es", "gleich\u00b7falls", "an\u00b7zu\u00b7sehn", ",", "als", "ein", "Theil", "von", "vie\u00b7len", "Ga"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVIZU", "$,", "KOUS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.25": {"text": "ben,", "tokens": ["ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Welche wir, zu unsrer Lust und zum Nutz, empfangen", "tokens": ["Wel\u00b7che", "wir", ",", "zu", "uns\u00b7rer", "Lust", "und", "zum", "Nutz", ",", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "$,", "APPR", "PPOSAT", "NN", "KON", "APPRART", "NN", "$,", "VVPP"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "haben.", "tokens": ["ha\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}