{"textgrid.poem.46256": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Eine sch\u00f6ne Bettlerin", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als von mir eine frau, von gottes reicher hand", "tokens": ["Als", "von", "mir", "ei\u00b7ne", "frau", ",", "von", "got\u00b7tes", "rei\u00b7cher", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "$,", "APPR", "ADJA", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit gr\u00f6\u00dferer sch\u00f6nheit, dan hab und gut, verehret,", "tokens": ["mit", "gr\u00f6\u00b7\u00dfe\u00b7rer", "sch\u00f6n\u00b7heit", ",", "dan", "hab", "und", "gut", ",", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VAFIN", "KON", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "mit fliegend sch\u00f6nen haar und lumpechten gewand,", "tokens": ["mit", "flie\u00b7gend", "sch\u00f6\u00b7nen", "haar", "und", "lum\u00b7pech\u00b7ten", "ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "um got in ihrer not ein st\u00fccklein gelds begehret:", "tokens": ["um", "got", "in", "ih\u00b7rer", "not", "ein", "st\u00fcck\u00b7lein", "gelds", "be\u00b7ge\u00b7hret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "PPOSAT", "NN", "ART", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Empfand mit andern ich, da\u00df ihrer augen brand", "tokens": ["Emp\u00b7fand", "mit", "an\u00b7dern", "ich", ",", "da\u00df", "ih\u00b7rer", "au\u00b7gen", "brand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "vil mehr, dan ihre bit, mit lieb das herz versehret,", "tokens": ["vil", "mehr", ",", "dan", "ih\u00b7re", "bit", ",", "mit", "lieb", "das", "herz", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "PPOSAT", "PTKANT", "$,", "APPR", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und ihr haupt, aug und leib sich (ihrem armen stand", "tokens": ["und", "ihr", "haupt", ",", "aug", "und", "leib", "sich", "(", "ih\u00b7rem", "ar\u00b7men", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "PRF", "$(", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zuwider) einen schatz unsch\u00e4tzlich reich vermehret.", "tokens": ["zu\u00b7wi\u00b7der", ")", "ei\u00b7nen", "schatz", "un\u00b7sch\u00e4tz\u00b7lich", "reich", "ver\u00b7meh\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "ART", "NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darum, o reiche frau, sprach seufzend ich zu ihr,", "tokens": ["Da\u00b7rum", ",", "o", "rei\u00b7che"