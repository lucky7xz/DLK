{"dta.poem.19325": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast den Geistern der Geschicht' ihr Recht gethan,", "tokens": ["Du", "hast", "den", "Geis\u00b7tern", "der", "Ge\u00b7schicht'", "ihr", "Recht", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du sie alle nimmst als Fortschritt' auf der Bahn,", "tokens": ["Wenn", "du", "sie", "al\u00b7le", "nimmst", "als", "Fort\u00b7schritt'", "auf", "der", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die wahre Seit' erkennst an den Einseitigkeiten,", "tokens": ["Die", "wah\u00b7re", "Seit'", "er\u00b7kennst", "an", "den", "Ein\u00b7sei\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und gleichst in Einsicht aus der Ansicht Streitigkeiten.", "tokens": ["Und", "gleichst", "in", "Ein\u00b7sicht", "aus", "der", "An\u00b7sicht", "Strei\u00b7tig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein Irrthum ist allein, da\u00df du zur Offenbarheit", "tokens": ["Dein", "Irr\u00b7thum", "ist", "al\u00b7lein", ",", "da\u00df", "du", "zur", "Of\u00b7fen\u00b7bar\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf deinem Standpunkt glaubst gelangt die ganze Wahrheit.", "tokens": ["Auf", "dei\u00b7nem", "Stand\u00b7punkt", "glaubst", "ge\u00b7langt", "die", "gan\u00b7ze", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch dir geschiht dein Recht, wie ihnen ihrs geschehn,", "tokens": ["Doch", "dir", "ge\u00b7schiht", "dein", "Recht", ",", "wie", "ih\u00b7nen", "ihrs", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn wir die Wahrheit auch in deinem Irrthum sehn.", "tokens": ["Wenn", "wir", "die", "Wahr\u00b7heit", "auch", "in", "dei\u00b7nem", "Irr\u00b7thum", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}