{"dta.poem.5603": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Ursprunge der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die in nur gar zu kleinen Schrancken", "tokens": ["Die", "in", "nur", "gar", "zu", "klei\u00b7nen", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschrenckte menschliche Gedancken", "tokens": ["Ver\u00b7schrenck\u00b7te", "menschli\u00b7che", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die haben sich umsonst zu zeigen vorgenommen,", "tokens": ["Die", "ha\u00b7ben", "sich", "um\u00b7sonst", "zu", "zei\u00b7gen", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "ADV", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob k\u00f6nn\u2019 aus Nichts, ein Etwas kommen.", "tokens": ["Ob", "k\u00f6nn'", "aus", "Nichts", ",", "ein", "Et\u00b7was", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "APPR", "PIS", "$,", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es leidet die Vernunfft ein solches Dencken nicht;", "tokens": ["Es", "lei\u00b7det", "die", "Ver\u00b7nunfft", "ein", "sol\u00b7ches", "Den\u00b7cken", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch wird bey allem seinem Licht", "tokens": ["Doch", "wird", "bey", "al\u00b7lem", "sei\u00b7nem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es diesem stolzen Geist noch weniger gelingen,", "tokens": ["Es", "die\u00b7sem", "stol\u00b7zen", "Geist", "noch", "we\u00b7ni\u00b7ger", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich einigen Begriff von c\u00f6rperlichen Dingen,", "tokens": ["Sich", "ei\u00b7ni\u00b7gen", "Be\u00b7griff", "von", "c\u00f6r\u00b7per\u00b7li\u00b7chen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als w\u00e4r von Ewigkeit ihr Ursprung, beyzubringen.", "tokens": ["Als", "w\u00e4r", "von", "E\u00b7wig\u00b7keit", "ihr", "Ur\u00b7sprung", ",", "bey\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "APPR", "NN", "PPOSAT", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}