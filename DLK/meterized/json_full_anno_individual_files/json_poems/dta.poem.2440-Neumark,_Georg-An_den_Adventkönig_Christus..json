{"dta.poem.2440": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An den Adventk\u00f6nig Christus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Odu starker Wundergott/ O du K\u00f6nig aller Ehren", "tokens": ["O\u00b7du", "star\u00b7ker", "Wun\u00b7der\u00b7gott", "/", "O", "du", "K\u00f6\u00b7nig", "al\u00b7ler", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "NE", "NE", "NN", "PIAT", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der du voll von Sanfft- und Demuht hin nach Zi-", "tokens": ["Der", "du", "voll", "von", "Sanf\u00b7ft", "und", "De\u00b7muht", "hin", "nach", "Zi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPR", "TRUNC", "KON", "NN", "ADV", "APPR", "TRUNC"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "on kommen bist/", "tokens": ["on", "kom\u00b7men", "bist", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VAFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Komm ach komm in mein Hertze mit Genaden Je-", "tokens": ["Komm", "ach", "komm", "in", "mein", "Hert\u00b7ze", "mit", "Ge\u00b7na\u00b7den", "Je"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "VVFIN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "su Christ/", "tokens": ["su", "Christ", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$("], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Da\u00df ich m\u00f6g\u2019 in Fr\u00f6ligkeit diesen Lobgesang vermehren:", "tokens": ["Da\u00df", "ich", "m\u00f6g'", "in", "Fr\u00f6\u00b7lig\u00b7keit", "die\u00b7sen", "Lob\u00b7ge\u00b7sang", "ver\u00b7meh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Hosianna Hosianna dir O grosser Da-", "tokens": ["Ho\u00b7si\u00b7an\u00b7na", "Ho\u00b7si\u00b7an\u00b7na", "dir", "O", "gros\u00b7ser", "Da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "PPER", "NE", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "vidssohn!", "tokens": ["vids\u00b7sohn", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Sey gelobet der du k\u00f6mmest zu uns aus", "tokens": ["Sey", "ge\u00b7lo\u00b7bet", "der", "du", "k\u00f6m\u00b7mest", "zu", "uns", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVFIN", "ART", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "des Himmelstheon.", "tokens": ["des", "Him\u00b7mel\u00b7sthe\u00b7on", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}