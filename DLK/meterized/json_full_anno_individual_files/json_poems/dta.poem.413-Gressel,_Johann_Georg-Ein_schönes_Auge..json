{"dta.poem.413": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ein sch\u00f6nes Auge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll ein Auge seyn/ wie ist es recht zu nennen?", "tokens": ["Was", "soll", "ein", "Au\u00b7ge", "seyn", "/", "wie", "ist", "es", "recht", "zu", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VAINF", "$(", "KOKOM", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Brenn-Glas/ dessen Strahl die Hertzen kan ver-", "tokens": ["Ein", "Brenn\u00b7Glas", "/", "des\u00b7sen", "Strahl", "die", "Hert\u00b7zen", "kan", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELAT", "NN", "ART", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "brennen/", "tokens": ["bren\u00b7nen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Cupidens sch\u00e4rffster Pfeil/ den er am ersten scheust", "tokens": ["Cu\u00b7pi\u00b7dens", "sch\u00e4rffs\u00b7ter", "Pfeil", "/", "den", "er", "am", "ers\u00b7ten", "scheust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "PRELS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn seine Tyranney uns eine Sch\u00f6nheit weist.", "tokens": ["Wenn", "sei\u00b7ne", "Ty\u00b7ran\u00b7ney", "uns", "ei\u00b7ne", "Sch\u00f6n\u00b7heit", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die ihre Augen braucht/ die Geister zu entz\u00fcnden/", "tokens": ["Die", "ih\u00b7re", "Au\u00b7gen", "braucht", "/", "die", "Geis\u00b7ter", "zu", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df man die Liebes Gluth mit Schmertzen mu\u00df empfinden/", "tokens": ["Da\u00df", "man", "die", "Lie\u00b7bes", "Gluth", "mit", "Schmert\u00b7zen", "mu\u00df", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein K\u00f6der/ so zwar s\u00fc\u00df/ doch ins Verderben zieht/", "tokens": ["Ein", "K\u00f6\u00b7der", "/", "so", "zwar", "s\u00fc\u00df", "/", "doch", "ins", "Ver\u00b7der\u00b7ben", "zieht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "ADJD", "$(", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil Drachen-Blut und Gifft bey seinen Rosen bl\u00fcht.", "tokens": ["Weil", "Dra\u00b7chen\u00b7Blut", "und", "Gifft", "bey", "sei\u00b7nen", "Ro\u00b7sen", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}