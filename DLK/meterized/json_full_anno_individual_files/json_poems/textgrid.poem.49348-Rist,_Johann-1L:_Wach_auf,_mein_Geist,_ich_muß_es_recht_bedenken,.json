{"textgrid.poem.49348": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wach auf, mein Geist, ich mu\u00df es recht bedenken,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wach auf, mein Geist, ich mu\u00df es recht bedenken,", "tokens": ["Wach", "auf", ",", "mein", "Geist", ",", "ich", "mu\u00df", "es", "recht", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Jesus itz bem\u00fchet ist, zu schenken", "tokens": ["Wie", "Je\u00b7sus", "itz", "be\u00b7m\u00fc\u00b7het", "ist", ",", "zu", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NE", "NE", "VVFIN", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir seinen Leib, der schwach und blutig hieng", "tokens": ["Mir", "sei\u00b7nen", "Leib", ",", "der", "schwach", "und", "blu\u00b7tig", "hieng"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am d\u00fcrren Holz, wo Gott den Tod empfieng.", "tokens": ["Am", "d\u00fcr\u00b7ren", "Holz", ",", "wo", "Gott", "den", "Tod", "emp\u00b7fi\u00b7eng", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ja, diesen Leib gibt er mir noch zu essen,", "tokens": ["Ja", ",", "die\u00b7sen", "Leib", "gibt", "er", "mir", "noch", "zu", "es\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "VVFIN", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wil, ich sol auch nimmermehr vergessen", "tokens": ["Und", "wil", ",", "ich", "sol", "auch", "nim\u00b7mer\u00b7mehr", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Lieb' und Treu', welch' er (o h\u00f6chstes Gut!].", "tokens": ["Der", "Lieb'", "und", "Treu'", ",", "welch'", "er", "(", "o", "h\u00f6chs\u00b7tes", "Gut", "!", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PRELS", "PPER", "$(", "FM", "ADJA", "NN", "$.", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An mir gethan, als er vergo\u00df sein Blut.", "tokens": ["An", "mir", "ge\u00b7than", ",", "als", "er", "ver\u00b7go\u00df", "sein", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Er hat mich ja der H\u00f6llenpein entfreiet,", "tokens": ["Er", "hat", "mich", "ja", "der", "H\u00f6l\u00b7len\u00b7pein", "ent\u00b7frei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wof\u00fcr mein Mund ein Danklied itz ausschreiet,", "tokens": ["Wo\u00b7f\u00fcr", "mein", "Mund", "ein", "Dank\u00b7lied", "itz", "aus\u00b7schrei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Auch die\u00df mein Herz bringt singend auf die Bahn", "tokens": ["Auch", "die\u00df", "mein", "Herz", "bringt", "sin\u00b7gend", "auf", "die", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das Gute, so mein Gott an mir gethan.", "tokens": ["Das", "Gu\u00b7te", ",", "so", "mein", "Gott", "an", "mir", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Er spricht: \u00bbNehmt hin den Leib, f\u00fcr euch gegeben,", "tokens": ["Er", "spricht", ":", "\u00bb", "Nehmt", "hin", "den", "Leib", ",", "f\u00fcr", "euch", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "ADV", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und trinkt mein Blut, das theure Pfand, daneben.\u00ab", "tokens": ["Und", "trinkt", "mein", "Blut", ",", "das", "theu\u00b7re", "Pfand", ",", "da\u00b7ne\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "PAV", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "O s\u00fc\u00dfe Lieb', o gro\u00dfe Wunderthat,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Lieb'", ",", "o", "gro\u00b7\u00dfe", "Wun\u00b7der\u00b7that", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Da\u00df in den Tod sich Gott gegeben hat!", "tokens": ["Da\u00df", "in", "den", "Tod", "sich", "Gott", "ge\u00b7ge\u00b7ben", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wo k\u00f6nte man doch solche Gnade finden,", "tokens": ["Wo", "k\u00f6n\u00b7te", "man", "doch", "sol\u00b7che", "Gna\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Dadurch ein Mensch befreiet wird von S\u00fcnden,", "tokens": ["Da\u00b7durch", "ein", "Mensch", "be\u00b7frei\u00b7et", "wird", "von", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Demnach Gott selbst zur S\u00fcnd' hat den gemacht,", "tokens": ["Dem\u00b7nach", "Gott", "selbst", "zur", "S\u00fcnd'", "hat", "den", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ADV", "APPRART", "NN", "VAFIN", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der an das B\u00f6s' auch nimmermehr gedacht?", "tokens": ["Der", "an", "das", "B\u00f6s'", "auch", "nim\u00b7mer\u00b7mehr", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Was n\u00fctzet denn das Essen und das Trinken", "tokens": ["Was", "n\u00fct\u00b7zet", "denn", "das", "Es\u00b7sen", "und", "das", "Trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Im Abendmahl? Es sol kein Mensch versinken", "tokens": ["Im", "A\u00b7bend\u00b7mahl", "?", "Es", "sol", "kein", "Mensch", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "PPER", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Im H\u00f6llenpfuhl, der diesen Worten traut:", "tokens": ["Im", "H\u00f6l\u00b7len\u00b7pfuhl", ",", "der", "die\u00b7sen", "Wor\u00b7ten", "traut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "\u00bbmein Tod hat euch den Himmel aufgebaut.\u00ab", "tokens": ["\u00bb", "mein", "Tod", "hat", "euch", "den", "Him\u00b7mel", "auf\u00b7ge\u00b7baut", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ist schon dein Glaub' hie schwach, da\u00df er gedenket:", "tokens": ["Ist", "schon", "dein", "Glaub'", "hie", "schwach", ",", "da\u00df", "er", "ge\u00b7den\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Ob Jesus gleich sich selbst den S\u00fcndern schenket,", "tokens": ["Ob", "Je\u00b7sus", "gleich", "sich", "selbst", "den", "S\u00fcn\u00b7dern", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wer wei\u00df, ob ich geh\u00f6r' in diese Zahl?", "tokens": ["Wer", "wei\u00df", ",", "ob", "ich", "ge\u00b7h\u00f6r'", "in", "die\u00b7se", "Zahl", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ja, Mensch, an dir komt zu die\u00df Abendmahl.", "tokens": ["Ja", ",", "Mensch", ",", "an", "dir", "komt", "zu", "die\u00df", "A\u00b7bend\u00b7mahl", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "APPR", "PPER", "VVFIN", "APPR", "PDS", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Es l\u00e4\u00dft die\u00df Pfand sich so gar kr\u00e4ftig sehen,", "tokens": ["Es", "l\u00e4\u00dft", "die\u00df", "Pfand", "sich", "so", "gar", "kr\u00e4f\u00b7tig", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Da\u00df du getrost kanst mit den S\u00fcndern gehen", "tokens": ["Da\u00df", "du", "ge\u00b7trost", "kanst", "mit", "den", "S\u00fcn\u00b7dern", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Zu Jesu hin und schlie\u00dfen festiglich,", "tokens": ["Zu", "Je\u00b7su", "hin", "und", "schlie\u00b7\u00dfen", "fes\u00b7tig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Da\u00df er den Tod gelitten auch f\u00fcr dich.", "tokens": ["Da\u00df", "er", "den", "Tod", "ge\u00b7lit\u00b7ten", "auch", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und ob du schon den Taufbund so gebrochen,", "tokens": ["Und", "ob", "du", "schon", "den", "Tauf\u00b7bund", "so", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Da\u00df dir darob ersch\u00fcttern alle Knochen,", "tokens": ["Da\u00df", "dir", "da\u00b7rob", "er\u00b7sch\u00fct\u00b7tern", "al\u00b7le", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "O Mensch, lauf hin, nim Christus Leib und Blut;", "tokens": ["O", "Mensch", ",", "lauf", "hin", ",", "nim", "Chris\u00b7tus", "Leib", "und", "Blut", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "PTKVZ", "$,", "VVIMP", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Was gilts, dein Herz wird frisch und wolgemut?", "tokens": ["Was", "gilts", ",", "dein", "Herz", "wird", "frisch", "und", "wol\u00b7ge\u00b7mut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und ob dich gleich die S\u00fcndenb\u00fcrden dr\u00fccken,", "tokens": ["Und", "ob", "dich", "gleich", "die", "S\u00fcn\u00b7den\u00b7b\u00fcr\u00b7den", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Kan doch allein dein Jesus dich erquicken;", "tokens": ["Kan", "doch", "al\u00b7lein", "dein", "Je\u00b7sus", "dich", "er\u00b7qui\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Derselbe gibt dir solche Speis' und Trank,", "tokens": ["Der\u00b7sel\u00b7be", "gibt", "dir", "sol\u00b7che", "Speis'", "und", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Wodurch dein Geist bleibt stark sein Leben lang.", "tokens": ["Wo\u00b7durch", "dein", "Geist", "bleibt", "stark", "sein", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Sobald wir nun den Leib und Blut genossen,", "tokens": ["So\u00b7bald", "wir", "nun", "den", "Leib", "und", "Blut", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Sind wir in ihm und er in uns geschlossen,", "tokens": ["Sind", "wir", "in", "ihm", "und", "er", "in", "uns", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "KON", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Denn wer nur glaubt, der wird ihm inverleibt,", "tokens": ["Denn", "wer", "nur", "glaubt", ",", "der", "wird", "ihm", "in\u00b7ver\u00b7leibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "PRELS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Auch so, da\u00df er in uns wahrhaftig bleibt.", "tokens": ["Auch", "so", ",", "da\u00df", "er", "in", "uns", "wahr\u00b7haf\u00b7tig", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Sein Fleisch und Blut da\u00df l\u00e4\u00