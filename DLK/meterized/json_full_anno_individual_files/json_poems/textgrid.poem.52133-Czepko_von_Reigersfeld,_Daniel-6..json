{"textgrid.poem.52133": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ihr die Prediger auff Predig[t]st\u00fchle stellt,", "tokens": ["Weil", "ihr", "die", "Pre\u00b7di\u00b7ger", "auff", "Pre\u00b7dig", "t", "st\u00fch\u00b7le", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NE", "$(", "VVFIN", "$(", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unterde\u00dfen blo\u00df sucht unser Gut und Geld,", "tokens": ["Und", "un\u00b7ter\u00b7de\u00b7\u00dfen", "blo\u00df", "sucht", "un\u00b7ser", "Gut", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Meint ihr, ihr bringet uns mit Spie\u00dfen und mit Stangen", "tokens": ["Meint", "ihr", ",", "ihr", "brin\u00b7get", "uns", "mit", "Spie\u00b7\u00dfen", "und", "mit", "Stan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Evangelium, das ihr habt umbgehangen.", "tokens": ["Das", "E\u00b7van\u00b7ge\u00b7li\u00b7um", ",", "das", "ihr", "habt", "umb\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verzeiht es mir, weil ihr mit Donnern und mit Qu\u00e4hln,", "tokens": ["Ver\u00b7zeiht", "es", "mir", ",", "weil", "ihr", "mit", "Don\u00b7nern", "und", "mit", "Qu\u00e4hln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die armen Leute wollt bi\u00df auff das Leben scheeln,", "tokens": ["Die", "ar\u00b7men", "Leu\u00b7te", "wollt", "bi\u00df", "auff", "das", "Le\u00b7ben", "scheeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und voller Raub und Mord erweitert eure Sch\u00e4tze,", "tokens": ["Und", "vol\u00b7ler", "Raub", "und", "Mord", "er\u00b7wei\u00b7tert", "eu\u00b7re", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich es sagen darff: Ihr bringet das Gesetze.", "tokens": ["Da\u00df", "ich", "es", "sa\u00b7gen", "darff", ":", "Ihr", "brin\u00b7get", "das", "Ge\u00b7set\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weil ihr die Prediger auff Predig[t]st\u00fchle stellt,", "tokens": ["Weil", "ihr