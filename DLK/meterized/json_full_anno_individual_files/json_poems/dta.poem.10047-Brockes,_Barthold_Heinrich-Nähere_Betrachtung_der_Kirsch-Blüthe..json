{"dta.poem.10047": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4here Betrachtung der Kirsch-Bl\u00fcthe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wunderbar ist doch der Bau", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "ist", "doch", "der", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wol von Z\u00e4rtlichkeit, als Farben und Figur,", "tokens": ["So", "wol", "von", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "als", "Far\u00b7ben", "und", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den ich von Fingern der Natur", "tokens": ["Den", "ich", "von", "Fin\u00b7gern", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einer Kirschen-Bl\u00fcht, die sich erst \u00f6ffnet, schau!", "tokens": ["In", "ei\u00b7ner", "Kir\u00b7schen\u00b7B\u00b7l\u00fcht", ",", "die", "sich", "erst", "\u00f6ff\u00b7net", ",", "schau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erst seh\u2019 ich Bl\u00e4tter-Knospen-Spitzen", "tokens": ["Erst", "seh'", "ich", "Bl\u00e4t\u00b7ter\u00b7Knos\u00b7pen\u00b7Spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umringt von Trage-Knospen, sitzen.", "tokens": ["Um\u00b7ringt", "von", "Tra\u00b7ge\u00b7Knos\u00b7pen", ",", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die letztern, welche rund, sind erstlich glatt und gr\u00fcn.", "tokens": ["Die", "letz\u00b7tern", ",", "wel\u00b7che", "rund", ",", "sind", "erst\u00b7lich", "glatt", "und", "gr\u00fcn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die braune Haut, die, \u00fcm sie vor Gefahr", "tokens": ["Die", "brau\u00b7ne", "Haut", ",", "die", ",", "\u00fcm", "sie", "vor", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zu sch\u00fctzen, fest und holtzig war,", "tokens": ["Zu", "sch\u00fct\u00b7zen", ",", "fest", "und", "holt\u00b7zig", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er\u00f6ffnet sich, und zeigt zwey auch noch harte Bl\u00e4tter,", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "sich", ",", "und", "zeigt", "zwey", "auch", "noch", "har\u00b7te", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KON", "VVFIN", "CARD", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die rund und hohl, und gantz voll zarter Spitzen,", "tokens": ["Die", "rund", "und", "hohl", ",", "und", "gantz", "voll", "zar\u00b7ter", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,", "KON", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und rauher Z\u00e4ser sind;", "tokens": ["Und", "rau\u00b7her", "Z\u00e4\u00b7ser", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Um ihr so zartes Kind,", "tokens": ["Um", "ihr", "so", "zar\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Als wie mit einem Peltz, zu sch\u00fctzen.", "tokens": ["Als", "wie", "mit", "ei\u00b7nem", "Peltz", ",", "zu", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Darauf erblickt man drey, die sonderlich formitt,", "tokens": ["Da\u00b7rauf", "er\u00b7blickt", "man", "drey", ",", "die", "son\u00b7der\u00b7lich", "for\u00b7mitt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "CARD", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Indem sie obenw\u00e4rts recht in der Mitten,", "tokens": ["In\u00b7dem", "sie", "o\u00b7ben\u00b7w\u00e4rts", "recht", "in", "der", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Als w\u00e4ren sie mit Flei\u00df so zierlich eingeschnitten,", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "mit", "Flei\u00df", "so", "zier\u00b7lich", "ein\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sich theilen, da sie denn ein eignes Spitzgen ziert.", "tokens": ["Sich", "thei\u00b7len", ",", "da", "sie", "denn", "ein", "eig\u00b7nes", "Spitz\u00b7gen", "ziert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nachher sind, aus dermassen sch\u00f6n,", "tokens": ["Nach\u00b7her", "sind", ",", "aus", "der\u00b7mas\u00b7sen", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "PDS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Drey schmale Bl\u00e4tterchen zu sehn,", "tokens": ["Drey", "schma\u00b7le", "Bl\u00e4t\u00b7ter\u00b7chen", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die unser\u2019 Augen recht erfrischen,", "tokens": ["Die", "un\u00b7ser'", "Au\u00b7gen", "recht", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wann sie ein z\u00e4rtlich roth zu ihrem gr\u00fcnen mischen,", "tokens": ["Wann", "sie", "ein", "z\u00e4rt\u00b7lich", "roth", "zu", "ih\u00b7rem", "gr\u00fc\u00b7nen", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJD", "ADJD", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Absonderlich,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Da selbe sich,", "tokens": ["Da", "sel\u00b7be", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Zumahl in hellen Sonnen-Strahlen,", "tokens": ["Zu\u00b7mahl", "in", "hel\u00b7len", "Son\u00b7nen\u00b7Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Zugleich mit zartem gelben mahlen.", "tokens": ["Zu\u00b7gleich", "mit", "zar\u00b7tem", "gel\u00b7ben", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Bis endlich man die Bluhmen selbst, bey drey,", "tokens": ["Bis", "end\u00b7lich", "man", "die", "Bluh\u00b7men", "selbst", ",", "bey", "drey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "ART", "NN", "ADV", "$,", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Bey vier bis f\u00fcnf, doch selten nur bey zwey,", "tokens": ["Bey", "vier", "bis", "f\u00fcnf", ",", "doch", "sel\u00b7ten", "nur", "bey", "zwey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "APPR", "CARD", "$,", "ADV", "ADJD", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "In gleichfalls r\u00f6thlich gr\u00fcner Haut", "tokens": ["In", "gleich\u00b7falls", "r\u00f6th\u00b7lich", "gr\u00fc\u00b7ner", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Noch einmahl eingeschlossen schaut.", "tokens": ["Noch", "ein\u00b7mahl", "ein\u00b7ge\u00b7schlos\u00b7sen", "schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wann diese nun bey lauem Wetter,", "tokens": ["Wann", "die\u00b7se", "nun", "bey", "lau\u00b7em", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Sich in f\u00fcnf Bl\u00e4tterchen zertheilt,", "tokens": ["Sich", "in", "f\u00fcnf", "Bl\u00e4t\u00b7ter\u00b7chen", "zer\u00b7theilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Dann werden allererst der Bluhmen rechte Bl\u00e4tter,", "tokens": ["Dann", "wer\u00b7den", "al\u00b7le\u00b7rerst", "der", "Bluh\u00b7men", "rech\u00b7te", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die ein fast blendend weiss- und heller Schimmer schm\u00fcckt,", "tokens": ["Die", "ein", "fast", "blen\u00b7dend", "weiss", "und", "hel\u00b7ler", "Schim\u00b7mer", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADJD", "TRUNC", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "In einem holden Glantz erblickt.", "tokens": ["In", "ei\u00b7nem", "hol\u00b7den", "Glantz", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die \u00f6ffnen sich darauf, bey lauer Lufft,", "tokens": ["Die", "\u00f6ff\u00b7nen", "sich", "da\u00b7rauf", ",", "bey", "lau\u00b7er", "Lufft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und zwar in solcher Meng, an allen Ecken,", "tokens": ["Und", "zwar", "in", "sol\u00b7cher", "Meng", ",", "an", "al\u00b7len", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Da\u00df sie, recht wie ein weisser Dufft.", "tokens": ["Da\u00df", "sie", ",", "recht", "wie", "ein", "weis\u00b7ser", "Dufft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die schlancken Zweig\u2019 \u00fcmgeben und verdecken.", "tokens": ["Die", "schlan\u00b7cken", "Zweig'", "\u00fcm\u00b7ge\u00b7ben", "und", "ver\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn so dann der Augen-Strahl", "tokens": ["Wenn", "so", "dann", "der", "Au\u00b7gen\u00b7Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die ungezehlte Zahl", "tokens": ["Durch", "die", "un\u00b7ge\u00b7zehl\u00b7te", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bluhmen-reicher Zweige streichet,", "tokens": ["Bluh\u00b7men\u00b7rei\u00b7cher", "Zwei\u00b7ge", "strei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von holder Kirschen-Bl\u00fcht,", "tokens": ["Und", "von", "hol\u00b7der", "Kir\u00b7schen\u00b7B\u00b7l\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann sie sich ge\u00f6ffnet, gleichet)", "tokens": ["Wann", "sie", "sich", "ge\u00b7\u00f6ff\u00b7net", ",", "glei\u00b7chet", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$,", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Millionen auf einmahl", "tokens": ["Mil\u00b7lion\u00b7en", "auf", "ein\u00b7mahl"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In so weissem Lichte sieht,", "tokens": ["In", "so", "weis\u00b7sem", "Lich\u00b7te", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deren iegliche verdienet,", "tokens": ["De\u00b7ren", "ieg\u00b7li\u00b7che", "ver\u00b7die\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Um der Form und Farben Schein,", "tokens": ["Um", "der", "Form", "und", "Far\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Stunden lang besehn zu seyn;", "tokens": ["Stun\u00b7den", "lang", "be\u00b7sehn", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "R\u00fchmt die Seel in ihrer Freude,", "tokens": ["R\u00fchmt", "die", "Seel", "in", "ih\u00b7rer", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fcr die Lust und Augen-Weide,", "tokens": ["F\u00fcr", "die", "Lust", "und", "Au\u00b7gen\u00b7Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Den, der aller Dinge Pracht", "tokens": ["Den", ",", "der", "al\u00b7ler", "Din\u00b7ge", "Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Durch ein einzigs Wort gemacht.", "tokens": ["Durch", "ein", "ein\u00b7zigs", "Wort", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}