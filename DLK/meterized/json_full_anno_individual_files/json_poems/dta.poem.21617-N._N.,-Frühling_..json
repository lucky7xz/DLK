{"dta.poem.21617": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist die Welt in Rosen erwacht,", "tokens": ["Nun", "ist", "die", "Welt", "in", "Ro\u00b7sen", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gel\u00f6st ist die liebliche Fraue.", "tokens": ["Ge\u00b7l\u00f6st", "ist", "die", "lieb\u00b7li\u00b7che", "Frau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "In St\u00fccken zerbrach der Stirnreif der Nacht,", "tokens": ["In", "St\u00fc\u00b7cken", "zer\u00b7brach", "der", "Stirn\u00b7reif", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und im Morgen lacht", "tokens": ["Und", "im", "Mor\u00b7gen", "lacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Der bl\u00fchende Wald und die Aue.", "tokens": ["Der", "bl\u00fc\u00b7hen\u00b7de", "Wald", "und", "die", "Au\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "An die Reise nun geht der rieselnde Quell,", "tokens": ["An", "die", "Rei\u00b7se", "nun", "geht", "der", "rie\u00b7seln\u00b7de", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es schimmert die N\u00e4h\u2019 und die Ferne.", "tokens": ["Es", "schim\u00b7mert", "die", "N\u00e4h'", "und", "die", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "O Tag, sei du mein Trautgesell", "tokens": ["O", "Tag", ",", "sei", "du", "mein", "Traut\u00b7ge\u00b7sell"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielhold und hell,", "tokens": ["Viel\u00b7hold", "und", "hell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dir wollt\u2019 ich dienen so gerne.", "tokens": ["Dir", "wollt'", "ich", "die\u00b7nen", "so", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PDS", "ADV", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf Lerchenschwingen steigt mein Gesang,", "tokens": ["Auf", "Ler\u00b7chen\u00b7schwin\u00b7gen", "steigt", "mein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sich \u00fcber den Wolken zu wiegen.", "tokens": ["Sich", "\u00fc\u00b7ber", "den", "Wol\u00b7ken", "zu", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch was im tiefsten Herzen erklang,", "tokens": ["Doch", "was", "im", "tiefs\u00b7ten", "Her\u00b7zen", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nie laut sich erschwang,", "tokens": ["Nie", "laut", "sich", "er\u00b7schwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Das wahr\u2019 ich getreu und verschwiegen.", "tokens": ["Das", "wahr'", "ich", "ge\u00b7treu", "und", "ver\u00b7schwie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Nur Eine h\u00f6rte das heimliche Wort,", "tokens": ["Nur", "Ei\u00b7ne", "h\u00f6r\u00b7te", "das", "heim\u00b7li\u00b7che", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Rufen der Lust und des Leides.", "tokens": ["Das", "Ru\u00b7fen", "der", "Lust", "und", "des", "Lei\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Nicht wei\u00df ich den Tag und nicht den Ort", "tokens": ["Nicht", "wei\u00df", "ich", "den", "Tag", "und", "nicht", "den", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "KON", "PTKNEG", "ART", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Sie k\u00fc\u00dfte mir fort", "tokens": ["Sie", "k\u00fc\u00df\u00b7te", "mir", "fort"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Vergessen und Wissen, beides. \u2014", "tokens": ["Ver\u00b7ges\u00b7sen", "und", "Wis\u00b7sen", ",", "bei\u00b7des", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$,", "PIS", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}