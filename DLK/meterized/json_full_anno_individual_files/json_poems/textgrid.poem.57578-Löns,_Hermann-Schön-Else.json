{"textgrid.poem.57578": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6n-Else", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Auge Lachen, im Munde Lieder,", "tokens": ["Im", "Au\u00b7ge", "La\u00b7chen", ",", "im", "Mun\u00b7de", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr jeden Gast ein freundlich Wort,", "tokens": ["F\u00fcr", "je\u00b7den", "Gast", "ein", "freund\u00b7lich", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Taucht sie im Hause auf und nieder,", "tokens": ["Taucht", "sie", "im", "Hau\u00b7se", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald ist sie hier, bald ist sie dort;", "tokens": ["Bald", "ist", "sie", "hier", ",", "bald", "ist", "sie", "dort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im ganzen Haus mu\u00df alles blank sein,", "tokens": ["Im", "gan\u00b7zen", "Haus", "mu\u00df", "al\u00b7les", "blank", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein St\u00e4ubchen darf auf Tisch und Schrank sein,", "tokens": ["Kein", "St\u00e4ub\u00b7chen", "darf", "auf", "Tisch", "und", "Schrank", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drum, sch\u00f6ne Else, soll mein Dank sein", "tokens": ["Drum", ",", "sch\u00f6\u00b7ne", "El\u00b7se", ",", "soll", "mein", "Dank", "sein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "VMFIN", "PPOSAT", "NN", "PPOSAT"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "F\u00fcr frohe Stunden dieses Lied.", "tokens": ["F\u00fcr", "fro\u00b7he", "Stun\u00b7den", "die\u00b7ses", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Umd\u00fcstert und die Stirn voll Falten", "tokens": ["Um\u00b7d\u00fcs\u00b7tert", "und", "die", "Stirn", "voll", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trat eines Abends hier ich ein \u2013", "tokens": ["Trat", "ei\u00b7nes", "A\u00b7bends", "hier", "ich", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPER", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sah ich dein zufriednes Walten", "tokens": ["Da", "sah", "ich", "dein", "zu\u00b7fried\u00b7nes", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und deiner Augen Vollmondschein;", "tokens": ["Und", "dei\u00b7ner", "Au\u00b7gen", "Voll\u00b7mond\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Geist so ha\u00dfvoll und verbittert,", "tokens": ["Mein", "Geist", "so", "ha\u00df\u00b7voll", "und", "ver\u00b7bit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herz, von Rachesucht zersplittert,", "tokens": ["Mein", "Herz", ",", "von", "Ra\u00b7che\u00b7sucht", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hat hoffnungsfroh hier aufgezittert", "tokens": ["Hat", "hoff\u00b7nungs\u00b7froh", "hier", "auf\u00b7ge\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und noch einmal an Gl\u00fcck gedacht.", "tokens": ["Und", "noch", "ein\u00b7mal", "an", "Gl\u00fcck", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ei Traum... ich werde niemals ketten", "tokens": ["Ei", "Traum", "...", "ich", "wer\u00b7de", "nie\u00b7mals", "ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$(", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An mein Geschick ein andres Herz,", "tokens": ["An", "mein", "Ge\u00b7schick", "ein", "and\u00b7res", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich kann nur Kampf und Arbeit retten,", "tokens": ["Mich", "kann", "nur", "Kampf", "und", "Ar\u00b7beit", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erl\u00f6sen nur der Todesschmerz.", "tokens": ["Er\u00b7l\u00f6\u00b7sen", "nur", "der", "To\u00b7des\u00b7schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will nicht mehr um Liebe werben,", "tokens": ["Ich", "will", "nicht", "mehr", "um", "Lie\u00b7be", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn meine Liebe bringt Verderben,", "tokens": ["Denn", "mei\u00b7ne", "Lie\u00b7be", "bringt", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "All, die ich liebte, mu\u00dften sterben,", "tokens": ["All", ",", "die", "ich", "lieb\u00b7te", ",", "mu\u00df\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch kann ein treuer Freund ich sein.", "tokens": ["Doch", "kann", "ein", "treu\u00b7er", "Freund", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Setz dich zu mir, dein helles Lachen", "tokens": ["Setz", "dich", "zu", "mir", ",", "dein", "hel\u00b7les", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Sonnenschein am Wintertag,", "tokens": ["Ist", "Son\u00b7nen\u00b7schein", "am", "Win\u00b7ter\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Plaudern l\u00e4hmt das Ha\u00dferwachen", "tokens": ["Dein", "Plau\u00b7dern", "l\u00e4hmt", "das", "Ha\u00b7\u00dfer\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hemmt des Blutes Fieberschlag \u2013", "tokens": ["Und", "hemmt", "des", "Blu\u00b7tes", "Fie\u00b7ber\u00b7schlag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das frohe Wort aus deinem Munde", "tokens": ["Das", "fro\u00b7he", "Wort", "aus", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bet\u00e4ubt die alte Herzenswunde,", "tokens": ["Be\u00b7t\u00e4ubt", "die", "al\u00b7te", "Her\u00b7zens\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verschafft mir eine frohe Stunde \u2013", "tokens": ["Ver\u00b7schafft", "mir", "ei\u00b7ne", "fro\u00b7he", "Stun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dies Glas, Sch\u00f6n-Else, auf dein Wohl!", "tokens": ["Dies", "Glas", ",", "Sch\u00f6n\u00b7El\u00b7se", ",", "auf", "dein", "Wohl", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Auge Lachen, im Munde Lieder,", "tokens": ["Im", "Au\u00b7ge", "La\u00b7chen", ",", "im", "Mun\u00b7de", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr jeden Gast ein freundlich Wort,", "tokens": ["F\u00fcr", "je\u00b7den", "Gast", "ein", "freund\u00b7lich", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Taucht sie im Hause auf und nieder,", "tokens": ["Taucht", "sie", "im", "Hau\u00b7se", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "m