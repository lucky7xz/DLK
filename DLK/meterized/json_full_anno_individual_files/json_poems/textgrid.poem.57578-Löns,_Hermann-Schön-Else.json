ter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn meine Liebe bringt Verderben,", "tokens": ["Denn", "mei\u00b7ne", "Lie\u00b7be", "bringt", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "All, die ich liebte, mu\u00dften sterben,", "tokens": ["All", ",", "die", "ich", "lieb\u00b7te", ",", "mu\u00df\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch kann ein treuer Freund ich sein.", "tokens": ["Doch", "kann", "ein", "treu\u00b7er", "Freund", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Setz dich zu mir, dein helles Lachen", "tokens": ["Setz", "dich", "zu", "mir", ",", "dein", "hel\u00b7les", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist Sonnenschein am Wintertag,", "tokens": ["Ist", "Son\u00b7nen\u00b7schein", "am", "Win\u00b7ter\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Plaudern l\u00e4hmt das Ha\u00dferwachen", "tokens": ["Dein", "Plau\u00b7dern", "l\u00e4hmt", "das", "Ha\u00b7\u00dfer\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hemmt des Blutes Fieberschlag \u2013", "tokens": ["Und", "hemmt", "des", "Blu\u00b7tes", "Fie\u00b7ber\u00b7schlag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das frohe Wort aus deinem Munde", "tokens": ["Das", "fro\u00b7he", "Wort", "aus", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bet\u00e4ubt die alte Herzenswunde,", "tokens": ["Be\u00b7t\u00e4ubt", "die", "al\u00b7te", "Her\u00b7zens\u00b7wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verschafft mir eine frohe Stunde \u2013", "tokens": ["Ver\u00b7schafft", "mir", "ei\u00b7ne", "fro\u00b7he", "Stun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dies Glas, Sch\u00f6n-Else, auf dein Wohl!", "tokens": ["Dies", "Glas", ",", "Sch\u00f6n\u00b7El\u00b7se", ",", "auf", "dein", "Wohl", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}