{"textgrid.poem.29100": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil F\u00fcrsten Menschen sind, die doch der Menschheit Bestes,", "tokens": ["Weil", "F\u00fcrs\u00b7ten", "Men\u00b7schen", "sind", ",", "die", "doch", "der", "Menschheit", "Bes\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Freundschafft, kennen nicht, weil Herrschafft nicht viel Festes", "tokens": ["Die", "Freund\u00b7schafft", ",", "ken\u00b7nen", "nicht", ",", "weil", "Herr\u00b7schafft", "nicht", "viel", "Fes\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PTKNEG", "$,", "KOUS", "NN", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Bund und Treuen hegt, so ists nat\u00fcrlich Ding,", "tokens": ["Von", "Bund", "und", "Treu\u00b7en", "hegt", ",", "so", "ists", "na\u00b7t\u00fcr\u00b7lich", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df auch ein F\u00fcrsten-Sinn nach diesem Guten hing.", "tokens": ["Da\u00df", "auch", "ein", "F\u00fcrs\u00b7ten\u00b7Sinn", "nach", "die\u00b7sem", "Gu\u00b7ten", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Am wehlen fehlt es nur; sie pflegen die zu kiesen,", "tokens": ["Am", "weh\u00b7len", "fehlt", "es", "nur", ";", "sie", "pfle\u00b7gen", "die", "zu", "kie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die mit gemahlter Zung und krummem Knie sich wiesen;", "tokens": ["Die", "mit", "ge\u00b7mahl\u00b7ter", "Zung", "und", "krum\u00b7mem", "Knie", "sich", "wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bey welchen freyes Wahr, der Freundschafft Seele, wohnt,", "tokens": ["Bey", "wel\u00b7chen", "frey\u00b7es", "Wahr", ",", "der", "Freund\u00b7schafft", "See\u00b7le", ",", "wohnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der bleibt von ihrer Gunst gar sicher und verschont.", "tokens": ["Der", "bleibt", "von", "ih\u00b7rer", "Gunst", "gar", "si\u00b7cher", "und", "ver\u00b7schont", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil F\u00fcrsten Menschen