{"dta.poem.4077": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Rhein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Im dunkeln Epheu sa\u00df ich, an der Pforte", "tokens": ["Im", "dun\u00b7keln", "E\u00b7pheu", "sa\u00df", "ich", ",", "an", "der", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Waldes, eben, da der goldene Mittag", "tokens": ["Des", "Wal\u00b7des", ",", "e\u00b7ben", ",", "da", "der", "gol\u00b7de\u00b7ne", "Mit\u00b7tag"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Quell besuchend, herunterkam", "tokens": ["Den", "Quell", "be\u00b7su\u00b7chend", ",", "her\u00b7un\u00b7ter\u00b7kam"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Treppen des Alpengebir'gs,", "tokens": ["Von", "Trep\u00b7pen", "des", "Al\u00b7pen\u00b7ge\u00b7bir'gs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das mir die g\u00f6ttlichgebaute,", "tokens": ["Das", "mir", "die", "g\u00f6tt\u00b7lich\u00b7ge\u00b7bau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Burg der Himmlischen hei\u00dft", "tokens": ["Die", "Burg", "der", "Himm\u00b7li\u00b7schen", "hei\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Nach alter Meinung, wo aber", "tokens": ["Nach", "al\u00b7ter", "Mei\u00b7nung", ",", "wo", "a\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Geheim noch Manches entschieden", "tokens": ["Ge\u00b7heim", "noch", "Man\u00b7ches", "ent\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PIS", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Zu Menschen gelanget; von da", "tokens": ["Zu", "Men\u00b7schen", "ge\u00b7lan\u00b7get", ";", "von", "da"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$.", "APPR", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Vernahm ich ohne Vermuthen", "tokens": ["Ver\u00b7nahm", "ich", "oh\u00b7ne", "Ver\u00b7mu\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Ein Schicksal, denn noch kaum", "tokens": ["Ein", "Schick\u00b7sal", ",", "denn", "noch", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "War mir im warmen Schatten", "tokens": ["War", "mir", "im", "war\u00b7men", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Sich Manches beredend, die Seele", "tokens": ["Sich", "Man\u00b7ches", "be\u00b7re\u00b7dend", ",", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "PIS", "VVPP", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Italia zugeschweift", "tokens": ["I\u00b7ta\u00b7lia", "zu\u00b7ge\u00b7schweift"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und an die K\u00fcsten Morea's.", "tokens": ["Und", "an", "die", "K\u00fcs\u00b7ten", "Mo\u00b7rea'", "s."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "ART", "NN", "NE", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Jetzt aber, drinn im Gebirg,", "tokens": ["Jetzt", "a\u00b7ber", ",", "drinn", "im", "Ge\u00b7birg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Tief unter den silbernen Gipfeln,", "tokens": ["Tief", "un\u00b7ter", "den", "sil\u00b7ber\u00b7nen", "Gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und unter fr\u00f6hlichem Gr\u00fcn,", "tokens": ["Und", "un\u00b7ter", "fr\u00f6h\u00b7li\u00b7chem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wo die W\u00e4lder schauernd zu ihm", "tokens": ["Wo", "die", "W\u00e4l\u00b7der", "schau\u00b7ernd", "zu", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPR", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Felsen H\u00e4upter \u00fcbereinander", "tokens": ["Und", "der", "Fel\u00b7sen", "H\u00e4up\u00b7ter", "\u00fc\u00b7be\u00b7re\u00b7in\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Hinabschaun, taglang, dort", "tokens": ["Hin\u00b7ab\u00b7schaun", ",", "ta\u00b7glang", ",", "dort"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "ADJD", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im k\u00e4ltesten Abgrund h\u00f6rt'", "tokens": ["Im", "k\u00e4l\u00b7tes\u00b7ten", "Ab\u00b7grund", "h\u00f6rt'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ich um Erl\u00f6sung jammern", "tokens": ["Ich", "um", "Er\u00b7l\u00f6\u00b7sung", "jam\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Den J\u00fcngling, es h\u00f6rten ihn, wie er tobt',", "tokens": ["Den", "J\u00fcng\u00b7ling", ",", "es", "h\u00f6r\u00b7ten", "ihn", ",", "wie", "er", "tobt'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Und die Mutter Erd' anklagt',", "tokens": ["Und", "die", "Mut\u00b7ter", "Erd'", "an\u00b7klagt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und den Donnerer, der ihn gezeuget,", "tokens": ["Und", "den", "Don\u00b7ne\u00b7rer", ",", "der", "ihn", "ge\u00b7zeu\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Erbarmend die Eltern, doch", "tokens": ["Er\u00b7bar\u00b7mend", "die", "El\u00b7tern", ",", "doch"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVPP", "ART", "NN", "$,", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Die Sterblichen flohn von dem Ort,", "tokens": ["Die", "Sterb\u00b7li\u00b7chen", "flohn", "von", "dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Denn furchtbar war, da lichtlos er", "tokens": ["Denn", "furcht\u00b7bar", "war", ",", "da", "licht\u00b7los", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In den Fesseln sich w\u00e4lzte,", "tokens": ["In", "den", "Fes\u00b7seln", "sich", "w\u00e4lz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Das Rasen des Halbgotts.", "tokens": ["Das", "Ra\u00b7sen", "des", "Halb\u00b7gotts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Stimme war's des edelsten der Str\u00f6me,", "tokens": ["Die", "Stim\u00b7me", "wa\u00b7r's", "des", "e\u00b7dels\u00b7ten", "der", "Str\u00f6\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des freigeborenen Rheins,", "tokens": ["Des", "frei\u00b7ge\u00b7bo\u00b7re\u00b7nen", "Rheins", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Anderes hoffte der, als droben von den", "tokens": ["Und", "An\u00b7de\u00b7res", "hoff\u00b7te", "der", ",", "als", "dro\u00b7ben", "von", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "$,", "KOUS", "ADV", "APPR", "ART"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Br\u00fcdern,", "tokens": ["Br\u00fc\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Dem Tessin und dem Rhodanus,", "tokens": ["Dem", "Tes\u00b7sin", "und", "dem", "Rho\u00b7da\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Er schied und wandern wollt', und ungeduldig ihn", "tokens": ["Er", "schied", "und", "wan\u00b7dern", "wollt'", ",", "und", "un\u00b7ge\u00b7dul\u00b7dig", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "VMFIN", "$,", "KON", "ADJD", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nach Asia trieb die k\u00f6nigliche Seele.", "tokens": ["Nach", "A\u00b7sia", "trieb", "die", "k\u00f6\u00b7nig\u00b7li\u00b7che", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.8": {"text": "Doch unverst\u00e4ndig ist", "tokens": ["Doch", "un\u00b7ver\u00b7st\u00e4n\u00b7dig", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Das W\u00fcnschen vor dem Schicksal.", "tokens": ["Das", "W\u00fcn\u00b7schen", "vor", "dem", "Schick\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die Blindesten aber", "tokens": ["Die", "Blin\u00b7des\u00b7ten", "a\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Sind G\u00f6tters\u00f6hne, denn es kennet der Mensch", "tokens": ["Sind", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne", ",", "denn", "es", "ken\u00b7net", "der", "Mensch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Sein Haus und dem Thier ward, wo", "tokens": ["Sein", "Haus", "und", "dem", "Thier", "ward", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "VAFIN", "$,", "PWAV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Es bauen solle, doch jenen ist", "tokens": ["Es", "bau\u00b7en", "sol\u00b7le", ",", "doch", "je\u00b7nen", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "KON", "PDS", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Der Fehl, da\u00df sie nicht wissen wohin?", "tokens": ["Der", "Fehl", ",", "da\u00df", "sie", "nicht", "wis\u00b7sen", "wo\u00b7hin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "PWAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "In die unerfahrne Seele gegeben.", "tokens": ["In", "die", "un\u00b7er\u00b7fahr\u00b7ne", "See\u00b7le", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein R\u00e4thsel ist Reinentsprungenes. Auch", "tokens": ["Ein", "R\u00e4th\u00b7sel", "ist", "Rei\u00b7nent\u00b7sprun\u00b7ge\u00b7nes", ".", "Auch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "$.", "ADV"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der Gesang kaum darf es enth\u00fcllen. Denn", "tokens": ["Der", "Ge\u00b7sang", "kaum", "darf", "es", "ent\u00b7h\u00fcl\u00b7len", ".", "Denn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$.", "KON"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie du anfiengst, wirst du bleiben,", "tokens": ["Wie", "du", "an\u00b7fi\u00b7engst", ",", "wirst", "du", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So viel auch wirket die Noth", "tokens": ["So", "viel", "auch", "wir\u00b7ket", "die", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und die Zucht, das Meiste nemlich", "tokens": ["Und", "die", "Zucht", ",", "das", "Meis\u00b7te", "nem\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PIS", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Vermag die Geburt", "tokens": ["Ver\u00b7mag", "die", "Ge\u00b7burt"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und der Lichtstral, der", "tokens": ["Und", "der", "Licht\u00b7stral", ",", "der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.8": {"text": "Dem Neugebornen begegnet.", "tokens": ["Dem", "Neu\u00b7ge\u00b7bor\u00b7nen", "be\u00b7geg\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wo aber ist Einer,", "tokens": ["Wo", "a\u00b7ber", "ist", "Ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Um frei zu bleiben", "tokens": ["Um", "frei", "zu", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Sein Leben lang und des Herzens Wunsch", "tokens": ["Sein", "Le\u00b7ben", "lang", "und", "des", "Her\u00b7zens", "Wunsch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Allein zu erf\u00fcllen, so", "tokens": ["Al\u00b7lein", "zu", "er\u00b7f\u00fcl\u00b7len", ",", "so"], "token_i