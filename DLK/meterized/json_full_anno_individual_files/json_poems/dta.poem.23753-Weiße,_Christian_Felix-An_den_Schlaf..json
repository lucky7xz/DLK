{"dta.poem.23753": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Komm, s\u00fc\u00dfer Schlaf, erquicke mich!", "tokens": ["Komm", ",", "s\u00fc\u00b7\u00dfer", "Schlaf", ",", "er\u00b7qui\u00b7cke", "mich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein m\u00fcdes Auge sehnet sich", "tokens": ["Mein", "m\u00fc\u00b7des", "Au\u00b7ge", "seh\u00b7net", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ruhe zu genie\u00dfen,", "tokens": ["Der", "Ru\u00b7he", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm, sanft es zuzuschlie\u00dfen!", "tokens": ["Komm", ",", "sanft", "es", "zu\u00b7zu\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie aber, Freund, o schl\u00f6ssest du", "tokens": ["Wie", "a\u00b7ber", ",", "Freund", ",", "o", "schl\u00f6s\u00b7sest", "du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "NN", "$,", "FM", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von nun an es auf ewig zu,", "tokens": ["Von", "nun", "an", "es", "auf", "e\u00b7wig", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPER", "APPR", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und diese Augenlieder", "tokens": ["Und", "die\u00b7se", "Au\u00b7gen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "S\u00e4hn nie den Morgen wieder?", "tokens": ["S\u00e4hn", "nie", "den", "Mor\u00b7gen", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So weis ich, da\u00df ein sch\u00f6nres Licht", "tokens": ["So", "weis", "ich", ",", "da\u00df", "ein", "sch\u00f6n\u00b7res", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst meinen Schlummer unterbricht,", "tokens": ["Einst", "mei\u00b7nen", "Schlum\u00b7mer", "un\u00b7ter\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einen Tag mir g\u00f6nnet,", "tokens": ["Und", "ei\u00b7nen", "Tag", "mir", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der keinen Abend kennet.", "tokens": ["Der", "kei\u00b7nen", "A\u00b7bend", "ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}