{"textgrid.poem.34117": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Einem Urspr\u00fcnglichen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jener Wilde erhob den Brief zum Ohre und lauschte,", "tokens": ["Je\u00b7ner", "Wil\u00b7de", "er\u00b7hob", "den", "Brief", "zum", "Oh\u00b7re", "und", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ob er nicht spr\u00e4che, er war k\u00fcrzlich in Briefen verklagt,", "tokens": ["Ob", "er", "nicht", "spr\u00e4\u00b7che", ",", "er", "war", "k\u00fcrz\u00b7lich", "in", "Brie\u00b7fen", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und er dachte sich nun die Bl\u00e4tter mit Zungen versehen;", "tokens": ["Und", "er", "dach\u00b7te", "sich", "nun", "die", "Bl\u00e4t\u00b7ter", "mit", "Zun\u00b7gen", "ver\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Welch ein poetischer Kopf nach dem modernsten Begriff!", "tokens": ["Welch", "ein", "po\u00b7e\u00b7ti\u00b7scher", "Kopf", "nach", "dem", "mo\u00b7derns\u00b7ten", "Be\u00b7griff", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Freund, erkenne dich selbst in diesem Wilden! Du gleichst ihm,", "tokens": ["Freund", ",", "er\u00b7ken\u00b7ne", "dich", "selbst", "in", "die\u00b7sem", "Wil\u00b7den", "!", "Du", "gleichst", "ihm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Da du der Einsamkeit \u00bbAugen\u00ab und \u00bbHaare\u00ab verleihst", "tokens": ["Da", "du", "der", "Ein\u00b7sam\u00b7keit", "\u00bb", "Au\u00b7gen", "\u00ab", "und", "\u00bb", "Haa\u00b7re", "\u00ab", "ver\u00b7leihst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "NN", "$(", "KON", "$(", "NN", "$(", "VVFIN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ja, du mu\u00dft ihm noch weichen, denn deine erhitzte Phantastik", "tokens": ["Ja", ",", "du", "mu\u00dft", "ihm", "noch", "wei\u00b7chen", ",", "denn", "dei\u00b7ne", "er\u00b7hitz\u00b7te", "Phan\u00b7tas\u00b7tik"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Ist nur betrunk'ner Verstand, er jedoch taumelt von selbst;", "tokens": ["Ist", "nur", "be\u00b7trunk'\u00b7ner", "Ver\u00b7stand", ",", "er", "je\u00b7doch", "tau\u00b7melt", "von", "selbst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$,", "PPER", "ADV", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und du rechnest nicht minder, nur schlechter, weil dir die Tafel", "tokens": ["Und", "du", "rech\u00b7nest", "nicht", "min\u00b7der", ",", "nur", "schlech\u00b7ter", ",", "weil", "dir", "die", "Ta\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "PTKNEG", "ADV", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Mit den Ziffern zerbricht, eh' du die Summe noch zogst!", "tokens": ["Mit", "den", "Zif\u00b7fern", "zer\u00b7bricht", ",", "eh'", "du", "die", "Sum\u00b7me", "noch", "zogst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Anzuschauen ist freilich in Kunst und Leben das H\u00f6chste,", "tokens": ["An\u00b7zu\u00b7schau\u00b7en", "ist", "frei\u00b7lich", "in", "Kunst", "und", "Le\u00b7ben", "das", "H\u00f6chs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "KON", "NN", "ART", "ADJA", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Aber man schaut noch nicht an, weil man nicht denkt und nicht sieht:", "tokens": ["A\u00b7ber", "man", "schaut", "noch", "nicht", "an", ",", "weil", "man", "nicht", "denkt", "und", "nicht", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "KOUS", "PIS", "PTKNEG", "VVFIN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Jenseits der Linie erst begiebt sich dies letzte der Wunder,", "tokens": ["Jen\u00b7seits", "der", "Li\u00b7nie", "erst", "be\u00b7giebt", "sich", "dies", "letz\u00b7te", "der", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PRF", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Diesseits sucht es der Thor, dem es mit Beidem nicht gl\u00fcckt.", "tokens": ["Dies\u00b7seits", "sucht", "es", "der", "Thor", ",", "dem", "es", "mit", "Bei\u00b7dem", "nicht", "gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "Wenn du's bezweifelst, so kr\u00f6ne den S\u00e4ugling als ersten Poeten,", "tokens": ["Wenn", "du's", "be\u00b7zwei\u00b7felst", ",", "so", "kr\u00f6\u00b7ne", "den", "S\u00e4ug\u00b7ling", "als", "ers\u00b7ten", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "ADV", "ADJA", "ART", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Denn er sprudelt wohl noch Aergeres aus, als du selbst,", "tokens": ["Denn", "er", "spru\u00b7delt", "wohl", "noch", "A\u00b7er\u00b7ge\u00b7res", "aus", ",", "als", "du", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.17": {"text": "Aber man hat doch die Freude, die Sprache entstehen zu sehen,", "tokens": ["A\u00b7ber", "man", "hat", "doch", "die", "Freu\u00b7de", ",", "die", "Spra\u00b7che", "ent\u00b7ste\u00b7hen", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Wenn er im Kampf um das Wort N\u00e4chstes und Fernstes verkn\u00fcpft,", "tokens": ["Wenn", "er", "im", "Kampf", "um", "das", "Wort", "N\u00e4chs\u00b7tes", "und", "Ferns\u00b7tes", "ver\u00b7kn\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "W\u00e4hrend ein hohler Gesell uns zeigt durch seine Bombastik,", "tokens": ["W\u00e4h\u00b7rend", "ein", "hoh\u00b7ler", "Ge\u00b7sell", "uns", "zeigt", "durch", "sei\u00b7ne", "Bom\u00b7bas\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Da\u00df sie im leeren Gehirn v\u00f6llig vergl\u00fcht und verdampft.", "tokens": ["Da\u00df", "sie", "im", "lee\u00b7ren", "Ge\u00b7hirn", "v\u00f6l\u00b7lig", "ver\u00b7gl\u00fcht", "und", "ver\u00b7dampft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Welche Verblendung! Du bringst es, verstrickt in die d\u00fcrftigsten Bilder,", "tokens": ["Wel\u00b7che", "Ver\u00b7blen\u00b7dung", "!", "Du", "bringst", "es", ",", "ver\u00b7strickt", "in", "die", "d\u00fcrf\u00b7tigs\u00b7ten", "Bil\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Wie sie zu Tausenden einst jegliche Sprache verschlang,", "tokens": ["Wie", "sie", "zu", "Tau\u00b7sen\u00b7den", "einst", "jeg\u00b7li\u00b7che", "Spra\u00b7che", "ver\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Nicht einmal zum Gedanken, du spielst nur mit H\u00fclsen und Schaalen,", "tokens": ["Nicht", "ein\u00b7mal", "zum", "Ge\u00b7dan\u00b7ken", ",", "du", "spielst", "nur", "mit", "H\u00fcl\u00b7sen", "und", "Schaa\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und du tr\u00e4umst, die Natur nackt, wie die G\u00f6tter, zu schau'n;", "tokens": ["Und", "du", "tr\u00e4umst", ",", "die", "Na\u00b7tur", "nackt", ",", "wie", "die", "G\u00f6t\u00b7ter", ",", "zu", "schau'n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Du entr\u00e4thselst nicht einmal die Hieroglyphen, du siehst nur", "tokens": ["Du", "ent\u00b7r\u00e4th\u00b7selst", "nicht", "ein\u00b7mal", "die", "Hie\u00b7ro\u00b7gly\u00b7phen", ",", "du", "siehst", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Schlangen und V\u00f6gel, und glaubst, dicht vor der Isis zu steh'n!", "tokens": ["Schlan\u00b7gen", "und", "V\u00f6\u00b7gel", ",", "und", "glaubst", ",", "dicht", "vor", "der", "I\u00b7sis", "zu", "steh'n", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+---+---+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Jener Wilde erhob den Brief zum Ohre und lauschte,", "tokens": ["Je\u00b7ner", "Wil\u00b7de", "er\u00b7hob", "den", "Brief", "zum", "Oh\u00b7re", "und", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ob er nicht spr\u00e4che, er war k\u00fcrzlich in Briefen verklagt,", "tokens": ["Ob", "er", "nicht", "spr\u00e4\u00b7che", ",", "er", "war", "k\u00fcrz\u00b7lich", "in", "Brie\u00b7fen", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und er dachte sich nun die Bl\u00e4tter mit Zungen versehen;", "tokens": ["Und", "er", "dach\u00b7te", "sich", "nun", "die", "Bl\u00e4t\u00b7ter", "mit", "Zun\u00b7gen", "ver\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Welch ein poetischer Kopf nach dem modernsten Begriff!", "tokens": ["Welch", "ein", "po\u00b7e\u00b7ti\u00b7scher", "Kopf", "nach", "dem", "mo\u00b7derns\u00b7ten", "Be\u00b7griff", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Freund, erkenne dich selbst in diesem Wilden! Du gleichst ihm,", "tokens": ["Freund", ",", "er\u00b7ken\u00b7ne", "dich", "selbst", "in", "die\u00b7sem", "Wil\u00b7den", "!", "Du", "gleichst", "ihm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Da du der Einsamkeit \u00bbAugen\u00ab und \u00bbHaare\u00ab verleihst", "tokens": ["Da", "du", "der", "Ein\u00b7sam\u00b7keit", "\u00bb", "Au\u00b7gen", "\u00ab", "und", "\u00bb", "Haa\u00b7re", "\u00ab", "ver\u00b7leihst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "NN", "$(", "KON", "$(", "NN", "$(", "VVFIN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ja, du mu\u00dft ihm noch weichen, denn deine erhitzte Phantastik", "tokens": ["Ja", ",", "du", "mu\u00dft", "ihm", "noch", "wei\u00b7chen", ",", "denn", "dei\u00b7ne", "er\u00b7hitz\u00b7te", "Phan\u00b7tas\u00b7tik"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Ist nur betrunk'ner Verstand, er jedoch taumelt von selbst;", "tokens": ["Ist", "nur", "be\u00b7trunk'\u00b7ner", "Ver\u00b7stand", ",", "er", "je\u00b7doch", "tau\u00b7melt", "von", "selbst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$,", "PPER", "ADV", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und du rechnest nicht minder, nur schlechter, weil dir die Tafel", "tokens": ["Und", "du", "rech\u00b7nest", "nicht", "min\u00b7der", ",", "nur", "schlech\u00b7ter", ",", "weil", "dir", "die", "Ta\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "PTKNEG", "ADV", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Mit den Ziffern zerbricht, eh' du die Summe noch zogst!", "tokens": ["Mit", "den", "Zif\u00b7fern", "zer\u00b7bricht", ",", "eh'", "du", "die", "Sum\u00b7me", "noch", "zogst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Anzuschauen ist freilich in Kunst und Leben das H\u00f6chste,", "tokens": ["An\u00b7zu\u00b7schau\u00b7en", "ist", "frei\u00b7lich", "in", "Kunst", "und", "Le\u00b7ben", "das", "H\u00f6chs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "KON", "NN", "ART", "ADJA", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Aber man schaut noch nicht an, weil man nicht denkt und nicht sieht:", "tokens": ["A\u00b7ber", "man", "schaut", "noch", "nicht", "an", ",", "weil", "man", "nicht", "denkt", "und", "nicht", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "KOUS", "PIS", "PTKNEG", "VVFIN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Jenseits der Linie erst begiebt sich dies letzte der Wunder,", "tokens": ["Jen\u00b7seits", "der", "Li\u00b7nie", "erst", "be\u00b7giebt", "sich", "dies", "letz\u00b7te", "der", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PRF", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Diesseits sucht es der Thor, dem es mit Beidem nicht gl\u00fcckt.", "tokens": ["Dies\u00b7seits", "sucht", "es", "der", "Thor", ",", "dem", "es", "mit", "Bei\u00b7dem", "nicht", "gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "Wenn du's bezweifelst, so kr\u00f6ne den S\u00e4ugling als ersten Poeten,", "tokens": ["Wenn", "du's", "be\u00b7zwei\u00b7felst", ",", "so", "kr\u00f6\u00b7ne", "den", "S\u00e4ug\u00b7ling", "als", "ers\u00b7ten", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "ADV", "ADJA", "ART", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Denn er sprudelt wohl noch Aergeres aus, als du selbst,", "tokens": ["Denn", "er", "spru\u00b7delt", "wohl", "noch", "A\u00b7er\u00b7ge\u00b7res", "aus", ",", "als", "du", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.17": {"text": "Aber man hat doch die Freude, die Sprache entstehen zu sehen,", "tokens": ["A\u00b7ber", "man", "hat", "doch", "die", "Freu\u00b7de", ",", "die", "Spra\u00b7che", "ent\u00b7ste\u00b7hen", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Wenn er im Kampf um das Wort N\u00e4chstes und Fernstes verkn\u00fcpft,", "tokens": ["Wenn", "er", "im", "Kampf", "um", "das", "Wort", "N\u00e4chs\u00b7tes", "und", "Ferns\u00b7tes", "ver\u00b7kn\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "W\u00e4hrend ein hohler Gesell uns zeigt durch seine Bombastik,", "tokens": ["W\u00e4h\u00b7rend", "ein", "hoh\u00b7ler", "Ge\u00b7sell", "uns", "zeigt", "durch", "sei\u00b7ne", "Bom\u00b7bas\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Da\u00df sie im leeren Gehirn v\u00f6llig vergl\u00fcht und verdampft.", "tokens": ["Da\u00df", "sie", "im", "lee\u00b7ren", "Ge\u00b7hirn", "v\u00f6l\u00b7lig", "ver\u00b7gl\u00fcht", "und", "ver\u00b7dampft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Welche Verblendung! Du bringst es, verstrickt in die d\u00fcrftigsten Bilder,", "tokens": ["Wel\u00b7che", "Ver\u00b7blen\u00b7dung", "!", "Du", "bringst", "es", ",", "ver\u00b7strickt", "in", "die", "d\u00fcrf\u00b7tigs\u00b7ten", "Bil\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Wie sie zu Tausenden einst jegliche Sprache verschlang,", "tokens": ["Wie", "sie", "zu", "Tau\u00b7sen\u00b7den", "einst", "jeg\u00b7li\u00b7che", "Spra\u00b7che", "ver\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Nicht einmal zum Gedanken, du spielst nur mit H\u00fclsen und Schaalen,", "tokens": ["Nicht", "ein\u00b7mal", "zum", "Ge\u00b7dan\u00b7ken", ",", "du", "spielst", "nur", "mit", "H\u00fcl\u00b7sen", "und", "Schaa\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und du tr\u00e4umst, die Natur nackt, wie die G\u00f6tter, zu schau'n;", "tokens": ["Und", "du", "tr\u00e4umst", ",", "die", "Na\u00b7tur", "nackt", ",", "wie", "die", "G\u00f6t\u00b7ter", ",", "zu", "schau'n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Du entr\u00e4thselst nicht einmal die Hieroglyphen, du siehst nur", "tokens": ["Du", "ent\u00b7r\u00e4th\u00b7selst", "nicht", "ein\u00b7mal", "die", "Hie\u00b7ro\u00b7gly\u00b7phen", ",", "du", "siehst", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Schlangen und V\u00f6gel, und glaubst, dicht vor der Isis zu steh'n!", "tokens": ["Schlan\u00b7gen", "und", "V\u00f6\u00b7gel", ",", "und", "glaubst", ",", "dicht", "vor", "der", "I\u00b7sis", "zu", "steh'n", "!"], "token_info": ["word", "word", "word", "punct", "word", "w