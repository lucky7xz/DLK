{"dta.poem.1303": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Geistliche Lehns-Muttung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "HeRR/ der du z\u00e4hlest unsre Tage/", "tokens": ["HeRR", "/", "der", "du", "z\u00e4h\u00b7lest", "uns\u00b7re", "Ta\u00b7ge", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELS", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den Tod und Leben Meister heist/", "tokens": ["Den", "Tod", "und", "Le\u00b7ben", "Meis\u00b7ter", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von welchem ich zu Lehen trage", "tokens": ["Von", "wel\u00b7chem", "ich", "zu", "Le\u00b7hen", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die edlen G\u00fctter/ Leib und Geist/", "tokens": ["Die", "ed\u00b7len", "G\u00fct\u00b7ter", "/", "Leib", "und", "Geist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schau dein getreuer Unterthan", "tokens": ["Schau", "dein", "ge\u00b7treu\u00b7er", "Un\u00b7ter\u00b7than"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Giebt sich in Demutt bey dir an.", "tokens": ["Giebt", "sich", "in", "De\u00b7mutt", "bey", "dir", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Ich war der Lehn verlustig worden", "tokens": ["Ich", "war", "der", "Lehn", "ver\u00b7lus\u00b7tig", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch angeerbter Bo\u00dfheit Schuld/", "tokens": ["Durch", "an\u00b7ge\u00b7erb\u00b7ter", "Bo\u00df\u00b7heit", "Schuld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mehrte der Verdammten Orden/", "tokens": ["Ich", "mehr\u00b7te", "der", "Ver\u00b7damm\u00b7ten", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tod war meiner Dienste Sold/", "tokens": ["Der", "Tod", "war", "mei\u00b7ner", "Diens\u00b7te", "Sold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein stoltzes Schlo\u00df/ mein Ritter-Sitz", "tokens": ["Mein", "stolt\u00b7zes", "Schlo\u00df", "/", "mein", "Rit\u00b7ter\u00b7Sitz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der H\u00f6llen Schlamm und Schwesel-Pf\u00fctz.", "tokens": ["Der", "H\u00f6l\u00b7len", "Schlamm", "und", "Schwe\u00b7sel\u00b7P\u00b7f\u00fctz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach GOtt/ wie kr\u00e4fftig kanst du lieben!", "tokens": ["Ach", "Gott", "/", "wie", "kr\u00e4ff\u00b7tig", "kanst", "du", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "PWAV", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Sohn macht alles vor mich gutt/", "tokens": ["Dein", "Sohn", "macht", "al\u00b7les", "vor", "mich", "gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "APPR", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wird in Holtz und Stein geschrieben/", "tokens": ["Es", "wird", "in", "Holtz", "und", "Stein", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Versiegelt durch sein theures Blutt/", "tokens": ["Ver\u00b7sie\u00b7gelt", "durch", "sein", "theu\u00b7res", "Blutt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich aus Gnaden und umsunst", "tokens": ["Da\u00df", "ich", "aus", "Gna\u00b7den", "und", "um\u00b7sunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll seyn ein Erbe deiner Gunst.", "tokens": ["Soll", "seyn", "ein", "Er\u00b7be", "dei\u00b7ner", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich soll dir Pflicht und Treue schweren/", "tokens": ["Ich", "soll", "dir", "Pflicht", "und", "Treu\u00b7e", "schwe\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch weil mein Alter dieses nicht", "tokens": ["Doch", "weil", "mein", "Al\u00b7ter", "die\u00b7ses", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PDS", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit eignem Munde kan gew\u00e4hren/", "tokens": ["Mit", "eig\u00b7nem", "Mun\u00b7de", "kan", "ge\u00b7w\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird ein Gedenck-Brieff auffgericht.", "tokens": ["Wird", "ein", "Ge\u00b7den\u00b7ck\u00b7Brieff", "auff\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Der Tauffe Zeugnis machet kund", "tokens": ["Der", "Tauf\u00b7fe", "Zeug\u00b7nis", "ma\u00b7chet", "kund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den zwischen uns getroffnen Bund.", "tokens": ["Den", "zwi\u00b7schen", "uns", "ge\u00b7troff\u00b7nen", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich habe drauff bi\u00dfher genossen", "tokens": ["Ich", "ha\u00b7be", "drauff", "bi\u00df\u00b7her", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gaben deiner milden Hand/", "tokens": ["Die", "Ga\u00b7ben", "dei\u00b7ner", "mil\u00b7den", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Minder-Jahre sind verflossen:", "tokens": ["Die", "Min\u00b7der\u00b7Jah\u00b7re", "sind", "ver\u00b7flos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich baue zwar ein fremdes Land/", "tokens": ["Ich", "bau\u00b7e", "zwar", "ein", "frem\u00b7des", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch dieses auch/ mein GOtt/ ist dein/", "tokens": ["Doch", "die\u00b7ses", "auch", "/", "mein", "Gott", "/", "ist", "dein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "$(", "PPOSAT", "NN", "$(", "VAFIN", "PPOSAT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Pflicht soll abgeleget seyn:", "tokens": ["Die", "Pflicht", "soll", "ab\u00b7ge\u00b7le\u00b7get", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nimm an di\u00df Opffer meiner H\u00e4nde/", "tokens": ["Nimm", "an", "di\u00df", "Opf\u00b7fer", "mei\u00b7ner", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDS", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich dein unverdienter Knecht", "tokens": ["Da\u00df", "ich", "dein", "un\u00b7ver\u00b7dien\u00b7ter", "Knecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu deiner hohen Wohnung sende/", "tokens": ["Zu", "dei\u00b7ner", "ho\u00b7hen", "Woh\u00b7nung", "sen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind die unn\u00fctzen Dienste schlecht/", "tokens": ["Sind", "die", "un\u00b7n\u00fct\u00b7zen", "Diens\u00b7te", "schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wei\u00df ich doch/ da\u00df deine Gnad", "tokens": ["So", "wei\u00df", "ich", "doch", "/", "da\u00df", "dei\u00b7ne", "Gnad"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O milder HErr/ kein Ende hat.", "tokens": ["O", "mil\u00b7der", "Herr", "/", "kein", "En\u00b7de", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Verzeihe durch dein Blutt/ verzeihe/", "tokens": ["Ver\u00b7zei\u00b7he", "durch", "dein", "Blutt", "/", "ver\u00b7zei\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJD", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich/ von Fleisch und Welt verf\u00fchrt/", "tokens": ["Wo", "ich", "/", "von", "Fleisch", "und", "Welt", "ver\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mi\u00dfbrauche deiner Lieb und Treue/", "tokens": ["Mi\u00df\u00b7brau\u00b7che", "dei\u00b7ner", "Lieb", "und", "Treu\u00b7e", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ver\u00fcbe/ was mir nicht geb\u00fchrt/", "tokens": ["Ver\u00b7\u00fc\u00b7be", "/", "was", "mir", "nicht", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie nur/ leider! offt geschicht/", "tokens": ["Und", "wie", "nur", "/", "lei\u00b7der", "!", "offt", "ge\u00b7schicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "$(", "ADV", "$.", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stell in Vergessen meine Pflicht.", "tokens": ["Stell", "in", "Ver\u00b7ges\u00b7sen", "mei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Best\u00e4rcke gn\u00e4dig mein Verlangen/", "tokens": ["Be\u00b7st\u00e4r\u00b7cke", "gn\u00e4\u00b7dig", "mein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein schwacher Wille steht dahin/", "tokens": ["Mein", "schwa\u00b7cher", "Wil\u00b7le", "steht", "da\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein neues Leben anzufangen/", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "an\u00b7zu\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn ich mein selbst eigen bin/", "tokens": ["Und", "wenn", "ich", "mein", "selbst", "ei\u00b7gen", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht mir/ nicht schn\u00f6der Welt so wohl", "tokens": ["Nicht", "mir", "/", "nicht", "schn\u00f6\u00b7der", "Welt", "so", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "$(", "PTKNEG", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als dir zu dienen/ wie ich soll.", "tokens": ["Als", "dir", "zu", "die\u00b7nen", "/", "wie", "ich", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "$(", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df mich die Wacht der Engel f\u00fchren/", "tokens": ["La\u00df", "mich", "die", "Wacht", "der", "En\u00b7gel", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohin mich meine Reise tr\u00e4gt/", "tokens": ["Wo\u00b7hin", "mich", "mei\u00b7ne", "Rei\u00b7se", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Vaterland und hier versp\u00fcren/", "tokens": ["Im", "Va\u00b7ter\u00b7land", "und", "hier", "ver\u00b7sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie GOttes Hand zu sch\u00fctzen pflegt/", "tokens": ["Wie", "Got\u00b7tes", "Hand", "zu", "sch\u00fct\u00b7zen", "pflegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und unser aller Wohlgedeyn/", "tokens": ["Und", "un\u00b7ser", "al\u00b7ler", "Wohl\u00b7ge\u00b7deyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Meinigen samt mir erfreun.", "tokens": ["Die", "Mei\u00b7ni\u00b7gen", "samt", "mir", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text"