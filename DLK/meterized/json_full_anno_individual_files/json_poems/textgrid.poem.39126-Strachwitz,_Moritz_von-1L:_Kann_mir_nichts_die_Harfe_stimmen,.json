: "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rollt's wie Weltenuntergang;", "tokens": ["Rollt's", "wie", "Wel\u00b7ten\u00b7un\u00b7ter\u00b7gang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sie zornig mich umsprudeln,", "tokens": ["Wie", "sie", "zor\u00b7nig", "mich", "um\u00b7spru\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Kl\u00e4nge wild und toll,", "tokens": ["Mei\u00b7ne", "Kl\u00e4n\u00b7ge", "wild", "und", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sie mich von dannen strudeln", "tokens": ["Wie", "sie", "mich", "von", "dan\u00b7nen", "stru\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unbez\u00e4hmbar, zaubervoll.", "tokens": ["Un\u00b7be\u00b7z\u00e4hm\u00b7bar", ",", "zau\u00b7ber\u00b7voll", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf den Nacken der Gemeinheit", "tokens": ["Auf", "den", "Na\u00b7cken", "der", "Ge\u00b7mein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich deine Sohle stampfen,", "tokens": ["Seh'", "ich", "dei\u00b7ne", "Soh\u00b7le", "stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER