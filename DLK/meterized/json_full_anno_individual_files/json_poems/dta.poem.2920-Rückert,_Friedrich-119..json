{"dta.poem.2920": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "119.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts sonderliches wird er lernen, der verstehn", "tokens": ["Nichts", "son\u00b7der\u00b7li\u00b7ches", "wird", "er", "ler\u00b7nen", ",", "der", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "PIS", "VAFIN", "PPER", "VVINF", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will alles was er lernt, und auf den Grund ihm sehn.", "tokens": ["Will", "al\u00b7les", "was", "er", "lernt", ",", "und", "auf", "den", "Grund", "ihm", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PWS", "PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur wenig f\u00f6rdert dich ein leicht Bezwingliches,", "tokens": ["Nur", "we\u00b7nig", "f\u00f6r\u00b7dert", "dich", "ein", "leicht", "Be\u00b7zwing\u00b7li\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PRF", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Blick der Forschung sch\u00e4rft nur Undurchdringliches.", "tokens": ["Den", "Blick", "der", "For\u00b7schung", "sch\u00e4rft", "nur", "Un\u00b7durch\u00b7dring\u00b7li\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem R\u00e4thselhaften, das vielsinnig ist zu deuten,", "tokens": ["Dem", "R\u00e4th\u00b7sel\u00b7haf\u00b7ten", ",", "das", "viel\u00b7sin\u00b7nig", "ist", "zu", "deu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du mit Sinnigkeit den tiefsten Sinn entbeuten.", "tokens": ["Wirst", "du", "mit", "Sin\u00b7nig\u00b7keit", "den", "tiefs\u00b7ten", "Sinn", "ent\u00b7beu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}