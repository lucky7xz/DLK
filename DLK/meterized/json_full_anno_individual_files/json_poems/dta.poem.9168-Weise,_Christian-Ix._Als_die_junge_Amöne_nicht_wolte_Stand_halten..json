{"dta.poem.9168": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n Als die junge Am\u00f6ne nicht wolte Stand halten.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Am\u00f6ne darff ich mich erk\u00fchnen", "tokens": ["A\u00b7m\u00f6\u00b7ne", "darff", "ich", "mich", "er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meiner schlechten h\u00f6fflichkeit", "tokens": ["Mit", "mei\u00b7ner", "schlech\u00b7ten", "h\u00f6ff\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die s\u00fcsse sch\u00f6nheit zu bedienen", "tokens": ["Die", "s\u00fcs\u00b7se", "sch\u00f6n\u00b7heit", "zu", "be\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sich au\u00df ihren augen streut?", "tokens": ["So", "sich", "au\u00df", "ih\u00b7ren", "au\u00b7gen", "streut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach soll ich allersch\u00f6nstes bild?", "tokens": ["Ach", "soll", "ich", "al\u00b7ler\u00b7sch\u00f6ns\u00b7tes", "bild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "A. Ach last mich gehn die mutter schilt.", "tokens": ["A.", "Ach", "last", "mich", "gehn", "die", "mut\u00b7ter", "schilt", "."], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Mein hertz ich habe schon die ehre", "tokens": ["Mein", "hertz", "ich", "ha\u00b7be", "schon", "die", "eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie anzusprechen l\u00e4ngst gesucht/", "tokens": ["Sie", "an\u00b7zu\u00b7spre\u00b7chen", "l\u00e4ngst", "ge\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie gebe mir geneigt geh\u00f6re/", "tokens": ["Sie", "ge\u00b7be", "mir", "ge\u00b7neigt", "ge\u00b7h\u00f6\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst bleibt mein lieben ohne frucht/", "tokens": ["Sonst", "bleibt", "mein", "lie\u00b7ben", "oh\u00b7ne", "frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst wird mein wunsch nur halb erf\u00fcllt.", "tokens": ["Sonst", "wird", "mein", "wunsch", "nur", "halb", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "A. Ach last mich gehn/ die mutter schilt.", "tokens": ["A.", "Ach", "last", "mich", "gehn", "/", "die", "mut\u00b7ter", "schilt", "."], "token_info": ["abbreviation", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Mein liebstes kind sie lasse schelten/", "tokens": ["Mein", "liebs\u00b7tes", "kind", "sie", "las\u00b7se", "schel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es geht de\u00dfwegen uns nicht an/", "tokens": ["Es", "geht", "de\u00df\u00b7we\u00b7gen", "uns", "nicht", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PPER", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mutter lehre kan nicht gelten/", "tokens": ["Der", "mut\u00b7ter", "leh\u00b7re", "kan", "nicht", "gel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie hats vor diesem selbst gethan/", "tokens": ["Sie", "hats", "vor", "die\u00b7sem", "selbst", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hat sich nicht also verh\u00fcllt.", "tokens": ["Sie", "hat", "sich", "nicht", "al\u00b7so", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "A. Ach last mich gehn/ die mutter schilt.", "tokens": ["A.", "Ach", "last", "mich", "gehn", "/", "die", "mut\u00b7ter", "schilt", "."], "token_info": ["abbreviation", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Sie ist in ihren besten jahren/", "tokens": ["Sie", "ist", "in", "ih\u00b7ren", "bes\u00b7ten", "jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da mu\u00df die lust geb\u00fcsset seyn:", "tokens": ["Da", "mu\u00df", "die", "lust", "ge\u00b7b\u00fcs\u00b7set", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mutter wird nicht alls erfahren/", "tokens": ["Die", "mut\u00b7ter", "wird", "nicht", "alls", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gebe sich doch willig drein.", "tokens": ["Sie", "ge\u00b7be", "sich", "doch", "wil\u00b7lig", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was l\u00e4ufft sie als ein sch\u00f6nes wild?", "tokens": ["Was", "l\u00e4ufft", "sie", "als", "ein", "sch\u00f6\u00b7nes", "wild", "?"], "token_info": ["word", "word", "