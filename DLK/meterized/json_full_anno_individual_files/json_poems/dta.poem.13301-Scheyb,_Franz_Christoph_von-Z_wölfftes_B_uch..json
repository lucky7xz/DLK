{"dta.poem.13301": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich rede von des Schiffs Bequemlichkeiten nicht", "tokens": ["\u201e", "ich", "re\u00b7de", "von", "des", "Schiffs", "Be\u00b7quem\u00b7lich\u00b7kei\u00b7ten", "nicht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso man zu dieser Reis\u2019 in Kostbarkeit erricht\u2019t;", "tokens": ["\u201e", "so", "man", "zu", "die\u00b7ser", "Reis'", "in", "Kost\u00b7bar\u00b7keit", "er\u00b7richt't", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "APPR", "PDAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201egedencket nur der Last, die dessen B\u00fchne dr\u00fccket,", "tokens": ["\u201e", "ge\u00b7den\u00b7cket", "nur", "der", "Last", ",", "die", "des\u00b7sen", "B\u00fch\u00b7ne", "dr\u00fc\u00b7cket", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PRELAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund diesen ganzen Bau mit ihrem Ansehn schm\u00fccket.", "tokens": ["\u201e", "und", "die\u00b7sen", "gan\u00b7zen", "Bau", "mit", "ih\u00b7rem", "An\u00b7sehn", "schm\u00fc\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "225\u201dGro\u00df- und geheiligte, von uns gew\u00fcnschte Last!", "tokens": ["\"", "Gro\u00df", "und", "ge\u00b7hei\u00b7lig\u00b7te", ",", "von", "uns", "ge\u00b7w\u00fcnschte", "Last", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "TRUNC", "KON", "VVFIN", "$,", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.6": {"text": "\u201edie du der L\u00e4nder Gl\u00fcck und Wohlseyn in dir hast!", "tokens": ["\u201e", "die", "du", "der", "L\u00e4n\u00b7der", "Gl\u00fcck", "und", "Wohl\u00b7seyn", "in", "dir", "hast", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ART", "NN", "NN", "KON", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eda wird nicht mehr die Wuth derselben Flutten br\u00fcllen,", "tokens": ["\u201e", "da", "wird", "nicht", "mehr", "die", "Wuth", "der\u00b7sel\u00b7ben", "Flut\u00b7ten", "br\u00fcl\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edie durch Erinnerung uns noch mit Graus erf\u00fcllen;", "tokens": ["\u201e", "die", "durch", "E\u00b7rin\u00b7ne\u00b7rung", "uns", "noch", "mit", "Graus", "er\u00b7f\u00fcl\u00b7len", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "APPR", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201enoch heut entsez\u2019 ich mich, wie damahls sich der Grund,", "tokens": ["\u201e", "noch", "heut", "ent\u00b7se\u00b7z'", "ich", "mich", ",", "wie", "da\u00b7mahls", "sich", "der", "Grund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "PRF", "$,", "PWAV", "ADV", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "230\u201dDas Schiff, die Last, die Flutt, und Kron und Thron befund.", "tokens": ["\"", "Das", "Schiff", ",", "die", "Last", ",", "die", "Flutt", ",", "und", "Kron", "und", "Thron", "be\u00b7fund", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}