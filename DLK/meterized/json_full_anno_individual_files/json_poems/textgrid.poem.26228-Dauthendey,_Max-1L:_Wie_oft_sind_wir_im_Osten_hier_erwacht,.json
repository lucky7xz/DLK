 "measure": "iambic.penta"}, "line.3": {"text": "Wo k\u00e4mpft die \u00bbEmden\u00ab diesen Augenblick,", "tokens": ["Wo", "k\u00e4mpft", "die", "\u00bb", "Em\u00b7den", "\u00ab", "die\u00b7sen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "$(", "NN", "$(", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und welches wird das Ende, das Geschick", "tokens": ["Und", "wel\u00b7ches", "wird", "das", "En\u00b7de", ",", "das", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Deutschen, die auf jenes Kriegsschiffs Planken", "tokens": ["Der", "Deut\u00b7schen", ",", "die", "auf", "je\u00b7nes", "Kriegs\u00b7schiffs", "Plan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eng Kameraden wurden den Gedanken?", "tokens": ["Eng", "Ka\u00b7me\u00b7ra\u00b7den", "wur\u00b7den", "den", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wir h\u00f6rten nah des fernen Schiffes Rauschen.", "tokens": ["Wir", "h\u00f6r\u00b7ten", "nah", "des", "fer\u00b7nen", "Schif\u00b7fes", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder Mann wollt' mit den M\u00e4nnern tauschen,", "tokens": ["Ein", "je\u0