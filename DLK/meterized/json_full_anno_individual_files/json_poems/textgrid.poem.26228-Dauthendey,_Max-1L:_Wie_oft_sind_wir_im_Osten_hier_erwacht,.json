{"textgrid.poem.26228": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie oft sind wir im Osten hier erwacht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft sind wir im Osten hier erwacht,", "tokens": ["Wie", "oft", "sind", "wir", "im", "Os\u00b7ten", "hier", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jeder hat dann rasch bei sich gedacht:", "tokens": ["Und", "je\u00b7der", "hat", "dann", "rasch", "bei", "sich", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo k\u00e4mpft die \u00bbEmden\u00ab diesen Augenblick,", "tokens": ["Wo", "k\u00e4mpft", "die", "\u00bb", "Em\u00b7den", "\u00ab", "die\u00b7sen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "$(", "NN", "$(", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und welches wird das Ende, das Geschick", "tokens": ["Und", "wel\u00b7ches", "wird", "das", "En\u00b7de", ",", "das", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Deutschen, die auf jenes Kriegsschiffs Planken", "tokens": ["Der", "Deut\u00b7schen", ",", "die", "auf", "je\u00b7nes", "Kriegs\u00b7schiffs", "Plan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eng Kameraden wurden den Gedanken?", "tokens": ["Eng", "Ka\u00b7me\u00b7ra\u00b7den", "wur\u00b7den", "den", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir h\u00f6rten nah des fernen Schiffes Rauschen.", "tokens": ["Wir", "h\u00f6r\u00b7ten", "nah", "des", "fer\u00b7nen", "Schif\u00b7fes", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder Mann wollt' mit den M\u00e4nnern tauschen,", "tokens": ["Ein", "je\u00b7der", "Mann", "wollt'", "mit", "den", "M\u00e4n\u00b7nern", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die dort als Wache standen auf dem Deck,", "tokens": ["Die", "dort", "als", "Wa\u00b7che", "stan\u00b7den", "auf", "dem", "Deck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stumm aufmerksam vom Bug bis an das Heck.", "tokens": ["Stumm", "auf\u00b7merk\u00b7sam", "vom", "Bug", "bis", "an", "das", "Heck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wir lebten dicht am Bord die Taten mit.", "tokens": ["Wir", "leb\u00b7ten", "dicht", "am", "Bord", "die", "Ta\u00b7ten", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jed' Herz fuhr auf der \u00bbEmden\u00ab hin und stritt.", "tokens": ["Jed'", "Herz", "fuhr", "auf", "der", "\u00bb", "Em\u00b7den", "\u00ab", "hin", "und", "stritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "$(", "NN", "$(", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie flog uns heilig hei\u00dfes Feuer an,", "tokens": ["Wie", "flog", "uns", "hei\u00b7lig", "hei\u00b7\u00dfes", "Feu\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kam Kunde, was ", "tokens": ["Kam", "Kun\u00b7de", ",", "was"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "PWS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Als sie zum erstenmal f\u00fcnf Boote nahm,", "tokens": ["Als", "sie", "zum", "ers\u00b7ten\u00b7mal", "f\u00fcnf", "Boo\u00b7te", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die \u00bbEmden\u00ab leuchtend in die Brust uns kam.", "tokens": ["Die", "\u00bb", "Em\u00b7den", "\u00ab", "leuch\u00b7tend", "in", "die", "Brust", "uns", "kam", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "VVPP", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch nachts der \u00bbEmden\u00ab Lichter uns umgl\u00fchten.", "tokens": ["Noch", "nachts", "der", "\u00bb", "Em\u00b7den", "\u00ab", "Lich\u00b7ter", "uns", "um\u00b7gl\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "$(", "NN", "$(", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Deutschen Wunsch war: Herr, wollst sie beh\u00fcten!", "tokens": ["Des", "Deut\u00b7schen", "Wunsch", "war", ":", "Herr", ",", "wollst", "sie", "be\u00b7h\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Damit begann der Abenteuer Reigen.", "tokens": ["Da\u00b7mit", "be\u00b7gann", "der", "A\u00b7bent\u00b7eu\u00b7er", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von da wir alle auf die \u00bbEmden\u00ab steigen.", "tokens": ["Von", "da", "wir", "al\u00b7le", "auf", "die", "\u00bb", "Em\u00b7den", "\u00ab", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PIS", "APPR", "ART", "$(", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir folgen ihr im dunkeln Weiterziehn", "tokens": ["Wir", "fol\u00b7gen", "ihr", "im", "dun\u00b7keln", "Wei\u00b7ter\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie Sonne, die auf blinden Nebel schien.", "tokens": ["Wie", "Son\u00b7ne", ",", "die", "auf", "blin\u00b7den", "Ne\u00b7bel", "schien", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald h\u00f6ren wir in grauer Ferne Kampf,", "tokens": ["Bald", "h\u00f6\u00b7ren", "wir", "in", "grau\u00b7er", "Fer\u00b7ne", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kanonensch\u00fcsse hinterm Nebeldampf.", "tokens": ["Ka\u00b7no\u00b7nen\u00b7sch\u00fcs\u00b7se", "hin\u00b7term", "Ne\u00b7bel\u00b7dampf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Welch' Mut doch diese tolle \u00bbEmden\u00ab hat!", "tokens": ["Welch'", "Mut", "doch", "die\u00b7se", "tol\u00b7le", "\u00bb", "Em\u00b7den", "\u00ab", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PDAT", "ADJA", "$(", "NN", "$(", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erst schie\u00dft das kleine Schiff auf eine Stadt,", "tokens": ["Erst", "schie\u00dft", "das", "klei\u00b7ne", "Schiff", "auf", "ei\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann dringt's verkappt in einen Hafen ein.", "tokens": ["Dann", "dringt's", "ver\u00b7kappt", "in", "ei\u00b7nen", "Ha\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Herzen weiten sich, die vorher klein.", "tokens": ["Die", "Her\u00b7zen", "wei\u00b7ten", "sich", ",", "die", "vor\u00b7her", "klein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Man h\u00f6rt berauscht, wie unerh\u00f6rt sie handelt,", "tokens": ["Man", "h\u00f6rt", "be\u00b7rauscht", ",", "wie", "un\u00b7er\u00b7h\u00f6rt", "sie", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die \u00bbEmden\u00ab wird zum Geisterschiff verwandelt.", "tokens": ["Die", "\u00bb", "Em\u00b7den", "\u00ab", "wird", "zum", "Geis\u00b7ter\u00b7schiff", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Denn eh' die Feinde zur Besinnung kamen,", "tokens": ["Denn", "eh'", "die", "Fein\u00b7de", "zur", "Be\u00b7sin\u00b7nung", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scho\u00df sie ein russisch Panzerschiff zusammen.", "tokens": ["Scho\u00df", "sie", "ein", "rus\u00b7sisch", "Pan\u00b7zer\u00b7schiff", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sie wird zum gro\u00dfen Grauen, k\u00e4mpft und raucht", "tokens": ["Sie", "wird", "zum", "gro\u00b7\u00dfen", "Grau\u00b7en", ",", "k\u00e4mpft", "und", "raucht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gleich wie ein Spuk, aus Hirnen aufgetaucht.", "tokens": ["Gleich", "wie", "ein", "Spuk", ",", "aus", "Hir\u00b7nen", "auf\u00b7ge\u00b7taucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verfolgt, bohrt sie die Schiffe in den Grund,", "tokens": ["Ver\u00b7folgt", ",", "bohrt", "sie", "die", "Schif\u00b7fe", "in", "den", "Grund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie wird zum Wunder bald in aller Mund.", "tokens": ["Sie", "wird", "zum", "Wun\u00b7der", "bald", "in", "al\u00b7ler", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie nimmt sich nach dem Kampf auch noch die Zeit,", "tokens": ["Sie", "nimmt", "sich", "nach", "dem", "Kampf", "auch", "noch", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Setzt Boote aus und rettet hilfsbereit", "tokens": ["Setzt", "Boo\u00b7te", "aus", "und", "ret\u00b7tet", "hilfs\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die, deren Schiff zur Tiefe hingerollt.", "tokens": ["Die", ",", "de\u00b7ren", "Schiff", "zur", "Tie\u00b7fe", "hin\u00b7ge\u00b7rollt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Feind bald selbst Bewunderung ihr zollt.", "tokens": ["Der", "Feind", "bald", "selbst", "Be\u00b7wun\u00b7de\u00b7rung", "ihr", "zollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Man spricht von ihrer Mannschaft wie von Rittern.", "tokens": ["Man", "spricht", "von", "ih\u00b7rer", "Mann\u00b7schaft", "wie", "von", "Rit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur um ihr Ende aller Herzen zittern.", "tokens": ["Nur", "um", "ihr", "En\u00b7de", "al\u00b7ler", "Her\u00b7zen", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nun sind es lange, stumme, stille Wochen.", "tokens": ["Nun", "sind", "es", "lan\u00b7ge", ",", "stum\u00b7me", ",", "stil\u00b7le", "Wo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Stunden kommen einf\u00f6rmig gekrochen.", "tokens": ["Die", "Stun\u00b7den", "kom\u00b7men", "ein\u00b7f\u00f6r\u00b7mig", "ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn nun ist's tot hier drau\u00dfen auf dem Meer.", "tokens": ["Denn", "nun", "ist's", "tot", "hier", "drau\u00b7\u00dfen", "auf", "dem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der \u00bbEmden\u00ab Wrack kreist auf dem Meergrund leer.", "tokens": ["Der", "\u00bb", "Em\u00b7den", "\u00ab", "Wrack", "kreist", "auf", "dem", "Meer\u00b7grund", "leer", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "$(", "NE", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Geister kehrten heim aus hoher