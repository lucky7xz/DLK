{"dta.poem.14982": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Wien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Begr\u00fc\u00df' ich dich, und folge meiner Pflicht,", "tokens": ["Be\u00b7gr\u00fc\u00df'", "ich", "dich", ",", "und", "fol\u00b7ge", "mei\u00b7ner", "Pflicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Auge will sich eine Thr\u00e4ne regen;", "tokens": ["Im", "Au\u00b7ge", "will", "sich", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PRF", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was str\u00e4ub' ich mich? die Thr\u00e4ne schm\u00e4ht mich                                 nicht. \u2014", "tokens": ["Was", "str\u00e4ub'", "ich", "mich", "?", "die", "Thr\u00e4\u00b7ne", "schm\u00e4ht", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ach! wo ich wandle, sei's auf Friedenswegen,", "tokens": ["Ach", "!", "wo", "ich", "wand\u00b7le", ",", "sei's", "auf", "Frie\u00b7dens\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sei's wo der Tod die blut'gen Kr\u00e4nze bricht,", "tokens": ["Sei's", "wo", "der", "Tod", "die", "blut'\u00b7gen", "Kr\u00e4n\u00b7ze", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Da werden deine theuern Huldgestalten", "tokens": ["Da", "wer\u00b7den", "dei\u00b7ne", "theu\u00b7ern", "Huld\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Lieb' und Sehnsucht meine Seele spalten.", "tokens": ["In", "Lieb'", "und", "Sehn\u00b7sucht", "mei\u00b7ne", "See\u00b7le", "spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verkennt mich nicht, Ihr Genien meines Lebens,", "tokens": ["Ver\u00b7kennt", "mich", "nicht", ",", "Ihr", "Ge\u00b7ni\u00b7en", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verkennt nicht meiner Seele ernsten Drang.", "tokens": ["Ver\u00b7kennt", "nicht", "mei\u00b7ner", "See\u00b7le", "erns\u00b7ten", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begreift die treue Richtung meines Strebens,", "tokens": ["Be\u00b7greift", "die", "treu\u00b7e", "Rich\u00b7tung", "mei\u00b7nes", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So in dem Liede, wie im Schwerdterklang.", "tokens": ["So", "in", "dem", "Lie\u00b7de", ",", "wie", "im", "Schwerd\u00b7ter\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es schw\u00e4rmten meine Tr\u00e4ume nicht vergebens;", "tokens": ["Es", "schw\u00e4rm\u00b7ten", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "nicht", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ich so oft gefeiert mit Gesang,", "tokens": ["Was", "ich", "so", "oft", "ge\u00b7fei\u00b7ert", "mit", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr Volk und Freiheit ein begeistert Sterben,", "tokens": ["F\u00fcr", "Volk", "und", "Frei\u00b7heit", "ein", "be\u00b7geis\u00b7tert", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "La\u00dft mich nun selbst um diese Krone werben.", "tokens": ["La\u00dft", "mich", "nun", "selbst", "um", "die\u00b7se", "Kro\u00b7ne", "wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl leichter m\u00f6gen sich die Kr\u00e4nze flechten,", "tokens": ["Wohl", "leich\u00b7ter", "m\u00f6\u00b7gen", "sich", "die", "Kr\u00e4n\u00b7ze", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Errungen mit des Liedes heit'rem Muth:", "tokens": ["Er\u00b7run\u00b7gen", "mit", "des", "Lie\u00b7des", "heit'\u00b7rem", "Muth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein rechtes Herz schl\u00e4gt freudig nach dem Rechten.", "tokens": ["Ein", "rech\u00b7tes", "Herz", "schl\u00e4gt", "freu\u00b7dig", "nach", "dem", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ich gepflegt mit jugendlicher Gluth,", "tokens": ["Die", "ich", "ge\u00b7pflegt", "mit", "ju\u00b7gend\u00b7li\u00b7cher", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "La\u00dft mich der Kunst ein Vaterland erfechten,", "tokens": ["La\u00dft", "mich", "der", "Kunst", "ein", "Va\u00b7ter\u00b7land", "er\u00b7fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und g\u00e4lt' es auch das eigne w\u00e4rmste Blut. \u2014", "tokens": ["Und", "g\u00e4lt'", "es", "auch", "das", "eig\u00b7ne", "w\u00e4rms\u00b7te", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch diesen Ku\u00df! und wenns der letzte bliebe,", "tokens": ["Noch", "die\u00b7sen", "Ku\u00df", "!", "und", "wenns", "der", "letz\u00b7te", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$.", "KON", "KOUS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es giebt ja keinen Tod f\u00fcr unsre Liebe.", "tokens": ["Es", "giebt", "ja", "kei\u00b7nen", "Tod", "f\u00fcr", "uns\u00b7re", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}