{"textgrid.poem.67142": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wenn die alten Wasser, die in andern Zorn,", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wenn die alten Wasser, die in andern Zorn,", "tokens": ["Wie", "wenn", "die", "al\u00b7ten", "Was\u00b7ser", ",", "die", "in", "an\u00b7dern", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In schr\u00f6cklichern verwandelt wieder", "tokens": ["In", "schr\u00f6ck\u00b7li\u00b7chern", "ver\u00b7wan\u00b7delt", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00e4men, zu reinigen, da es not war,", "tokens": ["K\u00e4\u00b7men", ",", "zu", "rei\u00b7ni\u00b7gen", ",", "da", "es", "not", "war", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "So g\u00e4hlt' und wuchs und wogte von Jahr zu Jahr", "tokens": ["So", "g\u00e4hlt'", "und", "wuchs", "und", "wog\u00b7te", "von", "Jahr", "zu", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Rastlos und \u00fcberschwemmte das bange Land", "tokens": ["Rast\u00b7los", "und", "\u00fc\u00b7bersc\u00b7hwemm\u00b7te", "das", "ban\u00b7ge", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die unerh\u00f6rte Schlacht, da\u00df weit h\u00fcllt", "tokens": ["Die", "un\u00b7er\u00b7h\u00f6r\u00b7te", "Schlacht", ",", "da\u00df", "weit", "h\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ADJD", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dunkel und Bl\u00e4sse das Haupt der Menschen.", "tokens": ["Dun\u00b7kel", "und", "Bl\u00e4s\u00b7se", "das", "Haupt", "der", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Die Heldenkr\u00e4fte flogen, wie Wellen, auf", "tokens": ["Die", "Hel\u00b7den\u00b7kr\u00e4f\u00b7te", "flo\u00b7gen", ",", "wie", "Wel\u00b7len", ",", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "$,", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und schwanden weg, du k\u00fcrztest, o R\u00e4cherin!", "tokens": ["Und", "schwan\u00b7den", "weg", ",", "du", "k\u00fcrz\u00b7test", ",", "o", "R\u00e4\u00b7che\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Dienern oft die Arbeit schnell und", "tokens": ["Den", "Die\u00b7nern", "oft", "die", "Ar\u00b7beit", "schnell", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADJD", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brachtest in Ruhe sie heim, die Streiter.", "tokens": ["Brach\u00b7test", "in", "Ru\u00b7he", "sie", "heim", ",", "die", "Strei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "O du, die unerbittlich und unbesiegt", "tokens": ["O", "du", ",", "die", "un\u00b7er\u00b7bitt\u00b7lich", "und", "un\u00b7be\u00b7siegt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Feigern und den \u00dcbergewaltgen trifft,", "tokens": ["Den", "Fei\u00b7gern", "und", "den", "\u00dc\u00b7ber\u00b7ge\u00b7walt\u00b7gen", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df bis ins letzte Glied hinab vom", "tokens": ["Da\u00df", "bis", "ins", "letz\u00b7te", "Glied", "hin\u00b7ab", "vom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN", "ADV", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlage sein armes Geschlecht erzittert,", "tokens": ["Schla\u00b7ge", "sein", "ar\u00b7mes", "Ge\u00b7schlecht", "er\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Die du geheim den Stachel und Z\u00fcgel h\u00e4ltst,", "tokens": ["Die", "du", "ge\u00b7heim", "den", "Sta\u00b7chel", "und", "Z\u00fc\u00b7gel", "h\u00e4ltst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu hemmen und zu f\u00f6rdern, o Nemesis,", "tokens": ["Zu", "hem\u00b7men", "und", "zu", "f\u00f6r\u00b7dern", ",", "o", "Ne\u00b7me\u00b7sis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,", "FM", "FM", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Strafst du die Toten noch, es schliefen", "tokens": ["Strafst", "du", "die", "To\u00b7ten", "noch", ",", "es", "schlie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter Italiens Lorbeerg\u00e4rten", "tokens": ["Un\u00b7ter", "I\u00b7ta\u00b7li\u00b7ens", "Lor\u00b7beer\u00b7g\u00e4r\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Sonst ungest\u00f6rt die alten Eroberer.", "tokens": ["Sonst", "un\u00b7ge\u00b7st\u00f6rt", "die", "al\u00b7ten", "Er\u00b7o\u00b7be\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schonst du auch des m\u00fc\u00dfigen Hirten nicht,", "tokens": ["Und", "schonst", "du", "auch", "des", "m\u00fc\u00b7\u00dfi\u00b7gen", "Hir\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und haben endlich wohl genug den", "tokens": ["Und", "ha\u00b7ben", "end\u00b7lich", "wohl", "ge\u00b7nug", "den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcppigen Schlummer geb\u00fc\u00dft die V\u00f6lker?", "tokens": ["\u00dcp\u00b7pi\u00b7gen", "Schlum\u00b7mer", "ge\u00b7b\u00fc\u00dft", "die", "V\u00f6l\u00b7ker", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Wer hub es an? wer brachte den Fluch? von heut", "tokens": ["Wer", "hub", "es", "an", "?", "wer", "brach\u00b7te", "den", "Fluch", "?", "von", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VVFIN", "ART", "NN", "$.", "APPR", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ists nicht und nicht von gestern, und die zuerst", "tokens": ["Ists", "nicht", "und", "nicht", "von", "ge\u00b7stern", ",", "und", "die", "zu\u00b7erst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKNEG", "KON", "PTKNEG", "APPR", "ADV", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Das Ma\u00df verloren, unsre V\u00e4ter", "tokens": ["Das", "Ma\u00df", "ver\u00b7lo\u00b7ren", ",", "uns\u00b7re", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wu\u00dften es nicht, und es trieb ihr Geist sie.", "tokens": ["Wu\u00df\u00b7ten", "es", "nicht", ",", "und", "es", "trieb", "ihr", "Geist", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Zu lang, zu lang schon treten die Sterblichen", "tokens": ["Zu", "lang", ",", "zu", "lang", "schon", "tre\u00b7ten", "die", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich gern aufs Haupt, und zanken um Herrschaft sich,", "tokens": ["Sich", "gern", "aufs", "Haupt", ",", "und", "zan\u00b7ken", "um", "Herr\u00b7schaft", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPRART", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Nachbar f\u00fcrchtend, und es hat auf", "tokens": ["Den", "Nach\u00b7bar", "f\u00fcrch\u00b7tend", ",", "und", "es", "hat", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PPER", "VAFIN", "APPR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eigenem Boden der Mann nicht Segen.", "tokens": ["Ei\u00b7ge\u00b7nem", "Bo\u00b7den", "der", "Mann", "nicht", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PTKNEG", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Und unst\u00e4t wehn und irren, dem Chaos gleich,", "tokens": ["Und", "un\u00b7st\u00e4t", "wehn", "und", "ir\u00b7ren", ",", "dem", "Chaos", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "KON", "VVINF", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem g\u00e4renden Geschlechte die W\u00fcnsche noch", "tokens": ["Dem", "g\u00e4\u00b7ren\u00b7den", "Ge\u00b7schlech\u00b7te", "die", "W\u00fcn\u00b7sche", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Umher und wild ist und verzagt und kalt von", "tokens": ["Um\u00b7her", "und", "wild", "ist", "und", "ver\u00b7zagt", "und", "kalt", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADJD", "VAFIN", "KON", "VVPP", "KON", "ADJD", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sorgen das Leben der Armen immer.", "tokens": ["Sor\u00b7gen", "das", "Le\u00b7ben", "der", "Ar\u00b7men", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Du aber wandelst ruhig die sichre Bahn,", "tokens": ["Du", "a\u00b7ber", "wan\u00b7delst", "ru\u00b7hig", "die", "sich\u00b7re", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Mutter Erd, im Lichte. Dein Fr\u00fchling bl\u00fcht,", "tokens": ["O", "Mut\u00b7ter", "Erd", ",", "im", "Lich\u00b7te", ".", "Dein", "Fr\u00fch\u00b7ling", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "APPRART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Melodischwechselnd gehn dir hin die", "tokens": ["Me\u00b7lo\u00b7disc\u00b7hwech\u00b7selnd", "gehn", "dir", "hin", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wachsenden Zeiten, du Lebensreiche!", "tokens": ["Wach\u00b7sen\u00b7den", "Zei\u00b7ten", ",", "du", "Le\u00b7bens\u00b7rei\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Komm du nun, du der heiligen Musen all,", "tokens": ["Komm", "du", "nun", ",", "du", "der", "hei\u00b7li\u00b7gen", "Mu\u00b7sen", "all", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "ART", "ADJA", "NN", "PIAT", "$,"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und der Gestirne Liebling, verj\u00fcngender", "tokens": ["Und", "der", "Ge\u00b7stir\u00b7ne", "Lieb\u00b7ling", ",", "ver\u00b7j\u00fcn\u00b7gen\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ersehnter Friede, komm und gib ein", "tokens": ["Er\u00b7sehn\u00b7ter", "Frie\u00b7de", ",", "komm", "und", "gib", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "VVIMP", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleiben im Leben, ein Herz uns wieder.", "tokens": ["Blei\u00b7ben", "im", "Le\u00b7ben", ",", "ein", "Herz", "uns", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ART", "NN", "PPER", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Unschuldiger! sind kl\u00fcger die Kinder doch", "tokens": ["Un\u00b7schul\u00b7di\u00b7ger", "!", "sind", "kl\u00fc\u00b7ger", "die", "Kin\u00b7der", "doch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "VAFIN", "ADJD", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beinahe, denn wir Alten; es irrt der Zwist", "tokens": ["Bei\u00b7na\u00b7he", ",", "denn", "wir", "Al\u00b7ten", ";", "es", "irrt", "der", "Zwist"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "PPER", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Guten nicht den Sinn, und klar und", "tokens": ["Den", "Gu\u00b7ten", "nicht", "den", "Sinn", ",", "und", "klar", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ART", "NN", "$,", "KON", "ADJD", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freudig ist ihnen ihr Auge blieben.", "tokens": ["Freu\u00b7dig", "ist", "ih\u00b7nen", "ihr", "Au\u00b7ge", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"li