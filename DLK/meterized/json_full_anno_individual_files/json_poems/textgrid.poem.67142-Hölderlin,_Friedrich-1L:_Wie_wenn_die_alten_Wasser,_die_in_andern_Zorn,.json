word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN", "ADV", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlage sein armes Geschlecht erzittert,", "tokens": ["Schla\u00b7ge", "sein", "ar\u00b7mes", "Ge\u00b7schlecht", "er\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "Die du geheim den Stachel und Z\u00fcgel h\u00e4ltst,", "tokens": ["Die", "du", "ge\u00b7heim", "den", "Sta\u00b7chel", "und", "Z\u00fc\u00b7gel", "h\u00e4ltst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu hemmen und zu f\u00f6rdern, o Nemesis,", "tokens": ["Zu", "hem\u00b7men", "und", "zu", "f\u00f6r\u00b7dern", ",", "o", "Ne\u00b7me\u00b7sis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,", "FM", "FM", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Strafst du die Toten noch, es schliefen", "tokens": ["Strafst", "du", "die", "To\u00b7ten", "noch", ",", "es", "schlie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter Italiens Lorbeerg\u00e4rten", "tokens": ["Un\u00b7ter", "I\u00b7ta\u00b7li\u00b7ens", "Lor\u00b7beer\u00b7g\u00e4r\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.20": {"line.1": {"text": "Son