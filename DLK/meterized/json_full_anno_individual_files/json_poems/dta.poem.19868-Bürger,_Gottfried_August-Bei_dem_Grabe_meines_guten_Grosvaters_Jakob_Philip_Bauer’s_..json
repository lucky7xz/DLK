{"dta.poem.19868": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Bei  \n  dem Grabe  \n meines  \n guten Grosvaters  \n  Jakob Philip Bauer\u2019s .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ruhe, s\u00fcsse Ruhe schwebe               ", "tokens": ["Ru\u00b7he", ",", "s\u00fcs\u00b7se", "Ru\u00b7he", "schwe\u00b7be"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friedlich \u00fcber dieser Gruft!", "tokens": ["Fried\u00b7lich", "\u00fc\u00b7ber", "die\u00b7ser", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand spotte dieser Asche,", "tokens": ["Nie\u00b7mand", "spot\u00b7te", "die\u00b7ser", "A\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ich jezt mit Thr\u00e4nen wasche,", "tokens": ["Die", "ich", "jezt", "mit", "Thr\u00e4\u00b7nen", "wa\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und kein Fluch ersch\u00fctt\u2019re diese Luft!", "tokens": ["Und", "kein", "Fluch", "er\u00b7sch\u00fctt'\u00b7re", "die\u00b7se", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Denn dem Frommen, der hier schlummert,", "tokens": ["Denn", "dem", "From\u00b7men", ",", "der", "hier", "schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Galt der Wehrt der Redlichkeit. \u2014", "tokens": ["Galt", "der", "Wehrt", "der", "Red\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was vordem, in goldnen Jahren,", "tokens": ["Was", "vor\u00b7dem", ",", "in", "gold\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Teutsche Biederm\u00e4nner waren,", "tokens": ["Teut\u00b7sche", "Bie\u00b7der\u00b7m\u00e4n\u00b7ner", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "War er den Genossen seiner Zeit. \u2014", "tokens": ["War", "er", "den", "Ge\u00b7nos\u00b7sen", "sei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dieser Biederseele Flecken", "tokens": ["Die\u00b7ser", "Bie\u00b7der\u00b7see\u00b7le", "Fle\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fcge keine L\u00e4sterung!", "tokens": ["R\u00fc\u00b7ge", "kei\u00b7ne", "L\u00e4s\u00b7te\u00b7rung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Denn was Flecken war, vermodert.", "tokens": ["Denn", "was", "Fle\u00b7cken", "war", ",", "ver\u00b7mo\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "NN", "VAFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der Himmelsfunken lodert", "tokens": ["Nur", "der", "Him\u00b7mels\u00b7fun\u00b7ken", "lo\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einst, gel\u00e4utert, zur Verherlichung. \u2014", "tokens": ["Einst", ",", "ge\u00b7l\u00e4u\u00b7tert", ",", "zur", "Ver\u00b7her\u00b7li\u00b7chung", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "VVPP", "$,", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ach! Er war mein treuer Pfleger,", "tokens": ["Ach", "!", "Er", "war", "mein", "treu\u00b7er", "Pfle\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Wiegenalter an.", "tokens": ["Von", "dem", "Wie\u00b7gen\u00b7al\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich bin, und was ich habe,", "tokens": ["Was", "ich", "bin", ",", "und", "was", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gab der Man, in diesem Grabe.", "tokens": ["Gab", "der", "Man", ",", "in", "die\u00b7sem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "$,", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles dank\u2019 ich dir, du guter Man! \u2014", "tokens": ["Al\u00b7les", "dank'", "ich", "dir", ",", "du", "gu\u00b7ter", "Man", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "$,", "PPER", "ADJA", "PIS", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ruhe, s\u00fcsse Ruhe schwebe", "tokens": ["Ru\u00b7he", ",", "s\u00fcs\u00b7se", "Ru\u00b7he", "schwe\u00b7be"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friedlich \u00fcber dieser Gruft!", "tokens": ["Fried\u00b7lich", "\u00fc\u00b7ber", "die\u00b7ser", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der himlische Beloner", "tokens": ["Bis", "der", "him\u00b7li\u00b7sche", "Be\u00b7lo\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren ehrlichen Bewoner,", "tokens": ["Ih\u00b7ren", "ehr\u00b7li\u00b7chen", "Be\u00b7wo\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Krone zu empfangen, ruft.", "tokens": ["Sei\u00b7ne", "Kro\u00b7ne", "zu", "emp\u00b7fan\u00b7gen", ",", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}