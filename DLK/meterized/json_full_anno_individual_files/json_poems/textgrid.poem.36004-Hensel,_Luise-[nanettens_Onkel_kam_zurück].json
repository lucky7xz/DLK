{"textgrid.poem.36004": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[nanettens Onkel kam zur\u00fcck]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nanettens Onkel kam zur\u00fcck", "tokens": ["Na\u00b7net\u00b7tens", "On\u00b7kel", "kam", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von einer langen Reise.", "tokens": ["Von", "ei\u00b7ner", "lan\u00b7gen", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Johann empf\u00e4ngt ihn, w\u00fcnscht ihm Gl\u00fcck", "tokens": ["Jo\u00b7hann", "emp\u00b7f\u00e4ngt", "ihn", ",", "w\u00fcnscht", "ihm", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spa\u00dft nach seiner Weise.", "tokens": ["Und", "spa\u00dft", "nach", "sei\u00b7ner", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Allein, Herr Hastig unterbricht", "tokens": ["Al\u00b7lein", ",", "Herr", "Has\u00b7tig", "un\u00b7ter\u00b7bricht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihn schnell mit zwanzig Fragen", "tokens": ["Ihn", "schnell", "mit", "zwan\u00b7zig", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und h\u00f6rt mit Wohlbehagen,", "tokens": ["Und", "h\u00f6rt", "mit", "Wohl\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Nichtchen hab' ein Angesicht", "tokens": ["Sein", "Nicht\u00b7chen", "hab'", "ein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So sch\u00f6n, da\u00df auch der Neider nicht", "tokens": ["So", "sch\u00f6n", ",", "da\u00df", "auch", "der", "Nei\u00b7der", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu tadeln k\u00f6nnte wagen.", "tokens": ["Zu", "ta\u00b7deln", "k\u00f6nn\u00b7te", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Eins, spricht Herr Hastig, wenn Ihr's wi\u00dft,", "tokens": ["Eins", ",", "spricht", "Herr", "Has\u00b7tig", ",", "wenn", "Ih\u00b7r's", "wi\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "NE", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "M\u00f6cht' ich noch gerne h\u00f6ren:", "tokens": ["M\u00f6cht'", "ich", "noch", "ger\u00b7ne", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wei\u00df Nettchen schon, wie sch\u00f6n sie ist?", "tokens": ["Wei\u00df", "Nett\u00b7chen", "schon", ",", "wie", "sch\u00f6n", "sie", "ist", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das kann ich als ein guter Christ,", "tokens": ["Das", "kann", "ich", "als", "ein", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Versetzt Johann, beschw\u00f6ren.", "tokens": ["Ver\u00b7setzt", "Jo\u00b7hann", ",", "be\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Zum Henker! konnte denn mein Weib", "tokens": ["Zum", "Hen\u00b7ker", "!", "konn\u00b7te", "denn", "mein", "Weib"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ihr keinen bessern Zeitvertreib,", "tokens": ["Ihr", "kei\u00b7nen", "bes\u00b7sern", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als den von schmeichlerischen Laffen,", "tokens": ["Als", "den", "von", "schmeich\u00b7le\u00b7ri\u00b7schen", "Laf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Die immer Weihrauch streu'n, verschaffen?", "tokens": ["Die", "im\u00b7mer", "Weih\u00b7rauch", "streu'n", ",", "ver\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Gefehlt, Herr Hastig, spricht Johann,", "tokens": ["Ge\u00b7fehlt", ",", "Herr", "Has\u00b7tig", ",", "spricht", "Jo\u00b7hann", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "NE", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Madame wies jeden aus dem Hause,", "tokens": ["Ma\u00b7da\u00b7me", "wies", "je\u00b7den", "aus", "dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Der zu des M\u00e4dchens Ohrenschmause", "tokens": ["Der", "zu", "des", "M\u00e4d\u00b7chens", "Oh\u00b7ren\u00b7schmau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Das kleinste Schmeichel(wort?) begann.", "tokens": ["Das", "kleins\u00b7te", "Schmei\u00b7chel", "(", "wort", "?", ")", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "$.", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nur einer richtet Unheil an,", "tokens": ["Nur", "ei\u00b7ner", "rich\u00b7tet", "Un\u00b7heil", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Doch der ist einmal in dem Hause.", "tokens": ["Doch", "der", "ist", "ein\u00b7mal", "in", "dem", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Madame, das sag ich ungelogen,", "tokens": ["Ma\u00b7da\u00b7me", ",", "das", "sag", "ich", "un\u00b7ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Ist selber ihm gar sehr gewogen", "tokens": ["Ist", "sel\u00b7ber", "ihm", "gar", "sehr", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und l\u00e4\u00dft es ungestraft geschehn,", "tokens": ["Und", "l\u00e4\u00dft", "es", "un\u00b7ge\u00b7straft", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da\u00df es dem eitlen Nichtchen schmeichelt,", "tokens": ["Da\u00df", "es", "dem", "eit\u00b7len", "Nicht\u00b7chen", "schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Wenn diese mit ihm kost und \u00e4ugelt", "tokens": ["Wenn", "die\u00b7se", "mit", "ihm", "kost", "und", "\u00e4u\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "APPR", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und ihn um jedes Band befr\u00e4gt,", "tokens": ["Und", "ihn", "um", "je\u00b7des", "Band", "be\u00b7fr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Das sie an Kopf und Busen tr\u00e4gt.", "tokens": ["Das", "sie", "an", "Kopf", "und", "Bu\u00b7sen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Herr Hastig zieht die St ...", "tokens": ["Herr", "Has\u00b7tig", "zieht", "die", "St", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Aus seinen Augen ...", "tokens": ["Aus", "sei\u00b7nen", "Au\u00b7gen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "So eil und wirf den Schurken gleich", "tokens": ["So", "eil", "und", "wirf", "den", "Schur\u00b7ken", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVIMP", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Hinunter in den Gartenteich.", "tokens": ["Hin\u00b7un\u00b7ter", "in", "den", "Gar\u00b7ten\u00b7teich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Er hebt den Stock, um auszuholen.", "tokens": ["Er", "hebt", "den", "Stock", ",", "um", "aus\u00b7zu\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUI", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Erschreckt und fl\u00fcchtig eilt Johann", "tokens": ["Er\u00b7schreckt", "und", "fl\u00fcch\u00b7tig", "eilt", "Jo\u00b7hann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die schmale Wendeltrepp' hinan", "tokens": ["Die", "schma\u00b7le", "Wen\u00b7del\u00b7trepp'", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und tut, was ihm sein Herr befohlen.", "tokens": ["Und", "tut", ",", "was", "ihm", "sein", "Herr", "be\u00b7foh\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Nanette hat indes vernommen,", "tokens": ["Na\u00b7net\u00b7te", "hat", "in\u00b7des", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Der Oheim sei zur\u00fcckgekommen,", "tokens": ["Der", "O\u00b7heim", "sei", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Zum Freunde will sie nur erst eilen,", "tokens": ["Zum", "Freun\u00b7de", "will", "sie", "nur", "erst", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Ihm ihre Freude mitzuteilen", "tokens": ["Ihm", "ih\u00b7re", "Freu\u00b7de", "mit\u00b7zu\u00b7tei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Und findet \u2013 ach, die St\u00e4tte leer.", "tokens": ["Und", "fin\u00b7det", "\u2013", "ach", ",", "die", "St\u00e4t\u00b7te", "leer", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "XY", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Ihr Freund, ihr Liebling ist nicht mehr.", "tokens": ["Ihr", "Freund", ",", "ihr", "Lieb\u00b7ling", "ist", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Sie weint, sie klagt und h\u00f6rt beklommen,", "tokens": ["Sie", "weint", ",", "sie", "klagt", "und", "h\u00f6rt", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Wohin ihr treuer Freund gekommen,", "tokens": ["Wo\u00b7hin", "ihr", "treu\u00b7er", "Freund", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Sie eilt hinab mit tiefen Wehn,", "tokens": ["Sie", "eilt", "hin\u00b7ab", "mit", "tie\u00b7fen", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Dem Lieben trauernd nachzusehn.", "tokens": ["Dem", "Lie\u00b7ben", "trau\u00b7ernd", "nach\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Will dicht sie ...", "tokens": ["Will", "dicht", "sie", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.52": {"text": "Und lacht des Oheims strenger Wut.", "tokens": ["Und", "lacht", "des", "O\u00b7heims", "stren\u00b7ger", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.53": {"text": "Denn schmeichelnd h\u00e4lt die klare Flut", "tokens": ["Denn", "schmei\u00b7chelnd", "h\u00e4lt", "die", "kla\u00b7re", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Ihr einen andren Freund entgegen.", "tokens": ["Ihr", "ei\u00b7nen", "an\u00b7dren", "Freund", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nanettens Onkel kam zur\u00fcck", "tokens": ["Na\u00b7net\u00b7tens", "On\u00b7kel", "kam", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von einer langen Reise.", "tokens": ["Von", "ei\u00b7ner", "lan\u00b7gen", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Johann empf\