{"dta.poem.13136": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enur die\u00df war, Wertheste! dem hoch-erleuchten Krei\u00df", "tokens": ["\u201e", "nur", "die\u00df", "war", ",", "Wert\u00b7hes\u00b7te", "!", "dem", "hoch\u00b7er\u00b7leuch\u00b7ten", "Krei\u00df"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "PDS", "VAFIN", "$,", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "\u201evon den Verrichtungen des Kiels, von meinem Prei\u00df", "tokens": ["\u201e", "von", "den", "Ver\u00b7rich\u00b7tun\u00b7gen", "des", "Kiels", ",", "von", "mei\u00b7nem", "Prei\u00df"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201evon meiner Treflichkeit und Pflicht hier vorzutragen;", "tokens": ["\u201e", "von", "mei\u00b7ner", "Tref\u00b7lich\u00b7keit", "und", "Pflicht", "hier", "vor\u00b7zu\u00b7tra\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "KON", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "60\u201dMich d\u00fcnckt, man k\u00f6nne mir den Vorzug nicht versagen.", "tokens": ["\"", "Mich", "d\u00fcnckt", ",", "man", "k\u00f6n\u00b7ne", "mir", "den", "Vor\u00b7zug", "nicht", "ver\u00b7sa\u00b7gen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "