{"textgrid.poem.52687": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Melancholie", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freuet euch, ihr lieben Menschen,", "tokens": ["Freu\u00b7et", "euch", ",", "ihr", "lie\u00b7ben", "Men\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling ist es aller Orten!", "tokens": ["Fr\u00fch\u00b7ling", "ist", "es", "al\u00b7ler", "Or\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des Himmels blauen Pforten", "tokens": ["Aus", "des", "Him\u00b7mels", "blau\u00b7en", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt Liebe, Lust und Gl\u00fcck.", "tokens": ["Wan\u00b7delt", "Lie\u00b7be", ",", "Lust", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00f6hlichkeit ist aller Wegen,", "tokens": ["Fr\u00f6h\u00b7lich\u00b7keit", "ist", "al\u00b7ler", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Quellen munter springen,", "tokens": ["Wie", "die", "Quel\u00b7len", "mun\u00b7ter", "sprin\u00b7gen", ","], "token_info": ["word", "word