{"textgrid.poem.53486": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Pojaz spricht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, ich bin schon lange fern der Schminke,", "tokens": ["Mein", "Kind", ",", "ich", "bin", "schon", "lan\u00b7ge", "fern", "der", "Schmin\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gern denk ich dran, das war die bunte Zeit!", "tokens": ["gern", "denk", "ich", "dran", ",", "das", "war", "die", "bun\u00b7te", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich gab dem Personal die letzten Winke,", "tokens": ["Ich", "gab", "dem", "Per\u00b7so\u00b7nal", "die", "letz\u00b7ten", "Win\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dann trat ich auf; zwei Meter zwanzig breit,", "tokens": ["dann", "trat", "ich", "auf", ";", "zwei", "Me\u00b7ter", "zwan\u00b7zig", "breit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "CARD", "NN", "CARD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "auf meinem Hut sang ein Kanarip\u00e4rchen,", "tokens": ["auf", "mei\u00b7nem", "Hut", "sang", "ein", "Ka\u00b7na\u00b7ri\u00b7p\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "auf Rollen zog ich nach ein kleines Licht . . .", "tokens": ["auf", "Rol\u00b7len", "zog", "ich", "nach", "ein", "klei\u00b7nes", "Licht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und doch: betracht ich mir die letzten J\u00e4hrchen \u2013", "tokens": ["Und", "doch", ":", "be\u00b7tracht", "ich", "mir", "die", "letz\u00b7ten", "J\u00e4hr\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nein! solche Purzelb\u00e4ume schlug ich nicht!", "tokens": ["Nein", "!", "sol\u00b7che", "Pur\u00b7zel\u00b7b\u00e4u\u00b7me", "schlug", "ich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich war gewi\u00df mal eine dolle Nummer,", "tokens": ["Ich", "war", "ge\u00b7wi\u00df", "mal", "ei\u00b7ne", "dol\u00b7le", "Num\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "trieb meinen besten Freunden N\u00e4gel in den Bauch", "tokens": ["trieb", "mei\u00b7nen", "bes\u00b7t