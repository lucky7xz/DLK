{"textgrid.poem.31148": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Religionsphilosophie", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Herr, aus tiefer Noth", "tokens": ["O", "Herr", ",", "aus", "tie\u00b7fer", "Noth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schrei ich zu Dir hinauf:", "tokens": ["Schrei", "ich", "zu", "Dir", "hin\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gieb mir mein t\u00e4glich Brod", "tokens": ["Gieb", "mir", "mein", "t\u00e4g\u00b7lich", "Brod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und etwas Butter drauf!", "tokens": ["Und", "et\u00b7was", "But\u00b7ter", "drauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein St\u00fcckchen Leberwurst", "tokens": ["Ein", "St\u00fcck\u00b7chen", "Le\u00b7ber\u00b7wurst"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4r schliesslich auch nicht ohne;", "tokens": ["W\u00e4r", "schliess\u00b7lich", "auch", "nicht", "oh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PTKNEG", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du weisst, mein Teufelsdurst", "tokens": ["Du", "weisst", ",", "mein", "Teu\u00b7fels\u00b7durst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ist Deiner Sch\u00f6pfung Krone!", "tokens": ["Ist", "Dei\u00b7ner", "Sch\u00f6p\u00b7fung", "Kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "W\u00e4r nur mein alter Hut", "tokens": ["W\u00e4r", "nur", "mein", "al\u00b7ter", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht so entsetzlich schief;", "tokens": ["Nicht", "so", "ent\u00b7setz\u00b7lich", "schief", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du weisst nicht, wie das thut,", "tokens": ["Du", "weisst", "nicht", ",", "wie", "das", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch hier, hier brennt es tief!", "tokens": ["Doch", "hier", ",", "hier", "brennt", "es", "tief", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Flaus h\u00e4lt nur soso,", "tokens": ["Mein", "Flaus", "h\u00e4lt", "nur", "so\u00b7so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich wollt, er w\u00e4re w\u00e4rmer;", "tokens": ["Ich", "wollt", ",", "er", "w\u00e4\u00b7re", "w\u00e4r\u00b7mer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Winterpaletot", "tokens": ["Ein", "Win\u00b7ter\u00b7pa\u00b7le\u00b7tot"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Macht Dich doch auch nicht \u00e4rmer!", "tokens": ["Macht", "Dich", "doch", "auch", "nicht", "\u00e4r\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du siehst, mir fehlt noch viel,", "tokens": ["Du", "siehst", ",", "mir", "fehlt", "noch", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und meine Seele schreit,", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "schreit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich finde keinen Stil", "tokens": ["Ich", "fin\u00b7de", "kei\u00b7nen", "Stil"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vor lauter Fr\u00f6mmigkeit!", "tokens": ["Vor", "lau\u00b7ter", "Fr\u00f6m\u00b7mig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch sei's. Ich bin ein Mann", "tokens": ["Doch", "sei'", "s.", "Ich", "bin", "ein", "Mann"], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VVIMP", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und will mich nicht erdreisten,", "tokens": ["Und", "will", "mich", "nicht", "er\u00b7dreis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur musst Du dann und wann", "tokens": ["Nur", "musst", "Du", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "KON", "PWAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mir auch was Extra's leisten!", "tokens": ["Mir", "auch", "was", "Ex\u00b7tra's", "leis\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PWS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcr Kl\u00e4rchen einen Zopf,", "tokens": ["F\u00fcr", "Kl\u00e4r\u00b7chen", "ei\u00b7nen", "Zopf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Cul f\u00fcr meine Frau", "tokens": ["Ein", "Cul", "f\u00fcr", "mei\u00b7ne", "Frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Sonntags in den Topf", "tokens": ["Und", "Sonn\u00b7tags", "in", "den", "Topf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wom\u00f6glich eine Sau!", "tokens": ["Wo\u00b7m\u00f6g\u00b7lich", "ei\u00b7ne", "Sau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und l\u00e4sst Du einmal, geht's,", "tokens": ["Und", "l\u00e4sst", "Du", "ein\u00b7mal", ",", "geht's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mich Calculator werden,", "tokens": ["Mich", "Cal\u00b7cu\u00b7la\u00b7tor", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dann will ich Dir auch stets", "tokens": ["Dann", "will", "ich", "Dir", "auch", "stets"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Erkenntlich sein auf Erden!", "tokens": ["Er\u00b7kennt\u00b7lich", "sein", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann halt ich h\u00fcbsch den Mund", "tokens": ["Dann", "halt", "ich", "h\u00fcbsch", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bei andrer Spott und Hohn", "tokens": ["Bei", "an\u00b7drer", "Spott", "und", "Hohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gr\u00fcnde einen Bund", "tokens": ["Und", "gr\u00fcn\u00b7de", "ei\u00b7nen", "Bund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr innere Mission.", "tokens": ["F\u00fcr", "in\u00b7ne\u00b7re", "Mis\u00b7si\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mein Fritz muss f\u00fcrchterlich", "tokens": ["Mein", "Fritz", "muss", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VMFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Theologie studiren", "tokens": ["Theo\u00b7lo\u00b7gie", "stu\u00b7di\u00b7ren"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und schliesslich lass ich mich", "tokens": ["Und", "schliess\u00b7lich", "lass", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zum Kirchenrath creiren!", "tokens": ["Zum", "Kir\u00b7chen\u00b7rath", "crei\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch, wenn Du filzig bist,", "tokens": ["Doch", ",", "wenn", "Du", "fil\u00b7zig", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dann dank ich f\u00fcr die Kur;", "tokens": ["Dann", "dank", "ich", "f\u00fcr", "die", "Kur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann werd ich Atheist", "tokens": ["Dann", "werd", "ich", "A\u00b7theist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und w\u00e4hle bebel'sch nur!", "tokens": ["Und", "w\u00e4h\u00b7le", "be\u00b7bel'sch", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann mag Altar und Thron", "tokens": ["Dann", "mag", "Al\u00b7tar", "und", "Thron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur dreist zusammenbrechen,", "tokens": ["Nur", "dreist", "zu\u00b7sam\u00b7men\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dann werd ich Deinen Lohn", "tokens": ["Dann", "werd", "ich", "Dei\u00b7nen", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In Gold und Blut Dir blechen!", "tokens": ["In", "Gold", "und", "Blut", "Dir", "ble\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch, wie man's treibt, so geht's.", "tokens": ["Doch", ",", "wie", "man's", "treibt", ",", "so", "geht'", "s."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KON", "$,", "PWAV", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Loos w\u00e4gt Deine Hand,", "tokens": ["Mein", "Loos", "w\u00e4gt", "Dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eine w\u00e4scht ja stets", "tokens": ["Und", "ei\u00b7ne", "w\u00e4scht", "ja", "stets"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die andre hier zu Land.", "tokens": ["Die", "and\u00b7re", "hier", "zu", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So nimm mein Herz denn hin,", "tokens": ["So", "nimm", "mein", "Herz", "denn", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich will's Dir ja nicht schenken;", "tokens": ["Ich", "will's", "Dir", "ja", "nicht", "schen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dass ich Gesch\u00e4ftsmann bin,", "tokens": ["Dass", "ich", "Ge\u00b7sch\u00e4fts\u00b7mann", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wirst Du mir nicht verdenken!", "tokens": ["Wirst", "Du", "mir", "nicht", "ver\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drum, Herr, aus tiefer Noth", "tokens": ["Drum", ",", "Herr", ",", "aus", "tie\u00b7fer", "Noth"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schrei ich zu Dir hinauf:", "tokens": ["Schrei", "ich", "zu", "Dir", "hin\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gieb mir mein t\u00e4glich Brod", "tokens": ["Gieb", "mir", "mein", "t\u00e4g\u00b7lich", "Brod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und etwas Butter drauf!", "tokens": ["Und", "et\u00b7was", "But\u00b7ter", "drauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein St\u00fcckchen Leberwurst", "tokens": ["Ein", "St\u00fcck\u00b7chen", "Le\u00b7ber\u00b7wurst"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4r schliesslich auch nicht ohne,", "tokens": ["W\u00e4r", "schliess\u00b7lich", "auch", "nicht", "oh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PTKNEG", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du weisst, mein Teufelsdurst", "tokens": ["Du", "weisst", ",", "mein", "Teu\u00b7fels\u00b7durst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ist Deiner Sch\u00f6pfung Krone!", "tokens": ["Ist", "Dei\u00b7ner", "Sch\u00f6p\u00b7fung", "Kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "