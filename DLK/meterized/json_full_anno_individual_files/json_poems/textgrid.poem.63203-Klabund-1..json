{"textgrid.poem.63203": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuerst rannte ich mit dem Kopf gegen die Wand", "tokens": ["Zu\u00b7erst", "rann\u00b7te", "ich", "mit", "dem", "Kopf", "ge\u00b7gen", "die", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und r\u00fcttelte an den St\u00e4ben.", "tokens": ["Und", "r\u00fct\u00b7tel\u00b7te", "an", "den", "St\u00e4\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich verfluchte Tod und Leben", "tokens": ["Ich", "ver\u00b7fluch\u00b7te", "Tod", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und steckte mit meinem feurigen Blick das ganze Gef\u00e4ngnis in Brand.", "tokens": ["Und", "steck\u00b7te", "mit", "mei\u00b7nem", "feu\u00b7ri\u00b7gen", "Blick", "das", "gan\u00b7ze", "Ge\u00b7f\u00e4ng\u00b7nis", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+-+--+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Das vergitterte Fenster oben war blind und klein.", "tokens": ["Das", "ver\u00b7git\u00b7ter\u00b7te", "Fens\u00b7ter", "o\u00b7ben", "war", "blind", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ich wusste nie, ob die Sonne schien oder Regen.", "tokens": ["Ich", "wuss\u00b7te", "nie", ",", "ob", "die", "Son\u00b7ne", "schien", "o\u00b7der", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ich hungerte und hatte tausend M\u00e4gen,", "tokens": ["Ich", "hun\u00b7ger\u00b7te", "und", "hat\u00b7te", "tau\u00b7send", "M\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ich wollte so gerne mein eigener Enkel sein.", "tokens": ["Und", "ich", "woll\u00b7te", "so", "ger\u00b7ne", "mein", "ei\u00b7ge\u00b7ner", "En\u00b7kel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Eine Schale Suppe ist durch die T\u00fcr geschwebt.", "tokens": ["Ei\u00b7ne", "Scha\u00b7le", "Sup\u00b7pe", "ist", "durch", "die", "T\u00fcr", "ge\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Ich habe wie ein hungriger Menageriel\u00f6we gebebt.", "tokens": ["Ich", "ha\u00b7be", "wie", "ein", "hung\u00b7ri\u00b7ger", "Me\u00b7na\u00b7ge\u00b7rie\u00b7l\u00f6\u00b7we", "ge\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Einmal ging ein Frauenschritt auf dem Gang vor\u00fcber. Der Schritt einer K\u00f6nigin.", "tokens": ["Ein\u00b7mal", "ging", "ein", "Frau\u00b7en\u00b7schritt", "auf", "dem", "Gang", "vor\u00b7\u00fc\u00b7ber", ".", "Der", "Schritt", "ei\u00b7ner", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+-+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.12": {"text": "Schliesslich bin ich davon \u00fcberzeugt, dass ich ein Verbrecher sei,", "tokens": ["Schliess\u00b7lich", "bin", "ich", "da\u00b7von", "\u00fc\u00b7berz\u00b7eugt", ",", "dass", "ich", "ein", "Ver\u00b7bre\u00b7cher", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PAV", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+--+---+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Und dass ich mit vollem Recht unsch\u00e4dlich gemacht bin.", "tokens": ["Und", "dass", "ich", "mit", "vol\u00b7lem", "Recht", "un\u00b7sch\u00e4d\u00b7lich", "ge\u00b7macht", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Ich dulde es, dass ich vom W\u00e4rter verlacht bin,", "tokens": ["Ich", "dul\u00b7de", "es", ",", "dass", "ich", "vom", "W\u00e4r\u00b7ter", "ver\u00b7lacht", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.15": {"text": "Und ich f\u00fchle, dass er so etwas wie ein Cherubim mit Flammenschwert und meiner Taten R\u00e4cher sei.", "tokens": ["Und", "ich", "f\u00fch\u00b7le", ",", "dass", "er", "so", "et\u00b7was", "wie", "ein", "Che\u00b7ru\u00b7bim", "mit", "Flam\u00b7men\u00b7schwert", "und", "mei\u00b7ner", "Ta\u00b7ten", "R\u00e4\u00b7cher", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIS", "KOKOM", "ART", "NE", "APPR", "NE", "KON", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "--+--+-+-+-+-+-+-+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Einmal wird die T\u00fcr sich \u00f6ffnen und wie eine Gnade", "tokens": ["Ein\u00b7mal", "wird", "die", "T\u00fcr", "sich", "\u00f6ff\u00b7nen", "und", "wie", "ei\u00b7ne", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "VVINF", "KON", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Wird mir die edle Freiheit wieder von Gott gew\u00e4hrt.", "tokens": ["Wird", "mir", "die", "ed\u00b7le", "Frei\u00b7heit", "wie\u00b7der", "von", "Gott", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Ich st\u00fcrze sofort in ein erstklassiges Hotel und bade", "tokens": ["Ich", "st\u00fcr\u00b7ze", "so\u00b7fort", "in", "ein", "erst\u00b7klas\u00b7si\u00b7ges", "Ho\u00b7tel", "und", "ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Und gehe in die Reitschule und besteige mein Lieblingspferd.", "tokens": ["Und", "ge\u00b7he", "in", "die", "Reit\u00b7schu\u00b7le", "und", "be\u00b7stei\u00b7ge", "mein", "Lieb\u00b7lings\u00b7pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Ich glaube, es hiess Mimi, wie das zarte M\u00e4dchen in dem bekannten Boh\u00e8me-Romane,", "tokens": ["Ich", "glau\u00b7be", ",", "es", "hiess", "Mi\u00b7mi", ",", "wie", "das", "zar\u00b7te", "M\u00e4d\u00b7chen", "in", "dem", "be\u00b7kann\u00b7ten", "Boh\u00e8\u00b7me\u00b7Ro\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "NE", "$,", "PWAV", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+--+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.21": {"text": "Und ich jage durch den englischen Garten und reite durch Felder von Korn und Mohn,", "tokens": ["Und", "ich", "ja\u00b7ge", "durch", "den", "eng\u00b7li\u00b7schen", "Gar\u00b7ten", "und", "rei\u00b7te", "durch", "Fel\u00b7der", "von", "Korn", "und", "Mohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"