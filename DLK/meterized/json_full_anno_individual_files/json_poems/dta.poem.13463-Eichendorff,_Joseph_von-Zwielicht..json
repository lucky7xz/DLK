{"dta.poem.13463": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zwielicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4mmrung will die Fl\u00fcgel spreiten,", "tokens": ["D\u00e4mm\u00b7rung", "will", "die", "Fl\u00fc\u00b7gel", "sprei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaurig r\u00fchren sich die B\u00e4ume,", "tokens": ["Schau\u00b7rig", "r\u00fch\u00b7ren", "sich", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolken zieh'n wie schwere Tr\u00e4ume \u2014", "tokens": ["Wol\u00b7ken", "zieh'n", "wie", "schwe\u00b7re", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was will dieses Grau'n bedeuten?", "tokens": ["Was", "will", "die\u00b7ses", "Grau'n", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast ein Reh du, lieb vor andern,", "tokens": ["Hast", "ein", "Reh", "du", ",", "lieb", "vor", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "ADJD", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df es nicht alleine grasen,", "tokens": ["La\u00df", "es", "nicht", "al\u00b7lei\u00b7ne", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "J\u00e4ger zieh'n im Wald' und blasen,", "tokens": ["J\u00e4\u00b7ger", "zieh'n", "im", "Wald'", "und", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stimmen hin und wieder wandern.", "tokens": ["Stim\u00b7men", "hin", "und", "wie\u00b7der", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hast du einen Freund hienieden,", "tokens": ["Hast", "du", "ei\u00b7nen", "Freund", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trau ihm nicht zu dieser Stunde,", "tokens": ["Trau", "ihm", "nicht", "zu", "die\u00b7ser", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundlich wohl mit Aug' und Munde,", "tokens": ["Freund\u00b7lich", "wohl", "mit", "Aug'", "und", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinnt er Krieg im t\u00fcck'schen Frieden.", "tokens": ["Sinnt", "er", "Krieg", "im", "t\u00fc\u00b7ck'\u00b7schen", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Was heut m\u00fcde gehet unter,", "tokens": ["Was", "heut", "m\u00fc\u00b7de", "ge\u00b7het", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt sich morgen neugeboren.", "tokens": ["Hebt", "sich", "mor\u00b7gen", "neu\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches bleibt in Nacht verloren \u2014", "tokens": ["Man\u00b7ches", "bleibt", "in", "Nacht", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcte dich, bleib' wach und munter!", "tokens": ["H\u00fc\u00b7te", "dich", ",", "bleib'", "wach", "und", "mun\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}