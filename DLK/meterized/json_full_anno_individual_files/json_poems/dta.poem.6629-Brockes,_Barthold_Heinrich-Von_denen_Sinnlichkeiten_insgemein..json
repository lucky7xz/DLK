{"dta.poem.6629": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch sonder hier von Thieren viel zu sagen", "tokens": ["Doch", "son\u00b7der", "hier", "von", "Thie\u00b7ren", "viel", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sonder Epicur um Rath zu fragen:", "tokens": ["Und", "son\u00b7der", "E\u00b7pi\u00b7cur", "um", "Rath", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob sie uns gleich? Lasst itzt die Dunckelheit", "tokens": ["Ob", "sie", "uns", "gleich", "?", "Lasst", "itzt", "die", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von dieser Frag\u2019 auf eine andre Zeit", "tokens": ["Von", "die\u00b7ser", "Frag'", "auf", "ei\u00b7ne", "and\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und einen andern Ort noch ausgesetzet seyn.", "tokens": ["Und", "ei\u00b7nen", "an\u00b7dern", "Ort", "noch", "aus\u00b7ge\u00b7set\u00b7zet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir reden itzo blo\u00df von Uns allein.", "tokens": ["Wir", "re\u00b7den", "it\u00b7zo", "blo\u00df", "von", "Uns", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}