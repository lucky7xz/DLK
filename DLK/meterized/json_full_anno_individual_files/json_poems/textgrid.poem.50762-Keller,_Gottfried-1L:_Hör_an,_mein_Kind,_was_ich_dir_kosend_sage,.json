{"textgrid.poem.50762": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6r an, mein Kind, was ich dir kosend sage,", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r an, mein Kind, was ich dir kosend sage,", "tokens": ["H\u00f6r", "an", ",", "mein", "Kind", ",", "was", "ich", "dir", "ko\u00b7send", "sa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PWS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie mich ein Traum betrog so wunderbar:", "tokens": ["Wie", "mich", "ein", "Traum", "be\u00b7trog", "so", "wun\u00b7der\u00b7bar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es war an einem stillen Feiertage,", "tokens": ["Es", "war", "an", "ei\u00b7nem", "stil\u00b7len", "Fei\u00b7er\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ich mit dir bei Gott im Himmel war.", "tokens": ["Als", "ich", "mit", "dir", "bei", "Gott", "im", "Him\u00b7mel", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er schaute eben noch vom Taubenschlage", "tokens": ["Er", "schau\u00b7te", "e\u00b7ben", "noch", "vom", "Tau\u00b7ben\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus in die Sonntagswelt, so weit und klar,", "tokens": ["Aus", "in", "die", "Sonn\u00b7tags\u00b7welt", ",", "so", "weit", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ob dem fernen Glockenklang allm\u00e4hlich", "tokens": ["Und", "ob", "dem", "fer\u00b7nen", "Glo\u00b7cken\u00b7klang", "all\u00b7m\u00e4h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Entschlief er auf ein St\u00fcndchen sanft und selig.", "tokens": ["Ent\u00b7schlief", "er", "auf", "ein", "St\u00fcnd\u00b7chen", "sanft", "und", "se\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man h\u00f6rte kaum die Menschen unten singen,", "tokens": ["Man", "h\u00f6r\u00b7te", "kaum", "die", "Men\u00b7schen", "un\u00b7ten", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Himmel aber war es still und leer;", "tokens": ["Im", "Him\u00b7mel", "a\u00b7ber", "war", "es", "still", "und", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur an der Sternenuhr das Pendelschwingen", "tokens": ["Nur", "an", "der", "Ster\u00b7ne\u00b7nuhr", "das", "Pen\u00b7del\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Klang langsam und gemessen hin und her,", "tokens": ["Klang", "lang\u00b7sam", "und", "ge\u00b7mes\u00b7sen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVPP", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und m\u00e4uschenstill, in seligem Umschlingen,", "tokens": ["Und", "m\u00e4u\u00b7schen\u00b7still", ",", "in", "se\u00b7li\u00b7gem", "Um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sah ich in deines Augs urtiefes Meer;", "tokens": ["Sah", "ich", "in", "dei\u00b7nes", "Augs", "ur\u00b7tie\u00b7fes", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da hatte pl\u00f6tzlich ich den Mut gefunden:", "tokens": ["Da", "hat\u00b7te", "pl\u00f6tz\u00b7lich", "ich", "den", "Mut", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bat um den ersten Ku\u00df dich unumwunden!", "tokens": ["Bat", "um", "den", "ers\u00b7ten", "Ku\u00df", "dich", "un\u00b7um\u00b7wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbum dreie von den Sternen, die dort schweben,", "tokens": ["\u00bb", "um", "drei\u00b7e", "von", "den", "Ster\u00b7nen", ",", "die", "dort", "schwe\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "CARD", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geb ich dir, Lieber, meinen ersten Ku\u00df!\u00ab", "tokens": ["Geb", "ich", "dir", ",", "Lie\u00b7ber", ",", "mei\u00b7nen", "ers\u00b7ten", "Ku\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sagtest l\u00e4chelnd du, mein s\u00fc\u00dfes Leben;", "tokens": ["So", "sag\u00b7test", "l\u00e4\u00b7chelnd", "du", ",", "mein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich aber eilte, schon im Vorgenu\u00df,", "tokens": ["Ich", "a\u00b7ber", "eil\u00b7te", ",", "schon", "im", "Vor\u00b7ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Goldnen aus den Angeln zu erheben,", "tokens": ["Die", "Gold\u00b7nen", "aus", "den", "An\u00b7geln", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und brachte sechse dir zum \u00dcberflu\u00df;", "tokens": ["Und", "brach\u00b7te", "sech\u00b7se", "dir", "zum", "\u00dc\u00b7berf\u00b7lu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du aber drauf: \u00bbWie mich die Dinger laben!", "tokens": ["Du", "a\u00b7ber", "drauf", ":", "\u00bb", "Wie", "mich", "die", "Din\u00b7ger", "la\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$.", "$(", "PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um noch zw\u00f6lf andre sollst den Ku\u00df du haben!\u00ab", "tokens": ["Um", "noch", "zw\u00f6lf", "and\u00b7re", "sollst", "den", "Ku\u00df", "du", "ha\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ADV", "CARD", "PIS", "VMFIN", "ART", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So ging es fort; verdoppelt immer wieder", "tokens": ["So", "ging", "es", "fort", ";", "ver\u00b7dop\u00b7pelt", "im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erh\u00f6htest du den teuren Liebespreis;", "tokens": ["Er\u00b7h\u00f6h\u00b7test", "du", "den", "teu\u00b7ren", "Lie\u00b7be\u00b7spreis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und zwiefach d\u00fcrstend holte ich hernieder", "tokens": ["Und", "zwie\u00b7fach", "d\u00fcrs\u00b7tend", "hol\u00b7te", "ich", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir Stern um Stern aus ihrer Br\u00fcder Kreis.", "tokens": ["Dir", "Stern", "um", "Stern", "aus", "ih\u00b7rer", "Br\u00fc\u00b7der", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du schm\u00fccktest emsig deine sch\u00f6nen Glieder,", "tokens": ["Du", "schm\u00fcck\u00b7test", "em\u00b7sig", "dei\u00b7ne", "sch\u00f6\u00b7nen", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verlachend heimlich meinen hei\u00dfen Flei\u00df;", "tokens": ["Ver\u00b7la\u00b7chend", "heim\u00b7lich", "mei\u00b7nen", "hei\u00b7\u00dfen", "Flei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und zu erkaufen meine h\u00f6chste Wonne,", "tokens": ["Und", "zu", "er\u00b7kau\u00b7fen", "mei\u00b7ne", "h\u00f6chs\u00b7te", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Blieb mir am Ende nur noch Mond und Sonne!", "tokens": ["Blieb", "mir", "am", "En\u00b7de", "nur", "noch", "Mond", "und", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich brachte sie; und um die Stirne hingest", "tokens": ["Ich", "brach\u00b7te", "sie", ";", "und", "um", "die", "Stir\u00b7ne", "hin\u00b7gest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die helle Sonne du mit stolzer Lust,", "tokens": ["Die", "hel\u00b7le", "Son\u00b7ne", "du", "mit", "stol\u00b7zer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Sternen du den Schwanenhals umfingest,", "tokens": ["Mit", "Ster\u00b7nen", "du", "den", "Schwa\u00b7nen\u00b7hals", "um\u00b7fin\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mond erstrahlte mild an deiner Brust;", "tokens": ["Der", "Mond", "er\u00b7strahl\u00b7te", "mild", "an", "dei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann himmelauf und -ab du dich ergingest,", "tokens": ["Dann", "him\u00b7me\u00b7lauf", "und", "du", "dich", "er\u00b7gin\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "NE", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "All deiner Sch\u00f6nheit siegreich dir bewu\u00dft!", "tokens": ["All", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "sieg\u00b7reich", "dir", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von dir allein nun str\u00f6mte alle Helle,", "tokens": ["Von", "dir", "al\u00b7lein", "nun", "str\u00f6m\u00b7te", "al\u00b7le", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich lag vor dir als vor des Lichtes Quelle!", "tokens": ["Ich", "lag", "vor", "dir", "als", "vor", "des", "Lich\u00b7tes", "Quel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Himmel ruhte noch im tiefsten Schweigen,", "tokens": ["Der", "Him\u00b7mel", "ruh\u00b7te", "noch", "im", "tiefs\u00b7ten", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie vor dem J\u00fcngsten Tag ein stilles Grab;", "tokens": ["Wie", "vor", "dem", "J\u00fcng\u00b7sten", "Tag", "ein", "stil\u00b7les", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und eben wolltest du dich selig neigen,", "tokens": ["Und", "e\u00b7ben", "woll\u00b7test", "du", "dich", "se\u00b7lig", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ger\u00fchrt, bezwungen, sanft auf mich herab,", "tokens": ["Ge\u00b7r\u00fchrt", ",", "be\u00b7zwun\u00b7gen", ",", "sanft", "auf", "mich", "her\u00b7ab", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "ADJD", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die s\u00fc\u00dfe Gunst mir endlich zu erzeigen,", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Gunst", "mir", "end\u00b7lich", "zu", "er\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wof\u00fcr ich Sterne, Sonn und Mond dir gab:", "tokens": ["Wo\u00b7f\u00fcr", "ich", "Ster\u00b7ne", ",", "Sonn", "und", "Mond", "dir", "gab", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "$,", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da brach ein Angstschrei durch des Himmels Hallen,", "tokens": ["Da", "brach", "ein", "Angst\u00b7schrei", "durch", "des", "Him\u00b7mels", "Hal\u00b7len", ","], "token_info": ["word", "wor