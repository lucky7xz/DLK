{"textgrid.poem.61790": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Deprekation", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets von allem Gesch\u00e4ft in der Welt das verha\u00dfteste war mir,", "tokens": ["Stets", "von", "al\u00b7lem", "Ge\u00b7sch\u00e4ft", "in", "der", "Welt", "das", "ver\u00b7ha\u00df\u00b7tes\u00b7te", "war", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "NN", "APPR", "ART", "NN", "ART", "ADJA", "VAFIN", "PPER", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Briefe zu schreiben. So leicht mir das Wort in lebendiger Rede", "tokens": ["Brie\u00b7fe", "zu", "schrei\u00b7ben", ".", "So", "leicht", "mir", "das", "Wort", "in", "le\u00b7ben\u00b7di\u00b7ger", "Re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$.", "ADV", "ADJD", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Flie\u00dft, wenn die Sache mich reizt, so schwer entstr\u00f6mt es der Feder,", "tokens": ["Flie\u00dft", ",", "wenn", "die", "Sa\u00b7che", "mich", "reizt", ",", "so", "schwer", "ent\u00b7str\u00f6mt", "es", "der", "Fe\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Langsam, br\u00fcchig und kalt, als ob auf dem l\u00e4ngeren Umweg", "tokens": ["Lang\u00b7sam", ",", "br\u00fc\u00b7chig", "und", "kalt", ",", "als", "ob", "auf", "dem", "l\u00e4n\u00b7ge\u00b7ren", "Um\u00b7weg"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "KOKOM", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Aus dem Herzen aufs Blatt mir Gef\u00fchl und Gedanke gefr\u00f6ren.", "tokens": ["Aus", "dem", "Her\u00b7zen", "aufs", "Blatt", "mir", "Ge\u00b7f\u00fchl", "und", "Ge\u00b7dan\u00b7ke", "ge\u00b7fr\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Kaum, da\u00df ich munter begann, gleich blickt die verw\u00fcnschte Kritik mir", "tokens": ["Kaum", ",", "da\u00df", "ich", "mun\u00b7ter", "be\u00b7gann", ",", "gleich", "blickt", "die", "ver\u00b7w\u00fcnschte", "Kri\u00b7tik", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "\u00dcber die Schulter herein, und den Ausdruck allzu bedenklich", "tokens": ["\u00dc\u00b7ber", "die", "Schul\u00b7ter", "her\u00b7ein", ",", "und", "den", "Aus\u00b7druck", "all\u00b7zu", "be\u00b7denk\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KON", "ART", "NN", "PTKA", "ADJD"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.8": {"text": "W\u00e4gend verpfusch' ich ihn leicht zu farblos steifer Korrektheit,", "tokens": ["W\u00e4\u00b7gend", "ver\u00b7pfusch'", "ich", "ihn", "leicht", "zu", "farb\u00b7los", "stei\u00b7fer", "Kor\u00b7rekt\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADJD", "PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+--", "measure": "elegiambus"}, "line.9": {"text": "Statt im behaglichen Flu\u00df frischweg von der Leber zu plaudern", "tokens": ["Statt", "im", "be\u00b7hag\u00b7li\u00b7chen", "Flu\u00df", "frischweg", "von", "der", "Le\u00b7ber", "zu", "plau\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Ganz, wie der Schnabel mir wuchs. Zum Teil wohl hab' ich's vom Vater,", "tokens": ["Ganz", ",", "wie", "der", "Schna\u00b7bel", "mir", "wuchs", ".", "Zum", "Teil", "wohl", "hab'", "ich's", "vom", "Va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$.", "APPRART", "NN", "ADV", "VAFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Der, ob Meister des Worts, sich besann, zwei Zeilen der Post nur", "tokens": ["Der", ",", "ob", "Meis\u00b7ter", "des", "Worts", ",", "sich", "be\u00b7sann", ",", "zwei", "Zei\u00b7len", "der", "Post", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "NN", "ART", "NN", "$,", "PRF", "VVFIN", "$,", "CARD", "NN", "ART", "NN", "ADV"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Anzuvertraun, und, an Freundschaft reich, nie Briefe gewechselt.", "tokens": ["An\u00b7zu\u00b7ver\u00b7traun", ",", "und", ",", "an", "Freund\u00b7schaft", "reich", ",", "nie", "Brie\u00b7fe", "ge\u00b7wech\u00b7selt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "$,", "APPR", "NN", "ADJD", "$,", "ADV", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Drum dafern ihr im Ernst, wie ihr sagt, mir freundlich gesinnt seid,", "tokens": ["Drum", "da\u00b7fern", "ihr", "im", "Ernst", ",", "wie", "ihr", "sagt", ",", "mir", "freund\u00b7lich", "ge\u00b7sinnt", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Dr\u00e4ngt unn\u00f6tig mich nicht zum Schreiben und fordert insonders", "tokens": ["Dr\u00e4ngt", "un\u00b7n\u00f6\u00b7tig", "mich", "nicht", "zum", "Schrei\u00b7ben", "und", "for\u00b7dert", "in\u00b7son\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PTKNEG", "APPRART", "NN", "KON", "VVFIN", "ADV"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Antwort nicht auf jedes Gef\u00fchl. Gern send' ich euch Auskunft,", "tokens": ["Ant\u00b7wort", "nicht", "auf", "je\u00b7des", "Ge\u00b7f\u00fchl", ".", "Gern", "send'", "ich", "euch", "Aus\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PIAT", "NN", "$.", "NN", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "B\u00fcndige, gilt's ein Gesch\u00e4ft, doch zu brieflicher Herzensergie\u00dfung", "tokens": ["B\u00fcn\u00b7di\u00b7ge", ",", "gilt's", "ein", "Ge\u00b7sch\u00e4ft", ",", "doch", "zu", "brief\u00b7li\u00b7cher", "Her\u00b7zen\u00b7ser\u00b7gie\u00b7\u00dfung"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Fehlt mir f\u00fcrwahr das Geschick und fehlt vor allem die Neigung.", "tokens": ["Fehlt", "mir", "f\u00fcr\u00b7wahr", "das", "Ge\u00b7schick", "und", "fehlt", "vor", "al\u00b7lem", "die", "Nei\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "KON", "VVFIN", "APPR", "PIS", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "\u2013 \u00bbAber es gl\u00fcckte dir doch manch Lied; wie darfst du behaupten,", "tokens": ["\u2013", "\u00bb", "A\u00b7ber", "es", "gl\u00fcck\u00b7te", "dir", "doch", "manch", "Lied", ";", "wie", "darfst", "du", "be\u00b7haup\u00b7ten", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "KON", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Da\u00df dir die kleinere M\u00fche zu viel?\u00ab \u2013 Nun, jeglicher hat ja", "tokens": ["Da\u00df", "dir", "die", "klei\u00b7ne\u00b7re", "M\u00fc\u00b7he", "zu", "viel", "?", "\u00ab", "\u2013", "Nun", ",", "jeg\u00b7li\u00b7cher", "hat", "ja"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PIS", "$.", "$(", "$(", "ADV", "$,", "ADV", "VAFIN", "ADV"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Seine Begabung f\u00fcr sich, und der schnell hinschie\u00dfende Habicht", "tokens": ["Sei\u00b7ne", "Be\u00b7ga\u00b7bung", "f\u00fcr", "sich", ",", "und", "der", "schnell", "hin\u00b7schie\u00b7\u00dfen\u00b7de", "Ha\u00b7bicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "$,", "KON", "ART", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Ist schwerf\u00e4llig zu Fu\u00df. Niemals auch hab' ich am Schreibtisch", "tokens": ["Ist", "schwer\u00b7f\u00e4l\u00b7lig", "zu", "Fu\u00df", ".", "Nie\u00b7mals", "auch", "hab'", "ich", "am", "Schreib\u00b7tisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "$.", "ADV", "ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "M\u00fchsam, was ich gesungen, erdacht. Stets kam es von selbst mir,", "tokens": ["M\u00fch\u00b7sam", ",", "was", "ich", "ge\u00b7sun\u00b7gen", ",", "er\u00b7dacht", ".", "Stets", "kam", "es", "von", "selbst", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVPP", "$,", "VVPP", "$.", "ADV", "VVFIN", "PPER", "APPR", "ADV", "PPER", "$,"], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.6": {"text": "Drau\u00dfen im Freien, auf schweifendem Gang, wenn der Odem des Fr\u00fchlings", "tokens": ["Drau\u00b7\u00dfen", "im", "Frei\u00b7en", ",", "auf", "schwei\u00b7fen\u00b7dem", "Gang", ",", "wenn", "der", "O\u00b7dem", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Leis hinzog durch den Wald, mich bezaubernd, oder zur Herbstzeit,", "tokens": ["Leis", "hin\u00b7zog", "durch", "den", "Wald", ",", "mich", "be\u00b7zau\u00b7bernd", ",", "o\u00b7der", "zur", "Herbst\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVPP", "$,", "KON", "APPRART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Wenn von den Wipfeln das Laub sacht rieselte, goldenen Tr\u00e4nen", "tokens": ["Wenn", "von", "den", "Wip\u00b7feln", "das", "Laub", "sacht", "rie\u00b7sel\u00b7te", ",", "gol\u00b7de\u00b7nen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "\u00c4hnlich, und tief im Gem\u00fct die entschlummerte Schwermut weckte.", "tokens": ["\u00c4hn\u00b7lich", ",", "und", "tief", "im", "Ge\u00b7m\u00fct", "die", "ent\u00b7schlum\u00b7mer\u00b7te", "Schwer\u00b7mut", "weck\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.10": {"text": "Oder im Bette, des Nachts, aufd\u00e4mmert' es mir, und am Morgen", "tokens": ["O\u00b7der", "im", "Bet\u00b7te", ",", "des", "Nachts", ",", "auf\u00b7d\u00e4m\u00b7mert'", "es", "mir", ",", "und", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "ART", "ADV", "$,", "VVFIN", "PPER", "PPER", "$,", "KON", "APPRART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.11": {"text": "War es zu Rhythmen erbl\u00fcht, und fertig schrieb ich es nieder.", "tokens": ["War", "es", "zu", "Rhyth\u00b7men", "er\u00b7bl\u00fcht", ",", "und", "fer\u00b7tig", "schrieb", "ich", "es", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$,", "KON", "ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Freilich \u00e4ndert' ich wohl mit Bedacht, und die Feile des K\u00fcnstlers", "tokens": ["Frei\u00b7lich", "\u00e4n\u00b7dert'", "ich", "wohl", "mit", "Be\u00b7dacht", ",", "und", "die", "Fei\u00b7le", "des", "K\u00fcnst\u00b7lers"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Braucht' ich mit Flei\u00df, doch zuvor in geheimnisvoller Empf\u00e4ngnis", "tokens": ["Braucht'", "ich", "mit", "Flei\u00df", ",", "doch", "zu\u00b7vor", "in", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Emp\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Ward mir immer das Beste zuteil als himmlische Gabe.", "tokens": ["Ward", "mir", "im\u00b7mer", "das", "Bes\u00b7te", "zu\u00b7teil", "als", "himm\u00b7li\u00b7sche", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nie willk\u00fcrlich darum, wenn die innere N\u00f6tigung ausblieb,", "tokens": ["Nie", "will\u00b7k\u00fcr\u00b7lich", "da\u00b7rum", ",", "wenn", "die", "in\u00b7ne\u00b7re", "N\u00f6\u00b7ti\u00b7gung", "aus\u00b7blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--++-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Hab' ich zu dichten gewu\u00dft, auf Begehr, wie der Meister des Handwerks", "tokens": ["Hab'", "ich", "zu", "dich\u00b7ten", "ge\u00b7wu\u00dft", ",", "auf", "Be\u00b7gehr", ",", "wie", "der", "Meis\u00b7ter", "des", "Hand\u00b7werks"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "VVPP", "$,", "APPR", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Rasch das Verlangte beschafft, zu Geburtstagsfeier und Hochzeit", "tokens": ["Rasch", "das", "Ver\u00b7lang\u00b7te", "be\u00b7schafft", ",", "zu", "Ge\u00b7burts\u00b7tags\u00b7fei\u00b7er", "und", "Hoch\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Oder zum Neujahrsgru\u00df. Und versucht' ich es dennoch, der Bitte", "tokens": ["O\u00b7der", "zum", "Neu\u00b7jahrs\u00b7gru\u00df", ".", "Und", "ver\u00b7sucht'", "ich", "es", "den\u00b7noch", ",", "der", "Bit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "$.", "KON", "VVFIN", "PPER", "PPER", "ADV", "$,", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Weichend, so ward es darnach: ein zusammengestoppeltes Machwerk", "tokens": ["Wei\u00b7chend", ",", "so", "ward", "es", "dar\u00b7nach", ":", "ein", "zu\u00b7sam\u00b7men\u00b7ge\u00b7stop\u00b7pel\u00b7tes", "Mach\u00b7werk"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADV", "VAFIN", "PPER", "PAV", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Statt des lebendigen Lieds. Nur wenn in begl\u00fcckender Stunde,", "tokens": ["Statt", "des", "le\u00b7ben\u00b7di\u00b7gen", "Lieds", ".", "Nur", "wenn", "in", "be\u00b7gl\u00fc\u00b7cken\u00b7der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "ADV", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+--+---+-+-+--+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Wie sie dem Alternden, ach, nur noch selten erscheint und im Fluge,", "tokens": ["Wie", "sie", "dem", "Al\u00b7tern\u00b7den", ",", "ach", ",", "nur", "noch", "sel\u00b7ten", "er\u00b7scheint", "und", "im", "Flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "ITJ", "$,", "ADV", "ADV", "ADJD", "VVFIN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Mir freiwillig die Muse genaht, da vermocht' ich zu schaffen,", "tokens": ["Mir", "frei\u00b7wil\u00b7lig", "die", "Mu\u00b7se", "ge\u00b7naht", ",", "da", "ver\u00b7mocht'", "ich", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Was mich selber erfreut' und vielleicht auch anderen echt schien.", "tokens": ["Was", "mich", "sel\u00b7ber", "er\u00b7freut'", "und", "viel\u00b7leicht", "auch", "an\u00b7de\u00b7ren", "echt", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "ADV", "ADV", "PIS", "ADJD", "VVFIN", "$."], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Stets von allem Gesch\u00e4ft in der Welt das verha\u00dfteste war mir,", "tokens": ["Stets", "von", "al\u00b7lem", "Ge\u00b7sch\u00e4ft", "in", "der", "Welt", "das", "ver\u00b7ha\u00df\u00b7tes\u00b7te", "war", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "NN", "APPR", "ART", "NN", "ART", "ADJA", "VAFIN", "PPER", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Briefe zu schreiben. So leicht mir das Wort in lebendiger Rede", "tokens": ["Brie\u00b7fe", "zu", "schrei\u00b7ben", ".", "So", "leicht", "mir", "das", "Wort", "in", "le\u00b7ben\u00b7di\u00b7ger", "Re\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$.", "ADV", "ADJD", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Flie\u00dft, wenn die Sache mich reizt, so schwer entstr\u00f6mt es der Feder,", "tokens": ["Flie\u00dft", ",", "wenn", "die", "Sa\u00b7che", "mich", "reizt", ",", "so", "schwer", "ent\u00b7str\u00f6mt", "es", "der", "Fe\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Langsam, br\u00fcchig und kalt, als ob auf dem l\u00e4ngeren Umweg", "tokens": ["Lang\u00b7sam", ",", "br\u00fc\u00b7chig", "und", "kalt", ",", "als", "ob", "auf", "dem", "l\u00e4n\u00b7ge\u00b7ren", "Um\u00b7weg"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "KOKOM", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Aus dem Herzen aufs Blatt mir Gef\u00fchl und Gedanke gefr\u00f6ren.", "tokens": ["Aus", "dem", "Her\u00b7zen", "aufs", "Blatt", "mir", "Ge\u00b7f\u00fchl", "und", "Ge\u00b7dan\u00b7ke", "ge\u00b7fr\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Kaum, da\u00df ich munter begann, gleich blickt die verw\u00fcnschte Kritik mir", "tokens": ["Kaum", ",", "da\u00df", "ich", "mun\u00b7ter", "be\u00b7gann", ",", "gleich", "blickt", "die", "ver\u00b7w\u00fcnschte", "Kri\u00b7tik", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "\u00dcber die Schulter herein, und den Ausdruck allzu bedenklich", "tokens": ["\u00dc\u00b7ber", "die", "Schul\u00b7ter", "her\u00b7ein", ",", "und", "den", "Aus\u00b7druck", "all\u00b7zu", "be\u00b7denk\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "KON", "ART", "NN", "PTKA", "ADJD"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.8": {"text": "W\u00e4gend verpfusch' ich ihn leicht zu farblos steifer Korrektheit,", "tokens": ["W\u00e4\u00b7gend", "ver\u00b7pfusch'", "ich", "ihn", "leicht", "zu", "farb\u00b7los", "stei\u00b7fer", "Kor\u00b7rekt\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADJD", "PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+--", "measure": "elegiambus"}, "line.9": {"text": "Statt im behaglichen Flu\u00df frischweg von der Leber zu plaudern", "tokens": ["Statt", "im", "be\u00b7hag\u00b7li\u00b7chen", "Flu\u00df", "frischweg", "von", "der", "Le\u00b7ber", "zu", "plau\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Ganz, wie der Schnabel mir wuchs. Zum Teil wohl hab' ich's vom Vater,", "tokens": ["Ganz", ",", "wie", "der", "Schna\u00b7bel", "mir", "wuchs", ".", "Zum", "Teil", "wohl", "hab'", "ich's", "vom", "Va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$.", "APPRART", "NN", "ADV", "VAFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Der, ob Meister des Worts, sich besann, zwei Zeilen der Post nur", "tokens": ["Der", ",", "ob", "Meis\u00b7ter", "des", "Worts", ",", "sich", "be\u00b7sann", ",", "zwei", "Zei\u00b7len", "der", "Post", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "NN", "ART", "NN", "$,", "PRF", "VVFIN", "$,", "CARD", "NN", "ART", "NN", "ADV"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Anzuvertraun, und, an Freundschaft reich, nie Briefe gewechselt.", "tokens": ["An\u00b7zu\u00b7ver\u00b7traun", ",", "und", ",", "an", "Freund\u00b7schaft", "reich", ",", "nie", "Brie\u00b7fe", "ge\u00b7wech\u00b7selt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "$,", "APPR", "NN", "ADJD", "$,", "ADV", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Drum dafern ihr im Ernst, wie ihr sagt, mir freundlich gesinnt seid,", "tokens": ["Drum", "da\u00b7fern", "ihr", "im", "Ernst", ",", "wie", "ihr", "sagt", ",", "mir", "freund\u00b7lich", "ge\u00b7sinnt", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Dr\u00e4ngt unn\u00f6tig mich nicht zum Schreiben und fordert insonders", "tokens": ["Dr\u00e4ngt", "un\u00b7n\u00f6\u00b7tig", "mich", "nicht", "zum", "Schrei\u00b7ben", "und", "for\u00b7dert", "in\u00b7son\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PTKNEG", "APPRART", "NN", "KON", "VVFIN", "ADV"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Antwort nicht auf jedes Gef\u00fchl. Gern send' ich euch Auskunft,", "tokens": ["Ant\u00b7wort", "nicht", "auf", "je\u00b7des", "Ge\u00b7f\u00fchl", ".", "Gern", "send'", "ich", "euch", "Aus\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PIAT", "NN", "$.", "NN", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "B\u00fcndige, gilt's ein Gesch\u00e4ft, doch zu brieflicher Herzensergie\u00dfung", "tokens": ["B\u00fcn\u00b7di\u00b7ge", ",", "gilt's", "ein", "Ge\u00b7sch\u00e4ft", ",", "doch", "zu", "brief\u00b7li\u00b7cher", "Her\u00b7zen\u00b7ser\u00b7gie\u00b7\u00dfung"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Fehlt mir f\u00fcrwahr das Geschick und fehlt vor allem die Neigung.", "tokens": ["Fehlt", "mir", "f\u00fcr\u00b7wahr", "das", "Ge\u00b7schick", "und", "fehlt", "vor", "al\u00b7lem", "die", "Nei\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "KON", "VVFIN", "APPR", "PIS", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}}, "stanza.5": {"line.1": {"text": "\u2013 \u00bbAber es gl\u00fcckte dir doch manch Lied; wie darfst du behaupten,", "tokens": ["\u2013", "\u00bb", "A\u00b7ber", "es", "gl\u00fcck\u00b7te", "dir", "doch", "manch", "Lied", ";", "wie", "darfst", "du", "be\u00b7haup\u00b7ten", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "KON", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Da\u00df dir die kleinere M\u00fche zu viel?\u00ab \u2013 Nun, jeglicher hat ja", "tokens": ["Da\u00df", "dir", "die", "klei\u00b7ne\u00b7re", "M\u00fc\u00b7he", "zu", "viel", "?", "\u00ab", "\u2013", "Nun", ",", "jeg\u00b7li\u00b7cher", "hat", "ja"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PIS", "$.", "$(", "$(", "ADV", "$,", "ADV", "VAFIN", "ADV"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Seine Begabung f\u00fcr sich, und der schnell hinschie\u00dfende Habicht", "tokens": ["Sei\u00b7ne", "Be\u00b7ga\u00b7bung", "f\u00fcr", "sich", ",", "und", "der", "schnell", "hin\u00b7schie\u00b7\u00dfen\u00b7de", "Ha\u00b7bicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "$,", "KON", "ART", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Ist schwerf\u00e4llig zu Fu\u00df. Niemals auch hab' ich am Schreibtisch", "tokens": ["Ist", "schwer\u00b7f\u00e4l\u00b7lig", "zu", "Fu\u00df", ".", "Nie\u00b7mals", "auch", "hab'", "ich", "am", "Schreib\u00b7tisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "$.", "ADV", "ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "M\u00fchsam, was ich gesungen, erdacht. Stets kam es von selbst mir,", "tokens": ["M\u00fch\u00b7sam", ",", "was", 