ON", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Braucht' ich mit Flei\u00df, doch zuvor in geheimnisvoller Empf\u00e4ngnis", "tokens": ["Braucht'", "ich", "mit", "Flei\u00df", ",", "doch", "zu\u00b7vor", "in", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Emp\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Ward mir immer das Beste zuteil als himmlische Gabe.", "tokens": ["Ward", "mir", "im\u00b7mer", "das", "Bes\u00b7te", "zu\u00b7teil", "als", "himm\u00b7li\u00b7sche", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Nie willk\u00fcrlich darum, wenn die innere N\u00f6tigung ausblieb,", "tokens": ["Nie", "will\u00b7k\u00fcr\u00b7lich", "da\u00b7rum", ",", "wenn", "die", "in\u00b7ne\u00b7re", "N\u00f6\u00b7ti\u00b7gung", "aus\u00b7blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--++-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Hab' ich zu dichten gewu\u00dft, auf Begehr, wie der Meister des Handwerks", "tokens": ["Hab'", "ich", "zu", "dich\u00b7ten", "ge\u00b7wu\u00dft", ",", "auf", "Be\u00b7gehr", ",", "wie", "der", "Meis\u00b7ter", "des", "Hand\u00b7werks"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "VVPP", "$,", "APPR", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Rasch das Verlangte beschafft, zu Geburtstagsfeier und Hochzeit", "tokens": ["Rasch", "das", "Ver\u00b7lang\u00b7te", "be\u00b7schafft", ",", "zu", "Ge\u00b7burts\u00b7tags\u00b7fei\u00b7er", "und", "Hoch\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Oder zum Neujahrsgru\u00df. Und versucht' ich es dennoch, der Bitte", "tokens": ["O\u00b7der", "zum", "Neu\u00b7jahrs\u00b7gru\u00df", ".", "Und", "ver\u00b7sucht'", "ich", "es", "den\u00b7noch", ",", "der", "Bit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "NN", "$.", "KON", "VVFIN", "PPER", "PPER", "ADV", "$,", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Weichend, so ward es darnach: ein zusammengestoppeltes Machwerk", "tokens": ["Wei\u00b7chend", ",", "so", "ward", "es", "dar\u00b7nach", ":", "ein", "zu\u00b7sam\u00b7men\u00b7ge\u00b7stop\u00b7pel\u00b7tes", "Mach\u00b7werk"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADV", "VAFIN", "PPER", "PAV", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.6": {"text": "Statt des lebendigen Lieds. Nur wenn in begl\u00fcckender Stunde,", "tokens": ["Statt", "des", "le\u00b7ben\u00b7di\u00b7gen", "Lieds", ".", "Nur", "wenn", "in", "be\u00b7gl\u00fc\u00b7cken\u00b7der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "ADV", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "+--+---+-+-+--+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Wie sie dem Alternden, ach, nur noch selten erscheint und im Fluge,", "tokens": ["Wie", "sie", "dem", "Al\u00b7tern\u00b7den", ",", "ach", ",", "nur", "noch", "sel\u00b7ten", "er\u00b7scheint", "und", "im", "Flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "ITJ", "$,", "ADV", "ADV", "ADJD", "VVFIN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Mir freiwillig die Muse genaht, da vermocht' ich zu schaffen,", "tokens": ["Mir", "frei\u00b7wil\u00b7lig", "die", "Mu\u00b7se", "ge\u00b7naht", ",", "da", "ver\u00b7mocht'", "ich", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Was mich selber erfreut' und vielleicht auch anderen echt schien.", "tokens": ["Was", "mich", "sel\u00b7ber", "er\u00b7freut'", "und", "viel\u00b7leicht", "auch", "an\u00b7de\u00b7ren", "echt", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "ADV", "ADV", "PIS", "ADJD", "VVFIN", "$."], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}}}}}