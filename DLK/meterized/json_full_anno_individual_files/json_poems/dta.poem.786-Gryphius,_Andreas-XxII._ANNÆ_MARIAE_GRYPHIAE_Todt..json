{"dta.poem.786": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxII.  \n  ANN\u00c6 MARIAE GRYPHIAE  \n Todt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dv bist den/ du bist hin! Der Himmel g\u00f6nte dich ", "tokens": ["Dv", "bist", "den", "/", "du", "bist", "hin", "!", "Der", "Him\u00b7mel", "g\u00f6n\u00b7te", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "$(", "PPER", "VAFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erden weiter nicht/ die Tugend kaum mehr kennet/", "tokens": ["Der", "Er\u00b7den", "wei\u00b7ter", "nicht", "/", "die", "Tu\u00b7gend", "kaum", "mehr", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "$(", "ART", "NN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darin man Laster preist/ vnd S\u00fcnde from seyn nennet/", "tokens": ["Da\u00b7rin", "man", "Las\u00b7ter", "preist", "/", "vnd", "S\u00fcn\u00b7de", "from", "seyn", "nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "NN", "VVFIN", "$(", "KON", "NN", "ADJD", "VAINF", "VVFIN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die au\u00df gejagte zucht nam deine Seel zu sich.", "tokens": ["Die", "au\u00df", "ge\u00b7jag\u00b7te", "zucht", "nam", "dei\u00b7ne", "Seel", "zu", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du gibst den gutte Nacht so vnversehns/ weil ich", "tokens": ["Du", "gibst", "den", "gut\u00b7te", "Nacht", "so", "vn\u00b7ver\u00b7sehns", "/", "weil", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$(", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In fernem Elend bin/ dein Eh-stand wird getrennet", "tokens": ["In", "fer\u00b7nem", "E\u00b7lend", "bin", "/", "dein", "Eh\u00b7stand", "wird", "ge\u00b7tren\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$(", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Eh\u2019 recht dein Hochzeit licht (holdseelige!) verbrennet", "tokens": ["Eh'", "recht", "dein", "Hoch\u00b7zeit", "licht", "(", "hold\u00b7see\u00b7li\u00b7ge", "!", ")", "ver\u00b7bren\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "NN", "$(", "VVFIN", "$.", "$(", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil mich dein ", "tokens": ["Weil", "mich", "dein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Was kan abwesend Ich mehr al\u00df dich selbst beweynen", "tokens": ["Was", "kan", "ab\u00b7we\u00b7send", "Ich", "mehr", "al\u00df", "dich", "selbst", "be\u00b7wey\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADJD", "PPER", "ADV", "KOUS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd deine zarte Frucht/ die als sie wolt erscheinen", "tokens": ["Vnd", "dei\u00b7ne", "zar\u00b7te", "Frucht", "/", "die", "als", "sie", "wolt", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$(", "ART", "KOUS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die schwachen Augen schlo\u00df vnd deine Seel entband.", "tokens": ["Die", "schwa\u00b7chen", "Au\u00b7gen", "schlo\u00df", "vnd", "dei\u00b7ne", "Seel", "ent\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Man w\u00fcndscht einmal zu sehn die Fr\u00f6migkeit geb\u00e4hren:", "tokens": ["Man", "w\u00fcnd\u00b7scht", "ein\u00b7mal", "zu", "sehn", "die", "Fr\u00f6\u00b7mig\u00b7keit", "ge\u00b7b\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKZU", "VVINF", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Al\u00df welche nichts der welt/ denn Tugend kan gewehren.", "tokens": ["Al\u00df", "wel\u00b7che", "nichts", "der", "welt", "/", "denn", "Tu\u00b7gend", "kan", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "PIS", "ART", "NN", "$(", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch ach! den wundsch durchstrich der Parcen strenge", "tokens": ["Doch", "ach", "!", "den", "wund\u00b7sch", "durch\u00b7strich", "der", "Par\u00b7cen", "stren\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "ADJD", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hand.", "tokens": ["Hand", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}