{"textgrid.poem.54557": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie offt hab ich verhofft/ Durchlauchtes Licht der Welt/", "tokens": ["Wie", "offt", "hab", "ich", "ver\u00b7hofft", "/", "Durch\u00b7lauch\u00b7tes", "Licht", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVFIN", "$(", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Wunder aller Zeit/ zu knien vor ihren F\u00fcssen/", "tokens": ["Und", "Wun\u00b7der", "al\u00b7ler", "Zeit", "/", "zu", "kni\u00b7en", "vor", "ih\u00b7ren", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$(", "PTKZU", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und Sie/ wiewol vorhin/ in tieffster Pflicht zu gr\u00fcssen.", "tokens": ["Und", "Sie", "/", "wie\u00b7wol", "vor\u00b7hin", "/", "in", "tieffs\u00b7ter", "Pflicht", "zu", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "KOUS", "ADV", "$(", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umsonst! weil Land und Stand mich als in Bande stellt/", "tokens": ["Um\u00b7sonst", "!", "weil", "Land", "und", "Stand", "mich", "als", "in", "Ban\u00b7de", "stellt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "wor