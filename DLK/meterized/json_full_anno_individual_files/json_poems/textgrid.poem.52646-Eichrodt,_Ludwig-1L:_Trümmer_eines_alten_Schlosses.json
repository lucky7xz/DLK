{"textgrid.poem.52646": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00fcmmer eines alten Schlosses", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcmmer eines alten Schlosses", "tokens": ["Tr\u00fcm\u00b7mer", "ei\u00b7nes", "al\u00b7ten", "Schlos\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht der stille Wandrer auf,", "tokens": ["Sucht", "der", "stil\u00b7le", "Wand\u00b7rer", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandelt rasch den sp\u00e4ten Lauf", "tokens": ["Wan\u00b7delt", "rasch", "den", "sp\u00e4\u00b7ten", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu der Burg, die oben thront.", "tokens": ["Zu", "der", "Burg", ",", "die", "o\u00b7ben", "thront", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Er bescheinet die Gestalt", "tokens": ["Er", "be\u00b7schei\u00b7net", "die", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwiefach Licht,", "tokens": ["Zwie\u00b7fach", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Oben kalt", "tokens": ["O\u00b7ben", "kalt"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Bleich der Mond", "tokens": ["Bleich", "der", "Mond"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Unten bricht", "tokens": ["Un\u00b7ten", "bricht"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "An dem fernsten Horizont", "tokens": ["An", "dem", "ferns\u00b7ten", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich der Abendsonne Strahl;", "tokens": ["Sich", "der", "A\u00b7bend\u00b7son\u00b7ne", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Tief im Dunkel ruht das Thal.", "tokens": ["Tief", "im", "Dun\u00b7kel", "ruht", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dr\u00fcben spr\u00fchn", "tokens": ["Dr\u00fc\u00b7ben", "spr\u00fchn"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Rosigen Schimmer", "tokens": ["Ro\u00b7si\u00b7gen", "Schim\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Alte Tr\u00fcmmer", "tokens": ["Al\u00b7te", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Von der Sonne letztem Gl\u00fchn.", "tokens": ["Von", "der", "Son\u00b7ne", "letz\u00b7tem", "Gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Durchs Gekl\u00fcft der Th\u00fcrme pfeifend,", "tokens": ["Durchs", "Ge\u00b7kl\u00fcft", "der", "Th\u00fcr\u00b7me", "pfei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weht der melancholsche Wind.", "tokens": ["Weht", "der", "me\u00b7lan\u00b7chol\u00b7sche", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sieh! der Schein des Mondes spinnt", "tokens": ["Sieh", "!", "der", "Schein", "des", "Mon\u00b7des", "spinnt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber wankendes Gem\u00e4uer", "tokens": ["Ue\u00b7ber", "wan\u00b7ken\u00b7des", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen lichtgewobnen Schleier,", "tokens": ["Ei\u00b7nen", "licht\u00b7ge\u00b7wob\u00b7nen", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sanft am Gl\u00fchn vor\u00fcberstreifend.", "tokens": ["Sanft", "am", "Gl\u00fchn", "vor\u00b7\u00fc\u00b7bers\u00b7trei\u00b7fend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Geisterathmen hauchet aus", "tokens": ["Geis\u00b7te\u00b7rath\u00b7men", "hau\u00b7chet", "aus"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Versch\u00fcttung d\u00fcstrer Graus,", "tokens": ["Der", "Ver\u00b7sch\u00fct\u00b7tung", "d\u00fcst\u00b7rer", "Graus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schatten ziehn sich lang", "tokens": ["Schat\u00b7ten", "ziehn", "sich", "lang"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schauerbergend zu dem Felsenhang.", "tokens": ["Schau\u00b7er\u00b7ber\u00b7gend", "zu", "dem", "Fel\u00b7sen\u00b7hang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und des Schlafes s\u00fc\u00dfes Reich", "tokens": ["Und", "des", "Schla\u00b7fes", "s\u00fc\u00b7\u00dfes", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Neigt auch schon", "tokens": ["Neigt", "auch", "schon"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Auf den m\u00fcden Erdensohn", "tokens": ["Auf", "den", "m\u00fc\u00b7den", "Er\u00b7den\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den bemohnten Zweig.", "tokens": ["Den", "be\u00b7mohn\u00b7ten", "Zweig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Aber in gemessnem Schritte", "tokens": ["A\u00b7ber", "in", "ge\u00b7mess\u00b7nem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallet zu des Hofes Mitte", "tokens": ["Wal\u00b7let", "zu", "des", "Ho\u00b7fes", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Majest\u00e4tisch Paar zu Paar", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "Paar", "zu", "Paar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Grauer M\u00f6nche stumme Schaar.", "tokens": ["Grau\u00b7er", "M\u00f6n\u00b7che", "stum\u00b7me", "Schaar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort zur Stelle", "tokens": ["Dort", "zur", "Stel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ragt verfallen die Kapelle.", "tokens": ["Ragt", "ver\u00b7fal\u00b7len", "die", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dort erschallen in den Hallen,", "tokens": ["Dort", "er\u00b7schal\u00b7len", "in", "den", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu der Jungfrau Preis und Ehre,", "tokens": ["Zu", "der", "Jung\u00b7frau", "Preis", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Ihre Ch\u00f6re,", "tokens": ["Ih\u00b7re", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Die des Windes leiser Flug", "tokens": ["Die", "des", "Win\u00b7des", "lei\u00b7ser", "Flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "S\u00e4uselnd durch die Tr\u00fcmmer trug.", "tokens": ["S\u00e4u\u00b7selnd", "durch", "die", "Tr\u00fcm\u00b7mer", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Immer lieblicher erklingen", "tokens": ["Im\u00b7mer", "lieb\u00b7li\u00b7cher", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann die T\u00f6ne, tiefer dringen", "tokens": ["Dann", "die", "T\u00f6\u00b7ne", ",", "tie\u00b7fer", "drin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie ins Herz dem J\u00fcngling ein.", "tokens": ["Sie", "ins", "Herz", "dem", "J\u00fcng\u00b7ling", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feierliche Worte rauschen", "tokens": ["Fei\u00b7er\u00b7li\u00b7che", "Wor\u00b7te", "rau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An sein Ohr, die M\u00f6nche tauschen", "tokens": ["An", "sein", "Ohr", ",", "die", "M\u00f6n\u00b7che", "tau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das Gewand im Mondenschein.", "tokens": ["Das", "Ge\u00b7wand", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und der Wandrer j\u00e4h erwacht,", "tokens": ["Und", "der", "Wand\u00b7rer", "j\u00e4h", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und der Wandrer hellauf lacht.", "tokens": ["Und", "der", "Wand\u00b7rer", "hel\u00b7lauf", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Fr\u00f6hlich lagen sie beisammen,", "tokens": ["Fr\u00f6h\u00b7lich", "la\u00b7gen", "sie", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Moose des Gesteins,", "tokens": ["Auf", "dem", "Moo\u00b7se", "des", "Ge\u00b7steins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Freunde, bei den Flammen", "tokens": ["Sei\u00b7ne", "Freun\u00b7de", ",", "bei", "den", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines hellen Feuerleins.", "tokens": ["Ei\u00b7nes", "hel\u00b7len", "Feu\u00b7er\u00b7leins", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Dort wo l\u00e4ngst verstummt die Kl\u00e4nge,", "tokens": ["Dort", "wo", "l\u00e4ngst", "ver\u00b7stummt", "die", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Walten Lieder mancherlei,", "tokens": ["Wal\u00b7ten", "Lie\u00b7der", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6nen kr\u00e4ftge Jubelch\u00f6re,", "tokens": ["T\u00f6\u00b7nen", "kr\u00e4ft\u00b7ge", "Ju\u00b7belc\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Vaterlandes Ehre", "tokens": ["Zu", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche frische Melodei.", "tokens": ["Man\u00b7che", "fri\u00b7sche", "Me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wanderung und Minne k