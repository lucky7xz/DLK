{"dta.poem.23340": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wasserfrage.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun kommt auch noch die Wasserkur", "tokens": ["Nun", "kommt", "auch", "noch", "die", "Was\u00b7ser\u00b7kur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu unsern vielen Tagesfragen,", "tokens": ["Zu", "un\u00b7sern", "vie\u00b7len", "Ta\u00b7ges\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als k\u00f6nnten uns die Aerzte nicht", "tokens": ["Als", "k\u00f6nn\u00b7ten", "uns", "die", "A\u00b7erz\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Genug auf andre Weise plagen.", "tokens": ["Ge\u00b7nug", "auf", "and\u00b7re", "Wei\u00b7se", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4r' eine Schwimmhaut mir beschert", "tokens": ["W\u00e4r'", "ei\u00b7ne", "Schwimm\u00b7haut", "mir", "be\u00b7schert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und h\u00e4tt' ich einen Haifischmagen,", "tokens": ["Und", "h\u00e4tt'", "ich", "ei\u00b7nen", "Hai\u00b7fischma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Da w\u00fcrde mir die Wasserkur", "tokens": ["Da", "w\u00fcr\u00b7de", "mir", "die", "Was\u00b7ser\u00b7kur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht tagt\u00e4glich auch behagen.", "tokens": ["Viel\u00b7leicht", "tag\u00b7t\u00e4g\u00b7lich", "auch", "be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch mi\u00dflich ist's auch sonst damit,", "tokens": ["Doch", "mi\u00df\u00b7lich", "ist's", "auch", "sonst", "da\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie mit den diplomat'schen Fragen:", "tokens": ["Wie", "mit", "den", "di\u00b7plo\u00b7mat'\u00b7schen", "Fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von ihrem Anfang kann man wohl,", "tokens": ["Von", "ih\u00b7rem", "An\u00b7fang", "kann", "man", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihrem Ende niemals sagen.", "tokens": ["Von", "ih\u00b7rem", "En\u00b7de", "nie\u00b7mals", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}