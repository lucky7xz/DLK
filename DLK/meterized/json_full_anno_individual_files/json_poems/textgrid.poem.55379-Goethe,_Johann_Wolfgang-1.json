{"textgrid.poem.55379": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedichte sind gemalte Fensterscheiben!", "tokens": ["Ge\u00b7dich\u00b7te", "sind", "ge\u00b7mal\u00b7te", "Fens\u00b7ter\u00b7schei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht man vom Markt in die Kirche hinein,", "tokens": ["Sieht", "man", "vom", "Markt", "in", "die", "Kir\u00b7che", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Da ist alles dunkel und d\u00fcster;", "tokens": ["Da", "ist", "al\u00b7les", "dun\u00b7kel", "und", "d\u00fcs\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und so sieht's auch der Herr Philister:", "tokens": ["Und", "so", "sieht's", "auch", "der", "Herr", "Phi\u00b7lis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "NE", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der mag denn wohl verdrie\u00dflich sein", "tokens": ["Der", "mag", "denn", "wohl", "ver\u00b7drie\u00df\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lebenslang verdrie\u00dflich bleiben.", "tokens": ["Und", "le\u00b7bens\u00b7lang", "ver\u00b7drie\u00df\u00b7lich", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kommt aber nur einmal herein,", "tokens": ["Kommt", "a\u00b7ber", "nur", "ein\u00b7mal", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begr\u00fc\u00dft die heilige Kapelle;", "tokens": ["Be\u00b7gr\u00fc\u00dft", "die", "hei\u00b7li\u00b7ge", "Ka\u00b7pel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist's auf einmal farbig helle,", "tokens": ["Da", "ist's", "auf", "ein\u00b7mal", "far\u00b7big", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschicht und Zierat gl\u00e4nzt in Schnelle,", "tokens": ["Ge\u00b7schicht", "und", "Zie\u00b7rat", "gl\u00e4nzt", "in", "Schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bedeutend wirkt ein edler Schein;", "tokens": ["Be\u00b7deu\u00b7tend", "wirkt", "ein", "ed\u00b7ler", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies wird euch Kindern Gottes taugen,", "tokens": ["Dies", "wird", "euch", "Kin\u00b7dern", "Got\u00b7tes", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erbaut euch und ergetzt die Augen!", "tokens": ["Er\u00b7baut", "euch", "und", "er\u00b7getzt", "die", "Au\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gedichte sind gemalte Fensterscheiben!", "tokens": ["Ge\u00b7dich\u00b7te", "sind", "ge\u00b7mal\u00b7te", "Fens\u00b7ter\u00b7schei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht man vom Markt in die Kirche hinein,", "tokens": ["Sieht", "man", "vom", "Markt", "in", "die", "Kir\u00b7che", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Da ist alles dunkel und d\u00fcster;", "tokens": ["Da", "ist", "al\u00b7les", "dun\u00b7kel", "und", "d\u00fcs\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und so sieht's auch der Herr Philister:", "tokens": ["Und", "so", "sieht's", "auch", "der", "Herr", "Phi\u00b7lis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "NE", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der mag denn w