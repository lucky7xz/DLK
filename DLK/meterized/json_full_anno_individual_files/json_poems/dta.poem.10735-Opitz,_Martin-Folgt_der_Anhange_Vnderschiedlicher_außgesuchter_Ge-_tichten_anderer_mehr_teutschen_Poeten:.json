{"dta.poem.10735": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Folgt der Anhange  \n Vnderschiedlicher au\u00dfgesuchter Ge-  \n tichten anderer mehr teutschen  \n Poeten:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab das mein gethan/ so vil mir Gott beschert:", "tokens": ["Ich", "hab", "das", "mein", "ge\u00b7than", "/", "so", "vil", "mir", "Gott", "be\u00b7schert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "PPOSAT", "VVPP", "$(", "ADV", "ADV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ander thue das sein/ so wirdt die Kunst gemehrt.", "tokens": ["Ein", "an\u00b7der", "thue", "das", "sein", "/", "so", "wirdt", "die", "Kunst", "ge\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "VAINF", "$(", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}