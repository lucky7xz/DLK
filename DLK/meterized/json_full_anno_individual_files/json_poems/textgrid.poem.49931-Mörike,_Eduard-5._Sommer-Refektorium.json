{"textgrid.poem.49931": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "5. Sommer-Refektorium", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sommerlich hell empf\u00e4ngt dich ein Saal; man glaubt sich in einem", "tokens": ["Som\u00b7mer\u00b7lich", "hell", "emp\u00b7f\u00e4ngt", "dich", "ein", "Saal", ";", "man", "glaubt", "sich", "in", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "ART", "NN", "$.", "PIS", "VVFIN", "PRF", "APPR", "ART"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dom; doch ein heiterer Geist spricht im Erhabnen dich an.", "tokens": ["Dom", ";", "doch", "ein", "hei\u00b7te\u00b7rer", "Geist", "spricht", "im", "Er\u00b7hab\u00b7nen", "dich", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ha, wie entz\u00fcckt aufsteiget das Aug im Flug mit den schlanken", "tokens": ["Ha", ",", "wie", "ent\u00b7z\u00fcckt", "auf\u00b7stei\u00b7get", "das", "Aug", "im", "Flug", "mit", "den", "schlan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVPP", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "ART", "VVINF"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Pfeilern! Der Palme vergleicht fast sich ihr luftiger Bau.", "tokens": ["Pfei\u00b7lern", "!", "Der", "Pal\u00b7me", "ver\u00b7gleicht", "fast", "sich", "ihr", "luf\u00b7ti\u00b7ger", "Bau", "."], "toke