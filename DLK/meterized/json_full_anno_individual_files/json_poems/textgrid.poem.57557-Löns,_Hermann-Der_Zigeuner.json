{"textgrid.poem.57557": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Zigeuner", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer reitet \u00fcber den gelben Sand", "tokens": ["Wer", "rei\u00b7tet", "\u00fc\u00b7ber", "den", "gel\u00b7ben", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf ungesatteltem Fohlen,", "tokens": ["Auf", "un\u00b7ge\u00b7sat\u00b7tel\u00b7tem", "Foh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das ist der braune Zigeunerbursch,", "tokens": ["Das", "ist", "der", "brau\u00b7ne", "Zi\u00b7geu\u00b7ner\u00b7bursch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Braunr\u00f6\u00dflein ist gestohlen,", "tokens": ["Braun\u00b7r\u00f6\u00df\u00b7lein", "ist", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Husaren jagen hinter ihm her,", "tokens": ["Hu\u00b7sa\u00b7ren", "ja\u00b7gen", "hin\u00b7ter", "ihm", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Vors Stuhlgericht ihn zu holen.", "tokens": ["Vors", "Stuhl\u00b7ge\u00b7richt", "ihn", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbich reite zu meiner Liebsten hin,", "tokens": ["\u00bb", "ich", "rei\u00b7te", "zu", "mei\u00b7ner", "Liebs\u00b7ten", "hin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Meiner braunen Herzensfreude,", "tokens": ["Mei\u00b7ner", "brau\u00b7nen", "Her\u00b7zens\u00b7freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Braunr\u00f6\u00dflein ist das Brautgeschenk,", "tokens": ["Braun\u00b7r\u00f6\u00df\u00b7lein", "ist", "das", "Braut\u00b7ge\u00b7schenk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Brautbett ist die Heide!\u00ab", "tokens": ["Das", "Braut\u00b7bett", "ist", "die", "Hei\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbhurra, du hast zu fr\u00fch gelacht,", "tokens": ["\u00bb", "hur\u00b7ra", ",", "du", "hast", "zu", "fr\u00fch", "ge\u00b7lacht", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du frecher Zigeunerhund!", "tokens": ["Du", "fre\u00b7cher", "Zi\u00b7geu\u00b7ner\u00b7hund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du f\u00e4hrst auf geraden Wegen", "tokens": ["Du", "f\u00e4hrst", "auf", "ge\u00b7ra\u00b7den", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem Galgenholz entgegen", "tokens": ["Dem", "Gal\u00b7gen\u00b7holz", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur ersten Morgenstund.\u00ab \u2013", "tokens": ["Zur", "ers\u00b7ten", "Mor\u00b7gen\u00b7stund", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbmein Vater starb am Galgenscheit,", "tokens": ["\u00bb", "mein", "Va\u00b7ter", "starb", "am", "Gal\u00b7gen\u00b7scheit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will's nicht besser haben,", "tokens": ["Ich", "will's", "nicht", "bes\u00b7ser", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann kommen im blanken Sonntagskleid", "tokens": ["Dann", "kom\u00b7men", "im", "blan\u00b7ken", "Sonn\u00b7tags\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die lieben, treuen Raben,", "tokens": ["Die", "lie\u00b7ben", ",", "treu\u00b7en", "Ra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und werden mich begraben.", "tokens": ["Und", "wer\u00b7den", "mich", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann fliegt meine Seele so lustig, juchhei!", "tokens": ["Dann", "fliegt", "mei\u00b7ne", "See\u00b7le", "so", "lus\u00b7tig", ",", "juch\u00b7hei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,", "ITJ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich wei\u00df nicht wohin, mir ist's auch einerlei,", "tokens": ["Ich", "wei\u00df", "nicht", "wo\u00b7hin", ",", "mir", "ist's", "auch", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "$,", "PPER", "VAFIN", "ADV", "PIS", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wei\u00df kaum, ob ich eine habe,", "tokens": ["Wei\u00df", "kaum", ",", "ob", "ich", "ei\u00b7ne", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Leib fri\u00dft Wurm und Rabe,", "tokens": ["Mei\u00b7nen", "Leib", "fri\u00dft", "Wurm", "und", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dann ist alles vorbei,", "tokens": ["Und", "dann", "ist", "al\u0