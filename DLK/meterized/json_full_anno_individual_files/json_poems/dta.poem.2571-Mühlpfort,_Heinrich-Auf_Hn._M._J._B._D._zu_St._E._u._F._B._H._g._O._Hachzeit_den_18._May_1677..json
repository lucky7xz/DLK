{"dta.poem.2571": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn.   M.   J. B.   D.   zu St. E. u. F. B. H.  \n g. O. Hachzeit/ den 18. May 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat/ Ehrw\u00fcrdiger/ der Himmel au\u00dfgeweinet/", "tokens": ["Es", "hat", "/", "Ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "/", "der", "Him\u00b7mel", "au\u00df\u00b7ge\u00b7wei\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "NN", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "De\u00df Traurens schwartze Nacht zertheilt ein liechter", "tokens": ["De\u00df", "Trau\u00b7rens", "schwart\u00b7ze", "Nacht", "zer\u00b7theilt", "ein", "liech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versichert da\u00df ihm jetzt der Wolfahrt Sonne scheinet/", "tokens": ["Ver\u00b7si\u00b7chert", "da\u00df", "ihm", "jetzt", "der", "Wol\u00b7fahrt", "Son\u00b7ne", "schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOUS", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Gl\u00fccke f\u00fchret ihn in seinen Freuden-Saal.", "tokens": ["Das", "Gl\u00fc\u00b7cke", "f\u00fch\u00b7ret", "ihn", "in", "sei\u00b7nen", "Freu\u00b7den\u00b7Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weg mit Napell und Bur und tunckelen Cypressen/", "tokens": ["Weg", "mit", "Na\u00b7pell", "und", "Bur", "und", "tun\u00b7cke\u00b7len", "Cyp\u00b7res\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "KON", "ADJA", "NN", "$("], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.6": {"text": "Hier mu\u00df der Lilgen Schnee/ der Rosen Purpur seyn.", "tokens": ["Hier", "mu\u00df", "der", "Lil\u00b7gen", "Schnee", "/", "der", "Ro\u00b7sen", "Pur\u00b7pur", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist das alte Leid auf diesen Tag vergessen/", "tokens": ["Es", "ist", "das", "al\u00b7te", "Leid", "auf", "die\u00b7sen", "Tag", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Lieb\u2019 und Fr\u00f6ligkeit stellt sich zu G\u00e4sten ein.", "tokens": ["Und", "Lieb'", "und", "Fr\u00f6\u00b7lig\u00b7keit", "stellt", "sich", "zu", "G\u00e4s\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Es pflegt de\u00df Himmels Schlu\u00df nicht ewig zu betr\u00fcben/", "tokens": ["Es", "pflegt", "de\u00df", "Him\u00b7mels", "Schlu\u00df", "nicht", "e\u00b7wig", "zu", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es plagt die Sterblichen nicht stets Egyptens Nacht:", "tokens": ["Es", "plagt", "die", "Sterb\u00b7li\u00b7chen", "nicht", "stets", "E\u00b7gyp\u00b7tens", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wenn der Wetter-Sturm fast alles auffgerieben/", "tokens": ["Und", "wenn", "der", "Wet\u00b7ter\u00b7Sturm", "fast", "al\u00b7les", "auff\u00b7ge\u00b7rie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat fr\u00fch ein Dornen-Strauch die sch\u00f6nsten Rosen bracht.", "tokens": ["Hat", "fr\u00fch", "ein", "Dor\u00b7nen\u00b7Strauch", "die", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df Leben ist vermischt mit Leiden und mit Freuden.", "tokens": ["Di\u00df", "Le\u00b7ben", "ist", "ver\u00b7mischt", "mit", "Lei\u00b7den", "und", "mit", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "VVPP", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ver\u00e4ndert doch das Jahr Lauff/ Wechsel und Gestalt:", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "doch", "das", "Jahr", "Lauff", "/", "Wech\u00b7sel", "und", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jm Fr\u00fchling wird es sich in bundten Taffend kleiden/", "tokens": ["Jm", "Fr\u00fch\u00b7ling", "wird", "es", "sich", "in", "bund\u00b7ten", "Taf\u00b7fend", "klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jm Winter ist es Schnee/ voll Runtzeln kalt und alt.", "tokens": ["Jm", "Win\u00b7ter", "ist", "es", "Schnee", "/", "voll", "Runt\u00b7zeln", "kalt", "und", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "$(", "ADJD", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es ist kein Becher nie der Traurigkeit so bitter/", "tokens": ["Es", "ist", "kein", "Be\u00b7cher", "nie", "der", "Trau\u00b7rig\u00b7keit", "so", "bit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df nicht der Wollust Hand geust was von Tropffen ein.", "tokens": ["Da\u00df", "nicht", "der", "Wol\u00b7lust", "Hand", "geust", "was", "von", "Tropf\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "NN", "VVPP", "PRELS", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So schrecklich ist auch nie das hefftigst Ungewitter/", "tokens": ["So", "schreck\u00b7lich", "ist", "auch", "nie", "das", "heff\u00b7tigst", "Un\u00b7ge\u00b7wit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es folgt nach Sturm und Plitz ein heller Sonnenschein.", "tokens": ["Es", "folgt", "nach", "Sturm", "und", "Plitz", "ein", "hel\u00b7ler", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er hat/ ", "tokens": ["Er", "hat", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Bi\u00dfher sich satt gequ\u00e4llt/ und seinen Geist betr\u00fcbt.", "tokens": ["Bi\u00df\u00b7her", "sich", "satt", "ge\u00b7qu\u00e4llt", "/", "und", "sei\u00b7nen", "Geist", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "VVPP", "$(", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jetzt wandelt sich das Leid in neue Freuden-Zeichen/", "tokens": ["Jetzt", "wan\u00b7delt", "sich", "das", "Leid", "in", "neu\u00b7e", "Freu\u00b7den\u00b7Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nun sein getreues Hertz in reinen Flammen liebt.", "tokens": ["Nun", "sein", "ge\u00b7treu\u00b7es", "Hertz", "in", "rei\u00b7nen", "Flam\u00b7men", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die angenehme Zeit/ (da alles bl\u00fcht und lachet.", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Zeit", "/", "(", "da", "al\u00b7les", "bl\u00fcht", "und", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "$(", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und Feld und G\u00e4rten stehn mit Blumen au\u00dfgestickt/", "tokens": ["Und", "Feld", "und", "G\u00e4r\u00b7ten", "stehn", "mit", "Blu\u00b7men", "au\u00df\u00b7ge\u00b7stickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da selbst die Flora sich zu einem Teppich machet/", "tokens": ["Da", "selbst", "die", "Flo\u00b7ra", "sich", "zu", "ei\u00b7nem", "Tep\u00b7pich", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und ihre Kinder gleich den Tocken hat geschm\u00fcckt/)", "tokens": ["Und", "ih\u00b7re", "Kin\u00b7der", "gleich", "den", "To\u00b7cken", "hat", "ge\u00b7schm\u00fcckt", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Lockt durch geheimen Zeug zu diesen s\u00fcssen Stunden/", "tokens": ["Lockt", "durch", "ge\u00b7hei\u00b7men", "Zeug", "zu", "die\u00b7sen", "s\u00fcs\u00b7sen", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und heist die Einsamkeit de\u00df Winters seyn verbannt.", "tokens": ["Und", "heist", "die", "Ein\u00b7sam\u00b7keit", "de\u00df", "Win\u00b7ters", "seyn", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er hat des Traurens Frost nur allzusehr empfunden/", "tokens": ["Er", "hat", "des", "Trau\u00b7rens", "Frost", "nur", "all\u00b7zu\u00b7sehr", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Jetzt soll sein Hertze gl\u00fchn in einem wahren Brand.", "tokens": ["Jetzt", "soll", "sein", "Hert\u00b7ze", "gl\u00fchn", "in", "ei\u00b7nem", "wah\u00b7ren", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Denn soll er f\u00fcr das Hau\u00df des HErrn embsig wachen/", "tokens": ["Denn", "soll", "er", "f\u00fcr", "das", "Hau\u00df", "des", "Herrn", "emb\u00b7sig", "wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.34": {"text": "So mu\u00df in seinem Hau\u00df\u2019 auch eine Leuchte seyn/", "tokens": ["So", "mu\u00df", "in", "sei\u00b7nem", "Hau\u00df'", "auch", "ei\u00b7ne", "Leuch\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die wei\u00df die Finstern\u00fc\u00df der Sorgen licht zu machen/", "tokens": ["Die", "wei\u00df", "die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "der", "Sor\u00b7gen", "licht", "zu", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und durch Beh\u00e4gligkeit vertreibt de\u00df Kummers Pein.", "tokens": ["Und", "durch", "Be\u00b7h\u00e4g\u00b7lig\u00b7keit", "ver\u00b7treibt", "de\u00df", "Kum\u00b7mers", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Unm\u00f6glich kan ein Mensch ihm selbst in allem rathen/", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "kan", "ein", "Mensch", "ihm", "selbst", "in", "al\u00b7lem", "ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADV", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bevor/ wo Ampt und Pflicht verzehrt das befle Theil.", "tokens": ["Be\u00b7vor", "/", "wo", "Ampt", "und", "Pflicht", "ver\u00b7zehrt", "das", "be\u00b7fle", "Theil", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "NN", "KON", "NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der K\u00f6rner Uberflu\u00df zersprenget die Granaten;", "tokens": ["Der", "K\u00f6r\u00b7ner", "U\u00b7ber\u00b7flu\u00df", "zer\u00b7spren\u00b7get", "die", "Gra\u00b7na\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Nicht nur ein eintzig Arm regiert der Schiffe Seil.", "tokens": ["Nicht", "nur", "ein", "eint\u00b7zig", "Arm", "re\u00b7giert", "der", "Schif\u00b7fe", "Seil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJD", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was ist ein Garten doch von keinem Zaun umbgeben?", "tokens": ["Was", "ist", "ein", "Gar\u00b7ten", "doch", "von", "kei\u00b7nem", "Zaun", "umb\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "