{"textgrid.poem.46814": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[seh' ich eure Bettchen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich eure Bettchen", "tokens": ["Seh'", "ich", "eu\u00b7re", "Bett\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Beide stehen leer,", "tokens": ["Bei\u00b7de", "ste\u00b7hen", "leer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wird das Herz mir schwer;", "tokens": ["Wird", "das", "Herz", "mir", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "In dem Ruhest\u00e4ttchen", "tokens": ["In", "dem", "Ru\u00b7he\u00b7st\u00e4tt\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Regt es sich nicht mehr.", "tokens": ["Regt", "es", "sich", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "O ihr Amorettchen,", "tokens": ["O", "ihr", "A\u00b7mo\u00b7rett\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Seid ihr ausgeschl\u00fcpft,", "tokens": ["Seid", "ihr", "aus\u00b7ge\u00b7schl\u00fcpft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$