{"dta.poem.8405": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Elegie  \n auf ein Landm\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Vom bemoosten Kirchenthurm herab.", "tokens": ["Vom", "be\u00b7moos\u00b7ten", "Kir\u00b7chen\u00b7thurm", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "V\u00e4ter weinen, Kinder, M\u00fctter, Br\u00e4ute;", "tokens": ["V\u00e4\u00b7ter", "wei\u00b7nen", ",", "Kin\u00b7der", ",", "M\u00fct\u00b7ter", ",", "Br\u00e4u\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Todtengr\u00e4ber gr\u00e4bt ein Grab.", "tokens": ["Und", "der", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "gr\u00e4bt", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Angethan mit einem Sterbekleide,", "tokens": ["An\u00b7ge\u00b7than", "mit", "ei\u00b7nem", "Ster\u00b7be\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Eine Blumenkron' im blonden Haar,", "tokens": ["Ei\u00b7ne", "Blu\u00b7men\u00b7kron'", "im", "blon\u00b7den", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Schlummert R\u00f6schen, so der Mutter Freude,", "tokens": ["Schlum\u00b7mert", "R\u00f6\u00b7schen", ",", "so", "der", "Mut\u00b7ter", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "So der Stolz des Dorfes war.", "tokens": ["So", "der", "Stolz", "des", "Dor\u00b7fes", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre Lieben, voll des Misgeschickes,", "tokens": ["Ih\u00b7re", "Lie\u00b7ben", ",", "voll", "des", "Mis\u00b7ge\u00b7schi\u00b7ckes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denken nicht an Pf\u00e4nderspiel und Tanz,", "tokens": ["Den\u00b7ken", "nicht", "an", "Pf\u00e4n\u00b7der\u00b7spiel", "und", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Stehn am Sarge, winden nasses Blickes", "tokens": ["Stehn", "am", "Sar\u00b7ge", ",", "win\u00b7den", "nas\u00b7ses", "Bli\u00b7ckes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihrer Freundin einen Todtenkranz.", "tokens": ["Ih\u00b7rer", "Freun\u00b7din", "ei\u00b7nen", "Tod\u00b7ten\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ach! kein M\u00e4dchen war der Thr\u00e4nen wehrter,", "tokens": ["Ach", "!", "kein", "M\u00e4d\u00b7chen", "war", "der", "Thr\u00e4\u00b7nen", "wehr\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Als du gutes frommes M\u00e4dchen bist,", "tokens": ["Als", "du", "gu\u00b7tes", "from\u00b7mes", "M\u00e4d\u00b7chen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und im Himmel ist kein Geist verkl\u00e4rter,", "tokens": ["Und", "im", "Him\u00b7mel", "ist", "kein", "Geist", "ver\u00b7kl\u00e4r\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Als die Seele R\u00f6schens ist.", "tokens": ["Als", "die", "See\u00b7le", "R\u00f6\u00b7schens", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}