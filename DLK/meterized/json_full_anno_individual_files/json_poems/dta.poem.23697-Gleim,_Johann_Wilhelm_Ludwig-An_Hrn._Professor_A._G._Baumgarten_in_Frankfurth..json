{"dta.poem.23697": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Hrn. Professor A. G. Baumgarten  \n in Frankfurth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lehrer, den die Gottheit lehrte,", "tokens": ["Leh\u00b7rer", ",", "den", "die", "Got\u00b7theit", "lehr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lehrer, den die Weisheit liebet,", "tokens": ["Leh\u00b7rer", ",", "den", "die", "Weis\u00b7heit", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehrer, der mit Licht und Leben,", "tokens": ["Leh\u00b7rer", ",", "der", "mit", "Licht", "und", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit freundlichen Beweisen,", "tokens": ["Und", "mit", "freund\u00b7li\u00b7chen", "Be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tugend, Witz und Warheit stiftet.", "tokens": ["Tu\u00b7gend", ",", "Witz", "und", "War\u00b7heit", "stif\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieh, wie stark sind deine Lehren!", "tokens": ["Sieh", ",", "wie", "stark", "sind", "dei\u00b7ne", "Leh\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh, sie \u00fcberwinden Zweifler;", "tokens": ["Sieh", ",", "sie", "\u00fc\u00b7berw\u00b7in\u00b7den", "Zweif\u00b7ler", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Sie entwafnen Warheitsfeinde;", "tokens": ["Sie", "ent\u00b7waf\u00b7nen", "War\u00b7heits\u00b7fein\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sie gewinnen Weisheitssp\u00f6tter!", "tokens": ["Sie", "ge\u00b7win\u00b7nen", "Weis\u00b7heits\u00b7sp\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seelen, nein, ich will sie nennen:", "tokens": ["See\u00b7len", ",", "nein", ",", "ich", "will", "sie", "nen\u00b7nen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Todte, schlafende Monaden,", "tokens": ["Tod\u00b7te", ",", "schla\u00b7fen\u00b7de", "Mo\u00b7na\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.12": {"text": "Wekken sie aus tiefem Schlummer.", "tokens": ["Wek\u00b7ken", "sie", "aus", "tie\u00b7fem", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Zwanzig fromme Hauspostillen", "tokens": ["Zwan\u00b7zig", "from\u00b7me", "Haus\u00b7pos\u00b7til\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Leiten nicht so schnell zur Tugend,", "tokens": ["Lei\u00b7ten", "nicht", "so", "schnell", "zur", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Als wenn du mit sch\u00f6nen Worten,", "tokens": ["Als", "wenn", "du", "mit", "sch\u00f6\u00b7nen", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Und mit freundlichen Beweisen,", "tokens": ["Und", "mit", "freund\u00b7li\u00b7chen", "Be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Einmal nur die Tugend lehrest.", "tokens": ["Ein\u00b7mal", "nur", "die", "Tu\u00b7gend", "leh\u00b7rest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Denk einmal an deine Siege,", "tokens": ["Denk", "ein\u00b7mal", "an", "dei\u00b7ne", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "An den Seegen deiner Lehren.", "tokens": ["An", "den", "See\u00b7gen", "dei\u00b7ner", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}