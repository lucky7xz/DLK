{"dta.poem.11477": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen unverst\u00e4ndigen vers-richter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich noch schlimme verse schrieb,", "tokens": ["Als", "ich", "noch", "schlim\u00b7me", "ver\u00b7se", "schrieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So waren sie dir trefflich lieb;", "tokens": ["So", "wa\u00b7ren", "sie", "dir", "treff\u00b7lich", "lieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jtzt aber pflegst du nur zu lachen:", "tokens": ["Jtzt", "a\u00b7ber", "pflegst", "du", "nur", "zu", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein tichten steht dir nicht mehr an.", "tokens": ["Mein", "tich\u00b7ten", "steht", "dir", "nicht", "mehr", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Daher ich sicher schl\u00fcssen kan:", "tokens": ["Da\u00b7her", "ich", "si\u00b7cher", "schl\u00fcs\u00b7sen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich m\u00fc\u00df\u2019 itzt bessre reime machen.", "tokens": ["Ich", "m\u00fc\u00df'", "itzt", "bess\u00b7re", "rei\u00b7me", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}