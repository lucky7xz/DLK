{"dta.poem.11759": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es kam ein Wanderer durch einen \u00f6den Raum", "tokens": ["Es", "kam", "ein", "Wan\u00b7de\u00b7rer", "durch", "ei\u00b7nen", "\u00f6\u00b7den", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An einen gr\u00fcnen Fleck, da stand ein sch\u00f6ner Baum.", "tokens": ["An", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Fleck", ",", "da", "stand", "ein", "sch\u00f6\u00b7ner", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und an des Baumes Fu\u00df ergo\u00df sich eine Quelle,", "tokens": ["Und", "an", "des", "Bau\u00b7mes", "Fu\u00df", "er\u00b7go\u00df", "sich", "ei\u00b7ne", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine Blume sah sich in der klaren Welle.", "tokens": ["Und", "ei\u00b7ne", "Blu\u00b7me", "sah", "sich", "in", "der", "kla\u00b7ren", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch auf dem Baume sa\u00df ein Vogel hoch und sang:", "tokens": ["Auch", "auf", "dem", "Bau\u00b7me", "sa\u00df", "ein", "Vo\u00b7gel", "hoch", "und", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wandrer ruhte froh sich aus von seinem Gang.", "tokens": ["Der", "Wand\u00b7rer", "ruh\u00b7te", "froh", "sich", "aus", "von", "sei\u00b7nem", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und sprach: wie Schad' um euch, da\u00df ihr hier beide singt", "tokens": ["Und", "sprach", ":", "wie", "Schad'", "um", "euch", ",", "da\u00df", "ihr", "hier", "bei\u00b7de", "singt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PWAV", "NN", "APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bl\u00fcht, wo keinem Aug' und Ohre Lust es bringt.", "tokens": ["Und", "bl\u00fcht", ",", "wo", "kei\u00b7nem", "Aug'", "und", "Oh\u00b7re", "Lust", "es", "bringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIAT", "NN", "KON", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da sprach die Gottheit, die im Baume wohnte, leise:", "tokens": ["Da", "sprach", "die", "Got\u00b7theit", ",", "die", "im", "Bau\u00b7me", "wohn\u00b7te", ",", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Wandrer, den zu mir gef\u00fchret hat die Reise!", "tokens": ["O", "Wand\u00b7rer", ",", "den", "zu", "mir", "ge\u00b7f\u00fch\u00b7ret", "hat", "die", "Rei\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie bl\u00fchen nicht umsonst, sie bl\u00fchn und singen mir,", "tokens": ["Sie", "bl\u00fc\u00b7hen", "nicht", "um\u00b7sonst", ",", "sie", "bl\u00fchn", "und", "sin\u00b7gen", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PPER", "VVINF", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weil du bei mir ruhst, bl\u00fchn sie und singen dir.", "tokens": ["Und", "weil", "du", "bei", "mir", "ruhst", ",", "bl\u00fchn", "sie", "und", "sin\u00b7gen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}