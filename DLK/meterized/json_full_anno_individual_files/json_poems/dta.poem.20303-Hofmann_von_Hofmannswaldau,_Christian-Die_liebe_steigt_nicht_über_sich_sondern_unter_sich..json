{"dta.poem.20303": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die liebe steigt nicht \u00fcber sich/  \n sondern unter sich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein auge solte mir zum tempel neulich dienen/", "tokens": ["Dein", "au\u00b7ge", "sol\u00b7te", "mir", "zum", "tem\u00b7pel", "neu\u00b7lich", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allein der grosse brand that meiner seelen weh:", "tokens": ["Al\u00b7lein", "der", "gros\u00b7se", "brand", "that", "mei\u00b7ner", "see\u00b7len", "weh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum zog sie sich hinab zu deiner wollust see/", "tokens": ["Drum", "zog", "sie", "sich", "hin\u00b7ab", "zu", "dei\u00b7ner", "wol\u00b7lust", "see", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und k\u00fchlte wieder sich mit nectar und rosienen.", "tokens": ["Und", "k\u00fchl\u00b7te", "wie\u00b7der", "sich", "mit", "nec\u00b7tar", "und", "ro\u00b7sie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-++-+--", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Sie tranck/ und ward ber\u00e4uscht aus deinen mund-rubinen/", "tokens": ["Sie", "tranck", "/", "und", "ward", "be\u00b7r\u00e4uscht", "aus", "dei\u00b7nen", "mun\u00b7dru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und taumelte von dar auff deiner br\u00fcste schnee/", "tokens": ["Und", "tau\u00b7mel\u00b7te", "von", "dar", "auff", "dei\u00b7ner", "br\u00fcs\u00b7te", "schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die zweyen bergen gleich/ von wegen ihrer h\u00f6h/", "tokens": ["Die", "zwe\u00b7yen", "ber\u00b7gen", "gleich", "/", "von", "we\u00b7gen", "ih\u00b7rer", "h\u00f6h", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "ADV", "$(", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Am gipffel etwas roth/ sonst gantz beeiset schienen.", "tokens": ["Am", "gipf\u00b7fel", "et\u00b7was", "roth", "/", "sonst", "gantz", "be\u00b7ei\u00b7set", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$(", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch/ weil hier k\u00e4lte war/ sie aber nackt und blo\u00df/", "tokens": ["Doch", "/", "weil", "hier", "k\u00e4l\u00b7te", "war", "/", "sie", "a\u00b7ber", "nackt", "und", "blo\u00df", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "ADV", "VVFIN", "VAFIN", "$(", "PPER", "ADV", "ADJD", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So kroch sie endlich gar in deinen warmen schoo\u00df/", "tokens": ["So", "kroch", "sie", "end\u00b7lich", "gar", "in", "dei\u00b7nen", "war\u00b7men", "schoo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da ward ihr allererst ihr lager angezeiget.", "tokens": ["Da", "ward", "ihr", "al\u00b7le\u00b7rerst", "ihr", "la\u00b7ger", "an\u00b7ge\u00b7zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Climene/ z\u00fcrne nicht. Sie folget der Natur/", "tokens": ["Cli\u00b7me\u00b7ne", "/", "z\u00fcr\u00b7ne", "nicht", ".", "Sie", "fol\u00b7get", "der", "Na\u00b7tur", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Sie geht den reglen nach/ und h\u00e4lt der liebe spur/", "tokens": ["Sie", "geht", "den", "reg\u00b7len", "nach", "/", "und", "h\u00e4lt", "der", "lie\u00b7be", "spur", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPR", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die mehrmahls unter sich/ nicht aber auffw\u00e4rts steiget.", "tokens": ["Die", "mehr\u00b7mahls", "un\u00b7ter", "sich", "/", "nicht", "a\u00b7ber", "auf\u00b7fw\u00e4rts", "stei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PRF", "$(", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}