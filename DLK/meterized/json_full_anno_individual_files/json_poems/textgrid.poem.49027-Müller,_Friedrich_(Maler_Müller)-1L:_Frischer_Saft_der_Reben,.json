, "blin\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie sein Ro\u00df beschwert.", "tokens": ["Wie", "sein", "Ro\u00df", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seht, das Schicksal winket", "tokens": ["Seht", ",", "das", "Schick\u00b7sal", "win\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und \u2013 der Prahlhans sinket", "tokens": ["Und", "\u2013", "der", "Prahl\u00b7hans", "sin\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$(", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nackt und blo\u00df zur Erd'.", "tokens": ["Nackt", "und", "blo\u00df", "zur", "Erd'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Stieg' zum Sternenheere", "tokens": ["Stieg'", "zum", "Ster\u00b7nen\u00b7hee\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jener siegend hin,", "tokens": ["Je\u00b7ner", "sie\u00b7gend", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn's noch h\u00f6her w\u00e4re \u2013", "tokens": ["Wenn's", "noch", "h\u00f6\u00b7her", "w\u00e4\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VAFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}