", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nicht beisammen sein.", "tokens": ["Nicht", "bei\u00b7sam\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}