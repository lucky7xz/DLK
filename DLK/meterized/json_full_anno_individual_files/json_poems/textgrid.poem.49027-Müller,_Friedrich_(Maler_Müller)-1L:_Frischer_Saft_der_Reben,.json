 "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Heut' die Lust zu z\u00fcgeln,", "tokens": ["Heut'", "die", "Lust", "zu", "z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gilt mehr uns als Kl\u00fcgeln,", "tokens": ["Gilt", "mehr", "uns", "als", "Kl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "KOUS", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bacchus steht hier still.", "tokens": ["Bac\u00b7chus", "steht", "hier", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Was hilft alles Sorgen", "tokens": ["Was", "hilft", "al\u00b7les", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "F\u00fcr der Zukunft Gl\u00fcck?", "tokens": ["F\u00fcr", "der", "Zu\u00b7kunft", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mir liegt nichts am Morgen,", "tokens": ["Mir", "liegt", "nichts", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bin ich nur geborgen", "tokens": ["Bin", "ich", "nur", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "F\u00fcr den Augenblick!", "tokens": ["F\u00fcr", "den", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "T\u00f6richt ist's zu gr\u00e4men", "tokens": ["T\u00f6\u00b7richt", "ist's", "zu", "gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sich um Gold und Pracht;", "tokens": ["Sich", "um", "Gold", "und", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn wir's mit uns n\u00e4hmen,", "tokens": ["Wenn", "wir's", "mit", "uns", "n\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenn wir wieder k\u00e4men", "tokens": ["Wenn", "wir", "wie\u00b7der", "k\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Aus des Hades Nacht!", "tokens": ["Aus", "des", "Ha\u00b7des", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Dieser will noch scharren", "tokens": ["Die\u00b7ser", "will", "noch", "schar\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Heut' nach Gut und Geld;", "tokens": ["Heut'", "nach", "Gut", "und", "Geld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wird die Parze harren,", "tokens": ["Wird", "die", "Par\u00b7ze", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bis es wohl dem Narren", "tokens": ["Bis", "es", "wohl", "dem", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mitzugehn gef\u00e4llt?", "tokens": ["Mit\u00b7zu\u00b7gehn", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Der von Orden blinket", "tokens": ["Der", "von", "Or\u00b7den", "blin\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie sein Ro\u00df beschwert.", "tokens": ["Wie", "sein", "Ro\u00df", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seht, das Schicksal winket", "tokens": ["Seht", ",", "das", "Schick\u00b7sal", "win\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und \u2013 der Prahlhans sinket", "tokens": ["Und", "\u2013", "der", "Prahl\u00b7hans", "sin\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$(", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nackt und blo\u00df zur Erd'.", "tokens": ["Nackt", "und", "blo\u00df", 