{"dta.poem.23140": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "220. Wie GOtt gefunden wird.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Gott recht finden wil/ mu\u00df sich zuvor verliehren/", "tokens": ["Wer", "Gott", "recht", "fin\u00b7den", "wil", "/", "mu\u00df", "sich", "zu\u00b7vor", "ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVINF", "VMFIN", "$(", "VMFIN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bi\u00df in Ewigkeit nicht wieder sehn noch sp\u00fcren.", "tokens": ["Und", "bi\u00df", "in", "E\u00b7wig\u00b7keit", "nicht", "wie\u00b7der", "sehn", "noch", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PTKNEG", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}