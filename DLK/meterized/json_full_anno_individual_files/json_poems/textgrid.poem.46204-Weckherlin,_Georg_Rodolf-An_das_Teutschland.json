{"textgrid.poem.46204": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An das Teutschland", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerbrich das schwere joch, darunder du gebunden,", "tokens": ["Zer\u00b7brich", "das", "schwe\u00b7re", "joch", ",", "da\u00b7run\u00b7der", "du", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "o Teutschland, wach doch auf, fa\u00df wider einen mut,", "tokens": ["o", "Teutschland", ",", "wach", "doch", "auf", ",", "fa\u00df", "wi\u00b7der", "ei\u00b7nen", "mut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "gebrauch dein altes herz und widersteh der wut", "tokens": ["ge\u00b7brauch", "dein", "al\u00b7tes", "herz", "und", "wi\u00b7der\u00b7steh", "der", "wut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die dich und die freiheit durch dich selbs \u00fcberwunden.", "tokens": ["die", "dich", "und", "die", "frei\u00b7heit", "durch", "dich", "selbs", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "ART", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Straf nu die tyrannei, die dich schier gar geschunden,", "tokens": ["Straf", "nu", "die", "ty\u00b7ran\u00b7nei", ",", "die", "dich", "schier", "gar", "ge\u00b7schun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "und l\u00f6sch doch endlich aus die (dich verzehrend) glut", "tokens": ["und", "l\u00f6sch", "doch", "end\u00b7lich", "aus", "die", "(", "dich", "ver\u00b7zeh\u00b7rend", ")", "glut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "ADV", "ADV", "APPR", "ART", "$(", "PPER", "VVPP", "$(", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "nicht mit dein eignem schwei\u00df, sondern dem b\u00f6sen blut,", "tokens": ["nicht", "mit", "dein", "eig\u00b7nem", "schwei\u00df", ",", "son\u00b7dern", "dem", "b\u00f6\u00b7sen", "blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "flie\u00dfend aus deiner feind und falschen br\u00fcdern wunden.", "tokens": ["flie\u00b7\u00dfend", "aus", "dei\u00b7ner", "feind", "und", "fal\u00b7schen", "br\u00fc\u00b7dern", "wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "KON", "ADJA", "ADJA", "ADJA", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Verlassend dich auf got, folg denen f\u00fcrsten nach,", "tokens": ["Ver\u00b7las\u00b7send", "dich", "auf", "got", ",", "folg", "de\u00b7nen", "f\u00fcrs\u00b7ten", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "NE", "$,", "VVFIN", "PDS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "die sein gerechte hand will, so du wilt, bewahren", "tokens": ["die", "sein", "ge\u00b7rech\u00b7te", "hand", "will", ",", "so", "du", "wilt", ",", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VMFIN", "$,", "ADV", "PPER", "VMFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "zu der getreuen trost, zu der treulosen rach:", "tokens": ["zu", "der", "ge\u00b7treu\u00b7en", "trost", ",", "zu", "der", "treu\u00b7lo\u00b7sen", "rach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So la\u00df nu alle forcht, und nicht die zeit, hinfahren,", "tokens": ["So", "la\u00df", "nu", "al\u00b7le", "forcht", ",", "und", "nicht", "die", "zeit", ",", "hin\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PIS", "VVFIN", "$,", "KON", "PTKNEG", "ART", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und got wird aller welt, da\u00df nichts dan schand und schmach", "tokens": ["und", "got", "wird", "al\u00b7ler", "welt", ",", "da\u00df", "nichts", "dan", "schand", "und", "schmach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "wo