{"dta.poem.13269": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Flug ist Adlern frey; sie fliegen wie sie wollen;", "tokens": ["\u201e", "der", "Flug", "ist", "Ad\u00b7lern", "frey", ";", "sie", "flie\u00b7gen", "wie", "sie", "wol\u00b7len", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "NN", "PTKVZ", "$.", "PPER", "VVINF", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewas k\u00f6nnten sie dann heut uns vorbedeuten sollen?", "tokens": ["\u201e", "was", "k\u00f6nn\u00b7ten", "sie", "dann", "heut", "uns", "vor\u00b7be\u00b7deu\u00b7ten", "sol\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "445\u201dGesezt: sie fl\u00f6gen hin auf eines Bauers Haus;", "tokens": ["\"", "Ge\u00b7sezt", ":", "sie", "fl\u00f6\u00b7gen", "hin", "auf", "ei\u00b7nes", "Bau\u00b7ers", "Haus", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewas br\u00e4cht\u2019 in solchem Fall des P\u00f6bels Wiz daraus?", "tokens": ["\u201e", "was", "br\u00e4cht'", "in", "sol\u00b7chem", "Fall", "des", "P\u00f6\u00b7bels", "Wiz", "da\u00b7raus", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201egesezt: man s\u00e4he sie nach Berg- und Klippen eilen;", "tokens": ["\u201e", "ge\u00b7sezt", ":", "man", "s\u00e4\u00b7he", "sie", "nach", "Ber\u00b7g", "und", "Klip\u00b7pen", "ei\u00b7len", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "PIS", "VVFIN", "PPER", "APPR", "TRUNC", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "\u201ewas wurde man dem Flug vor eine Kraft ertheilen?", "tokens": ["\u201e", "was", "wur\u00b7de", "man", "dem", "Flug", "vor", "ei\u00b7ne", "Kraft", "er\u00b7thei\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PIS", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewas Fl\u00fcgel hat, das fliegt; wer kehret sich daran?", "tokens": ["\u201e", "was", "Fl\u00fc\u00b7gel", "hat", ",", "das", "fliegt", ";", "wer", "keh\u00b7ret", "sich", "da\u00b7ran", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "VAFIN", "$,", "PDS", "VVFIN", "$.", "PWS", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "450\u201dEin Weiser fraget nicht, was es bedeuten kann.", "tokens": ["\"", "Ein", "Wei\u00b7ser", "fra\u00b7get", "nicht", ",", "was", "es", "be\u00b7deu\u00b7ten", "kann", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edie Adler pflegen sich an jenen Ort zu schwingen,", "tokens": ["\u201e", "die", "Ad\u00b7ler", "pfle\u00b7gen", "sich", "an", "je\u00b7nen", "Ort", "zu", "schwin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewo Sicherheit und Ruh und Nahrung aufzubringen;", "tokens": ["\u201e", "wo", "Si\u00b7cher\u00b7heit", "und", "Ruh", "und", "Nah\u00b7rung", "auf\u00b7zu\u00b7brin\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "KON", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewo ihrer Jungen Brut, die zwar kein Wetter scheut,", "tokens": ["\u201e", "wo", "ih\u00b7rer", "Jun\u00b7gen", "Brut", ",", "die", "zwar", "kein", "Wet\u00b7ter", "scheut", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201evon den Verfolgungen des Adler-Feinds befreyt.", "tokens": ["\u201e", "von", "den", "Ver\u00b7fol\u00b7gun\u00b7gen", "des", "Ad\u00b7ler\u00b7Feinds", "be\u00b7freyt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}