{"dta.poem.7592": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bez\u00e4hme deinen Zorn, und lasse dem die Rache,", "tokens": ["Be\u00b7z\u00e4h\u00b7me", "dei\u00b7nen", "Zorn", ",", "und", "las\u00b7se", "dem", "die", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der besser als du selbst kann f\u00fchren deine Sache.", "tokens": ["Der", "bes\u00b7ser", "als", "du", "selbst", "kann", "f\u00fch\u00b7ren", "dei\u00b7ne", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "PPER", "ADV", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der strenge K\u00f6nig, der nie ein Vergehn vergeben,", "tokens": ["Der", "stren\u00b7ge", "K\u00f6\u00b7nig", ",", "der", "nie", "ein", "Ver\u00b7gehn", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhielt, weil eines er vergab, dadurch sein Leben.", "tokens": ["Er\u00b7hielt", ",", "weil", "ei\u00b7nes", "er", "ver\u00b7gab", ",", "da\u00b7durch", "sein", "Le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du fragst, wie dieses war? ich will es dir berichten,", "tokens": ["Du", "fragst", ",", "wie", "die\u00b7ses", "war", "?", "ich", "will", "es", "dir", "be\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDS", "VAFIN", "$.", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie mir es kund gethan wahrhaftige Geschichten.", "tokens": ["Wie", "mir", "es", "kund", "ge\u00b7than", "wahr\u00b7haf\u00b7ti\u00b7ge", "Ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PTKVZ", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der K\u00f6nig auf der Jagd in k\u00fchnem Uebermuth", "tokens": ["Der", "K\u00f6\u00b7nig", "auf", "der", "Jagd", "in", "k\u00fch\u00b7nem", "Ue\u00b7ber\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwelgt in der Thiere jetzt wie sonst in Menschenblut.", "tokens": ["Schwelgt", "in", "der", "Thie\u00b7re", "jetzt", "wie", "sonst", "in", "Men\u00b7schen\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "KOKOM", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Auf einmal, wie er steht im stolzen J\u00e4gerchor,", "tokens": ["Auf", "ein\u00b7mal", ",", "wie", "er", "steht", "im", "stol\u00b7zen", "J\u00e4\u00b7ger\u00b7chor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fliegt her ein Ungl\u00fcckspfeil und streift sein linkes Ohr.", "tokens": ["Fliegt", "her", "ein", "Un\u00b7gl\u00fccks\u00b7pfeil", "und", "streift", "sein", "lin\u00b7kes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie wird der rasche Grimm des K\u00f6nigs jetzt entlodern,", "tokens": ["Wie", "wird", "der", "ra\u00b7sche", "Grimm", "des", "K\u00f6\u00b7nigs", "jetzt", "ent\u00b7lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NE", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sein vergo\u00dfnes Blut wie blut'ge Rache fodern!", "tokens": ["Und", "sein", "ver\u00b7go\u00df\u00b7nes", "Blut", "wie", "blut'\u00b7ge", "Ra\u00b7che", "fo\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Allein es ist alsob der Pfeil ihm hab' ins Ohr", "tokens": ["Al\u00b7lein", "es", "ist", "al\u00b7sob", "der", "Pfeil", "ihm", "hab'", "ins", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ART", "NN", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein leises Wort gesagt, das seinen Grimm beschwor.", "tokens": ["Ein", "lei\u00b7ses", "Wort", "ge\u00b7sagt", ",", "das", "sei\u00b7nen", "Grimm", "be\u00b7schwor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}