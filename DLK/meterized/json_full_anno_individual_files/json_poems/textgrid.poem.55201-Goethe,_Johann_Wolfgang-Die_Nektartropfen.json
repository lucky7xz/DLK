{"textgrid.poem.55201": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Nektartropfen", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Minerva, jenen Liebling,", "tokens": ["Als", "Mi\u00b7ner\u00b7va", ",", "je\u00b7nen", "Lieb\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Prometheus, zu beg\u00fcnst'gen,", "tokens": ["Den", "Pro\u00b7me\u00b7theus", ",", "zu", "be\u00b7g\u00fcnst'\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eine volle Nektarschale", "tokens": ["Ei\u00b7ne", "vol\u00b7le", "Nek\u00b7tarsc\u00b7ha\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Himmel niederbrachte,", "tokens": ["Von", "dem", "Him\u00b7mel", "nie\u00b7der\u00b7brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Menschen zu begl\u00fccken", "tokens": ["Sei\u00b7ne", "Men\u00b7schen", "zu", "be\u00b7gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Trieb zu holden K\u00fcnsten", "tokens": ["Und", "den", "Trieb", "zu", "hol\u00b7den", "K\u00fcns\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihrem Busen einzufl\u00f6\u00dfen,", "tokens": ["Ih\u00b7rem", "Bu\u00b7sen", "ein\u00b7zu\u00b7fl\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eilte sie mit schnellen F\u00fc\u00dfen,", "tokens": ["Eil\u00b7te", "sie", "mit", "schnel\u00b7len", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df sie Jupiter nicht s\u00e4he;", "tokens": ["Da\u00df", "sie", "Ju\u00b7pi\u00b7ter", "nicht", "s\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die goldne Schale schwankte,", "tokens": ["Und", "die", "gold\u00b7ne", "Scha\u00b7le", "schwank\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und es fielen wenig Tropfen", "tokens": ["Und", "es", "fie\u00b7len", "we\u00b7nig", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auf den gr\u00fcnen Boden nieder.", "tokens": ["Auf", "den", "gr\u00fc\u00b7nen", "Bo\u00b7den", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Emsig waren drauf die Bienen", "tokens": ["Em\u00b7sig", "wa\u00b7ren", "drauf", "die", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinterher und saugten flei\u00dfig;", "tokens": ["Hin\u00b7ter\u00b7her", "und", "saug\u00b7ten", "flei\u00b7\u00dfig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam der Schmetterling gesch\u00e4ftig,", "tokens": ["Kam", "der", "Schmet\u00b7ter\u00b7ling", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch ein Tr\u00f6pfchen zu erhaschen;", "tokens": ["Auch", "ein", "Tr\u00f6pf\u00b7chen", "zu", "er\u00b7ha\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst die ungestalte Spinne", "tokens": ["Selbst", "die", "un\u00b7ge\u00b7stal\u00b7te", "Spin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kroch herbei und sog gewaltig.", "tokens": ["Kroch", "her\u00b7bei", "und", "sog", "ge\u00b7wal\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gl\u00fccklich haben sie gekostet,", "tokens": ["Gl\u00fcck\u00b7lich", "ha\u00b7ben", "sie", "ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie und andre zarte Tierch