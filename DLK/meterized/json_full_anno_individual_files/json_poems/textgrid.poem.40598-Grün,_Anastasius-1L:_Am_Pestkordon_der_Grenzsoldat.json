{"textgrid.poem.40598": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Pestkordon der Grenzsoldat", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Pestkordon der Grenzsoldat", "tokens": ["Am", "Pest\u00b7kor\u00b7don", "der", "Grenz\u00b7sol\u00b7dat"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der Muskete steht,", "tokens": ["Mit", "der", "Mus\u00b7ke\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Jenseits des Stroms auf blum'gem Pfad", "tokens": ["Jen\u00b7seits", "des", "Stroms", "auf", "blum'\u00b7gem", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das T\u00fcrkenm\u00e4dchen geht.", "tokens": ["Das", "T\u00fcr\u00b7ken\u00b7m\u00e4d\u00b7chen", "geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dazwischen hin die Donau zieht,", "tokens": ["Da\u00b7zwi\u00b7schen", "hin", "die", "Do\u00b7nau", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Strom des Todes gleich,", "tokens": ["Dem", "Strom", "des", "To\u00b7des", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sel'ge und Lebend'ge schied", "tokens": ["Der", "Sel'\u00b7ge", "und", "Le\u00b7ben\u00b7d'\u00b7ge", "schied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Erd- und Geisterreich.", "tokens": ["Und", "Erd", "und", "Geis\u00b7ter\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was dr\u00fcben bl\u00fcht, was dr\u00fcben strebt,", "tokens": ["Was", "dr\u00fc\u00b7ben", "bl\u00fcht", ",", "was", "dr\u00fc\u00b7ben", "strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist f\u00fcr die Andern hie,", "tokens": ["Ist", "f\u00fcr", "die", "An\u00b7dern", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4r's verwelkt l\u00e4ngst und verlebt", "tokens": ["Als", "w\u00e4r's", "ver\u00b7welkt", "l\u00e4ngst", "und", "ver\u00b7lebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oder geboren nie.", "tokens": ["O\u00b7der", "ge\u00b7bo\u00b7ren", "nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Blumen, die dort dr\u00fcben stehn,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "dort", "dr\u00fc\u00b7ben", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind so fern f\u00fcr ihn,", "tokens": ["Sie", "sind", "so", "fern", "f\u00fcr", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als hab' er sie im Traum gesehn", "tokens": ["Als", "hab'", "er", "sie", "im", "Traum", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Himmelsgarten bl\u00fchn.", "tokens": ["Im", "Him\u00b7mels\u00b7gar\u00b7ten", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die goldnen Fr\u00fcchte, die gedr\u00e4ngt", "tokens": ["Die", "gold\u00b7nen", "Fr\u00fcch\u00b7te", ",", "die", "ge\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fruchthain dr\u00fcben beut,", "tokens": ["Der", "Frucht\u00b7hain", "dr\u00fc\u00b7ben", "beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr ihn sind sie wie aufgeh\u00e4ngt", "tokens": ["F\u00fcr", "ihn", "sind", "sie", "wie", "auf\u00b7ge\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "KOKOM", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Hain der Ewigkeit.", "tokens": ["Im", "Hain", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die T\u00fcrkenmaid, die dort entlang", "tokens": ["Die", "T\u00fcr\u00b7ken\u00b7maid", ",", "die", "dort", "ent\u00b7lang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des sch\u00f6nen Stroms lustwallt,", "tokens": ["Des", "sch\u00f6\u00b7nen", "Stroms", "lust\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr ihn wallt sie der Todten Gang", "tokens": ["F\u00fcr", "ihn", "wallt", "sie", "der", "Tod\u00b7ten", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "In eines Geists Gestalt.", "tokens": ["In", "ei\u00b7nes", "Geists", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Leuchten ihrer Augen quillt", "tokens": ["Das", "Leuch\u00b7ten", "ih\u00b7rer", "Au\u00b7gen", "quillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch wei\u00dfe Schleier vor,", "tokens": ["Durch", "wei\u00b7\u00dfe", "Schlei\u00b7er", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm sind's nur Sterne, schimmernd mild", "tokens": ["Ihm", "sin\u00b7d's", "nur", "Ster\u00b7ne", ",", "schim\u00b7mernd", "mild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "ADJD", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus wei\u00dfem Wolkenflor.", "tokens": ["Aus", "wei\u00b7\u00dfem", "Wol\u00b7ken\u00b7flor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da fa\u00dft der Sehnsucht tiefe Macht", "tokens": ["Da", "fa\u00dft", "der", "Sehn\u00b7sucht", "tie\u00b7fe", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des jungen Kriegers Herz,", "tokens": ["Des", "jun\u00b7gen", "Krie\u00b7gers", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie's zieht in stiller Vollmondnacht", "tokens": ["Wie's", "zieht", "in", "stil\u00b7ler", "Voll\u00b7mond\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Wandrer sternenw\u00e4rts.", "tokens": ["Den", "Wand\u00b7rer", "ster\u00b7nen\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Fast meint er einen Blick zu thun", "tokens": ["Fast", "meint", "er", "ei\u00b7nen", "Blick", "zu", "thun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In fernes Geisterland,", "tokens": ["In", "fer\u00b7nes", "Geis\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn nicht ganz andre Bilder nun", "tokens": ["Wenn", "nicht", "ganz", "and\u00b7re", "Bil\u00b7der", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar irdisch ihn gemahnt!", "tokens": ["Gar", "ir\u00b7disch", "ihn", "ge\u00b7mahnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auf raschem Pferd der Spahi Zahl,", "tokens": ["Auf", "ra\u00b7schem", "Pferd", "der", "Spa\u00b7hi", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dort vor\u00fcberbraust,", "tokens": ["Die", "dort", "vor\u00b7\u00fc\u00b7berb\u00b7raust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Staubgew\u00f6lk und S\u00e4belstrahl", "tokens": ["Da\u00df", "Staub\u00b7ge\u00b7w\u00f6lk", "und", "S\u00e4\u00b7bel\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Hufblitz sie umsaust!", "tokens": ["Und", "Huf\u00b7blitz", "sie", "um\u00b7saust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Aga, der im Moosdivan", "tokens": ["Der", "A\u00b7ga", ",", "der", "im", "Moos\u00b7di\u00b7van"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Strand die Pfeife raucht,", "tokens": ["Am", "Strand", "die", "Pfei\u00b7fe", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die als Musketenrohr hinan", "tokens": ["Die", "als", "Mus\u00b7ke\u00b7ten\u00b7rohr", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "KOUS", "NN", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Des Friedens Salven schmaucht!", "tokens": ["Des", "Frie\u00b7dens", "Sal\u00b7ven", "schmaucht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da stampft die Flinte der Soldat", "tokens": ["Da", "stampft", "die", "Flin\u00b7te", "der", "Sol\u00b7dat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Grunde unmuthvoll,", "tokens": ["Zum", "Grun\u00b7de", "un\u00b7muth\u00b7voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df aus dem Boden am Gestad'", "tokens": ["Da\u00df", "aus", "dem", "Bo\u00b7den", "am", "Ge\u00b7stad'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein banges Dr\u00f6hnen scholl!", "tokens": ["Ein", "ban\u00b7ges", "Dr\u00f6h\u00b7nen", "scholl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbo da\u00df ich steh' bei r\u00fcst'gem Leib", "tokens": ["\u00bb", "o", "da\u00df", "ich", "steh'", "bei", "r\u00fcst'\u00b7gem", "Leib"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier todt als Grenzepfahl!", "tokens": ["Hier", "todt", "als", "Gren\u00b7ze\u00b7pfahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ein alt Krankenw\u00e4rterweib", "tokens": ["Wie", "ein", "alt", "Kran\u00b7ken\u00b7w\u00e4r\u00b7ter\u00b7weib"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor einem Pestspital!", "tokens": ["Vor", "ei\u00b7nem", "Pest\u00b7spi\u00b7tal", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die Br\u00fccken schlagt', ihr Pontonier,", "tokens": ["Die", "Br\u00fc\u00b7cken", "schlagt'", ",", "ihr", "Pon\u00b7to\u00b7nier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Wagen und f\u00fcr Ro\u00df!", "tokens": ["F\u00fcr", "Wa\u00b7gen", "und", "f\u00fcr", "Ro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schiffen her, Tschaikisten ihr,", "tokens": ["Mit", "Schif\u00b7fen", "her", ",", "Tschai\u00b7kis\u00b7ten", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Mannschaft und f\u00fcr Tro\u00df!", "tokens": ["F\u00fcr", "Mann\u00b7schaft", "und", "f\u00fcr", "Tro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die Schlachten unsrer V\u00e4ter sind", "tokens": ["Die", "Schlach\u00b7ten", "uns\u00b7rer", "V\u00e4\u00b7ter", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch auszuk\u00e4mpfen dort;", "tokens": ["Noch", "aus\u00b7zu\u00b7k\u00e4mp\u00b7fen", "dort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein gutes Christenschwert gewinnt", "tokens": ["Ein", "gu\u00b7tes", "Chris\u00b7ten\u00b7schwert", "ge\u00b7winnt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch Arbeit fort und fort!", "tokens": ["Noch", "Ar\u00b7beit", "fort", "und", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Herr Hauptmann, dort von der Moschee", "tokens": ["Herr", "Haupt\u00b7mann", ",", "dort", "von", "der", "Mo\u00b7schee"], "token_info": ["word", "word", "punct", "word", "word", "word", 