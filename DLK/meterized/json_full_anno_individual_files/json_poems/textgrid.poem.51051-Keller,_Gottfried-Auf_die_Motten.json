{"textgrid.poem.51051": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Motten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwo ist ein Volk, so frei von allen Plagen,", "tokens": ["\u00bb", "wo", "ist", "ein", "Volk", ",", "so", "frei", "von", "al\u00b7len", "Pla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die andrer V\u00f6lker traurig Erbteil sind,", "tokens": ["Die", "an\u00b7drer", "V\u00f6l\u00b7ker", "trau\u00b7rig", "Erb\u00b7teil", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein gl\u00fccklicher nutznie\u00dfrisch Heldenkind", "tokens": ["Ein", "gl\u00fcck\u00b7li\u00b7cher", "nutz\u00b7nie\u00df\u00b7risch", "Hel\u00b7den\u00b7kind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als unser Schweizerv\u00f6lklein zu erfragen?", "tokens": ["Als", "un\u00b7ser", "Schwei\u00b7zer\u00b7v\u00f6l\u00b7klein", "zu", "er\u00b7fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und doch, wie fiebernd seine Pulse schlagen!", "tokens": ["Und", "doch", ",", "wie", "fie\u00b7bernd", "sei\u00b7ne", "Pul\u00b7se", "schla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr seiner Freiheit \u00dcberf\u00fclle blind,", "tokens": ["F\u00fcr", "sei\u00b7ner", "Frei\u00b7heit", "\u00dc\u00b7berf\u00b7\u00fcl\u00b7le", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hascht \u00fcberm\u00fctig es nach leerem Wind!", "tokens": ["Hascht", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", "es", "nach", "lee\u00b7rem", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wann enden seine undankbaren Klagen?\u00ab", "tokens": ["Wann", "en\u00b7den", "sei\u00b7ne", "un\u00b7dank\u00b7ba\u00b7ren", "Kla\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sprechen jene flink gelenken Motten,", "tokens": ["So", "spre\u00b7chen", "je\u00b7ne", "flink", "ge\u00b7len\u00b7ken", "Mot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die so gem\u00fctlich in dem Rauchwerk nisten,", "tokens": ["Die", "so", "ge\u00b7m\u00fct\u00b7lich", "in", "dem", "Rauch\u00b7werk", "nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem warmen, k\u00f6stlichen, und es zernagen.", "tokens": ["Dem", "war\u00b7men", ",", "k\u00f6st\u00b7li\u00b7chen", ",", "und", "es", "zer\u00b7na\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbnur eben euch gilt es noch auszurotten", "tokens": ["\u00bb", "nur", "e\u00b7ben", "euch", "gilt", "es", "noch", "aus\u00b7zu\u00b7rot\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "PPER", "VVFIN", "PPER", "ADV", "VVIZU"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "(so sprechen wir, die radikalen Christen),", "tokens": ["(", "so", "spre\u00b7chen", "wir", ",", "die", "ra\u00b7di\u00b7ka\u00b7len", "Chris\u00b7ten", ")", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit lindem Klopfen aus dem Pelz zu jagen!\u00ab", "tokens": ["Mit", "lin\u00b7dem", "Klop\u00b7fen", "aus", "dem", "Pelz", "zu", "ja\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbwo ist ein Volk, so frei von allen Plagen,", "tokens": ["\u00bb", "wo", "ist", "ein", "Volk", ",", "so", "frei", "von", "al\u00b7len", "Pla\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", 