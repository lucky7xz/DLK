{"dta.poem.2986": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das stille Volk, das sonst im Fr\u00fch- und Abendstral", "tokens": ["Das", "stil\u00b7le", "Volk", ",", "das", "sonst", "im", "Fr\u00fch", "und", "A\u00b7bend\u00b7stral"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPRART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus seinen Bergen zu den Menschen kam ins Thal,", "tokens": ["Aus", "sei\u00b7nen", "Ber\u00b7gen", "zu", "den", "Men\u00b7schen", "kam", "ins", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der stillen Feldarbeit zusah und half gewogen,", "tokens": ["Der", "stil\u00b7len", "Feld\u00b7ar\u00b7beit", "zu\u00b7sah", "und", "half", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "VVPP", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hat sich zur\u00fcck, wohin? man wei\u00df es nicht, gezogen.", "tokens": ["Hat", "sich", "zu\u00b7r\u00fcck", ",", "wo\u00b7hin", "?", "man", "wei\u00df", "es", "nicht", ",", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PRF", "PTKVZ", "$,", "PWAV", "$.", "PIS", "VVFIN", "PPER", "PTKNEG", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum? wovon ward hier das Huldenvolk verscheucht,", "tokens": ["Wa\u00b7rum", "?", "wo\u00b7von", "ward", "hier", "das", "Hul\u00b7den\u00b7volk", "ver\u00b7scheucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWAV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem verlassen nun die Arbeit schwerer keucht?", "tokens": ["Von", "dem", "ver\u00b7las\u00b7sen", "nun", "die", "Ar\u00b7beit", "schwe\u00b7rer", "keucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Einmal von wachsender Treulosigkeit der B\u00f6sen,", "tokens": ["Ein\u00b7mal", "von", "wach\u00b7sen\u00b7der", "Treu\u00b7lo\u00b7sig\u00b7keit", "der", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--++-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Dann von zunehmenden Pochhammerwerkget\u00f6sen.", "tokens": ["Dann", "von", "zu\u00b7neh\u00b7men\u00b7den", "Poch\u00b7ham\u00b7mer\u00b7werk\u00b7ge\u00b7t\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ehr die Treulosigkeit ertr\u00fcgen sie wol noch,", "tokens": ["Ehr", "die", "Treu\u00b7lo\u00b7sig\u00b7keit", "er\u00b7tr\u00fc\u00b7gen", "sie", "wol", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch hielten sie nicht aus das t\u00e4ubende Gepoch.", "tokens": ["Doch", "hiel\u00b7ten", "sie", "nicht", "aus", "das", "t\u00e4u\u00b7ben\u00b7de", "Ge\u00b7poch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es wird das stille Volk der Musen auch ausziehn,", "tokens": ["Es", "wird", "das", "stil\u00b7le", "Volk", "der", "Mu\u00b7sen", "auch", "aus\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r' ihnen nur ein Schlupf wie Zwergen auch verliehn!", "tokens": ["W\u00e4r'", "ih\u00b7nen", "nur", "ein", "Schlupf", "wie", "Zwer\u00b7gen", "auch", "ver\u00b7liehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "KOKOM", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Auch vor den B\u00f6sen w\u00e4r' im Lande noch zu bleiben,", "tokens": ["Auch", "vor", "den", "B\u00f6\u00b7sen", "w\u00e4r'", "im", "Lan\u00b7de", "noch", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch vor Get\u00f6sen nicht, die werden uns vertreiben,", "tokens": ["Doch", "vor", "Ge\u00b7t\u00f6\u00b7sen", "nicht", ",", "die", "wer\u00b7den", "uns", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKNEG", "$,", "PRELS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wann erst durchs ganze Land sich Eisenbahnen kreuzen,", "tokens": ["Wann", "erst", "durchs", "gan\u00b7ze", "Land", "sich", "Ei\u00b7sen\u00b7bah\u00b7nen", "kreu\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich h\u00f6rbar stundenweit Dampfwagen rasselnd schneuzen.", "tokens": ["Sich", "h\u00f6r\u00b7bar", "stun\u00b7den\u00b7weit", "Dampf\u00b7wa\u00b7gen", "ras\u00b7selnd", "schneu\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dann wird die Himmelskunst mit Schmach am Boden liegen,", "tokens": ["Dann", "wird", "die", "Him\u00b7mels\u00b7kunst", "mit", "Schmach", "am", "Bo\u00b7den", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann wolkenhoch der Dampf der irdischen gestiegen.", "tokens": ["Wann", "wol\u00b7ken\u00b7hoch", "der", "Dampf", "der", "ir\u00b7di\u00b7schen", "ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}