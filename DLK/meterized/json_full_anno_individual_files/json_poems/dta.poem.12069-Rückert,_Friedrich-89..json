{"dta.poem.12069": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du nach Ehre strebst, die dir die Welt soll geben,", "tokens": ["Wenn", "du", "nach", "Eh\u00b7re", "strebst", ",", "die", "dir", "die", "Welt", "soll", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00dft du, statt dir selbst, ihr zu Gefallen leben.", "tokens": ["So", "mu\u00dft", "du", ",", "statt", "dir", "selbst", ",", "ihr", "zu", "Ge\u00b7fal\u00b7len", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUI", "PPER", "ADV", "$,", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht leben in der That, nur leben auf den Schein;", "tokens": ["Nicht", "le\u00b7ben", "in", "der", "That", ",", "nur", "le\u00b7ben", "auf", "den", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht was du selber willst, was sie will, mu\u00dft du seyn.", "tokens": ["Nicht", "was", "du", "sel\u00b7ber", "willst", ",", "was", "sie", "will", ",", "mu\u00dft", "du", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "ADV", "VMFIN", "$,", "PRELS", "PPER", "VMFIN", "$,", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du nach Reichthum strebst, nach welchem alle streben,", "tokens": ["Wenn", "du", "nach", "Reicht\u00b7hum", "strebst", ",", "nach", "wel\u00b7chem", "al\u00b7le", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00dft du darum in Kampf mit allen dich begeben;", "tokens": ["Mu\u00dft", "du", "da\u00b7rum", "in", "Kampf", "mit", "al\u00b7len", "dich", "be\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "APPR", "NN", "APPR", "PIAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was andre haben, mu\u00dft du dir verloren achten,", "tokens": ["Was", "and\u00b7re", "ha\u00b7ben", ",", "mu\u00dft", "du", "dir", "ver\u00b7lo\u00b7ren", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "$,", "VMFIN", "PPER", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was du haben willst, zu rauben ihnen trachten.", "tokens": ["Und", "was", "du", "ha\u00b7ben", "willst", ",", "zu", "rau\u00b7ben", "ih\u00b7nen", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAINF", "VMFIN", "$,", "PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wenn du gar zugleich geehrt willst seyn und reich,", "tokens": ["Und", "wenn", "du", "gar", "zu\u00b7gleich", "ge\u00b7ehrt", "willst", "seyn", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVPP", "VMFIN", "VAINF", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00dft du seyn der Welt ein Freund und Feind zugleich;", "tokens": ["So", "mu\u00dft", "du", "seyn", "der", "Welt", "ein", "Freund", "und", "Feind", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "ART", "NN", "ART", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mu\u00dft stehlen ihren Schatz, und stehlen ihre Gunst;", "tokens": ["Mu\u00dft", "steh\u00b7len", "ih\u00b7ren", "Schatz", ",", "und", "steh\u00b7len", "ih\u00b7re", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist die misslichste und undankbarste Kunst.", "tokens": ["Das", "ist", "die", "miss\u00b7lichs\u00b7te", "und", "un\u00b7dank\u00b7bars\u00b7te", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Drum rath' ich: La\u00df die Welt, wen sie will ehren, ehren,", "tokens": ["Drum", "ra\u00b7th'", "ich", ":", "La\u00df", "die", "Welt", ",", "wen", "sie", "will", "eh\u00b7ren", ",", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "VVIMP", "ART", "NN", "$,", "PWS", "PPER", "VMFIN", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Und ihren Sold, wer ihn begehren will, begehren.", "tokens": ["Und", "ih\u00b7ren", "Sold", ",", "wer", "ihn", "be\u00b7geh\u00b7ren", "will", ",", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sich selbst in Ehren und sich selber reich zu halten,", "tokens": ["Sich", "selbst", "in", "Eh\u00b7ren", "und", "sich", "sel\u00b7ber", "reich", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "KON", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Mannes W\u00fcrd' und Kraft, derselben sollst du walten.", "tokens": ["Ist", "Man\u00b7nes", "W\u00fcrd'", "und", "Kraft", ",", "der\u00b7sel\u00b7ben", "sollst", "du", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VAFIN", "KON", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}