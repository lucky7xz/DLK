{"textgrid.poem.56704": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Speer", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich auch seh und h\u00f6re, glaub ich nicht,", "tokens": ["Was", "ich", "auch", "seh", "und", "h\u00f6\u00b7re", ",", "glaub", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und was ich glaube, glaub ich nimmermehr,", "tokens": ["und", "was", "ich", "glau\u00b7be", ",", "glaub", "ich", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "doch diese Weisheit, die sich selbst zerbricht,", "tokens": ["doch", "die\u00b7se", "Weis\u00b7heit", ",", "die", "sich", "selbst", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wirft mich wie einen blanken Eschenspeer,", "tokens": ["wirft", "mich", "wie", "ei\u00b7nen", "blan\u00b7ken", "E\u00b7schen\u00b7speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "der t\u00f6nend an der Scheibe Erz zerspellt,", "tokens": ["der", "t\u00f6\u00b7nend", "an", "der", "Schei\u00b7be", "Erz", "zer\u00b7spellt", ","], 