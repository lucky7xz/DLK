{"dta.poem.2781": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ejusdem. Desiderium IX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll als Actaeon ich itzt eine Fabel heissen", "tokens": ["Soll", "als", "Ac\u00b7tae\u00b7on", "ich", "itzt", "ei\u00b7ne", "Fa\u00b7bel", "heis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "NE", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der erz\u00fcrnten Hund\u2019 elende Beuthe seyn/", "tokens": ["Und", "der", "er\u00b7z\u00fcrn\u00b7ten", "Hund'", "e\u00b7len\u00b7de", "Beu\u00b7the", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Ich der ich vormals pflag der Jagt mich zu befleissen/", "tokens": ["Ich", "der", "ich", "vor\u00b7mals", "pflag", "der", "Jagt", "mich", "zu", "be\u00b7fleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "ADV", "VVFIN", "ART", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bin nunmehr selbst behetzt und lauf ins Netz hinein.", "tokens": ["Bin", "nun\u00b7mehr", "selbst", "be\u00b7hetzt", "und", "lauf", "ins", "Netz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "KON", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Schlu\u00df war durch die Jagt die Sorgen zu bestreiten/", "tokens": ["Mein", "Schlu\u00df", "war", "durch", "die", "Jagt", "die", "Sor\u00b7gen", "zu", "be\u00b7strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Diana deinen Wald den hab ich nie besucht/", "tokens": ["Di\u00b7a\u00b7na", "dei\u00b7nen", "Wald", "den", "hab", "ich", "nie", "be\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ART", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Noch in den tieffen Frost die Hunde wollen leiten/", "tokens": ["Noch", "in", "den", "tief\u00b7fen", "Frost", "die", "Hun\u00b7de", "wol\u00b7len", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df sie das Wild beharrt und vorgebeugt der Flucht.", "tokens": ["Da\u00df", "sie", "das", "Wild", "be\u00b7harrt", "und", "vor\u00b7ge\u00b7beugt", "der", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich habe nie die Hift auf ", "tokens": ["Ich", "ha\u00b7be", "nie", "die", "Hift", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Noch einem Backer je gegeben einen Fang/", "tokens": ["Noch", "ei\u00b7nem", "Ba\u00b7cker", "je", "ge\u00b7ge\u00b7ben", "ei\u00b7nen", "Fang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVPP", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Noch Wildseil aufgericht auf der Parthener Rasen/", "tokens": ["Noch", "Wild\u00b7seil", "auf\u00b7ge\u00b7richt", "auf", "der", "Par\u00b7the\u00b7ner", "Ra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Und Lappen f\u00fcrgesetzt zu hindern Spur und Gang.", "tokens": ["Und", "Lap\u00b7pen", "f\u00fcr\u00b7ge\u00b7setzt", "zu", "hin\u00b7dern", "Spur", "und", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "PTKZU", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich habe niemals Garn/ Schwerdt/ Spiese/ Pfeil und Bogen/", "tokens": ["Ich", "ha\u00b7be", "nie\u00b7mals", "Garn", "/", "Schwerdt", "/", "Spie\u00b7se", "/", "Pfeil", "und", "Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$(", "NE", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als wie die J\u00e4gerin Diana tr\u00e4gt/ gef\u00fchrt/", "tokens": ["Als", "wie", "die", "J\u00e4\u00b7ge\u00b7rin", "Di\u00b7a\u00b7na", "tr\u00e4gt", "/", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NE", "VVFIN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach wolte GOtt! ich w\u00e4r der Lust nur nachgezogen/", "tokens": ["Ach", "wol\u00b7te", "Gott", "!", "ich", "w\u00e4r", "der", "Lust", "nur", "nach\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So w\u00e4r\u2019 ich itzt kein Raub von eigner Kunst ber\u00fchrt.", "tokens": ["So", "w\u00e4r'", "ich", "itzt", "kein", "Raub", "von", "eig\u00b7ner", "Kunst", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich Unvern\u00fcnfftiger/ wo sind nun meine Jahre?", "tokens": ["Ich", "Un\u00b7ver\u00b7n\u00fcnff\u00b7ti\u00b7ger", "/", "wo", "sind", "nun", "mei\u00b7ne", "Jah\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ach h\u00e4tt\u2019 ich meine Bein\u2019 an einem Dorn verletzt?", "tokens": ["Ach", "h\u00e4tt'", "ich", "mei\u00b7ne", "Bein'", "an", "ei\u00b7nem", "Dorn", "ver\u00b7letzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Warumb hab ich gesucht des Bacchus s\u00fcsse Waare.", "tokens": ["Wa\u00b7rumb", "hab", "ich", "ge\u00b7sucht", "des", "Bac\u00b7chus", "s\u00fcs\u00b7se", "Waa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df drauf Cupido mich hat in sein Garn gesetzt.", "tokens": ["Da\u00df", "drauf", "Cu\u00b7pi\u00b7do", "mich", "hat", "in", "sein", "Garn", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "NE", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er braucht nicht nur allein den K\u00f6cher und die Pfeile/", "tokens": ["Er", "braucht", "nicht", "nur", "al\u00b7lein", "den", "K\u00f6\u00b7cher", "und", "die", "Pfei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er hat auch Netz und Strick mit welchen er ber\u00fcckt.", "tokens": ["Er", "hat", "auch", "Netz", "und", "Strick", "mit", "wel\u00b7chen", "er", "be\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "APPR", "PWAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und Bacchus leget auch den truncknen F\u00fcssen Seile/", "tokens": ["Und", "Bac\u00b7chus", "le\u00b7get", "auch", "den", "trunck\u00b7nen", "F\u00fcs\u00b7sen", "Sei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie sch\u00f6n er sonsten sich mit seinen Reben schm\u00fcckt.", "tokens": ["Wie", "sch\u00f6n", "er", "sons\u00b7ten", "sich", "mit", "sei\u00b7nen", "Re\u00b7ben", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie hat die Delila den Samson nicht gebunden/", "tokens": ["Wie", "hat", "die", "De\u00b7li\u00b7la", "den", "Sam\u00b7son", "nicht", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NE", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und ihn samt seinem Haar beraubet seine Kraft/", "tokens": ["Und", "ihn", "samt", "sei\u00b7nem", "Haar", "be\u00b7rau\u00b7bet", "sei\u00b7ne", "Kraft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ja ward der Noe nicht bey seinen T\u00f6chtern funden/", "tokens": ["Ja", "ward", "der", "Noe", "nicht", "bey", "sei\u00b7nen", "T\u00f6ch\u00b7tern", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Als ihm den Witz benahm des Weines s\u00fcsser Saft.", "tokens": ["Als", "ihm", "den", "Witz", "be\u00b7nahm", "des", "Wei\u00b7nes", "s\u00fcs\u00b7ser", "Saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mich hat die Venus auch gejaget in ihr Netze/", "tokens": ["Mich", "hat", "die", "Ve\u00b7nus", "auch", "ge\u00b7ja\u00b7get", "in", "ihr", "Net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie vielmals hat sie nicht zur Beuthe mich gemacht/", "tokens": ["Wie", "viel\u00b7mals", "hat", "sie", "nicht", "zur", "Beu\u00b7the", "mich", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Weh mir! wohin ich nur die bl\u00f6den Augen setze/", "tokens": ["Weh", "mir", "!", "wo\u00b7hin", "ich", "nur", "die", "bl\u00f6\u00b7den", "Au\u00b7gen", "set\u00b7ze", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So bin ich wie ein Wild das schon ins Garn gebracht.", "tokens": ["So", "bin", "ich", "wie", "ein", "Wild", "das", "schon", "ins", "Garn", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOKOM", "ART", "NN", "ART", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und di\u00df ist das Gesicht das den Anton erschrecket/", "tokens": ["Und", "di\u00df", "ist", "das", "Ge\u00b7sicht", "das", "den", "An\u00b7ton", "er\u00b7schre\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ART", "ART", "NE", "VVFIN", "$("], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.34": {"text": "Und das er abgemerckt auf einem Felsen hat/", "tokens": ["Und", "das", "er", "ab\u00b7ge\u00b7merckt", "auf", "ei\u00b7nem", "Fel\u00b7sen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "VVPP", "APPR", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er sah so weit sich nur der Erden Ziel erstrecket/", "tokens": ["Er", "sah", "so", "weit", "sich", "nur", "der", "Er\u00b7den", "Ziel", "er\u00b7stre\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PRF", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die gantze Welt gemahlt als wie auf einem Blat.", "tokens": ["Die", "gant\u00b7ze", "Welt", "ge\u00b7mahlt", "als", "wie", "auf", "ei\u00b7nem", "Blat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "KOUS", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und tausend C\u00f6rper auch von Menschen abgerissen/", "tokens": ["Und", "tau\u00b7send", "C\u00f6r\u00b7per", "auch", "von", "Men\u00b7schen", "ab\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die waren ingesamt mit Netzen \u00fcberspannt/", "tokens": ["Die", "wa\u00b7ren", "in\u00b7ge\u00b7samt", "mit", "Net\u00b7zen", "\u00fc\u00b7bers\u00b7pannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Jedweder f\u00fchlte Qual an H\u00e4nden/ Kopf und F\u00fcssen/", "tokens": ["Jed\u00b7we\u00b7der", "f\u00fchl\u00b7te", "Qual", "an", "H\u00e4n\u00b7den", "/", "Kopf", "und", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Wollust h\u00e4tte sie arglistig abgemannt.", "tokens": ["Die", "Wol\u00b7lust", "h\u00e4t\u00b7te", "sie", "arg\u00b7lis\u00b7tig", "ab\u00b7ge\u00b7mannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie blieben wie am Leim die albreu V\u00f6gel kleben/", "tokens": ["Sie", "blie\u00b7ben", "wie", "am", "Leim", "die", "al\u00b7breu", "V\u00f6\u00b7gel", "kle\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "ART", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Un wissend da\u