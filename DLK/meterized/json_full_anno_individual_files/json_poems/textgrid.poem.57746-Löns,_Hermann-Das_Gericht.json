{"textgrid.poem.57746": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Gericht", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Fallbeil fiel; auf dem Schafott", "tokens": ["Das", "Fall\u00b7beil", "fiel", ";", "auf", "dem", "Scha\u00b7fott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bekam er seinen Lohn;", "tokens": ["Be\u00b7kam", "er", "sei\u00b7nen", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den roten Ring um seinen Hals,", "tokens": ["Den", "ro\u00b7ten", "Ring", "um", "sei\u00b7nen", "Hals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stand er vor Gottes Thron.", "tokens": ["Stand", "er", "vor", "Got\u00b7tes", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die wei\u00dfen Engel schlugen all'", "tokens": ["Die", "wei\u00b7\u00dfen", "En\u00b7gel", "schlu\u00b7gen", "all'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die H\u00e4nde vors Gesicht,", "tokens": ["Die", "H\u00e4n\u00b7de", "vors", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und eine tiefe Stimme sprach:", "tokens": ["Und", "ei\u00b7ne", "tie\u00b7fe", "Stim\u00b7me", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbes sei ihm das Gericht!\u00ab", "tokens": ["\u00bb", "es", "sei", "ihm", "das", "Ge\u00b7richt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sprach der Geist mit hartem Blick:", "tokens": ["Es", "sprach", "der", "Geist", "mit", "har\u00b7tem", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbso fahr' zur H\u00f6lle hin;", "tokens": ["\u00bb", "so", "fahr'", "zur", "H\u00f6l\u00b7le", "hin", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast vergossen Bruderblut,", "tokens": ["Du", "hast", "ver\u00b7gos\u00b7sen", "Bru\u00b7der\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nacht sei dein Gewinn!\u00ab", "tokens": ["Die", "Nacht", "sei", "dein", "Ge\u00b7winn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die schwarzen Engel, augenlos,", "tokens": ["Die", "schwar\u00b7zen", "En\u00b7gel", ",", "au\u00b7gen\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Mienen tot und stumm,", "tokens": ["Mit", "Mie\u00b7nen", "tot", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die stellten um den Sch\u00e4cher sich", "tokens": ["Die", "stell\u00b7ten", "um", "den", "Sch\u00e4\u00b7cher", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ganz eng und dicht herum.", "tokens": ["Ganz", "eng", "und", "dicht", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es sprach der Sohn mit weichem Blick:", "tokens": ["Es", "sprach", "der", "Sohn", "mit", "wei\u00b7chem", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbla\u00dft ihn zum Lichte ein;", "tokens": ["\u00bb", "la\u00dft", "ihn", "zum", "Lich\u00b7te", "ein", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat mit Tod die Tat geb\u00fc\u00dft", "tokens": ["Er", "hat", "mit", "Tod", "die", "Tat", "ge\u00b7b\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und soll willkommen sein!\u00ab", "tokens": ["Und", "soll", "will\u00b7kom\u00b7men", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die wei\u00dfen Engel nahmen all'", "tokens": ["Die", "wei\u00b7\u00dfen", "En\u00b7gel", "nah\u00b7men", "all'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die H\u00e4nde vom Gesicht;", "tokens": ["Die", "H\u00e4n\u00b7de", "vom", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die tiefe Stimme aber sprach:", "tokens": ["Die", "tie\u00b7fe", "Stim\u00b7me", "a\u00b7ber", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbfahrt fort in dem Gericht!\u00ab", "tokens": ["\u00bb", "fahrt", "fort", "in", "dem", "Ge\u00b7richt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es sprach der Geist, es sprach der Sohn,", "tokens": ["Es", "sprach", "der", "Geist", ",", "es", "sprach", "der", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wage fiel und stieg;", "tokens": ["Die", "Wa\u00b7ge", "fiel", "und", "stieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie stieg und fiel, bis da\u00df der Sohn", "tokens": ["Sie", "stieg", "und", "fiel", ",", "bis", "da\u00df", "der", "Sohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beklommen stand und schwieg,", "tokens": ["Be\u00b7klom\u00b7men", "stand", "und", "schwieg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Stimme schwoll, die Stimme quoll,", "tokens": ["Die", "Stim\u00b7me", "schwoll", ",", "die", "Stim\u00b7me", "quoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie fiel wie Blei hinab:", "tokens": ["Sie", "fiel", "wie", "Blei", "hin\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbihr schwarzen Engel, tretet her,", "tokens": ["\u00bb", "ihr", "schwar\u00b7zen", "En\u00b7gel", ",", "tre\u00b7tet", "her", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fchrt ihn zum ewigen Grab!\u00ab", "tokens": ["F\u00fchrt", "ihn", "zum", "e\u00b7wi\u00b7gen", "Grab", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Der M\u00f6rder sah die Stimme an", "tokens": ["Der", "M\u00f6r\u00b7der", "sah", "die", "Stim\u00b7me", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach: \u00bbDas nennst du Recht?", "tokens": ["Und", "sprach", ":", "\u00bb", "Das", "nennst", "du", "Recht", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was schufest du zum Herren mich,", "tokens": ["Was", "schu\u00b7fest", "du", "zum", "Her\u00b7ren", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und machtest mich zum Knecht?\u00ab", "tokens": ["Und", "mach\u00b7test", "mich", "zum", "Knecht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ri\u00df das flammendhei\u00dfe Schwert", "tokens": ["Er", "ri\u00df", "das", "flam\u00b7mend\u00b7hei\u00b7\u00dfe", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Cherub aus der Hand", "tokens": ["Dem", "Che\u00b7rub", "aus", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schlug der schwarzen Engel Schar", "tokens": ["Und", "schlug", "der", "schwar\u00b7zen", "En\u00b7gel", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis an der H\u00f6llen Rand,", "tokens": ["Bis", "an", "der", "H\u00f6l\u00b7len", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und schrie der stummen Stimme zu:", "tokens": ["Und", "schrie", "der", "stum\u00b7men", "Stim\u00b7me", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdu tr\u00e4gst allein die Schuld;", "tokens": ["\u00bb", "du", "tr\u00e4gst", "al\u00b7lein", "die", "Schuld", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du gabst mir zu viel Leidenschaft", "tokens": ["Du", "gabst", "mir", "zu", "viel", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht genug Geduld;", "tokens": ["Und", "nicht", "ge\u00b7nug", "Ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gabst mir den Nacken steif und stolz", "tokens": ["Gabst", "mir", "den", "Na\u00b7cken", "steif", "und", "stolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und kochendhei\u00dfes Blut;", "tokens": ["Und", "ko\u00b7chend\u00b7hei\u00b7\u00dfes", "Blut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dich trifft, was ich verbrochen hab'", "tokens": ["Dich", "trifft", ",", "was", "ich", "ver\u00b7bro\u00b7chen", "hab'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In gl\u00fchendroter Wut!", "tokens": ["In", "gl\u00fc\u00b7hen\u00b7dro\u00b7ter", "Wut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Kommt her, ihr Engel ohne Blick,", "tokens": ["Kommt", "her", ",", "ihr", "En\u00b7gel", "oh\u00b7ne", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Engel, schwarz wie Nacht;", "tokens": ["Ihr", "En\u00b7gel", ",", "schwarz", "wie", "Nacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier steht ein Mann mit einer Wehr,", "tokens": ["Hier", "steht", "ein", "Mann", "mit", "ei\u00b7ner", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der euer aller lacht!", "tokens": ["Der", "eu\u00b7er", "al\u00b7ler", "lacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PIAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm' Satan her; mit Flammenschrift", "tokens": ["Komm'", "Sa\u00b7tan", "her", ";", "mit", "Flam\u00b7men\u00b7schrift"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "NN", "PTKVZ", "$.", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bemal' ich dein Gesicht;", "tokens": ["Be\u00b7mal'", "ich", "dein", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sind auch Milliarden hinter dir,", "tokens": ["Sind", "auch", "Mil\u00b7li\u00b7ar\u00b7den", "hin\u00b7ter", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ich folg' dir dennoch nicht!\u00ab", "tokens": ["Ich", "fol\u00b7g'", "dir", "den\u00b7noch", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Die tiefe Stimme stieg empor,", "tokens": ["Die", "tie\u00b7fe", "Stim\u00b7me", "stieg", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wurde leicht und hell,", "tokens": ["Sie", "wur\u00b7de", "leicht", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3