{"dta.poem.10569": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Untergang der Sonne .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Sonne", "tokens": ["Die", "sch\u00f6\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ist ruhig hinabgestiegen in's Meer;", "tokens": ["Ist", "ru\u00b7hig", "hin\u00b7ab\u00b7ge\u00b7stie\u00b7gen", "in's", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die wogenden Wasser sind schon gef\u00e4rbt", "tokens": ["Die", "wo\u00b7gen\u00b7den", "Was\u00b7ser", "sind", "schon", "ge\u00b7f\u00e4rbt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von der dunkeln Nacht,", "tokens": ["Von", "der", "dun\u00b7keln", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nur noch die Abendr\u00f6the", "tokens": ["Nur", "noch", "die", "A\u00b7bend\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ueberstreut sie mit goldnen Lichtern,", "tokens": ["Ue\u00b7bers\u00b7treut", "sie", "mit", "gold\u00b7nen", "Lich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und die rauschende Fluthgewalt", "tokens": ["Und", "die", "rau\u00b7schen\u00b7de", "Fluth\u00b7ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Dr\u00e4ngt an's Ufer die wei\u00dfen Wellen,", "tokens": ["Dr\u00e4ngt", "an's", "U\u00b7fer", "die", "wei\u00b7\u00dfen", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Die lustig und hastig h\u00fcpfen,", "tokens": ["Die", "lus\u00b7tig", "und", "has\u00b7tig", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wie wollige L\u00e4mmerheerden,", "tokens": ["Wie", "wol\u00b7li\u00b7ge", "L\u00e4m\u00b7mer\u00b7heer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die Abends der singende Hirtenjunge", "tokens": ["Die", "A\u00b7bends", "der", "sin\u00b7gen\u00b7de", "Hir\u00b7ten\u00b7jun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Nach Hause treibt.", "tokens": ["Nach", "Hau\u00b7se", "treibt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie sch\u00f6n ist die Sonne!", "tokens": ["Wie", "sch\u00f6n", "ist", "die", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So sprach nach langem Schweigen der Freund,", "tokens": ["So", "sprach", "nach", "lan\u00b7gem", "Schwei\u00b7gen", "der", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der mit mir am Strande wandelte,", "tokens": ["Der", "mit", "mir", "am", "Stran\u00b7de", "wan\u00b7del\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und scherzend halb und halb wehm\u00fcthig,", "tokens": ["Und", "scher\u00b7zend", "halb", "und", "halb", "weh\u00b7m\u00fct\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versichert' er mir: die Sonne sey", "tokens": ["Ver\u00b7si\u00b7chert'", "er", "mir", ":", "die", "Son\u00b7ne", "sey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$.", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eine sch\u00f6ne Frau, die den alten Meergott", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", "Frau", ",", "die", "den", "al\u00b7ten", "Meer\u00b7gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Aus Convenienz geheurathet;", "tokens": ["Aus", "Con\u00b7ve\u00b7ni\u00b7enz", "ge\u00b7heu\u00b7ra\u00b7thet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Des Tages \u00fcber wandle sie freudig", "tokens": ["Des", "Ta\u00b7ges", "\u00fc\u00b7ber", "wand\u00b7le", "sie", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Am hohen Himmel, purpurgeputzt,", "tokens": ["Am", "ho\u00b7hen", "Him\u00b7mel", ",", "pur\u00b7pur\u00b7ge\u00b7putzt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und diamantenblitzend,", "tokens": ["Und", "di\u00b7a\u00b7man\u00b7ten\u00b7blit\u00b7zend", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und allgeliebt und allbewundert", "tokens": ["Und", "all\u00b7ge\u00b7liebt", "und", "all\u00b7be\u00b7wun\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von allen Weltkreaturen,", "tokens": ["Von", "al\u00b7len", "Welt\u00b7kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und alle Weltkreaturen erfreuend", "tokens": ["Und", "al\u00b7le", "Welt\u00b7kre\u00b7a\u00b7tu\u00b7ren", "er\u00b7freu\u00b7end"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Mit ihres Blickes Licht und W\u00e4rme;", "tokens": ["Mit", "ih\u00b7res", "Bli\u00b7ckes", "Licht", "und", "W\u00e4r\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Aber des Abends, trostlos gezwungen,", "tokens": ["A\u00b7ber", "des", "A\u00b7bends", ",", "trost\u00b7los", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Kehre sie wieder zur\u00fcck", "tokens": ["Keh\u00b7re", "sie", "wie\u00b7der", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "In das nasse Haus, in die \u00f6den Arme", "tokens": ["In", "das", "nas\u00b7se", "Haus", ",", "in", "die", "\u00f6\u00b7den", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.15": {"text": "Des greisen Gemahls.", "tokens": ["Des", "grei\u00b7sen", "Ge\u00b7mahls", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Glaub mir's \u2014 setzte hinzu der Freund,", "tokens": ["Glaub", "mir's", "setz\u00b7te", "hin\u00b7zu", "der", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und lachte und seufzte und lachte wieder \u2014", "tokens": ["Und", "lach\u00b7te", "und", "seufz\u00b7te", "und", "lach\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die f\u00fchren dort unten die z\u00e4rtlichste Ehe!", "tokens": ["Die", "f\u00fch\u00b7ren", "dort", "un\u00b7ten", "die", "z\u00e4rt\u00b7lichs\u00b7te", "E\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Entweder sie schlafen oder sie zanken sich,", "tokens": ["Ent\u00b7we\u00b7der", "sie", "schla\u00b7fen", "o\u00b7der", "sie", "zan\u00b7ken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "KON", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da\u00df hochaufbraust hier oben das Meer,", "tokens": ["Da\u00df", "hoch\u00b7auf\u00b7braust", "hier", "o\u00b7ben", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und der Schiffer im Wellenger\u00e4usch es h\u00f6rt", "tokens": ["Und", "der", "Schif\u00b7fer", "im", "Wel\u00b7len\u00b7ge\u00b7r\u00e4usch", "es", "h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Wie der Alte sein Weib ausschilt:", "tokens": ["Wie", "der", "Al\u00b7te", "sein", "Weib", "aus\u00b7schilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "\u201erunde Metze des Weltalls!", "tokens": ["\u201e", "run\u00b7de", "Met\u00b7ze", "des", "Welt\u00b7alls", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Strahlenbuhlende!", "tokens": ["Strah\u00b7len\u00b7buh\u00b7len\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "Den ganzen Tag gl\u00fchst du f\u00fcr Andre,", "tokens": ["Den", "gan\u00b7zen", "Tag", "gl\u00fchst", "du", "f\u00fcr", "And\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Nachts, f\u00fcr Mich, bist du frostig und m\u00fcde!\u201c", "tokens": ["Und", "Nachts", ",", "f\u00fcr", "Mich", ",", "bist", "du", "fros\u00b7tig", "und", "m\u00fc\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPER", "$,", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach solcher Gardinenpredigt,", "tokens": ["Nach", "sol\u00b7cher", "Gar\u00b7di\u00b7nen\u00b7pre\u00b7digt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Versteht sich! bricht dann aus in Thr\u00e4nen", "tokens": ["Ver\u00b7steht", "sich", "!", "bricht", "dann", "aus", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$.", "VVFIN", "ADV", "APPR", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die stolze Sonne und klagt ihr Elend,", "tokens": ["Die", "stol\u00b7ze", "Son\u00b7ne", "und", "klagt", "ihr", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und klagt so jammerlang, da\u00df der Meergott", "tokens": ["Und", "klagt", "so", "jam\u00b7mer\u00b7lang", ",", "da\u00df", "der", "Meer\u00b7gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Pl\u00f6tzlich verzweiflungsvoll aus dem Bett springt,", "tokens": ["Pl\u00f6tz\u00b7lich", "ver\u00b7zwei\u00b7flungs\u00b7voll", "aus", "dem", "Bett", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und schnell nach der Meeresfl\u00e4che heraufschwimmt,", "tokens": ["Und", "schnell", "nach", "der", "Mee\u00b7res\u00b7fl\u00e4\u00b7che", "her\u00b7auf\u00b7schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Um Luft und Besinnung zu sch\u00f6pfen.", "tokens": ["Um", "Luft", "und", "Be\u00b7sin\u00b7nung", "zu", "sch\u00f6p\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "So sah ich ihn selbst, verflossene Nacht,", "tokens": ["So", "sah", "ich", "ihn", "selbst", ",", "ver\u00b7flos\u00b7se\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis an die Brust dem Meer' enttauchen.", "tokens": ["Bis", "an", "die", "Brust", "dem", "Meer'", "ent\u00b7tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er trug eine Jacke von gelbem Flanell,", "tokens": ["Er", "trug", "ei\u00b7ne", "Ja\u00b7cke", "von", "gel\u00b7bem", "Fla\u00b7nell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und eine lilienwei\u00dfe Schlafm\u00fctz,", "tokens": ["Und", "ei\u00b7ne", "li\u00b7li\u00b7en\u00b7wei\u00b7\u00dfe", "Schlaf\u00b7m\u00fctz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Und ein abgewelktes Gesicht.", "tokens": ["Und", "ein", "ab\u00b7ge\u00b7welk\u00b7tes", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}