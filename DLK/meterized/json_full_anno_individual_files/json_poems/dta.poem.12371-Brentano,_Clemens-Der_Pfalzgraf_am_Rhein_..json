{"dta.poem.12371": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Pfalzgraf am Rhein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wohnt' ein Pfalzgraf an dem Rhein,               ", "tokens": ["Es", "wohnt'", "ein", "Pfalz\u00b7graf", "an", "dem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der lie\u00df verjagen sein Schwesterlein,", "tokens": ["Der", "lie\u00df", "ver\u00b7ja\u00b7gen", "sein", "Schwes\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da kam der K\u00fcchenjung zu ihm:", "tokens": ["Da", "kam", "der", "K\u00fc\u00b7chen\u00b7jung", "zu", "ihm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ewillkommen! Willkommen, Pfalzgraf am Rhein!", "tokens": ["\u201e", "will\u00b7kom\u00b7men", "!", "Will\u00b7kom\u00b7men", ",", "Pfalz\u00b7graf", "am", "Rhein", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "NN", "$,", "NN", "APPRART", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201ewo ist dein sch\u00f6nes Schwesterlein?\u201c", "tokens": ["\u201e", "wo", "ist", "dein", "sch\u00f6\u00b7nes", "Schwes\u00b7ter\u00b7lein", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emein Schwesterlein die kriegst du nicht,", "tokens": ["\u201e", "mein", "Schwes\u00b7ter\u00b7lein", "die", "kriegst", "du", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ART", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201esie ist dir viel zu adelich,", "tokens": ["\u201e", "sie", "ist", "dir", "viel", "zu", "a\u00b7de\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eund du geh\u00f6rst zur K\u00fcch hinein.\u201c", "tokens": ["\u201e", "und", "du", "ge\u00b7h\u00f6rst", "zur", "K\u00fcch", "hin\u00b7ein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ewarum sollt ich sie kriegen nicht,", "tokens": ["\u201e", "wa\u00b7rum", "sollt", "ich", "sie", "krie\u00b7gen", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201esie hat von mir ein Kindelein.\u201c", "tokens": ["\u201e", "sie", "hat", "von", "mir", "ein", "Kin\u00b7de\u00b7lein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ehat sie von dir ein Kindelein,", "tokens": ["\u201e", "hat", "sie", "von", "dir", "ein", "Kin\u00b7de\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "PPER", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "\u201esoll sie nicht mehr mein Schwester seyn.\u201c", "tokens": ["\u201e", "soll", "sie", "nicht", "mehr", "mein", "Schwes\u00b7ter", "seyn", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Er lie\u00df sie gei\u00dfeln drei ganzer Tag,", "tokens": ["Er", "lie\u00df", "sie", "gei\u00b7\u00dfeln", "drei", "gan\u00b7zer", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bis man ihr Lung und Leber sah:", "tokens": ["Bis", "man", "ihr", "Lung", "und", "Le\u00b7ber", "sah", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eh\u00f6r auf, h\u00f6r auf, es ist genug,", "tokens": ["\u201e", "h\u00f6r", "auf", ",", "h\u00f6r", "auf", ",", "es", "ist", "ge\u00b7nug", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ees geh\u00f6rt dem K\u00f6nig aus Engelland.\u201c", "tokens": ["\u201e", "es", "ge\u00b7h\u00f6rt", "dem", "K\u00f6\u00b7nig", "aus", "En\u00b7gel\u00b7land", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$.", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201egeh\u00f6rt es dem K\u00f6nig von Engelland,", "tokens": ["\u201e", "ge\u00b7h\u00f6rt", "es", "dem", "K\u00f6\u00b7nig", "von", "En\u00b7gel\u00b7land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u201eso kostet mich's mein ganzes Land,", "tokens": ["\u201e", "so", "kos\u00b7tet", "mich's", "mein", "gan\u00b7zes", "Land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201emein ganzes Land ist nicht genug,", "tokens": ["\u201e", "mein", "gan\u00b7zes", "Land", "ist", "nicht", "ge\u00b7nug", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201emein Leben mu\u00df auch noch darzu.\u201c", "tokens": ["\u201e", "mein", "Le\u00b7ben", "mu\u00df", "auch", "noch", "dar\u00b7zu", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "ADV", "ADV", "PAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es stund nicht l\u00e4nger als drei Tag' an,", "tokens": ["Es", "stund", "nicht", "l\u00e4n\u00b7ger", "als", "drei", "Tag'", "an", ","], "to