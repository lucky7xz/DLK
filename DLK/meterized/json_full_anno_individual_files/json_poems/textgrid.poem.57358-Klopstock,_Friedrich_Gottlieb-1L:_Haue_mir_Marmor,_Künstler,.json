{"textgrid.poem.57358": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Haue mir Marmor, K\u00fcnstler,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haue mir Marmor, K\u00fcnstler,", "tokens": ["Hau\u00b7e", "mir", "Mar\u00b7mor", ",", "K\u00fcnst\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und grab' in den Marmor mit Goldschrift!", "tokens": ["Und", "grab'", "in", "den", "Mar\u00b7mor", "mit", "Gold\u00b7schrift", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "H\u00f6re genau, und versehle der Laute keinen;", "tokens": ["H\u00f6\u00b7re", "ge\u00b7nau", ",", "und", "ver\u00b7seh\u00b7le", "der", "Lau\u00b7te", "kei\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "PIAT", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Denn edel ist die That!", "tokens": ["Denn", "e\u00b7del", "ist", "die", "That", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sie geht nie durch die Vergessenheit unter:", "tokens": ["Und", "sie", "geht", "nie", "durch", "die", "Ver\u00b7ges\u00b7sen\u00b7heit", "un\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sieger sind meine Deutschen;", "tokens": ["Sie\u00b7ger", "sind", "mei\u00b7ne", "Deut\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch ist ihnen der Lorber Abscheu,", "tokens": ["Und", "doch", "ist", "ih\u00b7nen", "der", "Lor\u00b7ber", "Ab\u00b7scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Blut und Tod ist Greuel den siegenden Deutschen!", "tokens": ["Blut", "und", "Tod", "ist", "Greu\u00b7el", "den", "sie\u00b7gen\u00b7den", "Deut\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn so scholl vor der Franken Heer", "tokens": ["Denn", "so", "scholl", "vor", "der", "Fran\u00b7ken", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Drommete des Feldherrn:", "tokens": ["Die", "Drom\u00b7me\u00b7te", "des", "Feld\u00b7herrn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Freyheit schuft ihr euch;", "tokens": ["Frey\u00b7heit", "schuft", "ihr", "euch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Habt zum Ungeheuer die G\u00f6ttin umgeschaffen!", "tokens": ["Habt", "zum", "Un\u00b7ge\u00b7heu\u00b7er", "die", "G\u00f6t\u00b7tin", "um\u00b7ge\u00b7schaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Reiniget euch,", "tokens": ["Rei\u00b7ni\u00b7get", "euch", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und fleht der entweihten,", "tokens": ["Und", "fleht", "der", "ent\u00b7weih\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dass sie euch sie verzeih die Verwandlung,", "tokens": ["Dass", "sie", "euch", "sie", "ver\u00b7zeih", "die", "Ver\u00b7wand\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dass sie euch hold sey,", "tokens": ["Dass", "sie", "euch", "hold", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wieder werde zu dem, was sie war", "tokens": ["Wie\u00b7der", "wer\u00b7de", "zu", "dem", ",", "was", "sie", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "$,", "PRELS", "PPER", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Vor der grausen Verwandlung.", "tokens": ["Vor", "der", "grau\u00b7sen", "Ver\u00b7wand\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "(gram war diese, war", "tokens": ["(", "gram", "war", "die\u00b7se", ",", "war"], "token_info": ["punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "NE", "VAFIN", "PDS", "$,", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Den verstummenden Guten Entsetzen!)", "tokens": ["Den", "ver\u00b7stum\u00b7men\u00b7den", "Gu\u00b7ten", "Ent\u00b7set\u00b7zen", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Traget von der entweihten Altar", "tokens": ["Tra\u00b7get", "von", "der", "ent\u00b7weih\u00b7ten", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Den blutigen Staub weg,", "tokens": ["Den", "blu\u00b7ti\u00b7gen", "Staub", "weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weg das starre Gebein,", "tokens": ["Weg", "das", "star\u00b7re", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das an edle Todt' euch erinnert!", "tokens": ["Das", "an", "ed\u00b7le", "Todt'", "euch", "e\u00b7rin\u00b7nert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Reiniget euch!", "tokens": ["Rei\u00b7ni\u00b7get", "euch", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wir kommen zwar mit Wehr und mit Waffen:", "tokens": ["Wir", "kom\u00b7men", "zwar", "mit", "Wehr", "und", "mit", "Waf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aber wir kommen auch", "tokens": ["A\u00b7ber", "wir", "kom\u00b7men", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit dem Friedezweig in der Rechten;", "tokens": ["Mit", "dem", "Frie\u00b7de\u00b7zweig", "in", "der", "Rech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Kommen, mit euch vereint, den Staat zu bilden,", "tokens": ["Kom\u00b7men", ",", "mit", "euch", "ver\u00b7eint", ",", "den", "Staat", "zu", "bil\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie ihr ihn einst euch bildetet,", "tokens": ["Wie", "ihr", "ihn", "einst", "euch", "bil\u00b7de\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fest den Grund zu dem Baue zu legen!", "tokens": ["Fest", "den", "Grund", "zu", "dem", "Bau\u00b7e", "zu", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ohne tieferen Grund schwankt bald die gl\u00e4nzende Zinne.", "tokens": ["Oh\u00b7ne", "tie\u00b7fe\u00b7ren", "Grund", "schwankt", "bald", "die", "gl\u00e4n\u00b7zen\u00b7de", "Zin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}}, "stanza.9": {"line.1": {"text": "Nehmet als Freund uns auf; wir sind die \u00e4lteren Franken.", "tokens": ["Neh\u00b7met", "als", "Freund", "uns", "auf", ";", "wir", "sind", "die", "\u00e4l\u00b7te\u00b7ren", "Fran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Enkel, ein Wort ein Wort, ein Mann, ein Mann!", "tokens": ["En\u00b7kel", ",", "ein", "Wort", "ein", "Wort", ",", "ein", "Mann", ",", "ein", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Drommete k\u00fcndigte so den j\u00fcngeren Franken", "tokens": ["Die", "Drom\u00b7me\u00b7te", "k\u00fcn\u00b7dig\u00b7te", "so", "den", "j\u00fcn\u00b7ge\u00b7ren", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der \u00e4lteren Bund an.", "tokens": ["Der", "\u00e4l\u00b7te\u00b7ren", "Bund", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dumpfes Gemurmel wandelt' umher", "tokens": ["Dum\u00b7pfes", "Ge\u00b7mur\u00b7mel", "wan\u00b7delt'", "um\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In dem Lager der Feinde;", "tokens": ["In", "dem", "La\u00b7ger", "der", "Fein\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber von ihrer Drommet' erscholl nicht", "tokens": ["A\u00b7ber", "von", "ih\u00b7rer", "Drom\u00b7met'", "er\u00b7scholl", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJD", "PTKNEG"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der freudige Nachhall.", "tokens": ["Der", "freu\u00b7di\u00b7ge", "Nach\u00b7hall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Haue mir Marmor, K\u00fcnstler,", "tokens": ["Hau\u00b7e", "mir", "Mar\u00b7mor", ",", "K\u00fcnst\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und grab' in den Marmor mit Goldschrift!", "tokens": ["Und", "grab'", "in", "den", "Mar\u00b7mor", "mit", "Gold\u00b7schrift", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR