{"textgrid.poem.49267": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das starcke Liebes-Gifft, das unsre hohe Sinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1627, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das starcke Liebes-Gifft, das unsre hohe Sinnen,", "tokens": ["Das", "star\u00b7cke", "Lie\u00b7bes\u00b7Gifft", ",", "das", "uns\u00b7re", "ho\u00b7he", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die von dem Himmel sind, mit seiner Krafft gewinnen", "tokens": ["Die", "von", "dem", "Him\u00b7mel", "sind", ",", "mit", "sei\u00b7ner", "Krafft", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wann Vernunfft erliegt, zu Boden reissen kan,", "tokens": ["Und", "wann", "Ver\u00b7nunfft", "er\u00b7liegt", ",", "zu", "Bo\u00b7den", "reis\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVPP", "$,", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieh', o du Edles Par, auff diesem Schau-Platz an.", "tokens": ["Sieh'", ",", "o", "du", "Ed\u00b7les", "Par", ",", "auff", "die\u00b7sem", "Schau\u00b7Platz", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NE", "NE", "NE", "$,", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sieh an, du freyer Heldt, du Bildnu\u00df aller Tugendt,", "tokens": ["Sieh", "an", ",", "du", "frey\u00b7er", "Heldt", ",", "du", "Bild\u00b7nu\u00df", "al\u00b7ler", "Tu\u00b7gendt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du Prey\u00df der Zeit, und du, Sophie, Liecht der Jugendt,", "tokens": ["Du", "Prey\u00df", "der", "Zeit", ",", "und", "du", ",", "So\u00b7phie", ",", "Liecht", "der", "Ju\u00b7gendt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "KON", "PPER", "$,", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "De\u00df Vatters grosse Lust, der werthen Mutter Zier,", "tokens": ["De\u00df", "Vat\u00b7ters", "gros\u00b7se", "Lust", ",", "der", "wert\u00b7hen", "Mut\u00b7ter", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sieh' an der Liebe Macht, von der du f\u00fcr und f\u00fcr", "tokens": ["Sieh'", "an", "der", "Lie\u00b7be", "Macht", ",", "von", "der", "du", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$,", "APPR", "PRELS", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Befreyt und sicher bist. Wer so wie du sich liebet", "tokens": ["Be\u00b7freyt", "und", "si\u00b7cher", "bist", ".", "Wer", "so", "wie", "du", "sich", "lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$.", "PWS", "ADV", "KOKOM", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit ungef\u00e4rbter Pflicht, wer seine Huld ergiebet", "tokens": ["Mit", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Pflicht", ",", "wer", "sei\u00b7ne", "Huld", "er\u00b7gie\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In Urtheil unnd Verstandt, ist kl\u00fcger als der Gott,", "tokens": ["In", "Ur\u00b7theil", "unnd", "Ver\u00b7standt", ",", "ist", "kl\u00fc\u00b7ger", "als", "der", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der t\u00e4glich zu uns bringt das sch\u00f6ne Morgenroth.", "tokens": ["Der", "t\u00e4g\u00b7lich", "zu", "uns", "bringt", "das", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihm machet Dafne selbst von ihren frischen Zweygen", "tokens": ["Ihm", "ma\u00b7chet", "Daf\u00b7ne", "selbst", "von", "ih\u00b7ren", "fri\u00b7schen", "Zwey\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Krantz, der nicht verwelckt, sein Nachklang wird nicht schweigen,", "tokens": ["Den", "Krantz", ",", "der", "nicht", "ver\u00b7welckt", ",", "sein", "Nach\u00b7klang", "wird", "nicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVPP", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So lange Liebe wehrt. Nim dann in Gnaden an,", "tokens": ["So", "lan\u00b7ge", "Lie\u00b7be", "wehrt", ".", "Nim", "dann", "in", "Gna\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "$.", "NE", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du duppeltes Gestirn, was Dafne geben kan:", "tokens": ["Du", "dup\u00b7pel\u00b7tes", "Ge\u00b7stirn", ",", "was", "Daf\u00b7ne", "ge\u00b7ben", "kan", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.17": {"text": "Den immer-gr\u00fcnen Krantz, und dencke, da\u00df die Gaben", "tokens": ["Den", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "Krantz", ",", "und", "den\u00b7cke", ",", "da\u00df", "die", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So F\u00fcrsten als wie ihr vollauff zugeben haben,", "tokens": ["So", "F\u00fcrs\u00b7ten", "als", "wie", "ihr", "vol\u00b7lauff", "zu\u00b7ge\u00b7ben", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "KOKOM", "PPOSAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zwar gro\u00df, doch irrdisch sind. Die Flucht der Zeit vertreibt", "tokens": ["Zwar", "gro\u00df", ",", "doch", "irr\u00b7disch", "sind", ".", "Die", "Flucht", "der", "Zeit", "ver\u00b7treibt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$.", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das Unsrig' und uns auch; was Dafne gibt das bleibt.", "tokens": ["Das", "Uns\u00b7rig'", "und", "uns", "auch", ";", "was", "Daf\u00b7ne", "gibt", "das", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "ADV", "$.", "PWS", "NE", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Ovidius, Vorreder.Der erste Hirt.", "tokens": ["O\u00b7vi\u00b7dius", ",", "Vor\u00b7re\u00b7der", ".", "Der", "ers\u00b7te", "Hirt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dafne.Der ander Hirt.", "tokens": ["Daf\u00b7ne", ".", "Der", "an\u00b7der", "Hirt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, 