{"textgrid.poem.46062": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "7. Juni 1834. Aus einem Briefe nach Karlsbad", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vas hei\u00df aus meiner Seele fleht,", "tokens": ["Vas", "hei\u00df", "aus", "mei\u00b7ner", "See\u00b7le", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bang in diesen Zeilen steht", "tokens": ["Und", "bang", "in", "die\u00b7sen", "Zei\u00b7len", "steht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das soll dich nicht betr\u00fcben", "tokens": ["Das", "soll", "dich", "nicht", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Liebe hat es ausges\u00e4et", "tokens": ["Die", "Lie\u00b7be", "hat", "es", "aus\u00b7ge\u00b7s\u00e4et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe hat hindurchgeweht,", "tokens": ["Die", "Lie\u00b7be", "hat", "hin\u00b7durch\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Liebe hat's getrieben.", "tokens": ["Die", "Lie\u00b7be", "hat's", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ist dies Feld einst abgem\u00e4ht,", "tokens": ["Und", "ist", "dies", "Feld", "einst", "ab\u00b7ge\u00b7m\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Arm Lindi durch die Stoppeln geht,", "tokens": ["Arm", "Lin\u00b7di", "durch", "die", "Stop\u00b7peln", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sucht \u00c4hren, die geblieben,", "tokens": ["Sucht", "\u00c4h\u00b7ren", ",", "die", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sucht Lieb', die mit ihr untergeht,", "tokens": ["Sucht", "Lieb'", ",", "die", "mit", "ihr", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sucht Lieb', die mit ihr aufersteht,", "tokens": ["Sucht", "Lieb'", ",", "die", "mit", "ihr", "auf\u00b7er\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sucht Lieb', die ich mu\u00dft' lieben!", "tokens": ["Sucht", "Lieb'", ",", "die", "ich", "mu\u00dft'", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PRELS", "PPER", "VMF