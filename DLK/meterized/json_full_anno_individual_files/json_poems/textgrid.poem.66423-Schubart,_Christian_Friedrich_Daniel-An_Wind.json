{"textgrid.poem.66423": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Wind", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wind, der viele Kunden hat,", "tokens": ["Wind", ",", "der", "vie\u00b7le", "Kun\u00b7den", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreiet sich vor Eifer matt:", "tokens": ["Schrei\u00b7et", "sich", "vor", "Ei\u00b7fer", "matt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbschafft Quacksalber aus der Stadt!", "tokens": ["\u00bb", "schafft", "Quack\u00b7sal\u00b7ber", "aus", "der", "Stadt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die die Chirurgie,", "tokens": ["Die", "die", "Chi\u00b7rur\u00b7gie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Physiologie,", "tokens": ["Phy\u00b7sio\u00b7lo\u00b7gie", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und Anatomie,", "tokens": ["Und", "A\u00b7nat\u00b7o\u00b7mie", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nicht, wie ich, verstehen!\u00ab", "tokens": ["Nicht", ",", "wie", "ich", ",", "ver\u00b7ste\u00b7hen", "!", "\u00ab"], "token_