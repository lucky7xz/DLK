-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Erinnrung an dein stilles Tun,", "tokens": ["E\u00b7rinn\u00b7rung", "an", "dein", "stil\u00b7les", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An all, was ich gesehn,", "tokens": ["An", "all", ",", "was", "ich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll \u00fcber meinem Haupte ruhn,", "tokens": ["Soll", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll k\u00fchlend mich umwehn,", "tokens": ["Soll", "k\u00fch\u00b7lend", "mich", "um\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Und wird zu hei\u00df des Tages Pein,", "tokens": ["Und", "wird", "zu", "hei\u00df", "des", "Ta\u00b7ges", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKA", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Lebenssonne Stich,", "tokens": ["Der", "Le\u00b7bens\u00b7son\u00b7ne", "Stich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denk ich atmend an Gastein,", "tokens": ["So", "denk", "ich", "at\u00b7mend", "an", "Gas\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du Freundliche! und dich!", "tokens": ["Du", "Freund\u00b7li\u00b7che", "!", "und", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}