-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer ist es? da nicht ", "tokens": ["Wer", "ist", "es", "?", "da", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$.", "ADV", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.24": {"line.1": {"text": "Du kehrst zum Gatten nun zur\u00fcck,", "tokens": ["Du", "kehrst", "zum", "Gat\u00b7ten", "nun", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum eignen Hauseshalt,", "tokens": ["Zum", "eig\u00b7nen", "Hau\u00b7ses\u00b7halt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da findest du gen\u00fcgend Gl\u00fcck,", "tokens": ["Da", "fin\u00b7dest", "du", "ge\u00b7n\u00fc\u00b7gend", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00dft wohl meiner bald:", "tokens": ["Ver\u00b7gi\u00dft", "wohl", "mei\u00b7ner", "bald", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Ich aber, Frau, ich hab kein Haus,", "tokens": ["Ich", "a\u00b7ber", ",", "Frau", ",", "ich", "hab", "kein", "Haus", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Band, das Liebe flicht;", "tokens": ["Kein", "Band", ",", "das", "Lie\u00b7be", "flicht", ";"], "token_