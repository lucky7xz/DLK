{"textgrid.poem.44377": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wird mir denn so weh und bang,", "tokens": ["Wie", "wird", "mir", "denn", "so", "weh", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt, da du scheiden mu\u00dft?", "tokens": ["Jetzt", ",", "da", "du", "schei\u00b7den", "mu\u00dft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab dich gesehen Tage lang,", "tokens": ["Hab", "dich", "ge\u00b7se\u00b7hen", "Ta\u00b7ge", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und still war meine Brust,", "tokens": ["Und", "still", "war", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hab dich gesehen Wochen lang,", "tokens": ["Hab", "dich", "ge\u00b7se\u00b7hen", "Wo\u00b7chen", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "NN", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und ruhig war mein Herz;", "tokens": ["Und", "ru\u00b7hig", "war", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt, da des Scheidens Zeichen klang,", "tokens": ["Jetzt", ",", "da", "des", "Schei\u00b7dens", "Zei\u00b7chen", "klang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Woher jetzt dieser Schmerz?", "tokens": ["Wo\u00b7her", "jetzt", "die\u00b7ser", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Frau, zu der mein Abschied ruft,", "tokens": ["O", "Frau", ",", "zu", "der", "mein", "Ab\u00b7schied", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll stillem, frommen Sinn,", "tokens": ["Voll", "stil\u00b7lem", ",", "from\u00b7men", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So heiter, wie die heitre Luft,", "tokens": ["So", "hei\u00b7ter", ",", "wie", "die", "heit\u00b7re", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Gleichst auch der Luft darin?", "tokens": ["Gleichst", "auch", "der", "Luft", "da\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PAV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Da\u00df ihren Segen man kaum sp\u00fcrt,", "tokens": ["Da\u00df", "ih\u00b7ren", "Se\u00b7gen", "man", "kaum", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Tag auf Tag entflieht,", "tokens": ["Wenn", "Tag", "auf", "Tag", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch schaudernd dessen inne wird,", "tokens": ["Doch", "schau\u00b7dernd", "des\u00b7sen", "in\u00b7ne", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PDS", "PTKVZ", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald sie sich entzieht.", "tokens": ["So\u00b7bald", "sie", "sich", "ent\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Frau! du warest Mutter mir,", "tokens": ["O", "Frau", "!", "du", "wa\u00b7rest", "Mut\u00b7ter", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 Die meine schlummert tief \u2013", "tokens": ["\u2013", "Die", "mei\u00b7ne", "schlum\u00b7mert", "tief", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPOSAT", "VVFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein mahnend Wort kam wie von ihr,", "tokens": ["Dein", "mah\u00b7nend", "Wort", "kam", "wie", "von", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "KOKOM", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Ruf war, wie sie rief.", "tokens": ["Dein", "Ruf", "war", ",", "wie", "sie", "rief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Frau! du warst die Schwester mein;", "tokens": ["O", "Frau", "!", "du", "warst", "die", "Schwes\u00b7ter", "mein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar Schwestern hatt ich nie,", "tokens": ["Zwar", "Schwes\u00b7tern", "hatt", "ich", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch malte mirs so lieb und fein", "tokens": ["Doch", "mal\u00b7te", "mirs", "so", "lieb", "und", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00fchl und Phantasie,", "tokens": ["Ge\u00b7f\u00fchl", "und", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In andern seiner sich zu freun", "tokens": ["In", "an\u00b7dern", "sei\u00b7ner", "sich", "zu", "freun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PPOSAT", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und anderer in sich,", "tokens": ["Und", "an\u00b7de\u00b7rer", "in", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu zweien und doch eins zu sein,", "tokens": ["Zu", "zwei\u00b7en", "und", "doch", "eins", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "ADV", "PIS", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbunden inniglich.", "tokens": ["Ver\u00b7bun\u00b7den", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Frau! du hast mich wohl gelehrt,", "tokens": ["O", "Frau", "!", "du", "hast", "mich", "wohl", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was eine Gattin sei,", "tokens": ["Was", "ei\u00b7ne", "Gat\u00b7tin", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie viel ein holdes Wesen wert,", "tokens": ["Wie", "viel", "ein", "hol\u00b7des", "We\u00b7sen", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das lieb und gut und treu;", "tokens": ["Das", "lieb", "und", "gut", "und", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du zeigtest mir das sch\u00f6ne Bild,", "tokens": ["Du", "zeig\u00b7test", "mir", "das", "sch\u00f6\u00b7ne", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gegenbild dazu,", "tokens": ["Das", "Ge\u00b7gen\u00b7bild", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo find ich es, so lieb und mild?", "tokens": ["Wo", "find", "ich", "es", ",", "so", "lieb", "und", "mild", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer ist es? da nicht ", "tokens": ["Wer", "ist", "es", "?", "da", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$.", "ADV", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Du kehrst zum Gatten nun zur\u00fcck,", "tokens": ["Du", "kehrst", "zum", "Gat\u00b7ten", "nun", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum eignen Hauseshalt,", "tokens": ["Zum", "eig\u00b7nen", "Hau\u00b7ses\u00b7halt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da findest du gen\u00fcgend Gl\u00fcck,", "tokens": ["Da", "fin\u00b7dest", "du", "ge\u00b7n\u00fc\u00b7gend", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00dft wohl meiner bald:", "tokens": ["Ver\u00b7gi\u00dft", "wohl", "mei\u00b7ner", "bald", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich aber, Frau, ich hab kein Haus,", "tokens": ["Ich", "a\u00b7ber", ",", "Frau", ",", "ich", "hab", "kein", "Haus", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Band, das Liebe flicht;", "tokens": ["Kein", "Band", ",", "das", "Lie\u00b7be", "flicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter trugen sie hinaus,", "tokens": ["Die", "Mut\u00b7ter", "tru\u00b7gen", "sie", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schwestern kannt ich nicht;", "tokens": ["Und", "Schwes\u00b7tern", "kannt", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mir bleibt wohl keine andre Wahl,", "tokens": ["Mir", "bleibt", "wohl", "kei\u00b7ne", "and\u00b7re", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df denken sp\u00e4t und fr\u00fch. \u2013", "tokens": ["Mu\u00df", "den\u00b7ken", "sp\u00e4t", "und", "fr\u00fch", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "VVINF", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott segne dich zu tausendmal!", "tokens": ["Gott", "seg\u00b7ne", "dich", "zu", "tau\u00b7send\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frau! dein verge\u00df ich nie!", "tokens": ["Frau", "!", "dein", "ver\u00b7ge\u00df", "ich", "nie", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Erinnrung an dein stilles Tun,", "tokens": ["E\u00b7rinn\u00b7rung", "an", "dein", "stil\u00b7les", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An all, was ich gesehn,", "tokens": ["An", "all", ",", "was", "ich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll \u00fcber meinem Haupte ruhn,", "tokens": ["Soll", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll k\u00fchlend mich umwehn,", "tokens": ["Soll", "k\u00fch\u00b7lend", "mich", "um\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und wird zu hei\u00df des Tages Pein,", "tokens": ["Und", "wird", "zu", "hei\u00df", "des", "Ta\u00b7ges", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKA", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Lebenssonne Stich,", "tokens": ["Der", "Le\u00b7bens\u00b7son\u00b7ne", "Stich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denk ich atmend an Gastein,", "tokens": ["So", "denk", "ich", "at\u00b7mend", "an", "Gas\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du Freundliche! und dich!", "tokens": ["Du", "Freund\u00b7li\u00b7che", "!", "und", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wie wird mir denn so weh und bang,", "tokens": ["Wie", "wird", "mir", "denn", "so", "weh", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt, da du scheiden mu\u00dft?", "tokens": ["Jetzt", ",", "da", "du", "schei\u00b7den", "mu\u00dft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "ia