{"dta.poem.19224": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht erst vom Werkzeug wird Naturtrieb angehaucht,", "tokens": ["Nicht", "erst", "vom", "Werk\u00b7zeug", "wird", "Na\u00b7tur\u00b7trieb", "an\u00b7ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Naturtrieb bringt hervor das Werkzeug das er braucht.", "tokens": ["Na\u00b7tur\u00b7trieb", "bringt", "her\u00b7vor", "das", "Werk\u00b7zeug", "das", "er", "braucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das V\u00f6gelchen im Nest will schon Gefieder regen,", "tokens": ["Das", "V\u00f6\u00b7gel\u00b7chen", "im", "Nest", "will", "schon", "Ge\u00b7fie\u00b7der", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VMFIN", "ADV", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nicht gewachsen ist, und mu\u00df sich wieder legen.", "tokens": ["Das", "nicht", "ge\u00b7wach\u00b7sen", "ist", ",", "und", "mu\u00df", "sich", "wie\u00b7der", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVPP", "VAFIN", "$,", "KON", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit ungewachsnem Horn siehst du das B\u00f6ckchen sto\u00dfen,", "tokens": ["Mit", "un\u00b7ge\u00b7wachs\u00b7nem", "Horn", "siehst", "du", "das", "B\u00f6ck\u00b7chen", "sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit noch glatter Stirn vergebens sich erbo\u00dfen.", "tokens": ["Und", "mit", "noch", "glat\u00b7ter", "Stirn", "ver\u00b7ge\u00b7bens", "sich", "er\u00b7bo\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das B\u00f6ckchen f\u00fchlt sein Horn, das V\u00f6gelchen die Schwingen", "tokens": ["Das", "B\u00f6ck\u00b7chen", "f\u00fchlt", "sein", "Horn", ",", "das", "V\u00f6\u00b7gel\u00b7chen", "die", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Voraus, und ihr Trieb sucht sie hervor zu bringen.", "tokens": ["Zum", "Vo\u00b7raus", ",", "und", "ihr", "Trieb", "sucht", "sie", "her\u00b7vor", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So siehst du auch das Kind mit weicher Zunge lallen,", "tokens": ["So", "siehst", "du", "auch", "das", "Kind", "mit", "wei\u00b7cher", "Zun\u00b7ge", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh noch das Werkzeug l\u00e4\u00dft vollkommne T\u00f6n' erschallen,", "tokens": ["Eh", "noch", "das", "Werk\u00b7zeug", "l\u00e4\u00dft", "voll\u00b7komm\u00b7ne", "T\u00f6n'", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und siehst es zum Versuch mit seinen H\u00e4ndchen langen,", "tokens": ["Und", "siehst", "es", "zum", "Ver\u00b7such", "mit", "sei\u00b7nen", "H\u00e4nd\u00b7chen", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch eh sie ganz geschickt es wissen anzufangen.", "tokens": ["Noch", "eh", "sie", "ganz", "ge\u00b7schickt", "es", "wis\u00b7sen", "an\u00b7zu\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVPP", "PPER", "VVINF", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Geist gebraucht nicht, weil sie brauchbar ist, die Hand,", "tokens": ["Der", "Geist", "ge\u00b7braucht", "nicht", ",", "weil", "sie", "brauch\u00b7bar", "ist", ",", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die erst die Brauchbarkeit, weil er sie brauchte, fand.", "tokens": ["Die", "erst", "die", "Brauch\u00b7bar\u00b7keit", ",", "weil", "er", "sie", "brauch\u00b7te", ",", "fand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Er richtet nicht im schon gebauten Haus sich ein,", "tokens": ["Er", "rich\u00b7tet", "nicht", "im", "schon", "ge\u00b7bau\u00b7ten", "Haus", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "ADV", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von innen baut er es, und zieht nicht erst hinein;", "tokens": ["Von", "in\u00b7nen", "baut", "er", "es", ",", "und", "zieht", "nicht", "erst", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie nicht die Schnecke kriecht ins leere Schneckenhaus;", "tokens": ["Wie", "nicht", "die", "Schne\u00b7cke", "kriecht", "ins", "lee\u00b7re", "Schne\u00b7cken\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie w\u00f6lbt es um sich her und streckt den Kopf heraus.", "tokens": ["Sie", "w\u00f6lbt", "es", "um", "sich", "her", "und", "streckt", "den", "Kopf", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}