{"textgrid.poem.62426": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Soldaten-Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So z\u00fcndet nun die Feuer", "tokens": ["So", "z\u00fcn\u00b7det", "nun", "die", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Gottes Namen an,", "tokens": ["In", "Got\u00b7tes", "Na\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hat wol keiner treuer", "tokens": ["Es", "hat", "wol", "kei\u00b7ner", "treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Tagewerk gethan;", "tokens": ["Sein", "Ta\u00b7ge\u00b7werk", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und fern von Liebesarmen", "tokens": ["Und", "fern", "von", "Lie\u00b7be\u00b7sar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und fern von Weibesbrust", "tokens": ["Und", "fern", "von", "Wei\u00b7bes\u00b7brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df uns an dir erwarmen,", "tokens": ["La\u00df", "uns", "an", "dir", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du Feuer, unsre Lust.", "tokens": ["Du", "Feu\u00b7er", ",", "uns\u00b7re", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So ruht, ihr m\u00fcden Glieder,", "tokens": ["So", "ruht", ",", "ihr", "m\u00fc\u00b7den", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vielleicht zum letzten Mal;", "tokens": ["Viel\u00b7leicht", "zum", "letz\u00b7ten", "Mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bald, so sinkt ihr nieder,", "tokens": ["Wie", "bald", ",", "so", "sinkt", "ihr", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verletzt von Blei und Stahl.", "tokens": ["Ver\u00b7letzt", "von", "Blei", "und", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir haben uns ergeben,", "tokens": ["Wir", "ha\u00b7ben", "uns", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herr Gott, in deine Hand;", "tokens": ["Herr", "Gott", ",", "in", "dei\u00b7ne", "Hand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nimm hin den Leib, das Leben", "tokens": ["Nimm", "hin", "den", "Leib", ",", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr unser Vaterland.", "tokens": ["F\u00fcr", "un\u00b7ser", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr fernen theuren Seelen,", "tokens": ["Ihr", "fer\u00b7nen", "theu\u00b7ren", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir w\u00fcnschen gute Nacht;", "tokens": ["Wir", "w\u00fcn\u00b7schen", "gu\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen euch empfehlen", "tokens": ["Wir", "wol\u00b7len", "euch", "emp\u00b7feh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ew'gen Liebesmacht.", "tokens": ["Der", "ew'\u00b7gen", "Lie\u00b7bes\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir gr\u00fc\u00dfen, ach wir gr\u00fc\u00dfen", "tokens": ["Wir", "gr\u00fc\u00b7\u00dfen", ",", "ach", "wir", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "XY", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Viel tausend tausendmal,", "tokens": ["Viel", "tau\u00b7send", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und unsre Blicke k\u00fcssen", "tokens": ["Und", "uns\u00b7re", "Bli\u00b7cke", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich wol in Mondenstrahl.", "tokens": ["Sich", "wol", "in", "Mon\u00b7dens\u00b7trahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schlaf ruhig, Vater R\u00f6der,", "tokens": ["Schlaf", "ru\u00b7hig", ",", "Va\u00b7ter", "R\u00f6\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du lieber General;", "tokens": ["Du", "lie\u00b7ber", "Ge\u00b7ne\u00b7ral", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das betet wol ein Jeder", "tokens": ["Das", "be\u00b7tet", "wol", "ein", "Je\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus deiner Krieger Zahl.", "tokens": ["Aus", "dei\u00b7ner", "Krie\u00b7ger", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du bist uns Lust und Segen", "tokens": ["Du", "bist", "uns", "Lust", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Schlacht und Ungemach;", "tokens": ["In", "Schlacht", "und", "Un\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du schl\u00e4fst in Sturm und Regen,", "tokens": ["Du", "schl\u00e4fst", "in", "Sturm", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie wir, oft ohne Dach.", "tokens": ["Wie", "wir", ",", "oft", "oh\u00b7ne", "Dach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auch du im Lager dr\u00fcben", "tokens": ["Auch", "du", "im", "La\u00b7ger", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Magst ruhig schlafen, Feind,", "tokens": ["Magst", "ru\u00b7hig", "schla\u00b7fen", ",", "Feind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir ha'n mit Schu\u00df und Hieben", "tokens": ["Wir", "ha'n", "mit", "Schu\u00df", "und", "Hie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ehrlich stets gemeint.", "tokens": ["Es", "ehr\u00b7lich", "stets", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Einem aber ringen", "tokens": ["Mit", "Ei\u00b7nem", "a\u00b7ber", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir Morgens wie zu Nacht,", "tokens": ["Wir", "Mor\u00b7gens", "wie", "zu", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er m\u00f6cht' uns gern verschlingen,", "tokens": ["Er", "m\u00f6cht'", "uns", "gern", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der L\u00f6we br\u00fcllt und wacht.", "tokens": ["Der", "L\u00f6\u00b7we", "br\u00fcllt", "und", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du Feldwacht, und ihr Runden,", "tokens": ["Du", "Feld\u00b7wacht", ",", "und", "ihr", "Run\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seid wacker und bereit,", "tokens": ["Seid", "wa\u00b7cker", "und", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um flei\u00dfig zu erkunden,", "tokens": ["Um", "flei\u00b7\u00dfig", "zu", "er\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von wo Gefahr uns dr\u00e4ut;", "tokens": ["Von", "wo", "Ge\u00b7fahr", "uns", "dr\u00e4ut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Herr hat viele Schaaren", "tokens": ["Der", "Herr", "hat", "vie\u00b7le", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu unserm Schutz bestellt,", "tokens": ["Zu", "un\u00b7serm", "Schutz", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die heil'gen Engel wahren", "tokens": ["Die", "heil'\u00b7gen", "En\u00b7gel", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des frommen Kriegers Zelt.", "tokens": ["Des", "from\u00b7men", "Krie\u00b7gers", "Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr W\u00e4chter in der H\u00f6he,", "tokens": ["Ihr", "W\u00e4ch\u00b7ter", "in", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O schwebt um diesen Raum,", "tokens": ["O", "schwebt", "um", "die\u00b7sen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder Schl\u00e4fer sehe", "tokens": ["Und", "je\u00b7der", "Schl\u00e4\u00b7fer", "se\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Liebste heut im Traum.", "tokens": ["Das", "Liebs\u00b7te", "heut", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun gute Nacht, ihr Br\u00fcder,", "tokens": ["Nun", "gu\u00b7te", "Nacht", ",", "ihr", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gut' Nacht, mein Schlafkam'rad,", "tokens": ["Gut'", "Nacht", ",", "mein", "Schlaf\u00b7kam'\u00b7rad", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir sehn uns morgen wieder", "tokens": ["Wir", "sehn", "uns", "mor\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bei frischer Heldenthat.", "tokens": ["Bei", "fri\u00b7scher", "Hel\u00b7den\u00b7that", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So z\u00fcndet nun die Feuer", "tokens": ["So", "z\u00fcn\u00b7det", "nun", "die", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Gottes Namen an,", "tokens": ["In", "Got\u00b7te