{"textgrid.poem.36506": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5. Der Hahn und die Hausmagd", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hahn stand auf dem Mist, und scharrte tief, und fand,", "tokens": ["Ein", "Hahn", "stand", "auf", "dem", "Mist", ",", "und", "scharr\u00b7te", "tief", ",", "und", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt eines Gerstenkorns, den sch\u00f6nsten Diamant!", "tokens": ["Statt", "ei\u00b7nes", "Gers\u00b7ten\u00b7korns", ",", "den", "sch\u00f6ns\u00b7ten", "Di\u00b7a\u00b7mant", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ei! w\u00e4rst du, sprach der Hahn, ein Gerstenkorn gewesen!", "tokens": ["Ei", "!", "w\u00e4rst", "du", ",", "sprach", "der", "Hahn", ",", "ein", "Gers\u00b7ten\u00b7korn", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wetzte seinen Rittersporn,", "tokens": ["Und", "wetz\u00b7te", "sei\u00b7nen", "Rit\u00b7ter\u00b7sporn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und warf den sch\u00f6nen Stein weit von sich weg, im Zorn!", "tokens": ["Und", "warf", "den", "sch\u00f6\u00b7nen", "Stein", "weit", "von", "sich", "weg", ",", "im", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "PRF", "PTKVZ", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Hausmagd, fegend mit dem Besen,", "tokens": ["Die", "Haus\u00b7magd", ",", "fe\u00b7gend", "mit", "dem", "Be\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah um sich, sah den Wurf, hob auf den sch\u00f6nen Stein,", "tokens": ["Sah", "um", "sich", ",", "sah", "den", "Wurf", ",", "hob", "auf", "den", "sch\u00f6\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und: sch\u00f6nen Dank, Herr Hahn! sprach sie, sie sollen leben!", "tokens": ["Und", ":", "sch\u00f6\u00b7nen", "Dank", ",", "Herr", "Hahn", "!", "sprach", "sie", ",", "sie", "sol\u00b7len", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$.", "ADJA", "NN", "$,", "NN", "NE", "$.", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Daf\u00fcr, da\u00df sie den Stein mir zum Geschenk gegeben,", "tokens": ["Da\u00b7f\u00fcr", ",", "da\u00df", "sie", "den", "Stein", "mir", "zum", "Ge\u00b7schenk", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will ich, Herr Hahn! einmal zu ihren Diensten sein!", "tokens": ["Will", "ich", ",", "Herr", "Hahn", "!", "ein\u00b7mal", "zu", "ih\u00b7ren", "Diens\u00b7ten", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "NE", "$.", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die K\u00f6chin, die das Messer schleifen", "tokens": ["Die", "K\u00f6\u00b7chin", ",", "die", "das", "Mes\u00b7ser", "schlei\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und, eine M\u00f6rderin, an ihnen sich vergreifen", "tokens": ["Und", ",", "ei\u00b7ne", "M\u00f6r\u00b7de\u00b7rin", ",", "an", "ih\u00b7nen", "sich", "ver\u00b7grei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ART", "NN", "$,", "APPR", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit ihrem Messer will, die hats mit mir zu thun!", "tokens": ["Mit", "ih\u00b7rem", "Mes\u00b7ser", "will", ",", "die", "hats", "mit", "mir", "zu", "thun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "$,", "PRELS", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie, mein Herr Hahn, sie selbst, und auch ihr liebstes Huhn,", "tokens": ["Sie", ",", "mein", "Herr", "Hahn", ",", "sie", "selbst", ",", "und", "auch", "ihr", "liebs\u00b7tes", "Huhn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NE", "$,", "PPER", "ADV", "$,", "KON", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie beide sollen nicht grausamen Todes sterben!", "tokens": ["Sie", "bei\u00b7de", "sol\u00b7len", "nicht", "grau\u00b7sa\u00b7men", "To\u00b7des", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und sterb' einst ich, dann sollen sie", "tokens": ["Und", "sterb'", "einst", "ich", ",", "dann", "sol\u00b7len", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$,", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "(sie sind ein gutes Vieh!)", "tokens": ["(", "sie", "sind", "ein", "gu\u00b7tes", "Vieh", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Von mir ein sch\u00f6n Gef\u00e4\u00df, gef\u00fcllt mit Gersten, erben!", "tokens": ["Von", "mir", "ein", "sch\u00f6n", "Ge\u00b7f\u00e4\u00df", ",", "ge\u00b7f\u00fcllt", "mit", "Gers\u00b7ten", ",", "er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJD", "NN", "$,", "VVPP", "APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Hahn h\u00f6rt's alles, steht auf seinem Misthof, denkt:", "tokens": ["Der", "Hahn", "h\u00f6rt's", "al\u00b7les", ",", "steht", "auf", "sei\u00b7nem", "Mist\u00b7hof", ",", "denkt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ding hat sich gelenkt!", "tokens": ["Das", "Ding", "hat", "sich", "ge\u00b7lenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Stein enthielt den Grund von meinem l\u00e4ngern Leben,", "tokens": ["Der", "Stein", "ent\u00b7hielt", "den", "Grund", "von", "mei\u00b7nem", "l\u00e4n\u00b7gern", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ich, ich warf ihn weg! ich Dummkopf! h\u00e4tt ich ihn", "tokens": ["Und", "ich", ",", "ich", "warf", "ihn", "weg", "!", "ich", "Dumm\u00b7kopf", "!", "h\u00e4tt", "ich", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "NN", "$.", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Landesmutter zu Berlin,", "tokens": ["Der", "Lan\u00b7des\u00b7mut\u00b7ter", "zu", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "An ihre Krone, hingegeben:", "tokens": ["An", "ih\u00b7re", "Kro\u00b7ne", ",", "hin\u00b7ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer wei\u00df, was dann aus mir geworden w\u00e4r! Allein,", "tokens": ["Wer", "wei\u00df", ",", "was", "dann", "aus", "mir", "ge\u00b7wor\u00b7den", "w\u00e4r", "!", "Al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PPER", "VAPP", "VAFIN", "$.", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So denkt der weisre Hahn! So handelt und so spricht,", "tokens": ["So", "denkt", "der", "weis\u00b7re", "Hahn", "!", "So", "han\u00b7delt", "und", "so", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In H\u00e4usern und in H\u00fctten,", "tokens": ["In", "H\u00e4u\u00b7sern", "und", "in", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der kl\u00fcgre Mensch, und sieht sogleich das Gro\u00dfe nicht,", "tokens": ["Der", "kl\u00fcg\u00b7re", "Mensch", ",", "und", "sieht", "sog\u00b7leich", "das", "Gro\u00b7\u00dfe", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das aus dem Kleinen quillt! ", "tokens": ["Das", "aus", "dem", "Klei\u00b7nen", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und der, der alles lenkt,", "tokens": ["Und", "der", ",", "der", "al\u00b7les", "lenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gold, Kurhut, Majest\u00e4t, und Excellenz und Gnaden!", "tokens": ["Gold", ",", "Kur\u00b7hut", ",", "Ma\u00b7jes\u00b7t\u00e4t", ",", "und", "Ex\u00b7cel\u00b7lenz", "und", "Gna\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein Hahn stand auf dem Mist, und scharrte tief, und fand,", "tokens": ["Ein", "Hahn", "stand", "auf", "dem", "Mist", ",", "und", "scharr\u00b7te", "tief", ",", "und", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt eines Gerstenkorns, den sch\u00f6nsten Diamant!", "tokens": ["Statt", "ei\u00b7nes", "Gers\u00b7ten\u00b7korns", ",", "den", "sch\u00f6ns\u00b7ten", "Di\u00b7a\u00b7mant", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ei! w\u00e4rst du, sprach der Hahn, ein Gerstenkorn gewesen!", "tokens": ["Ei", "!", "w\u00e4rst", "du", ",", "sprach", "der", "Hahn", ",", "ein", "Gers\u00b7ten\u00b7korn", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wetzte seinen Rittersporn,", "tokens": ["Und", "wetz\u00b7te", "sei\u00b7nen", "Rit\u00b7ter\u00b7sporn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und warf den sch\u00f6nen Stein weit von sich weg, im Zorn!", "tokens": ["Und", "warf", "den", "sch\u00f6\u00b7nen", "Stein", "weit", "von", "sich", "weg", ",", "im", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "PRF", "PTKVZ", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Hausmagd, fegend mit dem Besen,", "tokens": ["Die", "Haus\u00b7magd", ",", "fe\u00b7gend", "mit", "dem", "Be\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah um sich, sah den Wurf, hob auf den sch\u00f6nen Stein,", "tokens": ["Sah", "um", "sich", ",", "sah", "den", "Wurf", ",", "hob", "auf", "den", "sch\u00f6\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und: sch\u00f6nen Dank, Herr Hahn! sprach sie, sie sollen leben!", "tokens": ["Und", ":", "sch\u00f6\u00b7nen", "Dank", ",", "Herr", "Hahn", "!", "sprach", "sie", ",", "sie", "sol\u00b7len", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$.", "ADJA", "NN", "$,", "NN", "NE", "$.", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Daf\u00fcr, da\u00df sie den Stein mir zum Geschenk gegeben,", "tokens": ["Da\u00b7f\u00fcr", ",", "da\u00df", "sie", "den", "Stein", "mir", "zum", "Ge\u00b7schenk", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will ich, Herr Hahn! einmal zu ihren Diensten sein!", "tokens": ["Will", "ich", ",", "Herr", "Hahn", "!", "ein\u00b7mal", "zu", "ih\u00b7ren", "Diens\u00b7ten", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "NE", "$.", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die K\u00f6chin, die das Messer schleifen", "tokens": ["Die", "K\u00f6\u00b7chin", ",", "die", "das", "Mes\u00b7ser", "schlei\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und, eine M\u00f6rderin, an ihnen sich vergreifen", "tokens": ["Und", ",", "ei\u00b7ne", "M\u00f6r\u00b7de\u00b7rin", ",", "an", "ih\u00b7nen", "sich", "ver\u00b7grei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ART", "NN", "$,", "APPR", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit ihrem Messer will, die hats mit mir zu thun!", "tokens": ["Mit", "ih\u00b7rem", "Mes\u00b7ser", "will", ",", "die", "hats", "mit", "mir", "zu", "thun", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "$,", "PRELS", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie, mein Herr Hahn, sie selbst, und auch ihr liebstes Huhn,", "tokens": ["Sie", ",", "mein", "Herr", "Hahn", ",", "sie", "selbst", ",", "und", "auch", "ihr", "liebs\u00b7tes", "Huhn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NE", "$,", "PPER", "ADV", "$,", "KON", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie beide sollen nicht grausamen Todes sterben!", "tokens": ["Sie", "bei\u00b7de", "sol\u00b7len", "nicht", "grau\u00b7sa\u00b7men", "To\u00b7des", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und sterb' einst ich, dann sollen sie", "tokens": ["Und", "sterb'", "einst", "ich", ",", "dann", "sol\u00b7len", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$,", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "(sie sind ein gutes Vieh!)", "tokens": ["(", "sie", "sind", "ein", "gu\u00b7tes", "Vieh", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Von mir ein sch\u00f6n Gef\u00e4\u00df, gef\u00fcllt mit Gersten, erben!", "tokens": ["Von", "mir", "ein", "sch\u00f6n", "Ge\u00b7f\u00e4\u00df", ",", "ge\u00b7f\u00fcllt", "mit", "Gers\u00b7ten", ",", "er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJD", "NN", "$,", "VVPP", "APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der Hahn h\u00f6rt's alles, steht auf seinem Misthof, denkt:", "tokens": ["Der", "Hahn", "h\u00f6rt's", "al\u00b7les", ",", "steht", "auf", "sei\u00b7nem", "Mist\u00b7hof", ",", "denkt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ding hat sich gelenkt!", "tokens": ["Das", "Ding", "hat", "sich", "ge\u00b7lenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Stein enthielt den Grund von meinem l\u00e4ngern Leben,", "tokens": ["Der", "Stein", "ent\u00b7hielt", "den", "Grund", "von", "mei\u00b7nem", "l\u00e4n\u00b7gern", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ich, ich warf ihn weg! ich Dummkopf! h\u00e4tt ich ihn", "tokens": ["Und", "ich", ",", "ich", "warf", "ihn", "weg", "!", "ich", "Dumm\u00b7kopf", "!", "h\u00e4tt", "ich", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "NN", "$.", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Landesmutter zu Berlin,", "tokens": ["Der", "Lan\u00b7des\u00b7mut\u00b7ter", "zu", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "An ihre Krone, hingegeben:", "tokens": ["An", "ih\u00b7re", "Kro\u00b7ne", ",", "hin\u00b7ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer wei\u00df, was dann aus mir geworden w\u00e4r! Allein,", "tokens": ["Wer", "wei\u00df", ",", "was", "dann", "aus", "mir", "ge\u00b7wor\u00b7den", "w\u00e4r", "!", "Al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PPER", "VAPP", "VAFIN", "$.", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "So denkt der weisre Hahn! So handelt und so spricht,", "tokens": ["So", "denkt", "der", "weis\u00b7re", "Hahn", "!", "So", "han\u00b7delt", "und", "so", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In H\u00e4usern und in H\u00fctten,", "tokens": ["In", "H\u00e4u\u00b7sern", "und", "in", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der kl\u00fcgre Mensch, und sieht sogleich das Gro\u00dfe nicht,", "tokens": ["Der", "kl\u00fcg\u00b7re", "Mensch", ",", "und", "sieht", "sog\u00b7leich", "das", "Gro\u00b7\u00dfe", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das aus dem Kleinen quillt! ", "tokens": ["Das", "aus", "dem", "Klei\u00b7nen", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und der, der alles lenkt,", "tokens": ["Und", "der", ",", "der", "al\u00b7les", "lenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gold, Kurhut, Majest\u00e4t, und Excellenz und Gnaden!", "tokens": ["Gold", ",", "Kur\u00b7hut", ",", "Ma\u00b7jes\u00b7t\u00e4t", ",", "und", "Ex\u00b7cel\u00b7lenz", "und", "Gna\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}