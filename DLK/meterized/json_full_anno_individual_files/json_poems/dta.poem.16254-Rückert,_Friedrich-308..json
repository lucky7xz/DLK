{"dta.poem.16254": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "308.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sollst mir auch dein Ohr vor b\u00f6ser Rede sparen", "tokens": ["Du", "sollst", "mir", "auch", "dein", "Ohr", "vor", "b\u00f6\u00b7ser", "Re\u00b7de", "spa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht minder als davor die Zunge selbst bewahren.", "tokens": ["Nicht", "min\u00b7der", "als", "da\u00b7vor", "die", "Zun\u00b7ge", "selbst", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOUS", "PAV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn auch das H\u00f6ren schon von b\u00f6ser Red', o Sohn,", "tokens": ["Denn", "auch", "das", "H\u00f6\u00b7ren", "schon", "von", "b\u00f6\u00b7ser", "Red'", ",", "o", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Theilt einem Herzen mit die Stimmung und den Ton.", "tokens": ["Theilt", "ei\u00b7nem", "Her\u00b7zen", "mit", "die", "Stim\u00b7mung", "und", "den", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "KON", "ART", "NN", "$.