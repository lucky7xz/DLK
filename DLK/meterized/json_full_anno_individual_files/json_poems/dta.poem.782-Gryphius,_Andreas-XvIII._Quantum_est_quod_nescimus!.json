{"dta.poem.782": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.  \n Quantum est quod nescimus!", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jsts! oder ists ein wahn! da\u00df anverwandter Blutt ", "tokens": ["Ists", "!", "o\u00b7der", "ists", "ein", "wahn", "!", "da\u00df", "an\u00b7ver\u00b7wand\u00b7ter", "Blutt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "KON", "VAFIN", "ART", "NN", "$.", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sey kr\u00e4fftig vnsern Geist durch frembde macht zu r\u00fchren:", "tokens": ["Sey", "kr\u00e4ff\u00b7tig", "vn\u00b7sern", "Geist", "durch", "fremb\u00b7de", "macht", "zu", "r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll wenn mein freundt betr\u00fcbt/ ich mich bek\u00fcmmert sp\u00fcren?", "tokens": ["Soll", "wenn", "mein", "freundt", "be\u00b7tr\u00fcbt", "/", "ich", "mich", "be\u00b7k\u00fcm\u00b7mert", "sp\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOUS", "PPOSAT", "ADJD", "VVPP", "$(", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob schon mir nicht endeckt wird seiner schmertzen glutt.", "tokens": ["Ob", "schon", "mir", "nicht", "en\u00b7deckt", "wird", "sei\u00b7ner", "schmert\u00b7zen", "glutt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PTKNEG", "VVPP", "VAFIN", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Soll/ wenn sein C\u00f6rper fault/ mein hochbest\u00fcrtzter mutt", "tokens": ["Soll", "/", "wenn", "sein", "C\u00f6r\u00b7per", "fault", "/", "mein", "hoch\u00b7be\u00b7st\u00fcrtz\u00b7ter", "mutt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In vnbekandt er angst sich kr\u00e4ncken vnd verlieren?", "tokens": ["In", "vn\u00b7be\u00b7kandt", "er", "angst", "sich", "kr\u00e4n\u00b7cken", "vnd", "ver\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PPER", "VVFIN", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll mich sein bild zu nacht in lust vnd schrecken f\u00fchren.", "tokens": ["Soll", "mich", "sein", "bild", "zu", "nacht", "in", "lust", "vnd", "schre\u00b7cken", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd tr\u00f6sten in der pein/ vnd rathen was mir gutt?", "tokens": ["Vnd", "tr\u00f6s\u00b7ten", "in", "der", "pein", "/", "vnd", "ra\u00b7then", "was", "mir", "gutt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "PWS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Bruder/ ehe man mir deinen todt entdecket:", "tokens": ["Mein", "Bru\u00b7der", "/", "e\u00b7he", "man", "mir", "dei\u00b7nen", "todt", "ent\u00b7de\u00b7cket", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PIS", "PPER", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du drey Nachte mich au\u00df meinem schlaff erwecket", "tokens": ["Hast", "du", "drey", "Nach\u00b7te", "mich", "au\u00df", "mei\u00b7nem", "schlaff", "er\u00b7we\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "CARD", "NN", "PPER", "APPR", "PPOSAT", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd mein vnendlich leid zu lindern dich bem\u00fcht.", "tokens": ["Vnd", "mein", "vnend\u00b7lich", "leid", "zu", "lin\u00b7dern", "dich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "ADJD", "PTKZU", "VVINF", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Du hast mir zeit vnd Ortt der abgelegnen reisen/", "tokens": ["Du", "hast", "mir", "zeit", "vnd", "Ortt", "der", "ab\u00b7ge\u00b7leg\u00b7nen", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da ich nicht reisen wolt\u2019 au\u00dfdr\u00fccklich wollen weisen;", "tokens": ["Da", "ich", "nicht", "rei\u00b7sen", "wolt'", "au\u00df\u00b7dr\u00fcck\u00b7lich", "wol\u00b7len", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ists! oder wissen wir/ weit minder al\u00df man stht.", "tokens": ["Ists", "!", "o\u00b7der", "wis\u00b7sen", "wir", "/", "weit", "min\u00b7der", "al\u00df", "man", "stht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KON", "VVFIN", "PPER", "$(", "ADJD", "ADV", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}