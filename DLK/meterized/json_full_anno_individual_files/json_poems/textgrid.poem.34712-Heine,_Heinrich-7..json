{"textgrid.poem.34712": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schmetterling ist in die Rose verliebt,", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", "ist", "in", "die", "Ro\u00b7se", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Umflattert sie tausendmal,", "tokens": ["Um\u00b7flat\u00b7tert", "sie", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihn selber aber, goldig zart,", "tokens": ["Ihn", "sel\u00b7ber", "a\u00b7ber", ",", "gol\u00b7dig", "zart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umflattert der liebende Sonnenstrahl.", "tokens": ["Um\u00b7flat\u00b7tert", "der", "lie\u00b7ben\u00b7de", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Jedoch, in wen ist die Rose verliebt?", "tokens": ["Je\u00b7doch", ",", "in", "wen", "ist", "die", "Ro\u00b7se", "ver\u00b7liebt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das w\u00fc\u00dft ich gar zu g