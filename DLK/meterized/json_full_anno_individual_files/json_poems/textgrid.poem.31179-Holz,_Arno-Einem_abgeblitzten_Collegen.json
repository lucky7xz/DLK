{"textgrid.poem.31179": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Einem abgeblitzten Collegen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Kritikern ein ganzes Rudel", "tokens": ["Von", "Kri\u00b7ti\u00b7kern", "ein", "gan\u00b7zes", "Ru\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprang dir wie W\u00f6lfe bissig ins Genick;", "tokens": ["Sprang", "dir", "wie", "W\u00f6l\u00b7fe", "bis\u00b7sig", "ins", "Ge\u00b7nick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sch\u00f6n begossen wie ein Pudel,", "tokens": ["Und", "sch\u00f6n", "be\u00b7gos\u00b7sen", "wie", "ein", "Pu\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Senkst du nun schamhaft vor der Welt den Blick.", "tokens": ["Senkst", "du", "nun", "scham\u00b7haft", "vor", "der", "Welt", "den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O dieses alberne Gelichter!", "tokens": ["O", "die\u00b7ses", "al\u00b7ber\u00b7ne", "Ge\u00b7lich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wann endlich wird es endlich sich denn klar:", "tokens": ["Wann", "end\u00b7lich", "wird", "es", "end\u00b7lich", "sich", "denn", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch niemals gab es einen Dichter,", "tokens": ["Noch", "nie\u00b7mals", "gab", "es", "ei\u00b7nen", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der d\u00fcmmer noch als seine Verse war!", "tokens": ["Der", "d\u00fcm\u00b7mer", "noch", "als", "sei\u00b7ne", "Ver\u00b7se", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von Kritikern ein ganzes Rudel", "tokens": ["Von", "Kri\u00b7ti\u00b7kern", "ein", "gan\u00b7zes", "Ru\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprang dir wie W\u00f6lfe bissig ins Genick;", "tokens": ["Sprang", "dir", "wie", "W\u00f6l\u00b7fe", "bis\u00b7sig", "ins", "Ge\u00b7nick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sch\u00f6n begossen wie ein Pudel,", "tokens": ["Und", "sch\u00f6n", "be\u00b7gos\u00b7sen", "wie", "ein", "Pu\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "KOKOM", "ART