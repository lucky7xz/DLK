{"dta.poem.860": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlVII.  Auf den Sontag de\u00df mitleidend\u1ebd Ey-  \n verers/ oder  X.  Sontag nach dem Fest der H.  \n Dreyeinigkeit. Lue. 19.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach mein Licht! wo r\u00fchrt es her? da\u00df du dich so hoch betr\u00fc- ", "tokens": ["Ach", "mein", "Licht", "!", "wo", "r\u00fchrt", "es", "her", "?", "da\u00df", "du", "dich", "so", "hoch", "be\u00b7tr\u00fc"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "KOUS", "PPER", "PRF", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+----+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "best? ", "tokens": ["best", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Meine Lust! was kr\u00e4ncket dich? was beschwehr\u2019t dein sanf-", "tokens": ["Mei\u00b7ne", "Lust", "!", "was", "kr\u00e4n\u00b7cket", "dich", "?", "was", "be\u00b7schwehr't", "dein", "san\u00b7f"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPOSAT", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "tes Hertz ", "tokens": ["tes", "Hertz"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Bin ich schuld an dieser angst/ da\u00df ich vnbedacht verschertz", "tokens": ["Bin", "ich", "schuld", "an", "die\u00b7ser", "angst", "/", "da\u00df", "ich", "vn\u00b7be\u00b7dacht", "ver\u00b7schertz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "$(", "KOUS", "PPER", "ADJD", "NN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Diese gnadenzeit in der du mir raum zur Busse giebest?", "tokens": ["Die\u00b7se", "gna\u00b7den\u00b7zeit", "in", "der", "du", "mir", "raum", "zur", "Bus\u00b7se", "gie\u00b7best", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PRELS", "PPER", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich erkenn\u2019 ich bin nicht werth/ da\u00df du heimbsuchst; da\u00df du", "tokens": ["Ich", "er\u00b7kenn'", "ich", "bin", "nicht", "werth", "/", "da\u00df", "du", "heimb\u00b7suchst", ";", "da\u00df", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "KOUS", "PPER", "VVFIN", "$.", "KOUS", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "liebest ", "tokens": ["lie\u00b7best"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Meine blind vnd taube Seel! ach all-sichtb\u2019re Lebenskertz", "tokens": ["Mei\u00b7ne", "blind", "vnd", "tau\u00b7be", "Seel", "!", "ach", "all\u00b7sicht\u00b7b'\u00b7re", "Le\u00b7bens\u00b7kertz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "KON", "ADJA", "NN", "$.", "XY", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Ach entdecke mir die Noht/ der geh\u00e4ufften plagen schmertz", "tokens": ["Ach", "ent\u00b7de\u00b7cke", "mir", "die", "Noht", "/", "der", "ge\u00b7h\u00e4uff\u00b7ten", "pla\u00b7gen", "schmertz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die mit grimmen Donner tob\u2019t/ wenn du Rach vnd Zorn", "tokens": ["Die", "mit", "grim\u00b7men", "Don\u00b7ner", "tob't", "/", "wenn", "du", "Rach", "vnd", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$(", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "ver\u00fcbest: ", "tokens": ["ver\u00b7\u00fc\u00b7best", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Schawe mich dein Zion an/ treib mit scharffen Geisseln", "tokens": ["Scha\u00b7we", "mich", "dein", "Zi\u00b7on", "an", "/", "treib", "mit", "scharf\u00b7fen", "Geis\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$(", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "aus ", "tokens": ["aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Meiner S\u00fcnden kr\u00e4merey/ die mein Hertz dein eig\u1ebd Hau\u00df", "tokens": ["Mei\u00b7ner", "S\u00fcn\u00b7den", "kr\u00e4\u00b7me\u00b7rey", "/", "die", "mein", "Hertz", "dein", "eig\u1ebd", "Hau\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$(", "ART", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Gleich den M\u00f6rder gruben macht/ wenn du diesen tand ge-", "tokens": ["Gleich", "den", "M\u00f6r\u00b7der", "gru\u00b7ben", "macht", "/", "wenn", "du", "die\u00b7sen", "tand", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF", "VVFIN", "$(", "KOUS", "PPER", "PDS", "VVFIN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "r\u00e4umet ", "tokens": ["r\u00e4u\u00b7met"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Wenn du in mir lehren wirst: werd\u2019 ich aller Rach entgehn", "tokens": ["Wenn", "du", "in", "mir", "leh\u00b7ren", "wirst", ":", "werd'", "ich", "al\u00b7ler", "Rach", "ent\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF", "VAFIN", "$.", "VAFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vnd was zu dem Friede dient: weil der Friede bl\u00fcht ver-", "tokens": ["Vnd", "was", "zu", "dem", "Frie\u00b7de", "dient", ":", "weil", "der", "Frie\u00b7de", "bl\u00fcht", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "VVFIN", "$.", "KOUS", "ART", "NN", "VVFIN", "TRUNC"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "stehn. ", "tokens": ["stehn", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Auch einbringen was bi\u00dfher/ meine tr\u00e4gheit hat vers\u00e4umet.", "tokens": ["Auch", "ein\u00b7brin\u00b7gen", "was", "bi\u00df\u00b7her", "/", "mei\u00b7ne", "tr\u00e4g\u00b7heit", "hat", "ver\u00b7s\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "PWS", "ADV", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}}}}}