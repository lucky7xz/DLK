{"textgrid.poem.52039": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Junckern, welche du eh' als die Glocke schl\u00e4gt,", "tokens": ["Die", "Jun\u00b7ckern", ",", "wel\u00b7che", "du", "eh'", "als", "die", "Glo\u00b7cke", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um die der B\u00fcrger sonst des Abends pflegt zu e\u00dfen,", "tokens": ["Um", "die", "der", "B\u00fcr\u00b7ger", "sonst", "des", "A\u00b7bends", "pflegt", "zu", "e\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dort a la mode siehst die Pflaster Steine me\u00dfen,", "tokens": ["Dort", "a", "la", "mo\u00b7de", "siehst", "die", "Pflas\u00b7ter", "Stei\u00b7ne", "me\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "FM", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die haben zu Pari\u00df die neuste Tracht verlegt.", "tokens": ["Die", "ha\u00b7ben", "zu", "Pa\u00b7ri\u00df", "die", "neus\u00b7te", "Tracht", "ver\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum wann die Cavalier de la Speranze gehn,", "tokens": ["Drum", "wann", "die", "Ca\u00b7va\u00b7lier", "de", "la", "Spe\u00b7ran\u00b7ze", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "ART", "NN", "FM", "FM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auff dem Marckte hin frantz\u00f6sisch schreyn und lallen,", "tokens": ["Und", "auff", "dem", "Marck\u00b7te", "hin", "frant\u00b7z\u00f6\u00b7sisch", "schreyn", "und", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ihrer Worte Macht mu\u00df an die Fenster schallen,", "tokens": ["Da\u00df", "ih\u00b7rer", "Wor\u00b7te", "Macht", "mu\u00df", "an", "die", "Fens\u00b7ter", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo ihre Damen sind, bleib nicht best\u00fcrtzet stehn.", "tokens": ["Wo", "ih\u00b7re", "Da\u00b7men", "sind", ",", "bleib", "nicht", "be\u00b7st\u00fcrt\u00b7zet", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$,", "VVFIN", "PTKNEG", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Reden, die sie f\u00fchrn, die sind so weit nicht her,", "tokens": ["Die", "Re\u00b7den", ",", "die", "sie", "f\u00fchrn", ",", "die", "sind", "so", "weit", "nicht", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Thun und Hand Werck ist; Wie au\u00dfer viel Beschwerden", "tokens": ["Ihr", "Thun", "und", "Hand", "Werck", "ist", ";", "Wie", "au\u00b7\u00dfer", "viel", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "VAFIN", "$.", "PWAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mit manier man sol der Heller ledig werden.", "tokens": ["Und", "mit", "ma\u00b7nier", "man", "sol", "der", "Hel\u00b7ler", "le\u00b7dig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "PIS", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nein, umb so arm zu seyn, rei\u00df ich nicht \u00fcber Meer.", "tokens": ["Nein", ",", "umb", "so", "arm", "zu", "seyn", ",", "rei\u00df", "ich", "nicht", "\u00fc\u00b7ber", "Meer", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUI", "ADV", "ADJD", "PTKZU", "VAINF", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Junckern, welche du eh' als die Glocke schl\u00e4gt,", "tokens": ["Die", "Jun\u00b7ckern", ",", "wel\u00b7che", "du", "eh'", "als", "die", "Glo\u00b7cke", "sch