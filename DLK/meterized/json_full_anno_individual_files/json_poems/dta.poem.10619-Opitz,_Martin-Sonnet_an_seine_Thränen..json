{"dta.poem.10619": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet an seine Thr\u00e4nen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr meiner Augen B\u00e4ch/ jhr angenehme Z\u00e4hren/", "tokens": ["Ihr", "mei\u00b7ner", "Au\u00b7gen", "B\u00e4ch", "/", "jhr", "an\u00b7ge\u00b7neh\u00b7me", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jhr in Trawrigkeit mir alle Freude bringt/", "tokens": ["Die", "jhr", "in", "Traw\u00b7rig\u00b7keit", "mir", "al\u00b7le", "Freu\u00b7de", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gl\u00fcckseelig weret jhr/ wann die/ so euch erzwingt/", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "we\u00b7ret", "jhr", "/", "wann", "die", "/", "so", "euch", "er\u00b7zwingt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$(", "PWAV", "ART", "$(", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie ich zu jhr mich/ sie zu euch sich wolte kehren/", "tokens": ["Wie", "ich", "zu", "jhr", "mich", "/", "sie", "zu", "euch", "sich", "wol\u00b7te", "keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "PRF", "$(", "PPER", "APPR", "PPER", "PRF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun aber/ ob auch gleich der Regen wol kan wehren", "tokens": ["Nun", "a\u00b7ber", "/", "ob", "auch", "gleich", "der", "Re\u00b7gen", "wol", "kan", "weh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "KOUS", "ADV", "ADV", "ART", "NN", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der h\u00f6chsten H\u00e4rtigkeit/ vnd durch die Steine dringt/", "tokens": ["Der", "h\u00f6chs\u00b7ten", "H\u00e4r\u00b7tig\u00b7keit", "/", "vnd", "durch", "die", "Stei\u00b7ne", "dringt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Euch aber jhren Sinn zuwenden nicht gelingt/", "tokens": ["Euch", "a\u00b7ber", "jhren", "Sinn", "zu\u00b7wen\u00b7den", "nicht", "ge\u00b7lingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVFIN", "PTKNEG", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ob jhr ohn vnderla\u00df gleich fliesset mit beschweren/", "tokens": ["Ob", "jhr", "ohn", "vn\u00b7der\u00b7la\u00df", "gleich", "flies\u00b7set", "mit", "be\u00b7schwe\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So lasset mich doch nicht/ so bleibet doch bey mir/", "tokens": ["So", "las\u00b7set", "mich", "doch", "nicht", "/", "so", "blei\u00b7bet", "doch", "bey", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "O jhr mein h\u00f6chster Trost/ jhr heisse liebes Thr\u00e4nen/", "tokens": ["O", "jhr", "mein", "h\u00f6chs\u00b7ter", "Trost", "/", "jhr", "heis\u00b7se", "lie\u00b7bes", "Thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vnd wann ich vnd mein Hertz nach euch sich werden sehnen/", "tokens": ["Vnd", "wann", "ich", "vnd", "mein", "Hertz", "nach", "euch", "sich", "wer\u00b7den", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "KON", "PPOSAT", "NN", "APPR", "PPER", "PRF", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Last ewer Br\u00fcnnelein ja fliessen f\u00fcr vnd f\u00fcr/", "tokens": ["Last", "e\u00b7wer", "Br\u00fcn\u00b7nel\u00b7ein", "ja", "flies\u00b7sen", "f\u00fcr", "vnd", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVINF", "APPR", "KON", "APPR", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Bi\u00df mein Lieb sicht/ da\u00df jhr mehr quellen k\u00f6nnet nicht/", "tokens": ["Bi\u00df", "mein", "Lieb", "sicht", "/", "da\u00df", "jhr", "mehr", "quel\u00b7len", "k\u00f6n\u00b7net", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "PTKNEG", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Bi\u00df mir die Seel au\u00dffehrt f\u00fcr jhrem Angesicht.", "tokens": ["Bi\u00df", "mir", "die", "Seel", "au\u00df\u00b7fehrt", "f\u00fcr", "jhrem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}}}}