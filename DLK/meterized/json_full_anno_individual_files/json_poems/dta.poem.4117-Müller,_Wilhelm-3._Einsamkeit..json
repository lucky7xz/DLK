{"dta.poem.4117": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "3.   Einsamkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mai ist vor der Th\u00fcr:", "tokens": ["Der", "Mai", "ist", "vor", "der", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Garten, auf der Wiesen,", "tokens": ["Im", "Gar\u00b7ten", ",", "auf", "der", "Wie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Bl\u00fcmlein kommt herf\u00fcr!", "tokens": ["Ihr", "Bl\u00fcm\u00b7lein", "kommt", "her\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da hab' ich den Stab genommen,", "tokens": ["Da", "hab'", "ich", "den", "Stab", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da hab' ich das B\u00fcndel geschn\u00fcrt,", "tokens": ["Da", "hab'", "ich", "das", "B\u00fcn\u00b7del", "ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Zieh weiter und immer weiter,", "tokens": ["Zieh", "wei\u00b7ter", "und", "im\u00b7mer", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohin die Stra\u00dfe mich f\u00fchrt.", "tokens": ["Wo\u00b7hin", "die", "Stra\u00b7\u00dfe", "mich", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und \u00fcber mir ziehen die V\u00f6gel,", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "zie\u00b7hen", "die", "V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie ziehen in lustigem Reihn,", "tokens": ["Sie", "zie\u00b7hen", "in", "lus\u00b7ti\u00b7gem", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie zwitschern und trillern und fl\u00f6ten,", "tokens": ["Sie", "zwit\u00b7schern", "und", "tril\u00b7lern", "und", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als ging's in den Himmel hinein.", "tokens": ["Als", "ging's", "in", "den", "Him\u00b7mel", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Der Wandrer geht alleine,", "tokens": ["Der", "Wand\u00b7rer", "geht", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht schweigend seinen Gang;", "tokens": ["Geht", "schwei\u00b7gend", "sei\u00b7nen", "Gang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das B\u00fcndel will ihn dr\u00fccken,", "tokens": ["Das", "B\u00fcn\u00b7del", "will", "ihn", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Weg wird ihm zu lang.", "tokens": ["Der", "Weg", "wird", "ihm", "zu", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ja, wenn wir allzusammen", "tokens": ["Ja", ",", "wenn", "wir", "all\u00b7zu\u00b7sam\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So z\u00f6gen in's Land hinein!", "tokens": ["So", "z\u00f6\u00b7gen", "in's", "Land", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wenn auch das nicht w\u00e4re,", "tokens": ["Und", "wenn", "auch", "das", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PDS", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "K\u00f6nnt' Eine nur mit mir sein!", "tokens": ["K\u00f6nnt'", "Ei\u00b7ne", "nur", "mit", "mir", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}