{"dta.poem.13253": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem man so zerstreut von diesem Zufall sprach,", "tokens": ["In\u00b7dem", "man", "so", "zer\u00b7streut", "von", "die\u00b7sem", "Zu\u00b7fall", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und es noch an der Sach Erkl\u00e4rungen gebrach:", "tokens": ["Und", "es", "noch", "an", "der", "Sach", "Er\u00b7kl\u00e4\u00b7run\u00b7gen", "ge\u00b7brach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.3": {"text": "So kahm die Fr\u00f6mmigkeit und wies sich voller Freuden,", "tokens": ["So", "kahm", "die", "Fr\u00f6m\u00b7mig\u00b7keit", "und", "wies", "sich", "vol\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als w\u00fcrde sie vielleicht des Zweifels Wahn entscheiden.", "tokens": ["Als", "w\u00fcr\u00b7de", "sie", "viel\u00b7leicht", "des", "Zwei\u00b7fels", "Wahn", "ent\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "285Sie truge jenen Stab (es war ein kleines Kreuz)", "tokens": ["tru\u00b7ge", "je\u00b7nen", "Stab", "(", "es", "war", "ein", "klei\u00b7nes", "Kreuz", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Vor dem Gesicht daher, da\u00df man es allerseits", "tokens": ["Vor", "dem", "Ge\u00b7sicht", "da\u00b7her", ",", "da\u00df", "man", "es", "al\u00b7ler\u00b7seits"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PAV", "$,", "KOUS", "PIS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bequemlich sehen konnt; ihr munteres Gesichte", "tokens": ["Be\u00b7quem\u00b7lich", "se\u00b7hen", "konnt", ";", "ihr", "mun\u00b7te\u00b7res", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVINF", "VMFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Lie\u00df, als ob sie zugleich auf etwas anders dichte.", "tokens": ["Lie\u00df", ",", "als", "ob", "sie", "zu\u00b7gleich", "auf", "et\u00b7was", "an\u00b7ders", "dich\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "APPR", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie war noch mit dem Kreuz nicht bey dem K\u00f6nigs Thron,", "tokens": ["Sie", "war", "noch", "mit", "dem", "Kreuz", "nicht", "bey", "dem", "K\u00f6\u00b7nigs", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "290Ja kaum noch von der Th\u00fcr herein, so sprach sie schon", "tokens": ["kaum", "noch", "von", "der", "Th\u00fcr", "her\u00b7ein", ",", "so", "sprach", "sie", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Zu jenen Tugenden, wo sie vorbey gegangen:", "tokens": ["Zu", "je\u00b7nen", "Tu\u00b7gen\u00b7den", ",", "wo", "sie", "vor\u00b7bey", "ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es habe das Geschick was wunderbars verhangen;", "tokens": ["Es", "ha\u00b7be", "das", "Ge\u00b7schick", "was", "wun\u00b7der\u00b7bars", "ver\u00b7han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PWS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So wuchse die Begier. Man bildete sich ein", "tokens": ["So", "wuch\u00b7se", "die", "Be\u00b7gier", ".", "Man", "bil\u00b7de\u00b7te", "sich", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PIS", "VVFIN", "PRF", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Nachricht wurde nur von diesen Adlern seyn.", "tokens": ["Die", "Nach\u00b7richt", "wur\u00b7de", "nur", "von", "die\u00b7sen", "Ad\u00b7lern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PDAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}