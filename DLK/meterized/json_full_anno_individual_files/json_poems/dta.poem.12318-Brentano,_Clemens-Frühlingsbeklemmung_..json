{"dta.poem.12318": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsbeklemmung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der tr\u00fcbe Winter ist vorbey,               ", "tokens": ["Der", "tr\u00fc\u00b7be", "Win\u00b7ter", "ist", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kranich wiederkehren,", "tokens": ["Die", "Kra\u00b7nich", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun reget sich der Vogelschrey,", "tokens": ["Nun", "re\u00b7get", "sich", "der", "Vo\u00b7gel\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nester sich vermehren;", "tokens": ["Die", "Nes\u00b7ter", "sich", "ver\u00b7meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Laub allgemach", "tokens": ["Laub", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Nun schleicht an Tag,", "tokens": ["Nun", "schleicht", "an", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Bl\u00fcmlein sich nun melden,", "tokens": ["Die", "Bl\u00fcm\u00b7lein", "sich", "nun", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Schl\u00e4nglein krumm,", "tokens": ["Wie", "Schl\u00e4ng\u00b7lein", "krumm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Gehn l\u00e4chelnd um", "tokens": ["Gehn", "l\u00e4\u00b7chelnd", "um"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Die B\u00e4chlein k\u00fchl in W\u00e4lden.", "tokens": ["Die", "B\u00e4ch\u00b7lein", "k\u00fchl", "in", "W\u00e4l\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Br\u00fcnnlein klar, und Quellen rein,", "tokens": ["Der", "Br\u00fcnn\u00b7lein", "klar", ",", "und", "Quel\u00b7len", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel hie, viel dort erscheinen,", "tokens": ["Viel", "hie", ",", "viel", "dort", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All silberwei\u00dfe T\u00f6chterlein", "tokens": ["All", "sil\u00b7ber\u00b7wei\u00b7\u00dfe", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hohen Berg und Steinen;", "tokens": ["Der", "ho\u00b7hen", "Berg", "und", "Stei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In gro\u00dfer Meng", "tokens": ["In", "gro\u00b7\u00dfer", "Meng"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sie mit Gedr\u00e4ng,", "tokens": ["Sie", "mit", "Ge\u00b7dr\u00e4ng", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie Pfeil von Felsen zielen,", "tokens": ["Wie", "Pfeil", "von", "Fel\u00b7sen", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Baldrauschens her,", "tokens": ["Bald\u00b7rau\u00b7schens", "her", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Nicht ohn Geplerr,", "tokens": ["Nicht", "ohn", "Ge\u00b7plerr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Und mit den Steinlein spielen.", "tokens": ["Und", "mit", "den", "Stein\u00b7lein", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die J\u00e4gerin, Diana stolz,", "tokens": ["Die", "J\u00e4\u00b7ge\u00b7rin", ",", "Di\u00b7a\u00b7na", "stolz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Wald- und Wasser-Nymphen,", "tokens": ["Auch", "Wald", "und", "Was\u00b7ser\u00b7Nym\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun wieder frisch im gr\u00fcnen Holz", "tokens": ["Nun", "wie\u00b7der", "frisch", "im", "gr\u00fc\u00b7nen", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gehn spielen, scherzend schimpfen;", "tokens": ["Gehn", "spie\u00b7len", ",", "scher\u00b7zend", "schimp\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die reine Sonn", "tokens": ["Die", "rei\u00b7ne", "Sonn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Schm\u00fcckt ihre Kron,", "tokens": ["Schm\u00fcckt", "ih\u00b7re", "Kron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den K\u00f6cher f\u00fcllt mit Pfeilen;", "tokens": ["Den", "K\u00f6\u00b7cher", "f\u00fcllt", "mit", "Pfei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr beste Ro\u00df", "tokens": ["Ihr", "bes\u00b7te", "Ro\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "L\u00e4\u00dft lauffen los", "tokens": ["L\u00e4\u00dft", "lauf\u00b7fen", "los"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Auf marmorglatten Meilen.", "tokens": ["Auf", "mar\u00b7mor\u00b7glat\u00b7ten", "Mei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mit ihr die k\u00fchlen Sommerwind,", "tokens": ["Mit", "ihr", "die", "k\u00fch\u00b7len", "Som\u00b7mer\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All J\u00fcngling still von Sitten,", "tokens": ["All", "J\u00fcng\u00b7ling", "still", "von", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Luft zu spielen seyn gesinnt,", "tokens": ["In", "Luft", "zu", "spie\u00b7len", "seyn", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Wolken leicht beritten;", "tokens": ["Auf", "Wol\u00b7ken", "leicht", "be\u00b7rit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die B\u00e4um und Aest", "tokens": ["Die", "B\u00e4um", "und", "A\u00b7est"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Auch thun das best,", "tokens": ["Auch", "thun", "das", "best", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "PDS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Bereichen sich mit Schatten,", "tokens": ["Be\u00b7rei\u00b7chen", "sich", "mit", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo sich verhalt", "tokens": ["Wo", "sich", "ver\u00b7halt"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PRF", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Das Wild im Wald,", "tokens": ["Das", "Wild", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Wenns will von Hitz ermatten.", "tokens": ["Wenns", "will", "von", "Hitz", "er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Meng der V\u00f6glein h\u00f6ren la\u00dft", "tokens": ["Die", "Meng", "der", "V\u00f6\u00b7glein", "h\u00f6\u00b7ren", "la\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Schir von Tire Lire,", "tokens": ["Ihr", "Schir", "von", "Ti\u00b7re", "Li\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sauset auch so mancher Ast,", "tokens": ["Da", "sau\u00b7set", "auch", "so", "man\u00b7cher", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob er musicire;", "tokens": ["Als", "ob", "er", "mu\u00b7si\u00b7ci\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Zweiglein schwank,", "tokens": ["Die", "Zwei\u00b7glein", "schwank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zum Vogelsang,", "tokens": ["Zum", "Vo\u00b7gel\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sich auf- und nieder neigen,", "tokens": ["Sich", "auf", "und", "nie\u00b7der", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "TRUNC", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch h\u00f6ret man", "tokens": ["Auch", "h\u00f6\u00b7ret", "man"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Auf gr\u00fcnem Plan,", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Plan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Spazieren Laut und Geigen.", "tokens": ["Spa\u00b7zie\u00b7ren", "Laut", "und", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo man nur schaut, fast alle Welt", "tokens": ["Wo", "man", "nur", "schaut", ",", "fast", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Freuden sich thut r\u00fcsten,", "tokens": ["Zu", "Freu\u00b7den", "sich", "thut", "r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Scherzen alles ist gestellt,", "tokens": ["Zum", "Scher\u00b7zen", "al\u00b7les", "ist", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwebt alles fast in L\u00fcsten;", "tokens": ["Schwebt", "al\u00b7les", "fast", "in", "L\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur ich allein", "tokens": ["Nur", "ich", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Leid s\u00fc\u00dfe Pein,", "tokens": ["Leid", "s\u00fc\u00b7\u00dfe", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Unendlich werd gequ\u00e4let,", "tokens": ["Un\u00b7end\u00b7lich", "werd", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Seit ich mit dir,", "tokens": ["Seit", "ich", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und du mit mir,", "tokens": ["Und", "du", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "O Jesu, dich verm\u00e4hlet.", "tokens": ["O", "Je\u00b7su", ",", "dich", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}