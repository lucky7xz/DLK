{"textgrid.poem.44408": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "In das Stammbuch einer Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das bittere Gef\u00fchl, wie arm dies Leben,", "tokens": ["Das", "bit\u00b7te\u00b7re", "Ge\u00b7f\u00fchl", ",", "wie", "arm", "dies", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ungen\u00fcgend irdschen Gl\u00fcckes Gunst,", "tokens": ["Wie", "un\u00b7ge\u00b7n\u00fc\u00b7gend", "ird\u00b7schen", "Gl\u00fc\u00b7ckes", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Derselbe Wunsch, das n\u00e4mliche Bestreben", "tokens": ["Der\u00b7sel\u00b7be", "Wunsch", ",", "das", "n\u00e4m\u00b7li\u00b7che", "Be\u00b7stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gab dich dem Glauben, mich der Kunst.", "tokens": ["Gab", "dich", "dem", "Glau\u00b7ben", ",", "mich", "der", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob scheinbar gleich sich unsre Pfade scheiden,", "tokens": ["Ob", "schein\u00b7bar", "gleich", "sich", "uns\u00b7re", "Pfa\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie gehn aus ", "tokens": ["Sie", "gehn", "aus"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Ist nur die Welt ein abgeschlo\u00dfnes Rund \u2013", "tokens": ["Ist", "nur", "die", "Welt", "ein", "ab\u00b7ge\u00b7schlo\u00df\u00b7nes", "Rund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So m\u00fcssen irgendwo die Linien sich schneiden.", "tokens": ["So", "m\u00fcs\u00b7sen", "ir\u00b7gend\u00b7wo", "die", "Li\u00b7ni\u00b7en", "sich", "schnei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das bittere Gef\u00fchl, wie arm dies Leben,", "tokens": ["Das", "bit\u00b7te\u00b7re", "Ge\u00b7f\u00fchl", ",", "wie", "arm", "dies", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie ungen\u00fcgend irdschen Gl\u00fcckes Gunst,", "tokens": ["Wie", "un\u00b7ge\u00b7n\u00fc\u00b7gend", "ird\u00b7schen", "Gl\u00fc\u00b7ckes", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Derselbe Wunsch, das n\u00e4mliche Bestreben", "tokens": ["Der\u00b7sel\u00b7be", "Wunsch", ",", "das", "n\u00e4m\u00b7li\u