{"textgrid.poem.32659": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An die Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holde Liebe, welchen J\u00fcngling du", "tokens": ["Hol\u00b7de", "Lie\u00b7be", ",", "wel\u00b7chen", "J\u00fcng\u00b7ling", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAT", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dir zum Freund erkoren,", "tokens": ["Dir", "zum", "Freund", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dem wird jeder Augenblick zur Ruh", "tokens": ["Dem", "wird", "je\u00b7der", "Au\u00b7gen\u00b7blick", "zur", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und zum Gl\u00fcck geboren!", "tokens": ["Und", "zum", "Gl\u00fcck", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Fr\u00f6hlich sieht sein bl\u00fchendes Gesicht", "tokens": ["Fr\u00f6h\u00b7lich", "sieht", "sein", "bl\u00fc\u00b7hen\u00b7des", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Jeden Tag entstehen;", "tokens": ["Je\u00b7den", "Tag", "ent\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Fr\u00f6hlich sieht er ihn im Purpurlicht", "tokens": ["Fr\u00f6h\u00b7lich", "sieht", "er", "ihn", "im", "Pur\u00b7pur\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wieder untergehen.", "tokens": ["Wie\u00b7der", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Alle V\u00f6gel singen ihm im Hain", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "sin\u00b7gen", "ihm", "im", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfe Melodieen;", "tokens": ["S\u00fc\u00b7\u00dfe", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Jedes Bl\u00fcmchen w\u00fcnscht ihm sch\u00f6n zu sein,", "tokens": ["Je\u00b7des", "Bl\u00fcm\u00b7chen", "w\u00fcnscht", "ihm", "sch\u00f6n", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und f\u00fcr ihn zu bl\u00fchen.", "tokens": ["Und", "f\u00fcr", "ihn", "zu", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jede Rose f\u00fchlet s\u00fc\u00dfe Lust,", "tokens": ["Je\u00b7de", "Ro\u00b7se", "f\u00fch\u00b7let", "s\u00fc\u00b7\u00dfe", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Die sein Finger pfl\u00fccket;", "tokens": ["Die", "sein", "Fin\u00b7ger", "pfl\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Weil er sie an die geliebte Brust", "tokens": ["Weil", "er", "sie", "an", "die", "ge\u00b7lieb\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Seines M\u00e4dchens dr\u00fccket.", "tokens": ["Sei\u00b7nes", "M\u00e4d\u00b7chens", "dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00dfe Freude trinkt er mit dem Blut", "tokens": ["S\u00fc\u00b7\u00dfe", "Freu\u00b7de", "trinkt", "er", "mit", "dem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von des Weingotts Reben;", "tokens": ["Von", "des", "Wein\u00b7gotts", "Re\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Von begl\u00fcckten Tr\u00e4umen, wenn er ruht,", "tokens": ["Von", "be\u00b7gl\u00fcck\u00b7ten", "Tr\u00e4u\u00b7men", ",", "wenn", "er", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ist sein Haupt umgeben.", "tokens": ["Ist", "sein", "Haupt", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sein Erwachen ist ein \u00dcbergang", "tokens": ["Sein", "Er\u00b7wa\u00b7chen", "ist", "ein", "\u00dc\u00b7ber\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Zu begl\u00fccktern Scenen;", "tokens": ["Zu", "be\u00b7gl\u00fcck\u00b7tern", "Sce\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Heiter eilt er, unter Lustgesang,", "tokens": ["Hei\u00b7ter", "eilt", "er", ",", "un\u00b7ter", "Lust\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "In den Kreis der Sch\u00f6nen \u2013", "tokens": ["In", "den", "Kreis", "der", "Sch\u00f6\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aber, welche Stunden voller Schmerz", "tokens": ["A\u00b7ber", ",", "wel\u00b7che", "Stun\u00b7den", "vol\u00b7ler", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Drohn des J\u00fcnglings Leben,", "tokens": ["Drohn", "des", "J\u00fcng\u00b7lings", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der umsonst sein jugendliches Herz,", "tokens": ["Der", "um\u00b7sonst", "sein", "ju\u00b7gend\u00b7li\u00b7ches", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "G\u00f6ttin, dir ergeben!", "tokens": ["G\u00f6t\u00b7tin", ",", "dir", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihm verl\u00e4ngert jeder Augenblick", "tokens": ["Ihm", "ver\u00b7l\u00e4n\u00b7gert", "je\u00b7der", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sich zu bangen Stunden!", "tokens": ["Sich", "zu", "ban\u00b7gen", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mit den Kinderjahren ist das Gl\u00fcck", "tokens": ["Mit", "den", "Kin\u00b7der\u00b7jah\u00b7ren", "ist", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ewig ihm verschwunden.", "tokens": ["E\u00b7wig", "ihm", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Thr\u00e4nen flie\u00dfen ihm im bangen Traum", "tokens": ["Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "ihm", "im", "ban\u00b7gen", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von den blassen Wangen,", "tokens": ["Von", "den", "blas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und er sieht die Morgensonne kaum", "tokens": ["Und", "er", "sieht", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Am Olympus prangen.", "tokens": ["Am", "O\u00b7lym\u00b7pus", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hoffnungslos sieht er den Winter fliehn", "tokens": ["Hoff\u00b7nungs\u00b7los", "sieht", "er", "den", "Win\u00b7ter", "fliehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und den Schnee verschwinden;", "tokens": ["Und", "den", "Schnee", "ver\u00b7schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Traurig schleicht er durch den Fr\u00fchling hin,", "tokens": ["Trau\u00b7rig", "schleicht", "er", "durch", "den", "Fr\u00fch\u00b7ling", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Kann ihn nicht empfinden.", "tokens": ["Kann", "ihn", "nicht", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sieht mit kaltem Blick die junge Flur", "tokens": ["Sieht", "mit", "kal\u00b7tem", "Blick", "die", "jun\u00b7ge", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sich allm\u00e4hlich f\u00e4rben;", "tokens": ["Sich", "all\u00b7m\u00e4h\u00b7lich", "f\u00e4r\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Halberstorbne Bl\u00fcmchen pfl\u00fcckt er nur,", "tokens": ["Hal\u00b7ber\u00b7storb\u00b7ne", "Bl\u00fcm\u00b7chen", "pfl\u00fcckt", "er", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00fcnscht, wie sie, zu sterben.", "tokens": ["W\u00fcnscht", ",", "wie", "sie", ",", "zu", "ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jedes M\u00e4dchen lockt ihm Thr\u00e4nen ab,", "tokens": ["Je\u00b7des", "M\u00e4d\u00b7chen", "lockt", "ihm", "Thr\u00e4\u00b7nen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Das dem seinen gleichet;", "tokens": ["Das", "dem", "sei\u00b7nen", "glei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Jeden H\u00fcgel w\u00fcnscht er sich zum Grab,", "tokens": ["Je\u00b7den", "H\u00fc\u00b7gel", "w\u00fcnscht", "er", "sich", "zum", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wo er einsam schleichet.", "tokens": ["Wo", "er", "ein\u00b7sam", "schlei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Die geliebte kleine Nachtigall", "tokens": ["Die", "ge\u00b7lieb\u00b7te", "klei\u00b7ne", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Singt ihm Grabelieder;", "tokens": ["Singt", "ihm", "Gra\u00b7be\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Endlich sinkt er, wie im Sonnenstrahl", "tokens": ["End\u00b7lich", "sinkt", "er", ",", "wie", "im", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Welke Blumen, nieder.", "tokens": ["Wel\u00b7ke", "Blu\u00b7men", ",", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seine Seele, die der Liebe Joch", "tokens": ["Sei\u00b7ne", "See\u00b7le", ",", "die", "der", "Lie\u00b7be", "Joch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Jahrelang getragen,", "tokens": ["Jah\u00b7re\u00b7lang", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Irrt um das geliebte M\u00e4dchen noch,", "tokens": ["Irrt", "um", "das", "ge\u00b7lieb\u00b7te", "M\u00e4d\u00b7chen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und zerfleu\u00dft in Klagen.", "tokens": ["Und", "zer\u00b7fleu\u00dft", "in", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "G\u00f6ttin Liebe! Will es mein Geschick,", "tokens": ["G\u00f6t\u00b7tin", "Lie\u00b7be", "!", "Will", "es", "mein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df auch ich dir diene;", "tokens": ["Da\u00df", "auch", "ich", "dir", "die\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "O so l\u00e4chle mir mit holdem Blick,", "tokens": ["O", "so", "l\u00e4ch\u00b7le", "mir", "mit", "hol\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Geu\u00df in Daphnens Miene", "tokens": ["Geu\u00df", "in", "Daph\u00b7nens", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Deine milde Flamme, da\u00df sie mir", "tokens": ["Dei\u00b7ne", "mil\u00b7de", "Flam\u00b7me", ",", "da\u00df", "sie", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sanft entgegen strahle,", "tokens": ["Sanft", "ent\u00b7ge\u00b7gen", "strah\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und ich dankbarliche Lieder dir", "tokens": ["Und", "ich", "dank\u00b7bar\u00b7li\u00b7che", "Lie\u00b7der", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Jeden Tag bezahle!", "tokens": ["Je\u00b7den", "Tag", "be\u00b7zah\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Holde Liebe, welchen J\u00fcngling du", "tokens": ["Hol\u00b7de", "Lie\u00b7be", ",", "wel\u00b7chen", "J\u00fcng\u00b7ling", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAT", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dir zum Freund erkoren,", "tokens": ["Dir", "zum", "Freund", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dem wird jed