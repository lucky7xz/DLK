"pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Singt ihm Grabelieder;", "tokens": ["Singt", "ihm", "Gra\u00b7be\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Endlich sinkt er, wie im Sonnenstrahl", "tokens": ["End\u00b7lich", "sinkt", "er", ",", "wie", "im", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Welke Blumen, nieder.", "tokens": ["Wel\u00b7ke", "Blu\u00b7men", ",", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seine Seele, die der Liebe Joch", "tokens": ["Sei\u00b7ne", "See\u00b7le", ",", "die", "der", "Lie\u00b7be", "Joch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Jahrelang getragen,", "tokens": ["Jah\u00b7re\u00b7lang", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Irrt um das geliebte M\u00e4dchen noch,", "tokens": ["Irrt", "um", "das", "ge\u00b7lieb\u00b7te", "M\u00e4d\u00b7chen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und zerfleu\u00dft in Klagen.", "tokens": ["Und", "zer\u00b7fleu\u00dft", "in", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "G\u00f6ttin Liebe! Will es mein Geschick,", "tokens": ["G\u00f6t\u00b7tin", "Lie\u00b7be", "!", "Will", "es", "mein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df auch ich dir diene;", "tokens": ["Da\u00df", "auch", "ich", "dir", "die\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "O so l\u00e4chle mir mit holdem Blick,", "tokens": ["O", "so", "l\u00e4ch\u00b7le", "mir", "mit", "hol\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Geu\u00df in Daphnens Miene", "tokens": ["Geu\u00df", "in", "Daph\u00b7nens", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Deine milde Flamme, da\u00df sie mir", "tokens": ["Dei\u00b7ne", "mil\u00b7de", "Flam\u00b7me", ",", "da\u00df", "sie", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sanft entgegen strahle,", "tokens": ["Sanft", "ent\u00b7ge\u00b7gen", "strah\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und ich dankbarliche Lieder dir", "tokens": ["U