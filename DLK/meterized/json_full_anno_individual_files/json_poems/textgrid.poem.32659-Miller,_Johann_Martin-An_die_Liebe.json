er": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Sich zu bangen Stunden!", "tokens": ["Sich", "zu", "ban\u00b7gen", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mit den Kinderjahren ist das Gl\u00fcck", "tokens": ["Mit", "den", "Kin\u00b7der\u00b7jah\u00b7ren", "ist", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ewig ihm verschwunden.", "tokens": ["E\u00b7wig", "ihm", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Thr\u00e4nen flie\u00dfen ihm im bangen Traum", "tokens": ["Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "ihm", "im", "ban\u00b7gen", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von den blassen Wangen,", "tokens": ["Von", "den", "blas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und er sieht die Morgensonne kaum", "tokens": ["Und", "er", "sieht", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Am Olympus prangen.", "tokens": ["Am", "O\u00b7lym\u00b7pus", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hoffnungslos sieht er den Winter fliehn", "tokens": ["Hoff\u00b7nungs\u00b7los", "sieht", "er", "den", "Win\u00b7ter", "fliehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und den Schnee verschwinden;", "tokens": ["Und", "den", "Schnee", "ver\u00b7schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Traurig schleicht er durch den Fr\u00fchling hin,", "tokens": ["Trau\u00b7rig", "schleicht", "er", "durch", "den", "Fr\u00fch\u00b7ling", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Kann ihn nicht empfinden.", "tokens": ["Kann", "ihn", "nicht", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Sieht mit kaltem Blick die junge Flur", "tokens": ["Sieht", "mit", "kal\u00b7tem", "Blick", "die", "jun\u00b7ge", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sich allm\u00e4hlich f\u00e4rben;", "tokens": ["Sich", "all\u00b7m\u00e4h\u00b7lich", "f\u00e4r\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Halberstorbne Bl\u00fcmchen pfl\u00fcckt er nur,", "tokens": ["Hal\u00b7ber\u00b7storb\u00b7ne", "Bl\u00fcm\u0