{"dta.poem.14189": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Frau Venus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was weckst du, Fr\u00fchling, mich von neuem wieder?", "tokens": ["Was", "weckst", "du", ",", "Fr\u00fch\u00b7ling", ",", "mich", "von", "neu\u00b7em", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PRF", "APPR", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df all' die alten W\u00fcnsche auferstehen,", "tokens": ["Da\u00df", "all'", "die", "al\u00b7ten", "W\u00fcn\u00b7sche", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geht \u00fcber's Land ein wunderbares Wehen;", "tokens": ["Geht", "\u00fc\u00b7ber's", "Land", "ein", "wun\u00b7der\u00b7ba\u00b7res", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das schauert mir so lieblich durch die Glieder.", "tokens": ["Das", "schau\u00b7ert", "mir", "so", "lieb\u00b7lich", "durch", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}