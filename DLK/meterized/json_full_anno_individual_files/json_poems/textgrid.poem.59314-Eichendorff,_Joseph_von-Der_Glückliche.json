{"textgrid.poem.59314": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fcckliche", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab ein Liebchen lieb recht von Herzen,", "tokens": ["Ich", "hab", "ein", "Lieb\u00b7chen", "lieb", "recht", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hellfrische Augen hat's wie zwei Kerzen,", "tokens": ["Hell\u00b7fri\u00b7sche", "Au\u00b7gen", "hat's", "wie", "zwei", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "KOKOM", "CARD", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und wo sie spielend streifen das Feld,", "tokens": ["Und", "wo", "sie", "spie\u00b7lend", "strei\u00b7fen", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ach, wie so lustig gl\u00e4nzet die Welt!", "tokens": ["Ach", ",", "wie", "so", "lus\u00b7tig", "gl\u00e4n\u00b7zet", "die", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Wie in der Waldnacht zwischen den Schl\u00fcften", "tokens": ["Wie", "in", "der", "Wald\u00b7nacht", "zwi\u00b7schen", "den", "Schl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Pl\u00f6tzlich die T\u00e4ler sonnig sich kl\u00fcften,", "tokens": ["Pl\u00f6tz\u00b7lich", "die", "T\u00e4\u00b7ler", "son\u00b7nig", "sich", "kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Funkeln die Str\u00f6me, rauscht himmelw\u00e4rts", "tokens": ["Fun\u00b7keln", "die", "Str\u00f6\u00b7me", ",", "rauscht", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADV"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.4": {"text": "Bl\u00fchende Wildnis \u2013 so ist mein Herz!", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Wild\u00b7nis", "\u2013", "so", "ist", "mein", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wie vom Gebirge ins Meer zu schauen,", "tokens": ["Wie", "vom", "Ge\u00b7bir\u00b7ge", "ins", "Meer", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wie wenn der Seefalk, hangend im Blauen,", "tokens": ["Wie", "wenn", "der", "See\u00b7falk", ",", "han\u00b7gend", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zuruft der d\u00e4mmernden Erd, wo sie blieb? \u2013", "tokens": ["Zu\u00b7ruft", "der", "d\u00e4m\u00b7mern\u00b7den", "Erd", ",", "wo", "sie", "blieb", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So unerme\u00dflich ist rechte Lieb!", "tokens": ["So", "un\u00b7er\u00b7me\u00df\u00b7lich", "ist", "rech\u00b7te", "Lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich hab ein Liebchen lieb recht von Herzen,", "tokens": ["Ich", "hab", "ein", "Lieb\u00b7chen", "lieb", "recht", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hellfrische Augen hat's wie zwei Kerzen,", "tokens": ["Hell\u00b7fri\u00b7sche", "Au\u00b7gen", "hat's", "wie", "zwei", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "KOKOM", "CARD", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und wo sie spielend streifen das Feld,", "tokens": ["Und", "wo", "sie", "spie\u00b7lend", "strei\u00b7fen", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ach, wie so lustig gl\u00e4nzet die Welt!", "tokens": ["Ach", ",", "wie", "so", "lus\u00b7tig", "gl\u00e4n\