{"dta.poem.20966": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxX.  \n Opitzens.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mich hat ein kleiner Ort der deutschen Welt gege-", "tokens": ["Mich", "hat", "ein", "klei\u00b7ner", "Ort", "der", "deut\u00b7schen", "Welt", "ge\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der wegen meiner wird mit Rom die Wette le-", "tokens": ["Der", "we\u00b7gen", "mei\u00b7ner", "wird", "mit", "Rom", "die", "Wet\u00b7te", "le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "VAFIN", "APPR", "NE", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ben.", "tokens": ["ben", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Ich suche nicht zu viel/ ich bin genug gepriesen/", "tokens": ["Ich", "su\u00b7che", "nicht", "zu", "viel", "/", "ich", "bin", "ge\u00b7nug", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKA", "PIS", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "