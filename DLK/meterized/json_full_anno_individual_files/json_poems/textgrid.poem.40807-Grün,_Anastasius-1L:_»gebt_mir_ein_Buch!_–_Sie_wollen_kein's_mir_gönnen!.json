{"textgrid.poem.40807": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbgebt mir ein Buch! \u2013 Sie wollen kein's mir g\u00f6nnen!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgebt mir ein Buch! \u2013 Sie wollen kein's mir g\u00f6nnen!", "tokens": ["\u00bb", "gebt", "mir", "ein", "Buch", "!", "\u2013", "Sie", "wol\u00b7len", "kein's", "mir", "g\u00f6n\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$.", "$(", "PPER", "VMFIN", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So mag mein Aug' im Buch des Himmels bl\u00e4ttern,", "tokens": ["So", "mag", "mein", "Aug'", "im", "Buch", "des", "Him\u00b7mels", "bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das dem Gefangnen sie nicht rauben k\u00f6nnen,", "tokens": ["Das", "dem", "Ge\u00b7fang\u00b7nen", "sie", "nicht", "rau\u00b7ben", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPER", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lesen, Herr, in deinen ew'gen Lettern!", "tokens": ["Und", "le\u00b7sen", ",", "Herr", ",", "in", "dei\u00b7nen", "ew'\u00b7gen", "Let\u00b7tern", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN