{"textgrid.poem.54368": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "9. Scherzgedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu dein und meiner Lust", "tokens": ["Zu", "dein", "und", "mei\u00b7ner", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gesteht die frohe Brust:", "tokens": ["Ge\u00b7steht", "die", "fro\u00b7he", "Brust", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es m\u00fcssen alle Minen", "tokens": ["Es", "m\u00fcs\u00b7sen", "al\u00b7le", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu unsrer Liebe dienen;", "tokens": ["Zu", "uns\u00b7rer", "Lie\u00b7be", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die kennen ich und du.", "tokens": ["Die", "ken\u00b7nen", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn siehst du mich nur an,", "tokens": ["Denn", "siehst", "du", "mich", "nur", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hast du genug gethan.", "tokens": ["Hast", "du", "ge\u00b7nug", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Auge wird dirs sagen:", "tokens": ["Das", "Au\u00b7ge", "wird", "dirs", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was andre m\u00fcssen fragen,", "tokens": ["Was", "and\u00b7re", "m\u00fcs\u00b7sen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das schliessen ich und du.", "tokens": ["Das", "schlies\u00b7sen", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und regt sich nur der Mund,", "tokens": ["Und", "regt", "sich", "nur", "der", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So thut das L\u00e4cheln kund,", "tokens": ["So", "thut", "das", "L\u00e4\u00b7cheln", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herze sey dein eigen.", "tokens": ["Das", "Her\u00b7ze", "sey", "dein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer liebt, und auch kann schweigen,", "tokens": ["Wer", "liebt", ",", "und", "auch", "kann", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KON", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der machts, wie ich und du.", "tokens": ["Der", "machts", ",", "wie", "ich", "und", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Man rede, was man will,", "tokens": ["Man", "re\u00b7de", ",", "was", "man", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich sitze M\u00e4uschen still;", "tokens": ["Ich", "sit\u00b7ze", "M\u00e4u\u00b7schen", "still", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja sehn wir andre scherzen,", "tokens": ["Ja", "sehn", "wir", "and\u00b7re", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gehts uns nicht zu Herzen;", "tokens": ["So", "gehts", "uns", "nicht", "zu", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das thun auch ich und du.", "tokens": ["Das", "thun", "auch", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn in der besten Welt,", "tokens": ["Denn", "in", "der", "bes\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die auf Ergetzung h\u00e4lt,", "tokens": ["Die", "auf", "Er\u00b7get\u00b7zung", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da gelten keine Grillen;", "tokens": ["Da", "gel\u00b7ten", "kei\u00b7ne", "Gril\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man lebt nach eignem Willen,", "tokens": ["Man", "lebt", "nach", "eig\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So als wie ich und du.", "tokens": ["So", "als", "wie", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PWAV", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Unschuld kn\u00fcpft das Band.", "tokens": ["Die", "Un\u00b7schuld", "kn\u00fcpft", "das", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nimmst du mich bey der Hand,", "tokens": ["Nimmst", "du", "mich", "bey", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So werd ich nicht erschrecken,", "tokens": ["So", "werd", "ich", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mich aus Furcht verstecken;", "tokens": ["Und", "mich", "aus", "Furcht", "ver\u00b7ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das hassen ich und du.", "tokens": ["Das", "has\u00b7sen", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich scheue nicht das Licht,", "tokens": ["Ich", "scheu\u00b7e", "nicht", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn dein Mund freundlich spricht.", "tokens": ["Wenn", "dein", "Mund", "freund\u00b7lich", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und willst du mich auch k\u00fcssen,", "tokens": ["Und", "willst", "du", "mich", "auch", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mags der Nachbar wissen;", "tokens": ["So", "mags", "der", "Nach\u00b7bar", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So haltens ich und du.", "tokens": ["So", "hal\u00b7tens", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir suchen keinen Wald,", "tokens": ["Wir", "su\u00b7chen", "kei\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu unserm Aufenthalt.", "tokens": ["Zu", "un\u00b7serm", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn unsre Nachtgespenster", "tokens": ["Wenn", "uns\u00b7re", "Nacht\u00b7ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich k\u00fcssen hinterm Fenster,", "tokens": ["Sich", "k\u00fcs\u00b7sen", "hin\u00b7term", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So merkens ich und du.", "tokens": ["So", "mer\u00b7kens", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn die und jene schimpft,", "tokens": ["Wenn", "die", "und", "je\u00b7ne", "schimpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das freye M\u00e4ulchen r\u00fcmpft,", "tokens": ["Das", "frey\u00b7e", "M\u00e4ul\u00b7chen", "r\u00fcmpft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denk ich, sie ist die rechte", "tokens": ["Denk", "ich", ",", "sie", "ist", "die", "rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey jedem L\u00f6ffelknechte.", "tokens": ["Bey", "je\u00b7dem", "L\u00f6f\u00b7fel\u00b7knech\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das weis l\u00e4ngst ich und du.", "tokens": ["Das", "weis", "l\u00e4ngst", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn jene Sch\u00e4fermagd", "tokens": ["Wenn", "je\u00b7ne", "Sch\u00e4\u00b7fer\u00b7magd"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sich mit der Keuschheit plagt,", "tokens": ["Sich", "mit", "der", "Keuschheit", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.3": {"text": "So gilt kein spassen, scherzen,", "tokens": ["So", "gilt", "kein", "spas\u00b7sen", ",", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie will im dunkeln herzen,", "tokens": ["Sie", "will", "im", "dun\u00b7keln", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht so wie ich und du.", "tokens": ["Nicht", "so", "wie", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Blondinchen weis und klug", "tokens": ["Blon\u00b7din\u00b7chen", "weis", "und", "klug"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf List, und auf Betrug,", "tokens": ["Auf", "List", ",", "und", "auf", "Be\u00b7trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erwartet mit Verlangen", "tokens": ["Er\u00b7war\u00b7tet", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den kleinen Mann zu fangen;", "tokens": ["Den", "klei\u00b7nen", "Mann", "zu", "fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da scherzen ich und du.", "tokens": ["Da", "scher\u00b7zen", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was macht der M\u00e4nnerfeind,", "tokens": ["Was", "macht", "der", "M\u00e4n\u00b7ner\u00b7feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Weiber bester Freund?", "tokens": ["Der", "Wei\u00b7ber", "bes\u00b7ter", "Freund", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort unter jener Eiche", "tokens": ["Dort", "un\u00b7ter", "je\u00b7ner", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spielt er verliebte Streiche?", "tokens": ["Spielt", "er", "ver\u00b7lieb\u00b7te", "Strei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da lachen ich und du.", "tokens": ["Da", "la\u00b7chen", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wer auf die Liebe flucht,", "tokens": ["Wer", "auf", "die", "Lie\u00b7be", "flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und doch die Winkel sucht,", "tokens": ["Und", "doch", "die", "Win\u00b7kel", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem wollen wir vermelden,", "tokens": ["Dem", "wol\u00b7len", "wir", "ver\u00b7mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dergleichen schlechte Helden", "tokens": ["Derg\u00b7lei\u00b7chen", "schlech\u00b7te", "Hel\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verspotten ich und du.", "tokens": ["Ver\u00b7spot\u00b7ten", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wir fliehen vor der Zunft;", "tokens": ["Wir", "flie\u00b7hen", "vor", "der", "Zunft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und lieben mit Vernunft;", "tokens": ["Und", "lie\u00b7ben", "mit", "Ver\u00b7nunft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wort, ein Mann ein Herze,", "tokens": ["Ein", "Wort", ",", "ein", "Mann", "ein", "Her\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Ernst und auch im Scherze:", "tokens": ["Im", "Ernst", "und", "auch", "im", "Scher\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So denken ich und du.", "tokens": ["So", "den\u00b7ken", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Eur Lieben ist gemein,", "tokens": ["Eur", "Lie\u00b7ben", "ist", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So wollen wir nicht seyn.", "tokens": ["So", "wol\u00b7len", "wir", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer unsre Sitten tadelt,", "tokens": ["Wer", "uns\u00b7re", "Sit\u00b7ten", "ta\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da uns die Tugend adelt,", "tokens": ["Da", "uns", "die", "Tu\u00b7gend", "a\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schimpft sich; nicht ich und du.", "tokens": ["Schimpft", "sich", ";", "nicht", "ich", "und", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$.", "PTKNEG", "PPER", "KON", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.16": {"line.1": {"text": "Wer lacht bey dem Entschlu\u00df,", "tokens": ["Wer", "lacht", "bey", "dem", "Ent\u00b7schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der thut sich selbst Verdru\u00df,", "tokens": ["Der", "thut", "sich", "selbst", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn sollten wirs auch h\u00f6ren,", "tokens": ["Denn", "soll\u00b7ten", "wirs", "auch", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kanns unsre Lust nicht st\u00f6ren;", "tokens": ["Kanns", "uns\u00b7re", "Lust", "nicht", "st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bleiben ich und du.", "tokens": ["So", "blei\u00b7ben", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Zu dein und meiner Lust", "tokens": ["Zu", "dein", "und", "mei\u00b7ner", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gesteht die frohe Brust:", "tokens": ["Ge\u00b7steht", "die", "fro\u00b7he", "Brust", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es m\u00fcssen alle Minen", "tokens": ["Es", "m\u00fcs\u00b7sen", "al\u00b7le", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu unsrer Liebe dienen;", "tokens": ["Zu", "uns\u00b7rer", "Lie\u00b7be", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die kennen ich und du.", "tokens": ["Die", "ken\u00b7nen", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Denn siehst du mich nur an,", "tokens": ["Denn", "siehst", "du", "mich", "nur", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hast du genug gethan.", "tokens": ["Hast", "du", "ge\u00b7nug", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Auge wird dirs sagen:", "tokens": ["Das", "Au\u00b7ge", "wird", "dirs", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was andre m\u00fcssen fragen,", "tokens": ["Was", "and\u00b7re", "m\u00fcs\u00b7sen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das schliessen ich und du.", "tokens": ["Das", "schlies\u00b7sen", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Und regt sich nur der Mund,", "tokens": ["Und", "regt", "sich", "nur", "der", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So thut das L\u00e4cheln kund,", "tokens": ["So", "thut", "das", "L\u00e4\u00b7cheln", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herze sey dein eigen.", "tokens": ["Das", "Her\u00b7ze", "sey", "dein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer liebt, und auch kann schweigen,", "tokens": ["Wer", "liebt", ",", "und", "auch", "kann", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KON", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der machts, wie ich und du.", "tokens": ["Der", "machts", ",", "wie", "ich", "und", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Man rede, was man will,", "tokens": ["Man", "re\u00b7de", ",", "was", "man", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich sitze M\u00e4uschen still;", "tokens": ["Ich", "sit\u00b7ze", "M\u00e4u\u00b7schen", "still", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja sehn wir andre scherzen,", "tokens": ["Ja", "sehn", "wir", "and\u00b7re", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gehts uns nicht zu Herzen;", "tokens": ["So", "gehts", "uns", "nicht", "zu", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das thun auch ich und du.", "tokens": ["Das", "thun", "auch", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Denn in der besten Welt,", "tokens": ["Denn", "in", "der", "bes\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die auf Ergetzung h\u00e4lt,", "tokens": ["Die", "auf", "Er\u00b7get\u00b7zung", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da gelten keine Grillen;", "tokens": ["Da", "gel\u00b7ten", "kei\u00b7ne", "Gril\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man lebt nach eignem Willen,", "tokens": ["Man", "lebt", "nach", "eig\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So als wie ich und du.", "tokens": ["So", "als", "wie", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PWAV", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Die Unschuld kn\u00fcpft das Band.", "tokens": ["Die", "Un\u00b7schuld", "kn\u00fcpft", "das", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nimmst du mich bey der Hand,", "tokens": ["Nimmst", "du", "mich", "bey", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So werd ich nicht erschrecken,", "tokens": ["So", "werd", "ich", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mich aus Furcht verstecken;", "tokens": ["Und", "mich", "aus", "Furcht", "ver\u00b7ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das hassen ich und du.", "tokens": ["Das", "has\u00b7sen", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Ich scheue nicht das Licht,", "tokens": ["Ich", "scheu\u00b7e", "nicht", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn dein Mund freundlich spricht.", "tokens": ["Wenn", "dein", "Mund", "freund\u00b7lich", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und willst du mich auch k\u00fcssen,", "tokens": ["Und", "willst", "du", "mich", "auch", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mags der Nachbar wissen;", "tokens": ["So", "mags", "der", "Nach\u00b7bar", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So haltens ich und du.", "tokens": ["So", "hal\u00b7tens", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Wir suchen keinen Wald,", "tokens": ["Wir", "su\u00b7chen", "kei\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu unserm Aufenthalt.", "tokens": ["Zu", "un\u00b7serm", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn unsre Nachtgespenster", "tokens": ["Wenn", "uns\u00b7re", "Nacht\u00b7ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich k\u00fcssen hinterm Fenster,", "tokens": ["Sich", "k\u00fcs\u00b7sen", "hin\u00b7term", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So merkens ich und du.", "tokens": ["So", "mer\u00b7kens", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Wenn die und jene schimpft,", "tokens": ["Wenn", "die", "und", "je\u00b7ne", "schimpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das freye M\u00e4ulchen r\u00fcmpft,", "tokens": ["Das", "frey\u00b7e", "M\u00e4ul\u00b7chen", "r\u00fcmpft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denk ich, sie ist die rechte", "tokens": ["Denk", "ich", ",", "sie", "ist", "die", "rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey jedem L\u00f6ffelknechte.", "tokens": ["Bey", "je\u00b7dem", "L\u00f6f\u00b7fel\u00b7knech\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das weis l\u00e4ngst ich und du.", "tokens": ["Das", "weis", "l\u00e4ngst", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Wenn jene Sch\u00e4fermagd", "tokens": ["Wenn", "je\u00b7ne", "Sch\u00e4\u00b7fer\u00b7magd"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sich mit der Keuschheit plagt,", "tokens": ["Sich", "mit", "der", "Keuschheit", "plagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.3": {"text": "So gilt kein spassen, scherzen,", "tokens": ["So", "gilt", "kein", "spas\u00b7sen", ",", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie will im dunkeln herzen,", "tokens": ["Sie", "will", "im", "dun\u00b7keln", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht so wie ich und du.", "tokens": ["Nicht", "so", "wie", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Blondinchen weis und klug", "tokens": ["Blon\u00b7din\u00b7chen", "weis", "und", "klug"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf List, und auf Betrug,", "tokens": ["Auf", "List", ",", "und", "auf", "Be\u00b7trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erwartet mit Verlangen", "tokens": ["Er\u00b7war\u00b7tet", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den kleinen Mann zu fangen;", "tokens": ["Den", "klei\u00b7nen", "Mann", "zu", "fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."