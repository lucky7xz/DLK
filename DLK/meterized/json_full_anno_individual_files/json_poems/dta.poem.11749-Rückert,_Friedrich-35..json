{"dta.poem.11749": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du unterscheidest hier Vernunft und dort Verstand,", "tokens": ["Du", "un\u00b7ter\u00b7schei\u00b7dest", "hier", "Ver\u00b7nunft", "und", "dort", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zwischen beiden denkst du eine Scheidewand.", "tokens": ["Und", "zwi\u00b7schen", "bei\u00b7den", "denkst", "du", "ei\u00b7ne", "Schei\u00b7de\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch ohne Ansto\u00df an den nur gedachten Schranken,", "tokens": ["Doch", "oh\u00b7ne", "An\u00b7sto\u00df", "an", "den", "nur", "ge\u00b7dach\u00b7ten", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Her und hin\u00fcber gehn die spielenden Gedanken.", "tokens": ["Her", "und", "hin\u00b7\u00fc\u00b7ber", "gehn", "die", "spie\u00b7len\u00b7den", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So unterscheidest du den Geist auch vom Gem\u00fcte,", "tokens": ["So", "un\u00b7ter\u00b7schei\u00b7dest", "du", "den", "Geist", "auch", "vom", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie am Basilikum vom duft'gen Blatt die Bl\u00fcte.", "tokens": ["Wie", "am", "Ba\u00b7si\u00b7li\u00b7kum", "vom", "duft'\u00b7gen", "Blatt", "die", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "So unterscheidest du die Seele von dem Leib,", "tokens": ["So", "un\u00b7ter\u00b7schei\u00b7dest", "du", "die", "See\u00b7le", "von", "dem", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als seyen beide so getrennt wie Mann und Weib.", "tokens": ["Als", "se\u00b7yen", "bei\u00b7de", "so", "ge\u00b7trennt", "wie", "Mann", "und", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wie nicht Mann und Weib getrennt sind im Erkennen,", "tokens": ["Doch", "wie", "nicht", "Mann", "und", "Weib", "ge\u00b7trennt", "sind", "im", "Er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKNEG", "NN", "KON", "NN", "VVPP", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kann auch Seel' und Leib nicht die Erkenntnis trennen.", "tokens": ["So", "kann", "auch", "Seel'", "und", "Leib", "nicht", "die", "Er\u00b7kennt\u00b7nis", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "NN", "KON", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und das nur macht dein Ich, da\u00df ungetrennt sie sind,", "tokens": ["Und", "das", "nur", "macht", "dein", "Ich", ",", "da\u00df", "un\u00b7ge\u00b7trennt", "sie", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VVFIN", "PPOSAT", "PPER", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ungetrennt sich Mann und Weib erkennt im Kind.", "tokens": ["Wie", "un\u00b7ge\u00b7trennt", "sich", "Mann", "und", "Weib", "er\u00b7kennt", "im", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So unterscheidest du den Gott von der Natur,", "tokens": ["So", "un\u00b7ter\u00b7schei\u00b7dest", "du", "den", "Gott", "von", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von den beiden Dich, und Eins die drei sind nur.", "tokens": ["Und", "von", "den", "bei\u00b7den", "Dich", ",", "und", "Eins", "die", "drei", "sind", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIAT", "PPER", "$,", "KON", "NN", "ART", "CARD", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Den Vater magst du ihn, und sie die Mutter nennen,", "tokens": ["Den", "Va\u00b7ter", "magst", "du", "ihn", ",", "und", "sie", "die", "Mut\u00b7ter", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "$,", "KON", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Kind, doch ungetrennt von beiden dich erkennen.", "tokens": ["O", "Kind", ",", "doch", "un\u00b7ge\u00b7trennt", "von", "bei\u00b7den", "dich", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADJD", "APPR", "PIAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "In deiner Liebe wirst du sie als Eins erkennen,", "tokens": ["In", "dei\u00b7ner", "Lie\u00b7be", "wirst", "du", "sie", "als", "Eins", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Liebesnamen unterscheiden und nicht trennen.", "tokens": ["Mit", "Lie\u00b7bes\u00b7na\u00b7men", "un\u00b7ter\u00b7schei\u00b7den", "und", "nicht", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Nie la\u00df dir dies Gef\u00fchl, es sei dein heil'ger Glauben,", "tokens": ["Nie", "la\u00df", "dir", "dies", "Ge\u00b7f\u00fchl", ",", "es", "sei", "dein", "heil'\u00b7ger", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PDS", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Unterschiedenem und Ungeschiednem rauben.", "tokens": ["Von", "Un\u00b7ter\u00b7schie\u00b7de\u00b7nem", "und", "Un\u00b7ge\u00b7schied\u00b7nem", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}