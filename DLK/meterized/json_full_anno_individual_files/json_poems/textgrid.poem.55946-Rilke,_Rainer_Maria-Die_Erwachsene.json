{"textgrid.poem.55946": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Erwachsene", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das alles stand auf ihr und war die Welt", "tokens": ["Das", "al\u00b7les", "stand", "auf", "ihr", "und", "war", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "PPER", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und stand auf ihr mit allem, Angst und Gnade,", "tokens": ["und", "stand", "auf", "ihr", "mit", "al\u00b7lem", ",", "Angst", "und", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PIS", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wie B\u00e4ume stehen, wachsend und gerade,", "tokens": ["wie", "B\u00e4u\u00b7me", "ste\u00b7hen", ",", "wach\u00b7send", "und", "ge\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ganz Bild und bildlos wie die Bundeslade", "tokens": ["ganz", "Bild", "und", "bild\u00b7los", "wie", "die", "Bun\u00b7des\u00b7la\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und feierlich, wie auf ein Volk gestellt.", "tokens": ["und", "fei\u00b7er\u00b7lich", ",", "wie", "auf", "ein", "Volk", "ge\u00b7stellt", "."], "token_info": ["word", "word", "punc