{"textgrid.poem.65641": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Einschiffung der Athener", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freies Element der Wogen, sei der Freiheit Kindern hold!", "tokens": ["Frei\u00b7es", "E\u00b7le\u00b7ment", "der", "Wo\u00b7gen", ",", "sei", "der", "Frei\u00b7heit", "Kin\u00b7dern", "hold", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Willst hinab du Opfer schlingen, schlinge Sklaven, schlinge Gold!", "tokens": ["Willst", "hin\u00b7ab", "du", "Op\u00b7fer", "schlin\u00b7gen", ",", "schlin\u00b7ge", "Skla\u00b7ven", ",", "schlin\u00b7ge", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "NN", "VVINF", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nicht des Wuchers D\u00e4mon treibt uns in das schwanke Bretterhaus,", "tokens": ["Nicht", "des", "Wu\u00b7chers", "D\u00e4\u00b7mon", "treibt", "uns", "in", "das", "schwan\u00b7ke", "Bret\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NE", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nicht nach Menschenraube schiffen in die Fluthen wir hinaus;", "tokens": ["Nicht", "nach", "Men\u00b7schen\u00b7rau\u00b7be", "schif\u00b7fen", "in", "die", "Flut\u00b7hen", "wir", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Nach der Freiheit Hafen haben wir die Segel ausgespannt \u2013", "tokens": ["Nach", "der", "Frei\u00b7heit", "Ha\u00b7fen", "ha\u00b7ben", "wir", "die", "Se\u00b7gel", "aus\u00b7ge\u00b7spannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Heil uns, wenn dereinst wir rufen: Land! Land! Freies Griechenland!", "tokens": ["Heil", "uns", ",", "wenn", "de\u00b7reinst", "wir", "ru\u00b7fen", ":", "Land", "!", "Land", "!", "Frei\u00b7es", "Grie\u00b7chen\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$.", "NN", "$.", "NN", "$.", "NE", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Was uns dr\u00fcckte, was uns engte, lie\u00dfen wir am Strande stehn,", "tokens": ["Was", "uns", "dr\u00fcck\u00b7te", ",", "was", "uns", "eng\u00b7te", ",", "lie\u00b7\u00dfen", "wir", "am", "Stran\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Nicht nach St\u00e4dten, nicht nach Burgen wollen wir zur\u00fccke sehn;", "tokens": ["Nicht", "nach", "St\u00e4d\u00b7ten", ",", "nicht", "nach", "Bur\u00b7gen", "wol\u00b7len", "wir", "zu\u00b7r\u00fc\u00b7cke", "sehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Vorw\u00e4rts schweifen unsre Blicke in die weite See hinaus,", "tokens": ["Vor\u00b7w\u00e4rts", "schwei\u00b7fen", "uns\u00b7re", "Bli\u00b7cke", "in", "die", "wei\u00b7te", "See", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "APZR", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und sie gr\u00fc\u00dft der Freiheit Flagge hoch mit donnerndem Gebraus.", "tokens": ["Und", "sie", "gr\u00fc\u00dft", "der", "Frei\u00b7heit", "Flag\u00b7ge", "hoch", "mit", "don\u00b7nern\u00b7dem", "Ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Freies Element der Wogen, unbegrenzte Meeresfluth!", "tokens": ["Frei\u00b7es", "E\u00b7le\u00b7ment", "der", "Wo\u00b7gen", ",", "un\u00b7be\u00b7grenz\u00b7te", "Mee\u00b7res\u00b7fluth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Mag der Kr\u00e4mer falsch dich nennen, zitternd f\u00fcr sein eitles Gut \u2013", "tokens": ["Mag", "der", "Kr\u00e4\u00b7mer", "falsch", "dich", "nen\u00b7nen", ",", "zit\u00b7ternd", "f\u00fcr", "sein", "eit\u00b7les", "Gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "PPER", "VVINF", "$,", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Hellas kennt aus alten Tagen deine feste Treue noch:", "tokens": ["Hel\u00b7las", "kennt", "aus", "al\u00b7ten", "Ta\u00b7gen", "dei\u00b7ne", "fes\u00b7te", "Treu\u00b7e", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Als Athen, die Burg der Freiheit, unterlag dem Sklavenjoch,", "tokens": ["Als", "A\u00b7then", ",", "die", "Burg", "der", "Frei\u00b7heit", ",", "un\u00b7ter\u00b7lag", "dem", "Skla\u00b7ven\u00b7joch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Als die Felsenw\u00e4lle brachen, als die Th\u00fcrme sanken ein,", "tokens": ["Als", "die", "Fel\u00b7sen\u00b7w\u00e4l\u00b7le", "bra\u00b7chen", ",", "als", "die", "Th\u00fcr\u00b7me", "san\u00b7ken", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Da, da wolltest ", "tokens": ["Da", ",", "da", "woll\u00b7test"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "NE"], "meter": "--+-", "measure": "anapaest.init"}, "line.17": {"text": "Und empor auf deinem R\u00fccken ein Athen auf Brettern stieg,", "tokens": ["Und", "em\u00b7por", "auf", "dei\u00b7nem", "R\u00fc\u00b7cken", "ein", "A\u00b7then", "auf", "Bret\u00b7tern", "stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "APPR", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und du trugst es fort zum Kampfe, und du trugst es hin zum Sieg.", "tokens": ["Und", "du", "trugst", "es", "fort", "zum", "Kamp\u00b7fe", ",", "und", "du", "trugst", "es", "hin", "zum", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Freies Element der Wogen, sei den sp\u00e4ten Enkeln treu,", "tokens": ["Frei\u00b7es", "E\u00b7le\u00b7ment", "der", "Wo\u00b7gen", ",", "sei", "den", "sp\u00e4\u00b7ten", "En\u00b7keln", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Wie du es den V\u00e4tern warest! Sieh, die alte Zeit wird neu!", "tokens": ["Wie", "du", "es", "den", "V\u00e4\u00b7tern", "wa\u00b7rest", "!", "Sieh", ",", "die", "al\u00b7te", "Zeit", "wird", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ART", "NN", "VAFIN", "$.", "NE", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Sieh, Athen, die Burg der Freiheit, ist in der Barbaren Hand!", "tokens": ["Sieh", ",", "A\u00b7then", ",", "die", "Burg", "der", "Frei\u00b7heit", ",", "ist", "in", "der", "Bar\u00b7ba\u00b7ren", "Hand", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ART", "NN", "ART", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Sieh, in deinen Fluthen spiegelt roth sich ihrer Tempel Brand!", "tokens": ["Sieh", ",", "in", "dei\u00b7nen", "Flut\u00b7hen", "spie\u00b7gelt", "roth", "sich", "ih\u00b7rer", "Tem\u00b7pel", "Brand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "PRF", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Nehmt uns ein, Ihr Brettermauern! Hebt vom Ufer euch geschwind!", "tokens": ["Nehmt", "uns", "ein", ",", "Ihr", "Bret\u00b7ter\u00b7mau\u00b7ern", "!", "Hebt", "vom", "U\u00b7fer", "euch", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "VVFIN", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Auf, die Segel! Nach der Insel Salamis weht frischer Wind.", "tokens": ["Auf", ",", "die", "Se\u00b7gel", "!", "Nach", "der", "In\u00b7sel", "Sa\u00b7la\u00b7mis", "weht", "fri\u00b7scher", "Wind", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ART", "N