{"textgrid.poem.25725": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dem Hain', wo vormals Harfen klangen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Hain', wo vormals Harfen klangen,", "tokens": ["In", "dem", "Hain'", ",", "wo", "vor\u00b7mals", "Har\u00b7fen", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und im Mondenscheine Barden sangen,", "tokens": ["Und", "im", "Mon\u00b7den\u00b7schei\u00b7ne", "Bar\u00b7den", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sitz' ich auf dem grauen Opfersteine", "tokens": ["Sitz'", "ich", "auf", "dem", "grau\u00b7en", "Op\u00b7fer\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einsam, und weine.", "tokens": ["Ein\u00b7sam", ",", "und", "wei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ihre Lieder, die auf allen Zungen", "tokens": ["Ih\u00b7re", "Lie\u00b7der", ",", "die", "auf", "al\u00b7len", "Zun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Damals