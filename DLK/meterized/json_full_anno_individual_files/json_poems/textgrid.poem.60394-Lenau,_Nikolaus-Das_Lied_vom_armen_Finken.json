{"textgrid.poem.60394": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom armen Finken", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Finkler ist ein Schlauer;", "tokens": ["Der", "Fink\u00b7ler", "ist", "ein", "Schlau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann d\u00fcrr die Bl\u00e4tter sinken,", "tokens": ["Wann", "d\u00fcrr", "die", "Bl\u00e4t\u00b7ter", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann sperrt er in den Bauer", "tokens": ["Dann", "sperrt", "er", "in", "den", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den eingefangnen Finken.", "tokens": ["Den", "ein\u00b7ge\u00b7fang\u00b7nen", "Fin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er macht den Finken kirre,", "tokens": ["Er", "macht", "den", "Fin\u00b7ken", "kir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er zu finden lerne", "tokens": ["Da\u00df", "er", "zu", "fin\u00b7den", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Wasser im Geschirre", "tokens": ["Das", "Was\u00b7ser", "im", "Ge\u00b7schir\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und seines Futters Kerne.", "tokens": ["Und", "sei\u00b7nes", "Fut\u00b7ters", "Ker\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wei\u00df das arme Finklein", "tokens": ["Und", "wei\u00df", "das", "ar\u00b7me", "Fin\u00b7klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seinen Sprossenw\u00e4nden", "tokens": ["In", "sei\u00b7nen", "Spros\u00b7sen\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bescheid in jedem Winklein,", "tokens": ["Be\u00b7scheid", "in", "je\u00b7dem", "Win\u00b7klein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So geht es an ein Blenden.", "tokens": ["So", "geht", "es", "an", "ein", "Blen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der V\u00f6gelpotentate", "tokens": ["Der", "V\u00f6\u00b7gel\u00b7po\u00b7ten\u00b7ta\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Brennt nun dem armen Tropfe", "tokens": ["Brennt", "nun", "dem", "ar\u00b7men", "Trop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit glutgehitztem Drahte", "tokens": ["Mit", "glut\u00b7ge\u00b7hitz\u00b7tem", "Drah\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die \u00c4uglein aus dem Kopfe.", "tokens": ["Die", "\u00c4ug\u00b7lein", "aus", "dem", "Kop\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und fragst du nach dem Witze", "tokens": ["Und", "fragst", "du", "nach", "dem", "Wit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von solchem schn\u00f6den Werke?", "tokens": ["Von", "sol\u00b7chem", "schn\u00f6\u00b7den", "Wer\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ei, da\u00df im Kerkersitze", "tokens": ["Ei", ",", "da\u00df", "im", "Ker\u00b7ker\u00b7sit\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Fink den Lenz nicht merke.", "tokens": ["Der", "Fink", "den", "Lenz", "nicht", "mer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Der Vogler kann nicht brauchen", "tokens": ["Der", "Vog\u00b7ler", "kann", "nicht", "brau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Finken Schlag im M\u00e4rzen.", "tokens": ["Des", "Fin\u00b7ken", "Schlag", "im", "M\u00e4r\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Lust und Lied ihm tauchen", "tokens": ["Da\u00df", "Lust", "und", "Lied", "ihm", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus lenzgewecktem Herzen.", "tokens": ["Aus", "lenz\u00b7ge\u00b7weck\u00b7tem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da sitzt er nun gefangen", "tokens": ["Da", "sitzt", "er", "nun", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im traurigen Verstecke,", "tokens": ["Im", "trau\u00b7ri\u00b7gen", "Ver\u00b7ste\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gar flei\u00dfig \u00fcberhangen,", "tokens": ["Gar", "flei\u00b7\u00dfig", "\u00fc\u00b7berh\u00b7an\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihn kein L\u00fcftlein wecke.", "tokens": ["Da\u00df", "ihn", "kein", "L\u00fcft\u00b7lein", "we\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und sollte seine Seele,", "tokens": ["Und", "soll\u00b7te", "sei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die doch den Fr\u00fchling sp\u00fcret,", "tokens": ["Die", "doch", "den", "Fr\u00fch\u00b7ling", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich wagen auf die Kehle,", "tokens": ["Sich", "wa\u00b7gen", "auf", "die", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sich der S\u00e4nger r\u00fchret:", "tokens": ["Wenn", "sich", "der", "S\u00e4n\u00b7ger", "r\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Vertreibt ihm bald sein Dr\u00e4nger", "tokens": ["Ver\u00b7treibt", "ihm", "bald", "sein", "Dr\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die frohen Lenzgedanken,", "tokens": ["Die", "fro\u00b7hen", "Lenz\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er spritzt dem kecken S\u00e4nger", "tokens": ["Er", "spritzt", "dem", "ke\u00b7cken", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kalt Wasser in die Flanken.", "tokens": ["Kalt", "Was\u00b7ser", "in", "die", "Flan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und l\u00e4\u00dft sich nicht bezwingen", "tokens": ["Und", "l\u00e4\u00dft", "sich", "nicht", "be\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Fink mit kalten B\u00e4dern,", "tokens": ["Der", "Fink", "mit", "kal\u00b7ten", "B\u00e4\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will selbst der Nasse singen,", "tokens": ["Will", "selbst", "der", "Nas\u00b7se", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So rupft man ein paar Federn.", "tokens": ["So", "rupft", "man", "ein", "paar", "Fe\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Er soll sein lautes Schlagen", "tokens": ["Er", "soll", "sein", "lau\u00b7tes", "Schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und seinen Fr\u00fchlingsglauben", "tokens": ["Und", "sei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7glau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis in den Herbst vertagen,", "tokens": ["Bis", "in", "den", "Herbst", "ver\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo sich die Hain' entlauben.", "tokens": ["Wo", "sich", "die", "Hain'", "ent\u00b7lau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann wird er singen d\u00fcrfen", "tokens": ["Dann", "wird", "er", "sin\u00b7gen", "d\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und seine Fl\u00fcgel dehnen,", "tokens": ["Und", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Waldesl\u00fcfte schl\u00fcrfen", "tokens": ["Die", "Wal\u00b7des\u00b7l\u00fcf\u00b7te", "schl\u00fcr\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sich im Fr\u00fchling w\u00e4hnen.", "tokens": ["Und", "sich", "im", "Fr\u00fch\u00b7ling", "w\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Dann auf dem Vogelherde", "tokens": ["Dann", "auf", "dem", "Vo\u00b7gel\u00b7her\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beginnt der Narr zu preisen", "tokens": ["Be\u00b7ginnt", "der", "Narr", "zu", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die freudenwelke Erde", "tokens": ["Die", "freu\u00b7den\u00b7wel\u00b7ke", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In frohen Fr\u00fchlingsweisen.", "tokens": ["In", "fro\u00b7hen", "Fr\u00fch\u00b7lings\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dann h\u00f6ren sein Frohlocken", "tokens": ["Dann", "h\u00f6\u00b7ren", "sein", "Froh\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und seine Fr\u00fchlingsl\u00fcge,", "tokens": ["Und", "sei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7l\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verwirrt und s\u00fc\u00df erschrocken,", "tokens": ["Ver\u00b7wirrt", "und", "s\u00fc\u00df", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der V\u00f6gel Wanderz\u00fcge.", "tokens": ["Der", "V\u00f6\u00b7gel", "Wan\u00b7der\u00b7z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und voller Lenzverlangen,", "tokens": ["Und", "vol\u00b7ler", "Lenz\u00b7ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Finkler zum Ergetzen,", "tokens": ["Dem", "Fink\u00b7ler", "zum", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Fallen sie ein und fangen", "tokens": ["Fal\u00b7len", "sie", "ein", "und", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sich auch in seinen Netzen. \u2013", "tokens": ["Sich", "auch", "in", "sei\u00b7nen", "Net\u00b7zen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Nun ist es Lenz, nun sitzet", "tokens": ["Nun", "ist", "es", "Lenz", ",", "nun", "sit\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Fink in seiner Steige,", "tokens": ["Der", "Fink", "in", "sei\u00b7ner", "Stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Vogler rupft und spritzet,", "tokens": ["Der", "Vog\u00b7ler", "rupft", "und", "sprit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er den Lenz verschweige.", "tokens": ["Da\u00df", "er", "den", "Lenz", "ver\