{"textgrid.poem.57635": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Nach einer alten Melodie", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gl\u00f6ckchen h\u00f6r' ich l\u00e4uten,", "tokens": ["Ein", "Gl\u00f6ck\u00b7chen", "h\u00f6r'", "ich", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es klingt so feierlich,", "tokens": ["Es", "klingt", "so", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es l\u00e4utet das S\u00fcndergl\u00f6ckchen,", "tokens": ["Es", "l\u00e4u\u00b7tet", "das", "S\u00fcn\u00b7der\u00b7gl\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du armes Kind, um dich.", "tokens": ["Du", "ar\u00b7mes", "Kind", ",", "um", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "KOUI", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Henker im blutroten Mantel,", "tokens": ["Der", "Hen\u00b7ker", "im", "blut\u00b7ro\u00b7ten", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der geht so still einher,", "tokens": ["Der", "geht", "so", "still", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat doch schon oft geschwungen", "tokens": ["Und", "hat", "doch", "schon", "oft", "ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Richtbeil, blank und schwer.", "tokens": ["Das", "Richt\u00b7beil", ",", "blank", "und", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Beil, das seh ich blinken,", "tokens": ["Das", "Beil", ",", "das", "seh", "ich", "blin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Blut spritzt rot in die H\u00f6h,", "tokens": ["Das", "Blut", "spritzt", "rot", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er hebt das Haupt bei den Locken,", "tokens": ["Er", "hebt", "das", "Haupt", "bei", "den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Damit es ein jeder auch seh.", "tokens": ["Da\u00b7mit", "es", "ein", "je\u00b7der", "auch", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measu