{"dta.poem.23667": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n h\u00f6rt sich's an, wenn Sch\u00f6n beim Schillerfest", "tokens": ["Sch\u00f6n", "h\u00f6rt", "sich's", "an", ",", "wenn", "Sch\u00f6n", "beim", "Schil\u00b7ler\u00b7fest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "PTKVZ", "$,", "KOUS", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die sch\u00f6nen Frauen leben l\u00e4sst.", "tokens": ["Die", "sch\u00f6\u00b7nen", "Frau\u00b7en", "le\u00b7ben", "l\u00e4sst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00f6n ging mit sch\u00f6nen Frauen schon voran,", "tokens": ["Sch\u00f6n", "ging", "mit", "sch\u00f6\u00b7nen", "Frau\u00b7en", "schon", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sch\u00f6n folgt auf sch\u00f6ne Fraun der Mann,", "tokens": ["Sch\u00f6n", "folgt", "auf", "sch\u00f6\u00b7ne", "Fraun", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht weil er war der erst' im Paradies,", "tokens": ["Nicht", "weil", "er", "war", "der", "er\u00b7st'", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "VAFIN", "ART", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sondern weil er ist der erste ohnedies.", "tokens": ["Son\u00b7dern", "weil", "er", "ist", "der", "ers\u00b7te", "oh\u00b7ne\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Ich meine unter Mann nicht allerlei Leute,", "tokens": ["Ich", "mei\u00b7ne", "un\u00b7ter", "Mann", "nicht", "al\u00b7ler\u00b7lei", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Die jeder Tag uns bringt, das Morgen und Heute.", "tokens": ["Die", "je\u00b7der", "Tag", "uns", "bringt", ",", "das", "Mor\u00b7gen", "und", "Heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wer m\u00e4nnlich strebt und wagt, steht und nicht f\u00e4llt,", "tokens": ["Wer", "m\u00e4nn\u00b7lich", "strebt", "und", "wagt", ",", "steht", "und", "nicht", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und m\u00e4nnlich lebt, unverzagt geht durch die Welt,", "tokens": ["Und", "m\u00e4nn\u00b7lich", "lebt", ",", "un\u00b7ver\u00b7zagt", "geht", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und m\u00e4nnlich sich m\u00fcht f\u00fcr's Gut' und Rechte,", "tokens": ["Und", "m\u00e4nn\u00b7lich", "sich", "m\u00fcht", "f\u00fcr's", "Gut'", "und", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "ADJD", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und m\u00e4nnlich ergl\u00fcht mit Muth wider das Schlechte,", "tokens": ["Und", "m\u00e4nn\u00b7lich", "er\u00b7gl\u00fcht", "mit", "Muth", "wi\u00b7der", "das", "Schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und m\u00e4nnlich auf eigenen Beinen steht,", "tokens": ["Und", "m\u00e4nn\u00b7lich", "auf", "ei\u00b7ge\u00b7nen", "Bei\u00b7nen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Und sich nicht nach jedem Wetter, Glauben und Meinen dreht,", "tokens": ["Und", "sich", "nicht", "nach", "je\u00b7dem", "Wet\u00b7ter", ",", "Glau\u00b7ben", "und", "Mei\u00b7nen", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "APPR", "PIAT", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Und m\u00e4nnlich, mit Geduld, bieder ertr\u00e4gt,", "tokens": ["Und", "m\u00e4nn\u00b7lich", ",", "mit", "Ge\u00b7duld", ",", "bie\u00b7der", "er\u00b7tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und m\u00e4nnlich ohne Schuld nieder sich legt,", "tokens": ["Und", "m\u00e4nn\u00b7lich", "oh\u00b7ne", "Schuld", "nie\u00b7der", "sich", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und frei noch ist in Gef\u00e4ngni\u00df,", "tokens": ["Und", "frei", "noch", "ist", "in", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Und froh noch ist in Bedr\u00e4ngni\u00df,", "tokens": ["Und", "froh", "noch", "ist", "in", "Be\u00b7dr\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Der wei\u00df was er will, und will was er kann,", "tokens": ["Der", "wei\u00df", "was", "er", "will", ",", "und", "will", "was", "er", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PWS", "PPER", "VMFIN", "$,", "KON", "VMFIN", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Ihr M\u00e4nner, sto\u00dfet an!", "tokens": ["Ihr", "M\u00e4n\u00b7ner", ",", "sto\u00b7\u00dfet", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}