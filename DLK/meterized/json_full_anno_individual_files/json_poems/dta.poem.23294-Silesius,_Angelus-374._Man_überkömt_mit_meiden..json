{"dta.poem.23294": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "374. Man \u00fcberk\u00f6mt mit meiden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Freund meide was dir Lieb/ fleuch was dein Sinn be-", "tokens": ["Freund", "mei\u00b7de", "was", "dir", "Lieb", "/", "fleuch", "was", "dein", "Sinn", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PWS", "PPER", "NN", "$(", "VVFIN", "PIS", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gehrt/", "tokens": ["gehrt", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Du wirst sonst nimmermehr ges\u00e4ttigt und gewehrt.", "tokens": ["Du", "wirst", "sonst", "nim\u00b7mer\u00b7mehr", "ge\u00b7s\u00e4t\u00b7tigt", "und", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Viel w\u00e4ren zum Genu\u00df der ewgen Wollust kommen/", "tokens": ["Viel", "w\u00e4\u00b7ren", "zum", "Ge\u00b7nu\u00df", "der", "ew\u00b7gen", "Wol\u00b7lust", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn sie mit Zeitlicher sich hier nicht \u00fcbernommen.", "tokens": ["Wenn", "sie", "mit", "Zeit\u00b7li\u00b7cher", "sich", "hier", "nicht", "\u00fc\u00b7ber\u00b7no