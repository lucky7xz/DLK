{"dta.poem.6624": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den C\u00f6rpern, welche sonderlich", "tokens": ["Den", "C\u00f6r\u00b7pern", ",", "wel\u00b7che", "son\u00b7der\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die GOETTLJCHE Verordnung mit Bedacht", "tokens": ["Die", "GoETTLJ\u00b7CHE", "Ver\u00b7ord\u00b7nung", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jm Ursprung unterschied\u2019, ist etwas beygebracht,", "tokens": ["Jm", "Ur\u00b7sprung", "un\u00b7ter\u00b7schied'", ",", "ist", "et\u00b7was", "bey\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wodurch sie immer sich erhalten und vermehren.", "tokens": ["Wo\u00b7durch", "sie", "im\u00b7mer", "sich", "er\u00b7hal\u00b7ten", "und", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PRF", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein St\u00e4ubchen, welches man", "tokens": ["Ein", "St\u00e4ub\u00b7chen", ",", "wel\u00b7ches", "man"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht f\u00fchlen und nicht sehen kan,", "tokens": ["Nicht", "f\u00fch\u00b7len", "und", "nicht", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Enth\u00e4lt ein Leben, kleine Sprossen,", "tokens": ["Ent\u00b7h\u00e4lt", "ein", "Le\u00b7ben", ",", "klei\u00b7ne", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die in einander eingeschlossen,", "tokens": ["Die", "in", "ein\u00b7an\u00b7der", "ein\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Entstehen mit der Zeit, und folgen sich.", "tokens": ["Ent\u00b7ste\u00b7hen", "mit", "der", "Zeit", ",", "und", "fol\u00b7gen", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es ist jedwedes Augleins Stelle", "tokens": ["Es", "ist", "jed\u00b7we\u00b7des", "Aug\u00b7leins", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von seiner Art, ein\u2019 unersch\u00f6pffte Quelle.", "tokens": ["Von", "sei\u00b7ner", "Art", ",", "ein'", "un\u00b7er\u00b7sch\u00f6pff\u00b7te", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein eintzige Maron\u2019 aus Indien gebracht,", "tokens": ["Ein", "eint\u00b7zi\u00b7ge", "Ma\u00b7ron'", "aus", "In\u00b7di\u00b7en", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hat uns so viel Maronen B\u00e4um\u2019 erziehlet,", "tokens": ["Hat", "uns", "so", "viel", "Ma\u00b7ro\u00b7nen", "B\u00e4um'", "er\u00b7zieh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die G\u00e4rten sind beschattet und gek\u00fchlet", "tokens": ["Die", "G\u00e4r\u00b7ten", "sind", "be\u00b7schat\u00b7tet", "und", "ge\u00b7k\u00fch\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Durch ihrer Bl\u00e4tter gr\u00fcne Nacht.", "tokens": ["Durch", "ih\u00b7rer", "Bl\u00e4t\u00b7ter", "gr\u00fc\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und in den allerletzten stecket", "tokens": ["Und", "in", "den", "al\u00b7ler\u00b7letz\u00b7ten", "ste\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ein Saamen Schatz zum Schmuck der gantzen Welt verdecket.", "tokens": ["Ein", "Saa\u00b7men", "Schatz", "zum", "Schmuck", "der", "gant\u00b7zen", "Welt", "ver\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}