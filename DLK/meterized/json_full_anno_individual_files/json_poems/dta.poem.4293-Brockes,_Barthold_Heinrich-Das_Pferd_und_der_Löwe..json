{"dta.poem.4293": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Pferd und der L\u00f6we.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, rettet dieses sch\u00f6ne Thier, das des Tyrannen Last", "tokens": ["Ach", ",", "ret\u00b7tet", "die\u00b7ses", "sch\u00f6\u00b7ne", "Thier", ",", "das", "des", "Ty\u00b7ran\u00b7nen", "Last"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PDAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "erdr\u00fcckt!", "tokens": ["er\u00b7dr\u00fcckt", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es steckt schon in des L\u00f6wen Rachen! Der Schmerz, zu-", "tokens": ["Es", "steckt", "schon", "in", "des", "L\u00f6\u00b7wen", "Ra\u00b7chen", "!", "Der", "Schmerz", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$.", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "sammt der Angst, erf\u00fcllen", "tokens": ["sammt", "der", "Angst", ",", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jhm alle Muskeln, alle Sehnen! Die aufgeri\u00df\u2019nen Adern", "tokens": ["Jhm", "al\u00b7le", "Mus\u00b7keln", ",", "al\u00b7le", "Seh\u00b7nen", "!", "Die", "auf\u00b7ge\u00b7ri\u00df'\u00b7nen", "A\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "$,", "PIAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "quillen!", "tokens": ["quil\u00b7len", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Jhm sieht der Tod schon aus den Augen; mich deucht, es", "tokens": ["Jhm", "sieht", "der", "Tod", "schon", "aus", "den", "Au\u00b7gen", ";", "mich", "deucht", ",", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "PPER"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "ist bereits zerst\u00fcckt!", "tokens": ["ist", "be\u00b7reits", "zer\u00b7st\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ich wollte dieses edle Thier und seinen harten Fall bekla-", "tokens": ["Ich", "woll\u00b7te", "die\u00b7ses", "ed\u00b7le", "Thier", "und", "sei\u00b7nen", "har\u00b7ten", "Fall", "be\u00b7kla"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "gen;", "tokens": ["gen", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Von dir, o ", "tokens": ["Von", "dir", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "PPER", "$,", "FM"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "sagen:", "tokens": ["sa\u00b7gen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Allein dein L\u00f6we bleckt mich an; ich stutz\u2019, und selbst mein", "tokens": ["Al\u00b7lein", "dein", "L\u00f6\u00b7we", "bleckt", "mich", "an", ";", "ich", "stutz'", ",", "und", "selbst", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NE", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "KON", "ADV", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kiel erschrickt.", "tokens": ["Kiel", "er\u00b7schrickt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}