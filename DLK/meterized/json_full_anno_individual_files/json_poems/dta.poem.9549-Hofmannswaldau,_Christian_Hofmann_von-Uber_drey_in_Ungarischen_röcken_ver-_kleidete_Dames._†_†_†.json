{"dta.poem.9549": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber drey in Ungarischen r\u00f6cken ver-  \n kleidete Dames.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drey masquen liessen sich bey hofe gestern sehn", "tokens": ["Drey", "mas\u00b7quen", "lies\u00b7sen", "sich", "bey", "ho\u00b7fe", "ge\u00b7stern", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PRF", "APPR", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In angenehmer tracht und sehr geschickten r\u00f6cken:", "tokens": ["In", "an\u00b7ge\u00b7neh\u00b7mer", "tracht", "und", "sehr", "ge\u00b7schick\u00b7ten", "r\u00f6\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man kante sie gar bald/ sie waren allzu sch\u00f6n;", "tokens": ["Man", "kan\u00b7te", "sie", "gar", "bald", "/", "sie", "wa\u00b7ren", "all\u00b7zu", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "ADV", "$(", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch konte die in blau sich weniger verstecken.", "tokens": ["Doch", "kon\u00b7te", "die", "in", "blau", "sich", "we\u00b7ni\u00b7ger", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "APPR", "ADJD", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verr\u00e4th der diamant sich in dem schatten nicht?", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "der", "di\u00b7a\u00b7mant", "sich", "in", "dem", "schat\u00b7ten", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Man wei\u00df schon wer sie sey/ auch sonder sie zu nennen.", "tokens": ["Man", "wei\u00df", "schon", "wer", "sie", "sey", "/", "auch", "son\u00b7der", "sie", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PWS", "PPER", "VAFIN", "$(", "ADV", "KON", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn deckt sie gleich vor uns ihr himmlisches gesicht;", "tokens": ["Denn", "deckt", "sie", "gleich", "vor", "uns", "ihr", "himm\u00b7li\u00b7sches", "ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kan durch den sch\u00f6nen leib man dennoch sie erkennen.", "tokens": ["Kan", "durch", "den", "sch\u00f6\u00b7nen", "leib", "man", "den\u00b7noch", "sie", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "PIS", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}