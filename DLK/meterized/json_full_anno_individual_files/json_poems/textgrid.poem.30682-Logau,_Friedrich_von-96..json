{"textgrid.poem.30682": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00f6sser Thorheit kan kaum seyn,", "tokens": ["Gr\u00f6s\u00b7ser", "Thor\u00b7heit", "kan", "kaum", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer in fremdes Land k\u00fcmt ein,", "tokens": ["Wer", "in", "frem\u00b7des", "Land", "k\u00fcmt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er wil, da\u00df alle sollen", "tokens": ["Da\u00df", "er", "wil", ",", "da\u00df", "al\u00b7le", "sol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "$,", "KOUS", "PIS", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Richten sich nach seinem Wollen.", "tokens": ["Rich\u00b7ten", "sich", "nach", "sei\u00b7nem", "Wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer das fremde so veracht,", "tokens": ["Wer", "das", "frem\u00b7de", "so", "ver\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird von Fremden auch verlacht;", "tokens": ["Wird", "von", "Frem\u00b7den", "auch", "ver\u00b7lacht", ";"], "token_info": ["word", "word", "wor