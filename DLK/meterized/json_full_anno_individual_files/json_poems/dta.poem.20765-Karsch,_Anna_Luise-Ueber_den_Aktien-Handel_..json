{"dta.poem.20765": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ueber den Aktien-Handel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Geld besitzt, dem drohen Diebe,               ", "tokens": ["Wer", "Geld", "be\u00b7sitzt", ",", "dem", "dro\u00b7hen", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er schlummert nie in Sicherheit,", "tokens": ["Er", "schlum\u00b7mert", "nie", "in", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel sichrer schl\u00e4ft die Z\u00e4rtlichkeit", "tokens": ["Viel", "sich\u00b7rer", "schl\u00e4ft", "die", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Aktien der Liebe.", "tokens": ["Bei", "Ak\u00b7ti\u00b7en", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}