{"textgrid.poem.61621": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf sonnverbrannten Matten", "tokens": ["Wenn", "auf", "sonn\u00b7ver\u00b7brann\u00b7ten", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die Zikade schrillt von fern,", "tokens": ["Die", "Zi\u00b7ka\u00b7de", "schrillt", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rast' ich in des Lorbeers Schatten", "tokens": ["Rast'", "ich", "in", "des", "Lor\u00b7beers", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei den alten Dichtern gern.", "tokens": ["Bei", "den", "al\u00b7ten", "Dich\u00b7tern", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sanft wie voller Segel Schwellen", "tokens": ["Sanft", "wie", "vol\u00b7ler", "Se\u00b7gel", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt Homers gefl\u00fcgelt Wort", "tokens": ["Tr\u00e4gt", "Ho\u00b7mers", "ge\u00b7fl\u00fc\u00b7gelt", "Wort"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NE", "VVPP", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mich durch Sturmgefahr und Wellen,", "tokens": ["Mich", "durch", "Sturm\u00b7ge\u00b7fahr", "und", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Volksgew\u00fchl und Schlachten fort.", "tokens": ["Volks\u00b7ge\u00b7w\u00fchl", "und", "Schlach\u00b7ten", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In Olympias staub'ge Bahnen", "tokens": ["In", "O\u00b7lym\u00b7pias", "staub'\u00b7ge", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rei\u00dft mich Pindars Siegeschor,", "tokens": ["Rei\u00dft", "mich", "Pin\u00b7dars", "Sie\u00b7ge\u00b7schor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des \u00c4schylus Titanen", "tokens": ["Und", "des", "\u00c4\u00b7schy\u00b7lus", "Ti\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steigen trotz'gen Blicks empor.", "tokens": ["Stei\u00b7gen", "trotz'\u00b7gen", "Blicks", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch von allen, die ich w\u00e4hle,", "tokens": ["Doch", "von", "al\u00b7len", ",", "die", "ich", "w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwichtigt mit erhabner Ruh'", "tokens": ["Schwich\u00b7tigt", "mit", "er\u00b7hab\u00b7ner", "Ruh'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner mir so ganz die Seele,", "tokens": ["Kei\u00b7ner", "mir", "so", "ganz", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoher Sophokles, wie du.", "tokens": ["Ho\u00b7her", "So\u00b7phok\u00b7les", ",", "wie", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Von erliegender Heroen", "tokens": ["Von", "er\u00b7lie\u00b7gen\u00b7der", "He\u00b7roen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unverstandnem Riesenleid", "tokens": ["Un\u00b7ver\u00b7stand\u00b7nem", "Rie\u00b7sen\u00b7leid"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrtest du dein Volk zum hohen", "tokens": ["F\u00fchr\u00b7test", "du", "dein", "Volk", "zum", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Urbild sch\u00f6ner Menschlichkeit;", "tokens": ["Ur\u00b7bild", "sch\u00f6\u00b7ner", "Menschlich\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Riefest aus dem Scho\u00df der N\u00e4chte,", "tokens": ["Rie\u00b7fest", "aus", "dem", "Scho\u00df", "der", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die von Mitleid nie gewu\u00dft,", "tokens": ["Die", "von", "Mit\u00b7leid", "nie", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihren Teil der Schicksalsm\u00e4chte", "tokens": ["Ih\u00b7ren", "Teil", "der", "Schick\u00b7sals\u00b7m\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die freigewordne Brust;", "tokens": ["In", "die", "frei\u00b7ge\u00b7word\u00b7ne", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df, was aus des Herzens Falten", "tokens": ["Da\u00df", ",", "was", "aus", "des", "Her\u00b7zens", "Fal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "R\u00e4tselvoll gezeitigt spro\u00df,", "tokens": ["R\u00e4t\u00b7sel\u00b7voll", "ge\u00b7zei\u00b7tigt", "spro\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der G\u00f6tter hehrem Walten", "tokens": ["Mit", "der", "G\u00f6t\u00b7ter", "heh\u00b7rem", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zum goldnen Ring beschlo\u00df.", "tokens": ["Sich", "zum", "gold\u00b7nen", "Ring", "be\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Also zwischen starrer Sitte,", "tokens": ["Al\u00b7so", "zwi\u00b7schen", "star\u00b7rer", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen frecher Neurung Wahn", "tokens": ["Zwi\u00b7schen", "fre\u00b7cher", "Neu\u00b7rung", "Wahn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Walltest du in sch\u00f6ner Mitte", "tokens": ["Wall\u00b7test", "du", "in", "sch\u00f6\u00b7ner", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch und heiter deine Bahn;", "tokens": ["Hoch", "und", "hei\u00b7ter", "dei\u00b7ne", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Kl\u00e4rtest mit dem Hauch der Musen", "tokens": ["Kl\u00e4r\u00b7test", "mit", "dem", "Hauch", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fromm der Leidenschaften Glut,", "tokens": ["Fromm", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein heilig Ma\u00df im Busen", "tokens": ["Und", "ein", "hei\u00b7lig", "Ma\u00df", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Priesest du als h\u00f6chstes Gut.", "tokens": ["Prie\u00b7sest", "du", "als", "h\u00f6chs\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sel'ger, dem sein Wort zu lohnen", "tokens": ["Sel'\u00b7ger", ",", "dem", "sein", "Wort", "zu", "loh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das entz\u00fcckte Griechenland", "tokens": ["Das", "ent\u00b7z\u00fcck\u00b7te", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine reichsten Lorbeerkronen", "tokens": ["Sei\u00b7ne", "reichs\u00b7ten", "Lor\u00b7beer\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die Priesterschl\u00e4fe wand;", "tokens": ["Um", "die", "Pries\u00b7ter\u00b7schl\u00e4\u00b7fe", "wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Der noch heut, vom wandelbaren", "tokens": ["Der", "noch", "heut", ",", "vom", "wan\u00b7del\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADV", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strom der Zeitflut unversehrt,", "tokens": ["Strom", "der", "Zeit\u00b7flut", "un\u00b7ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut nach zweimal tausend Jahren", "tokens": ["Heut", "nach", "zwei\u00b7mal", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nheit uns und Weisheit lehrt!", "tokens": ["Sch\u00f6n\u00b7heit", "uns", "und", "Weis\u00b7heit", "lehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wenn auf sonnverbrannten Matten", "tokens": ["Wenn", "auf", "sonn\u00b7ver\u00b7brann\u00b7ten", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die Zikade schrillt von fern,", "tokens": ["Die", "Zi\u00b7ka\u00b7de", "schrillt", "von", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rast' ich in des Lorbeers Schatten", "tokens": ["Rast'", "ich", "in", "des", "Lor\u00b7beers", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei den 