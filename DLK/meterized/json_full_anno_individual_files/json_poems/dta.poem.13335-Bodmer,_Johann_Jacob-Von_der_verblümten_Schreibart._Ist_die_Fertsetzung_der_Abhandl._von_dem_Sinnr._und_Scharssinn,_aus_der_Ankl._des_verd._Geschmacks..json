{"dta.poem.13335": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier sieht man euch als wie Smaragden gr\u00fcnen,", "tokens": ["Hier", "sieht", "man", "euch", "als", "wie", "Sma\u00b7rag\u00b7den", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "KOUS", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hier brennet ihr als wie Rubinen,", "tokens": ["Hier", "bren\u00b7net", "ihr", "als", "wie", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dort werdet ihr dem T\u00fcrckis gleich geacht.", "tokens": ["Dort", "wer\u00b7det", "ihr", "dem", "T\u00fcr\u00b7ckis", "gleich", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der bunte Schmeltz, den man auf euch erblicket,", "tokens": ["Der", "bun\u00b7te", "Sch\u00b7meltz", ",", "den", "man", "auf", "euch", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der Atlas und Damast, der eure Bl\u00e4tter schm\u00fccket,", "tokens": ["Der", "At\u00b7las", "und", "Da\u00b7mast", ",", "der", "eu\u00b7re", "Bl\u00e4t\u00b7ter", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist reicher, als was sonst Natur und Kunst erdacht.", "tokens": ["Ist", "rei\u00b7cher", ",", "als", "was", "sonst", "Na\u00b7tur", "und", "Kunst", "er\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PIS", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}