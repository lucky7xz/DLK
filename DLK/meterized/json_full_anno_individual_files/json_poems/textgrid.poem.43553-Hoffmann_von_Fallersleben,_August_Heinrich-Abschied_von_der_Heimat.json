{"textgrid.poem.43553": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von der Heimat", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thr\u00e4nen hab' ich viele, viele vergossen,", "tokens": ["Thr\u00e4\u00b7nen", "hab'", "ich", "vie\u00b7le", ",", "vie\u00b7le", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da\u00df ich scheiden mu\u00df von hier \u2013", "tokens": ["Da\u00df", "ich", "schei\u00b7den", "mu\u00df", "von", "hier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "APPR", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch mein lieber Vater hat es beschlossen,", "tokens": ["Doch", "mein", "lie\u00b7ber", "Va\u00b7ter", "hat", "es", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus der Heimat wandern wir,", "tokens": ["Aus", "der", "Hei\u00b7mat", "wan\u00b7dern", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heimat, heute wandern wir,", "tokens": ["Hei\u00b7mat", ",", "heu\u00b7te", "wan\u00b7dern", "wir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measu