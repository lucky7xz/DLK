{"textgrid.poem.42611": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Bidassoabr\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Bidassoabr\u00fccke", "tokens": ["Auf", "der", "Bi\u00b7das\u00b7soa\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Steht ein Heil'ger, altergrau,", "tokens": ["Steht", "ein", "Heil'\u00b7ger", ",", "al\u00b7ter\u00b7grau", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Segnet rechts die span'schen Berge,", "tokens": ["Seg\u00b7net", "rechts", "die", "span'\u00b7schen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segnet links den fr\u00e4nk'schen Gau.", "tokens": ["Seg\u00b7net", "links", "den", "fr\u00e4nk'\u00b7schen", "Gau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohl bedarf's an dieser Stelle", "tokens": ["Wohl", "be\u00b7da\u00b7rf's", "an", "die\u00b7ser", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Milden Trostes himmelher,", "tokens": ["Mil\u00b7den", "Tros\u00b7tes", "him\u00b7mel\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo so mancher von der Heimat", "tokens": ["Wo", "so", "man\u00b7cher", "von", "der", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Scheidet ohne Wiederkehr.", "tokens": ["Schei\u00b7det", "oh\u00b7ne", "Wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf der Bidassoabr\u00fccke", "tokens": ["Auf", "der", "Bi\u00b7das\u00b7soa\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Spielt ein zauberhaft Gesicht:", "tokens": ["Spielt", "ein", "zau\u00b7ber\u00b7haft", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der eine Schatten siehet,", "tokens": ["Wo", "der", "ei\u00b7ne", "Schat\u00b7ten", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht der andre goldnes Licht;", "tokens": ["Sieht", "der", "and\u00b7re", "gold\u00b7nes", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo dem einen Rosen lachen,", "tokens": ["Wo", "dem", "ei\u00b7nen", "Ro\u00b7sen", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht der andre d\u00fcrren Sand;", "tokens": ["Sieht", "der", "and\u00b7re", "d\u00fcr\u00b7ren", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jedem ist das Elend finster,", "tokens": ["Je\u00b7dem", "ist", "das", "E\u00b7lend", "fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jedem gl\u00e4nzt sein Vaterland.", "tokens": ["Je\u00b7dem", "gl\u00e4nzt", "sein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Friedlich rauscht die Bidassoa", "tokens": ["Fried\u00b7lich", "rauscht", "die", "Bi\u00b7das\u00b7soa"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Herde Glockenklang,", "tokens": ["Zu", "der", "Her\u00b7de", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Aber im Gebirge dr\u00f6hnet", "tokens": ["A\u00b7ber", "im", "Ge\u00b7bir\u00b7ge", "dr\u00f6h\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Knall auf Knall den Tag entlang;", "tokens": ["Knall", "auf", "Knall", "den", "Tag", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "APPO", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und am Abend steigt hernieder", "tokens": ["Und", "am", "A\u00b7bend", "steigt", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Schar zum Flu\u00dfgestad,", "tokens": ["Ei\u00b7ne", "Schar", "zum", "Flu\u00df\u00b7ge\u00b7stad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unstet, mit zerri\u00dfner Fahne,", "tokens": ["Un\u00b7stet", ",", "mit", "zer\u00b7ri\u00df\u00b7ner", "Fah\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blut betr\u00e4ufelt ihren Pfad.", "tokens": ["Blut", "be\u00b7tr\u00e4u\u00b7felt", "ih\u00b7ren", "Pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf der Bidassoabr\u00fccke", "tokens": ["Auf", "der", "Bi\u00b7das\u00b7soa\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Lehnen sie die B\u00fcchsen bei,", "tokens": ["Leh\u00b7nen", "sie", "die", "B\u00fcch\u00b7sen", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Binden sich die frischen Wunden,", "tokens": ["Bin\u00b7den", "sich", "die", "fri\u00b7schen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4hlen, wer noch \u00fcbrig sei?", "tokens": ["Z\u00e4h\u00b7len", ",", "wer", "noch", "\u00fcb\u00b7rig", "sei", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lange harren sie Vermi\u00dfter,", "tokens": ["Lan\u00b7ge", "har\u00b7ren", "sie", "Ver\u00b7mi\u00df\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch ihr H\u00e4uflein w\u00e4chset nicht,", "tokens": ["Doch", "ihr", "H\u00e4uf\u00b7lein", "w\u00e4ch\u00b7set", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Einmal wirbelt noch die Trommel,", "tokens": ["Ein\u00b7mal", "wir\u00b7belt", "noch", "die", "Trom\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ein alter Kriegsmann spricht:", "tokens": ["Und", "ein", "al\u00b7ter", "Kriegs\u00b7mann", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbrollt die Fahne denn zusammen,", "tokens": ["\u00bb", "rollt", "die", "Fah\u00b7ne", "denn", "zu\u00b7sam\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Freiheit Banner war!", "tokens": ["Die", "der", "Frei\u00b7heit", "Ban\u00b7ner", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht zum erstenmale wandelt", "tokens": ["Nicht", "zum", "ers\u00b7ten\u00b7ma\u00b7le", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen Grenzweg ihre Schar;", "tokens": ["Die\u00b7sen", "Grenz\u00b7weg", "ih\u00b7re", "Schar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht zum erstenmale sucht sie", "tokens": ["Nicht", "zum", "ers\u00b7ten\u00b7ma\u00b7le", "sucht", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Freistatt in der Fern,", "tokens": ["Ei\u00b7ne", "Freis\u00b7tatt", "in", "der", "Fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch sie zieht nicht arm an Ehre,", "tokens": ["Doch", "sie", "zieht", "nicht", "arm", "an", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zieht nicht ohne g\u00fcnst'gen Stern.", "tokens": ["Zieht", "nicht", "oh\u00b7ne", "g\u00fcnst'\u00b7gen", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der von vor'gen Freiheitsk\u00e4mpfen", "tokens": ["Der", "von", "vor'\u00b7gen", "Frei\u00b7heits\u00b7k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr als einer Narben f\u00fchrt,", "tokens": ["Mehr", "als", "ei\u00b7ner", "Nar\u00b7ben", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute, da wir alle bluten,", "tokens": ["Heu\u00b7te", ",", "da", "wir", "al\u00b7le", "blu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mina! bliebst du unber\u00fchrt;", "tokens": ["Mi\u00b7na", "!", "bliebst", "du", "un\u00b7be\u00b7r\u00fchrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ganz und heil ist uns der Retter,", "tokens": ["Ganz", "und", "heil", "ist", "uns", "der", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch verb\u00fcrgt ist Spaniens Gl\u00fcck;", "tokens": ["Noch", "ver\u00b7b\u00fcrgt", "ist", "Spa\u00b7ni\u00b7ens", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "NE", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Schreiten wir getrost hin\u00fcber!", "tokens": ["Schrei\u00b7ten", "wir", "ge\u00b7trost", "hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einst noch kehren wir zur\u00fcck.\u00ab", "tokens": ["Einst", "noch", "keh\u00b7ren", "wir", "zu\u00b7r\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mina rafft sich auf vom Steine,", "tokens": ["Mi\u00b7na", "rafft", "sich", "auf", "vom", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "M\u00fcde sa\u00df er dort und still,", "tokens": ["M\u00fc\u00b7de", "sa\u00df", "er", "dort", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickt noch einmal nach den Bergen,", "tokens": ["Blickt", "noch", "ein\u00b7mal", "nach", "den", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Sonne sinken will;", "tokens": ["Wo", "die", "Son\u00b7ne", "sin\u00b7ken", "will", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Hand, zur Brust gehalten,", "tokens": ["Sei\u00b7ne", "Hand", ",", "zur", "Brust", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hemmt nicht mehr des Blutes Lauf,", "tokens": ["Hemmt", "nicht", "mehr", "des", "Blu\u00b7tes", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf der Bidassoabr\u00fccke", "tokens": ["Auf", "der", "Bi\u00b7das\u00b7soa\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Brachen alte Wunden auf.", "tokens": ["Bra\u00b7chen", "al\u00b7te", "Wun\u00b7den", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf der Bidassoabr\u00fccke", "tokens": ["Auf", "der", "Bi\u00b7das\u00b7soa\u00b7br\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Steht ein Heil'ger, altergrau,", "tokens": ["Steht", "ein", "Heil'\u00b7ger", ",", "al\u00b7ter\u00b7grau", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Segnet rechts die span'schen Berge,", "tokens": ["Seg\u00b7net", "rechts", "die", "span'\u00b7schen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segnet links den fr\u00e4nk'schen Gau.", "tokens": ["Seg\u00b7net", "links", "den", "fr\u00e4nk'\u00b7schen", "Gau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": 