{"textgrid.poem.52576": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab es tief empfunden,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab es tief empfunden,", "tokens": ["Ich", "hab", "es", "tief", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab es wohl erkannt,", "tokens": ["Ich", "hab", "es", "wohl", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In ungl\u00fcckselgen Stunden,", "tokens": ["In", "un\u00b7gl\u00fcck\u00b7sel\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ich verlassen stand", "tokens": ["Wenn", "ich", "ver\u00b7las\u00b7sen", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von allen hohen Freuden,", "tokens": ["Von", "al\u00b7len", "ho\u00b7hen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die hei\u00dfe Liebe schafft \u2013", "tokens": ["Die", "hei\u00b7\u00dfe", "Lie\u00b7be", "schafft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie wenig will das Scheiden,", "tokens": ["Wie", "we\u00b7