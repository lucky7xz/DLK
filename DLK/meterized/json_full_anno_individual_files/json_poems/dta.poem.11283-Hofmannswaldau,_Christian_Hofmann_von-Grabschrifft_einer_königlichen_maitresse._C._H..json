{"dta.poem.11283": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft einer k\u00f6niglichen maitresse.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der leib, den Ludewig vor andern hochgesch\u00e4tzt,", "tokens": ["Der", "leib", ",", "den", "Lu\u00b7de\u00b7wig", "vor", "an\u00b7dern", "hoch\u00b7ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NE", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird nun den w\u00fcrmern hier zur speise vorgesetzt;", "tokens": ["Wird", "nun", "den", "w\u00fcr\u00b7mern", "hier", "zur", "spei\u00b7se", "vor\u00b7ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dieselben k\u00f6nnen sich itzt vom gebl\u00fcte schreiben,", "tokens": ["Die\u00b7sel\u00b7ben", "k\u00f6n\u00b7nen", "sich", "itzt", "vom", "ge\u00b7bl\u00fc\u00b7te", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie mit diesem fleisch auch ihre wollust treiben.", "tokens": ["Weil", "sie", "mit", "die\u00b7sem", "fleisch", "auch", "ih\u00b7re", "wol\u00b7lust", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "ADV", "PPOSAT", "NN"