{"dta.poem.9580": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie gegen den winter ihre wohnung ver-  \n \u00e4nderte an einem tage/ da die sonne aus ei-  \n nem zeichen des himmels in  \n das andere trat.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljbore/ die dem glantz des himmels zu vergleichen/", "tokens": ["Lj\u00b7bo\u00b7re", "/", "die", "dem", "glantz", "des", "him\u00b7mels", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die uns bi\u00dfher so offt begl\u00fcckt durch ihren schein/", "tokens": ["Die", "uns", "bi\u00df\u00b7her", "so", "offt", "be\u00b7gl\u00fcckt", "durch", "ih\u00b7ren", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird wohl was sparsam nun die strahlen auf uns streum", "tokens": ["Wird", "wohl", "was", "spar\u00b7sam", "nun", "die", "strah\u00b7len", "auf", "uns", "streum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PWS", "ADJD", "ADV", "ART", "ADJA", "APPR", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und uns von ferne nur die s\u00fcssen blicke reichen.", "tokens": ["Und", "uns", "von", "fer\u00b7ne", "nur", "die", "s\u00fcs\u00b7sen", "bli\u00b7cke", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADV", "ADV", "ART", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn ihre sonne tritt nun in ein andres zeichen/", "tokens": ["Denn", "ih\u00b7re", "son\u00b7ne", "tritt", "nun", "in", "ein", "and\u00b7res", "zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und hinterl\u00e4\u00dft uns nichts/ als frost und strenge pein.", "tokens": ["Und", "hin\u00b7ter\u00b7l\u00e4\u00dft", "uns", "nichts", "/", "als", "frost", "und", "stren\u00b7ge", "pein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "$(", "KOUS", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja/ kan es anders wohl um uns beschaffen seyn/", "tokens": ["Ja", "/", "kan", "es", "an\u00b7ders", "wohl", "um", "uns", "be\u00b7schaf\u00b7fen", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn licht und sonne will von unsrer gegend weichen?", "tokens": ["Wenn", "licht", "und", "son\u00b7ne", "will", "von", "uns\u00b7rer", "ge\u00b7gend", "wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VMFIN", "APPR", "PPOSAT", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie suche denn vor sich ein\u2019 andre wohnung aus/", "tokens": ["Sie", "su\u00b7che", "denn", "vor", "sich", "ein'", "and\u00b7re", "woh\u00b7nung", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und \u00e4ndre ihr bi\u00dfher so lang bestrahltes hau\u00df;", "tokens": ["Und", "\u00e4nd\u00b7re", "ihr", "bi\u00df\u00b7her", "so", "lang", "be\u00b7strahl\u00b7tes", "hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer aber darff de\u00dfhalb auff das verh\u00e4ngni\u00df fluchen?", "tokens": ["Wer", "a\u00b7ber", "darff", "de\u00df\u00b7halb", "auff", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "flu\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PAV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O st\u00fcnde mir doch auch dergleichen endrung frey!", "tokens": ["O", "st\u00fcn\u00b7de", "mir", "doch", "auch", "derg\u00b7lei\u00b7chen", "end\u00b7rung", "frey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein geist w\u00fcrd\u2019 heute noch/ ich sag\u2019 es ohne schen/", "tokens": ["Mein", "geist", "w\u00fcrd'", "heu\u00b7te", "noch", "/", "ich", "sag'", "es", "oh\u00b7ne", "schen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$(", "PPER", "VVFIN", "PPER", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kein\u2019 andre wohnung sonst/ als nur ihr hertze/ suchen.", "tokens": ["Kein'", "and\u00b7re", "woh\u00b7nung", "sonst", "/", "als", "nur", "ihr", "hert\u00b7ze", "/", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "$(", "KOKOM", "ADV", "PPOSAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}