{"textgrid.poem.43010": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Gru\u00df ins Blaue", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr verehrte, auserlesene,", "tokens": ["Sehr", "ver\u00b7ehr\u00b7te", ",", "au\u00b7ser\u00b7le\u00b7se\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Einmal nahe mir gewesene,", "tokens": ["Ein\u00b7mal", "na\u00b7he", "mir", "ge\u00b7we\u00b7se\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Nunmehr tote Damen und Herrn!", "tokens": ["Nun\u00b7mehr", "to\u00b7te", "Da\u00b7men", "und", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich h\u00e4tte all Ihnen gar zu gern", "tokens": ["Ich", "h\u00e4t\u00b7te", "all", "Ih\u00b7nen", "gar", "zu", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "PPER", "ADV", "PTKA", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch etwas vor dem Tode gesagt.", "tokens": ["Noch", "et\u00b7was", "vor", "dem", "To\u00b7de", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Hab ich vers\u00e4umt oder nicht gewagt,", "tokens": ["Hab", "ich", "ver\u00b7s\u00e4umt", "o\u00b7der", "nicht", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "KON", "PTKNEG", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Zu sagen, wonach kein Toter fragt,", "tokens": ["Zu", "sa\u00b7gen", ",", "wo\u00b7nach", "kein", "To\u00b7ter", "fragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Liegt nun jede Aufdringlichkeit fern.", "tokens": ["Liegt", "nun", "je\u00b7de", "Auf\u00b7dring\u00b7lich\u00b7keit", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-++-++", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Dorthin, wo Sie jetzt weilen, reicht keine", "tokens": ["Dor\u00b7thin", ",", "wo", "Sie", "jetzt", "wei\u00b7len", ",", "reicht", "kei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PIAT"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00fcge. Sie wissen auch, wie ich es meine,", "tokens": ["L\u00fc\u00b7ge", ".", "Sie", "wis\u00b7sen", "auch", ",", "wie", "ich", "es", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wenn ich aus reuevollem Bed\u00fcrfnis", "tokens": ["Wenn", "ich", "aus", "reu\u00b7e\u00b7vol\u00b7lem", "Be\u00b7d\u00fcrf\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Jetzt mit einem Whiskygeschl\u00fcrfnis", "tokens": ["Jetzt", "mit", "ei\u00b7nem", "Whis\u00b7ky\u00b7ge\u00b7schl\u00fcrf\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "X-w\u00e4rts proste. Ich wei\u00df, wer es wagen", "tokens": ["X\u00b7\u00b7w\u00e4rts", "pros\u00b7te", ".", "Ich", "wei\u00df", ",", "wer", "es", "wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Darf, eine Flunder noch breit zu schlagen.", "tokens": ["Darf", ",", "ei\u00b7ne", "Flun\u00b7der", "noch", "breit", "zu", "schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sehr verehrte, auserlesene,", "tokens": ["Sehr", "ver\u00b7ehr\u00b7te", ",", "au\u00b7ser\u00b7le\u00b7se\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Einmal nahe mir gewesene,", "tokens": ["Ein\u00b7mal", "na\u00b7he", "mir", "ge\u00b7we\u00b7se\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJA", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Nunmehr tote Damen und Herrn!", "tokens": ["Nun\u00b7mehr", "to\u00b7te", "Da\u00b7men", "und", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"