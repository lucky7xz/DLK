}, "stanza.22": {"line.1": {"text": "Sie sch\u00fcrzt sich auf als Fischerm\u00e4dchen", "tokens": ["Sie", "sch\u00fcrzt", "sich", "auf", "als", "Fi\u00b7scher\u00b7m\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sitzt an Waldstroms gr\u00fcnem Rand;", "tokens": ["Und", "sitzt", "an", "Wald\u00b7stroms", "gr\u00fc\u00b7nem", "Rand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Angel schwebt am leisen F\u00e4dchen,", "tokens": ["Die", "An\u00b7gel", "schwebt", "am", "lei\u00b7sen", "F\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann spielt der Fisch in ihrer Hand.", "tokens": ["Dann", "spielt", "der", "Fisch", "in", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Und wenn der J\u00e4ger kommt nach Hause", "tokens": ["Und", "wenn", "der", "J\u00e4\u00b7ger", "kommt", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bringt das Wildbret f\u00fcr den Tisch,", "tokens": ["Und", "bringt", "das", "Wild\u00b7bret", "f\u00fcr", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird erst das Mahl zum leckern Schmause,", "tokens": ["Wird", "erst", "das", "Mahl", "zum", "le\u00b7ckern", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den J\u00e4ger \u00fcberrascht der Fisch.", "tokens": ["Den", "J\u00e4\u00b7ger", "\u00fc\u00b7berr\u00b7ascht", "der", "Fisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "Es haben sich die m\u00fcden R\u00fcden", "tokens": ["Es", "ha\u00b7ben", "sich", "die", "m\u00fc\u00b7den", "R\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im hohen Gras zur Ruh' gelegt,", "tokens": ["Im", "ho\u00b7hen", "Gras", "zur", "Ruh'", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil auch den J\u00e4gersmann, den m\u00fcden,", "tokens": ["Weil", "auch", "den", "J\u00e4\u00b7gers\u00b7mann", ",", "den", "m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Laub' in k\u00fchlem Schatten hegt,", "tokens": ["Die", "Laub'", "in", "k\u00fch\u00b7lem", "Schat\u00b7ten", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Er horcht, entschlummernd, auf das Gleiten", "tokens": ["Er", "horcht", ",", "ent\u00b7schlum\u00b7mernd", ",", "auf", "das", "Glei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Stroms, der leis' hinunterzieht.", "tokens": ["Des", "Stroms", ",", "der", "leis'", "hin\u00b7un\u00b7ter\u00b7zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebste schmiegt sich ihm zur Seiten", "tokens": ["Die", "Liebs\u00b7te", "schmiegt", "sich", "ihm", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wiegt ihn ein mit einem Lied:", "tokens": ["Und", "wiegt", "ihn", "ein", "mit", "ei\u00b7nem", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Ihr Hirsch' im gr\u00fcnen Wald, ihr Rehe,", "tokens": ["Ihr", "Hirsch'", "im", "gr\u00fc\u00b7nen", "Wald", ",", "ihr", "Re\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun lagert euch an k\u00fchler Flut", "tokens": ["Nun", "la\u00b7gert", "euch", "an", "k\u00fch\u00b7ler", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sorget nicht, da\u00df euch geschehe", "tokens": ["Und", "sor\u00b7get", "nicht", ",", "da\u00df", "euch", "ge\u00b7sche\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Leid, denn euer Sch\u00fctze ruht!", "tokens": ["Ein", "Leid", ",", "denn", "eu\u00b7er", "Sch\u00fct\u00b7ze", "ruht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Du schau' mir, hohe Mittagsonne,", "tokens": ["Du", "schau'", "mir", ",", "ho\u00b7he", "Mit\u00b7ta\u00b7gson\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht durch die laub'ge Nacht herein;", "tokens": ["Nicht", "durch", "die", "laub'\u00b7ge", "Nacht", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was du sp\u00e4hst von unsrer Wonne,", "tokens": ["Und", "was", "du", "sp\u00e4hst", "von", "uns\u00b7rer", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das la\u00df der Welt verschwiegen sein!", "tokens": ["Das", "la\u00df", "der", "Welt", "ver\u00b7schwie\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.28": {"line.1": {"text": "Ihr Stromeswellen, die ihr rauschet", "tokens": ["Ihr", "Stro\u00b7mes\u00b7wel\u00b7len", ",", "die", "ihr", "rau\u00b7schet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus ins Land vom gr\u00fcnen Wald,", "tokens": ["Hin\u00b7aus", "ins", "Land", "vom", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sagt's keinem, da\u00df ihr habt belauschet", "tokens": ["Sagt's", "kei\u00b7nem", ",", "da\u00df", "ihr", "habt", "be\u00b7lau\u00b7schet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PIS", "$,", "KOUS", "PPER", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier unsrer Freuden Aufenthalt!", "tokens": ["Hier", "uns\u00b7rer", "Freu\u00b7den", "Auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}