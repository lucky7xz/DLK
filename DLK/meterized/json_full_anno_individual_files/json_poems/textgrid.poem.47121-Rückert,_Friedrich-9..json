{"textgrid.poem.47121": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem Walde m\u00f6cht' ich wohnen,", "tokens": ["In", "die\u00b7sem", "Wal\u00b7de", "m\u00f6cht'", "ich", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der freie J\u00e4ger m\u00f6cht' ich sein,", "tokens": ["Der", "frei\u00b7e", "J\u00e4\u00b7ger", "m\u00f6cht'", "ich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der in die dunklen Laubeskronen", "tokens": ["Der", "in", "die", "dunk\u00b7len", "Lau\u00b7bes\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich hat gepflanzt sein Haus hinein.", "tokens": ["Sich", "hat", "ge\u00b7pflanzt", "sein", "Haus", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VAFIN", "VVPP", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der erste Strahl der Sonne schauet", "tokens": ["Der", "ers\u00b7te", "Strahl", "der", "Son\u00b7ne", "schau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Tannengr\u00fcn ins Schlafgemach,", "tokens": ["Durch", "Tan\u00b7nen\u00b7gr\u00fcn", "ins", "Schlaf\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ihm der Schlaf im Aug' zertauet,", "tokens": ["Wo", "ihm", "der", "Schlaf", "im", "Aug'", "zer\u00b7tau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Liebchens Armen wird er wach.", "tokens": ["In", "Lieb\u00b7chens", "Ar\u00b7men", "wird", "er", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sogleich mit seinen treuen Hunden", "tokens": ["Sog\u00b7leich", "mit", "sei\u00b7nen", "treu\u00b7en", "Hun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht er hinaus durch Wald und Flur", "tokens": ["Zieht", "er", "hin\u00b7aus", "durch", "Wald", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APZR", "APPR", "NN", "KON", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und hat im Morgentau gefunden", "tokens": ["Und", "hat", "im", "Mor\u00b7gen\u00b7tau", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Hirsches und des Rehes Spur.", "tokens": ["Des", "Hir\u00b7sches", "und", "des", "Re\u00b7hes", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Sch\u00fctze jauchzt, die Hunde bellen,", "tokens": ["Der", "Sch\u00fct\u00b7ze", "jauchzt", ",", "die", "Hun\u00b7de", "bel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das scharfe Rohr gibt seinen Knall,", "tokens": ["Das", "schar\u00b7fe", "Rohr", "gibt", "sei\u00b7nen", "Knall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und J\u00e4gerruf und Waldhorngellen", "tokens": ["Und", "J\u00e4\u00b7ger\u00b7ruf", "und", "Wald\u00b7horn\u00b7gel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erweckt im Forst den Widerhall.", "tokens": ["Er\u00b7weckt", "im", "Forst", "den", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch drinnen sitzt im Morgenh\u00e4ubchen", "tokens": ["Doch", "drin\u00b7nen", "sitzt", "im", "Mor\u00b7gen\u00b7h\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Feinsliebchen, atmet Waldesduft", "tokens": ["Feins\u00b7lieb\u00b7chen", ",", "at\u00b7met", "Wal\u00b7des\u00b7duft"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und horcht, wie Amsel, Fink und T\u00e4ubchen", "tokens": ["Und", "horcht", ",", "wie", "Am\u00b7sel", ",", "Fink", "und", "T\u00e4ub\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Morgengru\u00df ins Fenster ruft.", "tokens": ["Den", "Mor\u00b7gen\u00b7gru\u00df", "ins", "Fens\u00b7ter", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie h\u00f6rt im Forst die Zweige fl\u00fcstern,", "tokens": ["Sie", "h\u00f6rt", "im", "Forst", "die", "Zwei\u00b7ge", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie ein s\u00fc\u00dfes Grausen sp\u00fcrt,", "tokens": ["Da\u00df", "sie", "ein", "s\u00fc\u00b7\u00dfes", "Grau\u00b7sen", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf dem Herd die Flammen knistern,", "tokens": ["Und", "auf", "dem", "Herd", "die", "Flam\u00b7men", "knis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sie mit duft'gem Kien gesch\u00fcrt.", "tokens": ["Die", "sie", "mit", "duft'\u00b7gem", "Ki\u00b7en", "ge\u00b7sch\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Wie lange mag der Liebste s\u00e4umen", "tokens": ["Wie", "lan\u00b7ge", "mag", "der", "Liebs\u00b7te", "s\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei seiner lust'gen J\u00e4gerei?", "tokens": ["Bei", "sei\u00b7ner", "lust'\u00b7gen", "J\u00e4\u00b7ge\u00b7rei", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der stille Strom mit Silbersch\u00e4umen", "tokens": ["Der", "stil\u00b7le", "Strom", "mit", "Sil\u00b7ber\u00b7sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flie\u00dft an des G\u00e4rtchens Zaun vorbei.", "tokens": ["Flie\u00dft", "an", "des", "G\u00e4rt\u00b7chens", "Zaun", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie sch\u00fcrzt sich auf als Fischerm\u00e4dchen", "tokens": ["Sie", "sch\u00fcrzt", "sich", "auf", "als", "Fi\u00b7scher\u00b7m\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sitzt an Waldstroms gr\u00fcnem Rand;", "tokens": ["Und", "sitzt", "an", "Wald\u00b7stroms", "gr\u00fc\u00b7nem", "Rand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Angel schwebt am leisen F\u00e4dchen,", "tokens": ["Die", "An\u00b7gel", "schwebt", "am", "lei\u00b7sen", "F\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann spielt der Fisch in ihrer Hand.", "tokens": ["Dann", "spielt", "der", "Fisch", "in", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wenn der J\u00e4ger kommt nach Hause", "tokens": ["Und", "wenn", "der", "J\u00e4\u00b7ger", "kommt", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bringt das Wildbret f\u00fcr den Tisch,", "tokens": ["Und", "bringt", "das", "Wild\u00b7bret", "f\u00fcr", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird erst das Mahl zum leckern Schmause,", "tokens": ["Wird", "erst", "das", "Mahl", "zum", "le\u00b7ckern", "Schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den J\u00e4ger \u00fcberrascht der Fisch.", "tokens": ["Den", "J\u00e4\u00b7ger", "\u00fc\u00b7berr\u00b7ascht", "der", "Fisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Es haben sich die m\u00fcden R\u00fcden", "tokens": ["Es", "ha\u00b7ben", "sich", "die", "m\u00fc\u00b7den", "R\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im hohen Gras zur Ruh' gelegt,", "tokens": ["Im", "ho\u00b7hen", "Gras", "zur", "Ruh'", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil auch den J\u00e4gersmann, den m\u00fcden,", "tokens": ["Weil", "auch", "den", "J\u00e4\u00b7gers\u00b7mann", ",", "den", "m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Laub' in k\u00fchlem Schatten hegt,", "tokens": ["Die", "Laub'", "in", "k\u00fch\u00b7lem", "Schat\u00b7ten", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Er horcht, entschlummernd, auf das Gleiten", "tokens": ["Er", "horcht", ",", "ent\u00b7schlum\u00b7mernd", ",", "auf", "das", "Glei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Stroms, der leis' hinunterzieht.", "tokens": ["Des", "Stroms", ",", "der", "leis'", "hin\u00b7un\u00b7ter\u00b7zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebste schmiegt sich ihm zur Seiten", "tokens": ["Die", "Liebs\u00b7te", "schmiegt", "sich", "ihm", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wiegt ihn ein mit einem Lied:", "tokens": ["Und", "wiegt", "ihn", "ein", "mit", "ei\u00b7nem", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ihr Hirsch' im gr\u00fcnen Wald, ihr Rehe,", "tokens": ["Ihr", "Hirsch'", "im", "gr\u00fc\u00b7nen", "Wald", ",", "ihr", "Re\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun lagert euch an k\u00fchler Flut", "tokens": ["Nun", "la\u00b7gert", "euch", "an", "k\u00fch\u00b7ler", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sorget nicht, da\u00df euch geschehe", "tokens": ["Und", "sor\u00b7get", "nicht", ",", "da\u00df", "euch", "ge\u00b7sche\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Leid, denn euer Sch\u00fctze ruht!", "tokens": ["Ein", "Leid", ",", "denn", "eu\u00b7er", "Sch\u00fct\u00b7ze", "ruht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Du schau' mir, hohe Mittagsonne,", "tokens": ["Du", "schau'", "mir", ",", "ho\u00b7he", "Mit\u00b7ta\u00b7gson\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht durch die laub'ge Nacht herein;", "tokens": ["Nicht", "durch", "die", "laub'\u00b7ge", "Nacht", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was du sp\u00e4hst von unsrer Wonne,", "tokens": ["Und", "was", "du", "sp\u00e4hst", "von", "uns\u00b7rer", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das la\u00df der Welt verschwiegen sein!", "tokens": ["Das", "la\u00df", "der", "Welt", "ver\u00b7schwie\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ihr Stromeswellen, die ihr rauschet", "tokens": ["Ihr", "Stro\u00b7mes\u00b7wel\u00b7len", ",", "die", "ihr", "rau\u00b7schet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus ins Land vom gr\u00fcnen Wald,", "tokens": ["Hin\u00