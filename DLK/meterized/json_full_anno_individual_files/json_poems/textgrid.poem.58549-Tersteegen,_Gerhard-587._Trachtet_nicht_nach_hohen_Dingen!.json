{"textgrid.poem.58549": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "587. Trachtet nicht nach hohen Dingen!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00f6hen der Vernunft, ihr bestes Kunstgeb\u00e4ude,", "tokens": ["Die", "H\u00f6\u00b7hen", "der", "Ver\u00b7nunft", ",", "ihr", "bes\u00b7tes", "Kunst\u00b7ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stellt die verfallne Mauer, so hoch, so voll Gefahr,", "tokens": ["Stellt", "die", "ver\u00b7fall\u00b7ne", "Mau\u00b7er", ",", "so", "hoch", ",", "so", "voll", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Worauf man m\u00fchsam steigt und pl\u00f6tzlich f\u00e4llet, dar;", "tokens": ["Wo\u00b7rauf", "man", "m\u00fch\u00b7sam", "steigt", "und", "pl\u00f6tz\u00b7lich", "f\u00e4l\u00b7let", ",", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PIS", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schau doch den bangen Fall! Des wundern sich die Leute.", "tokens": ["Schau", "doch", "den", "ban\u00b7gen", "Fall", "!", "Des", "wun\u00b7dern", "sich", "die", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "PRF", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Dort rennt ein K\u00fchner hin ins weite Feld hinein", "tokens": ["Dort", "rennt", "ein", "K\u00fch\u00b7ner", "hin", "ins", "wei\u00b7te", "Feld", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auf einem wilden Tier, verachtend Zaum und Z\u00fcgel:", "tokens": ["Auf", "ei\u00b7nem", "wil\u00b7den", "Tier", ",", "ver\u00b7ach\u00b7tend", "Zaum", "und", "Z\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der falschen Freiheit Bild; sollt's nicht gef\u00e4hrlich sein?", "tokens": ["Der", "fal\u00b7schen", "Frei\u00b7heit", "Bild", ";", "sollt's", "nicht", "ge\u00b7f\u00e4hr\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Geistesh\u00f6hen auch sieht man in diesem Spiegel,", "tokens": ["Die", "Geis\u00b7tes\u00b7h\u00f6\u00b7hen", "auch", "sieht", "man", "in", "die\u00b7sem", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Schau das Subtile nur, wie auf des Tempels H\u00f6h", "to