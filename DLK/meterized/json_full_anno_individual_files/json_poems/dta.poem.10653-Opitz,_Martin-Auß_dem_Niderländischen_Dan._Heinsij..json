{"dta.poem.10653": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Au\u00df dem Niderl\u00e4ndischen   Dan. Heinsij.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die Schiffer so vffs Meer die schwebendt Heuser bawen/", "tokens": ["Die", "Schif\u00b7fer", "so", "vffs", "Meer", "die", "schwe\u00b7bendt", "Heu\u00b7ser", "ba\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "ART", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Vnd jhren k\u00fcnen Leib den blossen Winden trawen/", "tokens": ["Vnd", "jhren", "k\u00fc\u00b7nen", "Leib", "den", "blos\u00b7sen", "Win\u00b7den", "tra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Sein kommen an das Land von aller Noth befreyt/", "tokens": ["Sein", "kom\u00b7men", "an", "das", "Land", "von", "al\u00b7ler", "Noth", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So fahren sie zu Port in Lust vnd Fr\u00f6ligkeit.", "tokens": ["So", "fah\u00b7ren", "sie", "zu", "Port", "in", "Lust", "vnd", "Fr\u00f6\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df/ wo wir sind/ eh wir zusammen vns gesellen/", "tokens": ["Da\u00df", "/", "wo", "wir", "sind", "/", "eh", "wir", "zu\u00b7sam\u00b7men", "vns", "ge\u00b7sel\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "PPER", "VAFIN", "$(", "KOUS", "PPER", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist eine w\u00fcste See: Die sorgen/ sind die Wellen/", "tokens": ["Ist", "ei\u00b7ne", "w\u00fcs\u00b7te", "See", ":", "Die", "sor\u00b7gen", "/", "sind", "die", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "$(", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Lieb/ ist vnser Wind/ die Klippen vnd die Stein/", "tokens": ["Die", "Lieb", "/", "ist", "vn\u00b7ser", "Wind", "/", "die", "Klip\u00b7pen", "vnd", "die", "Stein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist da wir allermeist darauff beflissen sein/", "tokens": ["Ist", "da", "wir", "al\u00b7ler\u00b7meist", "dar\u00b7auff", "be\u00b7flis\u00b7sen", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "PPER", "ADV", "PAV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Klippen sein gestellt in vnsers Lebens mitten/", "tokens": ["Die", "Klip\u00b7pen", "sein", "ge\u00b7stellt", "in", "vn\u00b7sers", "Le\u00b7bens", "mit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VVPP", "APPR", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da wirdt man allerseits vom wilden Meer bestritten/", "tokens": ["Da", "wirdt", "man", "al\u00b7ler\u00b7seits", "vom", "wil\u00b7den", "Meer", "be\u00b7strit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dann kompt der Westwind an gar lieblich sanfft vnd still/", "tokens": ["Dann", "kompt", "der", "West\u00b7wind", "an", "gar", "lieb\u00b7lich", "sanfft", "vnd", "still", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJD", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der kan vns in verderb einf\u00fchren/ wie er will.", "tokens": ["Der", "kan", "vns", "in", "ver\u00b7derb", "ein\u00b7f\u00fch\u00b7ren", "/", "wie", "er", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "ADJD", "VVINF", "$(", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Sternen/ die wir sehn/ das sind der Augen Strahlen/", "tokens": ["Die", "Ster\u00b7nen", "/", "die", "wir", "sehn", "/", "das", "sind", "der", "Au\u00b7gen", "Strah\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVINF", "$(", "PDS", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die vns von rechtem lauff verf\u00fchrn zu vielen mahlen/", "tokens": ["Die", "vns", "von", "rech\u00b7tem", "lauff", "ver\u00b7f\u00fchrn", "zu", "vie\u00b7len", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVINF", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dann treugt vns der Compas/ dann treuget vns die Lufft/", "tokens": ["Dann", "treugt", "vns", "der", "Com\u00b7pas", "/", "dann", "treu\u00b7get", "vns", "die", "Lufft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df wir in grosse Noth gerathen vnverhofft.", "tokens": ["Da\u00df", "wir", "in", "gros\u00b7se", "Noth", "ge\u00b7ra\u00b7then", "vn\u00b7ver\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Ruder ist Verstandt/ der Ancker/ Witz der Jugendt/", "tokens": ["Das", "Ru\u00b7der", "ist", "Ver\u00b7standt", "/", "der", "An\u00b7cker", "/", "Witz", "der", "Ju\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "$(", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.18": {"text": "Die Segel/ H\u00f6ffligkeit/ das Schifferseil/ die Tugendt/", "tokens": ["Die", "Se\u00b7gel", "/", "H\u00f6ff\u00b7lig\u00b7keit", "/", "das", "Schif\u00b7fer\u00b7seil", "/", "die", "Tu\u00b7gendt", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dann der geringste theil bringt mit gl\u00fcckhaffter Hand", "tokens": ["Dann", "der", "ge\u00b7rings\u00b7te", "theil", "bringt", "mit", "gl\u00fcck\u00b7haff\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sein vnbewegtes Schiff ohn ansto\u00df an das Land.", "tokens": ["Sein", "vn\u00b7be\u00b7weg\u00b7tes", "Schiff", "ohn", "an\u00b7sto\u00df", "an", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vnd vnser Breutigam kompt von des Schiffes Sande/", "tokens": ["Vnd", "vn\u00b7ser", "Breu\u00b7ti\u00b7gam", "kompt", "von", "des", "Schif\u00b7fes", "San\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Durch Wellen/ Wind/ vnd Stein/ vnd durch die See zu Lande/", "tokens": ["Durch", "Wel\u00b7len", "/", "Wind", "/", "vnd", "Stein", "/", "vnd", "durch", "die", "See", "zu", "Lan\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "$(", "KON", "NN", "$(", "KON", "APPR", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Zu Lande kompt er an/ befreyt von aller Noth/", "tokens": ["Zu", "Lan\u00b7de", "kompt", "er", "an", "/", "be\u00b7freyt", "von", "al\u00b7ler", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "VVPP", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Von Schiffbruch vnd Gefahr/ von Furchte f\u00fcr dem Todt.", "tokens": ["Von", "Schiff\u00b7bruch", "vnd", "Ge\u00b7fahr", "/", "von", "Furch\u00b7te", "f\u00fcr", "dem", "Todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das ist der letzte Wunsch: Ihr habt das Land nun innen/", "tokens": ["Das", "ist", "der", "letz\u00b7te", "Wunsch", ":", "Ihr", "habt", "das", "Land", "nun", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da euch der Ostwind nicht mehr wirdt verwerffen k\u00fcnnen.", "tokens": ["Da", "euch", "der", "Ost\u00b7wind", "nicht", "mehr", "wirdt", "ver\u00b7werf\u00b7fen", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "VAFIN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Herr Breutigam werfft au\u00df den Ancker in das Tieff/", "tokens": ["Herr", "Breu\u00b7ti\u00b7gam", "werfft", "au\u00df", "den", "An\u00b7cker", "in", "das", "Tieff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vnd f\u00fcr der Vngest\u00fcmm versichert ewer Schiff/", "tokens": ["Vnd", "f\u00fcr", "der", "Vn\u00b7ge\u00b7st\u00fcmm", "ver\u00b7si\u00b7chert", "e\u00b7wer", "Schiff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir sind noch in dem Meer/ darauff wir folgen sollen/", "tokens": ["Wir", "sind", "noch", "in", "dem", "Meer", "/", "dar\u00b7auff", "wir", "fol\u00b7gen", "sol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$(", "PAV", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wo vns der wilde Wind vnd Wellen haben wollen/", "tokens": ["Wo", "vns", "der", "wil\u00b7de", "Wind", "vnd", "Wel\u00b7len", "ha\u00b7ben", "wol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "KON", "NN", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Adieu/ vnd wann jhr dann in Lust vnd freuden steht/", "tokens": ["A\u00b7die\u00b7u", "/", "vnd", "wann", "jhr", "dann", "in", "Lust", "vnd", "freu\u00b7den", "steht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "PWAV", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "Gedenckt auch wie es vns mit vnsern Schiffen geht.", "tokens": ["Ge\u00b7denckt", "auch", "wie", "es", "vns", "mit", "vn\u00b7sern", "Schif\u00b7fen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "KOKOM", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}