{"dta.poem.14689": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Nonne und der Ritter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201efernher mich die Wellen tragen,", "tokens": ["\u201e", "fern\u00b7her", "mich", "die", "Wel\u00b7len", "tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die an's Land so traurig schlagen", "tokens": ["Die", "an's", "Land", "so", "trau\u00b7rig", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Deines Fensters Gitter,", "tokens": ["Un\u00b7ter", "Dei\u00b7nes", "Fens\u00b7ters", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fraue, kennst Du noch den Ritter?\u201c", "tokens": ["Frau\u00b7e", ",", "kennst", "Du", "noch", "den", "Rit\u00b7ter", "?", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}