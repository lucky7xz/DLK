{"dta.poem.650": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Satz", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Hat nichts das seiner zungen sey zugleichen ", "tokens": ["Hat", "nichts", "das", "sei\u00b7ner", "zun\u00b7gen", "sey", "zu\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein wildes Vieh' entdeckt mit stummen zeichen. ", "tokens": ["Ein", "wil\u00b7des", "Vieh'", "ent\u00b7deckt", "mit", "stum\u00b7men", "zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "De\u00df innern hertzens sinn; mit worten herrschen wir! ", "tokens": ["De\u00df", "in\u00b7nern", "hert\u00b7zens", "sinn", ";", "mit", "wor\u00b7ten", "herr\u00b7schen", "wir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der T\u00fcrme Last/ vnd was das Land beschwert. ", "tokens": ["Der", "T\u00fcr\u00b7me", "Last", "/", "vnd", "was", "das", "Land", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "KON", "PWS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Schiffe baw'/ vnd was die See durchf\u00e4hrt/ ", "tokens": ["Der", "Schif\u00b7fe", "ba\u00b7w'", "/", "vnd", "was", "die", "See", "durch\u00b7f\u00e4hrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "PWS", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der Sternen grosse krafft/ ", "tokens": ["Der", "Ster\u00b7nen", "gros\u00b7se", "krafft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was Lufft vnd flamme schafft/ ", "tokens": ["Was", "Lufft", "vnd", "flam\u00b7me", "schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Was Chloris l\u00e4st in jhren g\u00e4rtten schawen/ ", "tokens": ["Was", "Chlo\u00b7ris", "l\u00e4st", "in", "jhren", "g\u00e4rt\u00b7ten", "scha\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Was das gesetzte Recht von allen V\u00f6lckern wil. ", "tokens": ["Was", "das", "ge\u00b7setz\u00b7te", "Recht", "von", "al\u00b7len", "V\u00f6l\u00b7ckern", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was G", "tokens": ["Was", "G"], "token_info": ["word", "word"], "pos": ["PWS", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Was durch die zeit verfiel was in der bl\u00fctte steht ", "tokens": ["Was", "durch", "die", "zeit", "ver\u00b7fiel", "was", "in", "der", "bl\u00fct\u00b7te", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird durch di\u00df werckzeug nur entdecket. ", "tokens": ["Wird", "durch", "di\u00df", "wer\u00b7ck\u00b7zeug", "nur", "ent\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDS", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Freundschafft/ die todt vnd ende schrecket/ ", "tokens": ["Freund\u00b7schafft", "/", "die", "todt", "vnd", "en\u00b7de", "schre\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Macht/ die wildes Volck zu sitt&#7869; hat gezwung&#7869;/ ", "tokens": ["Die", "Macht", "/", "die", "wil\u00b7des", "Volck", "zu", "sitt", "&#7869;", "hat", "ge\u00b7zwung", "&#7869;", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "PTKZU", "VVFIN", "$(", "VAFIN", "NN", "$(", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "De\u00df Menschen leben selbst; beruht auf seiner zung&#7869;. ", "tokens": ["De\u00df", "Men\u00b7schen", "le\u00b7ben", "selbst", ";", "be\u00b7ruht", "auf", "sei\u00b7ner", "zung", "&#7869;", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}