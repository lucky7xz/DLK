{"dta.poem.19496": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Reisebecher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Gestern fand ich, r\u00e4umend eines langvergessnen Schrankes F\u00e4cher,", "tokens": ["Ge\u00b7stern", "fand", "ich", ",", "r\u00e4u\u00b7mend", "ei\u00b7nes", "lang\u00b7ver\u00b7gess\u00b7nen", "Schran\u00b7kes", "F\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Den vom Vater mir vererbten, meinen ersten Reisebecher.", "tokens": ["Den", "vom", "Va\u00b7ter", "mir", "ver\u00b7erb\u00b7ten", ",", "mei\u00b7nen", "ers\u00b7ten", "Rei\u00b7se\u00b7be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "W\u00e4hrendde\u00df ich leise singend reinigt' ihn vom Staub der Jahre,", "tokens": ["W\u00e4h\u00b7rend\u00b7de\u00df", "ich", "lei\u00b7se", "sin\u00b7gend", "rei\u00b7nigt'", "ihn", "vom", "Staub", "der", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "War's als h\u00f6be mir ein Bergwind aus der Stirn die grauen Haare,", "tokens": ["Wa\u00b7r's", "als", "h\u00f6\u00b7be", "mir", "ein", "Berg\u00b7wind", "aus", "der", "Stirn", "die", "grau\u00b7en", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.5": {"text": "War's als dufteten die Matten, drein ich schlummernd lag versunken,", "tokens": ["Wa\u00b7r's", "als", "duf\u00b7te\u00b7ten", "die", "Mat\u00b7ten", ",", "drein", "ich", "schlum\u00b7mernd", "lag", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "VVFIN", "ART", "NN", "$,", "ADV", "PPER", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "War's als rauschten alle Quelle, draus ich wandernd einst getrunken.", "tokens": ["Wa\u00b7r's", "als", "rauschten", "al\u00b7le", "Quel\u00b7le", ",", "draus", "ich", "wan\u00b7dernd", "einst", "ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "VVFIN", "PIAT", "NN", "$,", "PAV", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}