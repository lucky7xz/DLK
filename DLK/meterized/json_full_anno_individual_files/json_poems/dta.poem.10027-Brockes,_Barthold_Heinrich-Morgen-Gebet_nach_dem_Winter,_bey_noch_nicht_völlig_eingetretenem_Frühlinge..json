], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bem\u00fchet seyn mit Lust zu dienen!", "tokens": ["Be\u00b7m\u00fc\u00b7het", "seyn", "mit", "Lust", "zu", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb da\u00df ich, weil auch er von Dir hervorgebracht,", "tokens": ["Gieb", "da\u00df", "ich", ",", "weil", "auch", "er", "von", "Dir", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "$,", "KOUS", "ADV", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und er, so wol als ich, ein Vorwurff Deiner Macht,", "tokens": ["Und", "er", ",", "so", "wol", "als", "ich", ",", "ein", "Vor\u00b7wurff", "Dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "ADV", "KOUS", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch, selbst in ihm, Dein Lob vermehre", "tokens": ["Auch", ",", "selbst", "in", "ihm", ",", "Dein", "Lob", "ver\u00b7meh\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Dich in meinem R\u00e4chsten ehre!", "tokens": ["Und", "Dich", "in", "mei\u00b7nem", "R\u00e4chs\u00b7ten", "eh\u0