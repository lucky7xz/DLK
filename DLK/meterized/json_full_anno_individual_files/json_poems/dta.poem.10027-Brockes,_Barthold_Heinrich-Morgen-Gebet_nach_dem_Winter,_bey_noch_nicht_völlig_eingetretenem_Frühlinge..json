{"dta.poem.10027": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Gebet nach dem Winter,  \n bey noch nicht v\u00f6llig eingetretenem Fr\u00fchlinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Aug' er\u00f6ffnet sich, nach abgewichner Nacht,", "tokens": ["Mein", "Aug'", "er\u00b7\u00f6ff\u00b7net", "sich", ",", "nach", "ab\u00b7ge\u00b7wich\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Zeit, da die Natur, (nachdem die Finsternissen", "tokens": ["Zur", "Zeit", ",", "da", "die", "Na\u00b7tur", ",", "(", "nach\u00b7dem", "die", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "$,", "$(", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Winters, nebst dem Schnee und Eise, weichen m\u00fcssen,)", "tokens": ["Des", "Win\u00b7ters", ",", "nebst", "dem", "Schnee", "und", "Ei\u00b7se", ",", "wei\u00b7chen", "m\u00fcs\u00b7sen", ",", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "KON", "NN", "$,", "VVINF", "VMINF", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Schlaff\u2019 auch gleichsam aufgewacht.", "tokens": ["Vom", "Schlaff'", "auch", "gleich\u00b7sam", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Jahres D\u00e4mmerung ist allbereit erschienen,", "tokens": ["Des", "Jah\u00b7res", "D\u00e4m\u00b7me\u00b7rung", "ist", "all\u00b7be\u00b7reit", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die uns des Fr\u00fchlings Morgen-Licht", "tokens": ["Die", "uns", "des", "Fr\u00fch\u00b7lings", "Mor\u00b7gen\u00b7Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An den Sapphirnen Himmels-B\u00fchnen,", "tokens": ["An", "den", "Sap\u00b7phir\u00b7nen", "Him\u00b7mels\u00b7B\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und auch auf unsrer Welt, verspricht.", "tokens": ["Und", "auch", "auf", "uns\u00b7rer", "Welt", ",", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich dancke Dir, o HERR! mit tausend Freuden,", "tokens": ["Ich", "dan\u00b7cke", "Dir", ",", "o", "HeRR", "!", "mit", "tau\u00b7send", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zumahl zu dieser Zeit, da Nacht und Winter scheiden,", "tokens": ["Zu\u00b7mahl", "zu", "die\u00b7ser", "Zeit", ",", "da", "Nacht", "und", "Win\u00b7ter", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Du mich, nach so sanfter Rast,", "tokens": ["Da\u00df", "Du", "mich", ",", "nach", "so", "sanf\u00b7ter", "Rast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nebst allen Meinigen, gesund erwecket hast.", "tokens": ["Nebst", "al\u00b7len", "Mei\u00b7ni\u00b7gen", ",", "ge\u00b7sund", "er\u00b7we\u00b7cket", "hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da Du so manche Noth und Plage, Fluth und Brand,", "tokens": ["Da", "Du", "so", "man\u00b7che", "Noth", "und", "Pla\u00b7ge", ",", "Fluth", "und", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von uns, in dieser Nacht, so gn\u00e4dig abgewandt:", "tokens": ["Von", "uns", ",", "in", "die\u00b7ser", "Nacht", ",", "so", "gn\u00e4\u00b7dig", "ab\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PDAT", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da wir das Morgen-Licht, nur blo\u00df durch Dich, so sch\u00f6n,", "tokens": ["Da", "wir", "das", "Mor\u00b7gen\u00b7Licht", ",", "nur", "blo\u00df", "durch", "Dich", ",", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ADV", "ADV", "APPR", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit aufgekl\u00e4rtem Geist und Blicken wieder sehn.", "tokens": ["Mit", "auf\u00b7ge\u00b7kl\u00e4r\u00b7tem", "Geist", "und", "Bli\u00b7cken", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! la\u00df mich diese Gnad\u2019, als ein Geschencke, sch\u00e4tzen,", "tokens": ["Ach", "!", "la\u00df", "mich", "die\u00b7se", "Gnad'", ",", "als", "ein", "Ge\u00b7schen\u00b7cke", ",", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "PDAT", "NN", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Ehre, Preis und Danckens wehrt,", "tokens": ["Das", "Eh\u00b7re", ",", "Preis", "und", "Dan\u00b7ckens", "wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und wof\u00fcr darin Dir der beste Danck geh\u00f6rt,", "tokens": ["Und", "wo\u00b7f\u00fcr", "da\u00b7rin", "Dir", "der", "bes\u00b7te", "Danck", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn wir an Deinem Werck\u2019 uns laben und ergetzen!", "tokens": ["Wenn", "wir", "an", "Dei\u00b7nem", "Werck", "uns", "la\u00b7ben", "und", "er\u00b7get\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erweget denn auch heut, ihr Sinnen, und bedencket,", "tokens": ["Er\u00b7we\u00b7get", "denn", "auch", "heut", ",", "ihr", "Sin\u00b7nen", ",", "und", "be\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie GOTT den grossen Bau der Land-und Wasser-Welt", "tokens": ["Wie", "GoTT", "den", "gros\u00b7sen", "Bau", "der", "Lan\u00b7d\u00b7und", "Was\u00b7ser\u00b7Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ART", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In G\u00f6ttlich starcken Finger h\u00e4lt,", "tokens": ["In", "G\u00f6tt\u00b7lich", "star\u00b7cken", "Fin\u00b7ger", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und recht, als wie am Z\u00fcgel, lencket.", "tokens": ["Und", "recht", ",", "als", "wie", "am", "Z\u00fc\u00b7gel", ",", "len\u00b7cket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da iede Fl\u00e4che sich von unsrer Sonne zwar", "tokens": ["Da", "ie\u00b7de", "Fl\u00e4\u00b7che", "sich", "von", "uns\u00b7rer", "Son\u00b7ne", "zwar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zuweilen wol in etwas trennen,", "tokens": ["Zu\u00b7wei\u00b7len", "wol", "in", "et\u00b7was", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jedoch nicht \u00fcm ein einzigs Haar", "tokens": ["Je\u00b7doch", "nicht", "\u00fcm", "ein", "ein\u00b7zigs", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zuweit von ihr entfernen k\u00f6nnen.", "tokens": ["Zu\u00b7weit", "von", "ihr", "ent\u00b7fer\u00b7nen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erwegt die Ordnung doch, zu ihres Sch\u00f6pffers Ehren,", "tokens": ["Er\u00b7wegt", "die", "Ord\u00b7nung", "doch", ",", "zu", "ih\u00b7res", "Sch\u00f6pf\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wir schon wieder zu ihr kehren.", "tokens": ["Da", "wir", "schon", "wie\u00b7der", "zu", "ihr", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zwar sehen wir das junge Gr\u00fcn noch nicht,", "tokens": ["Zwar", "se\u00b7hen", "wir", "das", "jun\u00b7ge", "Gr\u00fcn", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf den noch nicht bewachsnen Feldern,", "tokens": ["Auf", "den", "noch", "nicht", "be\u00b7wachs\u00b7nen", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An den noch Bl\u00e4tter-losen W\u00e4ldern;", "tokens": ["An", "den", "noch", "Bl\u00e4t\u00b7ter\u00b7lo\u00b7sen", "W\u00e4l\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allein, da ich von Eis und Schnee", "tokens": ["Al\u00b7lein", ",", "da", "ich", "von", "Eis", "und", "Schnee"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Erde schon befreyet seh,", "tokens": ["Die", "Er\u00b7de", "schon", "be\u00b7fre\u00b7yet", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kann mir die Hoffnung bessrer Zeiten", "tokens": ["Kann", "mir", "die", "Hoff\u00b7nung", "bess\u00b7rer", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Schon zum voraus die Fr\u00fchlings-Lust bereiten:", "tokens": ["Schon", "zum", "vo\u00b7raus", "die", "Fr\u00fch\u00b7lings\u00b7Lust", "be\u00b7rei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zumahlen wir bereits, wie sich die Kr\u00e4ffte r\u00fchren,", "tokens": ["Zu\u00b7mah\u00b7len", "wir", "be\u00b7reits", ",", "wie", "sich", "die", "Kr\u00e4ff\u00b7te", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Theils in-theils ausser uns versp\u00fchren.", "tokens": ["Theils", "in\u00b7theils", "aus\u00b7ser", "uns", "ver\u00b7sp\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Es dringt der Safft ins Holtz: Wir sehn auf manchen Stellen", "tokens": ["Es", "dringt", "der", "Safft", "ins", "Holtz", ":", "Wir", "sehn", "auf", "man\u00b7chen", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die r\u00f6thlich-braunen Knospen schwellen.", "tokens": ["Die", "r\u00f6\u00b7th\u00b7lich\u00b7brau\u00b7nen", "Knos\u00b7pen", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Man sieht bereits, mit \u00e4msigen Vergn\u00fcgen", "tokens": ["Man", "sieht", "be\u00b7reits", ",", "mit", "\u00e4m\u00b7si\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Den Ackersmann sein Feld, worans kein Schnee mehr, pfl\u00fcgen.", "tokens": ["Den", "A\u00b7ckers\u00b7mann", "sein", "Feld", ",", "wo\u00b7rans", "kein", "Schnee", "mehr", ",", "pfl\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PWAV", "PIAT", "NN", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man pfl\u00fcgt, man s\u00e4\u2019t, man eg\u2019t, man gr\u00e4bt;", "tokens": ["Man", "pfl\u00fcgt", ",", "man", "s\u00e4't", ",", "man", "eg't", ",", "man", "gr\u00e4bt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der Acker r\u00fchret sich, das Feld ist gantz belebt,", "tokens": ["Der", "A\u00b7cker", "r\u00fch\u00b7ret", "sich", ",", "das", "Feld", "ist", "gantz", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "wor