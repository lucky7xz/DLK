{"textgrid.poem.42116": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Geh nicht vor mir...", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh nicht vor mir in dieses unbesungne", "tokens": ["Geh", "nicht", "vor", "mir", "in", "die\u00b7ses", "un\u00b7be\u00b7sung\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "APPR", "PPER", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dieses dunkle Reich, das Keiner kennt;", "tokens": ["In", "die\u00b7ses", "dunk\u00b7le", "Reich", ",", "das", "Kei\u00b7ner", "kennt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Damit Dein Name, dieser lang verklungne,", "tokens": ["Da\u00b7mit", "Dein", "Na\u00b7me", ",", "die\u00b7ser", "lang", "ver\u00b7klung\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$,", "PDAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich ihn ruf, noch Dich mit Namen nennt.", "tokens": ["Wenn", "ich", "ihn", "ruf", ",", "noch", "Dich", "mit", "Na\u00b7men", "nennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "$,", "ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vertausche nicht Dein Angesicht mit jenen", "tokens": ["Ver\u00b7tau\u00b7sche", "nicht", "Dein", "An\u00b7ge\u00b7sicht", "mit", "je\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "APPR", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ver\u00e4nderlichen aus dem fremden Kreis,", "tokens": ["Ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7chen", "aus", "dem", "frem\u00b7den", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die oft im Traum vor\u00fcbergehn und denen", "tokens": ["Die", "oft", "im", "Traum", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "und", "de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "VVINF", "KON", "PDS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich keinen Gru\u00df und keinen Wunsch mehr wei\u00df.", "tokens": ["Ich", "kei\u00b7nen", "Gru\u00df", "und", "kei\u00b7nen", "Wunsch", "mehr", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "KON", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df mich beim Brot gedenken und beim Wein,", "tokens": ["La\u00df", "mich", "beim", "Brot", "ge\u00b7den\u00b7ken", "und", "beim", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Du noch gl\u00fchst, la\u00df nicht mit Schatten-Speise,", "tokens": ["Da\u00df", "Du", "noch", "gl\u00fchst", ",", "la\u00df", "nicht", "mit", "Schat\u00b7ten\u00b7Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", 