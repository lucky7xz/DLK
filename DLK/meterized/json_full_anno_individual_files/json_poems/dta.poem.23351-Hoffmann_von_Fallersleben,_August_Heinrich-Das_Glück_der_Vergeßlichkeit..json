{"dta.poem.23351": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck der Verge\u00dflichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Dompfaff in dem Bauer sa\u00df", "tokens": ["Ein", "Dom\u00b7pfaff", "in", "dem", "Bau\u00b7er", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seinen Busch und Wald verga\u00df,", "tokens": ["Und", "sei\u00b7nen", "Busch", "und", "Wald", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hub fr\u00f6hlich an zu springen,", "tokens": ["Hub", "fr\u00f6h\u00b7lich", "an", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu pfeifen und zu singen", "tokens": ["Zu", "pfei\u00b7fen", "und", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gar h\u00fcbsch und fein nach Kunstmanier:", "tokens": ["Gar", "h\u00fcbsch", "und", "fein", "nach", "Kunst\u00b7ma\u00b7nier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eein freies Leben f\u00fchren wir.\u201c", "tokens": ["\u201e", "ein", "frei\u00b7es", "Le\u00b7ben", "f\u00fch\u00b7ren", "wir", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Menschen seid doch ebenso,", "tokens": ["Ihr", "Men\u00b7schen", "seid", "doch", "e\u00b7ben\u00b7so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr thut so frei, so frisch und froh \u2014", "tokens": ["Ihr", "thut", "so", "frei", ",", "so", "frisch", "und", "froh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr m\u00fc\u00dft im K\u00e4fich springen", "tokens": ["Ihr", "m\u00fc\u00dft", "im", "K\u00e4\u00b7fich", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hebt doch an zu singen", "tokens": ["Und", "hebt", "doch", "an", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie dieses unvern\u00fcnft'ge Thier:", "tokens": ["Wie", "die\u00b7ses", "un\u00b7ver\u00b7n\u00fcnft'\u00b7ge", "Thier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eein freies Leben f\u00fchren wir.\u201c", "tokens": ["\u201e", "ein", "frei\u00b7es", "Le\u00b7ben", "f\u00fch\u00b7ren", "wir", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}