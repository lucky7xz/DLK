{"textgrid.poem.33891": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Unbekannte", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die D\u00e4mmerung war l\u00e4ngst herein gebrochen,", "tokens": ["Die", "D\u00e4m\u00b7me\u00b7rung", "war", "l\u00e4ngst", "her\u00b7ein", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hatt' dich nie geseh'n, du tratst heran,", "tokens": ["Ich", "hatt'", "dich", "nie", "ge\u00b7seh'n", ",", "du", "tratst", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da hat dein Mund manch mildes Wort gesprochen", "tokens": ["Da", "hat", "dein", "Mund", "manch", "mil\u00b7des", "Wort", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In heil'gem Ernst, der dir mein Herz gewann.", "tokens": ["In", "heil'\u00b7gem", "Ernst", ",", "der", "dir", "mein", "Herz", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Still, wie du nahtest, hast du dich erhoben", "tokens": ["Still", ",", "wie", "du", "nah\u00b7test", ",", "hast", "du", "dich", "er\u00b7ho\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sanft uns Allen gute Nacht gesagt,", "tokens": ["Und", "sanft", "uns", "Al\u00b7len", "gu\u00b7te", "Nacht", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein Bild war tief von Finsterni\u00df umwoben,", "tokens": ["Dein", "Bild", "war", "tief", "von", "Fins\u00b7ter\u00b7ni\u00df", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach deinem Namen hab' ich nicht gefragt.", "tokens": ["Nach", "dei\u00b7nem", "Na\u00b7men", "hab'", "ich", "nicht", "ge\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun wird mein Auge nimmer dich erkennen,", "tokens": ["Nun", "wird", "mein", "Au\u00b7ge", "nim\u00b7mer", "dich", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du auch einst vor\u00fcber gehst an mir,", "tokens": ["Wenn", "du", "auch", "einst", "vor\u00b7\u00fc\u00b7ber", "gehst", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6r' ich dich von fremder Lippe nennen,", "tokens": ["Und", "h\u00f6r'", "ich", "dich", "von", "frem\u00b7der", "Lip\u00b7pe", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sagt dein Name selbst mir Nichts von dir.", "tokens": ["So", "sagt", "dein", "Na\u00b7me", "selbst", "mir", "Nichts", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PPER", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und dennoch wirst du ewig in mir leben,", "tokens": ["Und", "den\u00b7noch", "wirst", "du", "e\u00b7wig", "in", "mir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gleichwie ein Ton lebt in der stillen Luft,", "tokens": ["Gleich\u00b7wie", "ein", "Ton", "lebt", "in", "der", "stil\u00b7len", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und kann ich Form dir und Gestalt nicht geben,", "tokens": ["Und", "kann", "ich", "Form", "dir", "und", "Ge\u00b7stalt", "nicht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "PPER", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So rei\u00dft auch keine Form dich in die Gruft.", "tokens": ["So", "rei\u00dft", "auch", "kei\u00b7ne", "Form", "dich", "in", "die", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Leben hat geheimni\u00dfvolle Stunden,", "tokens": ["Das", "Le\u00b7ben", "hat", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "D'rin thut, selbst herrschend, die Natur sich kund;", "tokens": ["D'\u00b7rin", "thut", ",", "selbst", "herr\u00b7schend", ",", "die", "Na\u00b7tur", "sich", "kund", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "VVPP", "$,", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Da bluten wir und f\u00fchlen keine Wunden,", "tokens": ["Da", "blu\u00b7ten", "wir", "und", "f\u00fch\u00b7len", "kei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da freu'n wir uns und freu'n uns ohne Grund.", "tokens": ["Da", "freu'n", "wir", "uns", "und", "freu'n", "uns", "oh\u00b7ne", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "PPER", "APPR", "NN", "$."], "