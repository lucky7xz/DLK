{"dta.poem.10772": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ode  \n  Von de\u00df Todes gewi\u00dfheit/ vnd der Tu-  \n gentvnsterblichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Mann findet nichts vollkommen in der Welt/", "tokens": ["Mann", "fin\u00b7det", "nichts", "voll\u00b7kom\u00b7men", "in", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir Menschen sein mit sorgen pein vnd plagen", "tokens": ["Wir", "Men\u00b7schen", "sein", "mit", "sor\u00b7gen", "pein", "vnd", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PPOSAT", "APPR", "NN", "NE", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "All orch vnd Zeit/ in St\u00e4tten/ auff dem Feldt/", "tokens": ["All", "orch", "vnd", "Zeit", "/", "in", "St\u00e4t\u00b7ten", "/", "auff", "dem", "Feldt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$(", "APPR", "NN", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Himmel/ Lufft/ Meer/ vnd vns selbst geschlagen:", "tokens": ["Vom", "Him\u00b7mel", "/", "Lufft", "/", "Meer", "/", "vnd", "vns", "selbst", "ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ja auch der G\u00f6tter Macht", "tokens": ["Ja", "auch", "der", "G\u00f6t\u00b7ter", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hat jhr Wohnung vollkommen", "tokens": ["Hat", "jhr", "Woh\u00b7nung", "voll\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd Seelig nit gemacht.", "tokens": ["Vnd", "See\u00b7lig", "nit", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wer hat nicht wargenommen/", "tokens": ["Wer", "hat", "nicht", "war\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wie Sonn vnd Mon gemein", "tokens": ["Wie", "Sonn", "vnd", "Mon", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Verfinstern jhren schein?", "tokens": ["Ver\u00b7fins\u00b7tern", "jhren", "schein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Vnd wie de\u00df Himmels Zeichen", "tokens": ["Vnd", "wie", "de\u00df", "Him\u00b7mels", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Offt Mangelhafft verbleichen?", "tokens": ["Offt", "Man\u00b7gel\u00b7hafft", "ver\u00b7blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mit wie vil angst/ gefahren/ m\u00fch vnd noth", "tokens": ["Mit", "wie", "vil", "angst", "/", "ge\u00b7fah\u00b7ren", "/", "m\u00fch", "vnd", "noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "KOKOM", "ADV", "VVPP", "$(", "VVPP", "$(", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sein ohn abla\u00df wir Menschen vmbgegeben?", "tokens": ["Sein", "ohn", "ab\u00b7la\u00df", "wir", "Men\u00b7schen", "vmb\u00b7ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Diese mit List man vbergibt dem Todt", "tokens": ["Die\u00b7se", "mit", "List", "man", "vberg\u00b7ibt", "dem", "Todt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NE", "PIS", "VVFIN", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Jener Hertzhafft verkrieget selbst sein Leben/", "tokens": ["Je\u00b7ner", "Hertz\u00b7hafft", "ver\u00b7krie\u00b7get", "selbst", "sein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Dieser au\u00df vil verdru\u00df", "tokens": ["Die\u00b7ser", "au\u00df", "vil", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "PIAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.18": {"text": "Vnd trawren wil verderben/", "tokens": ["Vnd", "traw\u00b7ren", "wil", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Jener erbermlich mu\u00df", "tokens": ["Je\u00b7ner", "er\u00b7berm\u00b7lich", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJD", "VMFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.20": {"text": "In der Gef\u00e4ngnis sterben/", "tokens": ["In", "der", "Ge\u00b7f\u00e4ng\u00b7nis", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Etlich d\u00fcrstig nach Gut", "tokens": ["Et\u00b7lich", "d\u00fcrs\u00b7tig", "nach", "Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Fliehen vor der Armuth/", "tokens": ["Flie\u00b7hen", "vor", "der", "Ar\u00b7muth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Vnd jhren Geitz versincken/", "tokens": ["Vnd", "jhren", "Geitz", "ver\u00b7sin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Wann sie im Meer ertrincken.", "tokens": ["Wann", "sie", "im", "Meer", "er\u00b7trin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Diese mit Wasser/ Gifft/ Schwert/ oder Strick", "tokens": ["Die\u00b7se", "mit", "Was\u00b7ser", "/", "Gifft", "/", "Schwert", "/", "o\u00b7der", "Strick"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "APPR", "NN", "$(", "NN", "$(", "NE", "$(", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.26": {"text": "Selbst vber sich ein schrecklich Vrthel sprechen/", "tokens": ["Selbst", "vber", "sich", "ein", "schreck\u00b7lich", "Vr\u00b7thel", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ART", "ADJD", "NN", "VVINF", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Vnd rettend sich von zu schwerem vngl\u00fcck", "tokens": ["Vnd", "ret\u00b7tend", "sich", "von", "zu", "schwe\u00b7rem", "vn\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PRF", "APPR", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Zweifflen sie nicht sich wieder sich zu rechen.", "tokens": ["Zweiff\u00b7len", "sie", "nicht", "sich", "wie\u00b7der", "sich", "zu", "re\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PRF", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.29": {"text": "Jene kommend mit zwang", "tokens": ["Je\u00b7ne", "kom\u00b7mend", "mit", "zwang"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJD", "APPR", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "In dieses lebens leiden/", "tokens": ["In", "die\u00b7ses", "le\u00b7bens", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Finden gleich den au\u00dfgang/", "tokens": ["Fin\u00b7den", "gleich", "den", "au\u00df\u00b7gang", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "Vnd andre m\u00fch vermeiden/", "tokens": ["Vnd", "and\u00b7re", "m\u00fch", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Oder sich in jhr Grab/", "tokens": ["O\u00b7der", "sich", "in", "jhr", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Ehe sie einige Gaab", "tokens": ["E\u00b7he", "sie", "ei\u00b7ni\u00b7ge", "Ga\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "De\u00df Tags Seelig genie\u00dfen/", "tokens": ["De\u00df", "Tags", "See\u00b7lig", "ge\u00b7nie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$("], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "In Mutter Leib beschlie\u00dfen.", "tokens": ["In", "Mut\u00b7ter", "Leib", "be\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Der Todt gewi\u00df klopffet mit einem Bein", "tokens": ["Der", "Todt", "ge\u00b7wi\u00df", "klopf\u00b7fet", "mit", "ei\u00b7nem", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "An grosser Herrn Wolckentragende Schl\u00f6sser/", "tokens": ["An", "gros\u00b7ser", "Herrn", "Wol\u00b7cken\u00b7tra\u00b7gen\u00b7de", "Schl\u00f6s\u00b7ser", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Vnd armer Leut liegende H\u00fcttelein/", "tokens": ["Vnd", "ar\u00b7mer", "Leut", "lie\u00b7gen\u00b7de", "H\u00fct\u00b7tel\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.40": {"text": "Vnd ist f\u00fcr beed weder b\u00f6ser noch besser.", "tokens": ["Vnd", "ist", "f\u00fcr", "beed", "we\u00b7der", "b\u00f6\u00b7ser", "noch", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NE", "KON", "ADJD", "ADV", "ADJD", "$."], "meter": "+-----+--+-", "measure": "trochaic.tri.relaxed"}, "line.41": {"text": "Den Leib ein Tod allein", "tokens": ["Den", "Leib", "ein", "Tod", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Mit vnheilbaren plagen", "tokens": ["Mit", "vn\u00b7heil\u00b7ba\u00b7ren", "pla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Vnentfliehlicher pein", "tokens": ["Vnent\u00b7flieh\u00b7li\u00b7cher", "pein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.44": {"text": "Vnd vndienstlichen klagen", "tokens": ["Vnd", "vn\u00b7di\u00b7enst\u00b7li\u00b7chen", "kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Engstiget Tag vnd Nacht/", "tokens": ["Engs\u00b7ti\u00b7get", "Tag", "vnd", "Nacht", "/"], "