{"dta.poem.2366": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Hat sich nun dein Rad gedrehet/", "tokens": ["Hat", "sich", "nun", "dein", "Rad", "ge\u00b7dre\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du o mehr als falsches Gl\u00fckk?", "tokens": ["Du", "o", "mehr", "als", "fal\u00b7sches", "Gl\u00fckk", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat dein Westwind au\u00dfgewehet/", "tokens": ["Hat", "dein", "West\u00b7wind", "au\u00df\u00b7ge\u00b7we\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ubest du nun deine T\u00fckk?", "tokens": ["U\u00b7best", "du", "nun", "dei\u00b7ne", "T\u00fckk", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bl\u00e4sestu nun aus dem Norden/", "tokens": ["bl\u00e4\u00b7se\u00b7stu", "nun", "aus", "dem", "Nor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.6": {"text": "Und bist meine Feindin worden:", "tokens": ["Und", "bist", "mei\u00b7ne", "Fein\u00b7din", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Hat sich deine Heucheley/", "tokens": ["Hat", "sich", "dei\u00b7ne", "Heu\u00b7che\u00b7ley", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nun verkehrt in Tyranney?", "tokens": ["Nun", "ver\u00b7kehrt", "in", "Ty\u00b7ran\u00b7ney", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kurtz sa\u00df ich auff deinem Wagen/", "tokens": ["Kurtz", "sa\u00df", "ich", "auff", "dei\u00b7nem", "Wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Goldgleichgl\u00e4sern Thron'/", "tokens": ["Auf", "dem", "Gold\u00b7gleich\u00b7gl\u00e4\u00b7sern", "Thron'", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich war von dir \u00fcmgetragen/", "tokens": ["Ich", "war", "von", "dir", "\u00fcm\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als wer' ich dein liebster Sohn.", "tokens": ["Als", "wer'", "ich", "dein", "liebs\u00b7ter", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Jeder sch\u00e4tzte mich f\u00fcr selig/", "tokens": ["Je\u00b7der", "sch\u00e4tz\u00b7te", "mich", "f\u00fcr", "se\u00b7lig", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bi\u00df ich Nun von dir allm\u00e4hlich", "tokens": ["Bi\u00df", "ich", "Nun", "von", "dir", "all\u00b7m\u00e4h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eh' ichs dacht in meinem Sinn'/", "tokens": ["Eh'", "ichs", "dacht", "in", "mei\u00b7nem", "Sinn'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hoch herab gest\u00fcrtzetbin.", "tokens": ["Hoch", "her\u00b7ab", "ge\u00b7st\u00fcrt\u00b7zet\u00b7bin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch es ist ja deine Weise/", "tokens": ["Doch", "es", "ist", "ja", "dei\u00b7ne", "Wei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bletbst nirgends fest bestehn/", "tokens": ["Du", "bletbst", "nir\u00b7gends", "fest", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fassestu heut' eine Reise", "tokens": ["Fas\u00b7ses\u00b7tu", "heut'", "ei\u00b7ne", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wilst hin in Oosten gehn", "tokens": ["Und", "wilst", "hin", "in", "O\u00b7os\u00b7ten", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Morgen sieht man dich im Westen.", "tokens": ["Mor\u00b7gen", "sieht", "man", "dich", "im", "Wes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn man meint man steh' am besten/", "tokens": ["Wenn", "man", "meint", "man", "steh'", "am", "bes\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PIS", "VVFIN", "PTKA", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "und man sey dein liebster Freund", "tokens": ["und", "man", "sey", "dein", "liebs\u00b7ter", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "So bistu der \u00e4rgste Feind.", "tokens": ["So", "bis\u00b7tu", "der", "\u00e4rgs\u00b7te", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Polikrat der Samianer", "tokens": ["Po\u00b7li\u00b7krat", "der", "Sa\u00b7mi\u00b7a\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df von dir du falsches Gl\u00fckk/", "tokens": ["Wei\u00df", "von", "dir", "du", "fal\u00b7sches", "Gl\u00fckk", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und auch der Syrakusaner", "tokens": ["und", "auch", "der", "Sy\u00b7ra\u00b7ku\u00b7sa\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dioni\u00df kennt deine T\u00fckk", "tokens": ["Dio\u00b7ni\u00df", "kennt", "dei\u00b7ne", "T\u00fckk"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Bellisar der R\u00f6msche Sieger/", "tokens": ["Bel\u00b7li\u00b7sar", "der", "R\u00f6m\u00b7sche", "Sie\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und Sapor der Persche Krieger/", "tokens": ["und", "Sa\u00b7por", "der", "Per\u00b7sche", "Krie\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Seuftzen in so manchem Land'", "tokens": ["Seuft\u00b7zen", "in", "so", "man\u00b7chem", "Land'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00fcber deinen unbestand.", "tokens": ["\u00fc\u00b7ber", "dei\u00b7nen", "un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wolte Tamerlanes steigen/", "tokens": ["Wol\u00b7te", "Ta\u00b7mer\u00b7la\u00b7nes", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf sein k\u00f6nigliches Pferd/", "tokens": ["Auf", "sein", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Pferd", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Muste Bazait sich neigen", "tokens": ["Mus\u00b7te", "Ba\u00b7zait", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PRF", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "und sich legen auf die Erd'", "tokens": ["und", "sich", "le\u00b7gen", "auf", "die", "Erd'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "und so lange bleiben liegen/", "tokens": ["und", "so", "lan\u00b7ge", "blei\u00b7ben", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bi\u00df der K\u00f6nig aufgestiegen/", "tokens": ["Bi\u00df", "der", "K\u00f6\u00b7nig", "auf\u00b7ge\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da Er doch vor kurtzer Zeit/", "tokens": ["Da", "Er", "doch", "vor", "kurt\u00b7zer", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "War in gleicher Herligkeit.", "tokens": ["War", "in", "glei\u00b7cher", "Her\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hat Orontes nicht erfahren/", "tokens": ["Hat", "O\u00b7ron\u00b7tes", "nicht", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ber\u00fchmt' Armenian/", "tokens": ["Der", "be\u00b7r\u00fchmt'", "Ar\u00b7me\u00b7ni\u00b7an", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "und Orbeche zahrt von jahren/", "tokens": ["und", "Or\u00b7be\u00b7che", "zahrt", "von", "jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was du ihnen hast gethan?", "tokens": ["Was", "du", "ih\u00b7nen", "hast", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach wie hastu Sie gest\u00fcrtzet/", "tokens": ["Ach", "wie", "has\u00b7tu", "Sie", "ge\u00b7st\u00fcrt\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und ihr Gl\u00fckke so verw\u00fcrtzet!", "tokens": ["und", "ihr", "Gl\u00fck\u00b7ke", "so", "ver\u00b7w\u00fcrt\u00b7zet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df man sast dergleichen That", "tokens": ["Da\u00df", "man", "sast", "derg\u00b7lei\u00b7chen", "That"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PIS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Niemals noch geh\u00f6ret hat.", "tokens": ["Nie\u00b7mals", "noch", "ge\u00b7h\u00f6\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dr\u00fcm wol dem der dir nicht trauer", "tokens": ["Dr\u00fcm", "wol", "dem", "der", "dir", "nicht", "trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "PRELS", "PPER", "PTKNEG", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir du wankelhafftes Weib/", "tokens": ["Dir", "du", "wan\u00b7kel\u00b7haff\u00b7tes", "Weib", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der nicht auf dein Schmeicheln bauet", "tokens": ["Der", "nicht", "auf", "dein", "Schmei\u00b7cheln", "bau\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und vertraut dir seinen Leib.", "tokens": ["und", "ver\u00b7traut", "dir", "sei\u00b7nen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer auff dich sein Gl\u00fckke setzet", "tokens": ["Wer", "auff", "dich", "sein", "Gl\u00fck\u00b7ke", "set\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der wird oft von dir verletzet/", "tokens": ["Der", "wird", "oft", "von", "dir", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer dir glaubt schreibt in den Sand/", "tokens": ["Wer", "dir", "glaubt", "schreibt", "in", "den", "Sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "und bepfl\u00fcgt ein Klippenland.", "tokens": ["und", "be\u00b7pf\u00b7l\u00fcgt", "ein", "Klip\u00b7pen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}