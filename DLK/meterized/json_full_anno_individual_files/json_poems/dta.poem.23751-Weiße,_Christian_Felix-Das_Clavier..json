{"dta.poem.23751": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das Clavier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfert\u00f6nendes Clavier,", "tokens": ["S\u00fc\u00b7\u00dfer\u00b7t\u00f6\u00b7nen\u00b7des", "Cla\u00b7vier", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche Freuden schaffst du mir!", "tokens": ["Wel\u00b7che", "Freu\u00b7den", "schaffst", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Einsamkeit gebricht", "tokens": ["In", "der", "Ein\u00b7sam\u00b7keit", "ge\u00b7bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir es an Erg\u00f6tzen nicht;", "tokens": ["Mir", "es", "an", "Er\u00b7g\u00f6t\u00b7zen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du bist, was ich selber will,", "tokens": ["Du", "bist", ",", "was", "ich", "sel\u00b7ber", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bald Erweckung und bald Spiel.", "tokens": ["Bald", "Er\u00b7we\u00b7ckung", "und", "bald", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Scherz ich, so ert\u00f6net mir", "tokens": ["Scherz", "ich", ",", "so", "er\u00b7t\u00f6\u00b7net", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein scherzhaftes Lied von dir;", "tokens": ["Ein", "scherz\u00b7haf\u00b7tes", "Lied", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich aber traurig seyn,", "tokens": ["Will", "ich", "a\u00b7ber", "trau\u00b7rig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagend stimmst du mit mir ein;", "tokens": ["Kla\u00b7gend", "stimmst", "du", "mit", "mir", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heb ich fromme Lieder an,", "tokens": ["Heb", "ich", "from\u00b7me", "Lie\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie erhaben klingst du dann!", "tokens": ["Wie", "er\u00b7ha\u00b7ben", "klingst", "du", "dann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Niemals \u00f6ffne meine Brust", "tokens": ["Nie\u00b7mals", "\u00f6ff\u00b7ne", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der Lockung falscher Lust!", "tokens": ["Sich", "der", "Lo\u00b7ckung", "fal\u00b7scher", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Freuden m\u00fcssen rein,", "tokens": ["Mei\u00b7ne", "Freu\u00b7den", "m\u00fcs\u00b7sen", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So wie deine Saiten seyn,", "tokens": ["So", "wie", "dei\u00b7ne", "Sai\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und mein ganzes Leben nie", "tokens": ["Und", "mein", "gan\u00b7zes", "Le\u00b7ben", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne s\u00fc\u00dfe Harmonie.", "tokens": ["Oh\u00b7ne", "s\u00fc\u00b7\u00dfe", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}