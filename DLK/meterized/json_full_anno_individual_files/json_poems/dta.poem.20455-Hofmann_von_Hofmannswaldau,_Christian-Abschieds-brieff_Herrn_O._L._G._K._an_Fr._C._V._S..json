{"dta.poem.20455": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-brieff Herrn O. L. G. K.  \n an Fr. C. V. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein letzter abschieds-brieff wirfft sich zu deinen f\u00fcssen/", "tokens": ["Mein", "letz\u00b7ter", "ab\u00b7schieds\u00b7brieff", "wirfft", "sich", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verg\u00f6nne seiner noth ein gn\u00e4dig angesicht;", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "sei\u00b7ner", "noth", "ein", "gn\u00e4\u00b7dig", "an\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mehr thr\u00e4nen siehest du als schwartze dinte fliessen/", "tokens": ["Mehr", "thr\u00e4\u00b7nen", "sie\u00b7hest", "du", "als", "schwart\u00b7ze", "din\u00b7te", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das siegel ist das blut/ so aus den wunden bricht.", "tokens": ["Das", "sie\u00b7gel", "ist", "das", "blut", "/", "so", "aus", "den", "wun\u00b7den", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df nicht/ ob er dir erbarmni\u00df wird erwecken/", "tokens": ["Ich", "wei\u00df", "nicht", "/", "ob", "er", "dir", "er\u00b7barm\u00b7ni\u00df", "wird", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df wei\u00df ich/ da\u00df ich es mehr als zu viel verdient/", "tokens": ["Di\u00df", "wei\u00df", "ich", "/", "da\u00df", "ich", "es", "mehr", "als", "zu", "viel", "ver\u00b7dient", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PPER", "PIAT", "KOKOM", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht zwar/ da\u00df mich die furcht des todes solt erschrecken/", "tokens": ["Nicht", "zwar", "/", "da\u00df", "mich", "die", "furcht", "des", "to\u00b7des", "solt", "er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nein! di\u00df betr\u00fcbt mich nur/ da\u00df lieb im hertzen gr\u00fcnt.", "tokens": ["Nein", "!", "di\u00df", "be\u00b7tr\u00fcbt", "mich", "nur", "/", "da\u00df", "lieb", "im", "hert\u00b7zen", "gr\u00fcnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDS", "VVFIN", "PPER", "ADV", "$(", "KOUS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach m\u00f6cht ich diesen trost vor meinem ende h\u00f6ren/", "tokens": ["Ach", "m\u00f6cht", "ich", "die\u00b7sen", "trost", "vor", "mei\u00b7nem", "en\u00b7de", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die englische Charlott\u2019 erbarmt sich meiner noth/", "tokens": ["Die", "eng\u00b7li\u00b7sche", "Cha\u00b7rlott'", "er\u00b7barmt", "sich", "mei\u00b7ner", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wolt ich alles blut aus meines armes r\u00f6hren/", "tokens": ["So", "wolt", "ich", "al\u00b7les", "blut", "aus", "mei\u00b7nes", "ar\u00b7mes", "r\u00f6h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit freuden geben hin/ und k\u00fcssen meinen tod.", "tokens": ["Mit", "freu\u00b7den", "ge\u00b7ben", "hin", "/", "und", "k\u00fcs\u00b7sen", "mei\u00b7nen", "tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wolte mich vergn\u00fcgt in meinem jammer sch\u00e4tzen/", "tokens": ["Ich", "wol\u00b7te", "mich", "ver\u00b7gn\u00fcgt", "in", "mei\u00b7nem", "jam\u00b7mer", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erlaubte man mir nur zu k\u00fcssen deine hand/", "tokens": ["Er\u00b7laub\u00b7te", "man", "mir", "nur", "zu", "k\u00fcs\u00b7sen", "dei\u00b7ne", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es w\u00fcrde sich mein geist noch in der grufft ergetzen/", "tokens": ["Es", "w\u00fcr\u00b7de", "sich", "mein", "geist", "noch", "in", "der", "grufft", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Erblickt ich nur von dir ein holdes gnaden-pfand.", "tokens": ["Er\u00b7blickt", "ich", "nur", "von", "dir", "ein", "hol\u00b7des", "gna\u00b7den\u00b7pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So aber qv\u00e4l ich mich mit diesen traur-gedancken:", "tokens": ["So", "a\u00b7ber", "qv\u00e4l", "ich", "mich", "mit", "die\u00b7sen", "traur\u00b7ge\u00b7dan\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du bist in todes-noth/ sie aber wei\u00df es nicht/", "tokens": ["Du", "bist", "in", "to\u00b7des\u00b7noth", "/", "sie", "a\u00b7ber", "wei\u00df", "es", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$(", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie lacht und spielt vielleicht/ du \u00e4rmster must erkrancken/", "tokens": ["Sie", "lacht", "und", "spielt", "viel\u00b7leicht", "/", "du", "\u00e4rms\u00b7ter", "must", "er\u00b7kran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$(", "PPER", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie singt/ du aber bist zum sterben zugericht;", "tokens": ["Sie", "singt", "/", "du", "a\u00b7ber", "bist", "zum", "ster\u00b7ben", "zu\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "ADV", "VAFIN", "APPRART", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zudem so wird der neid vielleicht mich selber qv\u00e4len/", "tokens": ["Zu\u00b7dem", "so", "wird", "der", "neid", "viel\u00b7leicht", "mich", "sel\u00b7ber", "qv\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "ART", "NN", "ADV", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wer wei\u00df/ was man ihr noch nach meinem tode sagt/", "tokens": ["Wer", "wei\u00df", "/", "was", "man", "ihr", "noch", "nach", "mei\u00b7nem", "to\u00b7de", "sagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWS", "PIS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Man wird ihr di\u00df und das von meinem thun erzehlen/", "tokens": ["Man", "wird", "ihr", "di\u00df", "und", "das", "von", "mei\u00b7nem", "thun", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PDS", "KON", "ART", "APPR", "PPOSAT", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil ein verg\u00e4lltes maul nicht nach den todten fragt.", "tokens": ["Weil", "ein", "ver\u00b7g\u00e4ll\u00b7tes", "maul", "nicht", "nach", "den", "tod\u00b7ten", "fragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Di\u00df eisen/ gn\u00e4dige/ verdoppelt meine schmertzen/", "tokens": ["Di\u00df", "ei\u00b7sen", "/", "gn\u00e4\u00b7di\u00b7ge", "/", "ver\u00b7dop\u00b7pelt", "mei\u00b7ne", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "ADJA", "$(", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und macht/ da\u00df man bey mir das blut nicht stillen kan/", "tokens": ["Und", "macht", "/", "da\u00df", "man", "bey", "mir", "das", "blut", "nicht", "stil\u00b7len", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PIS", "APPR", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Weil dieses fieber wallt in dem geqv\u00e4lten hertzen/", "tokens": ["Weil", "die\u00b7ses", "fie\u00b7ber", "wallt", "in", "dem", "ge\u00b7qv\u00e4l\u00b7ten", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nimmt der zerscho\u00dfne leib auch keine pflaster an;", "tokens": ["Nimmt", "der", "zer\u00b7scho\u00df\u00b7ne", "leib", "auch", "kei\u00b7ne", "pflas\u00b7ter", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch mu\u00df ich auch gestehn/ wenn meine noth am gr\u00f6sten/", "tokens": ["Doch", "mu\u00df", "ich", "auch", "ge\u00b7stehn", "/", "wenn", "mei\u00b7ne", "noth", "am", "gr\u00f6s\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "PPOSAT", "NN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn die verzweifflung will aus allen Orten gehn/", "tokens": ["Wenn", "die", "ver\u00b7zweif\u00b7flung", "will", "aus", "al\u00b7len", "Or\u00b7ten", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So f\u00e4ngt best\u00e4ndigkeit mich wieder an zu tr\u00f6sten/", "tokens": ["So", "f\u00e4ngt", "be\u00b7st\u00e4n\u00b7dig\u00b7keit", "mich", "wie\u00b7der", "an", "zu", "tr\u00f6s\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ADV", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Weil deine tugend mich niemahlen falsch gesehn.", "tokens": ["Weil", "dei\u00b7ne", "tu\u00b7gend", "mich", "nie\u00b7mah\u00b7len", "falsch", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der allerh\u00f6chste wei\u00df/ der die gedancken kennet/", "tokens": ["Der", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "wei\u00df", "/", "der", "die", "ge\u00b7dan\u00b7cken", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$(", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vor dem mein kranckes hertz bald rechnung geben mu\u00df/", "tokens": ["Vor", "dem", "mein", "kran\u00b7ckes", "hertz", "bald", "rech\u00b7nung", "ge\u00b7ben", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df ich vor dich allein in keuscher glut gebrennet/", "tokens": ["Da\u00df", "ich", "vor", "dich", "al\u00b7lein", "in", "keu\u00b7scher", "glut", "ge\u00b7bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und keinen augenblick bereuet diesen schlu\u00df.", "tokens": ["Und", "kei\u00b7nen", "au\u00b7gen\u00b7blick", "be\u00b7re\u00b7u\u00b7et", "die\u00b7sen", "schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Ich bitt\u2019/ erwege nur den anfang meiner liebe/", "tokens": ["Ich", "bitt'", "/", "er\u00b7we\u00b7ge", "nur", "den", "an\u00b7fang", "mei\u00b7ner", "lie\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mit was vor regung ich sie dir zum ersten wie\u00df/", "tokens": ["Mit", "was", "vor", "re\u00b7gung", "ich", "sie", "dir", "zum", "ers\u00b7ten", "wie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "PPER", "PPER", "PPER", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bedencke/ wie sie mich aus ungemeinem triebe", "tokens": ["Be\u00b7den\u00b7cke", "/", "wie", "sie", "mich", "aus", "un\u00b7ge\u00b7mei\u00b7nem", "trie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PWAV", "PPER", "PRF", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Zu rettung deiner noth ins kalte wasser ri\u00df.", "tokens": ["Zu", "ret\u00b7tung", "dei\u00b7ner", "noth", "ins", "kal\u00b7te", "was\u00b7ser", "ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was hatt\u2019 ich dazumahl vor h\u00f6llen-pein erlitten/", "tokens": ["Was", "hatt'", "ich", "da\u00b7zu\u00b7mahl", "vor", "h\u00f6l\u00b7len\u00b7pein", "er\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Als man mir eyde schwur von deinem kloster-stand/", "tokens": ["Als", "man", "mir", "ey\u00b7de", "schwur", "von", "dei\u00b7nem", "klos\u00b7ter\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Als kranckheit/ lieb\u2019 und furcht in mir so hefftig stritten/", "tokens": ["Als", "kran\u00b7ck\u00b7heit", "/", "lieb'", "und", "furcht", "in", "mir", "so", "heff\u00b7tig", "strit\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "VVFIN", "KON", "NN", "APPR", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Da\u00df ich vor schmertzen mich warhafftig nicht gekannt.", "tokens": ["Da\u00df", "ich", "vor", "schmert\u00b7zen", "mich", "war\u00b7haff\u00b7tig", "nicht", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "VVFIN", "PPER", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Was seuffzer hab ich nicht zum himmel abgeschicket/", "tokens": ["Was", "seuff\u00b7zer", "hab", "ich", "nicht", "zum", "him\u00b7mel", "ab\u00b7ge\u00b7schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Der andern ihren wunsch offt ohne bitten giebt/", "tokens": ["Der", "an\u00b7dern", "ih\u00b7ren", "wunsch", "offt", "oh\u00b7ne", "bit\u00b7ten", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "ADV", "APPR", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Doch/ da\u00df er mein gebet nicht gn\u00e4dig angeblicket/", "tokens": ["Doch", "/", "da\u00df", "er", "mein", "ge\u00b7bet", "nicht", "gn\u00e4\u00b7dig", "an\u00b7ge\u00b7bli\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Macht/ da\u00df ich dich vielleicht mehr als ihn selbst geliebt.", "tokens": ["Macht", "/", "da\u00df", "ich", "dich", "viel\u00b7leicht", "mehr", "als", "ihn", "selbst", "ge\u00b7liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "ADV", "PIAT", "KOKOM", "PPER", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.49": {"text": "Ich sterbe/ doch vergn\u00fcgt/ weil er mir di\u00df gew\u00e4hret/", "tokens": ["Ich", "ster\u00b7be", "/", "doch", "ver\u00b7gn\u00fcgt", "/", "weil", "er", "mir", "di\u00df", "ge\u00b7w\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "VVPP", "$(", "KOUS", "PPER", "PPER", "PDS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Da\u00df ich dich lebend nicht in fremder hand gesehn/", "tokens": ["Da\u00df", "ich", "dich", "le\u00b7bend", "nicht", "in", "frem\u00b7der", "hand", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "PTKNEG", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Nunmehro/ da der tod mir marck und bein verzehret/", "tokens": ["Nun\u00b7meh\u00b7ro", "/", "da", "der", "tod", "mir", "marck", "und", "bein", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "NN", "PPER", "NN", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Mag alles/ wie es GOtt und dir gef\u00e4llt/ geschehn.", "tokens": ["Mag", "al\u00b7les", "/", "wie", "es", "Gott", "und", "dir", "ge\u00b7f\u00e4llt", "/", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "$(", "PWAV", "PPER", "NN", "KON", "PPER", "VVPP", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Du darffst um meinen tod dich nicht zu tode gr\u00e4men/", "tokens": ["Du", "darffst", "um", "mei\u00b7nen", "tod", "dich", "nicht", "zu", "to\u00b7de", "gr\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Mir schaffst du keinen rath/ dir st\u00f6hrst du deine ruh/", "tokens": ["Mir", "schaffst", "du", "kei\u00b7nen", "rath", "/", "dir", "st\u00f6hrst", "du", "dei\u00b7ne", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Man mu\u00df des H\u00f6chsten macht in allen sich beqvemen/", "tokens": ["Man", "mu\u00df", "des", "H\u00f6chs\u00b7ten", "macht", "in", "al\u00b7len", "sich", "be\u00b7qve\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVFIN", "APPR", "PIAT", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Di\u00df bitt ich/ schick mir nur ein dosin thr\u00e4nen zu.", "tokens": ["Di\u00df", "bitt", "ich", "/", "schick", "mir", "nur", "ein", "do\u00b7sin", "thr\u00e4\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "VVIMP", "PPER", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Der himmel gebe dir die jahre/ so mir fehlen/", "tokens": ["Der", "him\u00b7mel", "ge\u00b7be", "dir", "die", "jah\u00b7re", "/", "so", "mir", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Er sey auff deinen trost und wahre ruh bedacht/", "tokens": ["Er", "sey", "auff", "dei\u00b7nen", "trost", "und", "wah\u00b7re", "ruh", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Er lasse dich das gl\u00fcck mit millionen zehlen/", "tokens": ["Er", "las\u00b7se", "dich", "das", "gl\u00fcck", "mit", "mil\u00b7li\u00b7o\u00b7nen", "zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Genung! das blut entgeht. Charlotte gute nacht.", "tokens": ["Ge\u00b7nung", "!", "das", "blut", "ent\u00b7geht", ".", "Char\u00b7lot\u00b7te", "gu\u00b7te", "nacht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "$.", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}