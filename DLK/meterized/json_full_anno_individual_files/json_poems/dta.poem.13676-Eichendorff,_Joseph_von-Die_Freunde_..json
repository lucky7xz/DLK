{"dta.poem.13676": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Freunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und aus dem Herzen lang verhalt'ne Flammen", "tokens": ["Und", "aus", "dem", "Her\u00b7zen", "lang", "ver\u00b7halt'\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie brechen fr\u00f6hlich in des Morgens Lohe,", "tokens": ["Sie", "bre\u00b7chen", "fr\u00f6h\u00b7lich", "in", "des", "Mor\u00b7gens", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da gr\u00fc\u00df' ich, S\u00e4nger, Dich aus Herzensgrunde!", "tokens": ["Da", "gr\u00fc\u00df'", "ich", ",", "S\u00e4n\u00b7ger", ",", "Dich", "aus", "Her\u00b7zens\u00b7grun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}