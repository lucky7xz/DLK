{"dta.poem.5272": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der aufgeqvollnen Knospen-Menge,", "tokens": ["Der", "auf\u00b7ge\u00b7qvoll\u00b7nen", "Knos\u00b7pen\u00b7Men\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihren theils erst halb-theils schon gebohrnen Kindern,", "tokens": ["Mit", "ih\u00b7ren", "theils", "erst", "halb\u00b7theils", "schon", "ge\u00b7bohr\u00b7nen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADV", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Formirten in der Luft ein duftiges Gepr\u00e4nge,", "tokens": ["For\u00b7mir\u00b7ten", "in", "der", "Luft", "ein", "duf\u00b7ti\u00b7ges", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein gr\u00fcnliches Gew\u00f6lck, ein angenehm Gewirre.", "tokens": ["Ein", "gr\u00fcn\u00b7li\u00b7ches", "Ge\u00b7w\u00f6lck", ",", "ein", "an\u00b7ge\u00b7nehm", "Ge\u00b7wir\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es gleichet alles fast geflochtnen gr\u00fcnen Netzen,", "tokens": ["Es", "glei\u00b7chet", "al\u00b7les", "fast", "ge\u00b7flocht\u00b7nen", "gr\u00fc\u00b7nen", "Net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In welchen man, mit lieblichem Geschwirre,", "tokens": ["In", "wel\u00b7chen", "man", ",", "mit", "lieb\u00b7li\u00b7chem", "Ge\u00b7schwir\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die V\u00f6gel fliegen sieht, und sich und uns ergetzen.", "tokens": ["Die", "V\u00f6\u00b7gel", "flie\u00b7gen", "sieht", ",", "und", "sich", "und", "uns", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "KON", "PRF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die\u00df junge gr\u00fcne nun, zusammt der Pracht der Bl\u00fcthe", "tokens": ["Die\u00df", "jun\u00b7ge", "gr\u00fc\u00b7ne", "nun", ",", "zu\u00b7sammt", "der", "Pracht", "der", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "ADJA", "ADV", "$,", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erf\u00fcllete mit Lust und Andacht mein Gem\u00fchte.", "tokens": ["Er\u00b7f\u00fcl\u00b7le\u00b7te", "mit", "Lust", "und", "An\u00b7dacht", "mein", "Ge\u00b7m\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich kehrte mich demnach, mit h\u00f6chst erfreutem Sinn,", "tokens": ["Ich", "kehr\u00b7te", "mich", "dem\u00b7nach", ",", "mit", "h\u00f6chst", "er\u00b7freu\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zum Ursprung aller Pracht, zum grossen Sch\u00f6pfer hin;", "tokens": ["Zum", "Ur\u00b7sprung", "al\u00b7ler", "Pracht", ",", "zum", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Besunge seine Macht, voll froher Danckbarkeit,", "tokens": ["Be\u00b7sun\u00b7ge", "sei\u00b7ne", "Macht", ",", "voll", "fro\u00b7her", "Dan\u00b7ck\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Und wuste mich f\u00fcr Anmuth kaum zu fassen,", "tokens": ["Und", "wus\u00b7te", "mich", "f\u00fcr", "An\u00b7muth", "kaum", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df er die holde Fr\u00fchlings-Zeit", "tokens": ["Da\u00df", "er", "die", "hol\u00b7de", "Fr\u00fch\u00b7lings\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mich abermahl erleben lassen.", "tokens": ["Mich", "a\u00b7ber\u00b7mahl", "er\u00b7le\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ach! rief ich, m\u00f6cht\u2019 ich das, was, blos durch dich, so sch\u00f6n,", "tokens": ["Ach", "!", "rief", "ich", ",", "m\u00f6cht'", "ich", "das", ",", "was", ",", "blos", "durch", "dich", ",", "so", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PDS", "$,", "PRELS", "$,", "ADV", "APPR", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit nimmer satter Lust, dir oft zu Ehren, sehn!", "tokens": ["Mit", "nim\u00b7mer", "sat\u00b7ter", "Lust", ",", "dir", "oft", "zu", "Eh\u00b7ren", ",", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "PPER", "ADV", "APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}