{"dta.poem.18639": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "S chertzhaffte  G edancken bey  \n den Nahmens-Tag eines guten  \n Freundes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebter Hertzens Freund! Mein gestriges Versprechen", "tokens": ["Ge\u00b7lieb\u00b7ter", "Hert\u00b7zens", "Freund", "!", "Mein", "ge\u00b7stri\u00b7ges", "Ver\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Heist meinen schlechten Kiel dir heute dienstbar seyn;", "tokens": ["Heist", "mei\u00b7nen", "schlech\u00b7ten", "Kiel", "dir", "heu\u00b7te", "dienst\u00b7bar", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch deine wehrte Hand wird hier nichts kluges brechen/", "tokens": ["Doch", "dei\u00b7ne", "wehr\u00b7te", "Hand", "wird", "hier", "nichts", "klu\u00b7ges", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "PIS", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn unsre Losung hie\u00df: Sauff aus/ schenck wieder ein/", "tokens": ["Denn", "uns\u00b7re", "Lo\u00b7sung", "hie\u00df", ":", "Sauff", "aus", "/", "schenck", "wie\u00b7der", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "NN", "PTKVZ", "$(", "VVIMP", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der edle Reben-Safft will noch nicht Abschied haben/", "tokens": ["Der", "ed\u00b7le", "Re\u00b7ben\u00b7\u00b7S\u00b7afft", "will", "noch", "nicht", "Ab\u00b7schied", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "PTKNEG", "NN", "VAFIN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und wischt in dem Gehirn die klugen Grillen aus/", "tokens": ["Und", "wischt", "in", "dem", "Ge\u00b7hirn", "die", "klu\u00b7gen", "Gril\u00b7len", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es lieget die Vernunfft im Magen mit begraben/", "tokens": ["Es", "lie\u00b7get", "die", "Ver\u00b7nunfft", "im", "Ma\u00b7gen", "mit", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und was ich gestern soff/ will heute wieder raus.", "tokens": ["Und", "was", "ich", "ge\u00b7stern", "soff", "/", "will", "heu\u00b7te", "wie\u00b7der", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$(", "VMFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.9": {"text": "Juch hey sa klinget mir noch stets vor meinen Ohren/", "tokens": ["Juch", "hey", "sa", "klin\u00b7get", "mir", "noch", "stets", "vor", "mei\u00b7nen", "Oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.it", "FM.it", "FM.it", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie ruffte doch so sch\u00f6n das Echo wieder drauf.", "tokens": ["Wie", "ruff\u00b7te", "doch", "so", "sch\u00f6n", "das", "E\u00b7cho", "wie\u00b7der", "drauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADJD", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man fri\u00dft und s\u00e4uffet gleich/ so bald man wird gebohren/", "tokens": ["Man", "fri\u00dft", "und", "s\u00e4uf\u00b7fet", "gleich", "/", "so", "bald", "man", "wird", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "ADV", "$(", "ADV", "ADV", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Drum wartet uns zur Lust auch gestern Bachus auf.", "tokens": ["Drum", "war\u00b7tet", "uns", "zur", "Lust", "auch", "ge\u00b7stern", "Ba\u00b7chus", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "NE", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Dein froher Nahmens-Tag ist n\u00fctzlicher begangen/", "tokens": ["Dein", "fro\u00b7her", "Nah\u00b7mens\u00b7Tag", "ist", "n\u00fctz\u00b7li\u00b7cher", "be\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als mancher K\u00e4yser ihn in seinen Land bestellt.", "tokens": ["Als", "man\u00b7cher", "K\u00e4y\u00b7ser", "ihn", "in", "sei\u00b7nen", "Land", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es darf den edlen Wein des P\u00f6bels Durst nicht fangen/", "tokens": ["Es", "darf", "den", "ed\u00b7len", "Wein", "des", "P\u00f6\u00b7bels", "Durst", "nicht", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil die Verschwendung nicht bey dir die Hof-Stadt h\u00e4lt.", "tokens": ["Weil", "die", "Ver\u00b7schwen\u00b7dung", "nicht", "bey", "dir", "die", "Hof\u00b7Stadt", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--++-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Nein dieser theure Tranck/ den uns der Rhein gegeben/", "tokens": ["Nein", "die\u00b7ser", "theu\u00b7re", "Tranck", "/", "den", "uns", "der", "Rhein", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDAT", "ADJA", "NN", "$(", "ART", "PPER", "ART", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Flei\u00dft lieblicher in uns als in die tolle Brut/", "tokens": ["Flei\u00dft", "lieb\u00b7li\u00b7cher", "in", "uns", "als", "in", "die", "tol\u00b7le", "Brut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "KOKOM", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es bleibet doch der Wein die Qvintessentz im Leben/", "tokens": ["Es", "blei\u00b7bet", "doch", "der", "Wein", "die", "Qvin\u00b7tes\u00b7sentz", "im", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er st\u00e4rckt die Mutter wol/ und ist dem Vater gut.", "tokens": ["Er", "st\u00e4rckt", "die", "Mut\u00b7ter", "wol", "/", "und", "ist", "dem", "Va\u00b7ter", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$(", "KON", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So sch\u00f6n als nun der Wein im Glase hat gesehen/", "tokens": ["So", "sch\u00f6n", "als", "nun", "der", "Wein", "im", "Gla\u00b7se", "hat", "ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So delicat er dir in deine Kehle flo\u00df/", "tokens": ["So", "de\u00b7li\u00b7cat", "er", "dir", "in", "dei\u00b7ne", "Keh\u00b7le", "flo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So sch\u00f6n wird heute dir auch die Vergn\u00fcgung stehen/", "tokens": ["So", "sch\u00f6n", "wird", "heu\u00b7te", "dir", "auch", "die", "Ver\u00b7gn\u00fc\u00b7gung", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich wei\u00df die Farbe gibt sich an der Stirne blo\u00df.", "tokens": ["Ich", "wei\u00df", "die", "Far\u00b7be", "gibt", "sich", "an", "der", "Stir\u00b7ne", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jedoch ich wil den ", "tokens": ["Je\u00b7doch", "ich", "wil", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Hier ist ein treuer Wunsch aus der getreuen Brust:", "tokens": ["Hier", "ist", "ein", "treu\u00b7er", "Wunsch", "aus", "der", "ge\u00b7treu\u00b7en", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Halt was? Es will mir gar kein netter Reim gel\u00fccken/", "tokens": ["Halt", "was", "?", "Es", "will", "mir", "gar", "kein", "net\u00b7ter", "Reim", "ge\u00b7l\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "$.", "PPER", "VMFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Drum Prosit nur einmahl/ mich deucht/ du hast gehust.", "tokens": ["Drum", "Pro\u00b7sit", "nur", "ein\u00b7mahl", "/", "mich", "deucht", "/", "du", "hast", "ge\u00b7hust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ADV", "ADV", "$(", "PPER", "VVFIN", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Halt Prosit bringt den Kopff noch endlich in die Falten/", "tokens": ["Halt", "Pro\u00b7sit", "bringt", "den", "Kopff", "noch", "end\u00b7lich", "in", "die", "Fal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nun k\u00f6mmt die Poesie gestolpert auf das Blat/", "tokens": ["Nun", "k\u00f6mmt", "die", "Poe\u00b7sie", "ge\u00b7stol\u00b7pert", "auf", "das", "Blat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Der gestrige Discurs/ den wir dabey gehalten/", "tokens": ["Der", "ge\u00b7stri\u00b7ge", "Dis\u00b7curs", "/", "den", "wir", "da\u00b7bey", "ge\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PPER", "PAV", "VVPP", "$("], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Der ist/ der meiner Noth noch ausgeholffen hat/", "tokens": ["Der", "ist", "/", "der", "mei\u00b7ner", "Noth", "noch", "aus\u00b7ge\u00b7holf\u00b7fen", "hat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "ART", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich darff es dem Papier/ so wol als dir vertrauen/", "tokens": ["Ich", "darff", "es", "dem", "Pa\u00b7pier", "/", "so", "wol", "als", "dir", "ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "$(", "ADV", "ADV", "KOUS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es ist so stumm wie du/ und dein verschwiegner Mund/", "tokens": ["Es", "ist", "so", "stumm", "wie", "du", "/", "und", "dein", "ver\u00b7schwieg\u00b7ner", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und solt es unverhofft ein fremdes Auge schauen/", "tokens": ["Und", "solt", "es", "un\u00b7ver\u00b7hofft", "ein", "frem\u00b7des", "Au\u00b7ge", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So wei\u00df es doch so viel als unser Butel-Hund.", "tokens": ["So", "wei\u00df", "es", "doch", "so", "viel", "als", "un\u00b7ser", "Bu\u00b7tel\u00b7Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nimm denn getreuer Freund! Den Wunsch aus treuen Hertzen", "tokens": ["Nimm", "denn", "ge\u00b7treu\u00b7er", "Freund", "!", "Den", "Wunsch", "aus", "treu\u00b7en", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "NN", "$.", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der Galgen werde dir in rechten zu erkandt.", "tokens": ["Der", "Gal\u00b7gen", "wer\u00b7de", "dir", "in", "rech\u00b7ten", "zu", "er\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ADJA", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Deiu Nahme heisset mich/ Herr M\u00fcller mit dir schertzen/", "tokens": ["Dei\u00b7u", "Nah\u00b7me", "heis\u00b7set", "mich", "/", "Herr", "M\u00fcl\u00b7ler", "mit", "dir", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$(", "NN", "NE", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.40": {"text": "Ich habe/ nenn' ich dich schon einen Dieb genandt/", "tokens": ["Ich", "ha\u00b7be", "/", "nenn'", "ich", "dich", "schon", "ei\u00b7nen", "Dieb", "ge\u00b7nandt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ein Dieb/ der neulich nur was kostbares gestohlen/", "tokens": ["Ein", "Dieb", "/", "der", "neu\u00b7lich", "nur", "was", "kost\u00b7ba\u00b7res", "ge\u00b7stoh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADV", "PWS", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und zur Verwegenheit es allen l\u00e4sset sehn.", "tokens": ["Und", "zur", "Ver\u00b7we\u00b7gen\u00b7heit", "es", "al\u00b7len", "l\u00e4s\u00b7set", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wenn die aus Leipzig kommt/ und will es wieder holen/", "tokens": ["Wenn", "die", "aus", "Leip\u00b7zig", "kommt", "/", "und", "will", "es", "wie\u00b7der", "ho\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NE", "VVFIN", "$(", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "(du weist wohl/ was es ist/) wie wirst du da bestehn?", "tokens": ["(", "du", "weist", "wohl", "/", "was", "es", "ist", "/", ")", "wie", "wirst", "du", "da", "be\u00b7stehn", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$(", "PWS", "PPER", "VAFIN", "$(", "$(", "PWAV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dir mu\u00df die Facult\u00e4t das H\u00e4ngen zu erkennen/", "tokens": ["Dir", "mu\u00df", "die", "Fa\u00b7cul\u00b7t\u00e4t", "das", "H\u00e4n\u00b7gen", "zu", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Du hast den Strang darzu/ so kn\u00fcpffe dich nur an:", "tokens": ["Du", "hast", "den", "Strang", "dar\u00b7zu", "/", "so", "kn\u00fcpf\u00b7fe", "dich", "nur", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PAV", "$(", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Den Galgen wil ich auch/ die sch\u00f6ne - - nennen/", "tokens": ["Den", "Gal\u00b7gen", "wil", "ich", "auch", "/", "die", "sch\u00f6\u00b7ne", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "$(", "ART", "ADJA", "$(", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "So h\u00e4nge weil ich dich Herr M\u00fcller heissen kan.", "tokens": ["So", "h\u00e4n\u00b7ge", "weil", "ich", "dich", "Herr", "M\u00fcl\u00b7ler", "heis\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "PRF", "NN", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Doch ein gehangner Dieb ist zu anatomiren;", "tokens": ["Doch", "ein", "ge\u00b7hang\u00b7ner", "Dieb", "ist", "zu", "a\u00b7nat\u00b7o\u00b7mi\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Ach! armer M\u00fcller ach! wie w\u00fcrde dirs ergehn?", "tokens": ["Ach", "!", "ar\u00b7mer", "M\u00fcl\u00b7ler", "ach", "!", "wie", "w\u00fcr\u00b7de", "dirs", "er\u00b7gehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NE", "ITJ", "$.", "PWAV", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ich seh' im Geiste schon das sch\u00f6ne Werck vollf\u00fchren/", "tokens": ["Ich", "seh'", "im", "Geis\u00b7te", "schon", "das", "sch\u00f6\u00b7ne", "Werck", "voll\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und eine Legion von Jungfern \u00fcm dich stehn.", "tokens": ["Und", "ei\u00b7ne", "Le\u00b7gi\u00b7on", "von", "Jung\u00b7fern", "\u00fcm", "dich", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Viel wollen sich das Maul zum Zeitvertreibe nehmen/", "tokens": ["Viel", "wol\u00b7len", "sich", "das", "Maul", "zum", "Zeit\u00b7ver\u00b7trei\u00b7be", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und stecken zum Voraus die Zungen gar hinein/", "tokens": ["Und", "ste\u00b7cken", "zum", "Vo\u00b7raus", "die", "Zun\u00b7gen", "gar", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "PTKVZ", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.55": {"text": "Du weist wol/ da\u00df sich jetzt die M\u00e4dgen nicht mehr sch\u00e4men/", "tokens": ["Du", "weist", "wol", "/", "da\u00df", "sich", "jetzt", "die", "M\u00e4d\u00b7gen", "nicht", "mehr", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PRF", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die - - doch der Kiel wil mehr als alle z\u00fcchtig seyn.", "tokens": ["Die", "doch", "der", "Kiel", "wil", "mehr", "als", "al\u00b7le", "z\u00fcch\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "$(", "ADV", "ART", "NE", "VMFIN", "PIS", "KOKOM", "PIAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.57": {"text": "Di\u00df wil in - - - mir die Erfahrung lehren/", "tokens": ["Di\u00df", "wil", "in", "mir", "die", "Er\u00b7fah\u00b7rung", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "$(", "$(", "$(", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.58": {"text": "Die Meisten in der Stadt sind von der sch\u00f6nen Art/", "tokens": ["Die", "Meis\u00b7ten", "in", "der", "Stadt", "sind", "von", "der", "sch\u00f6\u00b7nen", "Art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Welch tugendhaffter Geist erstaunet nicht zu h\u00f6ren/", "tokens": ["Welch", "tu\u00b7gend\u00b7haff\u00b7ter", "Geist", "er\u00b7stau\u00b7net", "nicht", "zu", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Da\u00df sich das Geile Volck wie Mann und Weibchen paart.", "tokens": ["Da\u00df", "sich", "das", "Gei\u00b7le", "Volck", "wie", "Mann", "und", "Weib\u00b7chen", "paart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "KOKOM", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Die Eine wil sich gleich mit deinem Haar staffieren/", "tokens": ["Die", "Ei\u00b7ne", "wil", "sich", "gleich", "mit", "dei\u00b7nem", "Haar", "staf\u00b7fie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Es l\u00e4st dein reiner Kopff kein kleines Thierlein sehn/", "tokens": ["Es", "l\u00e4st", "dein", "rei\u00b7ner", "Kopff", "kein", "klei\u00b7nes", "Thier\u00b7lein", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Doch manche/ die sich wil mit Favoritgen zieren/", "tokens": ["Doch", "