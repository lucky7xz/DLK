{"dta.poem.14081": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Winzer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun sah ich auch sich heben", "tokens": ["Nun", "sah", "ich", "auch", "sich", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus Nebeln unser Haus,", "tokens": ["Aus", "Ne\u00b7beln", "un\u00b7ser", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du dehntest zwischen den Reben", "tokens": ["Du", "dehn\u00b7test", "zwi\u00b7schen", "den", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dich von der Schwelle hinaus,", "tokens": ["Dich", "von", "der", "Schwel\u00b7le", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APZR", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Da funkelt' auf einmal vor Wonne", "tokens": ["Da", "fun\u00b7kelt'", "auf", "ein\u00b7mal", "vor", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Strom und Wald und Au \u2014", "tokens": ["Der", "Strom", "und", "Wald", "und", "Au"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du bist mein Morgen, meine Sonne,", "tokens": ["Du", "bist", "mein", "Mor\u00b7gen", ",", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Meine liebe, verschlafene Frau!", "tokens": ["Mei\u00b7ne", "lie\u00b7be", ",", "ver\u00b7schla\u00b7fe\u00b7ne", "Frau", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}