{"textgrid.poem.37632": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachten", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weihnachten, ach Weihnachten,", "tokens": ["Weih\u00b7nach\u00b7ten", ",", "ach", "Weih\u00b7nach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "XY", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du warst der Kinder Trost,", "tokens": ["Du", "warst", "der", "Kin\u00b7der", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die noch im Schlafe lachten,", "tokens": ["Die", "noch", "im", "Schla\u00b7fe", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Schlaf mir bald entflohst,", "tokens": ["Du", "Schlaf", "mir", "bald", "ent\u00b7flohst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Stunden hell mir schlagen,", "tokens": ["Die", "Stun\u00b7den", "hell", "mir", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wem sagen", "tokens": ["Wem", "sa\u00b7gen"], "token_info": ["word", "word"], "pos": ["PWS", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sie an den Tag so schnell,", "tokens": ["Sie", "an", "den", "Tag", "so", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mein W\u00e4chter ist da dr\u00fcben,", "tokens": ["Mein", "W\u00e4ch\u00b7ter", "ist", "da", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Er sagt mir an den Tag,", "tokens": ["Er", "sagt", "mir", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "In Schmerzen vorzu\u00fcben,", "tokens": ["In", "Schmer\u00b7zen", "vor\u00b7zu\u00b7\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Was hohe Lust vermag.", "tokens": ["Was", "ho\u00b7he", "Lust", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Zur Kirch bin ich gegangen,", "tokens": ["Zur", "Kirch", "bin", "ich", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Vergangen", "tokens": ["Ver\u00b7gan\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "War mit Verzweiflung schnell,", "tokens": ["War", "mit", "Ver\u00b7zwei\u00b7flung", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Es bleibt zur\u00fcck", "tokens": ["Es", "bleibt", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Ein sinnend Gl\u00fcck,", "tokens": ["Ein", "sin\u00b7nend", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Und in dem Traum ein tiefer Blick,", "tokens": ["Und", "in", "dem", "Traum", "ein", "tie\u00b7fer", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wie in der Kinder Aug' entz\u00fcckt,", "tokens": ["Wie", "in", "der", "Kin\u00b7der", "Aug'", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wie ich sie halb noch schlafend dr\u00fcck,", "tokens": ["Wie", "ich", "sie", "halb", "noch", "schla\u00b7fend", "dr\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "S\u00fc\u00df springt der Augen Quell.", "tokens": ["S\u00fc\u00df", "springt", "der", "Au\u00b7gen", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Traumes deutend Summen", "tokens": ["Des", "Trau\u00b7mes", "deu\u00b7tend", "Sum\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich nun ermessen kann:", "tokens": ["Ich", "nun", "er\u00b7mes\u00b7sen", "kann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll alle Lust verstummen,", "tokens": ["Soll", "al\u00b7le", "Lust", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erstirbt ein hoher Mann?", "tokens": ["Er\u00b7stirbt", "ein", "ho\u00b7her", "Mann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Thr\u00e4nenfluthen brausen", "tokens": ["Die", "Thr\u00e4\u00b7nen\u00b7flu\u00b7then", "brau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Grausen,", "tokens": ["Mit", "Grau\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Der Menschen Haus versinkt!", "tokens": ["Der", "Men\u00b7schen", "Haus", "ver\u00b7sinkt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Alte steigt als Taube", "tokens": ["Der", "Al\u00b7te", "steigt", "als", "Tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Verj\u00fcnget aus der Fluth", "tokens": ["Ver\u00b7j\u00fcn\u00b7get", "aus", "der", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mit einem gr\u00fcnen Laube", "tokens": ["Mit", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Im Schn\u00e4blein sorgsam gut,", "tokens": ["Im", "Schn\u00e4b\u00b7lein", "sorg\u00b7sam", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Auf einem Buch sie sitzet,", "tokens": ["Auf", "ei\u00b7nem", "Buch", "sie", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Das blitzet,", "tokens": ["Das", "blit\u00b7zet", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Und schwimmt und nicht ertrinkt,", "tokens": ["Und", "schwimmt", "und", "nicht", "er\u00b7trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Mit Perlen ist", "tokens": ["Mit", "Per\u00b7len", "ist"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Beschlagen, wi\u00dft,", "tokens": ["Be\u00b7schla\u00b7gen", ",", "wi\u00dft", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Das war's, was da der Alte liest,", "tokens": ["Das", "wa\u00b7r's", ",", "was", "da", "der", "Al\u00b7te", "liest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Als er die arme Neugier gr\u00fc\u00dft;", "tokens": ["Als", "er", "die", "ar\u00b7me", "Neu\u00b7gier", "gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dies Buch such auf, du frommer Christ,", "tokens": ["Dies", "Buch", "such", "auf", ",", "du", "from\u00b7mer", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Das dir den Frieden bringt.", "tokens": ["Das", "dir", "den", "Frie\u00b7den", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Schmerzensfluthen weichen,", "tokens": ["Die", "Schmer\u00b7zens\u00b7flut\u00b7hen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Berg bleibt unverletzt,", "tokens": ["Der", "Berg", "bleibt", "un\u00b7ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die neuen Menschen gleichen", "tokens": ["Die", "neu\u00b7en", "Men\u00b7schen", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den St\u00e4mmen, die versetzt,", "tokens": ["Den", "St\u00e4m\u00b7men", ",", "die", "ver\u00b7setzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es treibt sie edler Leben,", "tokens": ["Es", "treibt", "sie", "ed\u00b7ler", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie geben", "tokens": ["Sie", "ge\u00b7ben"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nun edle Fr\u00fcchte nur.", "tokens": ["Nun", "ed\u00b7le", "Fr\u00fcch\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es wird aus Erdenschl\u00fcnden", "tokens": ["Es", "wird", "aus", "Er\u00b7den\u00b7schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das Buch der Vorzeit mein,", "tokens": ["Das", "Buch", "der", "Vor\u00b7zeit", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und ihre schweren S\u00fcnden", "tokens": ["Und", "ih\u00b7re", "schwe\u00b7ren", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Sind abgewaschen rein,", "tokens": ["Sind", "ab\u00b7ge\u00b7wa\u00b7schen", "rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "O wollt das Trauren stillen,", "tokens": ["O", "wollt", "das", "Trau\u00b7ren", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Will f\u00fcllen", "tokens": ["Will", "f\u00fcl\u00b7len"], "token_info": ["word", "word"], "pos": ["VMFIN", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Mosaisch jede Spur,", "tokens": ["Mo\u00b7sa\u00b7isch", "je\u00b7de", "Spur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Am Boden hell", "tokens": ["Am", "Bo\u00b7den", "hell"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Der Himmelsquell", "tokens": ["Der", "Him\u00b7mels\u00b7quell"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Ist eingelegt, so Well auf Well,", "tokens": ["Ist", "ein\u00b7ge\u00b7legt", ",", "so", "Well", "auf", "Well", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADV", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Taube bleibet mein Gesell", "tokens": ["Die", "Tau\u00b7be", "blei\u00b7bet", "mein", "Ge\u00b7sell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und trinkt des Buches ew'gen Quell,", "tokens": ["Und", "trinkt", "des", "Bu\u00b7ches", "ew'\u00b7gen", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Gott's Wort in der Natur.", "tokens": ["Gott's", "Wort", "in", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Weihnachten, ach Weihnachten,", "tokens": ["Weih\u00b7nach\u00b7ten", ",", "ach", "Weih\u00b7nach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "XY", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du warst der Kinder Trost,", "tokens": ["Du", "warst", "der", "Kin\u00b7der", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die noch im Schlafe lachten,", "tokens": ["Die", "noch", "im", "Schla\u00b7fe", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Schlaf mir bald entflohst,", "tokens": ["Du", "Schlaf", "mir", "bald", "ent\u00b7flohst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Stunden hell mir schlagen,", "tokens": ["Die", "Stun\u00b7den", "hell", "mir", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wem sagen", "tokens": ["Wem", "sa\u00b7gen"], "token_info": ["word", "word"], "pos": ["PWS", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sie an den Tag so schnell,", "tokens": ["Sie", "an", "den", "Tag", "so", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct