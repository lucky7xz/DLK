{"dta.poem.245": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Jhre Antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Verwegner sey so k\u00fchne nicht/", "tokens": ["Ver\u00b7weg\u00b7ner", "sey", "so", "k\u00fch\u00b7ne", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst habt ihr nichts zu hoffen/", "tokens": ["Sonst", "habt", "ihr", "nichts", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn euch die Gedult gebricht/", "tokens": ["Und", "wenn", "euch", "die", "Ge\u00b7dult", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stehn Thor und Th\u00fcren offen/", "tokens": ["Stehn", "Thor", "und", "Th\u00fc\u00b7ren", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gedenckt bey leibe nicht von mir/", "tokens": ["Ge\u00b7denckt", "bey", "lei\u00b7be", "nicht", "von", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "VVFIN", "PTKNEG", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich hielte nichts von keuscher Zier.", "tokens": ["Ich", "hiel\u00b7te", "nichts", "von", "keu\u00b7scher", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Griff entweyhet meine Brust/", "tokens": ["Ein", "Griff", "ent\u00b7wey\u00b7het", "mei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht ihr Mahl\u2019 und Flecken/", "tokens": ["Und", "macht", "ihr", "Mahl'", "und", "Fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich halte nichts von solcher Lust/", "tokens": ["Ich", "hal\u00b7te", "nichts", "von", "sol\u00b7cher", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wil sie gern bedecken/", "tokens": ["Und", "wil", "sie", "gern", "be\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn sie im Tuch verborgen liegt/", "tokens": ["Wenn", "sie", "im", "Tuch", "ver\u00b7bor\u00b7gen", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bin ich in keuscher Lust vergn\u00fcgt.", "tokens": ["Bin", "ich", "in", "keu\u00b7scher", "Lust", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was die Natur mir zugestelt/", "tokens": ["Was", "die", "Na\u00b7tur", "mir", "zu\u00b7ge\u00b7stelt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das darf ich keinen zeigen/", "tokens": ["Das", "darf", "ich", "kei\u00b7nen", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PIAT", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und der/ so etwas von mir h\u00e4lt/", "tokens": ["Und", "der", "/", "so", "et\u00b7was", "von", "mir", "h\u00e4lt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df davon stille schweigen/", "tokens": ["Mu\u00df", "da\u00b7von", "stil\u00b7le", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sonst wird er als ein Knecht gestrafft", "tokens": ["Sonst", "wird", "er", "als", "ein", "Knecht", "ge\u00b7strafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und aus dem Dienste weggeschafft.", "tokens": ["Und", "aus", "dem", "Diens\u00b7te", "weg\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das/ was die Liebe heilig heist/", "tokens": ["Das", "/", "was", "die", "Lie\u00b7be", "hei\u00b7lig", "heist", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mu\u00df man nicht entehren/", "tokens": ["Das", "mu\u00df", "man", "nicht", "en\u00b7teh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man die Brust entbl\u00f6sset weist/", "tokens": ["Wenn", "man", "die", "Brust", "ent\u00b7bl\u00f6s\u00b7set", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wil man die Pracht verst\u00f6hren/", "tokens": ["Wil", "man", "die", "Pracht", "ver\u00b7st\u00f6h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Denn weil sie eine Gottheit ist", "tokens": ["Denn", "weil", "sie", "ei\u00b7ne", "Got\u00b7theit", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man sie durch einen Vorhang k\u00fc\u00dft.", "tokens": ["Man", "sie", "durch", "ei\u00b7nen", "Vor\u00b7hang", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}