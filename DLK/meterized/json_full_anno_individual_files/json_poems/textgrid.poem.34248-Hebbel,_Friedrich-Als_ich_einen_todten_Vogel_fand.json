{"textgrid.poem.34248": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als ich einen todten Vogel fand", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein, todtes, du darfst nicht hier am Wege verwesen!", "tokens": ["V\u00f6\u00b7glein", ",", "tod\u00b7tes", ",", "du", "darfst", "nicht", "hier", "am", "We\u00b7ge", "ver\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Immer das reizendste Bild hast du dem Wand'rer geweckt,", "tokens": ["Im\u00b7mer", "das", "rei\u00b7zends\u00b7te", "Bild", "hast", "du", "dem", "Wan\u00b7d'\u00b7rer", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wenn er dich h\u00f6rte und sah, und solltest die Schrecken der Schrecken", "tokens": ["Wenn", "er", "dich", "h\u00f6r\u00b7te", "und", "sah", ",", "und", "soll\u00b7test", "die", "Schre\u00b7cken", "der", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "---+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Jetzt ihm enth\u00fcllen? O nein! Eilig begrabe ich dich!", "tokens": ["Jetzt", "ihm", "ent\u00b7h\u00fcl\u00b7len", "?", "O", "nein", "!", "Ei\u00b7lig", "be\u00b7gra\u00b7be", "ich", "dich