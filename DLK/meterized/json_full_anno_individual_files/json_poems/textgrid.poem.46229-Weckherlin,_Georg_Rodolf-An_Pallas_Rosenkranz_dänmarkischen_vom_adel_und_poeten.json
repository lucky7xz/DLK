{"textgrid.poem.46229": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Pallas Rosenkranz d\u00e4nmarkischen vom adel und poeten", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jud\u00e4a hat einmal die Musen from und weis", "tokens": ["Ju\u00b7d\u00e4a", "hat", "ein\u00b7mal", "die", "Mu\u00b7sen", "from", "und", "weis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "ADJD", "KON", "PTKVZ"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "mit perlein, agstein, gold recht k\u00f6niglich geschm\u00fccket:", "tokens": ["mit", "per\u00b7lein", ",", "ags\u00b7tein", ",", "gold", "recht", "k\u00f6\u00b7nig\u00b7lich", "ge\u00b7schm\u00fc\u00b7cket", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "FM.la", "$,", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Griechen hielten sie hernach auf s\u00fc\u00dfe weis", "tokens": ["die", "Grie\u00b7chen", "hiel\u00b7ten", "sie", "her\u00b7nach", "auf", "s\u00fc\u00b7\u00dfe", "weis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit lachen, lieb und lust mutwilliglich verstricket;", "tokens": ["mit", "la\u00b7chen", ",", "lieb", "und", "lust", "mut\u00b7wil\u00b7lig\u00b7lich", "ver\u00b7stri\u00b7cket", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADJD", "KON", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die R\u00f6mer haben auch darauf und nicht ohn flei\u00df", "tokens": ["Die", "R\u00f6\u00b7mer", "ha\u00b7ben", "auch", "da\u00b7rauf", "und", "nicht", "ohn", "flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PAV", "KON", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit lorber und \u00f6lzweig und myrten sie erquicket,", "tokens": ["mit", "lor\u00b7ber", "und", "\u00f6l\u00b7zweig", "und", "myr\u00b7ten", "sie", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "und andre v\u00f6lker mehr, nicht ohn m\u00fch, noch ohn preis", "tokens": ["und", "and\u00b7re", "v\u00f6l\u00b7ker", "mehr", ",", "nicht", "ohn", "m\u00fch", ",", "noch", "ohn", "preis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "$,", "PTKNEG", "APPR", "ADJD", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "gl\u00fcckselig haben sie mit mancher blum begl\u00fccket.", "tokens": ["gl\u00fcck\u00b7se\u00b7lig", "ha\u00b7ben", "sie", "mit", "man\u00b7cher", "blum", "be\u00b7gl\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "PIAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als die neun schwestern nu schier jeden stands und lands", "tokens": ["Als", "die", "neun", "schwes\u00b7tern", "nu", "schier", "je\u00b7den", "stands", "und", "lands"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "CARD", "VVFIN", "ADV", "ADJD", "PIAT", "ADJA", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "gunst und kunst, tracht und pracht begehrten zu volf\u00fchren", "tokens": ["gunst", "und", "kunst", ",", "tracht", "und", "pracht", "be\u00b7gehr\u00b7ten", "zu", "vol\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "PTKVZ", "$,", "VVFIN", "KON", "NN", "VVINF", "PTKZU", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "und sich bekleiden selbs mit reichtum, zier und glanz", "tokens": ["und", "sich", "be\u00b7klei\u00b7den", "selbs", "mit", "reich\u00b7tum", ",", "zier", "und", "glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PRF", "VVFIN", "ADV", "APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie, Ph\u00f6bus und Pallas, f\u00fcrhin stets zu prachtieren,", "tokens": ["Sie", ",", "Ph\u00f6\u00b7bus", "und", "Pal\u00b7las", ",", "f\u00fcr\u00b7hin", "stets", "zu", "prach\u00b7tie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "KON", "NN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "erkoren f\u00fcr ihr haupt den edlen Rosenkranz,", "tokens": ["er\u00b7ko\u00b7ren", "f\u00fcr", "ihr", "haupt", "den", "ed\u00b7len", "Ro\u00b7sen\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.