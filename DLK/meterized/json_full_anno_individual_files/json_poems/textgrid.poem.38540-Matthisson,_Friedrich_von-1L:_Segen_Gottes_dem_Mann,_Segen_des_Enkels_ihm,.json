{"textgrid.poem.38540": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Segen Gottes dem Mann, Segen des Enkels ihm,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Segen Gottes dem Mann, Segen des Enkels ihm,", "tokens": ["Se\u00b7gen", "Got\u00b7tes", "dem", "Mann", ",", "Se\u00b7gen", "des", "En\u00b7kels", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Und die Thr\u00e4ne des Danks ferner Jahrhunderte,", "tokens": ["Und", "die", "Thr\u00e4\u00b7ne", "des", "Danks", "fer\u00b7ner", "Jahr\u00b7hun\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADV", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Welcher schattende Wipfel", "tokens": ["Wel\u00b7cher", "schat\u00b7ten\u00b7de", "Wip\u00b7fel"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dir, o fr\u00f6hlicher H\u00fcgel, gab!", "tokens": ["Dir", ",", "o", "fr\u00f6h\u00b7li\u00b7cher", "H\u00fc\u00b7gel", ",", "gab", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Und das duftende Gr\u00fcn deiner Gestr\u00e4uche, die,", "tokens": ["Und", "das", "duf\u00b7ten\u00b7de", "Gr\u00fcn", "dei\u00b7ner", "Ge\u00b7str\u00e4u\u00b7che", ",", "die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,", "PRELS", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zweig geschlungen in Zweig, labende K\u00fchlung streun,", "tokens": ["Zweig", "ge\u00b7schlun\u00b7gen", "in", "Zweig", ",", "la\u00b7ben\u00b7de", "K\u00fch\u00b7lung", "streun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und das Schnekkengewinde", "tokens": ["Und", "das", "Schnek\u00b7ken\u00b7ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deines bl\u00fchenden Hekkengangs.", "tokens": ["Dei\u00b7nes", "bl\u00fc\u00b7hen\u00b7den", "Hek\u00b7ken\u00b7gangs", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ueberirrenden Bliks, schauet mein Aug' umher,", "tokens": ["Ue\u00b7be\u00b7rir\u00b7ren\u00b7den", "Bliks", ",", "schau\u00b7et", "mein", "Aug'", "um\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Ob kein Taxusgebild oder ein Buchsbaumrand", "tokens": ["Ob", "kein", "Ta\u00b7xus\u00b7ge\u00b7bild", "o\u00b7der", "ein", "Buchs\u00b7baum\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "KON", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Deine Sch\u00f6nheiten f\u00e4lsche,", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7hei\u00b7ten", "f\u00e4l\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und, o Freude! ich sp\u00e4h' umsonst!", "tokens": ["Und", ",", "o", "Freu\u00b7de", "!", "ich", "sp\u00e4h'", "um\u00b7sonst", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ungeschminkte Natur, eben so wundersch\u00f6n", "tokens": ["Un\u00b7ge\u00b7schmink\u00b7te", "Na\u00b7tur", ",", "e\u00b7ben", "so", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "ADJD"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Als in deinem Gefild, gl\u00fckliches Schweizerland,", "tokens": ["Als", "in", "dei\u00b7nem", "Ge\u00b7fild", ",", "gl\u00fck\u00b7li\u00b7ches", "Schwei\u00b7zer\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deinen blumichten Thalen,", "tokens": ["Dei\u00b7nen", "blu\u00b7mich\u00b7ten", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und bewipfelten Felsenreihn,", "tokens": ["Und", "be\u00b7wip\u00b7fel\u00b7ten", "Fel\u00b7sen\u00b7reihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Weilt im Maiengewand, m\u00fctterlichliebevoll,", "tokens": ["Weilt", "im", "Mai\u00b7en\u00b7ge\u00b7wand", ",", "m\u00fct\u00b7ter\u00b7lich\u00b7lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Nachtigallen im Schoos, Blumen um ihrer Stirn,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "im", "Schoos", ",", "Blu\u00b7men", "um", "ih\u00b7rer", "Stirn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Hier im traulichen Schatten", "tokens": ["Hier", "im", "trau\u00b7li\u00b7chen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Melancholischen Fichtengr\u00fcns,", "tokens": ["Me\u00b7lan\u00b7cho\u00b7li\u00b7schen", "Fich\u00b7ten\u00b7gr\u00fcns", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Winkt und l\u00e4chelt mir zu, winket und l\u00e4chelt sanft,", "tokens": ["Winkt", "und", "l\u00e4\u00b7chelt", "mir", "zu", ",", "win\u00b7ket", "und", "l\u00e4\u00b7chelt", "sanft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sch\u00fcttelt Bl\u00fcthen herab, s\u00e4uselt um mich herum,", "tokens": ["Sch\u00fct\u00b7telt", "Bl\u00fc\u00b7then", "her\u00b7ab", ",", "s\u00e4u\u00b7selt", "um", "mich", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "O ich folge, ich folge,", "tokens": ["O", "ich", "fol\u00b7ge", ",", "ich", "fol\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Allbelebende G\u00f6ttin, dir!", "tokens": ["All\u00b7be\u00b7le\u00b7ben\u00b7de", "G\u00f6t\u00b7tin", ",", "dir", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Sieh! den H\u00fcgel hinan leitet die G\u00f6ttin mich,", "tokens": ["Sieh", "!", "den", "H\u00fc\u00b7gel", "hi\u00b7nan", "lei\u00b7tet", "die", "G\u00f6t\u00b7tin", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Auf dem einsamen Pfad d\u00e4mmernder Schattennacht,", "tokens": ["Auf", "dem", "ein\u00b7sa\u00b7men", "Pfad", "d\u00e4m\u00b7mern\u00b7der", "Schat\u00b7ten\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "---+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zeigt des fr\u00f6hlichen H\u00fcgels", "tokens": ["Zeigt", "des", "fr\u00f6h\u00b7li\u00b7chen", "H\u00fc\u00b7gels"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ganze F\u00fclle der Sch\u00f6nheit mir!", "tokens": ["Gan\u00b7ze", "F\u00fcl\u00b7le", "der", "Sch\u00f6n\u00b7heit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Laubgew\u00f6lbe voll Duft, welche dem Rastenden", "tokens": ["Laub\u00b7ge\u00b7w\u00f6l\u00b7be", "voll", "Duft", ",", "wel\u00b7che", "dem", "Ras\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "K\u00fchlung bieten und Ruh', Rasen dem Schlummerer,", "tokens": ["K\u00fch\u00b7lung", "bie\u00b7ten", "und", "Ruh'", ",", "Ra\u00b7sen", "dem", "Schlum\u00b7me\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ueberh\u00fcllet von Geisblatt", "tokens": ["Ue\u00b7ber\u00b7h\u00fcl\u00b7let", "von", "Geis\u00b7blatt"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Oder rankendem Wintergr\u00fcn;", "tokens": ["O\u00b7der", "ran\u00b7ken\u00b7dem", "Win\u00b7ter\u00b7gr\u00fcn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Und vom Gipfel herab, bl\u00fchend wie Edens Flur,", "tokens": ["Und", "vom", "Gip\u00b7fel", "her\u00b7ab", ",", "bl\u00fc\u00b7hend", "wie", "E\u00b7dens", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "$,", "ADJD", "KOKOM", "NE", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das gesegnete Land, welches im Goldpallast", "tokens": ["Das", "ge\u00b7se\u00b7gne\u00b7te", "Land", ",", "wel\u00b7ches", "im", "Gold\u00b7pal\u00b7last"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und in d\u00f6rflicher H\u00fctte", "tokens": ["Und", "in", "d\u00f6rf\u00b7li\u00b7cher", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Himmelselige Wohner n\u00e4hrt!", "tokens": ["Him\u00b7mel\u00b7se\u00b7li\u00b7ge", "Woh\u00b7ner", "n\u00e4hrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Sag, o H\u00fcgel, mir an, warest vom Anbeginn", "tokens": ["Sag", ",", "o", "H\u00fc\u00b7gel", ",", "mir", "an", ",", "wa\u00b7rest", "vom", "An\u00b7be\u00b7ginn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "PTKVZ", "$,", "VAFIN", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Du von D\u00fcften bew\u00f6lkt? spro\u00dften dir Blumen auf?", "tokens": ["Du", "von", "D\u00fcf\u00b7ten", "be\u00b7w\u00f6lkt", "?", "spro\u00df\u00b7ten", "dir", "Blu\u00b7men", "auf", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$.", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und entwehte die K\u00fchle", "tokens": ["Und", "ent\u00b7weh\u00b7te", "die", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deinem zitternden Laube stets?", "tokens": ["Dei\u00b7nem", "zit\u00b7tern\u00b7den", "Lau\u00b7be", "stets", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Baumlos ragtest du einst, naket und d\u00fcrftig auf,", "tokens": ["Baum\u00b7los", "rag\u00b7test", "du", "einst", ",", "na\u00b7ket", "und", "d\u00fcrf\u00b7tig", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sparsam keimte dein Gras unter dem Sand' hervor,", "tokens": ["Spar\u00b7sam", "ke\u00b7im\u00b7te", "dein", "Gras", "un\u00b7ter", "dem", "Sand'", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+---+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Disteln zischten im Winde,", "tokens": ["Dis\u00b7teln", "zischten", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und dein Gipfel war schattenleer!", "tokens": ["Und", "dein", "Gip\u00b7fel", "war", "schat\u00b7ten\u00b7leer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Aber Blumen entbl\u00fchn, Lauben bekleiden sich,", "tokens": ["A\u00b7ber", "Blu\u00b7men", "ent\u00b7bl\u00fchn", ",", "Lau\u00b7ben", "be\u00b7klei\u00b7den", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "wo