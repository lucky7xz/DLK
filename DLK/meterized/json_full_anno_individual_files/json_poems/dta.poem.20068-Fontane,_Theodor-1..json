{"dta.poem.20068": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Leben war\u2019s, mit Kolben und mit Kn\u00fctteln", "tokens": ["Ein", "Le\u00b7ben", "wa\u00b7r's", ",", "mit", "Kol\u00b7ben", "und", "mit", "Kn\u00fct\u00b7teln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In diesen eitlen Jammer drein zu schlagen,", "tokens": ["In", "die\u00b7sen", "eit\u00b7len", "Jam\u00b7mer", "drein", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch hab ich still ein l\u00e4stig Joch getragen,", "tokens": ["Doch", "hab", "ich", "still", "ein", "l\u00e4s\u00b7tig", "Joch", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meiner Pflicht gehorcht und ihren B\u00fctteln.", "tokens": ["Und", "mei\u00b7ner", "Pflicht", "ge\u00b7horcht", "und", "ih\u00b7ren", "B\u00fct\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt aber, wo an Winters Thron zu r\u00fctteln,", "tokens": ["Jetzt", "a\u00b7ber", ",", "wo", "an", "Win\u00b7ters", "Thron", "zu", "r\u00fct\u00b7teln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "APPR", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Lerchenschlag, die Fr\u00fchlingsl\u00fcfte wagen,", "tokens": ["Voll", "Ler\u00b7chen\u00b7schlag", ",", "die", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt will auch ich, und m\u00fc\u00dft\u2019 ich sie zernagen,", "tokens": ["Jetzt", "will", "auch", "ich", ",", "und", "m\u00fc\u00dft'", "ich", "sie", "zer\u00b7na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "$,", "KON", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Ketten alle muthig von mir sch\u00fctteln.", "tokens": ["Die", "Ket\u00b7ten", "al\u00b7le", "mut\u00b7hig", "von", "mir", "sch\u00fct\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Lebewohl \u2014 kein Fluch Euch, meine Dr\u00e4nger;", "tokens": ["Ein", "Le\u00b7be\u00b7wohl", "kein", "Fluch", "Euch", ",", "mei\u00b7ne", "Dr\u00e4n\u00b7ger", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PIAT", "NN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Ihr seid gesch\u00fctzt vor meines Zorns Erg\u00fcssen,", "tokens": ["Ihr", "seid", "ge\u00b7sch\u00fctzt", "vor", "mei\u00b7nes", "Zorns", "Er\u00b7g\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil ihr zu klein dem neugebornen S\u00e4nger;", "tokens": ["Weil", "ihr", "zu", "klein", "dem", "neu\u00b7ge\u00b7bor\u00b7nen", "S\u00e4n\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er eilt hinaus den jungen Lenz zu k\u00fcssen,", "tokens": ["Er", "eilt", "hin\u00b7aus", "den", "jun\u00b7gen", "Lenz", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APZR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kein Gedanke nur geh\u00f6rt Euch l\u00e4nger,", "tokens": ["Und", "kein", "Ge\u00b7dan\u00b7ke", "nur", "ge\u00b7h\u00f6rt", "Euch", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als er Euch selber hat ertragen m\u00fcssen.", "tokens": ["Als", "er", "Euch", "sel\u00b7ber", "hat", "er\u00b7tra\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}