{"textgrid.poem.41518": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Feste vom s\u00fc\u00dfen Namen Jesus", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist s\u00fc\u00df wie Honigseim,", "tokens": ["Was", "ist", "s\u00fc\u00df", "wie", "Ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er sich der Wab' entgie\u00dft?", "tokens": ["Wenn", "er", "sich", "der", "Wab'", "ent\u00b7gie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfer ist des Lebens Keim,", "tokens": ["S\u00fc\u00b7\u00dfer", "ist", "des", "Le\u00b7bens", "Keim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der durch unsre Adern flie\u00dft;", "tokens": ["Der", "durch", "uns\u00b7re", "A\u00b7dern", "flie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch dein Name, lieber Jesu mein,", "tokens": ["Doch", "dein", "Na\u00b7me", ",", "lie\u00b7ber", "Je\u00b7su", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "NE", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der ist \u00fcber alles mild und s\u00fc\u00df!", "tokens": ["Der", "ist", "\u00fc\u00b7ber", "al\u00b7les", "mild", "und", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Da\u00df der Tod vergi\u00dft die herbe Pein,", "tokens": ["Da\u00df", "der", "Tod", "ver\u00b7gi\u00dft", "die", "her\u00b7be", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wo ein frommer Mund ihn t\u00f6nen lie\u00df.", "tokens": ["Wo", "ein", "from\u00b7mer", "Mund", "ihn", "t\u00f6\u00b7nen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Was ist gleich des L\u00f6wen Kraft,", "tokens": ["Was", "ist", "gleich", "des", "L\u00f6\u00b7wen", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn er durch die W\u00e4lder kreist?", "tokens": ["Wenn", "er", "durch", "die", "W\u00e4l\u00b7der", "kreist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4rker ist die Leidenschaft,", "tokens": ["St\u00e4r\u00b7ker", "ist", "die", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der widerspenst'ge Geist;", "tokens": ["Ist", "der", "wi\u00b7der\u00b7spenst'\u00b7ge", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch dein Name, lieber Jesu mein,", "tokens": ["Doch", "dein", "Na\u00b7me", ",", "lie\u00b7ber", "Je\u00b7su", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "NE", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der ist \u00fcber alles voll der Macht!", "tokens": ["Der", "ist", "\u00fc\u00b7ber", "al\u00b7les", "voll", "der", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIS", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Da\u00df er zw\u00e4ngt zu milden Lichtes Schein", "tokens": ["Da\u00df", "er", "zw\u00e4ngt", "zu", "mil\u00b7den", "Lich\u00b7tes", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Was die Welt bedr\u00e4ut in Flammenpracht.", "tokens": ["Was", "die", "Welt", "be\u00b7dr\u00e4ut", "in", "Flam\u00b7men\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Was ist reich wie Meeresfahrt,", "tokens": ["Was", "ist", "reich", "wie", "Mee\u00b7res\u00b7fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich des Schachtes goldner Hut?", "tokens": ["Gleich", "des", "Schach\u00b7tes", "gold\u00b7ner", "Hut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reicher ist wer sich bewahrt", "tokens": ["Rei\u00b7cher", "ist", "wer", "sich", "be\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "VAFIN", "PWS", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Ehre k\u00f6stlich Gut;", "tokens": ["Sei\u00b7ner", "Eh\u00b7re", "k\u00f6st\u00b7lich", "Gut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch dein Name, lieber Jesu mein,", "tokens": ["Doch", "dein", "Na\u00b7me", ",", "lie\u00b7ber", "Je\u00b7su", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "NE", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der ist mehr und reicher als das all!", "tokens": ["Der", "ist", "mehr", "und", "rei\u00b7cher", "als", "das", "all", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "KON", "ADJD", "KOKOM", "ART", "PIAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, um ihn ertr\u00e4gt man ganz allein", "tokens": ["Ach", ",", "um", "ihn", "er\u00b7tr\u00e4gt", "man", "ganz", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUI", "PPER", "VVFIN", "PIS", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Schmach, Verkennung, aller Ehre Fall.", "tokens": ["Schmach", ",", "Ver\u00b7ken\u00b7nung", ",", "al\u00b7ler", "Eh\u00b7re", "Fall", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Was ist sch\u00f6n wie Morgenlicht,", "tokens": ["Was", "ist", "sch\u00f6n", "wie", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich dem Sternendom der Nacht?", "tokens": ["Gleich", "dem", "Ster\u00b7nen\u00b7dom", "der", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ein lieblich Angesicht,", "tokens": ["Ach", "!", "ein", "lieb\u00b7lich", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Aug' des Geistes Pracht;", "tokens": ["Und", "im", "Aug'", "des", "Geis\u00b7tes", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch dein Name, lieber Jesu mein,", "tokens": ["Doch", "dein", "Na\u00b7me", ",", "lie\u00b7ber", "Je\u00b7su", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "NE", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der ist \u00fcber alles mild und sch\u00f6n!", "tokens": ["Der", "ist", "\u00fc\u00b7ber", "al\u00b7les", "mild", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wer ihn tr\u00e4gt im stillen Antlitz sein,", "tokens": ["Wer", "ihn", "tr\u00e4gt", "im", "stil\u00b7len", "Ant\u00b7litz", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Der ist hold, was auch Natur versehn.", "tokens": ["Der", "ist", "hold", ",", "was", "auch", "Na\u00b7tur", "ver\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$,", "PRELS", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Was ist freudig wie zu ziehn", "tokens": ["Was", "ist", "freu\u00b7dig", "wie", "zu", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADJD", "KOKOM", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die reiche Welt hinaus?", "tokens": ["In", "die", "rei\u00b7che", "Welt", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! viel freud'ger was wir fliehn,", "tokens": ["Ach", "!", "viel", "freu\u00b7d'\u00b7ger", "was", "wir", "fliehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "ADJA", "PWS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das verkannte Elternhaus;", "tokens": ["Das", "ver\u00b7kann\u00b7te", "El\u00b7tern\u00b7haus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch dein Name, lieber Jesu mein,", "tokens": ["Doch", "dein", "Na\u00b7me", ",", "lie\u00b7ber", "Je\u00b7su", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "NE", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der ist \u00fcber alles voll der Lust!", "tokens": ["Der", "ist", "\u00fc\u00b7ber", "al\u00b7les", "voll", "der", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIS", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "O, wer g\u00e4b' nicht um die Freuden sein", "tokens": ["O", ",", "wer", "g\u00e4b'", "nicht", "um", "die", "Freu\u00b7den", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "PPOSAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Heimat, Freiheit, was ihm nur bewu\u00dft!", "tokens": ["Hei\u00b7mat", ",", "Frei\u00b7heit", ",", "was", "ihm", "nur", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWS", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ja, dein Name, Jesus Christ,", "tokens": ["Ja", ",", "dein", "Na\u00b7me", ",", "Je\u00b7sus", "Christ", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ist stark und reich und mild!", "tokens": ["Der", "ist", "stark", "und", "reich", "und", "mild", "!"], "token_info": 