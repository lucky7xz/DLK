{"textgrid.poem.59635": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Zumutung des Mutes", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df sie schweben", "tokens": ["La\u00df", "sie", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Deine V\u00f6gel! La\u00df sie fliegen!", "tokens": ["Dei\u00b7ne", "V\u00f6\u00b7gel", "!", "La\u00df", "sie", "flie\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolzes Leben", "tokens": ["Stol\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Tr\u00e4umet nur von Siegen.", "tokens": ["Tr\u00e4u\u00b7met", "nur", "von", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wann sie fallen", "tokens": ["Wann", "sie", "fal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nieder vor erflogner Wonne,", "tokens": ["Nie\u00b7der", "vor", "er\u00b7flog\u00b7ner", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wisse, aus der Sonne,", "tokens": ["Wis\u00b7se", ",", "aus", "der", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Die sie wollten, wird der Spott nicht schallen.", "tokens": ["Die", "sie", "woll\u00b7ten", ",", "wird", "der", "Spott", "nicht", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "$,", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wackre J\u00e4ger", "tokens": ["Wack\u00b7re", "J\u00e4\u00b7ger"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Stellen nach dem sch\u00f6nsten Wilde,", "tokens": ["Stel\u00b7len", "nach", "dem", "sch\u00f6ns\u00b7ten", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tapfre Schl\u00e4ger", "tokens": ["Tapf\u00b7re", "Schl\u00e4\u00b7ger"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Decken mit dem Schilde", "tokens": ["De\u00b7cken", "mit", "dem", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nur die Seite,", "tokens": ["Nur", "die", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "\u00dcber der die Wehr nicht schwebet,", "tokens": ["\u00dc\u00b7ber", "der", "die", "Wehr", "nicht", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die andre strebet", "tokens": ["Doch", "die", "and\u00b7re", "stre\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "PIS", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Vorw\u00e4rts stets zu Kampf und Sieg und Beute.", "tokens": ["Vor\u00b7w\u00e4rts", "stets", "zu", "Kampf", "und", "Sieg", "und", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Also strebe", "tokens": ["Al\u00b7so", "stre\u00b7be"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "In des Kampfes frische Weite!", "tokens": ["In", "des", "Kamp\u00b7fes", "fri\u00b7sche", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also hebe", "tokens": ["Al\u00b7so", "he\u00b7be"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Blanke Wehr im Streite!", "tokens": ["Blan\u00b7ke", "Wehr", "im", "Strei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Alles decken", "tokens": ["Al\u00b7les", "de\u00b7cken"], "token_info": ["word", "word"], "pos": ["PIS", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wollen nur die Feigen, Matten,", "tokens": ["Wol\u00b7len", "nur", "die", "Fei\u00b7gen", ",", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die des Todes Schatten", "tokens": ["Die", "des", "To\u00b7des", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.8": {"text": "St\u00fcndlich \u00fcberh\u00e4ngt mit bleichen Schrecken.", "tokens": ["St\u00fcnd\u00b7lich", "\u00fc\u00b7ber\u00b7h\u00e4ngt", "mit", "blei\u00b7chen", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Darum fr\u00f6hlich,", "tokens": ["Da\u00b7rum", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "K\u00fchnes, gl\u00fchendes Herz des Mutes!", "tokens": ["K\u00fch\u00b7nes", ",", "gl\u00fc\u00b7hen\u00b7des", "Herz", "des", "Mu\u00b7tes", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Darum selig,", "tokens": ["Da\u00b7rum", "se\u00b7lig", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "ADJD", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.4": {"text": "Selig freien Blutes,", "tokens": ["Se\u00b7lig", "frei\u00b7en", "Blu\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Das verronnen,", "tokens": ["Das", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wo der Helden beste fielen!", "tokens": ["Wo", "der", "Hel\u00b7den", "bes\u00b7te", "fie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00dft so deinen Sonnen,", "tokens": ["Mu\u00dft", "so", "dei\u00b7nen", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Deiner Liebe frisch entgegenspielen.", "tokens": ["Dei\u00b7ner", "Lie\u00b7be", "frisch", "ent\u00b7ge\u00b7gen\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "O so schwebet,", "tokens": ["O", "so", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Meine V\u00f6gel, sonder Zagen!", "tokens": ["Mei\u00b7ne", "V\u00f6\u00b7gel", ",", "son\u00b7der", "Za\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebet! Schwebet!", "tokens": ["Schwe\u00b7bet", "!", "Schwe\u00b7bet", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "H\u00f6her wird es tagen;", "tokens": ["H\u00f6\u00b7her", "wird", "es", "ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dort versinken", "tokens": ["Dort", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nebel, die uns unten irren:", "tokens": ["Ne\u00b7bel", ",", "die", 