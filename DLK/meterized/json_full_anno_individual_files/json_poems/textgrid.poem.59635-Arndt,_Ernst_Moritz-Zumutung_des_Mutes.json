bet", "!", "Schwe\u00b7bet", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "H\u00f6her wird es tagen;", "tokens": ["H\u00f6\u00b7her", "wird", "es", "ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dort versinken", "tokens": ["Dort", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nebel, die uns unten irren:", "tokens": ["Ne\u00b7bel", ",", "die", "uns", "un\u00b7ten", "ir\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollt ihr Sonnen trinken,", "tokens": ["Wollt", "ihr", "Son\u00b7nen", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "D\u00fcrft ihr zwischen Tag und Nacht nicht schwirren.", "tokens": ["D\u00fcrft", "ihr", "zwi\u00b7schen", "Tag", "und", "Nacht", "nicht", "schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Schwebet, schwebet,", "tokens": ["Schwe\u00b7bet", ",", "schwe\u00b7bet", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Meine V\u00f6gel, sonder Weile!", "tokens": ["Mei\u00b7ne", "V\u00f6\u00b7gel", ",", "son\u00b7der", "Wei\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flieget, strebet", "tokens": ["Flie\u00b7get", ",", "stre\u00b7bet"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Fort in Blitzeseile!", "tokens": ["Fort", "in", "Blit\u00b7ze\u00b7sei\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Blitzeskinder,", "tokens": ["Blit\u00b7zes\u00b7kin\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sollt ihr k\u00fchn in Flammen baden,", "tokens": ["Sollt", "ihr", "k\u00fchn", "in", "Flam\u00b7men", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Erden\u00fcberwinder,", "tokens": ["Er\u00b7de\u00b7n\u00fc\u00b7berw\u00b7in\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Adler, fliegen zu den Sternenpfaden.", "tokens": ["Ad\u00b7ler", ",", "flie\u00b7gen", "zu", "den", "Ster\u00b7nen\u00b7pfa\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}