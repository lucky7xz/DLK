{"dta.poem.7465": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Gesch\u00e4ftigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Einen Hauch vergeuden sie, nicht Einen,", "tokens": ["Nicht", "Ei\u00b7nen", "Hauch", "ver\u00b7geu\u00b7den", "sie", ",", "nicht", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, Alles wird gleich f\u00fcr den Markt geboren,", "tokens": ["Nein", ",", "Al\u00b7les", "wird", "gleich", "f\u00fcr", "den", "Markt", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Herzensschlag geht ohne Zins verloren,", "tokens": ["Kein", "Her\u00b7zens\u00b7schlag", "geht", "oh\u00b7ne", "Zins", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Herren machen Brod aus ihren Steinen.", "tokens": ["Die", "Her\u00b7ren", "ma\u00b7chen", "Brod", "aus", "ih\u00b7ren", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie machen Brod aus Lachen und aus Weinen \u2014", "tokens": ["Sie", "ma\u00b7chen", "Brod", "aus", "La\u00b7chen", "und", "aus", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hab' mir die Beschaulichkeit erkoren,", "tokens": ["Ich", "hab'", "mir", "die", "Be\u00b7schau\u00b7lich\u00b7keit", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und niemals streng gerechnet mit den Horen,", "tokens": ["Und", "nie\u00b7mals", "streng", "ge\u00b7rech\u00b7net", "mit", "den", "Ho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich denke fromm: \u201eGott gibt's im Schlaf den Seinen!\u201c", "tokens": ["Ich", "den\u00b7ke", "fromm", ":", "\u201e", "Gott", "gibt's", "im", "Schlaf", "den", "Sei\u00b7nen", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "NN", "VVFIN", "APPRART", "NN", "ART", "PPOSS", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich kann des Lebens banggesch\u00e4ftig Rauschen,", "tokens": ["Ich", "kann", "des", "Le\u00b7bens", "bang\u00b7ge\u00b7sch\u00e4f\u00b7tig", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die\u00df laute Thun und Treiben nicht verstehn,", "tokens": ["Die\u00df", "lau\u00b7te", "Thun", "und", "Trei\u00b7ben", "nicht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00f6cht' mein einsam Gl\u00fcck nicht drum vertauschen.", "tokens": ["Und", "m\u00f6cht'", "mein", "ein\u00b7sam", "Gl\u00fcck", "nicht", "drum", "ver\u00b7tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJD", "NN", "PTKNEG", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00dft mich die stillen Pfade weiter gehn,", "tokens": ["La\u00dft", "mich", "die", "stil\u00b7len", "Pfa\u00b7de", "wei\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wolken und der Sterne Zug belauschen,", "tokens": ["Der", "Wol\u00b7ken", "und", "der", "Ster\u00b7ne", "Zug", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sch\u00f6nen Kindern in die Augen sehn!", "tokens": ["Und", "sch\u00f6\u00b7nen", "Kin\u00b7dern", "in", "die", "Au\u00b7gen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}