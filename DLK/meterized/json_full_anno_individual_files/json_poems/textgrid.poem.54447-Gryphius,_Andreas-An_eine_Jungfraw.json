{"textgrid.poem.54447": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "An eine Jungfraw", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist Ewr zarter Mund? ein K\u00f6cher voller Pfeile/", "tokens": ["Was", "ist", "Ewr", "zar\u00b7ter", "Mund", "?", "ein", "K\u00f6\u00b7cher", "vol\u00b7ler", "Pfei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dardurch manch weiches Hertz wird bi\u00df in Todt verletzt/", "tokens": ["Dar\u00b7durch", "manch", "wei\u00b7ches", "Hertz", "wird", "bi\u00df", "in", "Todt", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der hellen Augen glantz/ ist flammen gleich gesch\u00e4tzt/", "tokens": ["Der", "hel\u00b7len", "Au\u00b7gen", "glantz", "/", "ist", "flam\u00b7men", "gleich", "ge\u00b7sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "VAFIN", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An welchem jeder sich verbrent in kurtzer weile/", "tokens": ["An", "wel\u00b7chem", "je\u00b7der", "sich", "ver\u00b7brent", "in", "kurt\u00b7zer", "wei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "PRF", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die wundersch\u00f6nen Haar sind lauter Liebes-Seyle.", "tokens": ["Die", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Haar", "sind", "lau\u00b7ter", "Lie\u00b7bes\u00b7Sey\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer durch der Stirnen gl\u00e3tz/ nicht wird jn Euch verhetzt;", "tokens": ["Wer", "durch", "der", "Stir\u00b7nen", "gl\u00e3tz", "/", "nicht", "wird", "jn", "Euch", "ver\u00b7hetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NE", "$(", "PTKNEG", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer sich den Lilien des Halses wiedersetzt/", "tokens": ["Wer", "sich", "den", "Li\u00b7li\u00b7en", "des", "Hal\u00b7ses", "wie\u00b7der\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mu\u00df doch gewertig seyn/ da\u00df Ihn der Plitz ereyle/", "tokens": ["Mu\u00df", "doch", "ge\u00b7wer\u00b7tig", "seyn", "/", "da\u00df", "Ihn", "der", "Plitz", "e\u00b7rey\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der von der blossen Brust herstralt so vnverdeckt/", "tokens": ["Der", "von", "der", "blos\u00b7sen", "Brust", "her\u00b7stralt", "so", "vn\u00b7ver\u0