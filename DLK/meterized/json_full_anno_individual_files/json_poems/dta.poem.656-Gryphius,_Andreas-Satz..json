{"dta.poem.656": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "F", "tokens": ["F"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Da\u00df die verh\u00e4ngnis vn\u00df vor vnserm zufall schrecke!", "tokens": ["Da\u00df", "die", "ver\u00b7h\u00e4ng\u00b7nis", "vn\u00df", "vor", "vn\u00b7serm", "zu\u00b7fall", "schre\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ein ", "tokens": ["Da\u00df", "ein"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Oder ists nur Phantasey/ die den m\u00fcden Geist betr\u00fcbet", "tokens": ["O\u00b7der", "ists", "nur", "Phan\u00b7ta\u00b7sey", "/", "die", "den", "m\u00fc\u00b7den", "Geist", "be\u00b7tr\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "NN", "$(", "ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Welcher/ weil er in dem C\u00f6rper: seinen eignen kummer liebet?", "tokens": ["Wel\u00b7cher", "/", "weil", "er", "in", "dem", "C\u00f6r\u00b7per", ":", "sei\u00b7nen", "eig\u00b7nen", "kum\u00b7mer", "lie\u00b7bet", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}