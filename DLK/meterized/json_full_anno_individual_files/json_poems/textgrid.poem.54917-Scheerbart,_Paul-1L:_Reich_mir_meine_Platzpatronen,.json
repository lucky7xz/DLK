{"textgrid.poem.54917": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Reich mir meine Platzpatronen,", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reich mir meine Platzpatronen,", "tokens": ["Reich", "mir", "mei\u00b7ne", "Platz\u00b7pat\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn mich packt die Raserei!", "tokens": ["Denn", "mich", "packt", "die", "Ra\u00b7se\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen Menschen will ich schonen,", "tokens": ["Kei\u00b7nen", "Men\u00b7schen", "will", "ich", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles schlag ich jetzt entzwei.", "tokens": ["Al\u00b7les", "schlag", "ich", "jetzt", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hunderttausend K\u00f6pfe rei\u00df ich", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7send", "K\u00f6p\u00b7fe", "rei\u00df", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heute noch von ihrem Rumpf!", "tokens": ["Heu\u00b7te", "noch", "von", "ih\u00b7rem", "Rumpf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hei! Das wilde Morden preis ich,", "tokens": ["Hei", "!", "Das", "wil\u00b7de", "Mor\u00b7den", "preis", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "PTKVZ", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn das ist der letzte Trumpf!", "tokens": ["Denn", "das", "ist", "der", "letz\u00b7te", "Trumpf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welt, verschrumpf!", "tokens": ["Welt", ",", "ver\u00b7schrumpf", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Reich mir meine Platzpatronen,", "tokens": ["Reich", "mir", "mei\u00b7ne", "Platz\u00b7pat\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn mich packt die Raserei!", "tokens": ["Denn", "mich", "packt", "die", "Ra\u00b7se\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen Menschen will ich schonen,", "tokens": ["Kei\u00b7nen", "Men\u00b7schen", "will", "ich", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles schlag ich jetzt entzwei.", "tokens": ["Al\u00b7les", "schlag", "ich", "jetzt", "ent\u00b7zwei", "."], "token_info":