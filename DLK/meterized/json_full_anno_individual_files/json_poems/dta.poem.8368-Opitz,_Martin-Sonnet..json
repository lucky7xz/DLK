{"dta.poem.8368": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist gewagt; ich bin doch gantz entschlo\u00dfen\nJetzt noch ein mal zue la\u00dfen vnser landt/\nVndt hin zue ziehn wo auch ist mordt vndt brandt/\nWo auch das feldt mitt blute wirdt bego\u00dfen.", "tokens": ["Es", "ist", "ge\u00b7wagt", ";", "ich", "bin", "doch", "gantz", "ent\u00b7schlo\u00b7\u00dfen", "Jetzt", "noch", "ein", "mal", "zue", "la\u00b7\u00dfen", "vn\u00b7ser", "landt", "/", "Vndt", "hin", "zue", "ziehn", "wo", "auch", "ist", "mordt", "vndt", "brandt", "/", "Wo", "auch", "das", "feldt", "mitt", "blu\u00b7te", "wirdt", "be\u00b7go\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVFIN", "ADV", "ADV", "ART", "ADV", "PTKZU", "VVINF", "KON", "ADJD", "$(", "KON", "ADV", "PTKZU", "VVINF", "PWAV", "ADV", "VAFIN", "NE", "KON", "VVFIN", "$(", "PWAV", "ADV", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+--+--+--+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist gewagt: hei\u00dft aber di\u00df geno\u00dfen\nDer liebe frucht ?ist di\u00df des feste bandt\nDer waren gunst? schl\u00e4fft deine trewe handt?\nIst deiner lust ged\u00e4chtni\u00df gantz beiflo\u00dfen?", "tokens": ["Es", "ist", "ge\u00b7wagt", ":", "hei\u00dft", "a\u00b7ber", "di\u00df", "ge\u00b7no\u00b7\u00dfen", "Der", "lie\u00b7be", "frucht", "?", "ist", "di\u00df", "des", "fes\u00b7te", "bandt", "Der", "wa\u00b7ren", "gunst", "?", "schl\u00e4fft", "dei\u00b7ne", "tre\u00b7we", "handt", "?", "Ist", "dei\u00b7ner", "lust", "ge\u00b7d\u00e4cht\u00b7ni\u00df", "gantz", "bei\u00b7flo\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "VVFIN", "ADV", "PDS", "VVFIN", "ART", "ADJA", "NN", "$.", "VAFIN", "PDS", "ART", "ADJA", "NN", "PDS", "VAFIN", "ADJD", "$.", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "VAFIN", "PPOSAT", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo bleibet dann der mundt/ die augen/ dieses haar\nVndt was sonst mehr dein trost vndt kummer war?\nWas thue ich dann? ich bin selbselbst verlohren", "tokens": ["Wo", "blei\u00b7bet", "dann", "der", "mundt", "/", "die", "au\u00b7gen", "/", "die\u00b7ses", "haar", "Vndt", "was", "sonst", "mehr", "dein", "trost", "vndt", "kum\u00b7mer", "war", "?", "Was", "thue", "ich", "dann", "?", "ich", "bin", "selb\u00b7selbst", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "ADJD", "$(", "ART", "NN", "$(", "PDAT", "NN", "KON", "PWS", "ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$.", "PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-++-++-+-+-+-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}}, "stanza.4": {"line.1": {"text": "Verlier ich sie: verbleib' ich dann allhier\nSo ist doch nichts als wanckelmuth an mir:\nIch habe recht den wolff jetzt bey den ohren.", "tokens": ["Ver\u00b7lier", "ich", "sie", ":", "ver\u00b7bleib'", "ich", "dann", "all\u00b7hier", "So", "ist", "doch", "nichts", "als", "wan\u00b7ckel\u00b7muth", "an", "mir", ":", "Ich", "ha\u00b7be", "recht", "den", "wolff", "jetzt", "bey", "den", "oh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$.", "VVFIN", "PPER", "ADV", "ADV", "ADV", "VAFIN", "ADV", "PIS", "KOKOM", "VVFIN", "APPR", "PPER", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}