{"textgrid.poem.53337": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bey diesem hochbetr\u00fcbten Leben", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey diesem hochbetr\u00fcbten Leben", "tokens": ["Bey", "die\u00b7sem", "hoch\u00b7be\u00b7tr\u00fcb\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O wol uns, da\u00df der Todt", "tokens": ["O", "wol", "uns", ",", "da\u00df", "der", "Todt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vns aller M\u00fch' und Noht", "tokens": ["Vns", "al\u00b7ler", "M\u00fch'", "und", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mus eine selig' Endschafft geben,", "tokens": ["Mus", "ei\u00b7ne", "se\u00b7lig'", "End\u00b7schafft", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd bringt uns fein aus allem Jammer", "tokens": ["Vnd", "bringt", "uns", "fein", "aus", "al\u00b7lem", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In unsre Kammer.", "tokens": ["In", "uns\u00b7re", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mehr aber wol uns wegen dessen", "tokens": ["Mehr", "a\u00b7ber", "wol", "uns", "we\u00b7gen", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PPER", "APPR", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df, sind wir gleich auch gar", "tokens": ["Da\u00df", ",", "sind", "wir", "gleich", "auch", "gar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Haut, Gebein und Haar", "tokens": ["Mit", "Haut", ",", "Ge\u00b7bein", "und", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von der Verwesung auffgefressen,", "tokens": ["Von", "der", "Ver\u00b7we\u00b7sung", "auff\u00b7ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir aus dem Staube d\u00fcrrer Erden", "tokens": ["Wir", "aus", "dem", "Stau\u00b7be", "d\u00fcr\u00b7rer", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erwachen werden.", "tokens": ["Er\u00b7wa\u00b7chen", "wer\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wenn Gottes Trompte wird erklingen", "tokens": ["Wenn", "Got\u00b7tes", "Tromp\u00b7te", "wird", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von oben aus der Lufft,", "tokens": ["Von", "o\u00b7ben", "aus", "der", "Lufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd m\u00e4chtig durch die Grufft", "tokens": ["Vnd", "m\u00e4ch\u00b7tig", "durch", "die", "Grufft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der tieffen Gr\u00e4ber selber dringen,", "tokens": ["Der", "tief\u00b7fen", "Gr\u00e4\u00b7ber", "sel\u00b7ber", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd alle Menschen, wo sie stecken,", "tokens": ["Vnd", "al\u00b7le", "Men\u00b7schen", ",", "wo", "sie", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird aufferwecken,", "tokens": ["Wird", "auf\u00b7fer\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Dann werden die verkehrten Hertzen,", "tokens": ["Dann", "wer\u00b7den", "die", "ver\u00b7kehr\u00b7ten", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So Christus nie erkand,", "tokens": ["So", "Chris\u00b7tus", "nie", "er\u00b7kand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Lohn empfangen Brand", "tokens": ["Zu", "Lohn", "emp\u00b7fan\u00b7gen", "Brand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd unau\u00dfsprechlich grosse Schmertzen,", "tokens": ["Vnd", "un\u00b7au\u00df\u00b7sprech\u00b7lich", "gros\u00b7se", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd wir, die wir Ihm angeh\u00f6ren,", "tokens": ["Vnd", "wir", ",", "die", "wir", "Ihm", "an\u00b7ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Krantz der Ehren.", "tokens": ["Den", "Krantz", "der", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Dann werden wir das Lamb umbringen,", "tokens": ["Dann", "wer\u00b7den", "wir", "das", "Lamb", "um\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm dienen Tag und Nacht,", "tokens": ["Ihm", "die\u00b7nen", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd seiner Liebe Macht", "tokens": ["Vnd", "sei\u00b7ner", "Lie\u00b7be", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In seinem Tempel ewig singen,", "tokens": ["In", "sei\u00b7nem", "Tem\u00b7pel", "e\u00b7wig", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd \u00fcber uns wird Rhu und Leben", "tokens": ["Vnd", "\u00fc\u00b7ber", "uns", "wird", "Rhu", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn Ende schweben.", "tokens": ["Ohn", "En\u00b7de", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Mit dieser Hoffnung wol versehen", "tokens": ["Mit", "die\u00b7ser", "Hoff\u00b7nung", "wol", "ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Last uns gedultig seyn,", "tokens": ["Last", "uns", "ge\u00b7dul\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mit was Gefahr und Pein", "tokens": ["Mit", "was", "Ge\u00b7fahr", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "NN", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sich Zeit und Gl\u00fcck beginnt zu blehen,", "tokens": ["Sich", "Zeit", "und", "Gl\u00fcck", "be\u00b7ginnt", "zu", "ble\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kranckheit und des Alters plagen", "tokens": ["Der", "Kran\u00b7ck\u00b7heit", "und", "des", "Al\u00b7ters", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bescheiden tragen!", "tokens": ["Be\u00b7schei\u00b7den", "tra\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Nur la\u00df uns deinen Beystand mercken,", "tokens": ["Nur", "la\u00df", "uns", "dei\u00b7nen", "Beys\u00b7tand", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Jesu, unser Hort,", "tokens": ["O", "Je\u00b7su", ",", "un\u00b7ser", "Hort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd deines Trostes Wort", "tokens": ["Vnd", "dei\u00b7nes", "Tros\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vns stets in aller Schwachheit st\u00e4rcken,", "tokens": ["Vns", "stets", "in", "al\u00b7ler", "Schwach\u00b7heit", "st\u00e4r\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lehr wieder Hell und Tod uns kriegen", "tokens": ["Lehr", "wie\u00b7der", "Hell", "und", "Tod", "uns", "krie\u00b7gen"], "token_info": ["word", "word", "w