{"textgrid.poem.58103": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "141. Wer Gott nur will, ist immer still", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Weltkind f\u00fcrchtet, dies und jenes m\u00f6chte kommen,", "tokens": ["Ein", "Welt\u00b7kind", "f\u00fcrch\u00b7tet", ",", "dies", "und", "je\u00b7nes", "m\u00f6ch\u00b7te", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PDS", "KON", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sorgt, da\u00df, was es liebt, ihm werden m\u00f6cht' entnommen;", "tokens": ["Es", "sorgt", ",", "da\u00df", ",", "was", "es", "liebt", ",", "ihm", "wer\u00b7den", "m\u00f6cht'", "ent\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,", "PWS", "PPER", "VVFIN", "$,", "PPER", "VAINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald h\u00e4tt' es dieses gern, bald jenes ihm gebricht,", "tokens": ["Bald", "h\u00e4tt'", "es", "die\u00b7ses", "gern", ",", "bald", "je\u00b7nes", "ihm", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "ADV", "$,", "ADV", "PDAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald gr\u00e4mt und qu\u00e4lt es sich, da\u00df dies und das geschicht.", "tokens": ["Bald", "gr\u00e4mt", "und", "qu\u00e4lt", "es", "sich", ",", "da\u00df", "dies", "und", "das", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "PRF", "$,", "KOUS", "PDS", "KON", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich hab' und will nur Gott, ich leb' vergn\u00fcgt und stille,", "tokens": ["Ich", "hab'", "und", "will", "nur", "Gott", ",", "ich", "leb'", "ver\u00b7gn\u00fcgt", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VMFIN", "ADV", "NN", "$,", "PPER", "VVFIN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn stets geschiehet mein und meines Vaters Wille.", "tokens": ["Denn", "stets", "ge\u00b7schie\u00b7het", "mein", "und", "mei\u00b7nes", "Va\u00b7ters", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Weltkind f\u00fcrchtet, dies und jenes m\u00f6chte kommen,", "tokens": ["Ein", "Welt\u00b7kind", "f\u00fcrch\u00b7tet", ",", "dies", "und", "je\u00b7nes", "m\u00f6ch\u00b7te", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PDS", "KON", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sorgt, da\u00df, was es liebt, ihm werden m\u00f6cht' entnommen;", "tokens": ["Es", "sorgt", ",", "da\u00df", ",", "was", "es", "liebt", ",", "ihm", "wer\u00b7den", "m\u00f6cht'", "ent\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "punct