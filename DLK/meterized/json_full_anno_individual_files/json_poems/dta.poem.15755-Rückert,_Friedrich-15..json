{"dta.poem.15755": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Dichter ist das Weib die liebste Richterin,", "tokens": ["Dem", "Dich\u00b7ter", "ist", "das", "Weib", "die", "liebs\u00b7te", "Rich\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besonders wenn sie selbst ist keine Dichterin.", "tokens": ["Be\u00b7son\u00b7ders", "wenn", "sie", "selbst", "ist", "kei\u00b7ne", "Dich\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch sei sie Dicht'rin auch, wenn sie Gef\u00fchl nur singt", "tokens": ["Doch", "sei", "sie", "Dicht'\u00b7rin", "auch", ",", "wenn", "sie", "Ge\u00b7f\u00fchl", "nur", "singt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "ADV", "$,", "KOUS", "PPER", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gem\u00e4\u00dfigtes, das rein aus ihrer Brust entspringt.", "tokens": ["Ge\u00b7m\u00e4\u00b7\u00dfig\u00b7tes", ",", "das", "rein", "aus", "ih\u00b7rer", "Brust", "ent\u00b7springt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Noch widriger als die den Schlei'r der Zucht zerrei\u00dft,", "tokens": ["Noch", "wid\u00b7ri\u00b7ger", "als", "die", "den", "Schlei'r", "der", "Zucht", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist die ausschweifender Empfindung sich beflei\u00dft.", "tokens": ["Ist", "die", "aus\u00b7schwei\u00b7fen\u00b7der", "Emp\u00b7fin\u00b7dung", "sich", "be\u00b7flei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}