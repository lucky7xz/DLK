{"textgrid.poem.35210": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[ich lieb den stillen Pfad/ die Ruh der Einsamkeit]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lieb d\u0113 still\u0113 Pfad/ die Ruh der Einsamkeit/", "tokens": ["Ich", "lieb", "d\u0113", "still\u0113", "Pfad", "/", "die", "Ruh", "der", "Ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "FM", "FM", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entfernet v\u014d gepl\u00e4r vers\u00fcsend meine Zeit.", "tokens": ["Ent\u00b7fer\u00b7net", "v\u014d", "ge\u00b7pl\u00e4r", "ver\u00b7s\u00fc\u00b7send", "mei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier hat kein Wagenrad den seltnen Weg belastet/", "tokens": ["Hier", "hat", "kein", "Wa\u00b7gen\u00b7rad", "den", "selt\u00b7nen", "Weg", "be\u00b7las\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Fisch in diesem Deich hat angelfrey gemastet/", "tokens": ["Der", "Fisch", "in", "die\u00b7sem", "Deich", "hat", "an\u00b7gel\u00b7frey", "ge\u00b7mas\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat kein Wandersmann/ in seinem Durst entbrandt/", "tokens": ["Es", "hat", "kein", "Wan\u00b7ders\u00b7mann", "/", "in", "sei\u00b7nem", "Durst", "ent\u00b7brandt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erhaben aus der Qwell hier Wasser mit der Hand/", "tokens": ["Er\u00b7ha\u00b7ben", "aus", "der", "Qwell", "hier", "Was\u00b7ser", "mit", "der", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein leichtgef\u00fcstes Reh hat man hier m\u00f6gen f\u00e4llen/", "tokens": ["Kein", "leicht\u00b7ge\u00b7f\u00fcs\u00b7tes", "Reh", "hat", "man", "hier", "m\u00f6\u00b7gen", "f\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PIS", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch in dem dikken Busch nach schwartz\u0113 Wildpret stell\u0113/", "tokens": ["Noch", "in", "dem", "dik\u00b7ken", "Busch", "nach", "schwartz\u0113", "Wild\u00b7pret", "stell\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Es hegt in jenem Schlo\u00df der Igel seine Zucht/", "tokens": ["Es", "hegt", "in", "je\u00b7nem", "Schlo\u00df", "der", "I\u00b7gel", "sei\u00b7ne", "Zucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da nur die Fledermau\u00df ihr hole Wohnung sucht.", "tokens": ["Da", "nur", "die", "Fle\u00b7der\u00b7mau\u00df", "ihr", "ho\u00b7le", "Woh\u00b7nung", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das unverschlossne Haus zeigt der gew\u00f6lbte Bogen/", "tokens": ["Das", "un\u00b7ver\u00b7schloss\u00b7ne", "Haus", "zeigt", "der", "ge\u00b7w\u00f6lb\u00b7te", "Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Last hat seinen Grund vorlangsten \u00fcberwogen/", "tokens": ["Der", "Last", "hat", "sei\u00b7nen", "Grund", "vor\u00b7langs\u00b7ten", "\u00fc\u00b7ber\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das K\u00e4utzlein unn der Dachs sind wohnhaft hier zu Land/", "tokens": ["Das", "K\u00e4utz\u00b7lein", "unn", "der", "Dachs", "sind", "wohn\u00b7haft", "hier", "zu", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADJD", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es dekkt das Marderthier mit Jungen diesen Sand/", "tokens": ["Es", "de\u00b7kkt", "das", "Mar\u00b7der\u00b7thier", "mit", "Jun\u00b7gen", "die\u00b7sen", "Sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "PDAT", "NN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.15": {"text": "Im Keller findet man ein Bret von dritten Gaden/", "tokens": ["Im", "Kel\u00b7ler", "fin\u00b7det", "man", "ein", "Bret", "von", "drit\u00b7ten", "Ga\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Kr\u00f6ten samt der Maus in Otterleiche baden.", "tokens": ["Die", "Kr\u00f6\u00b7ten", "samt", "der", "Maus", "in", "Ot\u00b7ter\u00b7lei\u00b7che", "ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Nusbaum w\u00e4chset dort n\u00e4chst der verfallnen T\u00fcr/", "tokens": ["Ein", "Nus\u00b7baum", "w\u00e4ch\u00b7set", "dort", "n\u00e4chst", "der", "ver\u00b7fall\u00b7nen", "T\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Er stehet Wurtzelf\u00e4st/ und gr\u00fcnet hoch herf\u00fcr/", "tokens": ["Er", "ste\u00b7het", "Wurt\u00b7zel\u00b7f\u00e4st", "/", "und", "gr\u00fc\u00b7net", "hoch", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "KON", "VVFIN", "ADJD", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der d\u00fcsterrauhe Wald \u00fcmzirkt den \u00f6den Rangen/", "tokens": ["Der", "d\u00fcs\u00b7ter\u00b7rau\u00b7he", "Wald", "\u00fcm\u00b7zirkt", "den", "\u00f6\u00b7den", "Ran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den nie-gepfl\u00fcgten Ort/ die dikkbeb\u00e4umten Hangen.", "tokens": ["Den", "nie\u00b7ge\u00b7pfl\u00fcg\u00b7ten", "Ort", "/", "die", "dikk\u00b7be\u00b7b\u00e4um\u00b7ten", "Han\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie nennet man den Flu\u00df/ der keinen Namen hat?", "tokens": ["Wie", "nen\u00b7net", "man", "den", "Flu\u00df", "/", "der", "kei\u00b7nen", "Na\u00b7men", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ART", "NN", "$(", "ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sein Abfall dienet mir jetzt an Begleiters stat.", "tokens": ["Sein", "Ab\u00b7fall", "die\u00b7net", "mir", "jetzt", "an", "Be\u00b7glei\u00b7ters", "stat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ist dann der Schattenwald in diesen Deich gest\u00fcrtzet?", "tokens": ["Ist", "dann", "der", "Schat\u00b7ten\u00b7wald", "in", "die\u00b7sen", "Deich", "ge\u00b7st\u00fcrt\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sein gr\u00fcnbelaubter Thron ist Mahlerrecht gek\u00fcrtzet.", "tokens": ["Sein", "gr\u00fcn\u00b7be\u00b7laub\u00b7ter", "Thron", "ist", "Mah\u00b7ler\u00b7recht", "ge\u00b7k\u00fcrt\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "H\u00f6r/ leichtes Felsen-Kind/ bin ich hier gantz allein?", "tokens": ["H\u00f6r", "/", "leich\u00b7tes", "Fel\u00b7sen\u00b7Kind", "/", "bin", "ich", "hier", "gantz", "al\u00b7lein", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$(", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der gelblich-gr\u00fcne Frosch quakkt aus der Pf\u00fctzen/ nein.", "tokens": ["Der", "gelb\u00b7lich\u00b7gr\u00fc\u00b7ne", "Frosch", "quakkt", "aus", "der", "Pf\u00fct\u00b7zen", "/", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$(", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mich d\u00fcnkt in dieser Gruft solt Echo Lieb erfrieren/", "tokens": ["Mich", "d\u00fcnkt", "in", "die\u00b7ser", "Gruft", "solt", "E\u00b7cho", "Lieb", "er\u00b7frie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "VMFIN", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die pfleget meine Pfeiff und mein Gesang zu zieren.", "tokens": ["Die", "pfle\u00b7get", "mei\u00b7ne", "Pfeiff", "und", "mein", "Ge\u00b7sang", "zu", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich liebe diesen Ort/ der ferne von Geschrey", "tokens": ["Ich", "lie\u00b7be", "die\u00b7sen", "Ort", "/", "der", "fer\u00b7ne", "von", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$(", "ART", "ADJA", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mich auf so \u00f6dem Weg f\u00fcrt aller Sorgen frey.", "tokens": ["Mich", "auf", "so", "\u00f6\u00b7dem", "Weg", "f\u00fcrt", "al\u00b7ler", "Sor\u00b7gen", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es \u00fcberschatten mich der Felsen k\u00fcle Schatten/", "tokens": ["Es", "\u00fc\u00b7bersc\u00b7hat\u00b7ten", "mich", "der", "Fel\u00b7sen", "k\u00fc\u00b7le", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo sich mit dem Gesang die Nachtigallen gatten.", "tokens": ["Wo", "sich", "mit", "dem", "Ge\u00b7sang", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "gat\u00b7ten", "."], "token_info": ["word", "wo