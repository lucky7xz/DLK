{"dta.poem.7005": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herrn  B .  H .  B rockes  \n eigene,  \n so wohl  M oralische als  \n Physicalische,  \n  G edichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hag, o Mensch! auf welche Weise", "tokens": ["Hag", ",", "o", "Mensch", "!", "auf", "wel\u00b7che", "Wei\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$.", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann sich, zu des Sch\u00f6pfers Preise,", "tokens": ["Kann", "sich", ",", "zu", "des", "Sch\u00f6p\u00b7fers", "Prei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Seele sch\u00f6ner schm\u00fccken,", "tokens": ["Uns\u00b7re", "See\u00b7le", "sch\u00f6\u00b7ner", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wenn wir von Seinen Wercken,", "tokens": ["Als", "wenn", "wir", "von", "Sei\u00b7nen", "Wer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dadurch, da\u00df wir sie bemercken,", "tokens": ["Da\u00b7durch", ",", "da\u00df", "wir", "sie", "be\u00b7mer\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In den Geist ihr Bildni\u00df dr\u00fccken?", "tokens": ["In", "den", "Geist", "ihr", "Bild\u00b7ni\u00df", "dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}