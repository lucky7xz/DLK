{"dta.poem.623": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Niclas Serins,   des J\u00fcngern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt der k\u00fchne Held von einem Schwein entlebet/", "tokens": ["Hier", "liegt", "der", "k\u00fch\u00b7ne", "Held", "von", "ei\u00b7nem", "Schwein", "ent\u00b7le\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor welchen ", "tokens": ["Vor", "wel\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPR", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der T\u00fcrck und Tartern zwung/ den f\u00e4llt ein wildes Schwein/", "tokens": ["Der", "T\u00fcrck", "und", "Tar\u00b7tern", "zwung", "/", "den", "f\u00e4llt", "ein", "wil\u00b7des", "Schwein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$(", "ART", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df der Tod gewi\u00df/ die Art unwissend seyn.", "tokens": ["So", "mu\u00df", "der", "Tod", "ge\u00b7wi\u00df", "/", "die", "Art", "un\u00b7wis\u00b7send", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "$(", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}