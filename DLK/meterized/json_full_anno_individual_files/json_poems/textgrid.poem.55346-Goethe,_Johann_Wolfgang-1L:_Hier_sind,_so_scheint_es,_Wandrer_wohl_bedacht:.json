{"textgrid.poem.55346": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier sind, so scheint es, Wandrer wohl bedacht:", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sind, so scheint es, Wandrer wohl bedacht:", "tokens": ["Hier", "sind", ",", "so", "scheint", "es", ",", "Wand\u00b7rer", "wohl", "be\u00b7dacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn jeder f\u00e4nde Pfad um Mitternacht.", "tokens": ["Denn", "je\u00b7der", "f\u00e4n\u00b7de", "Pfad", "um", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir sagen nicht, wir h\u00e4tten's oft gesehn,", "tokens": ["Wir", "sa\u00b7gen", "nicht", ",", "wir", "h\u00e4t\u00b7ten's", "oft", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dergleichen Wege doch gelang's zu gehn;", "tokens": ["Derg\u00b7lei\u00b7chen", "We\u00b7ge", "doch", "ge\u00b7lang's", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn freilich, wo die M\u00fche war gehoben,", "tokens": ["Denn", "frei\u00b7lich", ",", "wo", "die", "M\u00fc\u00b7he", "war", "ge\u