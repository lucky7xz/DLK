{"dta.poem.11032": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schreiben  \n an  Mademoiselle  Weisen  \n in Merseburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Recht wie die Werke der Natur,", "tokens": ["Recht", "wie", "die", "Wer\u00b7ke", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wovon wir zwar das Ganze sehen,", "tokens": ["Wo\u00b7von", "wir", "zwar", "das", "Gan\u00b7ze", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch fast nicht die geringste Spur,", "tokens": ["Doch", "fast", "nicht", "die", "ge\u00b7rings\u00b7te", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Theilchen, woraus sie bestehen:", "tokens": ["Von", "Theil\u00b7chen", ",", "wo\u00b7raus", "sie", "be\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So scheinen ebenfalls der weisen ", "tokens": ["So", "schei\u00b7nen", "e\u00b7ben\u00b7falls", "der", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Indem ich ", "tokens": ["In\u00b7dem", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Da doch die Stich, als Theile, nicht nur klein,", "tokens": ["Da", "doch", "die", "Stich", ",", "als", "Thei\u00b7le", ",", "nicht", "nur", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KOUS", "NN", "$,", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ihres Zwirns so zarte F\u00e4ser,", "tokens": ["Und", "ih\u00b7res", "Zwirns", "so", "zar\u00b7te", "F\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch selber durch Vergr\u00f6\u00dfrungs-Gl\u00e4ser,", "tokens": ["Auch", "sel\u00b7ber", "durch", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7Gl\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kaum unsern Augen sichtbar seyn.", "tokens": ["Kaum", "un\u00b7sern", "Au\u00b7gen", "sicht\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}