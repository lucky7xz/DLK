{"textgrid.poem.47642": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[auf meinem Lager wach' ich in der Nacht]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meinem Lager wach' ich in der Nacht", "tokens": ["Auf", "mei\u00b7nem", "La\u00b7ger", "wach'", "ich", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und misse den, der mir den Frieden giebt;", "tokens": ["Und", "mis\u00b7se", "den", ",", "der", "mir", "den", "Frie\u00b7den", "giebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich schlummer' ein und wache wieder auf", "tokens": ["Ich", "schlum\u00b7mer'", "ein", "und", "wa\u00b7che", "wie\u00b7der", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und suche den, den meine Seele liebt.", "tokens": ["Und", "su\u00b7che", "den", ",", "den", "mei\u00b7ne", "See\u00b7le", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch greif' ich, ach, in eitle Leere nur", "tokens": ["Doch", "greif'", "ich", ",", "ach", ",", "in", "eit\u00b7le", "Lee\u00b7re", "nur"], "token_info": ["word", "word", "word",