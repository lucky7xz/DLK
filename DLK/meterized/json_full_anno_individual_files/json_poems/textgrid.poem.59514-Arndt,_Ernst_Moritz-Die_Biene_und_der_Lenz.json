{"textgrid.poem.59514": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Die Biene und der Lenz", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ziehst du dein goldnes R\u00f6ckchen an?", "tokens": ["Ziehst", "du", "dein", "gold\u00b7nes", "R\u00f6ck\u00b7chen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die goldnen Stiefeln auch?", "tokens": ["Die", "gold\u00b7nen", "Stie\u00b7feln", "auch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Bienchen, V\u00f6glein wohlgemut", "tokens": ["O", "Bien\u00b7chen", ",", "V\u00f6\u00b7glein", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit leichtem Sinn und leichtem Blut,", "tokens": ["Mit", "leich\u00b7tem", "Sinn", "und", "leich\u00b7tem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was locket dich das Sonnenlicht?", "tokens": ["Was", "lo\u00b7cket", "dich", "das", "Son\u00b7nen\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was lockt dich Bl\u00fctenhauch?", "tokens": ["Was", "lockt", "dich", "Bl\u00fc\u00b7ten\u00b7hauch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was summst du lustig hin und her,", "tokens": ["Was", "summst", "du", "lus\u00b7tig", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast nie des Spiels genug?", "tokens": ["Hast", "nie", "des", "Spiels", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Lenz ist kurz, du s\u00fc\u00dfes Kind,", "tokens": ["Der", "Lenz", "ist", "kurz", ",", "du", "s\u00fc\u00b7\u00dfes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich fa\u00dft der Strom, dich nimmt der Wind,", "tokens": ["Dich", "fa\u00dft", "der", "Strom", ",", "dich", "nimmt", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich bringet um den Bl\u00fctenraub", "tokens": ["Dich", "brin\u00b7get", "um", "den", "Bl\u00fc\u00b7ten\u00b7raub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Menschen List und Trug.", "tokens": ["Der", "Men\u00b7schen", "List", "und", "Trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohl zieh ich an den goldnen Rock", "tokens": ["Wohl", "zieh", "ich", "an", "den", "gold\u00b7nen", "Rock"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kleid' in Gold den Fu\u00df,", "tokens": ["Und", "kleid'", "in", "Gold", "den", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leicht ist mein Blut und leicht mein Sinn,", "tokens": ["Leicht", "ist", "mein", "Blut", "und", "leicht", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In Freuden ich geboren bin;", "tokens": ["In", "Freu\u00b7den", "ich", "ge\u00b7bo\u00b7ren", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum locket mich das Sonnenlicht", "tokens": ["Drum", "lo\u00b7cket", "mich", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Blumenliebesgru\u00df.", "tokens": ["Und", "Blu\u00b7men\u00b7lie\u00b7bes\u00b7gru\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Lenz ist kurz, das Leben schnell,", "tokens": ["Der", "Lenz", "ist", "kurz", ",", "das", "Le\u00b7ben", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum flieg' ich schnell dahin;", "tokens": ["Drum", "flieg'", "ich", "schnell", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Fr\u00fchlingsschein, mein Blumenreich,", "tokens": ["Mein", "Fr\u00fch\u00b7lings\u00b7schein", ",", "mein", "Blu\u00b7men\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jedem Kelch mein Bettchen weich,", "tokens": ["In", "je\u00b7dem", "Kelch", "mein", "Bett\u00b7chen", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf jeder Flur mein Leben bunt \u2013", "tokens": ["Auf", "je\u00b7der", "Flur", "mein", "Le\u00b7ben", "bunt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drob trag' ich frohen Sinn.", "tokens": ["Drob", "trag'", "ich", "fro\u00b7hen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "O Bienchen, V\u00f6glein wohlgemut,", "tokens": ["O", "Bien\u00b7chen", ",", "V\u00f6\u00b7glein", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O s\u00fc\u00dfes Fr\u00fchlingskind!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Fr\u00fch\u00b7lings\u00b7kind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Horch', horch', wie klagt die Nachtigall", "tokens": ["Horch'", ",", "horch'", ",", "wie", "klagt", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Erlenbusch mit Trauerschall!", "tokens": ["Im", "Er\u00b7len\u00b7busch", "mit", "Trau\u00b7er\u00b7schall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch sie im Lenz geboren ist,", "tokens": ["Auch", "sie", "im", "Lenz", "ge\u00b7bo\u00b7ren", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nur auf Trauern sinnt.", "tokens": ["Doch", "nur", "auf", "Trau\u00b7ern", "sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl h\u00f6re ich die Nachtigall,", "tokens": ["Wohl", "h\u00f6\u00b7re", "ich", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Klagen fromm und still;", "tokens": ["Ihr", "Kla\u00b7gen", "fromm", "und", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie ist die schmerzenreiche Frau,", "tokens": ["Sie", "ist", "die", "schmer\u00b7zen\u00b7rei\u00b7che", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Trauerkleid ist dunkelgrau;", "tokens": ["Ihr", "Trau\u00b7er\u00b7kleid", "ist", "dun\u00b7kel\u00b7grau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch sprich, warum ich trauern soll,", "tokens": ["Doch", "sprich", ",", "wa\u00b7rum", "ich", "trau\u00b7ern", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil sie nicht froh sein will?", "tokens": ["Weil", "sie", "nicht", "froh", "sein", "will", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Schau' her, wie bebet Strauch und Laub", "tokens": ["Schau'", "her", ",", "wie", "be\u00b7bet", "Strauch", "und", "Laub"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im jungen Sonnenschein!", "tokens": ["Im", "jun\u00b7gen", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie k\u00fcssen sich die Blumen lieb!", "tokens": ["Wie", "k\u00fcs\u00b7sen", "sich", "die", "Blu\u00b7men", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rufen: Kleiner Honigdieb,", "tokens": ["Und", "ru\u00b7fen", ":", "Klei\u00b7ner", "Ho\u00b7nig\u00b7dieb", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, sammle Blumenliebeskost!", "tokens": ["Komm", ",", "samm\u00b7le", "Blu\u00b7men\u00b7lie\u00b7bes\u00b7kost", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Denn dieser Lenz ist dein.", "tokens": ["Denn", "die\u00b7ser", "Lenz", "ist", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O V\u00f6glein, V\u00f6glein wohlgemut,", "tokens": ["O", "V\u00f6\u00b7glein", ",", "V\u00f6\u00b7glein", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit goldnem Fl\u00fcgelpaar!", "tokens": ["Mit", "gold\u00b7nem", "Fl\u00fc\u00b7gel\u00b7paar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O leichtes Leben frommer Brust!", "tokens": ["O", "leich\u00b7tes", "Le\u00b7ben", "from\u00b7mer", "Brust", "!"], "token_info": ["word",