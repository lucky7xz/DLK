{"textgrid.poem.52236": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Eine Seele", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flog zum Himmel eine junge Seele,", "tokens": ["Flog", "zum", "Him\u00b7mel", "ei\u00b7ne", "jun\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leisen Fluges hob sie sich empor;", "tokens": ["Lei\u00b7sen", "Flu\u00b7ges", "hob", "sie", "sich", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fast ein Kind noch, rein und ohne Fehle,", "tokens": ["Fast", "ein", "Kind", "noch", ",", "rein", "und", "oh\u00b7ne", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Trat sie sch\u00fcchtern durch das goldne Tor.", "tokens": ["Trat", "sie", "sch\u00fcch\u00b7tern", "durch", "das", "gold\u00b7ne", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und: \u00bbSieh da, das Kind des Patrioten!\u00ab", "tokens": ["Und", ":", "\u00bb", "Sieh", "da", ",", "das", "Kind", "des", "Pat\u00b7ri\u00b7o\u00b7ten", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$.", "$(", "NE", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Irrt' ein Murmeln hier und dort im Nu.", "tokens": ["Irrt'", "ein", "Mur\u00b7meln", "hier", "und", "dort", "im", "Nu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "KON", "ADV", "APPRART", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Standen auf die besten deutschen Toten,", "tokens": ["Stan\u00b7den", "auf", "die", "bes\u00b7ten", "deut\u00b7schen", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schritten hastig auf die Tote zu.", "tokens": ["Schrit\u00b7ten", "has\u00b7tig", "auf", "die", "To\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Kam heran der edle starre Seume,", "tokens": ["Kam", "he\u00b7ran", "der", "ed\u00b7le", "star\u00b7re", "Seu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mann der Freiheit und der Poesie;", "tokens": ["Mann", "der", "Frei\u00b7heit", "und", "der", "Poe\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Eilte Schiller durch die lichten R\u00e4ume.", "tokens": ["Eil\u00b7te", "Schil\u00b7ler", "durch", "die", "lich\u00b7ten", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hutten, Schubart \u2013 alle kamen sie.", "tokens": ["Hut\u00b7ten", ",", "Schu\u00b7bart", "\u2013", "al\u00b7le", "ka\u00b7men", "sie", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "PIS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sahn sie an mit unverstellter Klage;", "tokens": ["Sahn", "sie", "an", "mit", "un\u00b7ver\u00b7stell\u00b7ter", "Kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Boten Gru\u00df ihr, warm und fest und schlicht;", "tokens": ["Bo\u00b7ten", "Gru\u00df", "ihr", ",", "warm", "und", "fest", "und", "schlicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "ADJD", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Blickten stumm und \u00e4ngstlich eine Frage", "tokens": ["Blick\u00b7ten", "stumm", "und", "\u00e4ngst\u00b7lich", "ei\u00b7ne", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In das schmerzlich l\u00e4chelnde Gesicht.", "tokens": ["In", "das", "schmerz\u00b7lich", "l\u00e4\u00b7cheln\u00b7de", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ach, senkt' es, sah zur Erde nieder;", "tokens": ["Ach", ",", "senkt'", "es", ",", "sah", "zur", "Er\u00b7de", "nie\u00b7der", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zitternd stand sie, zitternd und geknickt:", "tokens": ["Zit\u00b7ternd", "stand", "sie", ",", "zit\u00b7ternd", "und", "ge\u00b7knickt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hei\u00dfe Tr\u00e4nen sprangen durch die Lider,", "tokens": ["Hei\u00b7\u00dfe", "Tr\u00e4\u00b7nen", "spran\u00b7gen", "durch", "die", "Li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die des Vaters Hand \u2013 ", "tokens": ["Die", "des", "Va\u00b7ters", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sieh, da zuckt' es in der Faust dem Seume;", "tokens": ["Sieh", ",", "da", "zuckt'", "es", "in", "der", "Faust", "dem", "Seu\u00b7me", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schubarts dunkle, breite Stirne schwoll;", "tokens": ["Schu\u00b7barts", "dunk\u00b7le", ",", "brei\u00b7te", "Stir\u00b7ne", "schwoll", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbfreiheit ist nur in dem Reich der Tr\u00e4ume\u00ab,", "tokens": ["\u00bb", "frei\u00b7heit", "ist", "nur", "in", "dem", "Reich", "der", "Tr\u00e4u\u00b7me", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$(", "$,"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sagte Schiller bittern Zornes voll.", "tokens": ["Sag\u00b7te", "Schil\u00b7ler", "bit\u00b7tern", "Zor\u00b7nes", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aber Seume: \u00bbM\u00e4dchen, sei zufrieden!", "tokens": ["A\u00b7ber", "Seu\u00b7me", ":", "\u00bb", "M\u00e4d\u00b7chen", ",", "sei", "zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "$(", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auch der Tod, du wei\u00dft es, kann befrein!", "tokens": ["Auch", "der", "Tod", ",", "du", "wei\u00dft", "es", ",", "kann", "be\u00b7fr\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VMFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df die Schl\u00f6sser, la\u00df sie Ketten schmieden \u2013", "tokens": ["La\u00df", "die", "Schl\u00f6s\u00b7ser", ",", "la\u00df", "sie", "Ket\u00b7ten", "schmie\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "PPER", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Frei mit Freien wird dein Vater sein!", "tokens": ["Frei", "mit", "Frei\u00b7en", "wird", "dein", "Va\u00b7ter", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Frei zu mir und diesen wird er treten,", "tokens": ["Frei", "zu", "mir", "und", "die\u00b7sen", "wird", "er", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PDS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auch ein Toter f\u00fcr das Vaterland!", "tokens": ["Auch", "ein", "To\u00b7ter", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auch ein Licht, zu dem in Sturmesn\u00f6ten", "tokens": ["Auch", "ein", "Licht", ",", "zu", "dem", "in", "Stur\u00b7mes\u00b7n\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ART", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deutsche M\u00e4nner heben Herz und Hand!", "tokens": ["Deut\u00b7sche", "M\u00e4n\u00b7ner", "he\u00b7ben", "Herz", "und", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "O, wie stolz dann wird der M\u00fcde rasten!", "tokens": ["O", ",", "wie", "stolz", "dann", "wird", "der", "M\u00fc\u00b7de", "ras\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Freilich \u2013 dann erst! Bete, da\u00b4er stirbt!", "tokens": ["Frei\u00b7lich", "\u2013", "dann", "erst", "!", "Be\u00b7te", ",", "da'\u00b7er", "stirbt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "ADV", "$.", "NN", "$,", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bete, Kind! ich kenne die Dynasten,", "tokens": ["Be\u00b7te", ",", "Kind", "!", "ich", "ken\u00b7ne", "die", "Dy\u00b7nas\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deren Willk\u00fchr seine Kraft zerm\u00fcrbt!", "tokens": ["De\u00b7ren", "Will\u00b7k\u00fchr", "sei\u00b7ne", "Kraft", "zer\u00b7m\u00fcrbt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Ihn ins Enge, mich vordem ins Weite", "tokens": ["Ihn", "ins", "En\u00b7ge", ",", "mich", "vor\u00b7dem", "ins", "Wei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "$,", "PPER", "ADV", "APPRART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Trieb derselbe finstre Herrscherstamm;", "tokens": ["Trieb", "der\u00b7sel\u00b7be", "finst\u00b7re", "Herr\u00b7scher\u00b7stamm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sagten dir nicht eher schon die Leute,", "tokens": ["Sag\u00b7ten", "dir", "nicht", "e\u00b7her", "schon", "die", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df der Seume nach Neuschottland schwamm?", "tokens": ["Da\u00df", "der", "Seu\u00b7me", "nach", "Neu\u00b7schott\u00b7land", "schwamm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.11": {"line.1": {"text": "Drum so fleh', da\u00df bald mit gr\u00fcnen Spitzen", "tokens": ["Drum", "so", "fleh'", ",", "da\u00df", "bald", "mit", "gr\u00fc\u00b7nen", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gras der Lahn um einen H\u00fcgel kost!", "tokens": ["Gras", "der", "Lahn", "um", "ei\u00b7nen", "H\u00fc\u00b7gel", "kost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Neben Hutten soll dein Vater sitzen \u2013", "tokens": ["Ne\u00b7ben", "Hut\u00b7ten", "soll", "dein", "Va\u00b7ter", "sit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tochter Jordans, bet' und sei getrost!\u00ab", "tokens": ["Toch\u00b7ter", "Jor\u00b7dans", ",", "bet'", "und", "sei", "ge\u00b7trost", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "KON", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Flog zum Himmel eine junge Seele,", "tokens": ["Flog", "zum", "Him\u00b7mel", "ei\u00b7ne", "jun\u00b7ge", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leisen Fluges hob sie sich empor;", "tokens": ["Lei\u00b7sen", "Flu\u00b7ges", "hob", "sie", "sich", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fast ein Kind noch, rein und ohne Fehle,", "tokens": ["Fast", "ein", "Kind", "noch", ",", "rein", "und", "oh\u00b7ne", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Trat sie sch\u00fcchtern durch das goldne Tor.", "tokens": ["Trat", "sie", "sch\u00fcch\u00b7tern", "durch", "das", "gold\u00b7ne", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Und: \u00bbSieh da, das Kind des Patrioten!\u00ab", "tokens": ["Und", ":", "\u00bb", "Sieh", "da", ",", "