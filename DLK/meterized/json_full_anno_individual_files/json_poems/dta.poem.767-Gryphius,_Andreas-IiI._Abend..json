{"dta.poem.767": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  Abend.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der schnelle Tag ist hin/ die Nacht schwingt jhre fahn/ ", "tokens": ["Der", "schnel\u00b7le", "Tag", "ist", "hin", "/", "die", "Nacht", "schwingt", "jhre", "fahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Vnd f\u00fchrt die Sternen auff. Der Menschen m\u00fcde scha-", "tokens": ["Vnd", "f\u00fchrt", "die", "Ster\u00b7nen", "auff", ".", "Der", "Men\u00b7schen", "m\u00fc\u00b7de", "scha"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ren", "tokens": ["ren"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Verlassen feld vnd werck/ ", "tokens": ["Ver\u00b7las\u00b7sen", "feld", "vnd", "werck", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trawrt jtzt die Einsamkeit. Wie ist die zeit verthan!", "tokens": ["Trawrt", "jtzt", "die", "Ein\u00b7sam\u00b7keit", ".", "Wie", "ist", "die", "zeit", "ver\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der port naht mehr vnd mehr sich/ zu der glieder Kahn. ", "tokens": ["Der", "port", "naht", "mehr", "vnd", "mehr", "sich", "/", "zu", "der", "glie\u00b7der", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "KON", "ADV", "PRF", "$(", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich wie di\u00df licht verfiel/ so wird in wenig Jahren", "tokens": ["Gleich", "wie", "di\u00df", "licht", "ver\u00b7fiel", "/", "so", "wird", "in", "we\u00b7nig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PDS", "NN", "VVFIN", "$(", "ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich/ du/ vnd was man hat/ vnd was man siht/ hinfahren. ", "tokens": ["Ich", "/", "du", "/", "vnd", "was", "man", "hat", "/", "vnd", "was", "man", "siht", "/", "hin\u00b7fah\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$(", "PPER", "$(", "KON", "PWS", "PIS", "VAFIN", "$(", "KON", "PWS", "PIS", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df Leben k\u00f6mmt mir vor al\u00df eine renne bahn.", "tokens": ["Di\u00df", "Le\u00b7ben", "k\u00f6mmt", "mir", "vor", "al\u00df", "ei\u00b7ne", "ren\u00b7ne", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "APPR", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df h\u00f6chster Gott mich doch nicht auff dem Laufplatz gleiten/", "tokens": ["La\u00df", "h\u00f6chs\u00b7ter", "Gott", "mich", "doch", "nicht", "auff", "dem", "Lauf\u00b7platz", "glei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df mich nicht ach/ nicht pracht/ nicht lust/ nicht angst ver-", "tokens": ["La\u00df", "mich", "nicht", "ach", "/", "nicht", "pracht", "/", "nicht", "lust", "/", "nicht", "angst", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "XY", "$(", "PTKNEG", "VVFIN", "$(", "PTKNEG", "VVFIN", "$(", "PTKNEG", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "leiten.", "tokens": ["lei\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Dein ewig heller glantz sey vor vnd neben mir/", "tokens": ["Dein", "e\u00b7wig", "hel\u00b7ler", "glantz", "sey", "vor", "vnd", "ne\u00b7ben", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "VAFIN", "APPR", "KON", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00df /wenn der m\u00fcde Leib entschl\u00e4fft/ die Seele wachen", "tokens": ["La\u00df", "/", "wenn", "der", "m\u00fc\u00b7de", "Leib", "ent\u00b7schl\u00e4fft", "/", "die", "See\u00b7le", "wa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$(", "KOUS", "ART", "ADJA", "NN", "VVPP", "$(", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd wenn der letzte Tag wird mit mir abend machen/", "tokens": ["Vnd", "wenn", "der", "letz\u00b7te", "Tag", "wird", "mit", "mir", "a\u00b7bend", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So rei\u00df mich au\u00df dem thal der ", "tokens": ["So", "rei\u00df", "mich", "au\u00df", "dem", "thal", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}