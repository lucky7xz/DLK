easure": "trochaic.tetra"}, "line.4": {"text": "Sah, wie Jesus Christus Licht", "tokens": ["Sah", ",", "wie", "Je\u00b7sus", "Chris\u00b7tus", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "NE", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir die Todeswange hellte!", "tokens": ["Dir", "die", "To\u00b7des\u00b7wan\u00b7ge", "hell\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Blick ins Paradies", "tokens": ["Wie", "ein", "Blick", "ins", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dir die Lust der Welt verg\u00e4llte", "tokens": ["Dir", "die", "Lust", "der", "Welt", "ver\u00b7g\u00e4ll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dir deine Krone wies!", "tokens": ["Und", "dir", "dei\u00b7ne", "Kro\u00b7ne", "wies", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.14": {"line.1": {"text": "Mutter, Bruder dr\u00fcben, dr\u00fcben,", "tokens": ["Mut\u00b7ter", ",", "Bru\u00b7der", "dr\u00fc\u00b7ben", ",", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "ADV", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dachtest du, in Christus Reich,", "tokens": ["Dach\u00b7test", "du", ",", "in", "Chris\u00b7tus", "Reich", ","], "token_info": ["word", "word", "punct", "word", "wo