{"textgrid.poem.66361": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Klage, tiefgestimmte Saite,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klage, tiefgestimmte Saite,", "tokens": ["Kla\u00b7ge", ",", "tief\u00b7ge\u00b7stimm\u00b7te", "Sai\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem weinenden Klavier!", "tokens": ["Aus", "dem", "wei\u00b7nen\u00b7den", "Kla\u00b7vier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Keinen Silberton der Freude,", "tokens": ["Kei\u00b7nen", "Sil\u00b7ber\u00b7ton", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Todeston entlock' ich dir!", "tokens": ["To\u00b7des\u00b7ton", "ent\u00b7lock'", "ich", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, Minetten, die dich spielte,", "tokens": ["Ach", ",", "Mi\u00b7net\u00b7ten", ",", "die", "dich", "spiel\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die dir Geisterodem gab,", "tokens": ["Die", "dir", "Geis\u00b7te\u00b7ro\u00b7dem", "gab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn sie Lieb' und Mitleid f\u00fchlte,", "tokens": ["Wenn", "sie", "Lieb'", "und", "Mit\u00b7leid", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, Minetten deckt das Grab!", "tokens": ["Ach", ",", "Mi\u00b7net\u00b7ten", "deckt", "das", "Grab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch seh' ich die Holde stehen,", "tokens": ["Noch", "seh'", "ich", "die", "Hol\u00b7de", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie sie sanft auf mich geblickt,", "tokens": ["Wie", "sie", "sanft", "auf", "mich", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich alle meine Wehen", "tokens": ["Wenn", "ich", "al\u00b7le", "mei\u00b7ne", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Saiten ausgedr\u00fcckt.", "tokens": ["Mit", "den", "Sai\u00b7ten", "aus\u00b7ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sang ich: \u00bbAch, ich bin gefangen!\u00ab", "tokens": ["Sang", "ich", ":", "\u00bb", "Ach", ",", "ich", "bin", "ge\u00b7fan\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "ITJ", "$,", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O, wie f\u00fchlte sie mein Loos!", "tokens": ["O", ",", "wie", "f\u00fchl\u00b7te", "sie", "mein", "Loos", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bleicher wurden ihre Wangen", "tokens": ["Blei\u00b7cher", "wur\u00b7den", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Mitleidsz\u00e4hre flo\u00df.", "tokens": ["Und", "die", "Mit\u00b7le\u00b7ids\u00b7z\u00e4h\u00b7re", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn ich ", "tokens": ["Wenn", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "++", "measure": "spondeus"}, "line.2": {"text": "Las ich es im hellern Blick,", "tokens": ["Las", "ich", "es", "im", "hel\u00b7lern", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ihr Herz vor Liebe brannte,", "tokens": ["Wie", "ihr", "Herz", "vor", "Lie\u00b7be", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie f\u00fchlte Christengl\u00fcck!", "tokens": ["Wie", "sie", "f\u00fchl\u00b7te", "Chris\u00b7ten\u00b7gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O dann schwammen Himmelst\u00f6ne", "tokens": ["O", "dann", "schwam\u00b7men", "Him\u00b7mel\u00b7st\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uni mein Ohr: Die Blume f\u00e4llt!", "tokens": ["U\u00b7ni", "mein", "Ohr", ":", "Die", "Blu\u00b7me", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Schau sie an in ihrer Sch\u00f6ne,", "tokens": ["Schau", "sie", "an", "in", "ih\u00b7rer", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie ist reif f\u00fcr jene Welt!", "tokens": ["Sie", "ist", "reif", "f\u00fcr", "je\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O, verherrlichte Minette!", "tokens": ["O", ",", "ver\u00b7herr\u00b7lich\u00b7te", "Mi\u00b7net\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Engel, warum stand ich nicht", "tokens": ["En\u00b7gel", ",", "wa\u00b7rum", "stand", "ich", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch vor deinem Sterbebette?", "tokens": ["Auch", "vor", "dei\u00b7nem", "Ster\u00b7be\u00b7bet\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah, wie Jesus Christus Licht", "tokens": ["Sah", ",", "wie", "Je\u00b7sus", "Chris\u00b7tus", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "NE", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir die Todeswange hellte!", "tokens": ["Dir", "die", "To\u00b7des\u00b7wan\u00b7ge", "hell\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Blick ins Paradies", "tokens": ["Wie", "ein", "Blick", "ins", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dir die Lust der Welt verg\u00e4llte", "tokens": ["Dir", "die", "Lust", "der", "Welt", "ver\u00b7g\u00e4ll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dir deine Krone wies!", "tokens": ["Und", "dir", "dei\u00b7ne", "Kro\u00b7ne", "wies", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Mutter, Bruder dr\u00fcben, dr\u00fcben,", "tokens": ["Mut\u00b7ter", ",", "Bru\u00b7der", "dr\u00fc\u00b7ben", ",", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "ADV", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dachtest du, in Christus Reich,", "tokens": ["Dach\u00b7test", "du", ",", "in", "Chris\u00b7tus", "Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sich Fromme ewig lieben!", "tokens": ["Wo", "sich", "From\u00b7me", "e\u00b7wig", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' ich euch, umarm' ich euch!", "tokens": ["Seh'", "ich", "euch", ",", "um\u00b7arm'", "ich", "euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So entlastet von den B\u00fcrden", "tokens": ["So", "ent\u00b7las\u00b7tet", "von", "den", "B\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser Zeit, gingst du zur Ruh'.", "tokens": ["Die\u00b7ser", "Zeit", ",", "gingst", "du", "zur", "Ruh'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, wenn Engel sterben w\u00fcrden,", "tokens": ["Ach", ",", "wenn", "En\u00b7gel", "ster\u00b7ben", "w\u00fcr\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "NE", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "St\u00fcrben sie so sch\u00f6n wie du.", "tokens": ["St\u00fcr\u00b7ben", "sie", "so", "sch\u00f6n", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fromme Mutter, weine milder;", "tokens": ["From\u00b7me", "Mut\u00b7ter", ",", "wei\u00b7ne", "mil\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bruder, klage nicht zu sehr!", "tokens": ["Bru\u00b7der", ",", "kla\u00b7ge", "nicht", "zu", "sehr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "PTKA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmlisch schweben ja die Bilder", "tokens": ["Himm\u00b7lisch", "schwe\u00b7ben", "ja", "die", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Wonne um euch her.", "tokens": ["Ih\u00b7rer", "Won\u00b7ne", "um", "euch", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht, im Paradiese schreitet", "tokens": ["Seht", ",", "im", "Pa\u00b7ra\u00b7die\u00b7se", "schrei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie mit ihrem Ahnherrn Veit,", "tokens": ["Sie", "mit", "ih\u00b7rem", "Ahn\u00b7herrn", "Veit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Himmlisch sch\u00f6n und schon gekleidet", "tokens": ["Himm\u00b7lisch", "sch\u00f6n", "und", "schon", "ge\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ins Gewand der Herrlichkeit.", "tokens": ["Ins", "Ge\u00b7wand", "der", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sanfter werde meine Klage", "tokens": ["Sanf\u00b7ter", "wer\u00b7de", "mei\u00b7ne", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem weinenden Klavier,", "tokens": ["Aus", "dem", "wei\u00b7nen\u00b7den", "Kla\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dank, und Lieb', und Wehmuth sage:", "tokens": ["Dank", ",", "und", "Lieb'", ",", "und", "Weh\u00b7muth", "sa\u00b7ge", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies ist ein Geschenk von ihr!", "tokens": ["Dies", "ist", "ein", "Ge\u00b7schenk", "von", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, so lang ich noch die Saite", "tokens": ["Ach", ",", "so", "lang", "ich", "noch", "die", "Sai\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ADJD", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bebend r\u00fchre, t\u00f6n' ihr Klang", "tokens": ["Be\u00b7bend", "r\u00fch\u00b7re", ",", "t\u00f6n'", "ihr", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bald, Minette, deine Freude,", "tokens": ["Bald", ",", "Mi\u00b7net\u00b7te", ",", "dei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bald der Freundschaft Klaggesang.", "tokens": ["Bald", "der", "Freund\u00b7schaft", "Klag\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dort seh' ich dich wieder, dorten,", "tokens": ["Dort", "seh'", "ich", "dich", "wie\u00b7der", ",", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo du Lebenswasser trinkst,", "tokens": ["Wo", "du", "Le\u00b7bens\u00b7was\u00b7ser", "trinkst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du mir an goldnen Pforten", "tokens": ["Wenn", 