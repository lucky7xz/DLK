 "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du junge Himmelsbraut!", "tokens": ["Du", "jun\u00b7ge", "Him\u00b7mels\u00b7braut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Lammes Neuverm\u00e4hlte,", "tokens": ["Des", "Lam\u00b7mes", "Neu\u00b7ver\u00b7m\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm ewig nun vertraut!", "tokens": ["Ihm", "e\u00b7wig", "nun", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df deine Blicke fallen", "tokens": ["La\u00df", "dei\u00b7ne", "Bli\u00b7cke", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herab auf unsre Noth.", "tokens": ["Her\u00b7ab", "auf", "uns\u00b7re", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Minette, w\u00fcnsch' uns allen", "tokens": ["Mi\u00b7net\u00b7te", ",", "w\u00fcn\u00b7sch'", "uns", "al\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PIAT"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Den sch\u00f6nen Christentod!", "tokens": ["Den", "sch\u00f6\u00b7nen", "Chris\u00b7ten\u00b7tod", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Klage, tiefgestimmte Saite,", "tokens": ["Kla\u00b7ge", ",", "tief\u00b7ge\u00b7stimm\u00b