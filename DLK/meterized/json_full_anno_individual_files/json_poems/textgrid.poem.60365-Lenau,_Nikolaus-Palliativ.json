{"textgrid.poem.60365": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Palliativ", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Gras gewachsen \u00fcber die Geschichte,", "tokens": ["Ist", "Gras", "ge\u00b7wach\u00b7sen", "\u00fc\u00b7ber", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wei\u00df nicht mehr recht, wie sie sich zugetragen;", "tokens": ["Wei\u00df", "nicht", "mehr", "recht", ",", "wie", "sie", "sich", "zu\u00b7ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur manchmal schwebt mirs vor im D\u00e4mmerlichte,", "tokens": ["Nur", "manch\u00b7mal", "schwebt", "mirs", "vor", "im", "D\u00e4m\u00b7mer\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als h\u00e4tt ich einer Schuld mich anzuklagen.", "tokens": ["Als", "h\u00e4tt", "ich", "ei\u00b7ner", "Schuld", "mich", "an\u00b7zu\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch abgewandt vom st\u00f6renden Gesichte,", "tokens": ["Doch", "ab\u00b7ge\u00b7wandt", "vom", "st\u00f6\u00b7ren\u00b7den", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruf ichs nicht an und will es nicht befragen,", "tokens": ["Ruf", "ichs", "nicht", "an", "und", "will", "es", "nicht", "be\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PTKNEG", "PTKVZ", "KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil Blick und Mut ich in die Zukunft richte;", "tokens": ["Weil", "Blick", "und", "Mut", "ich", "in", "die", "Zu\u00b7kunft", "rich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich schlage mich nicht gern mit alten Tagen.", "tokens": ["Ich", "schla\u00b7ge", "mich", "nicht", "gern", "mit", "al\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbwenn dir der Sensenmann den Leib hinstrecket,", "tokens": ["\u00bb", "wenn", "dir", "der", "Sen\u00b7sen\u00b7mann", "den", "Leib", "hins\u00b7tre\u00b7cket", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird er auch s\u00e4uberlich das Gras dir m\u00e4hen,", "tokens": ["Wird", "er", "auch", "s\u00e4u\u00b7ber\u00b7lich", "das", "Gras", "dir", "m\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das jene Schuldgeschichte dir verdecket.", "tokens": ["Das", "je\u00b7ne", "Schuld\u00b7ge\u00b7schich\u00b7te", "dir", "ver\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kehr mutig um zu den verla\u00dfnen B\u00fchnen,", "tokens": ["Kehr", "mu\u00b7tig", "um", "zu", "den", "ver\u00b7la\u00df\u00b7nen", "B\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schuld mit scharfem Reueblick zu sehen;", "tokens": ["Die", "Schuld", "mit", "schar\u00b7fem", "Reu\u00b7e\u00b7blick", "zu", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll sie dir sterben, eile, sie zu s\u00fchnen.\u00ab", "tokens": ["Soll", "sie", "dir", "ster\u00b7ben", ",", "ei\u00b7le", ",", "sie", "zu", "s\u00fch\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ist Gras gewachsen \u00fcber die Geschichte,", "tokens": ["Ist", "Gras", "ge\u00b7wach\u00b7sen", "\u00fc\u00b7ber", "die", "Ge\u00b7schich\u00b7te", ","], "token