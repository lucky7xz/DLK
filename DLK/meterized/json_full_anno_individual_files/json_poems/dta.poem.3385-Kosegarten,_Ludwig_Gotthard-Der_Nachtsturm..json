{"dta.poem.3385": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachtsturm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sturm der geilenden Nacht, f&#252;rchterlich tos't", "tokens": ["Sturm", "der", "gei\u00b7len\u00b7den", "Nacht", ",", "f", "&#252;", "rch\u00b7ter\u00b7lich", "to\u00b7s't"], "token_info": ["word", "word", "word", "word", "punct", "word", "XML_entity", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NE", "$(", "ADJD", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "und sch\u00f6n", "tokens": ["und", "sch\u00f6n"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Dein Gel\u00e4ut durch die Nacht. Starker, des J\u00fcnglings", "tokens": ["Dein", "Ge\u00b7l\u00e4ut", "durch", "die", "Nacht", ".", "Star\u00b7ker", ",", "des", "J\u00fcng\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$.", "NN", "$,", "ART", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Geist", "tokens": ["Geist"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Schwellt dein Schmettern und Prasseln,", "tokens": ["Schwellt", "dein", "Schmet\u00b7tern", "und", "Pras\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Wie den Krieger der Pomp der Schlacht.", "tokens": ["Wie", "den", "Krie\u00b7ger", "der", "Pomp", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Rabenschwarz ist die Nacht. Durch die Erebische", "tokens": ["Ra\u00b7ben\u00b7schwarz", "ist", "die", "Nacht", ".", "Durch", "die", "E\u00b7re\u00b7bi\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "W\u00e4lzt der M\u00e4chtige sich sausenden Schwungs daher,", "tokens": ["W\u00e4lzt", "der", "M\u00e4ch\u00b7ti\u00b7ge", "sich", "sau\u00b7sen\u00b7den", "Schwungs", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Beugt die Grossen der Sch\u00f6pfung,", "tokens": ["Beugt", "die", "Gros\u00b7sen", "der", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "St\u00e4upt die H\u00f6hen, und zaus't den Wald.", "tokens": ["St\u00e4upt", "die", "H\u00f6\u00b7hen", ",", "und", "zaus't", "den", "Wald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Orkan, Orkan, was raufst du mir mein Halmendach,", "tokens": ["Or\u00b7kan", ",", "Or\u00b7kan", ",", "was", "raufst", "du", "mir", "mein", "Hal\u00b7men\u00b7dach", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PWS", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Trennst der Sparren Geflecht, schmetterst die Schei-", "tokens": ["Trennst", "der", "Spar\u00b7ren", "Ge\u00b7flecht", ",", "schmet\u00b7terst", "die", "Schei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "ben mir", "tokens": ["ben", "mir"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "In den Fenstern? \u2014 Ich komme,", "tokens": ["In", "den", "Fens\u00b7tern", "?", "Ich", "kom\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wogenth\u00fcrmer, und Trotz sey dir!", "tokens": ["Wo\u00b7gent\u00b7h\u00fcr\u00b7mer", ",", "und", "Trotz", "sey", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VAFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Dem die Erd' und das Meer zittern, ich biete dir", "tokens": ["Dem", "die", "Erd'", "und", "das", "Meer", "zit\u00b7tern", ",", "ich", "bie\u00b7te", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eine freudige Stirn, spotte der feigen Wuth,", "tokens": ["Ei\u00b7ne", "freu\u00b7di\u00b7ge", "Stirn", ",", "spot\u00b7te", "der", "fei\u00b7gen", "Wuth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die die Sterne des Himmels,", "tokens": ["Die", "die", "Ster\u00b7ne", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die die Fackel des Mondes l\u00f6scht,", "tokens": ["Die", "die", "Fa\u00b7ckel", "des", "Mon\u00b7des", "l\u00f6scht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Zittre nicht ob des Walds lautem Gekrach', und", "tokens": ["Zitt\u00b7re", "nicht", "ob", "des", "Walds", "lau\u00b7tem", "Ge\u00b7krach'", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PTKNEG", "KOUS", "ART", "NN", "ADJA", "NN", "$,", "KON"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "nicht", "tokens": ["nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ob des grollenden Meers dumpfen Gebr\u00fcll, und nicht", "tokens": ["Ob", "des", "grol\u00b7len\u00b7den", "Meers", "dum\u00b7pfen", "Ge\u00b7br\u00fcll", ",", "und", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "KON", "PTKNEG"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ob den Riesengebilden,", "tokens": ["Ob", "den", "Rie\u00b7sen\u00b7ge\u00b7bil\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Die dein Hauch in den L\u00fcften ballt. \u2014", "tokens": ["Die", "dein", "Hauch", "in", "den", "L\u00fcf\u00b7ten", "ballt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Doch, ich z\u00fcrne dir nicht. \u2014 Starker, in deiner Kraft", "tokens": ["Doch", ",", "ich", "z\u00fcr\u00b7ne", "dir", "nicht", ".", "Star\u00b7ker", ",", "in", "dei\u00b7ner", "Kraft"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sey mir festlich gegr\u00fcsst! K\u00fchner, ich liebe dich,", "tokens": ["Sey", "mir", "fest\u00b7lich", "ge\u00b7gr\u00fcs\u00b7st", "!", "K\u00fch\u00b7ner", ",", "ich", "lie\u00b7be", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn Allfadern dein P\u00e4an", "tokens": ["Wenn", "All\u00b7fa\u00b7dern", "dein", "P\u00e4\u00b7an"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Harfe der Windsbraut singt.", "tokens": ["Auf", "der", "Har\u00b7fe", "der", "Winds\u00b7braut", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6n und f\u00fcrchterlich ists, wenn du die Weizensaat", "tokens": ["Sch\u00f6n", "und", "f\u00fcrch\u00b7ter\u00b7lich", "ists", ",", "wenn", "du", "die", "Wei\u00b7zen\u00b7saat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Niederm\u00e4hst, wenn dein Arm geisselt den stolzen", "tokens": ["Nie\u00b7der\u00b7m\u00e4hst", ",", "wenn", "dein", "Arm", "geis\u00b7selt", "den", "stol\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Forst,", "tokens": ["Forst", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und mit Pappel und Eiche,", "tokens": ["Und", "mit", "Pap\u00b7pel", "und", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie ein Knabe mit Disteln, spielt.", "tokens": ["Wie", "ein", "Kna\u00b7be", "mit", "Dis\u00b7teln", ",", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Sch\u00f6n und f\u00fcrchterlich ists, wenn du das Meer", "tokens": ["Sch\u00f6n", "und", "f\u00fcrch\u00b7ter\u00b7lich", "ists", ",", "wenn", "du", "das", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "erw\u00fchlst,", "tokens": ["er\u00b7w\u00fchlst", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Sein Verm\u00f6gen zerst\u00e4upst, Schiffe, wie Kr\u00e4usel", "tokens": ["Sein", "Ver\u00b7m\u00f6\u00b7gen", "zer\u00b7st\u00e4u\u00b7pst", ",", "Schif\u00b7fe", ",", "wie", "Kr\u00e4u\u00b7sel"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "NN", "$,", "PWAV", "NN"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "drehst,", "tokens": ["drehst", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Masten knickest, wie Binsen,", "tokens": ["Mas\u00b7ten", "kni\u00b7ckest", ",", "wie", "Bin\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Taue reissest, wie m\u00fcrben Zwirn.", "tokens": ["Tau\u00b7e", "reis\u00b7sest", ",", "wie", "m\u00fcr\u00b7ben", "Zwirn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Nein, ich z\u00fcrne dir nicht, Lauter! Ich preise dich,", "tokens": ["Nein", ",", "ich", "z\u00fcr\u00b7ne", "dir", "nicht", ",", "Lau\u00b7ter", "!", "Ich", "prei\u00b7se", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Deiner Fittige Schwung fachet zur Flamm' empor", "tokens": ["Dei\u00b7ner", "Fit\u00b7ti\u00b7ge", "Schwung", "fa\u00b7chet", "zur", "Flamm'", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jeden glimmenden Funken,", "tokens": ["Je\u00b7den", "glim\u00b7men\u00b7den", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Straffet jeden erschlafften Nerv.", "tokens": ["Straf\u00b7fet", "je\u00b7den", "er\u00b7schlaff\u00b7ten", "Nerv", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Freund, mir hallt dein Gebr\u00fcll fei'rlich, wie Chor-", "tokens": ["Freund", ",", "mir", "hallt", "dein", "Ge\u00b7br\u00fcll", "fei'\u00b7rlich", ",", "wie", "Chor"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "PWAV", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "gesang,", "tokens": ["ge\u00b7sang", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Hehr, wie Tempelgel\u00e4ut, pr\u00e4chtig, wie Orgelsturm \u2014", "tokens": ["Hehr", ",", "wie", "Tem\u00b7pel\u00b7ge\u00b7l\u00e4ut", ",", "pr\u00e4ch\u00b7tig", ",", "wie", "Or\u00b7gel\u00b7sturm"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "$,", "ADJD", "$,", "PWAV", "NN", "$("], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Wilder toben die Pulse,", "tokens": ["Wil\u00b7der", "to\u00b7ben", "die", "Pul\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Rascher schl\u00e4gt das emp\u00f6rte Herz.", "tokens": ["Ra\u00b7scher", "schl\u00e4gt", "das", "em\u00b7p\u00f6r\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "In das Dunkel hinaus st\u00fcrm' ich, in schwarzer Nacht", "tokens": ["In", "das", "Dun\u00b7kel", "hin\u00b7aus", "st\u00fcrm'", "ich", ",", "in", "schwar\u00b7zer", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Klimm' ich felsenhinan, schaue vom stickeln Fels", "tokens": ["Klimm'", "ich", "fel\u00b7sen\u00b7hi\u00b7nan", ",", "schau\u00b7e", "vom", "sti\u00b7ckeln", "Fels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "In das g\u00e4hrende Chaos,", "tokens": ["In", "das", "g\u00e4h\u00b7ren\u00b7de", "Chaos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In die w\u00fchlende Nacht hinaus.", "tokens": ["In", "die", "w\u00fch\u00b7len\u00b7de", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Erd' und Himmel und Meer zittern dir, Trotziger.", "tokens": ["Erd'", "und", "Him\u00b7mel", 