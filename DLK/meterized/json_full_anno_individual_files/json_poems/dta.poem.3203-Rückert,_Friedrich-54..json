{"dta.poem.3203": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist es \u00fcberall, ein Stellvertreter seyn,", "tokens": ["Sch\u00f6n", "ist", "es", "\u00fc\u00b7be\u00b7rall", ",", "ein", "Stell\u00b7ver\u00b7tre\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu gelten f\u00fcr die Welt, und nicht f\u00fcr sich allein.", "tokens": ["Zu", "gel\u00b7ten", "f\u00fcr", "die", "Welt", ",", "und", "nicht", "f\u00fcr", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "KON", "PTKNEG", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die vielen gehn dahin, vom Drang des Tags getrieben,", "tokens": ["Die", "vie\u00b7len", "gehn", "da\u00b7hin", ",", "vom", "Drang", "des", "Tags", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PAV", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo sie gingen, ist nicht ihre Spur geblieben.", "tokens": ["Und", "wo", "sie", "gin\u00b7gen", ",", "ist", "nicht", "ih\u00b7re", "Spur", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Stehn bleiben wenige, das Zeugni\u00df nachzutragen", "tokens": ["Stehn", "blei\u00b7ben", "we\u00b7ni\u00b7ge", ",", "das", "Zeug\u00b7ni\u00df", "nach\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Streben ihrer Zeit, wann andre Zeiten tagen.", "tokens": ["Vom", "Stre\u00b7ben", "ih\u00b7rer", "Zeit", ",", "wann", "and\u00b7re", "Zei\u00b7ten", "ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "PWAV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das sind die Geister auf der Menschheit h\u00f6chsten Stufen,", "tokens": ["Das", "sind", "die", "Geis\u00b7ter", "auf", "der", "Menschheit", "h\u00f6chs\u00b7ten", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bei deren Namen sind die Zeiten aufgerufen.", "tokens": ["Bei", "de\u00b7ren", "Na\u00b7men", "sind", "die", "Zei\u00b7ten", "auf\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VAFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wie ein weit Gebirg am Horizonte sinkt,", "tokens": ["Doch", "wie", "ein", "weit", "Ge\u00b7birg", "am", "Ho\u00b7ri\u00b7zon\u00b7te", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJD", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und endlich sichtbar nur der h\u00f6chste Gipfel blinkt;", "tokens": ["Und", "end\u00b7lich", "sicht\u00b7bar", "nur", "der", "h\u00f6chs\u00b7te", "Gip\u00b7fel", "blinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die vielen Gipfel, die im Ferneduft verschwammen,", "tokens": ["Die", "vie\u00b7len", "Gip\u00b7fel", ",", "die", "im", "Fer\u00b7ne\u00b7duft", "ver\u00b7schwam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind gleichsam unsichtbar im Einen nun beisammen;", "tokens": ["Sind", "gleich\u00b7sam", "un\u00b7sicht\u00b7bar", "im", "Ei\u00b7nen", "nun", "bei\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "APPRART", "ART", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}