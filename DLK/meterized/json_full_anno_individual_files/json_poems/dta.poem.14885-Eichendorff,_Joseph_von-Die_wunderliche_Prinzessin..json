{"dta.poem.14885": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Unter fremdes Volk verschlagen,", "tokens": ["Un\u00b7ter", "frem\u00b7des", "Volk", "ver\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arm und ausgeh\u00f6hnt, verrathen,", "tokens": ["Arm", "und", "aus\u00b7ge\u00b7h\u00f6hnt", ",", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat er treu sich durchgeschlagen,", "tokens": ["Hat", "er", "treu", "sich", "durch\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingedenk der Heldenthaten", "tokens": ["Ein\u00b7ge\u00b7denk", "der", "Hel\u00b7den\u00b7tha\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der gro\u00dfen alten Zeiten,", "tokens": ["Und", "der", "gro\u00b7\u00dfen", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis er, ganz von Wahnsinn trunken,", "tokens": ["Bis", "er", ",", "ganz", "von", "Wahn\u00b7sinn", "trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Endlich so nach langem Streiten", "tokens": ["End\u00b7lich", "so", "nach", "lan\u00b7gem", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Br\u00fcder hat gefunden.", "tokens": ["Sei\u00b7ne", "Br\u00fc\u00b7der", "hat", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}