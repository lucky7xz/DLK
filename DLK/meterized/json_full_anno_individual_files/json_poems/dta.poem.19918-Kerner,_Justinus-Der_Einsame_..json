{"dta.poem.19918": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Einsame .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl gehest du an Liebeshand,", "tokens": ["Wohl", "ge\u00b7hest", "du", "an", "Lie\u00b7bes\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein \u00fcbersel'ger Mann;", "tokens": ["Ein", "\u00fc\u00b7ber\u00b7sel'\u00b7ger", "Mann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich geh' allein, doch mit mir geht", "tokens": ["Ich", "geh'", "al\u00b7lein", ",", "doch", "mit", "mir", "geht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was mich begl\u00fccken kann.", "tokens": ["Was", "mich", "be\u00b7gl\u00fc\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ist des Himmels heilig Blau,", "tokens": ["Es", "ist", "des", "Him\u00b7mels", "hei\u00b7lig", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Aue Blumenpracht,", "tokens": ["Der", "Au\u00b7e", "Blu\u00b7men\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einsamer Nachtigallen Schlag", "tokens": ["Ein\u00b7sa\u00b7mer", "Nach\u00b7ti\u00b7gal\u00b7len", "Schlag"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In wolk'ger W\u00e4lder Nacht.", "tokens": ["In", "wol\u00b7k'\u00b7ger", "W\u00e4l\u00b7der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist der Wolke stiller Lauf", "tokens": ["Es", "ist", "der", "Wol\u00b7ke", "stil\u00b7ler", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lebend'ger Wasser Zug,", "tokens": ["Le\u00b7ben\u00b7d'\u00b7ger", "Was\u00b7ser", "Zug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der gr\u00fcnen Saaten wogend Meer", "tokens": ["Der", "gr\u00fc\u00b7nen", "Saa\u00b7ten", "wo\u00b7gend", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leichter V\u00f6gel Flug.", "tokens": ["Und", "leich\u00b7ter", "V\u00f6\u00b7gel", "Flug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du ruhst im zarten Frauenarm,", "tokens": ["Du", "ruhst", "im", "zar\u00b7ten", "Frau\u00b7en\u00b7arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Rosenmund voll Duft;", "tokens": ["Am", "Ro\u00b7sen\u00b7mund", "voll", "Duft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Einsam geh' ich, im Mantel spielt", "tokens": ["Ein\u00b7sam", "geh'", "ich", ",", "im", "Man\u00b7tel", "spielt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "APPRART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die k\u00fchle Abendluft.", "tokens": ["Die", "k\u00fch\u00b7le", "A\u00b7ben\u00b7dluft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es kommt kein Wandrer mehr des Wegs,", "tokens": ["Es", "kommt", "kein", "Wand\u00b7rer", "mehr", "des", "Wegs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vogel ruht im Baum:", "tokens": ["Der", "Vo\u00b7gel", "ruht", "im", "Baum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schreite durch die d\u00fcstre Nacht,", "tokens": ["Ich", "schrei\u00b7te", "durch", "die", "d\u00fcst\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In mir den hellsten Traum.", "tokens": ["In", "mir", "den", "hells\u00b7ten", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}