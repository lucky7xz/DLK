{"textgrid.poem.66328": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die Forelle", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem B\u00e4chlein helle,", "tokens": ["In", "ei\u00b7nem", "B\u00e4ch\u00b7lein", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da scho\u00df in froher Eil'", "tokens": ["Da", "scho\u00df", "in", "fro\u00b7her", "Eil'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die launige Forelle", "tokens": ["Die", "lau\u00b7ni\u00b7ge", "Fo\u00b7rel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor\u00fcber wie ein Pfeil.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "wie", "ein", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich stand an dem Gestade,", "tokens": ["Ich", "stand", "an", "dem", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sah in s\u00fc\u00dfer Ruh'", "tokens": ["Und", "sah", "in", "s\u00fc\u00b7\u00dfer", "Ruh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des muntern Fisches Bade", "tokens": ["Des", "mun\u00b7tern", "Fi\u00b7sches", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im klaren B\u00e4chlein zu.", "tokens": ["Im", "kla\u00b7ren", "B\u00e4ch\u00b7lein", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Fischer mit der Ruthe", "tokens": ["Ein", "Fi\u00b7scher", "mit", "der", "Ru\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl an dem Ufer stand,", "tokens": ["Wohl", "an", "dem", "U\u00b7fer", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sah's mit kaltem Blute,", "tokens": ["Und", "sah's", "mit", "kal\u00b7tem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sich das Fischlein wand.", "tokens": ["Wie", "sich", "das", "Fisc\u00b7hlein", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So lang dem Wasser Helle,", "tokens": ["So", "lang", "dem", "Was\u00b7ser", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So dacht' ich, nicht gebricht,", "tokens": ["So", "dacht'", "ich", ",", "nicht", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "