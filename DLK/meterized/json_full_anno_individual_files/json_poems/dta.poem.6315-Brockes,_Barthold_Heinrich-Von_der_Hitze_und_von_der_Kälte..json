{"dta.poem.6315": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Damit es sich entz\u00fcnd': auch da\u00df es lange w\u00e4hre;", "tokens": ["Da\u00b7mit", "es", "sich", "ent\u00b7z\u00fcnd'", ":", "auch", "da\u00df", "es", "lan\u00b7ge", "w\u00e4h\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKVZ", "$.", "ADV", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df das Feur verborg\u2019ne Nahrung finden", "tokens": ["So", "mu\u00df", "das", "Feur", "ver\u00b7bor\u00b7g'\u00b7ne", "Nah\u00b7rung", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von Theilchen, die sich leicht entbinden,", "tokens": ["Von", "Theil\u00b7chen", ",", "die", "sich", "leicht", "ent\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit es sich an selben n\u00e4hre.", "tokens": ["Da\u00b7mit", "es", "sich", "an", "sel\u00b7ben", "n\u00e4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein, mu\u00df etwas gleich ihm stets die Nahrung reichen;", "tokens": ["Al\u00b7lein", ",", "mu\u00df", "et\u00b7was", "gleich", "ihm", "stets", "die", "Nah\u00b7rung", "rei\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PIS", "ADV", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So mu\u00df zugleich die Lufft auch seinem Triebe weichen.", "tokens": ["So", "mu\u00df", "zu\u00b7gleich", "die", "Lufft", "auch", "sei\u00b7nem", "Trie\u00b7be", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Daher bek\u00f6mmt die Gluht das fl\u00fcchtige Bewegen,", "tokens": ["Da\u00b7her", "be\u00b7k\u00f6mmt", "die", "Gluht", "das", "fl\u00fcch\u00b7ti\u00b7ge", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wodurch die Theilchen sich im Circkel immer regen,", "tokens": ["Wo\u00b7durch", "die", "Theil\u00b7chen", "sich", "im", "Cir\u00b7ckel", "im\u00b7mer", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "APPRART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als welche sonst sich nicht erheben k\u00f6nnten,", "tokens": ["Als", "wel\u00b7che", "sonst", "sich", "nicht", "er\u00b7he\u00b7ben", "k\u00f6nn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "ADV", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wofern die Lufft, als die ihr Druck verdringt,", "tokens": ["Wo\u00b7fern", "die", "Lufft", ",", "als", "die", "ihr", "Druck", "ver\u00b7dringt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht eine andre zwingt", "tokens": ["Nicht", "ei\u00b7ne", "and\u00b7re", "zwingt"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Denselben Platz, den sie verlassen,", "tokens": ["Den\u00b7sel\u00b7ben", "Platz", ",", "den", "sie", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Geschwinde wiederum zu fassen.", "tokens": ["Ge\u00b7schwin\u00b7de", "wie\u00b7de\u00b7rum", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Gluht vertreibet stets die Lufft, so sie umschr\u00e4nckt", "tokens": ["Die", "Gluht", "ver\u00b7trei\u00b7bet", "stets", "die", "Lufft", ",", "so", "sie", "um\u00b7schr\u00e4nckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Durch ihren schnellen Druck.", "tokens": ["Durch", "ih\u00b7ren", "schnel\u00b7len", "Druck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Die Lufft, um ihr nun leicht den Durchgang zu bereiten,", "tokens": ["Die", "Lufft", ",", "um", "ihr", "nun", "leicht", "den", "Durch\u00b7gang", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PPER", "ADV", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird zu der Gluht im Circkel stets gelenckt.", "tokens": ["Wird", "zu", "der", "Gluht", "im", "Cir\u00b7ckel", "stets", "ge\u00b7lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}