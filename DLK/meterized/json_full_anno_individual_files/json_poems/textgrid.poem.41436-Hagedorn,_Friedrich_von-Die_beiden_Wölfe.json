{"textgrid.poem.41436": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden W\u00f6lfe", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem dicken Wald, wo Wind und Hunger heulten,", "tokens": ["In", "ei\u00b7nem", "di\u00b7cken", "Wald", ",", "wo", "Wind", "und", "Hun\u00b7ger", "heul\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War zweener W\u00f6lfe Sitz, die sich in mancher Nacht", "tokens": ["War", "zwee\u00b7ner", "W\u00f6l\u00b7fe", "Sitz", ",", "die", "sich", "in", "man\u00b7cher", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "NN", "$,", "PRELS", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nichts im Gebi\u00df, als Raubsucht, heimgebracht,", "tokens": ["Nichts", "im", "Ge\u00b7bi\u00df", ",", "als", "Raub\u00b7sucht", ",", "heim\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "APPRART", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die sie recht br\u00fcderlich, und ohne Mi\u00dfgunst theilten.", "tokens": ["Die", "sie", "recht", "br\u00fc\u00b7der\u00b7lich", ",", "und", "oh\u00b7ne", "Mi\u00df\u00b7gunst", "theil\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "$,", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein, sie hatten sich verirrt,", "tokens": ["Al\u00b7lein", ",", "sie", "hat\u00b7ten", "sich", "ver\u00b7irrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zu der Beute nicht den rechten Weg genommen.", "tokens": ["Und", "zu", "der", "Beu\u00b7te", "nicht", "den", "rech\u00b7ten", "Weg", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald aber sehen sie die sch\u00f6nsten Schafe kommen;", "tokens": ["Bald", "a\u00b7ber", "se\u00b7hen", "sie", "die", "sch\u00f6ns\u00b7ten", "Scha\u00b7fe", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch kommen auch zugleich der Hylax und der Hirt.", "tokens": ["Doch", "kom\u00b7men", "auch", "zu\u00b7gleich", "der", "Hy\u00b7lax", "und", "der", "Hirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NE", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wo die Gewalt unbrauchbar ist,", "tokens": ["Wo", "die", "Ge\u00b7walt", "un\u00b7brauch\u00b7bar", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bedient sich auch ein Wolf der List.", "tokens": ["Be\u00b7dient", "sich", "auch", "ein", "Wolf", "der", "List", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie halten Kriegesrath. Lycaons Enkel spricht:", "tokens": ["Sie", "hal\u00b7ten", "Krie\u00b7ges\u00b7rath", ".", "Ly\u00b7ca\u00b7ons", "En\u00b7kel", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein rechter Angriff hilft hier nicht.", "tokens": ["Ein", "rech\u00b7ter", "An\u00b7griff", "hilft", "hier", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will mich hinter jenen Hecken,", "tokens": ["Ich", "will", "mich", "hin\u00b7ter", "je\u00b7nen", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Graben, tief genug verstecken,", "tokens": ["Im", "Gra\u00b7ben", ",", "tief", "ge\u00b7nug", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann mu\u00dft du, fern von mir, der Heerde Furcht erwecken.", "tokens": ["Dann", "mu\u00dft", "du", ",", "fern", "von", "mir", ",", "der", "Heer\u00b7de", "Furcht", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJD", "APPR", "PPER", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Trab auf sie zu, und la\u00df dich sehn:", "tokens": ["Trab", "auf", "sie", "zu", ",", "und", "la\u00df", "dich", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sch\u00e4fer wird dich bald entdecken,", "tokens": ["Der", "Sch\u00e4\u00b7fer", "wird", "dich", "bald", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und mit dem Hunde dir gewi\u00df entgegen gehn.", "tokens": ["Und", "mit", "dem", "Hun\u00b7de", "dir", "ge\u00b7wi\u00df", "ent\u00b7ge\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADV", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da werd' ich schnell den Raub vollstrecken;", "tokens": ["Da", "werd'", "ich", "schnell", "den", "Raub", "voll\u00b7stre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Kunst der Flucht mu\u00dft du verstehn.", "tokens": ["Die", "Kunst", "der", "Flucht", "mu\u00dft", "du", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der andre Wolf bejaht's, gestand, da\u00df sein Gef\u00e4hrte", "tokens": ["Der", "and\u00b7re", "Wolf", "be\u00b7jaht's", ",", "ge\u00b7stand", ",", "da\u00df", "sein", "Ge\u00b7f\u00e4hr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "NE", "$,", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sich, als ein alter Wolf, erkl\u00e4rte,", "tokens": ["Sich", ",", "als", "ein", "al\u00b7ter", "Wolf", ",", "er\u00b7kl\u00e4r\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "$,", "KOUS", "ART", "ADJA", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und hie\u00df den Anschlag wundersch\u00f6n.", "tokens": ["Und", "hie\u00df", "den", "An\u00b7schlag", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie trennen sich, und dieser naht hinan.", "tokens": ["Sie", "tren\u00b7nen", "sich", ",", "und", "die\u00b7ser", "naht", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KON", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man sieht ihn; Hylax bellt! den Erbfeind zu erwischen,", "tokens": ["Man", "sieht", "ihn", ";", "Hy\u00b7lax", "bellt", "!", "den", "Erb\u00b7feind", "zu", "er\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$.", "NE", "VVFIN", "$.", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sucht ihn der Sch\u00e4fer oft im Wettlauf anzufrischen.", "tokens": ["Sucht", "ihn", "der", "Sch\u00e4\u00b7fer", "oft", "im", "Wett\u00b7lauf", "an\u00b7zu\u00b7fri\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihm setzen beide nach: doch k\u00f6mmt ihm keiner an,", "tokens": ["Ihm", "set\u00b7zen", "bei\u00b7de", "nach", ":", "doch", "k\u00f6mmt", "ihm", "kei\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und jener schleicht aus den Geb\u00fcschen,", "tokens": ["Und", "je\u00b7ner", "schleicht", "aus", "den", "Ge\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und stiehlt das beste Schaf, das man nur stehlen kann.", "tokens": ["Und", "stiehlt", "das", "bes\u00b7te", "Schaf", ",", "das", "man", "nur", "steh\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So wird man oftmals der Gefahr,", "tokens": ["So", "wird", "man", "oft\u00b7mals", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", 