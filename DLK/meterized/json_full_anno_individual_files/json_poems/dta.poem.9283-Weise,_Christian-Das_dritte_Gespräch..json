{"dta.poem.9283": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.71", "af:0.28"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein werthes kind wofern ihr ja beliebt", "tokens": ["Mein", "wert\u00b7hes", "kind", "wo\u00b7fern", "ihr", "ja", "be\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu wissen was mein freyes hertz betr\u00fcbt/", "tokens": ["Zu", "wis\u00b7sen", "was", "mein", "frey\u00b7es", "hertz", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRELS", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So gebe sie ein zeichen im gesicht/", "tokens": ["So", "ge\u00b7be", "sie", "ein", "zei\u00b7chen", "im", "ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Darf ich mich ihr vertrauen oder nicht?", "tokens": ["Darf", "ich", "mich", "ihr", "ver\u00b7trau\u00b7en", "o\u00b7der", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PPER", "VVINF", "KON", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "2. Die mutter wil; drum mu\u00df es auch geschehn/", "tokens": ["Die", "mut\u00b7ter", "wil", ";", "drum", "mu\u00df", "es", "auch", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$.", "PAV", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich soll nunmehr nach einer liebsten sehn/", "tokens": ["Ich", "soll", "nun\u00b7mehr", "nach", "ei\u00b7ner", "liebs\u00b7ten", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie beut mir hau\u00df/ hof und die nahrung an", "tokens": ["Sie", "beut", "mir", "hau\u00df", "/", "hof", "und", "die", "nah\u00b7rung", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "NN", "KON", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich mich selbst nicht l\u00e4nger wehren kan.", "tokens": ["Da\u00df", "ich", "mich", "selbst", "nicht", "l\u00e4n\u00b7ger", "weh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}