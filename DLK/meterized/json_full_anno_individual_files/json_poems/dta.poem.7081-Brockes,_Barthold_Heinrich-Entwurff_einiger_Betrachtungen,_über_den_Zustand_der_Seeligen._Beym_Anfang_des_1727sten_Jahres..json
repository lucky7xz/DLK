{"dta.poem.7081": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOTT! was stellt sich mir", "tokens": ["Mein", "GoTT", "!", "was", "stellt", "sich", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VVFIN", "PRF", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Daselbst f\u00fcr eine Tieff\u2019 und Weite f\u00fcr!", "tokens": ["Da\u00b7selbst", "f\u00fcr", "ei\u00b7ne", "Tief\u00b7f'", "und", "Wei\u00b7te", "f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "APPR", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Indem wir, nicht wie hier von unsrer Erden,", "tokens": ["In\u00b7dem", "wir", ",", "nicht", "wie", "hier", "von", "uns\u00b7rer", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PTKNEG", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den halben Himmels-Crei\u00df, nur \u00fcbersehen werden;", "tokens": ["Den", "hal\u00b7ben", "Him\u00b7mels\u00b7Crei\u00df", ",", "nur", "\u00fc\u00b7ber\u00b7se\u00b7hen", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man wird so gar die Himmels-R\u00fcnde gantz,", "tokens": ["Man", "wird", "so", "gar", "die", "Him\u00b7mels\u00b7R\u00fcn\u00b7de", "gantz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O Wunder! voller Licht und Glantz,", "tokens": ["O", "Wun\u00b7der", "!", "vol\u00b7ler", "Licht", "und", "Glantz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O w\u00fcrdigs Haus des Sch\u00f6pffers aller Welt!", "tokens": ["O", "w\u00fcr\u00b7digs", "Haus", "des", "Sch\u00f6pf\u00b7fers", "al\u00b7ler", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als einen hellen Saal der pr\u00e4chtig ausgeziert,", "tokens": ["Als", "ei\u00b7nen", "hel\u00b7len", "Saal", "der", "pr\u00e4ch\u00b7tig", "aus\u00b7ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und der, an statt mit Licht und Fackeln sich zu schm\u00fccken,", "tokens": ["Und", "der", ",", "an", "statt", "mit", "Licht", "und", "Fa\u00b7ckeln", "sich", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "APPR", "ADJD", "APPR", "NN", "KON", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Von einem Sonnen-Heer illuminirt;", "tokens": ["Von", "ei\u00b7nem", "Son\u00b7nen\u00b7Heer", "il\u00b7lu\u00b7mi\u00b7nirt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Durch solche Pracht entz\u00fcckt, in seel\u2019ger Lust, erblicken.", "tokens": ["Durch", "sol\u00b7che", "Pracht", "ent\u00b7z\u00fcckt", ",", "in", "seel'\u00b7ger", "Lust", ",", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}