{"dta.poem.11931": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gebrauche deine Kraft nur G\u00fcter zu erwerben,", "tokens": ["Ge\u00b7brau\u00b7che", "dei\u00b7ne", "Kraft", "nur", "G\u00fc\u00b7ter", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du gebrauchen kannst zum Leben und zum Sterben.", "tokens": ["Die", "du", "ge\u00b7brau\u00b7chen", "kannst", "zum", "Le\u00b7ben", "und", "zum", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "N\u00fctzt irdischer Erwerb zum einen nur allein,", "tokens": ["N\u00fctzt", "ir\u00b7di\u00b7scher", "Er\u00b7werb", "zum", "ei\u00b7nen", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "PIS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist der geistige gleich n\u00fctz zu allen zwein.", "tokens": ["So", "ist", "der", "geis\u00b7ti\u00b7ge", "gleich", "n\u00fctz", "zu", "al\u00b7len", "zwein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn wie der Leib bestehn nicht ohne Speise kann,", "tokens": ["Denn", "wie", "der", "Leib", "be\u00b7stehn", "nicht", "oh\u00b7ne", "Spei\u00b7se", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ohne Wissen nicht, wer einen Geist gewann.", "tokens": ["So", "oh\u00b7ne", "Wis\u00b7sen", "nicht", ",", "wer", "ei\u00b7nen", "Geist", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKNEG", "$,", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den irdischen Besitz vererbest du beim Sterben;", "tokens": ["Den", "ir\u00b7di\u00b7schen", "Be\u00b7sitz", "ver\u00b7er\u00b7best", "du", "beim", "Ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O such den geist'gen auch beim Scheiden zu vererben.", "tokens": ["O", "such", "den", "geist'\u00b7gen", "auch", "beim", "Schei\u00b7den", "zu", "ver\u00b7er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du l\u00e4ssest irgendwie der Welt ihn eingepr\u00e4gt,", "tokens": ["Du", "l\u00e4s\u00b7sest", "ir\u00b7gend\u00b7wie", "der", "Welt", "ihn", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Korn, das Wurzel schl\u00e4gt, als Zweig, der Fr\u00fcchte tr\u00e4gt.", "tokens": ["Als", "Korn", ",", "das", "Wur\u00b7zel", "schl\u00e4gt", ",", "als", "Zweig", ",", "der", "Fr\u00fcch\u00b7te", "tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "NN", "VVFIN", "$,", "KOUS", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dir selbst ist dort villeicht, wie was du hier besessen,", "tokens": ["Dir", "selbst", "ist", "dort", "vil\u00b7leicht", ",", "wie", "was", "du", "hier", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADV", "$,", "PWAV", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch was du hier gewu\u00dft, verloren und vergessen.", "tokens": ["Auch", "was", "du", "hier", "ge\u00b7wu\u00dft", ",", "ver\u00b7lo\u00b7ren", "und", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "ADV", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Allein die Kraft, die es erwarb, ist nicht verloren;", "tokens": ["Al\u00b7lein", "die", "Kraft", ",", "die", "es", "er\u00b7warb", ",", "ist", "nicht", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zu h\u00f6herem Erwerb ist sie dir neu geboren.", "tokens": ["Zu", "h\u00f6\u00b7he\u00b7rem", "Er\u00b7werb", "ist", "sie", "dir", "neu", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Drum auf Erwerben \u00fcb' im Ernst der Kr\u00e4fte Spiel;", "tokens": ["Drum", "auf", "Er\u00b7wer\u00b7ben", "\u00fcb'", "im", "Ernst", "der", "Kr\u00e4f\u00b7te", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht der Erwerb ist hier, die Uebung ist das Ziel.", "tokens": ["Nicht", "der", "Er\u00b7werb", "ist", "hier", ",", "die", "Ue\u00b7bung", "ist", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie eines Knaben Flei\u00df bald bunte Steinchen sammelt,", "tokens": ["Wie", "ei\u00b7nes", "Kna\u00b7ben", "Flei\u00df", "bald", "bun\u00b7te", "Stein\u00b7chen", "sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald W\u00f6rter ohne Sinn' in fremder Sprache stammelt;", "tokens": ["Bald", "W\u00f6r\u00b7ter", "oh\u00b7ne", "Sinn'", "in", "frem\u00b7der", "Spra\u00b7che", "stam\u00b7melt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Was hat der Mann dereinst vom Steinchen und vom Wort?", "tokens": ["Was", "hat", "der", "Mann", "de\u00b7reinst", "vom", "Stein\u00b7chen", "und", "vom", "Wort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat nun Lust und Kraft zu sammeln andern Hort.", "tokens": ["Er", "hat", "nun", "Lust", "und", "Kraft", "zu", "sam\u00b7meln", "an\u00b7dern", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}