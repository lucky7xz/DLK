{"dta.poem.1617": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sperantzens Grabschrifft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des J\u00e4gers Hoffnung ist nun todt/ als sie schlo\u00df ihr bejahr-", "tokens": ["Des", "J\u00e4\u00b7gers", "Hoff\u00b7nung", "ist", "nun", "todt", "/", "als", "sie", "schlo\u00df", "ihr", "be\u00b7jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "$(", "KOUS", "PPER", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "tes Leben/", "tokens": ["tes", "Le\u00b7ben", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So hat ihr Herr das W\u00e4yde-Werck voll Leyd und Unmutt \u00fc-", "tokens": ["So", "hat", "ihr", "Herr", "das", "W\u00e4y\u00b7de\u00b7\u00b7Werck", "voll", "Leyd", "und", "Un\u00b7mutt", "\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "NN", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "bergeben.", "tokens": ["ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wer ohne Hoffnung jagt/ wird wenig Wildpret fangen/", "tokens": ["Wer", "oh\u00b7ne", "Hoff\u00b7nung", "jagt", "/", "wird", "we\u00b7nig", "Wild\u00b7pret", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$(", "VAFIN", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verdru\u00df vor seine M\u00fch zum besten Lohn erlangen.", "tokens": ["Ver\u00b7dru\u00df", "vor", "sei\u00b7ne", "M\u00fch", "zum", "bes\u00b7ten", "Lohn", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er schrieb mit Trauren bey ihr Grab: Hier liegt der Prei\u00df von", "tokens": ["Er", "schrieb", "mit", "Trau\u00b7ren", "bey", "ihr", "Grab", ":", "Hier", "liegt", "der", "Prei\u00df", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "tausend Winden/", "tokens": ["tau\u00b7send", "Win\u00b7den", "/"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "An zarter Sch\u00f6nheit/ G\u00fctt und Treu ist ihres gleichen nicht zu", "tokens": ["An", "zar\u00b7ter", "Sch\u00f6n\u00b7heit", "/", "G\u00fctt", "und", "Treu", "ist", "ih\u00b7res", "glei\u00b7chen", "nicht", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "NN", "KON", "NN", "VAFIN", "PPOSAT", "ADJA", "PTKNEG", "PTKZU"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.10": {"text": "finden.", "tokens": ["fin\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "In sechzehn Jahren war ihr wenig Wild entgangen/", "tokens": ["In", "sech\u00b7zehn", "Jah\u00b7ren", "war", "ihr", "we\u00b7nig", "Wild", "ent\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Izt mu\u00df sie selber sich vom Tode lassen fangen.", "tokens": ["Izt", "mu\u00df", "sie", "sel\u00b7ber", "sich", "vom", "To\u00b7de", "las\u00b7sen", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PRF", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}