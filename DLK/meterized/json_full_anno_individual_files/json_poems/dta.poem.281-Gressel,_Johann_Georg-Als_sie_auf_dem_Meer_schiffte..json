{"dta.poem.281": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie auf dem Meer schiffte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wehet ihr L\u00fcffte/ verdoppelt euch Winde/", "tokens": ["We\u00b7het", "ihr", "L\u00fcff\u00b7te", "/", "ver\u00b7dop\u00b7pelt", "euch", "Win\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Eilet und jaget", "tokens": ["Ei\u00b7let", "und", "ja\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bringet und traget", "tokens": ["Brin\u00b7get", "und", "tra\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Syris die Sch\u00f6ne zur Stelle geschwinde.", "tokens": ["Sy\u00b7ris", "die", "Sch\u00f6\u00b7ne", "zur", "Stel\u00b7le", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Spielet ihr Fluhten fein sanfft und gelinde/", "tokens": ["Spie\u00b7let", "ihr", "Fluh\u00b7ten", "fein", "sanfft", "und", "ge\u00b7lin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "ADJD", "KON", "ADJA", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Lasset die Wellen", "tokens": ["Las\u00b7set", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Friedsahm sich stellen/", "tokens": ["Fried\u00b7sahm", "sich", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "St\u00fcrme vergehet doch vor diesen Kinde.", "tokens": ["St\u00fcr\u00b7me", "ver\u00b7ge\u00b7het", "doch", "vor", "die\u00b7sen", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Stehe auf ", "tokens": ["Ste\u00b7he", "auf"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Lege die Fluhten/", "tokens": ["Le\u00b7ge", "die", "Fluh\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zeige die Ruhten/", "tokens": ["Zei\u00b7ge", "die", "Ruh\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Deiner Hand dem Meer/ da\u00df sie den Port finde.", "tokens": ["Dei\u00b7ner", "Hand", "dem", "Meer", "/", "da\u00df", "sie", "den", "Port", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+-++-", "measure": "unknown.measure.hexa"}}}}}