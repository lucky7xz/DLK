{"dta.poem.1232": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du angenehmer H\u00e4yn voll stiller Einsamkeiten/", "tokens": ["Du", "an\u00b7ge\u00b7neh\u00b7mer", "H\u00e4yn", "voll", "stil\u00b7ler", "Ein\u00b7sam\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie s\u00fc\u00df und lieblich bist du mir!", "tokens": ["Wie", "s\u00fc\u00df", "und", "lieb\u00b7lich", "bist", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was mein betr\u00fcbter Mund verschweigen mu\u00df bey Leuten/", "tokens": ["Was", "mein", "be\u00b7tr\u00fcb\u00b7ter", "Mund", "ver\u00b7schwei\u00b7gen", "mu\u00df", "bey", "Leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVINF", "VMFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das bringt er ohne Scheu den stummen B\u00e4umen f\u00fcr.", "tokens": ["Das", "bringt", "er", "oh\u00b7ne", "Scheu", "den", "stum\u00b7men", "B\u00e4u\u00b7men", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein andrer sey bem\u00fcht zu bergen seine Plagen/", "tokens": ["Ein", "an\u00b7drer", "sey", "be\u00b7m\u00fcht", "zu", "ber\u00b7gen", "sei\u00b7ne", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschliesse schweigend seine Zeit;", "tokens": ["Ver\u00b7schlies\u00b7se", "schwei\u00b7gend", "sei\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich werde dir hinfort mit heller Stimme sagen/", "tokens": ["Ich", "wer\u00b7de", "dir", "hin\u00b7fort", "mit", "hel\u00b7ler", "Stim\u00b7me", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was meinen Geist versenckt in schweres Hertzeleid.", "tokens": ["Was", "mei\u00b7nen", "Geist", "ver\u00b7senckt", "in", "schwe\u00b7res", "Hert\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Kinder leichter Lufft/ so um die B\u00e4ume stecken/", "tokens": ["Die", "Kin\u00b7der", "leich\u00b7ter", "Lufft", "/", "so", "um", "die", "B\u00e4u\u00b7me", "ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich beginn ein Trauer-Lied/", "tokens": ["Wenn", "ich", "be\u00b7ginn", "ein", "Trau\u00b7e\u00b7rLied", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ver\u00e4ndern ihren Schall alsbald auff deinen Hecken/", "tokens": ["Ver\u00b7\u00e4n\u00b7dern", "ih\u00b7ren", "Schall", "als\u00b7bald", "auff", "dei\u00b7nen", "He\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Seyn zu beklagen mich durch gleichen Thon bem\u00fcht.", "tokens": ["Seyn", "zu", "be\u00b7kla\u00b7gen", "mich", "durch", "glei\u00b7chen", "Thon", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKZU", "VVINF", "PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Der heisre Widerhall in deinen W\u00fcsteneyen", "tokens": ["Der", "heis\u00b7re", "Wi\u00b7der\u00b7hall", "in", "dei\u00b7nen", "W\u00fcs\u00b7te\u00b7ne\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Verdoppelt seinen Leid-Gesang/", "tokens": ["Ver\u00b7dop\u00b7pelt", "sei\u00b7nen", "Lei\u00b7dGe\u00b7sang", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht/ da\u00df er seine Lieb und Schmertzen will beschreyen/", "tokens": ["Nicht", "/", "da\u00df", "er", "sei\u00b7ne", "Lieb", "und", "Schmert\u00b7zen", "will", "be\u00b7schre\u00b7yen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$(", "KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur da\u00df er meine Klag und Seufftzer mache lang.", "tokens": ["Nur", "da\u00df", "er", "mei\u00b7ne", "Klag", "und", "Seufft\u00b7zer", "ma\u00b7che", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die B\u00e4che welche sonst in ihrer Ordnung fliessen", "tokens": ["Die", "B\u00e4\u00b7che", "wel\u00b7che", "sonst", "in", "ih\u00b7rer", "Ord\u00b7nung", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch das begr\u00fcnte Blumen-Feld/", "tokens": ["Durch", "das", "be\u00b7gr\u00fcn\u00b7te", "Blu\u00b7men\u00b7Feld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sieht man von sich selbst die Wiesen \u00fcbergiessen/", "tokens": ["Die", "sieht", "man", "von", "sich", "selbst", "die", "Wie\u00b7sen", "\u00fc\u00b7berg\u00b7ies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "PRF", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als w\u00e4ren sie von mir mit Thr\u00e4nen auffgeschwellt.", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "von", "mir", "mit", "Thr\u00e4\u00b7nen", "auff\u00b7ge\u00b7schwellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Eichen fester Stamm/ die Last der harten Steine/", "tokens": ["Der", "Ei\u00b7chen", "fes\u00b7ter", "Stamm", "/", "die", "Last", "der", "har\u00b7ten", "Stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bewegt durch meine Pein und Qual/", "tokens": ["Be\u00b7wegt", "durch", "mei\u00b7ne", "Pein", "und", "Qual", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerreist in St\u00fcck und springt in Dr\u00fcmmer/ wenn ich weine/", "tokens": ["Zer\u00b7reist", "in", "St\u00fcck", "und", "springt", "in", "Dr\u00fcm\u00b7mer", "/", "wenn", "ich", "wei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "VVFIN", "APPR", "NN", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum Zeichen/ da\u00df sie mich beklagen allzumahl.", "tokens": ["Zum", "Zei\u00b7chen", "/", "da\u00df", "sie", "mich", "be\u00b7kla\u00b7gen", "all\u00b7zu\u00b7mahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Drum/ angenehmer Wald/ du Trotz der rauhen Winde/", "tokens": ["Drum", "/", "an\u00b7ge\u00b7neh\u00b7mer", "Wald", "/", "du", "Trotz", "der", "rau\u00b7hen", "Win\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "ADJA", "NN", "$(", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie s\u00fc\u00df und lieblich bist du mir!", "tokens": ["Wie", "s\u00fc\u00df", "und", "lieb\u00b7lich", "bist", "du", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dieweil ich \u00fcberall bey dir Erbarmen finde/", "tokens": ["Die\u00b7weil", "ich", "\u00fc\u00b7be\u00b7rall", "bey", "dir", "Er\u00b7bar\u00b7men", "fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So leg ich iederzeit mein Seufftzen ab bey dir.", "tokens": ["So", "leg", "ich", "ie\u00b7der\u00b7zeit", "mein", "Seufft\u00b7zen", "ab", "bey", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}