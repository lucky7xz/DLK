{"textgrid.poem.52608": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbla\u00df, o la\u00df mich weinen, la\u00df mich klagen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbla\u00df, o la\u00df mich weinen, la\u00df mich klagen,", "tokens": ["\u00bb", "la\u00df", ",", "o", "la\u00df", "mich", "wei\u00b7nen", ",", "la\u00df", "mich", "kla\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "FM", "VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wolle nicht nach meinen Thr\u00e4nen fragen,", "tokens": ["Wol\u00b7le", "nicht", "nach", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach mein Klagen, ach mein seltsam Weinen", "tokens": ["Ach", "mein", "Kla\u00b7gen", ",", "ach", "mein", "selt\u00b7sam", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "$,", "XY", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Locket in die Augen auch die Deinen.", "tokens": ["Lo\u00b7cket", "in", "die", "Au\u00b7gen", "auch", "die", "Dei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ART", "PPOSAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Leb wohl, leb wohl!", "tokens": ["Leb", "wohl", ",", "leb", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich wei\u00df nicht, was ich sagen soll.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "sa\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was ich gerne sagen m\u00f6cht,", "tokens": ["Und", "was", "ich", "ger\u00b7ne", "sa\u00b7gen", "m\u00f6cht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ueber die Zung ichs nimmer br\u00e4cht.", "tokens": ["Ue\u00b7ber", "die", "Zung", "ichs", "nim\u00b7mer", "br\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Blicke freundlich, liebe gute Seele,", "tokens": ["Bli\u00b7cke", "freund\u00b7lich", ",", "lie\u00b7be", "gu\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deinen stillen Kummer nicht verhehle!", "tokens": ["Dei\u00b7nen", "stil\u00b7len", "Kum\u00b7mer", "nicht", "ver\u00b7heh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sprich du nur, in einem sanften Herzen", "tokens": ["Sprich", "du", "nur", ",", "in", "ei\u00b7nem", "sanf\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Will ich treu bewahren deine Schmerzen.", "tokens": ["Will", "ich", "treu", "be\u00b7wah\u00b7ren", "dei\u00b7ne", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Du bist so weich,", "tokens": ["Du", "bist", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ach, sch\u00f6nes Kind, du bist so bleich!", "tokens": ["Ach", ",", "sch\u00f6\u00b7nes", "Kind", ",", "du", "bist", "so", "bleich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gib mir die Hand, wie stumm du bist!", "tokens": ["Gib", "mir", "die", "Hand", ",", "wie", "stumm", "du", "bist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du wei\u00dft, da\u00df man zum Abschied k\u00fc\u00dft.", "tokens": ["Du", "wei\u00dft", ",", "da\u00df", "man", "zum", "Ab\u00b7schied", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df, o la\u00df, denn ich mu\u00df immer weinen,", "tokens": ["La\u00df", ",", "o", "la\u00df", ",", "denn", "ich", "mu\u00df", "im\u00b7mer", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "FM", "$,", "KON", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mond und Sonne werden nicht mehr scheinen,", "tokens": ["Mond", "und", "Son\u00b7ne", "wer\u00b7den", "nicht", "mehr", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lenz und Lust sind ewig mir verflogen,", "tokens": ["Lenz", "und", "Lust", "sind", "e\u00b7wig", "mir", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit den Schwalben sind sie fortgezogen.", "tokens": ["Mit", "den", "Schwal\u00b7ben", "sind", "sie", "fort\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Du theurer Mann!", "tokens": ["Du", "theu\u00b7rer", "Mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Verzeih, da\u00df ich nur weinen kann.", "tokens": ["Ver\u00b7zeih", ",", "da\u00df", "ich", "nur", "wei\u00b7nen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein W\u00f6rtlein dr\u00fcckt das Herz mir ab,", "tokens": ["Ein", "W\u00f6rt\u00b7lein", "dr\u00fcckt", "das", "Herz", "mir", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Schweigen selber ist mein Grab.\u00ab", "tokens": ["Und", "Schwei\u00b7gen", "sel\u00b7ber", "ist", "mein", "Grab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kannst du reden nicht, und kannst nicht schweigen,", "tokens": ["Kannst", "du", "re\u00b7den", "nicht", ",", "und", "kannst", "nicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKNEG", "$,", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00f6chtest mir dein ganzes Innre zeigen \u2013", "tokens": ["M\u00f6ch\u00b7test", "mir", "dein", "gan\u00b7zes", "Inn\u00b7re", "zei\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lebe wohl! la\u00df dich zum Abschied k\u00fcssen;", "tokens": ["Le\u00b7be", "wohl", "!", "la\u00df", "dich", "zum", "Ab\u00b7schied", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sollten wir f\u00fcr immer scheiden m\u00fcssen?", "tokens": ["Soll\u00b7ten", "wir", "f\u00fcr", "im\u00b7mer", "schei\u00b7den", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Geliebte du!", "tokens": ["Ge\u00b7lieb\u00b7te", "du", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nimm diesen Ku\u00df und den dazu!", "tokens": ["Nimm", "die\u00b7sen", "Ku\u00df", "und", "den", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "KON", "ART", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon sind wir uns gesunken, sieh!", "tokens": ["Schon", "sind", "wir", "uns", "ge\u00b7sun\u00b7ken", ",", "sieh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,", "VVIMP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ans Herz und wissen selbst nicht wie.", "tokens": ["Ans", "Herz", "und", "wis\u00b7sen", "selbst", "nicht", "wie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbla\u00df, o la\u00df mich weinen, la\u00df mich klagen,", "tokens": ["\u00bb", "la\u00df", ",", "o", "la\u00df", "mich", "wei\u00b7nen", ",", "la\u00df", "mich", "kla\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "FM", "VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "met