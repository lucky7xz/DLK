{"dta.poem.13803": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-Tafel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr aber, klug' Gesellen,", "tokens": ["Ihr", "a\u00b7ber", ",", "klug'", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hier mit in dem Kreise,", "tokens": ["Die", "hier", "mit", "in", "dem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl qu\u00e4lt Ihr mich seit Jahren", "tokens": ["Wohl", "qu\u00e4lt", "Ihr", "mich", "seit", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit weisem Rath und Wort. \u2014", "tokens": ["Mit", "wei\u00b7sem", "Rath", "und", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sto\u00dft an, es sei vergessen!", "tokens": ["Sto\u00dft", "an", ",", "es", "sei", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Meere, ungemessen,", "tokens": ["Im", "Mee\u00b7re", ",", "un\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sind viele tausend Wellen", "tokens": ["Sind", "vie\u00b7le", "tau\u00b7send", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und tausend Schiffe fahren", "tokens": ["Und", "tau\u00b7send", "Schif\u00b7fe", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein jedes seine Reise,", "tokens": ["Ein", "je\u00b7des", "sei\u00b7ne", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Komm' jedes in seinen Port!", "tokens": ["Komm'", "je\u00b7des", "in", "sei\u00b7nen", "Port", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}