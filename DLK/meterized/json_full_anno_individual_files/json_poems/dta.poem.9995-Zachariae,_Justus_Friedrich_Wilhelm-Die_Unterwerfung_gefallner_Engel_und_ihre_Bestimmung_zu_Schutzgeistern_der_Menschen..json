{"dta.poem.9995": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Unterwerfung gefallner Engel  \n und ihre Bestimmung  \n zu  \n Schutzgeistern der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Also sprach er: Sie folgten ihm nach, und wand-", "tokens": ["Al\u00b7so", "sprach", "er", ":", "Sie", "folg\u00b7ten", "ihm", "nach", ",", "und", "wan\u00b7d"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "TRUNC"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nach dem einsamen Lager. In melancholischer Stille", "tokens": ["Nach", "dem", "ein\u00b7sa\u00b7men", "La\u00b7ger", ".", "In", "me\u00b7lan\u00b7cho\u00b7li\u00b7scher", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Lag es, und alles umher war stumm, und ver\u00f6det, und", "tokens": ["Lag", "es", ",", "und", "al\u00b7les", "um\u00b7her", "war", "stumm", ",", "und", "ver\u00b7\u00f6\u00b7det", ",", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "PIS", "PTKVZ", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "$,", "KON"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Aufgeth\u00fcrmt lagen im Feld die hellen schimmernden", "tokens": ["Auf\u00b7ge\u00b7th\u00fcrmt", "la\u00b7gen", "im", "Feld", "die", "hel\u00b7len", "schim\u00b7mern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPRART", "NN", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Oder hiengen zerstreut an den Aesten. In h\u00e4ufigen", "tokens": ["O\u00b7der", "hien\u00b7gen", "zer\u00b7streut", "an", "den", "A\u00b7es\u00b7ten", ".", "In", "h\u00e4u\u00b7fi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "APPR", "ART", "NN", "$.", "APPR", "ADJA"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Jrrten die kriegrischen Geister umher in Th\u00e4lern und", "tokens": ["Jrr\u00b7ten", "die", "krie\u00b7gri\u00b7schen", "Geis\u00b7ter", "um\u00b7her", "in", "Th\u00e4\u00b7lern", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "APPR", "NN", "KON"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Ohne Waffen, und hiengen best\u00fcrzt, voll Kummer im", "tokens": ["Oh\u00b7ne", "Waf\u00b7fen", ",", "und", "hien\u00b7gen", "be\u00b7st\u00fcrzt", ",", "voll", "Kum\u00b7mer", "im"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "ADJD", "$,", "ADJD", "NN", "APPRART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Jhren finstern Gedanken nach, die helle Posaune", "tokens": ["Ih\u00b7ren", "fins\u00b7tern", "Ge\u00b7dan\u00b7ken", "nach", ",", "die", "hel\u00b7le", "Po\u00b7sau\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Weckte zu Klagen allein; und von den schimmernden", "tokens": ["Weck\u00b7te", "zu", "Kla\u00b7gen", "al\u00b7lein", ";", "und", "von", "den", "schim\u00b7mern\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ADV", "$.", "KON", "APPR", "ART", "ADJA"], "meter": "+--+---+--+--", "measure": "dactylic.di.plus"}, "line.10": {"text": "Wehten die hohen Paniere nicht mehr vom Winde", "tokens": ["Weh\u00b7ten", "die", "ho\u00b7hen", "Pa\u00b7nie\u00b7re", "nicht", "mehr", "vom", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Einer der m\u00e4chtigsten Thronen, Orion, der F\u00fchrer des", "tokens": ["Ei\u00b7ner", "der", "m\u00e4ch\u00b7tigs\u00b7ten", "Thro\u00b7nen", ",", "O\u00b7rion", ",", "der", "F\u00fch\u00b7rer", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "$,", "NN", "$,", "ART", "NN", "ART"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.12": {"text": "Sa\u00df im stillen Gezelt. Jhn dr\u00fcckten Lasten von Qualen", "tokens": ["Sa\u00df", "im", "stil\u00b7len", "Ge\u00b7zelt", ".", "Jhn", "dr\u00fcck\u00b7ten", "Las\u00b7ten", "von", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN", "APPR", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Auf der Seele, mit Unruh und Reu, da\u00df Satans", "tokens": ["Auf", "der", "See\u00b7le", ",", "mit", "Un\u00b7ruh", "und", "Reu", ",", "da\u00df", "Sa\u00b7tans"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NE", "$,", "KOUS", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Er gefolgt; ihn verzehrte der Gram; die brennenden", "tokens": ["Er", "ge\u00b7folgt", ";", "ihn", "ver\u00b7zehr\u00b7te", "der", "Gram", ";", "die", "bren\u00b7nen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "ART", "ADJA"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Rannen ihm \u00fcber die Wangen, ihm lag die Erwartung", "tokens": ["Ran\u00b7nen", "ihm", "\u00fc\u00b7ber", "die", "Wan\u00b7gen", ",", "ihm", "lag", "die", "Er\u00b7war\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.16": {"text": "Ueber seine Gef\u00e4hrten, und sich, auf \u00e4ngstlichem Herzen,", "tokens": ["Ue\u00b7ber", "sei\u00b7ne", "Ge\u00b7f\u00e4hr\u00b7ten", ",", "und", "sich", ",", "auf", "\u00e4ngst\u00b7li\u00b7chem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PRF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Wie ein Geb\u00fcrge. Er hatte voll Schmerz die himm-", "tokens": ["Wie", "ein", "Ge\u00b7b\u00fcr\u00b7ge", ".", "Er", "hat\u00b7te", "voll", "Schmerz", "die", "him\u00b7m"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$.", "PPER", "VAFIN", "ADJD", "NN", "ART", "TRUNC"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Sich zu bet\u00e4uben, genommen. Die sanften g\u00fcldenen", "tokens": ["Sich", "zu", "be\u00b7t\u00e4u\u00b7ben", ",", "ge\u00b7nom\u00b7men", ".", "Die", "sanf\u00b7ten", "g\u00fcl\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+--", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Schallten in melancholische Klagen, und fl\u00f6\u00dften der Seele", "tokens": ["Schall\u00b7ten", "in", "me\u00b7lan\u00b7cho\u00b7li\u00b7sche", "Kla\u00b7gen", ",", "und", "fl\u00f6\u00df\u00b7ten", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+---+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Himmlische Linderung ein; denn welches Gem\u00fcth wird", "tokens": ["Himm\u00b7li\u00b7sche", "Lin\u00b7de\u00b7rung", "ein", ";", "denn", "wel\u00b7ches", "Ge\u00b7m\u00fcth", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "$.", "KON", "PWAT", "NN", "VAFIN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.21": {"text": "Wenn es sich in Ges\u00e4ngen ergie\u00dft? Und welche Betr\u00fcbni\u00df", "tokens": ["Wenn", "es", "sich", "in", "Ge\u00b7s\u00e4n\u00b7gen", "er\u00b7gie\u00dft", "?", "Und", "wel\u00b7che", "Be\u00b7tr\u00fcb\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$.", "KON", "PWAT", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Hat nicht die Tonkunst, die Tochter des Himmels, be-", "tokens": ["Hat", "nicht", "die", "Ton\u00b7kunst", ",", "die", "Toch\u00b7ter", "des", "Him\u00b7mels", ",", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "TRUNC"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.23": {"text": "Oder besiegt? Die g\u00f6ttlichen Lieder erklangen von fern", "tokens": ["O\u00b7der", "be\u00b7siegt", "?", "Die", "g\u00f6tt\u00b7li\u00b7chen", "Lie\u00b7der", "er\u00b7klan\u00b7gen", "von", "fern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "$.", "ART", "ADJA", "NN", "VVFIN", "APPR", "ADJD"], "meter": "+--+-+--+--+--+", "measure": "iambic.hexa.invert"}, "line.24": {"text": "In des entz\u00fcckten Gabriels Herz. Der stralende Teppich", "tokens": ["In", "des", "ent\u00b7z\u00fcck\u00b7ten", "Ga\u00b7bri\u00b7els", "Herz", ".", "Der", "stra\u00b7len\u00b7de", "Tep\u00b7pich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--++", "measure": "dactylic.di.plus"}, "line.25": {"text": "Rauscht vor dem Seraph ietzt auf. So bald ihn Orion", "tokens": ["Rauscht", "vor", "dem", "Se\u00b7raph", "ietzt", "auf", ".", "So", "bald", "ihn", "O\u00b7rion"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$.", "ADV", "ADV", "PPER", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.26": {"text": "Sank ihm die Leyer best\u00fcrzt aus der Hand, er erhub sich;", "tokens": ["Sank", "ihm", "die", "Le\u00b7yer", "be\u00b7st\u00fcrzt", "aus", "der", "Hand", ",", "er", "er\u00b7hub", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.27": {"text": "Sprach er: Erhabner Seraph, Gesandter des H\u00f6ch-", "tokens": ["Sprach", "er", ":", "Er\u00b7hab\u00b7ner", "Se\u00b7raph", ",", "Ge\u00b7sand\u00b7ter", "des", "H\u00f6ch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ADJA", "NN", "$,", "NN", "ART", "TRUNC"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.28": {"text": "Schickt der Allm\u00e4chtige dich zu seinen gefallenen Knechten.", "tokens": ["Schickt", "der", "All\u00b7m\u00e4ch\u00b7ti\u00b7ge", "dich", "zu", "sei\u00b7nen", "ge\u00b7fal\u00b7le\u00b7nen", "Knech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.29": {"text": "O da\u00df endlich die Bothschaft des Himmels uns Arme", "tokens": ["O", "da\u00df", "end\u00b7lich", "die", "Both\u00b7schaft", "des", "Him\u00b7mels", "uns", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "ART", "NN", "ART", "NN", "PPER", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Die wir in Thr\u00e4nen vergehn! Vielleicht da\u00df unsere", "tokens": ["Die", "wir", "in", "Thr\u00e4\u00b7nen", "ver\u00b7gehn", "!", "Viel\u00b7leicht", "da\u00df", "un\u00b7se\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF", "$.", "ADV", "KOUS", "PPOSAT"], "meter": "++-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Seinen verderbenden Zorn entwafnet! vielleicht! \u2014", "tokens": ["Sei\u00b7nen", "ver\u00b7der\u00b7ben\u00b7den", "Zorn", "ent\u00b7waf\u00b7net", "!", "viel\u00b7leicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$.", "ADV", "$.", "$("], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.32": {"text": "La\u00df uns nicht l\u00e4nger in schwerer Erwartung, und la\u00df", "tokens": ["La\u00df", "uns", "nicht", "l\u00e4n\u00b7ger", "in", "schwe\u00b7rer", "Er\u00b7war\u00b7tung", ",", "und", "la\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJD", "APPR", "ADJA", "NN", "$,", "KON", "XY"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.33": {"text": "Unser Urtheil vernehmen! \u2014 So sprach er. Der", "tokens": ["Un\u00b7ser", "Ur\u00b7theil", "ver\u00b7neh\u00b7men", "!", "So", "sprach", "er", ".", "Der"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVINF", "$.", "$(", "ADV", "VVFIN", "PPER", "$.", "ART"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "La\u00df die Posaunen ert\u00f6nen, damit sich alle versammeln,", "tokens": ["La\u00df", "die", "Po\u00b7sau\u00b7nen", "er\u00b7t\u00f6\u00b7nen", ",", "da\u00b7mit", "sich", "al\u00b7le", "ver\u00b7sam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,", "KOUS", "PRF", "PIS", "VVINF", "$,"], "meter": "---+--+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Welche zu deinem Panier geh\u00f6ren. Des H\u00f6chsten Befehle", "tokens": ["Wel\u00b7che", "zu", "dei\u00b7nem", "Pa\u00b7nier", "ge\u00b7h\u00f6\u00b7ren", ".", "Des", "H\u00f6chs\u00b7ten", "Be\u00b7feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "APPR", "PPOSAT", "NN", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.36": {"text": "Warten auf euren Gehorsam; er gab sie mit tiefem", "tokens": ["War\u00b7ten", "auf", "eu\u00b7ren", "Ge\u00b7hor\u00b7sam", ";", "er", "gab", "sie", "mit", "tie\u00b7fem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.37": {"text": "Gl\u00fccklich bin ich, sie euch zu verk\u00fcndgen; \u2014 So sag-", "tokens": ["Gl\u00fcck\u00b7lich", "bin", "ich", ",", "sie", "euch", "zu", "ver\u00b7k\u00fcnd\u00b7gen", ";", "So", "sag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PPER", "PPER", "PTKZU", "VVINF", "$.", "$(", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}