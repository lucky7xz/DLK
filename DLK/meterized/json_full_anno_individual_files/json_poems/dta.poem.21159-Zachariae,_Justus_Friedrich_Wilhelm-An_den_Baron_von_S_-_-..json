{"dta.poem.21159": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Baron von S - -.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, setze dich ruhig zu mir im Schatten hoher\nOrangen,", "tokens": ["Freund", ",", "set\u00b7ze", "dich", "ru\u00b7hig", "zu", "mir", "im", "Schat\u00b7ten", "ho\u00b7her", "O\u00b7ran\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Umw\u00f6lket vom paradiesischen Duft!", "tokens": ["Um\u00b7w\u00f6l\u00b7ket", "vom", "pa\u00b7ra\u00b7die\u00b7si\u00b7schen", "Duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch sitzest du lieber vielleicht in jenem heiligen Dunkel", "tokens": ["Doch", "sit\u00b7zest", "du", "lie\u00b7ber", "viel\u00b7leicht", "in", "je\u00b7nem", "hei\u00b7li\u00b7gen", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Des schattenreichen Castanienwalds?", "tokens": ["Des", "schat\u00b7ten\u00b7rei\u00b7chen", "Cas\u00b7ta\u00b7ni\u00b7en\u00b7walds", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du wirst mich bald nicht mehr sehn! Viel Meilen voll", "tokens": ["Du", "wirst", "mich", "bald", "nicht", "mehr", "sehn", "!", "Viel", "Mei\u00b7len", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$.", "PIAT", "NN", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sind zwischen uns, eh noch die Thr\u00e4ne versiegt.", "tokens": ["Sind", "zwi\u00b7schen", "uns", ",", "eh", "noch", "die", "Thr\u00e4\u00b7ne", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dann wirst du nicht mehr mich sehn; nicht unter den", "tokens": ["Dann", "wirst", "du", "nicht", "mehr", "mich", "sehn", ";", "nicht", "un\u00b7ter", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "PPER", "VVINF", "$.", "PTKNEG", "APPR", "ART"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht mehr am Springbrunn der gro\u00dfen Allee.", "tokens": ["Nicht", "mehr", "am", "Spring\u00b7brunn", "der", "gro\u00b7\u00dfen", "Al\u00b7lee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wenn ich nun weg bin, o Freund, wenn du die z\u00e4rt-", "tokens": ["Wenn", "ich", "nun", "weg", "bin", ",", "o", "Freund", ",", "wenn", "du", "die", "z\u00e4r\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,", "FM", "NN", "$,", "KOUS", "PPER", "ART", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der holden Freundschaft durch mich nicht mehr", "tokens": ["Der", "hol\u00b7den", "Freund\u00b7schaft", "durch", "mich", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKNEG", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn meine Pflicht dich nicht mehr zu edlen Thaten er-", "tokens": ["Wenn", "mei\u00b7ne", "Pflicht", "dich", "nicht", "mehr", "zu", "ed\u00b7len", "Tha\u00b7ten", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und zur Umarmung der Musen dich lockt;", "tokens": ["Und", "zur", "Um\u00b7ar\u00b7mung", "der", "Mu\u00b7sen", "dich", "lockt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn ich nun weg bin, und fern von mir, und fern", "tokens": ["Wenn", "ich", "nun", "weg", "bin", ",", "und", "fern", "von", "mir", ",", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,", "KON", "ADJD", "APPR", "PPER", "$,", "KON", "ADJD"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Den dir der Himmel zur Nachfolge setzt,", "tokens": ["Den", "dir", "der", "Him\u00b7mel", "zur", "Nach\u00b7fol\u00b7ge", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du selbst Gesetze dir giebst: so folge doch immer dem", "tokens": ["Du", "selbst", "Ge\u00b7set\u00b7ze", "dir", "giebst", ":", "so", "fol\u00b7ge", "doch", "im\u00b7mer", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "ADV", "ART"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In dem die himmlische Tugend erscheint!", "tokens": ["In", "dem", "die", "himm\u00b7li\u00b7sche", "Tu\u00b7gend", "er\u00b7scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Sey gro\u00df, nicht durch die Geburt, die oft auch Tho-", "tokens": ["Sey", "gro\u00df", ",", "nicht", "durch", "die", "Ge\u00b7burt", ",", "die", "oft", "auch", "Tho"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "PTKNEG", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Gro\u00df durch ein edles gef\u00e4lliges Herz.", "tokens": ["Gro\u00df", "durch", "ein", "ed\u00b7les", "ge\u00b7f\u00e4l\u00b7li\u00b7ges", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "H\u00f6r nicht den schmeichelnden Ruf der Wollust, welche", "tokens": ["H\u00f6r", "nicht", "den", "schmei\u00b7cheln\u00b7den", "Ruf", "der", "Wol\u00b7lust", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Zum ewgen Tempel der Ehre zu gehn.", "tokens": ["Zum", "ew\u00b7gen", "Tem\u00b7pel", "der", "Eh\u00b7re", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "So werd ich mit fr\u00f6hlichem Blick in aller Entfernung", "tokens": ["So", "werd", "ich", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Blick", "in", "al\u00b7ler", "Ent\u00b7fer\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wenn du die gegebne Hofnung erf\u00fcllst.", "tokens": ["Wenn", "du", "die", "ge\u00b7geb\u00b7ne", "Hof\u00b7nung", "er\u00b7f\u00fcllst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So wird, zufrieden mein Herz, in s\u00fc\u00dfen Freuden er-", "tokens": ["So", "wird", ",", "zu\u00b7frie\u00b7den", "mein", "Herz", ",", "in", "s\u00fc\u00b7\u00dfen", "Freu\u00b7den", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "ADJD", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn du mit reinem Leben mich lohnst.", "tokens": ["Wenn", "du", "mit", "rei\u00b7nem", "Le\u00b7ben", "mich", "lohnst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}