{"textgrid.poem.42334": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "[johann Heinrich Vo\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leckt Votzen, Ihr neun Pindars-Luder,", "tokens": ["Leckt", "Vot\u00b7zen", ",", "Ihr", "neun", "Pin\u00b7dar\u00b7sLu\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Leckt mit Apoll, der schl\u00e4frig geigt;", "tokens": ["Leckt", "mit", "A\u00b7poll", ",", "der", "schl\u00e4f\u00b7rig", "geigt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und dessen kleiner matter Bruder,", "tokens": ["Und", "des\u00b7sen", "klei\u00b7ner", "mat\u00b7ter", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur durch das Fingern aufw\u00e4rts steigt:", "tokens": ["Nur", "durch", "das", "Fin\u00b7gern", "auf\u00b7w\u00e4rts", "steigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Priap! beseele meine Leier,", "tokens": ["Priap", "!", "be\u00b7see\u00b7le", "mei\u00b7ne", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und g\u00f6nne ihr das rege Feuer,", "tokens": ["Und", "g\u00f6n\u00b7ne", "ihr", "das", "re\u00b7ge", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Das sich durch deine Kl\u00f6t ergeu\u00dft;", "tokens": ["Das", "sich", "durch", "dei\u00b7ne", "Kl\u00f6t", "er\u00b7geu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und durch die aufgeschwollenen R\u00f6hren,", "tokens": ["Und", "durch", "die", "auf\u00b7ge\u00b7schwol\u00b7le\u00b7nen", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Um deine Wollust zu vermehren,", "tokens": ["Um", "dei\u00b7ne", "Wol\u00b7lust", "zu", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dicksch\u00e4umend in die Votze fleu\u00dft.", "tokens": ["Dick\u00b7sch\u00e4u\u00b7mend", "in", "die", "Vot\u00b7ze", "fleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kommt Hurenbuben, kommt zusammen,", "tokens": ["Kommt", "Hu\u00b7ren\u00b7bu\u00b7ben", ",", "kommt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt euren Mut und fuchst euch satt,", "tokens": ["Zeigt", "eu\u00b7ren", "Mut", "und", "fuchst", "euch", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schauspiel setzt mich jetzt in Flammen,", "tokens": ["Ein", "Schau\u00b7spiel", "setzt", "mich", "jetzt", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das nie der Himmel sch\u00f6ner hat:", "tokens": ["Das", "nie", "der", "Him\u00b7mel", "sch\u00f6\u00b7ner", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sehe Br\u00fcste, Zitzen strotzen,", "tokens": ["Ich", "se\u00b7he", "Br\u00fcs\u00b7te", ",", "Zit\u00b7zen", "strot\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nebst tausend auserlesenen Votzen,", "tokens": ["Nebst", "tau\u00b7send", "au\u00b7ser\u00b7le\u00b7se\u00b7nen", "Vot\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Von kaltem Bauer \u00fcberschwemmt;", "tokens": ["Von", "kal\u00b7tem", "Bau\u00b7er", "\u00fc\u00b7bersc\u00b7hwemmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich sehe tausend Kl\u00f6te gl\u00e4nzen,", "tokens": ["Ich", "se\u00b7he", "tau\u00b7send", "Kl\u00f6\u00b7te", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bei tausend auserlesenen Schw\u00e4nzen,", "tokens": ["Bei", "tau\u00b7send", "au\u00b7ser\u00b7le\u00b7se\u00b7nen", "Schw\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "In feiste Lenden eingestemmt.", "tokens": ["In", "feis\u00b7te", "Len\u00b7den", "ein\u00b7ge\u00b7stemmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O, reiz mich oft mit solchen Bildern,", "tokens": ["O", ",", "reiz", "mich", "oft", "mit", "sol\u00b7chen", "Bil\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du meiner Sehnsucht Gegenstand;", "tokens": ["Du", "mei\u00b7ner", "Sehn\u00b7sucht", "Ge\u00b7gen\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wollust ist nie genug zu schildern,", "tokens": ["Die", "Wol\u00b7lust", "ist", "nie", "ge\u00b7nug", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die nur zu sehn mein Herz empfand.", "tokens": ["Die", "nur", "zu", "sehn", "mein", "Herz", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Priap! Dir bau ich einen Tempel,", "tokens": ["Priap", "!", "Dir", "bau", "ich", "ei\u00b7nen", "Tem\u00b7pel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und v\u00f6gle andern zum Exempel", "tokens": ["Und", "v\u00f6g\u00b7le", "an\u00b7dern", "zum", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zw\u00f6lfmal, den Altar einzuweihn;", "tokens": ["Zw\u00f6lf\u00b7mal", ",", "den", "Al\u00b7tar", "ein\u00b7zu\u00b7weihn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Statt Gold soll kalter Bauer gl\u00e4nzen,", "tokens": ["Statt", "Gold", "soll", "kal\u00b7ter", "Bau\u00b7er", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und Votzenhaar die T\u00fcr umkr\u00e4nzen,", "tokens": ["Und", "Vot\u00b7zen\u00b7haar", "die", "T\u00fcr", "um\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Schwanz soll Hoherpriester sein.", "tokens": ["Mein", "Schwanz", "soll", "Ho\u00b7her\u00b7pries\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hirsch, Adler, Wolf und Walfisch lehren,", "tokens": ["Hirsch", ",", "Ad\u00b7ler", ",", "Wolf", "und", "Wal\u00b7fisch", "leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NN", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man best\u00e4ndig v\u00f6geln soll;", "tokens": ["Wie", "man", "be\u00b7st\u00e4n\u00b7dig", "v\u00f6\u00b7geln", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sperling ist nie genug zu ehren,", "tokens": ["Der", "Sper\u00b7ling", "ist", "nie", "ge\u00b7nug", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn der ist immer samenvoll.", "tokens": ["Denn", "der", "ist", "im\u00b7mer", "sa\u00b7men\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kurz, alles mu\u00df gev\u00f6gelt werden,", "tokens": ["Kurz", ",", "al\u00b7les", "mu\u00df", "ge\u00b7v\u00f6\u00b7gelt", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PIS", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Votz enth\u00e4lt, was man auf Erden", "tokens": ["Die", "Votz", "ent\u00b7h\u00e4lt", ",", "was", "man", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erhabenes nur denken kann;", "tokens": ["Er\u00b7ha\u00b7be\u00b7nes", "nur", "den\u00b7ken", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie zeigt sich, \u2013 tausend Schw\u00e4nze starren,", "tokens": ["Sie", "zeigt", "sich", ",", "\u2013", "tau\u00b7send", "Schw\u00e4n\u00b7ze", "star\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "$(", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Weise v\u00f6gelt mit dem Narren,", "tokens": ["Der", "Wei\u00b7se", "v\u00f6\u00b7gelt", "mit", "dem", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der B\u00fcrger mit dem Edelmann.", "tokens": ["Der", "B\u00fcr\u00b7ger", "mit", "dem", "E\u00b7del\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sind meine Kl\u00f6t nur voll von Feuer,", "tokens": ["Sind", "mei\u00b7ne", "Kl\u00f6t", "nur", "voll", "von", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht mein Schwanz sein Meisterst\u00fcck,", "tokens": ["Und", "macht", "mein", "Schwanz", "sein", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bin ich reich bei einem Dreier,", "tokens": ["Dann", "bin", "ich", "reich", "bei", "ei\u00b7nem", "Drei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schei\u00dfe fast auf alles Gl\u00fcck.", "tokens": ["Und", "schei\u00b7\u00dfe", "fast", "auf", "al\u00b7les", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zufrieden und entfernt vom Neide,", "tokens": ["Zu\u00b7frie\u00b7den", "und", "ent\u00b7fernt", "vom", "Nei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seh ich in meinem schlechten Kleide", "tokens": ["Seh", "ich", "in", "mei\u00b7nem", "schlech\u00b7ten", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Pracht der gro\u00dfen Herren an,", "tokens": ["Die", "Pracht", "der", "gro\u00b7\u00dfen", "Her\u00b7ren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil der, der auf dem Throne sitzet,", "tokens": ["Weil", "der", ",", "der", "auf", "dem", "Thro\u00b7ne", "sit\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn er den Samen von sich spr\u00fctzet,", "tokens": ["Wenn", "er", "den", "Sa\u00b7men", "von", "sich", "spr\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht mehr als ich, empfinden kann.", "tokens": ["Nicht", "mehr", "als", "ich", ",", "emp\u00b7fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOUS", "PPER", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Seht auf Athens erhab'nen Pl\u00e4tzen,", "tokens": ["Seht", "auf", "A\u00b7thens", "er\u00b7hab'\u00b7nen", "Pl\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Melkt sich ein Schwanz der Zyniker;", "tokens": ["Melkt", "sich", "ein", "Schwanz", "der", "Zy\u00b7ni\u00b7ker", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Die Menge sieht ihn mit Erg\u00f6tzen", "tokens": ["Die", "Men\u00b7ge", "sieht", "ihn", "mit", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und steht m