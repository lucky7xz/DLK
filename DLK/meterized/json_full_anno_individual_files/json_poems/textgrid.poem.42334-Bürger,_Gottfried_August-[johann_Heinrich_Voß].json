a"}, "line.6": {"text": "Die Votz enth\u00e4lt, was man auf Erden", "tokens": ["Die", "Votz", "ent\u00b7h\u00e4lt", ",", "was", "man", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erhabenes nur denken kann;", "tokens": ["Er\u00b7ha\u00b7be\u00b7nes", "nur", "den\u00b7ken", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie zeigt sich, \u2013 tausend Schw\u00e4nze starren,", "tokens": ["Sie", "zeigt", "sich", ",", "\u2013", "tau\u00b7send", "Schw\u00e4n\u00b7ze", "star\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "$(", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Weise v\u00f6gelt mit dem Narren,", "tokens": ["Der", "Wei\u00b7se", "v\u00f6\u00b7gelt", "mit", "dem", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der B\u00fcrger mit dem Edelmann.", "tokens": ["Der", "B\u00fcr\u00b7ger", "mit", "dem", "E\u00b7del\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sind meine Kl\u00f6t nur voll von Feuer,", "tokens": ["Sind", "mei\u00b7ne", "Kl\u00f6t", "nur", "voll", "von", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht mein Schwanz sein Meisterst\u00fcck,", "tokens": ["Und", "macht", "mein", "Schwanz", "sein", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bin ich reich bei einem Dreier,", "tokens": ["Dann", "bin", "ich", "reich", "bei", "ei\u00b7nem", "Drei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schei\u00dfe fast auf alles Gl\u00fcck.", "tokens": ["Und", "schei\u00b7\u00dfe", "fast", "auf", "al\u00b7les", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zufrieden und entfernt vom Neide,", "tokens": ["Zu\u00b7frie\u00b7den", "und", "ent\u00b7fernt", "vom", "Nei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seh ich in meinem schlechten Kleide", "tokens": ["Seh", "ich", "in", "mei\u00b7nem", "schlech\u00b7ten", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Pracht der gro\u00dfen Herren an,", "tokens": ["Die", "Pracht", "der", "gro\u00b7\u00dfen", "Her\u00b7ren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measu