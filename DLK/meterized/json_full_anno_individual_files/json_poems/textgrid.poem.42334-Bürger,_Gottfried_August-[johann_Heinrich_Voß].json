 "line.4": {"text": "Die nur zu sehn mein Herz empfand.", "tokens": ["Die", "nur", "zu", "sehn", "mein", "Herz", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Priap! Dir bau ich einen Tempel,", "tokens": ["Priap", "!", "Dir", "bau", "ich", "ei\u00b7nen", "Tem\u00b7pel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und v\u00f6gle andern zum Exempel", "tokens": ["Und", "v\u00f6g\u00b7le", "an\u00b7dern", "zum", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zw\u00f6lfmal, den Altar einzuweihn;", "tokens": ["Zw\u00f6lf\u00b7mal", ",", "den", "Al\u00b7tar", "ein\u00b7zu\u00b7weihn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Statt Gold soll kalter Bauer gl\u00e4nzen,", "tokens": ["Statt", "Gold", "soll", "kal\u00b7ter", "Bau\u00b7er", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und Votzenhaar die T\u00fcr umkr\u00e4nzen,", "tokens": ["Und", "Vot\u00b7zen\u00b7haar", "die", "T\u00fcr", "um\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Schwanz