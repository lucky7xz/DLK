{"dta.poem.9635": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "4.  \n  Cupido/ der kleine Marggraf von Anspach/  \n von seiner mutter der Venus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der kleine wunder-knabe/", "tokens": ["Ich", "bin", "der", "klei\u00b7ne", "wun\u00b7der\u00b7kna\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem ost und west zu f\u00fcssen liegt/", "tokens": ["Dem", "ost", "und", "west", "zu", "f\u00fcs\u00b7sen", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich die sch\u00f6ne mutter habe/", "tokens": ["Weil", "ich", "die", "sch\u00f6\u00b7ne", "mut\u00b7ter", "ha\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die \u00fcber alle g\u00f6tter siegt.", "tokens": ["Die", "\u00fc\u00b7ber", "al\u00b7le", "g\u00f6t\u00b7ter", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer kennt nicht meines bogens t\u00fccke?", "tokens": ["Wer", "kennt", "nicht", "mei\u00b7nes", "bo\u00b7gens", "t\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch siegt die mutter mehr als er;", "tokens": ["Doch", "siegt", "die", "mut\u00b7ter", "mehr", "als", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie zwingt mit einem blossen blicke", "tokens": ["Sie", "zwingt", "mit", "ei\u00b7nem", "blos\u00b7sen", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mehr als mein gantzes k\u00f6cher-heer.", "tokens": ["Mehr", "als", "mein", "gant\u00b7zes", "k\u00f6\u00b7cher\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O m\u00f6chte sie bey diesem reihen/", "tokens": ["O", "m\u00f6ch\u00b7te", "sie", "bey", "die\u00b7sem", "rei\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "PDAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da alles schertzt bey tantz und wein/", "tokens": ["Da", "al\u00b7les", "schertzt", "bey", "tantz", "und", "wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mir einmahl ihre augen leihen/", "tokens": ["Mir", "ein\u00b7mahl", "ih\u00b7re", "au\u00b7gen", "lei\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Was hertzen solten dienstbar seyn!", "tokens": ["Was", "hert\u00b7zen", "sol\u00b7ten", "dienst\u00b7bar", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}