{"dta.poem.21117": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An seinen Schutzgeist .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der du vom stralenden Thron des Unerschafnen dich\nschwungest,", "tokens": ["Der", "du", "vom", "stra\u00b7len\u00b7den", "Thron", "des", "Un\u00b7er\u00b7schaf\u00b7nen", "dich", "schwun\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Um der Besch\u00fctzer zu seyn von meiner unsterblichen", "tokens": ["Um", "der", "Be\u00b7sch\u00fct\u00b7zer", "zu", "seyn", "von", "mei\u00b7ner", "uns\u00b7terb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VAINF", "APPR", "PPOSAT", "NN"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Himmlischer! sing in mein Lied mit T\u00f6nen der g\u00f6ttli-", "tokens": ["Himm\u00b7li\u00b7scher", "!", "sing", "in", "mein", "Lied", "mit", "T\u00f6\u00b7nen", "der", "g\u00f6tt\u00b7li"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "ART", "TRUNC"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vom Halleluja der Himmel beseelt.", "tokens": ["Vom", "Hal\u00b7le\u00b7lu\u00b7ja", "der", "Him\u00b7mel", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "L\u00e4chle gef\u00e4llig herab auf eine sterbliche Leyer,", "tokens": ["L\u00e4ch\u00b7le", "ge\u00b7f\u00e4l\u00b7lig", "her\u00b7ab", "auf", "ei\u00b7ne", "sterb\u00b7li\u00b7che", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Welche f\u00fcr dich nur ert\u00f6nt in mittern\u00e4chtlichen Stun-", "tokens": ["Wel\u00b7che", "f\u00fcr", "dich", "nur", "er\u00b7t\u00f6nt", "in", "mit\u00b7ter\u00b7n\u00e4cht\u00b7li\u00b7chen", "Stun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Sage, wie dank ich dir doch die Sorgen, \u00e4therischer", "tokens": ["Sa\u00b7ge", ",", "wie", "dank", "ich", "dir", "doch", "die", "Sor\u00b7gen", ",", "\u00e4t\u00b7he\u00b7ri\u00b7scher"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PPER", "PPER", "ADV", "ART", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Die mich schon in der Kindheit besch\u00fctzt;", "tokens": ["Die", "mich", "schon", "in", "der", "Kind\u00b7heit", "be\u00b7sch\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Aber die ietzo noch mehr in einem reifenden Alter", "tokens": ["A\u00b7ber", "die", "iet\u00b7zo", "noch", "mehr", "in", "ei\u00b7nem", "rei\u00b7fen\u00b7den", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wider den m\u00e4chtigen Reiz der lockenden Wollust mich", "tokens": ["Wi\u00b7der", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Reiz", "der", "lo\u00b7cken\u00b7den", "Wol\u00b7lust", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PPER"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Tief in der Seele h\u00f6r ich die Stimme von meinem", "tokens": ["Tief", "in", "der", "See\u00b7le", "h\u00f6r", "ich", "die", "Stim\u00b7me", "von", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die mir erhabne Gedanken zuruft.", "tokens": ["Die", "mir", "er\u00b7hab\u00b7ne", "Ge\u00b7dan\u00b7ken", "zu\u00b7ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "O! warum kanst du mir nicht, o mein Beschir-", "tokens": ["O", "!", "wa\u00b7rum", "kanst", "du", "mir", "nicht", ",", "o", "mein", "Be\u00b7schir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VMFIN", "PPER", "PPER", "PTKNEG", "$,", "FM", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn mein erzitterndes Herz des Ewigen Throne sich", "tokens": ["Wenn", "mein", "er\u00b7zit\u00b7tern\u00b7des", "Herz", "des", "E\u00b7wi\u00b7gen", "Thro\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und hingeneigt in den Staub, in Thr\u00e4nen der Reu", "tokens": ["Und", "hin\u00b7ge\u00b7neigt", "in", "den", "Staub", ",", "in", "Thr\u00e4\u00b7nen", "der", "Reu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "NN", "ART", "NE"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sich seiner Erbarmung unwerth erkennt.", "tokens": ["Sich", "sei\u00b7ner", "Er\u00b7bar\u00b7mung", "un\u00b7werth", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Oder erschienest du doch in meiner erkenntlichen", "tokens": ["O\u00b7der", "er\u00b7schie\u00b7nest", "du", "doch", "in", "mei\u00b7ner", "er\u00b7kennt\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "VVINF"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wenn sie die Sorgfalt erw\u00e4gt, mit der ihr Engel sie", "tokens": ["Wenn", "sie", "die", "Sorg\u00b7falt", "er\u00b7w\u00e4gt", ",", "mit", "der", "ihr", "En\u00b7gel", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "PPOSAT", "NN", "PPER"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wenn sie in einsamer Nacht, in einem heiligen Tief-", "tokens": ["Wenn", "sie", "in", "ein\u00b7sa\u00b7mer", "Nacht", ",", "in", "ei\u00b7nem", "hei\u00b7li\u00b7gen", "Tie\u00b7f"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zum stralendem Kraise der Seligen k\u00f6mmt.", "tokens": ["Zum", "stra\u00b7len\u00b7dem", "Krai\u00b7se", "der", "Se\u00b7li\u00b7gen", "k\u00f6mmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "O! mein unsterblicher Freund, besch\u00fctze noch fer-", "tokens": ["O", "!", "mein", "uns\u00b7terb\u00b7li\u00b7cher", "Freund", ",", "be\u00b7sch\u00fct\u00b7ze", "noch", "fer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ADV", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die dir der Sch\u00f6pfer vertraut; da\u00df ich einst froh dich", "tokens": ["Die", "dir", "der", "Sch\u00f6p\u00b7fer", "ver\u00b7traut", ";", "da\u00df", "ich", "einst", "froh", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "$.", "KOUS", "PPER", "ADV", "ADJD", "PPER"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn du mit m\u00e4chtiger Hand mich \u00fcber die Felder des", "tokens": ["Wenn", "du", "mit", "m\u00e4ch\u00b7ti\u00b7ger", "Hand", "mich", "\u00fc\u00b7ber", "die", "Fel\u00b7der", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "ART"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zu jenem Triumphe der Ewigkeit bringst.", "tokens": ["Zu", "je\u00b7nem", "Tri\u00b7um\u00b7phe", "der", "E\u00b7wig\u00b7keit", "bringst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}}, "stanza.7": {"line.1": {"text": "Wann du nun da stehst vor mir in feyerlichem", "tokens": ["Wann", "du", "nun", "da", "stehst", "vor", "mir", "in", "fey\u00b7er\u00b7li\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "APPR", "PPER", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und voll Vertraulichkeit mich und ewiger Freundschaft", "tokens": ["Und", "voll", "Ver\u00b7trau\u00b7lich\u00b7keit", "mich", "und", "e\u00b7wi\u00b7ger", "Freund\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "PPER", "KON", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "G\u00f6ttlicher, werd ich alsdann nicht deiner Umarmung", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", ",", "werd", "ich", "als\u00b7dann", "nicht", "dei\u00b7ner", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Schnell als ein Stral aus dem Meere des Lichts?", "tokens": ["Schnell", "als", "ein", "Stral", "aus", "dem", "Mee\u00b7re", "des", "Lichts", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Lehre die Seel\u2019 alsdann, mit deinem Feuer zu", "tokens": ["Leh\u00b7re", "die", "Seel'", "als\u00b7dann", ",", "mit", "dei\u00b7nem", "Feu\u00b7er", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "APPR"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Lehre mich, g\u00f6ttlicher Freund, die Lieder der heiligen", "tokens": ["Leh\u00b7re", "mich", ",", "g\u00f6tt\u00b7li\u00b7cher", "Freund", ",", "die", "Lie\u00b7der", "der", "hei\u00b7li\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Bis die Seele mit dir am Throne meines Erretters", "tokens": ["Bis", "die", "See\u00b7le", "mit", "dir", "am", "Thro\u00b7ne", "mei\u00b7nes", "Er\u00b7ret\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich in unendliche Jubel verliert.", "tokens": ["Sich", "in", "un\u00b7end\u00b7li\u00b7che", "Ju\u00b7bel", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}