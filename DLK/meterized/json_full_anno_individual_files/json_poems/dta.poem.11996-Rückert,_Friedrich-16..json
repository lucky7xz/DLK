{"dta.poem.11996": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne selber siehst du nur durch Sonnenlicht,", "tokens": ["Die", "Son\u00b7ne", "sel\u00b7ber", "siehst", "du", "nur", "durch", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schaust du Gott durch Gott, durch andres Mittel nicht.", "tokens": ["So", "schaust", "du", "Gott", "durch", "Gott", ",", "durch", "and\u00b7res", "Mit\u00b7tel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sonne, die das Licht die Welt zu sehn, dir spendet,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "das", "Licht", "die", "Welt", "zu", "sehn", ",", "dir", "spen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siehst du ihr Angesicht, bist du davon geblendet.", "tokens": ["Siehst", "du", "ihr", "An\u00b7ge\u00b7sicht", ",", "bist", "du", "da\u00b7von", "ge\u00b7blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und so im Menschengeist erlischt was in ihm denkt,", "tokens": ["Und", "so", "im", "Men\u00b7schen\u00b7geist", "er\u00b7lischt", "was", "in", "ihm", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er sein Denken dreist im h\u00f6chsten Geist versenkt.", "tokens": ["Wenn", "er", "sein", "Den\u00b7ken", "dreist", "im", "h\u00f6chs\u00b7ten", "Geist", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mu\u00dft du die Sonne sehn? sieh Fluren sonnerhellt;", "tokens": ["Mu\u00dft", "du", "die", "Son\u00b7ne", "sehn", "?", "sieh", "Flu\u00b7ren", "son\u00b7ner\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "VVIMP", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und willst du Gott sehn, sieh die gotterf\u00fcllte Welt.", "tokens": ["Und", "willst", "du", "Gott", "sehn", ",", "sieh", "die", "got\u00b7ter\u00b7f\u00fcll\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Sonne echte Kraft siehst du im Schmelz der Flur,", "tokens": ["Der", "Son\u00b7ne", "ech\u00b7te", "Kraft", "siehst", "du", "im", "Schmelz", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Gott, den du nicht siehst, in seinen Werken nur.", "tokens": ["Und", "Gott", ",", "den", "du", "nicht", "siehst", ",", "in", "sei\u00b7nen", "Wer\u00b7ken", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}