{"textgrid.poem.23977": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lachender Himmel. Es ziehen gem\u00e4chlich", "tokens": ["La\u00b7chen\u00b7der", "Him\u00b7mel", ".", "Es", "zie\u00b7hen", "ge\u00b7m\u00e4ch\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "ADJD"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schaumige Sch\u00e4fchenwolken dar\u00fcber,", "tokens": ["Schau\u00b7mi\u00b7ge", "Sch\u00e4f\u00b7chen\u00b7wol\u00b7ken", "da\u00b7r\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sonnenscheinschimmer durchflutet die Luft.", "tokens": ["Son\u00b7nen\u00b7schein\u00b7schim\u00b7mer", "durch\u00b7flu\u00b7tet", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Maiengr\u00fcn, die reine, feine,", "tokens": ["Mai\u00b7en\u00b7gr\u00fcn", ",", "die", "rei\u00b7ne", ",", "fei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jungfernfarbe der Natur,", "tokens": ["Jung\u00b7fern\u00b7far\u00b7be", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4chelt br\u00e4utlich hold und heiter", "tokens": ["L\u00e4\u00b7chelt", "br\u00e4ut\u00b7lich", "hold", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von Millionen leise schwankenden,", "tokens": ["Von", "Mil\u00b7lion\u00b7en", "lei\u00b7se", "schwan\u00b7ken\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Zierlich auf- und niederschwebenden", "tokens": ["Zier\u00b7lich", "auf", "und", "nie\u00b7der\u00b7schwe\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "TRUNC", "KON", "ADJA"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Zarten Bl\u00e4ttern.", "tokens": ["Zar\u00b7ten", "Bl\u00e4t\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Fr\u00fchling!", "tokens": ["Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Welch ein Glanz ruht auf der Wiese.", "tokens": ["Welch", "ein", "Glanz", "ruht", "auf", "der", "Wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oh, du lockendes, leises Klingen", "tokens": ["Oh", ",", "du", "lo\u00b7cken\u00b7des", ",", "lei\u00b7ses", "Klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPER", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ueber der ruhig bl\u00fchenden Sch\u00f6nheit!", "tokens": ["Ue\u00b7ber", "der", "ru\u00b7hig", "bl\u00fc\u00b7hen\u00b7den", "Sch\u00f6n\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hoffnung weht mir in die Seele", "tokens": ["Hoff\u00b7nung", "weht", "mir", "in", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Friedevoll bewegt.", "tokens": ["Frie\u00b7de\u00b7voll", "be\u00b7wegt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Weich umhaucht mich W\u00e4rme der Liebe,", "tokens": ["Weich", "um\u00b7haucht", "mich", "W\u00e4r\u00b7me", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie der Atem des bebenden M\u00e4dchens,", "tokens": ["Wie", "der", "A\u00b7tem", "des", "be\u00b7ben\u00b7den", "M\u00e4d\u00b7chens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das den schlummernden Freund an die wogende,", "tokens": ["Das", "den", "schlum\u00b7mern\u00b7den", "Freund", "an", "die", "wo\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Hei\u00dfe sehnsuchtsvolle Brust,", "tokens": ["Hei\u00b7\u00dfe", "sehn\u00b7suchts\u00b7vol\u00b7le", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leise sich \u00fcberbiegend, pre\u00dft.", "tokens": ["Lei\u00b7se", "sich", "\u00fc\u00b7berb\u00b7ie\u00b7gend", ",", "pre\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Oh, F\u00fclle! F\u00fclle! Dr\u00e4ngende, treibende", "tokens": ["Oh", ",", "F\u00fcl\u00b7le", "!", "F\u00fcl\u00b7le", "!", "Dr\u00e4n\u00b7gen\u00b7de", ",", "trei\u00b7ben\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ITJ", "$,", "NN", "$.", "NN", "$.", "NN", "$,", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fclle des Gl\u00fccks!", "tokens": ["F\u00fcl\u00b7le", "des", "Gl\u00fccks", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Eben, eben noch klang die Klage,", "tokens": ["E\u00b7ben", ",", "e\u00b7ben", "noch", "klang", "die", "Kla\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Klang die Klage um Hei\u00dfbegehrtes,", "tokens": ["Klang", "die", "Kla\u00b7ge", "um", "Hei\u00df\u00b7be\u00b7gehr\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sch\u00f6nheitsstrahlendes, Gro\u00dfes,", "tokens": ["Sch\u00f6n\u00b7heits\u00b7strah\u00b7len\u00b7des", ",", "Gro\u00b7\u00dfes", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Klang die Klage um das Geheimste,", "tokens": ["Klang", "die", "Kla\u00b7ge", "um", "das", "Ge\u00b7heims\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Herzerf\u00fcllende, Heiter-Heilige mir im Herzen.", "tokens": ["Her\u00b7zer\u00b7f\u00fcl\u00b7len\u00b7de", ",", "Hei\u00b7ter\u00b7Hei\u00b7li\u00b7ge", "mir", "im", "Her\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Nun, im gr\u00fcnen Bl\u00e4tterschwanken,", "tokens": ["Nun", ",", "im", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter\u00b7schwan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nun, im blauen Himmelsl\u00e4cheln,", "tokens": ["Nun", ",", "im", "blau\u00b7en", "Him\u00b7mels\u00b7l\u00e4\u00b7cheln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nun, im goldigen Sonnenstrahlen,", "tokens": ["Nun", ",", "im", "gol\u00b7di\u00b7gen", "Son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Ist mir schnell das Gl\u00fcck geworden,", "tokens": ["Ist", "mir", "schnell", "das", "Gl\u00fcck", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gl\u00fcck im Sch\u00f6nen und im Schauen", "tokens": ["Gl\u00fcck", "im", "Sch\u00f6\u00b7nen", "und", "im", "Schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Werdender Sch\u00f6nheit.", "tokens": ["Wer\u00b7den\u00b7der", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "In mein Auge strahlte das Gl\u00fcck,", "tokens": ["In", "mein", "Au\u00b7ge", "strahl\u00b7te", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mir im Herzen hebt es die Fl\u00fcgel:", "tokens": ["Mir", "im", "Her\u00b7zen", "hebt", "es", "die", "Fl\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ach, du lachendes, lustiges Ding,", "tokens": ["Ach", ",", "du", "la\u00b7chen\u00b7des", ",", "lus\u00b7ti\u00b7ges", "Ding", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Lustiges, lustiges Ding!", "tokens": ["Lus\u00b7ti\u00b7ges", ",", "lus\u00b7ti\u00b7ges", "Ding", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Meine Arme breite ich aus:", "tokens": ["Mei\u00b7ne", "Ar\u00b7me", "brei\u00b7te", "ich", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gl\u00fcck! Gl\u00fcck!", "tokens": ["Gl\u00fcck", "!", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "K\u00f6nnt ich es allen Menschen schenken,", "tokens": ["K\u00f6nnt", "ich", "es", "al\u00b7len", "Men\u00b7schen", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allen Menschen im dr\u00fcckenden Joch,", "tokens": ["Al\u00b7len", "Men\u00b7schen", "im", "dr\u00fc\u00b7cken\u00b7den", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Allen Menschen mit krampfendem Herzen,", "tokens": ["Al\u00b7len", "Men\u00b7schen", "mit", "kramp\u00b7fen\u00b7dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Allen denen, die im Hochflug", "tokens": ["Al\u00b7len", "de\u00b7nen", ",", "die", "im", "Hoch\u00b7flug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PDS", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihre Fl\u00fcgel zur goldenen Sonne", "tokens": ["Ih\u00b7re", "Fl\u00fc\u00b7gel", "zur", "gol\u00b7de\u00b7nen", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Breiten m\u00f6chten und im Schmutz", "tokens": ["Brei\u00b7ten", "m\u00f6ch\u00b7ten", "und", "im", "Schmutz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "KON", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Harter Not sich m\u00fchen m\u00fcssen;", "tokens": ["Har\u00b7ter", "Not", "sich", "m\u00fc\u00b7hen", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber denen, denen zuerst,", "tokens": ["A\u00b7ber", "de\u00b7nen", ",", "de\u00b7nen", "zu\u00b7erst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Deren Herzen liebem\u00e4chtig", "tokens": ["De\u00b7ren", "Her\u00b7zen", "lie\u00b7be\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["PDS", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Selbst in K\u00fcmmernis g\u00fctevoll, milde,", "tokens": ["Selbst", "in", "K\u00fcm\u00b7mer\u00b7nis", "g\u00fc\u00b7te\u00b7voll", ",", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Still in treuer Neigung schlagen:", "tokens": ["Still", "in", "treu\u00b7er", "Nei\u00b7gung", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dir zuerst drum, oh du mein braunes,", "tokens": ["Dir", "zu\u00b7erst", "drum", ",", "oh", "du", "mein", "brau\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Scheues Rehaug.", "tokens": ["Scheu\u00b7es", "Re\u00b7haug", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.16": {"text": "Oh du Gute, Liebe, Milde!", "tokens": ["Oh", "du", "Gu\u00b7te", ",", "Lie\u00b7be", ",", "Mil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "PPER", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ob auch im Herzen das Gl\u00fcck mir lacht,", "tokens": ["Ob", "auch", "im", "Her\u00b7zen", "das", "Gl\u00fcck", "mir", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Lacht und tanzt, das lustige Ding:", "tokens": ["Lacht", "und", "tanzt", ",", "das", "lus\u00b7ti\u00b7ge", "Ding", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Dein mu\u00df ich denken,", "tokens": ["Dein", "mu\u00df", "ich", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Traurig,", "tokens": ["Trau\u00b7rig", ","], "tok