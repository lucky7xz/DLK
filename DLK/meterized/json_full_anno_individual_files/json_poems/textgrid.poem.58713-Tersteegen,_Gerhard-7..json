{"textgrid.poem.58713": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid nicht tr\u00e4ge in dem, was ihr tun sollt! Seid br\u00fcnstig im Geiste! R\u00f6m. 12, 11", "tokens": ["Seid", "nicht", "tr\u00e4\u00b7ge", "in", "dem", ",", "was", "ihr", "tun", "sollt", "!", "Seid", "br\u00fcns\u00b7tig", "im", "Geis\u00b7te", "!", "R\u00f6m.", "12", ",", "11"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "abbreviation", "number", "punct", "number"], "pos": ["VAIMP", "PTKNEG", "VVFIN", "APPR", "ART", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$.", "VAIMP", "ADJD", "APPRART", "NN", "$.", "XY", "CARD", "$,", "CARD"], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "So lau im Christentum, so tr\u00e4g sein zum Gebet,", "tokens": ["So", "lau", "im", "Chris\u00b7ten\u00b7tum", ",", "so", "tr\u00e4g", "sein", "zum", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "ADV", "ADJD", "VAINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kann meinem Herren nicht behagen;", "tokens": ["Kann", "mei\u00b7nem", "Her\u00b7ren", "nicht", "be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lern dich doch weislicher betragen", "tokens": ["Lern", "dich", "doch", "weis\u00b7li\u00b7cher", "be\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und dien mit bess'rem Ernst der h\u00f6chsten Majest\u00e4t!", "tokens": ["Und", "di\u00b7en", "mit", "bess'\u00b7rem", "Ernst", "der", "h\u00f6chs\u00b7ten", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Dies ist die falsche Ruh, dies Stillesein betreugt,", "tokens": ["Dies", "ist", "die", "fal\u00b7sche", "Ruh", ",", "dies", "Stil\u00b7le\u00b7sein", "be\u00b7treugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weit anders gehen Gottes Wege;", "tokens": ["Weit", "an\u00b7ders", "ge\u00b7hen", "Got\u00b7tes", "We\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man l\u00e4uft dem Br\u00e4ut'gam nach, das Herz ist frisch und leicht,", "tokens": ["Man", "l\u00e4uft", "dem", "Br\u00e4ut'\u00b7gam", "nach", ",", "das", "Herz", "ist", "frisch", "und", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NE", "PTKVZ", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man folgt ihm \u00fcberall im dornenvollen Stege.", "tokens": ["Man", "folgt", "ihm", "\u00fc\u00b7be\u00b7rall", "im", "dor\u00b7nen\u00b7vol\u00b7len", "Ste\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Liebe und das Kreuz, drauf st\u00fctzen wir allein,", "tokens": ["Die", "Lie\u00b7be", "und", "das", "Kreuz", ",", "drauf", "st\u00fct\u00b7zen", "wir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn solche Wege schl\u00e4gt Gott ein.", "tokens": ["Denn", "sol\u00b7che", "We\u00b7ge", "schl\u00e4gt", "Gott", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das andre ist der Andacht Larve nur,", "tokens": ["Das", "and\u00b7re", "ist", "der", "An\u00b7dacht", "Lar\u00b7ve", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ART", "NN", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein totes Bild und Schein von dem Frommsein,", "tokens": ["Ein", "to\u00b7tes", "Bild", "und", "Schein", "von", "dem", "Fromm\u00b7sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In Wahrheit nur Betrug und ein Dummsein;", "tokens": ["In", "Wahr\u00b7heit", "nur", "Be\u00b7trug", "und", "ein", "Dumm\u00b7sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR"