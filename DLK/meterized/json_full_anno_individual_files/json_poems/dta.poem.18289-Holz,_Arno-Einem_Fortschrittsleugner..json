{"dta.poem.18289": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Einem Fortschrittsleugner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Hat mich noch niemals recht erbaut.", "tokens": ["Hat", "mich", "noch", "nie\u00b7mals", "recht", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Weltgeist ist ein Wiederk\u00e4uer,", "tokens": ["Der", "Welt\u00b7geist", "ist", "ein", "Wie\u00b7der\u00b7k\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der ewig fri\u00dft und nie verdaut?", "tokens": ["Der", "e\u00b7wig", "fri\u00dft", "und", "nie", "ver\u00b7daut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Still, still, mein Lieber; also spricht", "tokens": ["Still", ",", "still", ",", "mein", "Lie\u00b7ber", ";", "al\u00b7so", "spricht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "PPOSAT", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Einer, den der Haber sticht,", "tokens": ["Nur", "Ei\u00b7ner", ",", "den", "der", "Ha\u00b7ber", "sticht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn k\u00f6nnt' ich, hoch im Himmel hausend,", "tokens": ["Denn", "k\u00f6nnt'", "ich", ",", "hoch", "im", "Him\u00b7mel", "hau\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur um ein lumpiges Zehnjahrtausend", "tokens": ["Nur", "um", "ein", "lum\u00b7pi\u00b7ges", "Zehn\u00b7jahr\u00b7tau\u00b7send"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Dein Hirn nach r\u00fcckw\u00e4rtshin verrenken,", "tokens": ["Dein", "Hirn", "nach", "r\u00fcck\u00b7w\u00e4rts\u00b7hin", "ver\u00b7ren\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du w\u00fcrdest anders dr\u00fcber denken!", "tokens": ["Du", "w\u00fcr\u00b7dest", "an\u00b7ders", "dr\u00fc\u00b7ber", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}