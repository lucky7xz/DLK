{"dta.poem.20768": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An den Wein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wein! ich m\u00f6chte dich bald ha\u00dfen,               ", "tokens": ["Wein", "!", "ich", "m\u00f6ch\u00b7te", "dich", "bald", "ha\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich bin deiner Allmacht feind,", "tokens": ["Ich", "bin", "dei\u00b7ner", "All\u00b7macht", "feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn du willst mir meinen Freund", "tokens": ["Denn", "du", "willst", "mir", "mei\u00b7nen", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer nicht vom Becher lassen.", "tokens": ["Im\u00b7mer", "nicht", "vom", "Be\u00b7cher", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du bist meiner Freuden Dieb,", "tokens": ["Du", "bist", "mei\u00b7ner", "Freu\u00b7den", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnt ich dich doch ganz verachten.", "tokens": ["K\u00f6nnt", "ich", "dich", "doch", "ganz", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Milon hat dich gar zu lieb,", "tokens": ["Mi\u00b7lon", "hat", "dich", "gar", "zu", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mich l\u00e4\u00dft er schmachten. \u2014", "tokens": ["Und", "mich", "l\u00e4\u00dft", "er", "schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}