{"textgrid.poem.37846": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Unerh\u00f6rte Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist irgend zu erfragen", "tokens": ["Ist", "ir\u00b7gend", "zu", "er\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sch\u00e4fer um den Rhein,", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "um", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sehnlich sich beklagen", "tokens": ["Der", "sehn\u00b7lich", "sich", "be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df \u00fcber Liebespein,", "tokens": ["Mu\u00df", "\u00fc\u00b7ber", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wird mir m\u00fcssen weichen,", "tokens": ["Der", "wird", "mir", "m\u00fcs\u00b7sen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich wei\u00df sie plagt mich mehr,", "tokens": ["Ich", "wei\u00df", "sie", "plagt", "mich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Niemand ist mir zu gleichen,", "tokens": ["Nie\u00b7mand", "ist", "mir", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und liebt er noch so sehr.", "tokens": ["Und", "liebt", "er", "noch", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ist vorbey gegangen", "tokens": ["Es", "ist", "vor\u00b7bey", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fast jetzt ein ganzes Jahr,", "tokens": ["Fast", "jetzt", "ein", "gan\u00b7zes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Eine mich gefangen", "tokens": ["Da\u00df", "Ei\u00b7ne", "mich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Liebe ganz und gar,", "tokens": ["Mit", "Lie\u00b7be", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie mir hat genommen", "tokens": ["Da\u00df", "sie", "mir", "hat", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gedanken, Muth und Sinn,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "Muth", "und", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Jahr ist's, da\u00df ich kommen", "tokens": ["Ein", "Jahr", "ist's", ",", "da\u00df", "ich", "kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In ihre Liebe bin.", "tokens": ["In", "ih\u00b7re", "Lie\u00b7be", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seitdem bin ich verwirret", "tokens": ["Seit\u00b7dem", "bin", "ich", "ver\u00b7wir\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gewesen f\u00fcr und f\u00fcr,", "tokens": ["Ge\u00b7we\u00b7sen", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es haben auch geirret", "tokens": ["Es", "ha\u00b7ben", "auch", "ge\u00b7ir\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schaafe neben mir,", "tokens": ["Die", "Schaa\u00b7fe", "ne\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Feld hab ich verlassen,", "tokens": ["Das", "Feld", "hab", "ich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gelebt in Einsamkeit,", "tokens": ["Ge\u00b7lebt", "in", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hab alles m\u00fcssen hassen,", "tokens": ["Hab", "al\u00b7les", "m\u00fcs\u00b7sen", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Warum ein Mensch sich freut.", "tokens": ["Wa\u00b7rum", "ein", "Mensch", "sich", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nichts hab ich k\u00f6nnen singen,", "tokens": ["Nichts", "hab", "ich", "k\u00f6n\u00b7nen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als nur ihr klares Licht,", "tokens": ["Als", "nur", "ihr", "kla\u00b7res", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ihr hab ich zu klingen", "tokens": ["Von", "ihr", "hab", "ich", "zu", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die Lauten abgericht,", "tokens": ["Die", "Lau\u00b7ten", "ab\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie sehr ich sie mu\u00df lieben", "tokens": ["Wie", "sehr", "ich", "sie", "mu\u00df", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ihre gro\u00dfe Zier,", "tokens": ["Und", "ih\u00b7re", "gro\u00b7\u00dfe", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das hab ich fast geschrieben", "tokens": ["Das", "hab", "ich", "fast", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An alle B\u00e4ume hier.", "tokens": ["An", "al\u00b7le", "B\u00e4u\u00b7me", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kein Trinken und kein Essen,", "tokens": ["Kein", "Trin\u00b7ken", "und", "kein", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja nichts hat mir behagt,", "tokens": ["Ja", "nichts", "hat", "mir", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin nur stets gesessen,", "tokens": ["Ich", "bin", "nur", "stets", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und habe mich beklagt:", "tokens": ["Und", "ha\u00b7be", "mich", "be\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In diesem schweren Orden", "tokens": ["In", "die\u00b7sem", "schwe\u00b7ren", "Or\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ver\u00e4ndert alles sich,", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "al\u00b7les", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Heerd' ist mager worden,", "tokens": ["Die", "He\u00b7erd'", "ist", "ma\u00b7ger", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und ich bin nicht mehr ich.", "tokens": ["Und", "ich", "bin", "nicht", "mehr", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie aber hat die Sinnen", "tokens": ["Sie", "a\u00b7ber", "hat", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit von mir abgekehrt,", "tokens": ["Weit", "von", "mir", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ist gar nicht zu gewinnen,", "tokens": ["Ist", "gar", "nicht", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat mich noch nie erh\u00f6rt;", "tokens": ["Hat", "mich", "noch", "nie", "er\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da doch was ich gesungen", "tokens": ["Da", "doch", "was", "ich", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weit in das Land erschallt,", "tokens": ["Weit", "in", "das", "Land", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auch mein Ton gedrungen", "tokens": ["Und", "auch", "mein", "Ton", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis durch den B\u00f6hmer Wald.", "tokens": ["Bis", "durch", "den", "B\u00f6h\u00b7mer", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Schaafe, die am Flusse", "tokens": ["Die", "Schaa\u00b7fe", ",", "die", "am", "Flus\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im tiefsten Grase stehn,", "tokens": ["Im", "tiefs\u00b7ten", "Gra\u00b7se", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie horchten meinem Gru\u00dfe,", "tokens": ["Sie", "horch\u00b7ten", "mei\u00b7nem", "Gru\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie wollen zu mir gehn;", "tokens": ["Sie", "wol\u00b7len", "zu", "mir", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sammelt sich die Menge,", "tokens": ["Es", "sam\u00b7melt", "sich", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es winken mir die Fraun,", "tokens": ["Es", "win\u00b7ken", "mir", "die", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch selbst in dem Gedr\u00e4nge,", "tokens": ["Doch", "selbst", "in", "dem", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kann ich die Lieb nicht schaun.", "tokens": ["Kann", "ich", "die", "Lieb", "nicht", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was soll mein Lied erschallen?", "tokens": ["Was", "soll", "mein", "Lied", "er\u00b7schal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel lieber bin ich still,", "tokens": ["Viel", "lie\u00b7ber", "bin", "ich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebsten zu gefallen", "tokens": ["Der", "Liebs\u00b7ten", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich einig singen will:", "tokens": ["Ich", "ei\u00b7nig", "sin\u00b7gen", "will", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil alles sie auf Erden", "tokens": ["Weil", "al\u00b7les", "sie", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Allein zusammenh\u00e4lt,", "tokens": ["Al\u00b7lein", "zu\u00b7sam\u00b7men\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kann ihre Gunst mir werden,", "tokens": ["Kann", "ih\u00b7re", "Gunst", "mir", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So hab ich alle Welt.", "tokens": ["So", "hab", "ich", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ist irgend zu erfragen", "tokens": ["Ist", "ir\u00b7gend", "zu", "er\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sch\u00e4fer um den Rhein,", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "um", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sehnlich sich beklagen", "tokens": ["Der", "sehn\u00b7lich", "sich", "be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df \u00fcber Liebespein,", "tokens": ["Mu\u00df", "\u00fc\u00b7ber", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wird mir m\u00fcssen weichen,", "tokens": ["Der", "wird", "mir", "m\u00fcs\u00b7sen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich wei\u00df sie plagt mich mehr,", "tokens": ["Ich", "wei\u00df", "sie", "plagt", "mich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Niemand ist mir zu gleichen,", "tokens": ["Nie\u00b7mand", "ist", "mir", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und liebt er noch so sehr.", "tokens": ["Und", "liebt", "er", "noch", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es ist vorbey gegangen", "tokens": ["Es", "ist", "vor\u00b7bey", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fast jetzt ein ganzes Jahr,", "tokens": ["Fast", "jetzt", "ein", "gan\u00b7zes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Eine mich gefangen", "tokens": ["Da\u00df", "Ei\u00b7ne", "mich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Liebe ganz und gar,", "tokens": ["Mit", "Lie\u00b7be", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie mir hat genommen", "tokens": ["Da\u00df", "sie", "mir", "hat", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gedanken, Muth und Sinn,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "Muth", "und", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Jahr ist's, da\u00df ich kommen", "tokens": ["Ein", "Jahr", "ist's", ",", "da\u00df", "ich", "kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "