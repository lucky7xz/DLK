er": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das hab ich fast geschrieben", "tokens": ["Das", "hab", "ich", "fast", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An alle B\u00e4ume hier.", "tokens": ["An", "al\u00b7le", "B\u00e4u\u00b7me", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Kein Trinken und kein Essen,", "tokens": ["Kein", "Trin\u00b7ken", "und", "kein", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja nichts hat mir behagt,", "tokens": ["Ja", "nichts", "hat", "mir", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin nur stets gesessen,", "tokens": ["Ich", "bin", "nur", "stets", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und habe mich beklagt:", "tokens": ["Und", "ha\u00b7be", "mich", "be\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In diesem schweren Orden", "tokens": ["In", "die\u00b7sem", "schwe\u00b7ren", "Or\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ver\u00e4ndert alles sich,", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "al\u00b7les", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Heerd' ist mager worden,", "tokens": ["Die", "He\u00b7erd'", "ist", "ma\u00b7ger", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Und ich bin nicht mehr ich.", "tokens": ["Und", "ich", "bin", "nicht", "mehr", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie aber hat die Sinnen", "tokens": ["Sie", "a\u00b7ber", "hat", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit von mir abgekehrt,", "tokens": ["Weit", "von", "mir", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ist gar nicht zu gewinnen,", "tokens": ["Ist", "gar", "nicht", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat mich noch nie erh\u00f6rt;", "tokens": ["Hat", "mich", "noch", "nie", "er\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da doch was ich gesungen", "tokens": ["Da", "doch", "was", "ich", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weit in das Land erschallt,", "tokens": ["Weit", "in", "das", "Land", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auch mein Ton gedrungen", "tokens": ["Und", "auch", "mein", "Ton", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis durch den B\u00f6hmer Wald.", "tokens": ["Bis", "durch", "den", "B\u00f6h\u00b7mer", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die Schaafe, die am Flusse", "tokens": ["Die", "Schaa\u00b7fe", ",", "die", "am", "Flus\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im tiefsten Grase stehn,", "tokens": ["Im", "tiefs\u00b7ten", "Gra\u00b7se", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie horchten meinem Gru\u00dfe,", "tokens": ["Sie", "horch\u00b7ten", "mei\u00b7nem", "Gru\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie wollen zu mir gehn;", "tokens": ["Sie", "wol\u00b7len", "zu", "mir", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sammelt sich die Menge,", "tokens": ["Es", "sam\u00b7melt", "sich", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es winken mir die Fraun,", "tokens": ["Es", "win\u00b7ken", "mir", "die", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch selbst in dem Gedr\u00e4nge,", "tokens": ["Doch", "selbst", "in", "dem", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kann ich die Lieb nicht schaun.", "tokens": ["Kann", "ich", "die", "Lieb", "nicht", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Was soll mein Lied erschallen?", "tokens": ["Was", "soll", "mein", "Lied", "er\u00b7schal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel lieber bin ich still,", "tokens": ["Viel", "lie\u00b7ber", "bin", "ich", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebsten zu gefallen", "tokens": ["Der", "Liebs\u00b7ten", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich einig singen will:", "tokens": ["Ich", "ei\u00b7nig", "sin\u00b7gen", "will", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil alles sie auf Erden", "tokens": ["Weil", "al\u00b7les", "sie", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Allein zusammenh\u00e4lt,", "tokens": ["Al\u00b7lein", "zu\u00b7sam\u00b7men\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kann ihre Gunst mir werden,", "tokens": ["Kann", "ih\u00b7re", "Gunst", "mir", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So hab ich alle Welt.", "tokens": ["So", "hab", "ich", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}