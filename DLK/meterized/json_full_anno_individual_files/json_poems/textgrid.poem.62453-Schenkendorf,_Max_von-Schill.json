, "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Th\u00fcrmen h\u00f6rt ich's schallen,", "tokens": ["Von", "den", "Th\u00fcr\u00b7men", "h\u00f6rt", "ich's", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Bergen schien ein Licht.", "tokens": ["Auf", "den", "Ber\u00b7gen", "schien", "ein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Tag des Volkes, du wirst tagen,", "tokens": ["Tag", "des", "Vol\u00b7kes", ",", "du", "wirst", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich oben feiern will,", "tokens": ["Den", "ich", "o\u00b7ben", "fei\u00b7ern", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein K\u00f6nig selbst wird sagen:", "tokens": ["Und", "mein", "K\u00f6\u00b7nig", "selbst", "wird", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruh' in Frieden, treuer Schill.", "tokens": ["Ruh'", "in", "Frie\u00b7den", ",", "treu\u00b7er", "Schill", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}