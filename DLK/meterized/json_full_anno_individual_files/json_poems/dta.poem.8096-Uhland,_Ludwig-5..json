{"dta.poem.8096": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schreitet k\u00f6niglich der Leu!", "tokens": ["Wie", "schrei\u00b7tet", "k\u00f6\u00b7nig\u00b7lich", "der", "Leu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00fcttelt die M\u00e4hn\u2019 in die L\u00fcfte.", "tokens": ["Sch\u00fct\u00b7telt", "die", "M\u00e4hn'", "in", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Er ruft sein Machtgebot", "tokens": ["Er", "ruft", "sein", "Macht\u00b7ge\u00b7bot"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch W\u00e4lder und Kl\u00fcfte.", "tokens": ["Durch", "W\u00e4l\u00b7der", "und", "Kl\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch werd\u2019 ich ihn st\u00fcrzen", "tokens": ["Doch", "werd'", "ich", "ihn", "st\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit dem Speer in starker Hand,", "tokens": ["Mit", "dem", "Speer", "in", "star\u00b7ker", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die Schultern mir sch\u00fcrzen", "tokens": ["Um", "die", "Schul\u00b7tern", "mir", "sch\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PPER", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sein Goldgewand.", "tokens": ["Sein", "Gold\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Aar, ein K\u00f6nig, schwebet auf,", "tokens": ["Der", "Aar", ",", "ein", "K\u00f6\u00b7nig", ",", "schwe\u00b7bet", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er rauschet in Wonne,", "tokens": ["Er", "rau\u00b7schet", "in", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Will langen sich zur Kron\u2019 herab", "tokens": ["Will", "lan\u00b7gen", "sich", "zur", "Kron'", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJA", "PRF", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die goldene Sonne.", "tokens": ["Die", "gol\u00b7de\u00b7ne", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch in den Wolken hoch", "tokens": ["Doch", "in", "den", "Wol\u00b7ken", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Soll ihn fahen und spie\u00dfen", "tokens": ["Soll", "ihn", "fa\u00b7hen", "und", "spie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Mein gefl\u00fcgelter Pfeil,", "tokens": ["Mein", "ge\u00b7fl\u00fc\u00b7gel\u00b7ter", "Pfeil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Da\u00df er mir sinke zu F\u00fc\u00dfen.", "tokens": ["Da\u00df", "er", "mir", "sin\u00b7ke", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}