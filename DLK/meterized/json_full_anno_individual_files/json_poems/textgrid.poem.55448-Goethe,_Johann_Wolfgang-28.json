{"textgrid.poem.55448": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "28", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Schachtel Mirabellen", "tokens": ["Ei\u00b7ne", "Schach\u00b7tel", "Mi\u00b7ra\u00b7bel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam von S\u00fcden, zog nach Norden;", "tokens": ["Kam", "von", "S\u00fc\u00b7den", ",", "zog", "nach", "Nor\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Frucht gespeist geworden,", "tokens": ["Als", "die", "Frucht", "ge\u00b7speist", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilt sich wieder einzustellen", "tokens": ["Eilt", "sich", "wie\u00b7der", "ein\u00b7zu\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Geh\u00e4us, woher es kommen.", "tokens": ["Das", "Ge\u00b7h\u00e4us", ",", "wo\u00b7her", "es", "kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringet keine s\u00fc\u00dfen Fr\u00fcchte,", "tokens": ["Brin\u00b7get", "kei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bringt vielmehr ein ernst Gesichte,", "tokens": ["Bringt", "viel\u00b7mehr", "ein", "ernst", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das im Weiten und im Fernen", "tokens": ["Das", "im", "Wei\u00b7ten", "und", "im", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Nimmer will Entbehrung lernen.", "tokens": ["Nim\u00b7mer", "will", "Ent\u00b7beh\u00b7rung", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Schachtel Mirabellen", "tokens": ["Ei\u00b7ne", "Schach\u00b7tel", "Mi\u00b7ra\u00b7bel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam von S\u00fcden, zog nach Norden;", "tokens": ["Kam", "von", "S\u00fc\u00b7den", ",", "zog", "nach", "Nor\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Frucht gespeist geworden,", "tokens": ["Als", "die", "Frucht", "ge\u00b7speist", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilt sich wieder einzustellen", "tokens": ["Eilt", "sich", "wie\u00b7der", "ein\u00