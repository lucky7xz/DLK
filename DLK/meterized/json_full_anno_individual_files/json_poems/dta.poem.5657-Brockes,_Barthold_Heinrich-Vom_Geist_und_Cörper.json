{"dta.poem.5657": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der C\u00f6rper werde d\u00fcnn, er werde leicht,", "tokens": ["Der", "C\u00f6r\u00b7per", "wer\u00b7de", "d\u00fcnn", ",", "er", "wer\u00b7de", "leicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er h\u00e4uffe sich in Dunst und werde feucht,", "tokens": ["Er", "h\u00e4uf\u00b7fe", "sich", "in", "Dunst", "und", "wer\u00b7de", "feucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er theile sich. Kan man sich wohl mit Recht bequemen,", "tokens": ["Er", "thei\u00b7le", "sich", ".", "Kan", "man", "sich", "wohl", "mit", "Recht", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$.", "VMFIN", "PIS", "PRF", "ADV", "APPR", "NN", "ADJA", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Jhn als ein Wesen anzunehmen,", "tokens": ["Jhn", "als", "ein", "We\u00b7sen", "an\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das \u00fcberlegt und denckt? Wird Witz und wird Verstand", "tokens": ["Das", "\u00fc\u00b7ber\u00b7legt", "und", "denckt", "?", "Wird", "Witz", "und", "wird", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$.", "VAFIN", "NN", "KON", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In selbigen erkannt?", "tokens": ["In", "sel\u00b7bi\u00b7gen", "er\u00b7kannt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Nein, mit demjenigen, was kein Erkentni\u00df weis\u2019t,", "tokens": ["Nein", ",", "mit", "dem\u00b7je\u00b7ni\u00b7gen", ",", "was", "kein", "Er\u00b7kent\u00b7ni\u00df", "weis't", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "PDS", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kan unser Geist", "tokens": ["Kan", "un\u00b7ser", "Geist"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Nie etwas \u00e4hnliches und gleiches hegen.", "tokens": ["Nie", "et\u00b7was", "\u00e4hn\u00b7li\u00b7ches", "und", "glei\u00b7ches", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "KON", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In allen W\u00fcrckungen wird sich", "tokens": ["In", "al\u00b7len", "W\u00fcr\u00b7ckun\u00b7gen", "wird", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PRF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Ein Unterscheid zu Tage legen.", "tokens": ["Ein", "Un\u00b7ter\u00b7scheid", "zu", "Ta\u00b7ge", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}