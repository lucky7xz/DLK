{"textgrid.poem.39308": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[stets singt und jubelt der Venetianer]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets singt und jubelt der Venetianer,", "tokens": ["Stets", "singt", "und", "ju\u00b7belt", "der", "Ve\u00b7ne\u00b7ti\u00b7a\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihn st\u00f6ren kaum die S\u00e4ulen alter Tage,", "tokens": ["Ihn", "st\u00f6\u00b7ren", "kaum", "die", "S\u00e4u\u00b7len", "al\u00b7ter", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ihn umragen, steinerne Ermahner!", "tokens": ["Die", "ihn", "um\u00b7ra\u00b7gen", ",", "stei\u00b7ner\u00b7ne", "Er\u00b7mah\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier schwimmt Musik im Silberwellenschlage", "tokens": ["Hier", "schwimmt", "Mu\u00b7sik", "im", "Sil\u00b7ber\u00b7wel\u00b7len\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die Piazza trieft von Licht und Leben,", "tokens": ["Und", "die", "Piaz\u00b7za", "trieft", "von", "Licht", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Verloren scheint die Sage und die Klage!", "tokens": ["Ver\u00b7lo\u00b7ren", "scheint", "die", "Sa\u00b7ge", "und", "die", "Kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mich aber packt ein innerstes Erbeben,", "tokens": ["Mich", "a\u00b7ber", "packt", "ein", "in\u00b7ners\u00b7tes", "Er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seh' ich um dieses wimmelnde Gew\u00fcrme", "tokens": ["Seh'", "ich", "um", "die\u00b7ses", "wim\u00b7meln\u00b7de", "Ge\u00b7w\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die alte Pracht ihr f\u00fcrstlich Haupt erheben!", "tokens": ["Die", "al\u00b7te", "Pracht", "ihr", "f\u00fcrst\u00b7lich", "Haupt", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie dumpfer Vorwurf t\u00f6nt der Mund der T\u00fcrme,", "tokens": ["Wie", "dum\u00b7pfer", "Vor\u00b7wurf", "t\u00f6nt", "der", "Mund", "der", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von dem Meere durch des L\u00f6wen M\u00e4hne", "tokens": ["Und", "von", "dem", "Mee\u00b7re", "durch", "des", "L\u00f6\u00b7wen", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ergeht ein Wehen l\u00e4ngst verbrauster St\u00fcrme!", "tokens": ["Er\u00b7geht", "ein", "We\u00b7hen", "l\u00e4ngst", "ver\u00b7braus\u00b7ter", "St\u00fcr\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hinaus, hinaus, wie stille, schwarze Schw\u00e4ne", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", ",", "wie", "stil\u00b7le", ",", "schwar\u00b7ze", "Schw\u00e4\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PWAV", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ziehn dort die Gondeln, drau\u00dfen ist es stille,", "tokens": ["Ziehn", "dort", "die", "Gon\u00b7deln", ",", "drau\u00b7\u00dfen", "ist", "es", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich mu\u00df im Stillen weinen eine Tr\u00e4ne:", "tokens": ["Ich", "mu\u00df", "im", "Stil\u00b7len", "wei\u00b7nen", "ei\u00b7ne", "Tr\u00e4\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"