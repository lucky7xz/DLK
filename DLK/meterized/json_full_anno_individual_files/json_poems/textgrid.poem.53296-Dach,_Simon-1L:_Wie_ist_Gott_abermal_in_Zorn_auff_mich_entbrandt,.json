{"textgrid.poem.53296": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ist Gott abermal in Zorn auff mich entbrandt,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist Gott abermal in Zorn auff mich entbrandt,", "tokens": ["Wie", "ist", "Gott", "a\u00b7ber\u00b7mal", "in", "Zorn", "auff", "mich", "ent\u00b7brandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "ADV", "APPR", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd hat die G\u00fcte gantz in Eiffer vmbgewandt!", "tokens": ["Vnd", "hat", "die", "G\u00fc\u00b7te", "gantz", "in", "Eif\u00b7fer", "vmb\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er reisst ein grosses St\u00fcck mir wieder von dem Hertzen,", "tokens": ["Er", "reisst", "ein", "gros\u00b7ses", "St\u00fcck", "mir", "wie\u00b7der", "von", "dem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd schl\u00e4gt mich, wo es vns am meisten pflegt zu schmertzen,", "tokens": ["Vnd", "schl\u00e4gt", "mich", ",", "wo", "es", "vns", "am", "meis\u00b7ten", "pflegt", "zu", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "PTKA", "PIS", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er heisset vnverhofft mich tieff herunter stehen,", "tokens": ["Er", "heis\u00b7set", "vn\u00b7ver\u00b7hofft", "mich", "tieff", "her\u00b7un\u00b7ter", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich meint', Er w\u00fcrde mich auff einen Fel\u00df erh\u00f6hen.", "tokens": ["Ich", "meint'", ",", "Er", "w\u00fcr\u00b7de", "mich", "auff", "ei\u00b7nen", "Fel\u00df", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich lege hochbetr\u00fcbt mir Trawer-Kleider an,", "tokens": ["Ich", "le\u00b7ge", "hoch\u00b7be\u00b7tr\u00fcbt", "mir", "Tra\u00b7wer\u00b7Klei\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd seufftze, da\u00df ich nicht mehr Athem holen kan,", "tokens": ["Vnd", "seufft\u00b7ze", ",", "da\u00df", "ich", "nicht", "mehr", "A\u00b7them", "ho\u00b7len", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr Schmertzen hab ich fast kein Marck mehr in den Beinen,", "tokens": ["F\u00fcr", "Schmert\u00b7zen", "hab", "ich", "fast", "kein", "Marck", "mehr", "in", "den", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Augen werden Blut vnd schwellen auff von weinen.", "tokens": ["Mein", "Au\u00b7gen", "wer\u00b7den", "Blut", "vnd", "schwel\u00b7len", "auff", "von", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "VVFIN", "APPR", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Jammers Vnmuth hat mir allen Muth genommen,", "tokens": ["Des", "Jam\u00b7mers", "Vn\u00b7muth", "hat", "mir", "al\u00b7len", "Muth", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fcr grossen Sorgen bin ich von mir selbst gekommen.", "tokens": ["F\u00fcr", "gros\u00b7sen", "Sor\u00b7gen", "bin", "ich", "von", "mir", "selbst", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie heisses Wachs zergeht das krancke Hertz in mir,", "tokens": ["Wie", "heis\u00b7ses", "Wachs", "zer\u00b7geht", "das", "kran\u00b7cke", "Hertz", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich gehe Schatten gleich, das Leben ist kaum hier.", "tokens": ["Ich", "ge\u00b7he", "Schat\u00b7ten", "gleich", ",", "das", "Le\u00b7ben", "ist", "kaum", "hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Turtel-Taube liebt nicht so die W\u00fcsteneyen,", "tokens": ["Die", "Tur\u00b7tel\u00b7Tau\u00b7be", "liebt", "nicht", "so", "die", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Als ich die Einsamkeit, ich mu\u00df das Tag-Liecht schewen.", "tokens": ["Als", "ich", "die", "Ein\u00b7sam\u00b7keit", ",", "ich", "mu\u00df", "das", "Tag\u00b7Liecht", "sche\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Das viele Trawren macht mir alle Zier zu nichte,", "tokens": ["Das", "vie\u00b7le", "Traw\u00b7ren", "macht", "mir", "al\u00b7le", "Zier", "zu", "nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bin gantz vngestalt vnd scheu\u00dflich im Gesichte.", "tokens": ["Ich", "bin", "gantz", "vn\u00b7ge\u00b7stalt", "vnd", "scheu\u00df\u00b7lich", "im", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn alles umb die Nacht empfindet seine Ruh,", "tokens": ["Wenn", "al\u00b7les", "umb", "die", "Nacht", "emp\u00b7fin\u00b7det", "sei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wach ich gantz allein vnd thu kein Auge zu,", "tokens": ["So", "wach", "ich", "gantz", "al\u00b7lein", "vnd", "thu", "kein", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann pfleg ich meiner Noth am meisten nach zu dencken,", "tokens": ["Dann", "pfleg", "ich", "mei\u00b7ner", "Noth", "am", "meis\u00b7ten", "nach", "zu", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "PIS", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dan ist es mir bequem mich jnniglich zu kr\u00e4ncken.", "tokens": ["Dan", "ist", "es", "mir", "be\u00b7quem", "mich", "jn\u00b7nig\u00b7lich", "zu", "kr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dan \u00fcberkomm' ich Lust die Vnlust nicht zu hemmen,", "tokens": ["Dan", "\u00fc\u00b7ber\u00b7komm'", "ich", "Lust", "die", "Vn\u00b7lust", "nicht", "zu", "hem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dan m\u00f6chte man erst sehn das Lager mich durchschwemmen.", "tokens": ["Dan", "m\u00f6ch\u00b7te", "man", "erst", "sehn", "das", "La\u00b7ger", "mich", "durch\u00b7schwem\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gott, heb ich kl\u00e4glich an: Z\u00fcrnst du nun ewiglich?", "tokens": ["Gott", ",", "heb", "ich", "kl\u00e4g\u00b7lich", "an", ":", "Z\u00fcrnst", "du", "nun", "e\u00b7wig\u00b7lich", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verbirget gantz vnd gar dein Gnaden-Antlitz sich?", "tokens": ["Ver\u00b7bir\u00b7get", "gantz", "vnd", "gar", "dein", "Gna\u00b7den\u00b7Ant\u00b7litz", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie streck ich Nacht vnd Tag nach dir aus meine H\u00e4nde?", "tokens": ["Wie", "streck", "ich", "Nacht", "vnd", "Tag", "nach", "dir", "aus", "mei\u00b7ne", "H\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du aber fleuchst, je mehr ich, Herr, mich zu dir wende.", "tokens": ["Du", "a\u00b7ber", "fleuchst", ",", "je", "mehr", "ich", ",", "Herr", ",", "mich", "zu", "dir", "wen\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "PPER", "$,", "NN", "$,", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Warumb verfolgstu mich? was wiltu von mir haben?", "tokens": ["Wa\u00b7rumb", "ver\u00b7folgs\u00b7tu", "mich", "?", "was", "wil\u00b7tu", "von", "mir", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PWS", "VMFIN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was hat f\u00fcr dich ein Mensch? was forderst du f\u00fcr Gaben?", "tokens": ["Was", "hat", "f\u00fcr", "dich", "ein", "Mensch", "?", "was", "for\u00b7derst", "du", "f\u00fcr", "Ga\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Begehrstu Hertzens-Angst? der hab' ich gnug bey mir.", "tokens": ["Be\u00b7gehrs\u00b7tu", "Hert\u00b7zens\u00b7Angst", "?", "der", "hab'", "ich", "gnug", "bey", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vieleicht ist dir gedient mit Thr\u00e4nen? die sind hier.", "tokens": ["Vie\u00b7leicht", "ist", "dir", "ge\u00b7dient", "mit", "Thr\u00e4\u00b7nen", "?", "die", "sind", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "NN", "$.", "ART", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Demut? lieg' ich doch, Gott, f\u00fcr dir auff der Erden.", "tokens": ["Mit", "De\u00b7mut", "?", "lieg'", "ich", "doch", ",", "Gott", ",", "f\u00fcr", "dir", "auff", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit seufftzern? jhrer wird nicht mehr gefunden werden.", "tokens": ["Mit", "seufft\u00b7zern", "?", "jhrer", "wird", "nicht", "mehr", "ge\u00b7fun\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ach w\u00fcst' ich deine Gnad, Herr, hiedurch zu erwerben,", "tokens": ["Ach", "w\u00fcst'", "ich", "dei\u00b7ne", "Gnad", ",", "Herr", ",", "hie\u00b7durch", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$,", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wolt' in Klag vnd Leid' ach! mehr, als zehnmal, sterben.", "tokens": ["Ich", "wolt'", "in", "Klag", "vnd", "Leid'", "ach", "!", "mehr", ",", "als", "zehn\u00b7mal", ",", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "XY", "XY", "$.", "ADV", "$,", "KOUS", "ADV", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was f\u00fchrstu, meine Seel', jtzt gar zu grosse Pein,", "tokens": ["Was", "f\u00fchr\u00b7stu", ",", "mei\u00b7ne", "Seel'", ",", "jtzt", "gar", "zu", "gros\u00b7se", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als solte Gott nur feind, nicht Vater wollen seyn?", "tokens": ["Als", "sol\u00b7te", "Gott", "nur", "feind", ",", "nicht", "Va\u00b7ter", "wol\u00b7len", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "NN", "ADV", "NN", "$,", "PTKNEG", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was bistu nur bedacht auff gar zu grossen Schmertzen,", "tokens": ["Was", "bis\u00b7tu", "nur", "be\u00b7dacht", "auff", "gar", "zu", "gros\u00b7sen", "Schmert\u00b7zen", ","], "token_info": ["word", "word", 