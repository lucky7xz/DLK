, "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dan \u00fcberkomm' ich Lust die Vnlust nicht zu hemmen,", "tokens": ["Dan", "\u00fc\u00b7ber\u00b7komm'", "ich", "Lust", "die", "Vn\u00b7lust", "nicht", "zu", "hem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dan m\u00f6chte man erst sehn das Lager mich durchschwemmen.", "tokens": ["Dan", "m\u00f6ch\u00b7te", "man", "erst", "sehn", "das", "La\u00b7ger", "mich", "durch\u00b7schwem\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Gott, heb ich kl\u00e4glich an: Z\u00fcrnst du nun ewiglich?", "tokens": ["Gott", ",", "heb", "ich", "kl\u00e4g\u00b7lich", "an", ":", "Z\u00fcrnst", "du", "nun", "e\u00b7wig\u00b7lich", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verbirget gantz vnd gar dein Gnaden-Antlitz sich?", "tokens": ["Ver\u00b7bir\u00b7get", "gantz", "vnd", "gar", "dein", "Gna\u00b7den\u00b7Ant\u00b7litz", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie streck ich Nacht vnd Tag nach dir aus meine H\u00e4nde?", "tokens": ["Wie", "streck", "ich", "Nacht", "vnd", "Tag", "nach", "dir", "aus", "mei\u00b7ne", "H\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du aber fleuchst, je mehr ich, Herr, mich zu dir wende.", "tokens": ["Du", "a\u00b7ber", "fleuchst", ",", "je", "mehr", "ich", ",", "Herr", ",", "mich", "zu", "dir", "wen\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "PPER", "$,", "NN", "$,", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Warumb verfolgstu mich? was wiltu von mir haben?", "tokens": ["Wa\u00b7rumb", "ver\u00b7folgs\u00b7tu", "mich", "?", "was", "wil\u00b7tu", "von", "mir", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PWS", "VMFIN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was hat f\u00fcr dich ein Mensch? was forderst du f\u00fcr Gaben?", "tokens": ["Was", "hat", "f\u00fcr", "dich", "ein", "Mensch", "?", "was", "for\u00b7derst", "du", "f\u00fcr", "Ga\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Begehrstu Hertzens-Angst? der hab' ich gnug bey mir.", "tokens": ["Be\u00b7gehrs\u00b7tu", "Hert\u00b7zens\u00b7Angst", "?", "der", "hab'", "ich", "gnug", "bey", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vieleicht ist dir gedient mit Thr\u00e4nen? die sind hier.", "tokens": ["Vie\u00b7leicht", "ist", "dir", "ge\u00b7dient", "mit", "Thr\u00e4\u00b7nen", "?", "die", "sind", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "NN", "$.", "ART", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Demut? lieg' ich doch, Gott, f\u00fcr dir auff der Erden.", "tokens": ["Mit", "De\u00b7mut", "?", "lieg'", "ich", "doch", ",", "Gott", ",", "f\u00fcr", "dir", "auff", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit seufftzern? jhrer wird nicht mehr gefunden werden.", "tokens": ["Mit", "seufft\u00b7zern", "?", "jhrer", "wird", "nicht", "mehr", "ge\u00b7fun\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ach w\u00fcst' ich deine Gnad, Herr, hiedurch zu erwerben,", "tokens": ["Ach", "w\u00fcst'", "ich", "dei\u00b7ne", "Gnad", ",", "Herr", ",", "hie\u00b7durch", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$,", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wolt' in Klag vnd Leid' ach! mehr, als zehnmal, sterben.", "tokens": ["Ich", "wolt'", "in", "Klag", "vnd", "Leid'", "ach", "!", "mehr", ",", "als", "zehn\u00b7mal", ",", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "XY", "XY", "$.", "ADV", "$,", "KOUS", "ADV", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Was f\u00fchrstu, meine Seel', jtzt gar zu grosse Pein,", "tokens": ["Was", "f\u00fchr\u00b7stu", ",", "mei\u00b7ne", "Seel'", ",", "jtzt", "gar", "zu", "gros\u00b7se", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als solte Gott nur feind, nicht Vater wollen seyn?", "tokens": ["Als", "sol\u00b7te", "Gott", "nur", "feind", ",", "nicht", "Va\u00b7ter", "wol\u00b7len", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "NN", "ADV", "NN", "$,", "PTKNEG", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was bistu nur bedacht auff gar zu grossen Schmertzen,", "tokens": ["Was", "bis\u00b7tu", "nur", "be\u00b7dacht", "auff", "gar", "zu", "gros\u00b7sen", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "APPR", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd h\u00e4uffst ohn allen Trost die Vnruh deinem Hertzen?", "tokens": ["Vnd", "h\u00e4uffst", "ohn", "al\u00b7len", "Trost", "die", "Vn\u00b7ruh", "dei\u00b7nem", "Hert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Komm' endlich auch zu dir vnd Gott in zuvertrawen,", "tokens": ["Komm'", "end\u00b7lich", "auch", "zu", "dir", "vnd", "Gott", "in", "zu\u00b7ver\u00b7tra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "APPR", "PPER", "KON", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Harr seiner in Geduld, Er wird noch auff dich schawen.", "tokens": ["Harr", "sei\u00b7ner", "in", "Ge\u00b7duld", ",", "Er", "wird", "noch", "auff", "dich", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "APPR", "NN", "$,", "PPER", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.16": {"line.1": {"text": "Ich hoff' es k\u00f6mpt, mich d\u00fcnckt, ich sehe schon die Zeit,", "tokens": ["Ich", "hoff'", "es", "k\u00f6mpt", ",", "mich", "d\u00fcnckt", ",", "ich", "se\u00b7he", "schon", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der Er enden wird mein gantzes Hertzeleid,", "tokens": ["In", "der", "Er", "en\u00b7den", "wird", "mein", "gant\u00b7zes", "Hert\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd setzen meiner Noth gewisses Ziel vnd Schrancken.", "tokens": ["Vnd", "set\u00b7zen", "mei\u00b7ner", "Noth", "ge\u00b7wis\u00b7ses", "Ziel", "vnd", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich aber werd Ihm noch f\u00fcr seine Rettung dancken", "tokens": ["Ich", "a\u00b7ber", "werd", "Ihm", "noch", "f\u00fcr", "sei\u00b7ne", "Ret\u00b7tung", "dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd r\u00fchmen: Da\u00df Er mich l\u00e4sst starck in Ihm bekleiben,", "tokens": ["Vnd", "r\u00fch\u00b7men", ":", "Da\u00df", "Er", "mich", "l\u00e4sst", "starck", "in", "Ihm", "be\u00b7klei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "KOUS", "PPER", "PRF", "VVFIN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd meines Angesichts H\u00fclff' vnd mein Gott wil bleiben.", "tokens": ["Vnd", "mei\u00b7nes", "An\u00b7ge\u00b7sichts", "H\u00fclff'", "vnd", "mein", "Gott", "wil", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPR", "NN", "KON", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}