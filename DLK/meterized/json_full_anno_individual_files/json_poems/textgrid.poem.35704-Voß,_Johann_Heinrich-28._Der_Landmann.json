{"textgrid.poem.35704": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "28. Der Landmann", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr St\u00e4dter, sucht ihr Freude,", "tokens": ["Ihr", "St\u00e4d\u00b7ter", ",", "sucht", "ihr", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So kommt aufs Land heraus.", "tokens": ["So", "kommt", "aufs", "Land", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seht, Garten, Feld und Weide", "tokens": ["Seht", ",", "Gar\u00b7ten", ",", "Feld", "und", "Wei\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umgr\u00fcnt hier jedes Haus.", "tokens": ["Um\u00b7gr\u00fcnt", "hier", "je\u00b7des", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein reicher Mann verbauet", "tokens": ["Kein", "rei\u00b7cher", "Mann", "ver\u00b7bau\u00b7et"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Uns Mond und Sonnenschein;", "tokens": ["Uns", "Mond", "und", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und abends \u00fcberschauet", "tokens": ["Und", "a\u00b7bends", "\u00fc\u00b7bersc\u00b7hau\u00b7et"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Man jedes Sternelein.", "tokens": ["Man", "je\u00b7des", "Ster\u00b7ne\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn fr\u00fch des Dorfes Wecker", "tokens": ["Wenn", "fr\u00fch", "des", "Dor\u00b7fes", "We\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus leichtem Schlaf uns kr\u00e4ht,", "tokens": ["Aus", "leich\u00b7tem", "Schlaf", "uns", "kr\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchjauchzt man rasch die \u00c4cker", "tokens": ["Durch\u00b7jauchzt", "man", "rasch", "die", "\u00c4\u00b7cker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit blankem Feldger\u00e4t.", "tokens": ["Mit", "blan\u00b7kem", "Feld\u00b7ge\u00b7r\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Weib indes treibt singend", "tokens": ["Das", "Weib", "in\u00b7des", "treibt", "sin\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Milchk\u00fch' aus dem Stall:", "tokens": ["Die", "Milch\u00b7k\u00fch'", "aus", "dem", "Stall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Laut folgen sie und springend", "tokens": ["Laut", "fol\u00b7gen", "sie", "und", "sprin\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Hirtenhornes Schall.", "tokens": ["Des", "Hir\u00b7ten\u00b7hor\u00b7nes", "Schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir sehn, wie Gott den Segen", "tokens": ["Wir", "sehn", ",", "wie", "Gott", "den", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus milden H\u00e4nden streut:", "tokens": ["Aus", "mil\u00b7den", "H\u00e4n\u00b7den", "streut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Fr\u00fchlingssonn' und Regen", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7sonn'", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns Wald und Flur erneut;", "tokens": ["Uns", "Wald", "und", "Flur", "er\u00b7neut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Uns bl\u00fchn des Gartens B\u00e4ume;", "tokens": ["Uns", "bl\u00fchn", "des", "Gar\u00b7tens", "B\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Uns wallt das gr\u00fcne Korn;", "tokens": ["Uns", "wallt", "das", "gr\u00fc\u00b7ne", "Korn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Uns schw\u00e4rmt nach Honigseime", "tokens": ["Uns", "schw\u00e4rmt", "nach", "Ho\u00b7ni\u00b7gsei\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Bien' um Blum' und Born.", "tokens": ["Die", "Bi\u00b7en'", "um", "Blum'", "und", "Born", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Uns singt das V\u00f6glein Lieder;", "tokens": ["Uns", "singt", "das", "V\u00f6\u00b7glein", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns rauscht die blaue Flut;", "tokens": ["Uns", "rauscht", "die", "blau\u00b7e", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns schwirrt des Hofs Gefieder,", "tokens": ["Uns", "schwirrt", "des", "Hofs", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umpiept von junger Brut;", "tokens": ["Um\u00b7piept", "von", "jun\u00b7ger", "Brut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Uns bl\u00f6ken rings und br\u00fcllen", "tokens": ["Uns", "bl\u00f6\u00b7ken", "rings", "und", "br\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Herden durch die Au'n;", "tokens": ["Die", "Her\u00b7den", "durch", "die", "Au'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Uns tanzt das schlanke F\u00fcllen,", "tokens": ["Uns", "tanzt", "das", "schlan\u00b7ke", "F\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und gaffet \u00fcbern Zaun.", "tokens": ["Und", "gaf\u00b7fet", "\u00fc\u00b7bern", "Zaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Arbeit aber w\u00fcrzet", "tokens": ["Die", "Ar\u00b7beit", "a\u00b7ber", "w\u00fcr\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Landmann seine Kost,", "tokens": ["Dem", "Land\u00b7mann", "sei\u00b7ne", "Kost", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Mut und Freude k\u00fcrzet", "tokens": ["Und", "Mut", "und", "Freu\u00b7de", "k\u00fcr\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die M\u00fch' in Hitz' und Frost.", "tokens": ["Die", "M\u00fch'", "in", "Hitz'", "und", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Weib begr\u00fc\u00dft ihn schmeichelnd,", "tokens": ["Sein", "Weib", "be\u00b7gr\u00fc\u00dft", "ihn", "schmei\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn er vom Felde kehrt,", "tokens": ["Wenn", "er", "vom", "Fel\u00b7de", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und, seine Kindlein streichelnd,", "tokens": ["Und", ",", "sei\u00b7ne", "Kin\u00b7dlein", "strei\u00b7chelnd", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich setzt am hellen Herd.", "tokens": ["Sich", "setzt", "am", "hel\u00b7len", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Bursch' und M\u00e4gde strotzen", "tokens": ["Die", "Bur\u00b7sch'", "und", "M\u00e4g\u00b7de", "strot\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Von Jugendreiz und Mark;", "tokens": ["Von", "Ju\u00b7gen\u00b7dreiz", "und", "Mark", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja selbst die Greise trotzen", "tokens": ["Ja", "selbst", "die", "Grei\u00b7se", "trot\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Alter, frisch und stark.", "tokens": ["Dem", "Al\u00b7ter", ",", "frisch", "und", "stark", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und hei\u00dft der Tod uns wandern;", "tokens": ["Und", "hei\u00dft", "der", "Tod", "uns", "wan\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir gehn, wie \u00fcber Feld,", "tokens": ["Wir", "gehn", ",", "wie", "\u00fc\u00b7ber", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus einer Welt zur andern", "tokens": ["Aus", "ei\u00b7ner", "Welt", "zur", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und sch\u00f6nern Gotteswelt.", "tokens": ["Und", "sch\u00f6\u00b7nern", "Got\u00b7tes\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr armen St\u00e4dter trauert", "tokens": ["Ihr", "ar\u00b7men", "St\u00e4d\u00b7ter", "trau\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambi