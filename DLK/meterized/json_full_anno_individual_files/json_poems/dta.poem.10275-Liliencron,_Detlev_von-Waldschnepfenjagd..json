{"dta.poem.10275": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Waldschnepfenjagd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Vor Tagesanbruch ging ich einst zum Busch,           ", "tokens": ["Vor", "Ta\u00b7ge\u00b7san\u00b7bruch", "ging", "ich", "einst", "zum", "Busch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den scheuen Vogel zu erlegen, der,", "tokens": ["Den", "scheu\u00b7en", "Vo\u00b7gel", "zu", "er\u00b7le\u00b7gen", ",", "der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "PRELS", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Fr\u00fchlingswanderzug nach ferner K\u00fcste,", "tokens": ["Im", "Fr\u00fch\u00b7lings\u00b7wan\u00b7der\u00b7zug", "nach", "fer\u00b7ner", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geheimni\u00dfvoll durch unsre W\u00e4lder zieht.", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7voll", "durch", "uns\u00b7re", "W\u00e4l\u00b7der", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bald stand ich schu\u00dfbereit am Holzesrande,", "tokens": ["Bald", "stand", "ich", "schu\u00df\u00b7be\u00b7reit", "am", "Hol\u00b7zes\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu F\u00fc\u00dfen, jagdgierzitternd, sa\u00df der Hund.", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", ",", "jagd\u00b7gier\u00b7zit\u00b7ternd", ",", "sa\u00df", "der", "Hund", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In schwerem Dunste lag die feuchte Wiese,", "tokens": ["In", "schwe\u00b7rem", "Duns\u00b7te", "lag", "die", "feuch\u00b7te", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und dr\u00fcber weg, trotz D\u00e4mmerung und Nebel", "tokens": ["Und", "dr\u00fc\u00b7ber", "weg", ",", "trotz", "D\u00e4m\u00b7me\u00b7rung", "und", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PAV", "PTKVZ", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sah deutlich ich\u2019s, bog sich ein Kranz von Tannen.", "tokens": ["Sah", "deut\u00b7lich", "ich's", ",", "bog", "sich", "ein", "Kranz", "von", "Tan\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PIS", "$,", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Schon zwitscherten, doch klang es noch aus Tr\u00e4umen,", "tokens": ["Schon", "zwit\u00b7scher\u00b7ten", ",", "doch", "klang", "es", "noch", "aus", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Vereinzelt Vogelstimmen, und es brach,", "tokens": ["Ver\u00b7ein\u00b7zelt", "Vo\u00b7gel\u00b7stim\u00b7men", ",", "und", "es", "brach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wie fl\u00fcsternd durch die kahlen, schwarzen \u00c4ste,", "tokens": ["Wie", "fl\u00fcs\u00b7ternd", "durch", "die", "kah\u00b7len", ",", "schwar\u00b7zen", "\u00c4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein kurzer, k\u00fchler Windsto\u00df, der, ein L\u00e4ufer,", "tokens": ["Ein", "kur\u00b7zer", ",", "k\u00fch\u00b7ler", "Wind\u00b7sto\u00df", ",", "der", ",", "ein", "L\u00e4u\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "PRELS", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den Sonnenaufgang eilig pflegt zu k\u00fcnden.", "tokens": ["Den", "Son\u00b7nen\u00b7auf\u00b7gang", "ei\u00b7lig", "pflegt", "zu", "k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Da sah zwei Menschen ich am Tannensaum.", "tokens": ["Da", "sah", "zwei", "Men\u00b7schen", "ich", "am", "Tan\u00b7nen\u00b7saum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Im Jagdrock er, die B\u00fcchse umgehangen,", "tokens": ["Im", "Jag\u00b7drock", "er", ",", "die", "B\u00fcch\u00b7se", "um\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Den Hut ein wenig auf das Ohr geschoben.", "tokens": ["Den", "Hut", "ein", "we\u00b7nig", "auf", "das", "Ohr", "ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Das M\u00e4dchen eingeschmiegt in dichte Pelze,", "tokens": ["Das", "M\u00e4d\u00b7chen", "ein\u00b7ge\u00b7schmiegt", "in", "dich\u00b7te", "Pel\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ein wei\u00dfes T\u00fcchelchen um Kopf und Schulter.", "tokens": ["Ein", "wei\u00b7\u00dfes", "T\u00fc\u00b7chel\u00b7chen", "um", "Kopf", "und", "Schul\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Es lagen ihre H\u00e4ndchen in den seinen.", "tokens": ["Es", "la\u00b7gen", "ih\u00b7re", "H\u00e4nd\u00b7chen", "in", "den", "sei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Aus Nebelthoren zog die Siegersonne: \u2014", "tokens": ["Aus", "Ne\u00b7belt\u00b7ho\u00b7ren", "zog", "die", "Sie\u00b7ger\u00b7son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und von des M\u00e4dchens Sch\u00f6nheit wie berauscht,", "tokens": ["Und", "von", "des", "M\u00e4d\u00b7chens", "Sch\u00f6n\u00b7heit", "wie", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Nahm schnell er ihr das wei\u00dfe Tuch vom Haupte,", "tokens": ["Nahm", "schnell", "er", "ihr", "das", "wei\u00b7\u00dfe", "Tuch", "vom", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Da\u00df schwer, in goldenroten, breiten Str\u00f6men,", "tokens": ["Da\u00df", "schwer", ",", "in", "gol\u00b7den\u00b7ro\u00b7ten", ",", "brei\u00b7ten", "Str\u00f6\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Das ungebund\u2019ne Haar sie ganz umflo\u00df.", "tokens": ["Das", "un\u00b7ge\u00b7bun\u00b7d'\u00b7ne", "Haar", "sie", "ganz", "um\u00b7flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Wie halb ertappt auf unerlaubten Wegen,", "tokens": ["Wie", "halb", "er\u00b7tappt", "auf", "un\u00b7er\u00b7laub\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Fand ich mich bald in anderen Gehegen.", "tokens": ["Fand", "ich", "mich", "bald", "in", "an\u00b7de\u00b7ren", "Ge\u00b7he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}