{"textgrid.poem.40840": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Circus dort, ob einer dunklen Zelle", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Circus dort, ob einer dunklen Zelle", "tokens": ["Im", "Cir\u00b7cus", "dort", ",", "ob", "ei\u00b7ner", "dunk\u00b7len", "Zel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verfallnem Thor, winkt aus der Quadern Rissen", "tokens": ["Ver\u00b7fall\u00b7nem", "Thor", ",", "winkt", "aus", "der", "Qua\u00b7dern", "Ris\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Bl\u00fcthenstrauch, gerankt gar fr\u00f6hlich helle,", "tokens": ["Ein", "Bl\u00fc\u00b7then\u00b7strauch", ",", "ge\u00b7rankt", "gar", "fr\u00f6h\u00b7lich", "hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie einer Schenke Kranz mit lust'gem Gr\u00fc\u00dfen!", "tokens": ["Wie", "ei\u00b7ner", "Schen\u00b7ke", "Kranz", "mit", "lust'\u00b7gem", "Gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir treten ein! Nicht m\u00fcht um seine G\u00e4ste", "tokens": ["Wir", "tre\u00b7ten", "ein", "!", "Nicht", "m\u00fcht", "um", "sei\u00b7ne", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wirth, der hag're, sich in diesen R\u00e4umen:", "tokens": ["Der", "Wirth", ",", "der", "hag'\u00b7re", ",", "sich", "in", "die\u00b7sen", "R\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In einer Ecke h\u00e4lt er ruhig Sieste,", "tokens": ["In", "ei\u00b7ner", "E\u00b7cke", "h\u00e4lt", "er", "ru\u00b7hig", "Sies\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die tausendj\u00e4hr'gen Tr\u00e4ume auszutr\u00e4umen.", "tokens": ["Die", "tau\u00b7send\u00b7j\u00e4hr'\u00b7gen", "Tr\u00e4u\u00b7me", "aus\u00b7zu\u00b7tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Seht auf den Polstern tausendj\u00e4hr'ger Laven,", "tokens": ["Seht", "auf", "den", "Pols\u00b7tern", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ger", "La\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die einst geprunkt in Purpurs K\u00f6nigsfarbe,", "tokens": ["Die", "einst", "ge\u00b7prunkt", "in", "Pur\u00b7purs", "K\u00f6\u00b7nigs\u00b7far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gekauert das Geripp' des Fechtersklaven,", "tokens": ["Ge\u00b7kau\u00b7ert", "das", "Ge\u00b7ripp'", "des", "Fech\u00b7ter\u00b7skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwischt selbst seiner Stirne Siegesnarbe!", "tokens": ["Ver\u00b7wischt", "selbst", "sei\u00b7ner", "Stir\u00b7ne", "Sie\u00b7ges\u00b7nar\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er tr\u00e4umt vielleicht noch fort die dunkle Kunde", "tokens": ["Er", "tr\u00e4umt", "viel\u00b7leicht", "noch", "fort", "die", "dunk\u00b7le", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Spartakus, der Knechtschaft Ahasvere,", "tokens": ["Vom", "Spar\u00b7ta\u00b7kus", ",", "der", "Knecht\u00b7schaft", "A\u00b7has\u00b7ve\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "De\u00df bleich Gespenst noch wandelt seine Runde,", "tokens": ["De\u00df", "bleich", "Ge\u00b7spenst", "noch", "wan\u00b7delt", "sei\u00b7ne", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erneuend stets die alte, blut'ge M\u00e4hre!", "tokens": ["Er\u00b7neu\u00b7end", "stets", "die", "al\u00b7te", ",", "blut'\u00b7ge", "M\u00e4h\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er tr\u00e4umt von der Arena Bahn und Stufen,", "tokens": ["Er", "tr\u00e4umt", "von", "der", "A\u00b7re\u00b7na", "Bahn", "und", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vom Siegeskranze, der ihm zugeflogen;", "tokens": ["Vom", "Sie\u00b7ges\u00b7kran\u00b7ze", ",", "der", "ihm", "zu\u00b7ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fast sch\u00fcttelte des Volkes Beifallrufen", "tokens": ["Fast", "sch\u00fct\u00b7tel\u00b7te", "des", "Vol\u00b7kes", "Bei\u00b7fall\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sterne noch dazu vom Himmelsbogen!", "tokens": ["Die", "Ster\u00b7ne", "noch", "da\u00b7zu", "vom", "Him\u00b7mels\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohl d\u00fcnkt die bandumwundne Blumenkrone", "tokens": ["Wohl", "d\u00fcnkt", "die", "ban\u00b7dum\u00b7wund\u00b7ne", "Blu\u00b7men\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm ein versch\u00f6nert Nachbild nur des Strickes,", "tokens": ["Ihm", "ein", "ver\u00b7sch\u00f6\u00b7nert", "Nach\u00b7bild", "nur", "des", "Stri\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "VVPP", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den er als Zeichen seiner Knechtschaftsfrohne", "tokens": ["Den", "er", "als", "Zei\u00b7chen", "sei\u00b7ner", "Knecht\u00b7schafts\u00b7froh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einst trug als grausen Kranzreif des Genickes!", "tokens": ["Einst", "trug", "als", "grau\u00b7sen", "Kranz\u00b7reif", "des", "Ge\u00b7ni\u00b7ckes", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Wort durchschl\u00e4ngelt dort den Stein der W\u00e4nde,", "tokens": ["Ein", "Wort", "durch\u00b7schl\u00e4n\u00b7gelt", "dort", "den", "Stein", "der", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl ritzten's ins Gestein des Sklaven H\u00e4nde", "tokens": ["Wohl", "ritz\u00b7ten's", "ins", "Ge\u00b7stein", "des", "Skla\u00b7ven", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Einst statt des Griffels mit des Kampfdolchs Spitze.", "tokens": ["Einst", "statt", "des", "Grif\u00b7fels", "mit", "des", "Kampf\u00b7dolchs", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Noch ist die fahle Stirn' dahin gerichtet,", "tokens": ["Noch", "ist", "die", "fah\u00b7le", "Stirn'", "da\u00b7hin", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch ist das hohle Aug' dahin gewendet,", "tokens": ["Noch", "ist", "das", "hoh\u00b7le", "Aug'", "da\u00b7hin", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie nach dem Sterne, der sein Dunkel lichtet,", "tokens": ["Wie", "nach", "dem", "Ster\u00b7ne", ",", "der", "sein", "Dun\u00b7kel", "lich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie nach der Sonne, deren Glanz ihn blendet.", "tokens": ["Wie", "nach", "der", "Son\u00b7ne", ",", "de\u00b7ren", "Glanz", "ihn", "blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wie aus dem Becher Weins, des guten, alten,", "tokens": ["Wie", "aus", "dem", "Be\u00b7cher", "Weins", ",", "des", "gu\u00b7ten", ",", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sehnen Kraft und Muth die Herzen saugen,", "tokens": ["Die", "Seh\u00b7nen", "Kraft", "und", "Muth", "die", "Her\u00b7zen", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So tr\u00e4nkt' aus jenem Wort, sie wach zu halten,", "tokens": ["So", "tr\u00e4nkt'", "aus", "je\u00b7nem", "Wort", ",", "sie", "wach", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Licht f\u00fcr lange N\u00e4cht' er seine Augen.", "tokens": ["Mit", "Licht", "f\u00fcr", "lan\u00b7ge", "N\u00e4cht'", "er", "sei\u00b7ne", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Du sch\u00f6ner Strauch vor'm Thor, den fremden G\u00e4sten", "tokens": ["Du", "sch\u00f6\u00b7ner", "Strauch", "vor'm", "Thor", ",", "den", "frem\u00b7den", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Log nicht dein Zeiger, der gewinkt zum Weine!", "tokens": ["Log", "nicht", "dein", "Zei\u00b7ger", ",", "der", "ge\u00b7winkt", "zum", "Wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja hier ist Wein! Und zwar vom st\u00e4rksten, besten!", "tokens": ["Ja", "hier", "ist", "Wein", "!", "Und", "zwar", "vom", "st\u00e4rks\u00b7ten", ",", "bes\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "ADV", "VAFIN", "NN", "$.", "KON", "ADV", "APPRART", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier wird geschenkt der Tausendj\u00e4hr'ge, Reine!", "tokens": ["Hier", "wird", "ge\u00b7schenkt", "der", "Tau\u00b7send\u00b7j\u00e4hr'\u00b7ge", ",", "Rei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ihr aber, Franken und Germanen, Britten", "tokens": ["Ihr", "a\u00b7ber", ",", "Fran\u00b7ken", "und", "Ger\u00b7ma\u00b7nen", ",", "Brit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "NN", "KON", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sonst all' dieser Tr\u00fcmmerwelt Nomaden,", "tokens": ["Und", "sonst", "all'", "die\u00b7ser", "Tr\u00fcm\u00b7mer\u00b7welt", "No\u00b7ma\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00dft einzutreten euch nicht lange bitten!", "tokens": ["La\u00dft", "ein\u00b7zu\u00b7tre\u00b7ten", "euch", "nicht", "lan\u00b7ge", "bit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schl\u00fcckchen im Vorbeigehn wird nicht schaden.", "tokens": ["Ein", "Schl\u00fcck\u00b7chen", "im", "Vor\u00b7bei\u00b7gehn", "wird", "nicht", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Im Circus dort, ob einer dunklen Zelle", "tokens": ["Im", "Cir\u00b7cus", "dort", ",", "ob", "ei\u00b7ner", "dunk\u00b7len", "Zel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verfallnem Thor, winkt aus der Quadern Rissen", "tokens": ["Ver\u00b7fall\u00b7nem", "Thor", ",", "winkt", "aus", "der", "Qua\u00b7dern", "Ris\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Bl\u00fcthenstrauch, gerankt gar fr\u00f6hlich helle,", "tokens": ["Ein", "Bl\u00fc\u00b7then\u00b7strauch", ",", "ge\u00b7rankt", "gar", "fr\u00f6h\u00b7lich", "hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "ADV", "ADJD", "ADJA", "$,"],