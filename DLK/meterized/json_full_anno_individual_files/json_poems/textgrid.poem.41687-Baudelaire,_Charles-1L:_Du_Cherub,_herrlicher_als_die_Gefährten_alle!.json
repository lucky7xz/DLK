{"textgrid.poem.41687": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Cherub, herrlicher als die Gef\u00e4hrten alle!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Cherub, herrlicher als die Gef\u00e4hrten alle!", "tokens": ["Du", "Che\u00b7rub", ",", "herr\u00b7li\u00b7cher", "als", "die", "Ge\u00b7f\u00e4hr\u00b7ten", "al\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADJA", "KOKOM", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott ohne Ruhm! Gest\u00fcrzt in allgewaltgem Falle,", "tokens": ["Gott", "oh\u00b7ne", "Ruhm", "!", "Ge\u00b7st\u00fcrzt", "in", "all\u00b7ge\u00b7walt\u00b7gem", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O K\u00f6nig des Exils, der unrecht ward verbannt,", "tokens": ["O", "K\u00f6\u00b7nig", "des", "E\u00b7xils", ",", "der", "un\u00b7recht", "ward", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der, wenngleich besiegt, stets st\u00e4rker neu erstand,", "tokens": ["Und", "der", ",", "wenn\u00b7gleich", "be\u00b7siegt", ",", "stets", "st\u00e4r\u00b7ker", "neu", "er\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "VVPP", "$,", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O du, allweiser F\u00fcrst in unterirdschen Reichen,", "tokens": ["O", "du", ",", "all\u00b7wei\u00b7ser", "F\u00fcrst", "in", "un\u00b7ter\u00b7ird\u00b7schen", "Rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Heiland jeder Angst, die Menschen l\u00e4\u00dft erbleichen,", "tokens": ["Du", "Hei\u00b7land", "je\u00b7der", "Angst", ",", "die", "Men\u00b7schen", "l\u00e4\u00dft", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Aussatzkranken selbst und Parias verleiht", "tokens": ["Der", "Aus\u00b7satz\u00b7kran\u00b7ken", "selbst", "und", "Pa\u00b7ri\u00b7as", "ver\u00b7leiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Liebe den Genu\u00df von Edens Seligkeit,", "tokens": ["Durch", "Lie\u00b7be", "den", "Ge\u00b7nu\u00df", "von", "E\u00b7dens", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der zur Geliebten die Verwesung sich erkoren,", "tokens": ["Der", "zur", "Ge\u00b7lieb\u00b7ten", "die", "Ver\u00b7we\u00b7sung", "sich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jene T\u00f6rin dir, die Hoffnung, hat geboren,", "tokens": ["Die", "je\u00b7ne", "T\u00f6\u00b7rin", "dir", ",", "die", "Hoff\u00b7nung", ",", "hat", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "PPER", "$,", "ART", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Der Todgeweihten gibt den Blick voll stolzem Trotz,", "tokens": ["Der", "Tod\u00b7ge\u00b7weih\u00b7ten", "gibt", "den", "Blick", "voll", "stol\u00b7zem", "Trotz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Volk verdammend von der H\u00f6he des Schafotts,", "tokens": ["Ein", "Volk", "ver\u00b7dam\u00b7mend", "von", "der", "H\u00f6\u00b7he", "des", "Scha\u00b7fotts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Du, welcher wei\u00df und kennt, in was f\u00fcr nie entdeckte", "tokens": ["Du", ",", "wel\u00b7cher", "wei\u00df", "und", "kennt", ",", "in", "was", "f\u00fcr", "nie", "ent\u00b7deck\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,", "APPR", "PRELS", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gekl\u00fcfte Gott voll Neid den Edelstein versteckte,", "tokens": ["Ge\u00b7kl\u00fcf\u00b7te", "Gott", "voll", "Neid", "den", "E\u00b7del\u00b7stein", "ver\u00b7steck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Du, dessen klarer Blick die Kammern \u00fcberfliegt,", "tokens": ["Du", ",", "des\u00b7sen", "kla\u00b7rer", "Blick", "die", "Kam\u00b7mern", "\u00fc\u00b7berf\u00b7liegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo der Metalle Volk versargt im Schlummer liegt,", "tokens": ["Wo", "der", "Me\u00b7tal\u00b7le", "Volk", "ver\u00b7sargt", "im", "Schlum\u00b7mer", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Du, dessen m\u00e4chtge Hand am Abgrund sicher leitet", "tokens": ["Du", ",", "des\u00b7sen", "m\u00e4cht\u00b7ge", "Hand", "am", "Ab\u00b7grund", "si\u00b7cher", "lei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "APPRART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Menschen, der im Schlaf l\u00e4ngs hoher Zinnen schreitet,", "tokens": ["Den", "Men\u00b7schen", ",", "der", "im", "Schlaf", "l\u00e4ngs", "ho\u00b7her", "Zin\u00b7nen", "schrei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Du, der des S\u00e4ufers alt Gebein durch Zauberkraft", "tokens": ["Du", ",", "der", "des", "S\u00e4u\u00b7fers", "alt", "Ge\u00b7bein", "durch", "Zau\u00b7ber\u00b7kraft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADJD", "NN", "APPR", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Selbst unter Rosseshuf heil und geschmeidig schafft,", "tokens": ["Selbst", "un\u00b7ter", "Ros\u00b7ses\u00b7huf", "heil", "und", "ge\u00b7schmei\u00b7dig", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Erbarm dich, Satan, mein und meiner tiefen Qualen!", "tokens": ["Er\u00b7barm", "dich", ",", "Sa\u00b7tan", ",", "mein", "und", "mei\u00b7ner", "tie\u00b7fen", "Qua\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.21": {"line.1": {"text": "Du, der zum Trost des Manns, den Elend ganz verzehrt hat,", "tokens": ["Du", ",", "der", "zum", "Trost", "des", "Manns", ",", "den", "E\u00