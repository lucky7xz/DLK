{"dta.poem.21496": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "In der Einsamkeit.  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Fernab f\u00e4llt wie fortwandelnder St\u00fcrme Sausen", "tokens": ["Fern\u00b7ab", "f\u00e4llt", "wie", "fort\u00b7wan\u00b7deln\u00b7der", "St\u00fcr\u00b7me", "Sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJA", "NN", "NN"], "meter": "-+--++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hin verworrener L\u00e4rm der Riesenweltstadt,", "tokens": ["Hin", "ver\u00b7wor\u00b7re\u00b7ner", "L\u00e4rm", "der", "Rie\u00b7sen\u00b7welt\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und in\u2019s Ohr nur t\u00f6nt mir selten", "tokens": ["Und", "in's", "Ohr", "nur", "t\u00f6nt", "mir", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ein Ruf und m\u00fcdes Kinderlallen.", "tokens": ["Noch", "ein", "Ruf", "und", "m\u00fc\u00b7des", "Kin\u00b7der\u00b7lal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Lockte der erste Maiensonntag", "tokens": ["Lock\u00b7te", "der", "ers\u00b7te", "Mai\u00b7en\u00b7sonn\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bunte jubelnde Menschenfluthen", "tokens": ["Bun\u00b7te", "ju\u00b7beln\u00b7de", "Men\u00b7schen\u00b7flu\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Fort und weg zu goldigspiegelnden Wassern,", "tokens": ["Fort", "und", "weg", "zu", "gol\u00b7digs\u00b7pie\u00b7geln\u00b7den", "Was\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In das wei\u00dflichschillernde Fr\u00fchlingsgr\u00fcn;", "tokens": ["In", "das", "wei\u00df\u00b7lich\u00b7schil\u00b7lern\u00b7de", "Fr\u00fch\u00b7lings\u00b7gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Walten alle, jauchzenden Herzens,", "tokens": ["Wal\u00b7ten", "al\u00b7le", ",", "jauch\u00b7zen\u00b7den", "Her\u00b7zens", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wie zum Gnadenbilde der Himmelsf\u00fcrstin", "tokens": ["Wie", "zum", "Gna\u00b7den\u00b7bil\u00b7de", "der", "Him\u00b7mels\u00b7f\u00fcrs\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Singende M\u00f6nche mit seidenen Bannern wallen.", "tokens": ["Sin\u00b7gen\u00b7de", "M\u00f6n\u00b7che", "mit", "sei\u00b7de\u00b7nen", "Ban\u00b7nern", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch mich warf die gl\u00e4nzende Fluth zur Seite,", "tokens": ["Doch", "mich", "warf", "die", "gl\u00e4n\u00b7zen\u00b7de", "Fluth", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Da in Schmerzen erschauerte meine Seele,", "tokens": ["Da", "in", "Schmer\u00b7zen", "er\u00b7schau\u00b7er\u00b7te", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und ich wandte, Dunkel im Herzen,", "tokens": ["Und", "ich", "wand\u00b7te", ",", "Dun\u00b7kel", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wandte die Schritte denn ein jedes", "tokens": ["Wand\u00b7te", "die", "Schrit\u00b7te", "denn", "ein", "je\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "PIAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Liebeathmende Frauenantlitz", "tokens": ["Lie\u00b7be\u00b7ath\u00b7men\u00b7de", "Frau\u00b7en\u00b7ant\u00b7litz"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Mahnte mich an deine Sch\u00f6nheit,", "tokens": ["Mahn\u00b7te", "mich", "an", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine trunkenen K\u00fcsse und die L\u00fcge", "tokens": ["Dei\u00b7ne", "trun\u00b7ke\u00b7nen", "K\u00fcs\u00b7se", "und", "die", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Deines Herzens.", "tokens": ["Dei\u00b7nes", "Her\u00b7zens", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Nimm mich auf, nimm mich auf,", "tokens": ["Nimm", "mich", "auf", ",", "nimm", "mich", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Einsamkeit in deinen Dom,", "tokens": ["Ein\u00b7sam\u00b7keit", "in", "dei\u00b7nen", "Dom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df eintreten mich, Friedsuchenden,", "tokens": ["La\u00df", "ein\u00b7tre\u00b7ten", "mich", ",", "Fried\u00b7su\u00b7chen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+++--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und vor deinem Altar in Opferschalen", "tokens": ["Und", "vor", "dei\u00b7nem", "Al\u00b7tar", "in", "Op\u00b7fer\u00b7scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ausgie\u00dfen mein Blut und meine Thr\u00e4nen.", "tokens": ["Aus\u00b7gie\u00b7\u00dfen", "mein", "Blut", "und", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "An deinen Busen nimm mein Haupt!", "tokens": ["An", "dei\u00b7nen", "Bu\u00b7sen", "nimm", "mein", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ueber mir nur Sternflammen", "tokens": ["Ue\u00b7ber", "mir", "nur", "Stern\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und wehende Wolken . . . . . .", "tokens": ["Und", "we\u00b7hen\u00b7de", "Wol\u00b7ken", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hier versink\u2019 ich im weiten Raum,", "tokens": ["Hier", "ver\u00b7sink'", "ich", "im", "wei\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wandle wie Ihr leuchtende Himmelsseelen", "tokens": ["Wand\u00b7le", "wie", "Ihr", "leuch\u00b7ten\u00b7de", "Him\u00b7mels\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "Allein \u2014 allein in endlosen Weiten.", "tokens": ["Al\u00b7lein", "al\u00b7lein", "in", "end\u00b7lo\u00b7sen", "Wei\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Einsamkeit, wie bebte ich einst vor dir,", "tokens": ["Ein\u00b7sam\u00b7keit", ",", "wie", "beb\u00b7te", "ich", "einst", "vor", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schrak vor dir, wie die erste Bl\u00fcthe", "tokens": ["Schrak", "vor", "dir", ",", "wie", "die", "ers\u00b7te", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schrickt im Garten vor nachziehenden Winterfr\u00f6sten.", "tokens": ["Schrickt", "im", "Gar\u00b7ten", "vor", "nach\u00b7zie\u00b7hen\u00b7den", "Win\u00b7ter\u00b7fr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Schauernd vor dir barg ich mein Haupt", "tokens": ["Schau\u00b7ernd", "vor", "dir", "barg", "ich", "mein", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "An der Frauen wei\u00dfem Busen,", "tokens": ["An", "der", "Frau\u00b7en", "wei\u00b7\u00dfem", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchte dich heilige Liebe,", "tokens": ["Such\u00b7te", "dich", "hei\u00b7li\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Helles, k\u00fchles Morgenwasser du,", "tokens": ["Hel\u00b7les", ",", "k\u00fch\u00b7les", "Mor\u00b7gen\u00b7was\u00b7ser", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df ich in dir baden wollte", "tokens": ["Da\u00df", "ich", "in", "dir", "ba\u00b7den", "woll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF", "VMFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und gesunden zu ewiger hoher Wunderfreude!", "tokens": ["Und", "ge\u00b7sun\u00b7den", "zu", "e\u00b7wi\u00b7ger", "ho\u00b7her", "Wun\u00b7der\u00b7freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PTKZU", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Liebe! Rosige Briefchen ihr,", "tokens": ["Lie\u00b7be", "!", "Ro\u00b7si\u00b7ge", "Brief\u00b7chen", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Beschmutzt mit L\u00fcgen und falschen Schw\u00fcren,", "tokens": ["Be\u00b7schmutzt", "mit", "L\u00fc\u00b7gen", "und", "fal\u00b7schen", "Schw\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "In\u2019s Feuer, in\u2019s Feuer!", "tokens": ["In's", "Feu\u00b7er", ",", "in's", "Feu\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Vor\u00fcber wallen an mir Gestalten \u2014 \u2014", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "wal\u00b7len", "an", "mir", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVINF", "APPR", "PPER", "NN", "$(", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Hinunter, hinunter ihr Glei\u00dfenden,", "tokens": ["Hin\u00b7un\u00b7ter", ",", "hin\u00b7un\u00b7ter", "ihr", "Glei\u00b7\u00dfen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Nicht lockt ihr mich wieder!", "tokens": ["Nicht", "lockt", "ihr", "mich", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Und auch du!", "tokens": ["Und", "auch", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Waffengenosse, mit dem ich stets zusammenstand,", "tokens": ["Waf\u00b7fen\u00b7ge\u00b7nos\u00b7se", ",", "mit", "dem", "ich", "stets", "zu\u00b7sam\u00b7men\u00b7stand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Umqualmt vom Rauch der Schlacht,", "tokens": ["Um\u00b7qualmt", "vom", "Rauch", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du, mein Schild, Du, mein Streitbeil \u2014", "tokens": ["Du", ",", "mein", "Schild", ",", "Du", ",", "mein", "Streit\u00b7beil"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Ein Mantel deckte uns, ein Becher labte uns &#8212;               ", "tokens": ["Ein", "Man\u00b7tel", "deck\u00b7te", "uns", ",", "ein", "Be\u00b7cher", "lab\u00b7te", "uns", "&#8212;"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir beide, Zweige am selben Baum,", "tokens": ["Wir", "bei\u00b7de", ",", "Zwei\u00b7ge", "am", "sel\u00b7ben", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Br\u00fcder wir, \u2014", "tokens": ["Br\u00fc\u00b7der", "wir", ","], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$,", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Nach anderem sch\u00f6neren Sterne", "tokens": ["Nach", "an\u00b7de\u00b7rem", "sch\u00f6\u00b7ne\u00b7ren", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text":