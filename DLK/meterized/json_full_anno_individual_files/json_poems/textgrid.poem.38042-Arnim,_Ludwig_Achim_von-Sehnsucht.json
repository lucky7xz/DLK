ic.tetra"}, "line.6": {"text": "Wird mir zu lang in Schmerzen,", "tokens": ["Wird", "mir", "zu", "lang", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich oft klag,", "tokens": ["Da\u00df", "ich", "oft", "klag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Es scheint kein Tag,", "tokens": ["Es", "scheint", "kein", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Des wird gedacht im Herzen.", "tokens": ["Des", "wird", "ge\u00b7dacht", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Seit ich mich th\u00e4te scheiden,", "tokens": ["Seit", "ich", "mich", "th\u00e4\u00b7te", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von dir mein Schatz und h\u00f6chste Freud,", "tokens": ["Von", "dir", "mein", "Schatz", "und", "h\u00f6chs\u00b7te", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APP