n,", "tokens": ["Bald", "ist", "sie", "auch", "ein", "Schild", ",", "wo\u00b7run\u00b7ter", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die Recensenten Ruthen scheu'n,", "tokens": ["Die", "Re\u00b7cen\u00b7sen\u00b7ten", "Ru\u00b7then", "scheu'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Doch freilich meist vergebens, sich verbergen,", "tokens": ["Doch", "frei\u00b7lich", "meist", "ver\u00b7ge\u00b7bens", ",", "sich", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und bald ist sie ein Monument", "tokens": ["Und", "bald", "ist", "sie", "ein", "Mo\u00b7nu\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.23": {"text": "Der Freundschaft, bald ein leeres Kompliment;", "tokens": ["Der", "Freund\u00b7schaft", ",", "bald", "ein", "lee\u00b7res", "Kom\u00b7pli\u00b7ment", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Von allen den Gestalten hat die meine,", "tokens": ["Von", "al\u00b7len", "den", "Ge\u00b7stal\u00b7ten", "hat", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "VAFIN", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ich sag' es offenherzig, keine.", "tokens": ["Ich", "sag'", "es", "of\u00b7fen\u00b7her\u00b7zig", ",", "kei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Denn, um f\u00fcr dich ein Monument zu sein,", "tokens": ["Denn", ",", "um", "f\u00fcr", "dich", "ein", "Mo\u00b7nu\u00b7ment", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "APPR", "PPER", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ist diese Posse viel zu klein.", "tokens": ["Ist", "die\u00b7se", "Pos\u00b7se", "viel", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sie soll, wenn du zuweilen mit Voltairen", "tokens": ["Sie", "soll", ",", "wenn", "du", "zu\u00b7wei\u00b7len", "mit", "Vol\u00b7tai\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Kandidisirst, den b\u00f6sen Geist beschw\u00f6ren,", "tokens": ["Kan\u00b7di\u00b7di\u00b7sirst", ",", "den", "b\u00f6\u00b7sen", "Geist", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und wenn dann Schwermuth oder Spleen", "tokens": ["Und", "wenn", "dann", "Schwer\u00b7muth", "o\u00b7der", "Spleen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Zum Timon oder Freudenh\u00e4sser", "tokens": ["Zum", "Ti\u00b7mon", "o\u00b7der", "Freu\u00b7den\u00b7h\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Dich machen will, den Mund zum L\u00e4cheln dir verzieh'n,", "tokens": ["Dich", "ma\u00b7chen", "will", ",", "den", "Mund", "zum", "L\u00e4\u00b7cheln", "dir", "ver\u00b7zieh'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und bringt sie's bis zum Lachen \u2013 desto besser!", "tokens": ["Und", "bringt", "sie's", "bis", "zum", "La\u00b7chen", "\u2013", "des\u00b7to", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}