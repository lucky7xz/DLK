{"textgrid.poem.33376": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Joseph Edlen v. Retzer", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt, o Freund, der Dedicationen", "tokens": ["Es", "gibt", ",", "o", "Freund", ",", "der", "De\u00b7di\u00b7ca\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So vielerlei, als der Patronen.", "tokens": ["So", "vie\u00b7ler\u00b7lei", ",", "als", "der", "Pat\u00b7ro\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der weiht sein Buch sich selbst, ein anderer", "tokens": ["Der", "weiht", "sein", "Buch", "sich", "selbst", ",", "ein", "an\u00b7de\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PRF", "ADV", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der losen Zunft der Kritiker,", "tokens": ["Der", "lo\u00b7sen", "Zunft", "der", "Kri\u00b7ti\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.5": {"text": "Der macht das Publikum, und jener", "tokens": ["Der", "macht", "das", "Pub\u00b7li\u00b7kum", ",", "und", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "KON", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Esel gar zu seinem G\u00f6nner,", "tokens": ["Den", "E\u00b7sel", "gar", "zu", "sei\u00b7nem", "G\u00f6n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und einer, den nichts Irdisches mehr freut \u2013", "tokens": ["Und", "ei\u00b7ner", ",", "den", "nichts", "Ir\u00b7di\u00b7sches", "mehr", "freut", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PIS", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die heilige Dreifaltigkeit;", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Drei\u00b7fal\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und hier in dieser Menschlichkeit", "tokens": ["Und", "hier", "in", "die\u00b7ser", "Menschlich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ist wohl kein Rang, kein Stand, dem diese Ehre", "tokens": ["Ist", "wohl", "kein", "Rang", ",", "kein", "Stand", ",", "dem", "die\u00b7se", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nicht l\u00e4ngst schon wiederfahren w\u00e4re.", "tokens": ["Nicht", "l\u00e4ngst", "schon", "wie\u00b7der\u00b7fah\u00b7ren", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "D'rum ist auch eine Dedication", "tokens": ["D'\u00b7rum", "ist", "auch", "ei\u00b7ne", "De\u00b7di\u00b7ca\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Ver\u00e4nderlich, wie ein Cham\u00e4leon.", "tokens": ["Ver\u00b7\u00e4n\u00b7der\u00b7lich", ",", "wie", "ein", "Cha\u00b7m\u00e4\u00b7le\u00b7on", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bald ist sie ein Memorial um eine Pfr\u00fcnde,", "tokens": ["Bald", "ist", "sie", "ein", "Me\u00b7mo\u00b7ri\u00b7al", "um", "ei\u00b7ne", "Pfr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und bald ein Kniff, womit oft ein Poet", "tokens": ["Und", "bald", "ein", "Kniff", ",", "wo\u00b7mit", "oft", "ein", "Po\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Zu einem gr\u00f6sseren Gevatter bitten geht,", "tokens": ["Zu", "ei\u00b7nem", "gr\u00f6s\u00b7se\u00b7ren", "Ge\u00b7vat\u00b7ter", "bit\u00b7ten", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Um seinem namenlosen Kinde", "tokens": ["Um", "sei\u00b7nem", "na\u00b7men\u00b7lo\u00b7sen", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "So was von Namen zu verleih'n;", "tokens": ["So", "was", "von", "Na\u00b7men", "zu", "ver\u00b7leih'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Bald ist sie auch ein Schild, worunter Zwergen,", "tokens": ["Bald", "ist", "sie", "auch", "ein", "Schild", ",", "wo\u00b7run\u00b7ter", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die Recensenten Ruthen scheu'n,", "tokens": ["Die", "Re\u00b7cen\u00b7sen\u00b7ten", "Ru\u00b7then", "scheu'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Doch freilich meist vergebens, sich verbergen,", "tokens": ["Doch", "frei\u00b7lich", "meist", "ver\u00b7ge\u00b7bens", ",", "sich", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und bald ist sie ein Monument", "tokens": ["Und", "bald", "ist", "sie", "ein", "Mo\u00b7nu\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.23": {"text": "Der Freundschaft, bald ein leeres Kompliment;", "tokens": ["Der", "Freund\u00b7schaft", ",", "bald", "ein", "lee\u00b7res", "Kom\u00b7pli\u00b7ment", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Von allen den Gestalten hat die meine,", "tokens": ["Von", "al\u00b7len", "den", "Ge\u00b7stal\u00b7ten", "hat", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "VAFIN", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ich sag' es offenherzig, keine.", "tokens": ["Ich", "sag'", "es", "of\u00b7fen\u00b7her\u00b7zig", ",", "kei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Denn, um f\u00fcr dich ein Monument zu sein,", "tokens": ["Denn", ",", "um", "f\u00fcr", "dich", "ein", "Mo\u00b7nu\u00b7ment", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "APPR", "PPER", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ist diese Posse viel zu klein.", "tokens": ["Ist", "die\u00b7se", "Pos\u00b7se", "viel", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sie soll, wenn du zuweilen mit Voltairen", "tokens": ["Sie", "soll", ",", "wenn", "du", "zu\u00b7wei\u00b7len", "mit", "Vol\u00b7tai\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Kandidisirst, den b\u00f6sen Geist beschw\u00f6ren,", "tokens": ["Kan\u00b7di\u00b7di\u00b7sirst", ",", "den", "b\u00f6\u00b7sen", "Geist", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und wenn dann Schwermuth oder Spleen", "tokens": ["Und", "wenn", "dann", "Schwer\u00b7muth", "o\u00b7der", "Spleen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Zum Timon oder Freudenh\u00e4sser", "tokens": ["Zum", "Ti\u00b7mon", "o\u00b7der", "Freu\u00b7den\u00b7h\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Dich machen will, den Mund zum L\u00e4cheln dir verzieh'n,", "tokens": ["Dich", "ma\u00b7chen", "will", ",", "den", "Mund", "zum", "L\u00e4\u00b7cheln", "dir", "ver\u00b7zieh'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und bringt sie's bis zum Lachen \u2013 desto besser!", "tokens": ["Und", "bringt", "sie's", "bis", "zum", "La\u00b7chen", "\u2013", "des\u00b7to", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es gibt, o Freund, der Dedicationen", "tokens": ["Es", "gibt", ",", "o", "Freund", ",", "der", "De\u00b7di\u00b7ca\u00b7ti\u00b7o\u00b7nen"], "token_info": ["wo