{"dta.poem.18839": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Himmel steigend Jesus bracht\u2019", "tokens": ["Vom", "Him\u00b7mel", "stei\u00b7gend", "Je\u00b7sus", "bracht'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Evangeliums ewige Schrift,", "tokens": ["Des", "E\u00b7van\u00b7ge\u00b7li\u00b7ums", "e\u00b7wi\u00b7ge", "Schrift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "------+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Den J\u00fcngern las er sie Tag und Nacht;", "tokens": ["Den", "J\u00fcn\u00b7gern", "las", "er", "sie", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein g\u00f6ttlich Wort es wirkt und trifft.", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Wort", "es", "wirkt", "und", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er stieg zur\u00fcck, nahm\u2019s wieder mit;", "tokens": ["Er", "stieg", "zu\u00b7r\u00fcck", ",", "nahm's", "wie\u00b7der", "mit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADJA", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie aber hatten\u2019s gut gef\u00fchlt", "tokens": ["Sie", "a\u00b7ber", "hat\u00b7ten's", "gut", "ge\u00b7f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jeder schrieb, so Schritt vor Schritt,", "tokens": ["Und", "je\u00b7der", "schrieb", ",", "so", "Schritt", "vor", "Schritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie ers in seinem Sinn behielt", "tokens": ["Wie", "ers", "in", "sei\u00b7nem", "Sinn", "be\u00b7hielt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verschieden. Es hat nichts zu bedeuten:", "tokens": ["Ver\u00b7schie\u00b7den", ".", "Es", "hat", "nichts", "zu", "be\u00b7deu\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sie hatten nicht gleiche F\u00e4higkeiten;", "tokens": ["Sie", "hat\u00b7ten", "nicht", "glei\u00b7che", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Doch damit k\u00f6nnen sich die Christen", "tokens": ["Doch", "da\u00b7mit", "k\u00f6n\u00b7nen", "sich", "die", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bis zu dem j\u00fcngsten Tage fristen.", "tokens": ["Bis", "zu", "dem", "j\u00fcng\u00b7sten", "Ta\u00b7ge", "fris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}