{"textgrid.poem.53964": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Beschlu\u00df und Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": " am 3. Dezember 1928 j\u00e4hrt sich zum zweiten Mal der Todestag Siegfried Jacobsohns", "tokens": ["am", "3.", "De\u00b7zem\u00b7ber", "19\u00b728", "j\u00e4hrt", "sich", "zum", "zwei\u00b7ten", "Mal", "der", "To\u00b7des\u00b7tag", "Sieg\u00b7fried", "Ja\u00b7cob\u00b7sohns"], "token_info": ["word", "ordinal", "word", "number", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "CARD", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "NE", "NE"], "meter": "-+--+--+-+-+--+-+--", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Bei allem, was ich tu und treibe,", "tokens": ["Bei", "al\u00b7lem", ",", "was", "ich", "tu", "und", "trei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "NE", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "denk ich an eine starke Hand;", "tokens": ["denk", "ich", "an", "ei\u00b7ne", "star\u00b7ke", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die lenkt mich heut noch, wenn ich schreibe,", "tokens": ["die", "lenkt", "mich", "heut", "noch", ",", "wenn", "ich", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ob auch der Freund uns j\u00e4h entschwand.", "tokens": ["ob", "auch", "der", "Freund", "uns", "j\u00e4h", "ent\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Freund \u2013 ich nannt ihn dann und wann:", "tokens": ["Der", "Freund", "\u2013", "ich", "nannt", "ihn", "dann", "und", "wann", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "KON", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "den kleinen Mann.", "tokens": ["den", "klei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Er war uns viel.", "tokens": ["Er", "war", "uns", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der wollt nicht d\u00e4mpfen,", "tokens": ["Der", "wollt", "nicht", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "er packte wuchtig seine Zeit.", "tokens": ["er", "pack\u00b7te", "wuch\u00b7tig", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In L\u00e4rm und Streit und lauten K\u00e4mpfen;", "tokens": ["In", "L\u00e4rm", "und", "Streit", "und", "lau\u00b7ten", "K\u00e4mp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ein Blick \u2013 wir wu\u00dften gleich Bescheid.", "tokens": ["ein", "Blick", "\u2013", "wir", "wu\u00df\u00b7ten", "gleich", "Be\u00b7scheid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00e4mpf ich heut \u2013 wie fehlt mir dann", "tokens": ["Und", "k\u00e4mpf", "ich", "heut", "\u2013", "wie", "fehlt", "mir", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der kleine Mann!", "tokens": ["der", "klei\u00b7ne", "Mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Er hat uns vieles hinterlassen:", "tokens": ["Er", "hat", "uns", "vie\u00b7les", "hin\u00b7ter\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "den Dienst am Werk und Schuld und Pflicht.", "tokens": ["den", "Dienst", "am", "Werk", "und", "Schuld", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will im Lieben und im Hassen", "tokens": ["Ich", "will", "im", "Lie\u00b7ben", "und", "im", "Has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "KON