{"dta.poem.3303": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling wird nun bald entweichen:", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wird", "nun", "bald", "ent\u00b7wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne f\u00e4rbt sein Angesicht:", "tokens": ["Die", "Son\u00b7ne", "f\u00e4rbt", "sein", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schmachtet unter welken Str\u00e4uchen;", "tokens": ["Er", "schmach\u00b7tet", "un\u00b7ter", "wel\u00b7ken", "Str\u00e4u\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und findet seinen Zephyr nicht.", "tokens": ["Und", "fin\u00b7det", "sei\u00b7nen", "Ze\u00b7phyr", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hinterl\u00e4\u00dft uns, da er fliehet,", "tokens": ["Er", "hin\u00b7ter\u00b7l\u00e4\u00dft", "uns", ",", "da", "er", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Ausbund seiner Lieblichkeit.", "tokens": ["Den", "Aus\u00b7bund", "sei\u00b7ner", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rose, die in Purpur bl\u00fchet,", "tokens": ["Die", "Ro\u00b7se", ",", "die", "in", "Pur\u00b7pur", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verherrlicht seine lezte Zeit.", "tokens": ["Ver\u00b7herr\u00b7licht", "sei\u00b7ne", "lez\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du, Rose! sollst mein Haupt umkr\u00e4nzen:", "tokens": ["Du", ",", "Ro\u00b7se", "!", "sollst", "mein", "Haupt", "um\u00b7kr\u00e4n\u00b7zen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$.", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich lieben Venus und ihr Sohn.", "tokens": ["Dich", "lie\u00b7ben", "Ve\u00b7nus", "und", "ihr", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Kaum seh ich dich im Busche gl\u00e4nzen,", "tokens": ["Kaum", "seh", "ich", "dich", "im", "Bu\u00b7sche", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wallt mein Blut, so brenn ich schon.", "tokens": ["So", "wallt", "mein", "Blut", ",", "so", "brenn", "ich", "schon", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchl ein jugendlich Verlangen,", "tokens": ["Ich", "f\u00fchl", "ein", "ju\u00b7gend\u00b7lich", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein bl\u00fchend M\u00e4dchen hier zu sehn,", "tokens": ["Ein", "bl\u00fc\u00b7hend", "M\u00e4d\u00b7chen", "hier", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um dessen rosenvolle Wangen", "tokens": ["Um", "des\u00b7sen", "ro\u00b7sen\u00b7vol\u00b7le", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die jungen Weste s\u00fcsser wehn.", "tokens": ["Die", "jun\u00b7gen", "Wes\u00b7te", "s\u00fcs\u00b7ser", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}