{"textgrid.poem.65591": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "64. Der Ehrenplatz", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem hohen Schneeberg br\u00fcstet sich das Kind mit Stolz und Trutz,", "tokens": ["Auf", "dem", "ho\u00b7hen", "Schnee\u00b7berg", "br\u00fcs\u00b7tet", "sich", "das", "Kind", "mit", "Stolz", "und", "Trutz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch die Sonne schmelzt den Gipfel, und der Knabe liegt im Schmuz.", "tokens": ["Doch", "die", "Son\u00b7ne", "schmelzt", "den", "Gip\u00b7fel", ",", "und", "der", "Kna\u00b7be", "liegt", "im", "Schmuz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eitler Thor, so prangst du thronend auf der Ehre lockerm Stuhl!", "tokens": ["Eit\u00b7ler", "Thor", ",", "so", "prangst", "du", "thro\u00b7nend", "auf", "der", "Eh\u00b7re", "lo\u00b7ckerm", "Stuhl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Deck' ihn vor der Wahrheit Strahlen, oder er versinkt im Pfuhl.", "tokens": ["Deck'", "ihn", "vor", "der", "Wahr\u00