{"textgrid.poem.24681": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Erfahrung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann so gut verstehen die ungetreuen Frauen,", "tokens": ["Ich", "kann", "so", "gut", "ver\u00b7ste\u00b7hen", "die", "un\u00b7ge\u00b7treu\u00b7en", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So gut, mir ist, als k\u00f6nnt ich in ihre Seelen schauen.", "tokens": ["So", "gut", ",", "mir", "ist", ",", "als", "k\u00f6nnt", "ich", "in", "ih\u00b7re", "See\u00b7len", "schau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich seh um ihre Stirnen die stumme Klage schweben,", "tokens": ["Ich", "seh", "um", "ih\u00b7re", "Stir\u00b7nen", "die", "stum\u00b7me", "Kla\u00b7ge", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Qual am langen, leeren, am lebenleeren Leben;", "tokens": ["Die", "Qual", "am", "lan\u00b7gen", ",", "lee\u00b7ren", ",", "am", "le\u00b7ben\u00b7lee\u00b7ren", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "$,", "ADJA", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich seh in ihren Augen die Lust, sich aufzugeben,", "tokens": ["Ich", "seh", "in", "ih\u00b7ren", "Au\u00b7gen", "die", "Lust", ",", "sich", "auf\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "PRF", "VVIZU", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Unergr\u00fcndlichen, Verbotenen zu beben,", "tokens": ["Im", "Un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7chen", ",", "Ver\u00b7bo\u00b7te\u00b7nen", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Lust am Spiel, die Lust, das Letzte einzusetzen,", "tokens": ["Die", "Lust", "am", "Spiel", ",", "die", "Lust", ",", "das", "Letz\u00b7te", "ein\u00b7zu\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Lust am Sieg und Rausch, am Tr\u00fcgen und Verletzen.", "tokens": ["Die", "Lust", "am", "Sieg", "und", "Rausch", ",", "am", "Tr\u00fc\u00b7gen", "und", "Ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "$,", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich seh ihr L\u00e4cheln und die heimlichen, die Tr\u00e4nen,", "tokens": ["Ich", "seh", "ihr", "L\u00e4\u00b7cheln", "und", "die", "heim\u00b7li\u00b7chen", ",", "die", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ART", "ADJA", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das r\u00e4tselhafte Suchen, das ruhelose Sehnen.", "tokens": ["Das", "r\u00e4t\u00b7sel\u00b7haf\u00b7te", "Su\u00b7chen", ",", "das", "ru\u00b7he\u00b7lo\u00b7se", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich f\u00fchle, wie sies dr\u00e4ngt zu t\u00f6richten Entschl\u00fcssen,", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "wie", "sies", "dr\u00e4ngt", "zu", "t\u00f6\u00b7rich\u00b7ten", "Ent\u00b7schl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie sie die Augen schlie\u00dfen, und wie sie qu\u00e4len m\u00fcssen;", "tokens": ["Wie", "sie", "die", "Au\u00b7gen", "schlie\u00b7\u00dfen", ",", "und", "wie", "sie", "qu\u00e4\u00b7len", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$,", "KON", "PWAV", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie sie f\u00fcr jedes Morgen ein jedes Heut begraben,", "tokens": ["Wie", "sie