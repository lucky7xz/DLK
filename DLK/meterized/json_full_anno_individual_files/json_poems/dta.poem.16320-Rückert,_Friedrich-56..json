{"dta.poem.16320": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund im fernen Gau! wie oft noch denk' ich nach", "tokens": ["Mein", "Freund", "im", "fer\u00b7nen", "Gau", "!", "wie", "oft", "noch", "denk'", "ich", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$.", "PWAV", "ADV", "ADV", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Worte, das dein Mund einst unbefangen sprach:", "tokens": ["Dem", "Wor\u00b7te", ",", "das", "dein", "Mund", "einst", "un\u00b7be\u00b7fan\u00b7gen", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df dirs unleidlich sei, im Leben wem zu nahn,", "tokens": ["Da\u00df", "dirs", "un\u00b7leid\u00b7lich", "sei", ",", "im", "Le\u00b7ben", "wem", "zu", "nahn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,", "APPRART", "NN", "PWS", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' ihm zu geben Lieb' und Liebe zu empfahn.", "tokens": ["Ohn'", "ihm", "zu", "ge\u00b7ben", "Lieb'", "und", "Lie\u00b7be", "zu", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sag', hast du warm bis jetzt den Anspruch fortgesetzt?", "tokens": ["Sag'", ",", "hast", "du", "warm", "bis", "jetzt", "den", "An\u00b7spruch", "fort\u00b7ge\u00b7setzt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADJD", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat die kalte Welt gewis dich oft verletzt.", "tokens": ["So", "hat", "die", "kal\u00b7te", "Welt", "ge\u00b7wis", "dich", "oft", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch gl\u00fccklich wenn dir ward zum Stachel dis Verletzen,", "tokens": ["Doch", "gl\u00fcck\u00b7lich", "wenn", "dir", "ward", "zum", "Sta\u00b7chel", "dis", "Ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herzhaft die Forderung des Herzens durchzusetzen.", "tokens": ["Herz\u00b7haft", "die", "For\u00b7de\u00b7rung", "des", "Her\u00b7zens", "durch\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Ja, Liebe l\u00e4\u00dft nicht ruhn den so sie recht durchdrungen,", "tokens": ["Ja", ",", "Lie\u00b7be", "l\u00e4\u00dft", "nicht", "ruhn", "den", "so", "sie", "recht", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VVFIN", "PTKNEG", "VVINF", "ART", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis er von allem was kan lieben Lieb' errungen.", "tokens": ["Bis", "er", "von", "al\u00b7lem", "was", "kan", "lie\u00b7ben", "Lieb'", "er\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "PWS", "VMFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}