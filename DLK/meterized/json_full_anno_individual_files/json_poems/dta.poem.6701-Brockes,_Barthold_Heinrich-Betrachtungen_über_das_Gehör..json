{"dta.poem.6701": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Also ward Soerates, so vieler stoltzen Geister,", "tokens": ["Al\u00b7so", "ward", "Soe\u00b7ra\u00b7tes", ",", "so", "vie\u00b7ler", "stolt\u00b7zen", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$,", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "O kluge Jronie! durch deine St\u00e4rcke, Meister.", "tokens": ["O", "klu\u00b7ge", "Jro\u00b7nie", "!", "durch", "dei\u00b7ne", "St\u00e4r\u00b7cke", ",", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch sanfte T\u00f6ne mehr, als durch ein starck Geschrey", "tokens": ["Durch", "sanf\u00b7te", "T\u00f6\u00b7ne", "mehr", ",", "als", "durch", "ein", "starck", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwang er der w\u00fctigen Sophisten Raserey.", "tokens": ["Zwang", "er", "der", "w\u00fc\u00b7ti\u00b7gen", "So\u00b7phis\u00b7ten", "Ra\u00b7se\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Er lehrte sie ohn alle Leidenschafft", "tokens": ["Er", "lehr\u00b7te", "sie", "ohn", "al\u00b7le", "Lei\u00b7den\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der richtigen Vernunft Gesetze, Recht und Krafft.", "tokens": ["Der", "rich\u00b7ti\u00b7gen", "Ver\u00b7nunft", "Ge\u00b7set\u00b7ze", ",", "Recht", "und", "Krafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}