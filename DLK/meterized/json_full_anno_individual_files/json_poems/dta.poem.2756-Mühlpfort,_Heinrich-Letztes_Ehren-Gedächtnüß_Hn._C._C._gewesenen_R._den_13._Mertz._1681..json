{"dta.poem.2756": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Letztes Ehren-Ged\u00e4chtn\u00fc\u00df/  \n  Hn. C. C. gewesenen R. den 13. Mertz.  1681.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erblaster/ den sein Muth und Degen hat geadelt/", "tokens": ["Er\u00b7blas\u00b7ter", "/", "den", "sein", "Muth", "und", "De\u00b7gen", "hat", "ge\u00b7a\u00b7delt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "PPOSAT", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Pulver riechen kont\u2019 und keinen Feind gescheut/", "tokens": ["Der", "Pul\u00b7ver", "rie\u00b7chen", "kont'", "und", "kei\u00b7nen", "Feind", "ge\u00b7scheut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "KON", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja dessen Tapfferkeit der schele Neid nicht tadelt/", "tokens": ["Ja", "des\u00b7sen", "Tapf\u00b7fer\u00b7keit", "der", "sche\u00b7le", "Neid", "nicht", "ta\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELAT", "NN", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sonst die Tugend doch mit seinem Gifft bespeyt.", "tokens": ["So", "sonst", "die", "Tu\u00b7gend", "doch", "mit", "sei\u00b7nem", "Gifft", "be\u00b7speyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist ein Gewaltiger denn \u00fcber dich jetzt kommen/", "tokens": ["Ist", "ein", "Ge\u00b7wal\u00b7ti\u00b7ger", "denn", "\u00fc\u00b7ber", "dich", "jetzt", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der dir den Harnisch nimmt und gibt dir kein Quartier?", "tokens": ["Der", "dir", "den", "Har\u00b7nisch", "nimmt", "und", "gibt", "dir", "kein", "Quar\u00b7tier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den vormals nie erschreckt hat der Carthaunen Brommen/", "tokens": ["Den", "vor\u00b7mals", "nie", "er\u00b7schreckt", "hat", "der", "Car\u00b7thau\u00b7nen", "Brom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kreuchst du nunmehr zu Loch und gehest nicht herf\u00fcr?", "tokens": ["Kreuchst", "du", "nun\u00b7mehr", "zu", "Loch", "und", "ge\u00b7hest", "nicht", "her\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der du zuvor zu Feld als wie in Tantz gegangen;", "tokens": ["Der", "du", "zu\u00b7vor", "zu", "Feld", "als", "wie", "in", "Tantz", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "KOUS", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem blitzendes Gescho\u00df ein sch\u00f6nes S\u00e4iten-Spiel", "tokens": ["Dem", "blit\u00b7zen\u00b7des", "Ge\u00b7scho\u00df", "ein", "sch\u00f6\u00b7nes", "S\u00e4i\u00b7ten\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und viel sieghaffte Streich\u2019 hast Feinden angehangen", "tokens": ["Und", "viel", "sieg\u00b7haff\u00b7te", "Streich'", "hast", "Fein\u00b7den", "an\u00b7ge\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wirst doch zu aller Letzt de\u00df Todes Raub und Ziel?", "tokens": ["Wirst", "doch", "zu", "al\u00b7ler", "Letzt", "de\u00df", "To\u00b7des", "Raub", "und", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er scheut sich nicht f\u00fcr dir noch deinen grauen Haaren;", "tokens": ["Er", "scheut", "sich", "nicht", "f\u00fcr", "dir", "noch", "dei\u00b7nen", "grau\u00b7en", "Haa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie du die tapffre Faust offt nach dem Schwerdt gestreckt/", "tokens": ["Wie", "du", "die", "tapf\u00b7fre", "Faust", "offt", "nach", "dem", "Schwerdt", "ge\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So will er jetzt nach dir und deinen Knochen fahren/", "tokens": ["So", "will", "er", "jetzt", "nach", "dir", "und", "dei\u00b7nen", "Kno\u00b7chen", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Damit er seinen Sieg mit k\u00fchlem Sande deckt.", "tokens": ["Da\u00b7mit", "er", "sei\u00b7nen", "Sieg", "mit", "k\u00fch\u00b7lem", "San\u00b7de", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was aber deckt er zu? die welcke Haut und Glieder/", "tokens": ["Was", "a\u00b7ber", "deckt", "er", "zu", "?", "die", "wel\u00b7cke", "Haut", "und", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der m\u00fcden Jahre Schnee/ die ausgekreischten Bein\u2019", "tokens": ["Der", "m\u00fc\u00b7den", "Jah\u00b7re", "Schnee", "/", "die", "aus\u00b7ge\u00b7kreischten", "Bein'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Und Adern ohne Safft und finstern Augen-Lieder/", "tokens": ["Und", "A\u00b7dern", "oh\u00b7ne", "Safft", "und", "fins\u00b7tern", "Au\u00b7gen\u00b7Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und was Verwe\u00dfliches sonst kan an Menschen seyn.", "tokens": ["Und", "was", "Ver\u00b7we\u00df\u00b7li\u00b7ches", "sonst", "kan", "an", "Men\u00b7schen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "ADV", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So viel zwar frisst das Grab. Allein das Licht der Ehren", "tokens": ["So", "viel", "zwar", "frisst", "das", "Grab", ".", "Al\u00b7lein", "das", "Licht", "der", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "$.", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "F\u00fchrt seine Flamme noch in unbeflecktem Glantz.", "tokens": ["F\u00fchrt", "sei\u00b7ne", "Flam\u00b7me", "noch", "in", "un\u00b7be\u00b7fleck\u00b7tem", "Glantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die kluge Nachwelt wird von deinem Lob noch h\u00f6ren/", "tokens": ["Die", "klu\u00b7ge", "Nach\u00b7welt", "wird", "von", "dei\u00b7nem", "Lob", "noch", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und flicht zu Danck und Ruhm dir einen Sieges-Krantz.", "tokens": ["Und", "flicht", "zu", "Danck", "und", "Ruhm", "dir", "ei\u00b7nen", "Sie\u00b7ges\u00b7Krantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du hast mit Muth und Blut den hohen Prei\u00df erhalten/", "tokens": ["Du", "hast", "mit", "Muth", "und", "Blut", "den", "ho\u00b7hen", "Prei\u00df", "er\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Als unser Vaterland in Krieges-Flammen glam/", "tokens": ["Als", "un\u00b7ser", "Va\u00b7ter\u00b7land", "in", "Krie\u00b7ge\u00b7sFlam\u00b7men", "glam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da Degen und Pistol mehr als Gesetze galten/", "tokens": ["Da", "De\u00b7gen", "und", "Pis\u00b7tol", "mehr", "als", "Ge\u00b7set\u00b7ze", "gal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "KOUS", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Und das bedr\u00e4ngte Land in eignem Blute schwam/", "tokens": ["Und", "das", "be\u00b7dr\u00e4ng\u00b7te", "Land", "in", "eig\u00b7nem", "Blu\u00b7te", "schwam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Beschlo\u00df dein munter Geist dem rauhen Sturm der Zeiten/", "tokens": ["Be\u00b7schlo\u00df", "dein", "mun\u00b7ter", "Geist", "dem", "rau\u00b7hen", "Sturm", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der offenen Gefahr entgegen frey zu gehn;", "tokens": ["Der", "of\u00b7fe\u00b7nen", "Ge\u00b7fahr", "ent\u00b7ge\u00b7gen", "frey", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPO", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df ein unsterblich Lob durch k\u00e4mpffen und durch streiten", "tokens": ["Da\u00df", "ein", "uns\u00b7terb\u00b7lich", "Lob", "durch", "k\u00e4mpf\u00b7fen", "und", "durch", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "NN", "APPR", "VVINF", "KON", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dich hiesse dermaleins bey tapffern Leuten stehn.", "tokens": ["Dich", "hies\u00b7se", "der\u00b7ma\u00b7leins", "bey", "tapf\u00b7fern", "Leu\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Eltern Helm und Schild war gar nicht deine Zierde/", "tokens": ["Der", "El\u00b7tern", "Helm", "und", "Schild", "war", "gar", "nicht", "dei\u00b7ne", "Zier\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein Leib hie\u00df dein Palast/ dein Sinn dein K\u00f6nigreich,", "tokens": ["Dein", "Leib", "hie\u00df", "dein", "Pa\u00b7last", "/", "dein", "Sinn", "dein", "K\u00f6\u00b7nig\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dein eintzig Adel-Stand die brennende Begierde/", "tokens": ["Dein", "eint\u00b7zig", "A\u00b7del\u00b7Stand", "die", "bren\u00b7nen\u00b7de", "Be\u00b7gier\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Durch wackre Thaten sich zu machen andern gleich.", "tokens": ["Durch", "wack\u00b7re", "Tha\u00b7ten", "sich", "zu", "ma\u00b7chen", "an\u00b7dern", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du hast vom Niedrigsten zu steigen angehoben/", "tokens": ["Du", "hast", "vom", "Nied\u00b7rigs\u00b7ten", "zu", "stei\u00b7gen", "an\u00b7ge\u00b7ho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Durch Hitze Frost und Schnee den k\u00fchnen Fu\u00df gesetzt.", "tokens": ["Durch", "Hit\u00b7ze", "Frost", "und", "Schnee", "den", "k\u00fch\u00b7nen", "Fu\u00df", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "In vielen F\u00e4llen auch erwiesen solche Proben/", "tokens": ["In", "vie\u00b7len", "F\u00e4l\u00b7len", "auch", "er\u00b7wie\u00b7sen", "sol\u00b7che", "Pro\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df das Gel\u00fccke sich fast an dir matt gewetzt.", "tokens": ["Da\u00df", "das", "Ge\u00b7l\u00fc\u00b7cke", "sich", "fast", "an", "dir", "matt", "ge\u00b7wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Offt hat es dich gebraucht nicht anders wie Ballonen/", "tokens": ["Offt", "hat", "es", "dich", "ge\u00b7braucht", "nicht", "an\u00b7ders", "wie", "Bal\u00b7lo\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVFIN", "PTKNEG", "ADV", "KOKOM", "NN", "$("], "meter": "-+-+