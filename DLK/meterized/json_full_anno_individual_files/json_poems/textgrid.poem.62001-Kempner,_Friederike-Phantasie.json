{"textgrid.poem.62001": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Phantasie", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sanften, blauen L\u00fcfte, sie fl\u00fcsterten mich ein,", "tokens": ["Die", "sanf\u00b7ten", ",", "blau\u00b7en", "L\u00fcf\u00b7te", ",", "sie", "fl\u00fcs\u00b7ter\u00b7ten", "mich", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir tr\u00e4umte, ich sei auf Erden, ganz mutterseelenallein,", "tokens": ["Mir", "tr\u00e4um\u00b7te", ",", "ich", "sei", "auf", "Er\u00b7den", ",", "ganz", "mut\u00b7ter\u00b7see\u00b7len\u00b7al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es war so bunt und \u00fcppig, es war so frisch und gr\u00fcn,", "tokens": ["Es", "war", "so", "bunt", "und", "\u00fcp\u00b7pig", ",", "es", "war", "so", "frisch", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich sah zum ersten Male purpurne Rosen bl\u00fch'n!", "tokens": ["Ich", "sah", "zum", "ers\u00b7ten", "Ma\u00b7le", "pur\u00b7pur\u00b7ne", "Ro\u00b7sen", "bl\u00fch'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein buntes Heer von Bl\u00fcten umgab das Rasenland,", "tokens": ["Ein", "bun\u00b7tes", "Heer", "von", "Bl\u00fc\u00b7ten", "um\u00b7gab", "das", "Ra\u00b7sen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vergi\u00dfmeinnicht und Epheu sich um die Felsen wand,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "und", "E\u00b7pheu", "sich", "um", "die", "Fel\u00b7sen", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und tief aus hohem Grase, da schauten lieblich scheu", "tokens": ["Und", "tief", "aus", "ho\u00b7hem", "Gra\u00b7se", ",", "da", "schau\u00b7ten", "lieb\u00b7lich", "scheu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hervor die blauen Veilchen, so sch\u00fcchtern und so treu.", "tokens": ["Her\u00b7vor", "die", "blau\u00b7en", "Veil\u00b7chen", ",", "so", "sch\u00fcch\u00b7tern", "und", "so", "treu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADV", "VVINF", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Es wiegten in den Wipfeln der hohen Palmen sich", "tokens": ["Es", "wieg\u00b7ten", "in", "den", "Wip\u00b7feln", "der", "ho\u00b7hen", "Pal\u00b7men", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die sch\u00f6nsten Papageien und gr\u00fc\u00dften jubelnd mich:", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Pa\u00b7pa\u00b7gei\u00b7en", "und", "gr\u00fc\u00df\u00b7ten", "ju\u00b7belnd", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mein Herz schlug laut und lauter, doch ich vernahm es nicht,", "tokens": ["Mein", "Herz", "schlug", "laut", "und", "lau\u00b7ter", ",", "doch", "ich", "ver\u00b7nahm", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn voller Freud' und Staunen sach ich ins Sonnenlicht!", "tokens": ["Denn", "vol\u00b7ler", "Freud'", "und", "Stau\u00b7nen", "sach", "ich", "ins", "Son\u00b7nen\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Sah ich zur Erde nieder, zu allen Blumen hin,", "tokens": ["Sah", "ich", "zur", "Er\u00b7de", "nie\u00b7der", ",", "zu", "al\u00b7len", "Blu\u00b7men", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und f\u00fchlte wonnetrunken, da\u00df ich so selig bin.", "tokens": ["Und", "f\u00fchl\u00b7te", "won\u00b7ne\u00b7trun\u00b7ken", ",", "da\u00df", "ich", "so", "se\u00b7lig", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die frischen, jungen Rosen, die Lilien wei\u00df und schlank,", "tokens": ["Die", "fri\u00b7schen", ",", "jun\u00b7gen", "Ro\u00b7sen", ",", "die", "Li\u00b7li\u00b7en", "wei\u00df", "und", "schlank", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die tausend kleinen Bl\u00fcten, und all der V\u00f6gel Sang;", "tokens": ["Die", "tau\u00b7send", "klei\u00b7nen", "Bl\u00fc\u00b7ten", ",", "und", "all", "der", "V\u00f6\u00b7gel", "Sang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "$,", "KON", "PIAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie schienen mir zu sagen, sie h\u00e4tten auch ein Herz,", "tokens": ["Sie", "schie\u00b7nen", "mir", "zu", "sa\u00b7gen", ",", "sie", "h\u00e4t\u00b7ten", "auch", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie wollten mit mir f\u00fchlen und teilen Freud' und Schmerz!", "tokens": ["Sie", "woll\u00b7ten", "mit", "mir", "f\u00fch\u00b7len", "und", "tei\u00b7len", "Freud'", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zwei Nachtigallen riefen einander liebend zu,", "tokens": ["Zwei", "Nach\u00b7ti\u00b7gal\u00b7len", "rie\u00b7fen", "ein\u00b7an\u00b7der", "lie\u00b7bend", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und dem Gesange folgte harmonisch tiefe Ruh'!", "tokens": ["Und", "dem", "Ge\u00b7san\u00b7ge", "folg\u00b7te", "har\u00b7mo\u00b7nisch", "tie\u00b7fe", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich sah die Sonne scheiden mit tr\u00fcbem Angesicht,", "tokens": ["Ich", "sah", "die", "Son\u00b7ne", "schei\u00b7den", "mit", "tr\u00fc\u00b7bem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich wu\u00dft' nicht, was es w\u00e4re, und sah ins Mondenlicht,", "tokens": ["Ich", "wu\u00dft'", "nicht", ",", "was", "es", "w\u00e4\u00b7re", ",", "und", "sah", "ins", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Schmetterlinge flogen zu Myrtenb\u00fcschen auf,", "tokens": ["Die", "Schmet\u00b7ter\u00b7lin\u00b7ge", "flo\u00b7gen", "zu", "Myr\u00b7ten\u00b7b\u00fc\u00b7schen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich blickte immer h\u00f6her und sah der Sterne Lauf.", "tokens": ["Ich", "blick\u00b7te", "im\u00b7mer", "h\u00f6\u00b7her", "und", "sah", "der", "Ster\u00b7ne", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Verwundert und erhoben, schaut' endlich ich ins Herz,", "tokens": ["Ver\u00b7wun\u00b7dert", "und", "er\u00b7ho\u00b7ben", ",", "schaut'", "end\u00b7lich", "ich", "ins", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und f\u00fchlte drin vereinigt die Sehnsucht, Lieb und Schmerz,", "tokens": ["Und", "f\u00fchl\u00b7te", "drin", "ver\u00b7ei\u00b7nigt", "die", "Sehn\u00b7sucht", ",", "Lieb", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da fragt' ich mich ganz ernsthaft, wer schuf dies Sch\u00f6ne, sprich,", "tokens": ["Da", "fragt'", "ich", "mich", "ganz", "ernst\u00b7haft", ",", "wer", "schuf", "dies", "Sch\u00f6\u00b7ne", ",", "sprich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "PWS", "VVFIN", "PDS", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sprich, Seele, Herz, o sage: erschufst Du selber Dich?", "tokens": ["Sprich", ",", "See\u00b7le", ",", "Herz", ",", "o", "sa\u00b7ge", ":", "er\u00b7schufst", "Du", "sel\u00b7ber", "Dich", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "NN", "$,", "FM", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Da rauscht es in den Palmen, mich fa\u00dft ein selig Weh!", "tokens": ["Da", "rauscht", "es", "in", "den", "Pal\u00b7men", ",", "mich", "fa\u00dft", "ein", "se\u00b7lig", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer schuf mich und was bin ich, wer schuf das, was ich seh'?", "tokens": ["Wer", "schuf", "mich", "und", "was", "bin", "ich", ",", "wer", "schuf", "das", ",", "was", "ich", "seh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KON", "PWS", "VAFIN", "PPER", "$,", "PWS", "VVFIN", "PDS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mein Auge hatte Tr\u00e4nen, vernehmlich rief's in mir:", "tokens": ["Mein", "Au\u00b7ge", "hat\u00b7te", "Tr\u00e4\u00b7nen", ",", "ver\u00b7nehm\u00b7lich", "rie\u00b7f's", "in", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "ADJD", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er schuf aus Liebe alles, Er schuf das Herz in Dir!", "tokens": ["Er", "schuf", "aus", "Lie\u00b7be", "al\u00b7les", ",", "Er", "schuf", "das", "Herz", "in", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PIS", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Gerecht ist Er und weise, die Gr\u00f6\u00dfe ist nur Er,", "tokens": ["Ge\u00b7recht", "ist", "Er", "und", "wei\u00b7se", ",", "die", "Gr\u00f6\u00b7\u00dfe", "ist", "nur", "Er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und heilig ist sein Name, er lautet Ewiger!", "tokens": ["Und", "hei\u00b7lig", "ist", "sein", "Na\u00b7me", ",", "er", "lau\u00b7tet", "E\u00b7wi\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erhebe Dich, erkenne, wie er unendlich gut,", "tokens": ["Er\u00b7he\u00b7be", "Dich", ",", "er\u00b7ken\u00b7ne", ",", "wie", "er", "un\u00b7end\u00b7lich", "gut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch mehr kannst Du nicht wissen, Du klebst an Fleisch und Blut", "tokens": ["Doch", "mehr", "kannst", "Du", "nicht", "wis\u00b7sen", ",", "Du", "klebst", "an", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.10": {"line.1": {"text": "Du kannst das nie ergr\u00fcnden, was unergr\u00fcndlich ist,", "tokens": ["Du", "kannst", "das", "nie", "er\u00b7gr\u00fcn\u00b7den", ",", "was", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADV", "VVINF", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So wie Du nicht ergr\u00fcndest, wie tief die Tiefe ist. \u2013", "tokens": ["So", "wie", "Du", "nicht", "er\u00b7gr\u00fcn\u00b7dest", ",", "wie", "tief", "die", "Tie\u00b7fe", "ist", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PTKNEG", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur leise wehten Zweige durch blaue Fr\u00fchlingsluft,", "tokens": ["Nur", "lei\u00b7se", "weh\u00b7ten", "Zwei\u00b7ge", "durch", "blau\u00b7e", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des Mondes bleiche Helle schien in die Felsenkluft.", "tokens": ["Des", "Mon\u00b7des", "blei\u00b7che", "Hel\u00b7le", "schien", "in", "die", "Fel\u00b7sen\u00b7kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Ich war bewegt und setzte mich an des Meeres Strand,", "tokens": ["Ich", "war", "be\u00b7wegt", "und", "setz\u00b7te", "mich", "an", "des", "Mee\u00b7res", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sah' in die H\u00f6h' und Tiefe, sah in der Wellen Brand;", "tokens": ["Sah'", "in", "die", "H\u00f6h'", "und", "Tie\u00b7fe", ",", "sah", "in", "der", "Wel\u00b7len", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ger\u00fchrt und dankergl\u00fchet, rief ich: \u00bbO Allmacht mein,", "tokens": ["Ge\u00b7r\u00fchrt", "und", "dan\u00b7ker\u00b7gl\u00fc\u00b7het", ",", "rief", "ich", ":", "\u00bb", "O", "All\u00b7macht", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "VVFIN", "PPER", "$.", "$(", "NE", "NN", "PPOSAT", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Gnade und Erbarmen und Liebe, sie sind Dein!\u00ab", "tokens": ["Die", "Gna\u00b7de", "und", "Er\u00b7bar\u00b7men", "und", "Lie\u00b7be", ",", "sie", "sind", "Dein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Ich weinte Freudentr\u00e4nen, schon schien das D\u00e4mmerlicht,", "tokens": ["Ich", "wein\u00b7te", "Freu\u00b7den\u00b7tr\u00e4\u00b7nen", ",", "schon", "schien", "das", "D\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Tau sank auf die Palmen, wie auf's Vergi\u00dfmeinnicht,", "tokens": ["Der", "Tau", "sank", "auf", "die", "Pal\u00b7men", ",", "wie", "auf's", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da kam der Morgen wieder, vor\u00fcber war die Nacht,", "tokens": ["Da", "kam", "der", "Mor\u00b7gen", "wie\u00b7der", ",", "vor\u00b7\u00fc\u00b7ber", "war", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mich d\u00fcnkt, als wenn ich schliefe, \u2013 ich war erst aufgewacht!", "tokens": ["Mich", "d\u00fcnkt", ",", "als", "wenn", "ich", "schlie\u00b7fe", ",", "\u2013", "ich", "war", "erst", "auf\u00b7ge\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Die sanften, blauen L\u00fcfte, sie fl\u00fcsterten mich ein,", "tokens": ["Die", "sanf\u00b7ten", ",", "blau\u00b7en", "L\u00fcf\u00b7te", ",", "sie", "fl\u00fcs\u00b7ter\u00b7ten", "mich", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir tr\u00e4umte, ich sei auf Erden, ganz mutterseelenallein,", "tokens": ["Mir", "tr\u00e4um\u00b7te", ",", "ich", "sei", "auf", "Er\u00b7den", ",", "ganz", "mut\u00b7ter\u00b7see\u00b7len\u00b7al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es war so bunt und \u00fcppig, es war so frisch und gr\u00fcn,", "tokens": ["Es", "war", "so", "bunt", "und", "\u00fcp\u00b7pig", ",", "es", "war", "so", "frisch", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich sah zum ersten Male purpurne Rosen bl\u00fch'n!", "tokens": ["Ich", "sah", "zum", "ers\u00b7ten", "Ma\u00b7le", "pur\u00b7pur\u00b7ne", "Ro\u00b7sen", "bl\u00fch'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Ein buntes Heer von Bl\u00fcten umgab das Rasenland,", "tokens": ["Ein", "bun\u00b7tes", "Heer", "von", "Bl\u00fc\u00b7ten", "um\u00b7gab", "das", "Ra\u00b7sen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vergi\u00dfmeinnicht und Epheu sich um die Felsen wand,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "und", "E\u00b7pheu", "sich", "um", "die", "Fel\u00b7sen", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und tief aus hohem Grase, da schauten lieblich scheu", "tokens": ["Und", "tief", "aus", "ho\u00b7hem", "Gra\u00b7se", ",", "da", "schau\u00b7ten", "lieb\u00b7lich", "scheu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hervor die blauen Veilchen, so sch\u00fcchtern und so treu.", "tokens": ["Her\u00b7vor", "die", "blau\u00b7en", "Veil\u00b7chen", ",", "so", "sch\u00fcch\u00b7tern", "und", "so", "treu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADV", "VVINF", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Es wiegten in den Wipfeln der hohen Palmen sich", "tokens": ["Es", "wieg\u00b7ten", "in", "den", "Wip\u00b7feln", "der", "ho\u00b7hen", "Pal\u00b7men", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die sch\u00f6nsten Papageien und gr\u00fc\u00dften jubelnd mich:", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "Pa\u00b7pa\u00b7gei\u00b7en", "und", "gr\u00fc\u00df\u00b7ten", "ju\u00b7belnd", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mein Herz schlug laut und lauter, doch ich vernahm es nicht,", "tokens": ["Mein", "Herz", "schlug", "laut", "und", "lau\u00b7ter", ",", "doch", "ich", "ver\u00b7nahm", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn voller Freud' und Staunen sach ich ins Sonnenlicht!", "tokens": ["Denn", "vol\u00b7ler", "Freud'", "und", "Stau\u00b7nen", "sach", "ich", "ins", "Son\u00b7nen\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Sah ich zur Erde nieder, zu allen Blumen hin,", "tokens": ["Sah", "ich", "zur", "Er\u00b7de", "nie\u00b7der", ",", "zu", "al\u00b7len", "Blu\u00b7men", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und f\u00fchlte wonnetrunken, da\u00df ich so selig bin.", "tokens": ["Und", "f\u00fchl\u00b7te", "won\u00b7ne\u00b7trun\u00b7ken", ",", "da\u00df", "ich", "so", "se\u00b7lig", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die frischen, jungen Rosen, die Lilien wei\u00df und schlank,", "tokens": ["Die", "fri\u00b7schen", ",", "jun\u00b7gen", "Ro\u00b7sen", ",", "die", "Li\u00b7li\u00b7en", "wei\u00df", "und", "schlank", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die tausend kleinen Bl\u00fcten, und all der V\u00f6gel Sang;", "tokens": ["Die", "tau\u00b7send", "klei\u00b7nen", "Bl\u00fc\u00b7ten", ",", "und", "all", "der", "V\u00f6\u00b7gel", "Sang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "$,", "KON", "PIAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Sie schienen mir zu sagen, sie h\u00e4tten auch ein Herz,", "tokens": ["Sie", "schie\u00b7nen", "mir", "zu", "sa\u00b7gen", ",", "sie", "h\u00e4t\u00b7ten", "auch", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie wollten mit mir f\u00fchlen und teilen Freud' und Schmerz!", "tokens": ["Sie", "woll\u00b7ten", "mit", "mir", "f\u00fch\u00b7len", "und", "tei\u00b7len", "Freud'", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zwei Nachtigallen riefen einander liebend zu,", "tokens": ["Zwei", "Nach\u00b7ti\u00b7gal\u00b7len", "rie\u00b7fen", "ein\u00b7an\u00b7der", "lie\u00b7bend", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und dem Gesange folgte harmonisch tiefe Ruh'!", "tokens": ["Und", "dem", "Ge\u00b7san\u00b7ge", "folg\u00b7te", "har\u00b7mo\u00b7nisch", "tie\u00b7fe", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "Ich sah die Sonne scheiden mit tr\u00fcbem Angesicht,", "tokens": ["Ich", "sah", "die", "Son\u00b7ne", "schei\u00b7den", "mit", "tr\u00fc\u00b7bem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich wu\u00dft' nicht, was es w\u00e4re, und sah ins Mondenlicht,", "tokens": ["Ich", "wu\u00dft'", "nicht", ",", "was", "es", "w\u00e4\u00b7re", ",", "und", "sah", "ins", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Schmetterlinge flogen zu Myrtenb\u00fcschen auf,", "tokens": ["Die", "Schmet\u00b7ter\u00b7lin\u00b7ge", "flo\u00b7gen", "zu", "Myr\u00b7ten\u00b7b\u00fc\u00b7schen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich blickte immer h\u00f6her und sah der Sterne Lauf.", "tokens": ["Ich", "blick\u00b7te", "im\u00b7mer", "h\u00f6\u00b7her", "und", "sah", "der", "Ster\u00b7ne", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "Verwundert und erhoben, schaut' endlich ich ins Herz,", "tokens": ["Ver\u00b7wun\u00b7dert", "und", "er\u00b7ho\u00b7ben", ",", "schaut'", "end\u00b7lich", "ich", "ins", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und f\u00fchlte drin vereinigt die Sehnsucht, Lieb und Schmerz,", "tokens": ["Und", "f\u00fchl\u00b7te", "drin", "ver\u00b7ei\u00b7nigt", "die", "Sehn\u00b7sucht", ",", "Lieb", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da fragt' ich mich ganz ernsthaft, wer schuf dies Sch\u00f6ne, sprich,", "tokens": ["Da", "fragt'", "ich", "mich", "ganz", "ernst\u00b7haft", ",", "wer", "schuf", "dies", "Sch\u00f6\u00b7ne", ",", "sprich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "PWS", "VVFIN", "PDS", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sprich, Seele, Herz, o sage: erschufst Du selber Dich?", "tokens": ["Sprich", ",", "See\u00b7le", ",", "Herz", ",", "o", "sa\u00b7ge", ":", "er\u00b7schufst", "Du", "sel\u00b7ber", "Dich", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "NN", "$,", "FM", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.20": {"line.1": {"text": "Da rauscht es in den Palmen, mich fa\u00dft ein selig Weh!", "tokens": ["Da", "rauscht", "es", "in", "den", "Pal\u00b7men", ",", "mich", "fa\u00dft", "ein", "se\u00b7lig", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer schuf mich und was bin ich, wer schuf das, was ich seh'?", "tokens": ["Wer", "schuf", "mich", "und", "was", "bin", "ich", ",", "wer", "schuf", "das", ",", "was", "ich", "seh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KON", "PWS", "VAFIN", "PPER", "$,", "PWS", "VVFIN", "PDS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mein Auge hatte Tr\u00e4nen, vernehmlich rief's in mir:", "tokens": ["Mein", "Au\u00b7ge", "hat\u00b7te", "Tr\u00e4\u00b7nen", ",", "ver\u00b7nehm\u00b7lich", "rie\u00b7f's", "in", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "ADJD", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er schuf aus Liebe alles, Er schuf das Herz in Dir!", "tokens": ["Er", "schuf", "aus", "Lie\u00b7be", "al\u00b7les", ",", "Er", "schuf", "das", "Herz", "in", "Dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PIS", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.21": {"line.1": {"text": "Gerecht ist Er und weise, die Gr\u00f6\u00dfe ist nur Er,", "tokens": ["Ge\u00b7recht", "ist", "Er", "und", "wei\u00b7se", ",", "die", "Gr\u00f6\u00b7\u00dfe", "ist", "nur", "Er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und heilig ist sein Name, er lautet Ewiger!", "tokens": ["Und", "hei\u00b7lig", "ist", "sein", "Na\u00b7me", ",", "er", "lau\u00b7tet", "E\u00b7wi\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erhebe Dich, erkenne, wie er unendlich gut,", "tokens": ["Er\u00b7he\u00b7be", "Dich", ",", "er\u00b7ken\u00b7ne", ",", "wie", "er", "un\u00b7end\u00b7lich", "gut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch mehr kannst Du nicht wissen, Du klebst an Fleisch und Blut", "tokens": ["Doch", "mehr", "kannst", "Du", "nicht", "wis\u00b7sen", ",", "Du", "klebst", "an", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.22": {"line.1": {"text": "Du kannst das nie ergr\u00fcnden, was unergr\u00fcndlich ist,", "tokens": ["Du", "kannst", "das", "nie", "er\u00b7gr\u00fcn\u00b7den", ",", "was", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADV", "VVINF", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So wie Du nicht ergr\u00fcndest, wie tief die Tiefe ist. \u2013", "tokens": ["So", "wie", "Du", "nicht", "er\u00b7gr\u00fcn\u00b7dest", ",", "wie", "tief", "die", "Tie\u00b7fe", "ist", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PTKNEG", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur leise wehten Zweige durch blaue Fr\u00fchlingsluft,", "tokens": ["Nur", "lei\u00b7se", "weh\u00b7ten", "Zwei\u00b7ge", "durch", "blau\u00b7e", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des Mondes bleiche Helle schien in die Felsenkluft.", "tokens": ["Des", "Mon\u00b7des", "blei\u00b7che", "Hel\u00b7le", "schien", "in", "die", "Fel\u00b7sen\u00b7kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.23": {"line.1": {"text": "Ich war bewegt und setzte mich an des Meeres Strand,", "tokens": ["Ich", "war", "be\u00b7wegt", "und", "setz\u00b7te", "mich", "an", "des", "Mee\u00b7res", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sah' in die H\u00f6h' und Tiefe, sah in der Wellen Brand;", "tokens": ["Sah'", "in", "die", "H\u00f6h'", "und", "Tie\u00b7fe", ",", "sah", "in", "der", "Wel\u00b7len", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ger\u00fchrt und dankergl\u00fchet, rief ich: \u00bbO Allmacht mein,", "tokens": ["Ge\u00b7r\u00fchrt", "und", "dan\u00b7ker\u00b7gl\u00fc\u00b7het", ",", "rief", "ich", ":", "\u00bb", "O", "All\u00b7macht", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "VVFIN", "PPER", "$.", "$(", "NE", "NN", "PPOSAT", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Gnade und Erbarmen und Liebe, sie sind Dein!\u00ab", "tokens": ["Die", "Gna\u00b7de", "und", "Er\u00b7bar\u00b7men", "und", "Lie\u00b7be", ",", "sie", "sind", "Dein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.24": {"line.1": {"text": "Ich weinte Freudentr\u00e4nen, schon schien das D\u00e4mmerlicht,", "tokens": ["Ich", "wein\u00b7te", "Freu\u00b7den\u00b7tr\u00e4\u00b7nen", ",", "schon", "schien", "das", "D\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Tau sank auf die Palmen, wie auf's Vergi\u00dfmeinnicht,", "tokens": ["Der", "Tau", "sank", "auf", "die", "Pal\u00b7men", ",", "wie", "auf's", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da kam der Morgen wieder, vor\u00fcber war die Nacht,", "tokens": ["Da", "kam", "der", "Mor\u00b7gen", "wie\u00b7der", ",", "vor\u00b7\u00fc\u00b7ber", "war", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mich d\u00fcnkt, als wenn ich schliefe, \u2013 ich war erst aufgewacht!", "tokens": ["Mich", "d\u00fcnkt", ",", "als", "wenn", "ich", "schlie\u00b7fe", ",", "\u2013", "ich", "war", "erst", "auf\u00b7ge\u00b7wacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}