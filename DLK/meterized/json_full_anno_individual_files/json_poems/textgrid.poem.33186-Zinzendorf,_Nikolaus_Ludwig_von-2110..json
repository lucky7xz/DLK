{"textgrid.poem.33186": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2110.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem volk, de\u00df sein Gott der Herr ist!", "tokens": ["Wohl", "dem", "volk", ",", "de\u00df", "sein", "Gott", "der", "Herr", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn man sich umsieht etc.", "tokens": ["Wenn", "man", "sich", "um\u00b7sieht", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PIS", "PRF", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und dazu haben wir uns verbunden.", "tokens": ["Und", "da\u00b7zu", "ha\u00b7ben", "wir", "uns", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Das s\u00fcnder-kirchlein gl\u00e4ubet, etc.", "tokens": ["Das", "s\u00fcn\u00b7der\u00b7kirch\u00b7lein", "gl\u00e4u\u00b7bet", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "punct", "abbreviation"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ists m\u00f6glich, herz der seele! etc.", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "herz", "der", "see\u00b7le", "!", "et\u00b7c."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "abbreviation"], "pos": ["ADV", "ADJD", "$,", "NN", "ART", "NN", "$.", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du vierfaches n\u00e4gel-loch beyd in hand und fusse, und du seite! habet doch dank f\u00fcr eure busse.", "tokens": ["Du", "vier\u00b7fa\u00b7ches", "n\u00e4\u00b7gel\u00b7loch", "beyd", "in", "hand", "und", "fus\u00b7se", ",", "und", "du", "sei\u00b7te", "!", "ha\u00b7bet", "doch", "dank", "f\u00fcr", "eu\u00b7re", "bus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "PIS", "APPR", "NN", "KON", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$.", "VAFIN", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-++-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Herr Christ! dein tod, die maal so roth, die f\u00fchrens heer dort dr\u00fcben \u00fcberm meer, etc.", "tokens": ["Herr", "Christ", "!", "dein", "tod", ",", "die", "maal", "so", "roth", ",", "die", "f\u00fch\u00b7rens", "heer", "dort", "dr\u00fc\u00b7ben", "\u00fc\u00b7berm", "meer", ",", "et\u00b7c."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["NN", "NE", "$.", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,", "PRELS", "ADV", "ADJD", "ADV", "ADV", "APPRART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Dem Lamm ist nichts zu schlecht, etc.", "tokens": ["Dem", "Lamm", "ist", "nichts", "zu", "schlecht", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["ART", "NN", "VAFIN", "PIS", "PTKZU", "VVFIN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O k\u00f6nt m