{"textgrid.poem.66806": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Mitternacht ging l\u00e4ngst vorbei \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mitternacht ging l\u00e4ngst vorbei \u2013", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "ging", "l\u00e4ngst", "vor\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich nur fr\u00fch genug erwache", "tokens": ["Wenn", "ich", "nur", "fr\u00fch", "ge\u00b7nug", "er\u00b7wa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meinen Aufsatz fertig mache:", "tokens": ["Und", "mei\u00b7nen", "Auf\u00b7satz", "fer\u00b7tig", "ma\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwie Cicero zu preisen sei!\u00ab", "tokens": ["\u00bb", "wie", "Ci\u00b7ce\u00b7ro", "zu", "prei\u00b7sen", "sei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "NN", "PTKZU", "VVINF", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch \u00f6der Firlefanz,", "tokens": ["Welch", "\u00f6\u00b7der", "Fir\u00b7le\u00b7fanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sch\u00f6nredner-Mummenschanz!", "tokens": ["Sch\u00f6n\u00b7red\u00b7ner\u00b7Mum\u00b7men\u00b7schanz", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Fahl g\u00e4hnt mich grauenhafte Leere", "tokens": ["Fahl", "g\u00e4hnt", "mich", "grau\u00b7en\u00b7haf\u00b7te", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus diesem Phrasenspiegel an.", "tokens": ["Aus", "die\u00b7sem", "Phra\u00b7sen\u00b7spie\u00b7gel", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu eines hohlen Schw\u00e4tzers Ehre", "tokens": ["Zu", "ei\u00b7nes", "hoh\u00b7len", "Schw\u00e4t\u00b7zers", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie man doch flei\u00dfig flunkern kann!", "tokens": ["Wie", "man", "doch", "flei\u00b7\u00dfig", "flun\u00b7kern", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das ist ein Plunder", "tokens": ["Das", "ist", "ein", "Plun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Ohnegleichen,", "tokens": ["Oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Wer wird den Zunder", "tokens": ["Wer", "wird", "den", "Zun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Der Zerst\u00f6rung reichen?", "tokens": ["Der", "Zer\u00b7st\u00f6\u00b7rung", "rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Soll das Verderben fressend immer weiter schleichen?", "tokens": ["Soll", "das", "Ver\u00b7der\u00b7ben", "fres\u00b7send", "im\u00b7mer", "wei\u00b7ter", "schlei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit all den toten Formeln, Regeln", "tokens": ["Mit", "all", "den", "to\u00b7ten", "For\u00b7meln", ",", "Re\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll ich durch Wind und Wellen segeln?", "tokens": ["Soll", "ich", "durch", "Wind", "und", "Wel\u00b7len", "se\u00b7geln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6r' die Welt da drau\u00dfen branden,", "tokens": ["Ich", "h\u00f6r'", "die", "Welt", "da", "drau\u00b7\u00dfen", "bran\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schauder f\u00e4hrt mir durchs Gebein,", "tokens": ["Ein", "Schau\u00b7der", "f\u00e4hrt", "mir", "durchs", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als blinder Schiffer mu\u00df ich stranden \u2013", "tokens": ["Als", "blin\u00b7der", "Schif\u00b7fer", "mu\u00df", "ich", "stran\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schreibe klassisches Latein.", "tokens": ["Und", "schrei\u00b7be", "klas\u00b7si\u00b7sches", "La\u00b7tein."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kann Roms Monarchen 'runterhaspeln", "tokens": ["Kann", "Roms", "Mon\u00b7ar\u00b7chen", "'r\u00b7un\u00b7ter\u00b7has\u00b7peln"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NE", "NE", "NE"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Von C\u00e4sar bis Augustulus,", "tokens": ["Von", "C\u00e4\u00b7sar", "bis", "Au\u00b7gus\u00b7tu\u00b7lus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kann klafterweise S\u00fc\u00dfholz raspeln", "tokens": ["Kann", "klaf\u00b7ter\u00b7wei\u00b7se", "S\u00fc\u00df\u00b7holz", "ras\u00b7peln"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Zum \u00dcberdru\u00df.", "tokens": ["Zum", "\u00dc\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Kann wie 'ne Puppe konjugieren,", "tokens": ["Kann", "wie", "'ne", "Pup\u00b7pe", "kon\u00b7ju\u00b7gi\u00b7e\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Am Schn\u00fcrchen plappr' ich's nur ", "tokens": ["Am", "Schn\u00fcr\u00b7chen", "plappr'", "ich's", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und mu\u00df mich vor mir selbst genieren,", "tokens": ["Und", "mu\u00df", "mich", "vor", "mir", "selbst", "ge\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Frag' ich nach Lebens Ziel und Sinn.", "tokens": ["Frag'", "ich", "nach", "Le\u00b7bens", "Ziel", "und", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ich mach' euch recht ein brav Examen,", "tokens": ["Ich", "mach'", "euch", "recht", "ein", "brav", "E\u00b7xa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ich rede noch zum Publikum,", "tokens": ["Ich", "re\u00b7de", "noch", "zum", "Pub\u00b7li\u00b7kum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Den eingeladnen Herrn und Damen,", "tokens": ["Den", "ein\u00b7ge\u00b7lad\u00b7nen", "Herrn", "und", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und bin ja doch \u2013 in Gottes Namen! \u2013", "tokens": ["Und", "bin", "ja", "doch", "\u2013", "in", "Got\u00b7tes", "Na\u00b7men", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "$(", "APPR", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Mordsmicheldumm.", "tokens": ["Mords\u00b7mi\u00b7chel\u00b7dumm", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Gepfropft mit Reisig der Verstand,", "tokens": ["Ge\u00b7pfropft", "mit", "Rei\u00b7sig", "der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Lust zur Tat, des Sehnens Brand", "tokens": ["Die", "Lust", "zur", "Tat", ",", "des", "Seh\u00b7nens", "Brand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Erstickt mit hagrer Knochenhand!", "tokens": ["Er\u00b7stickt", "mit", "hag\u00b7rer", "Kno\u00b7chen\u00b7hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die meiner Jugend Feierkleid", "tokens": ["Die", "mei\u00b7ner", "Ju\u00b7gend", "Fei\u00b7er\u00b7kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit d\u00fcrrer Vettelfaust zerfetzt,", "tokens": ["Mit", "d\u00fcr\u00b7rer", "Vet\u00b7tel\u00b7faust", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die meine g\u00e4rende Heiterkeit", "tokens": ["Die", "mei\u00b7ne", "g\u00e4\u00b7ren\u00b7de", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit m\u00fcrrischem Gei\u00dfelhieb verletzt;", "tokens": ["Mit", "m\u00fcr\u00b7ri\u00b7schem", "Gei\u00b7\u00df\u00b7el\u00b7hieb", "ver\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die den emporgereckten Sinn", "tokens": ["Die", "den", "em\u00b7por\u00b7ge\u00b7reck\u00b7ten", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Kleinlichen herniederzwang,", "tokens": ["Zum", "Klein\u00b7li\u00b7chen", "her\u00b7nie\u00b7der\u00b7zwang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich fluche dir, Zerst\u00f6rerin,", "tokens": ["Ich", "flu\u00b7che", "dir", ",", "Zer\u00b7st\u00f6\u00b7re\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hasse dich mein Leben lang.", "tokens": ["Und", "has\u00b7se", "dich", "mein", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit deinem Wust sollst du vergehn,", "tokens": ["Mit", "dei\u00b7nem", "Wust", "sollst", "du", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sturm soll durch Staub und Moder wehn,", "tokens": ["Sturm", "soll", "durch", "Staub", "und", "Mo\u00b7der", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und ein Geschlecht, des Mutterwort", "tokens": ["Und", "ein", "Ge\u00b7schlecht", ",", "des", "Mut\u00b7ter\u00b7wort"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht auf der Schulbank schon verdorrt,", "tokens": ["Nicht", "auf", "der", "Schul\u00b7bank", "schon", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drin unsres Volkes Bild und Art", "tokens": ["Drin", "uns\u00b7res", "Vol\u00b7kes", "Bild", "und", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich wunderquellend offenbart,", "tokens": ["Sich", "wun\u00b7der\u00b7quel\u00b7lend", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geist, der sich Bahn zur Sonne bricht,", "tokens": ["Geist", ",", "der", "sich", "Bahn", "zur", "Son\u00b7ne", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Bl\u00fch auf, bl\u00fch auf im Morgenlicht!", "tokens": ["Bl\u00fch", "auf", ",", "bl\u00fch", "auf", "im", "Mor\u00b7gen\u00b7licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Mitternacht ging l\u00e4ngst vorbei \u2013", "tokens": ["Die", "Mit\u00b7ter\u00b7nacht", "ging", "l\u00e4ngst", "vor\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich nur fr\u00fch genug erwache", "tokens": ["Wenn", "ich", "nur", "fr\u00fch", "ge\u00b7nug", "er\u00b7wa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meinen Aufsatz fertig mache:", "tokens": ["Und", "mei\u00b7nen", "Auf\u00b7satz", "fer\u00b7tig", "ma\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwie Cicero zu preisen sei!\u00ab", "tokens": ["\u00bb", "wie", "Ci\u00b7ce\u00b7ro", "zu", "prei\u00b7sen", "sei", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "NN", "PTKZU", "VVINF", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch \u00f6der Firlefanz,", "tokens": ["Welch", "\u00f6\u00b7der", "Fir\u00b7le\u00b7fanz", ","], 