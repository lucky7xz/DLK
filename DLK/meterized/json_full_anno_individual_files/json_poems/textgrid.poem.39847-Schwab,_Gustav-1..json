{"textgrid.poem.39847": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von jener Weisheit, die vor Gottes Thronen", "tokens": ["Von", "je\u00b7ner", "Weis\u00b7heit", ",", "die", "vor", "Got\u00b7tes", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verst\u00e4ndig, heilig, einig, mannichfaltig,", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", ",", "hei\u00b7lig", ",", "ei\u00b7nig", ",", "man\u00b7nich\u00b7fal\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rein, freundlich, sanft und klar und doch gewaltig,", "tokens": ["Rein", ",", "freund\u00b7lich", ",", "sanft", "und", "klar", "und", "doch", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Anbeginn mitsch\u00f6pfend durfte wohnen:", "tokens": ["Von", "An\u00b7be\u00b7ginn", "mit\u00b7sch\u00f6p\u00b7fend", "durf\u00b7te", "woh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von ihr, die alldurchleuchtend Millionen", "tokens": ["Von", "ihr", ",", "die", "all\u00b7durch\u00b7leuch\u00b7tend", "Mil\u00b7lio