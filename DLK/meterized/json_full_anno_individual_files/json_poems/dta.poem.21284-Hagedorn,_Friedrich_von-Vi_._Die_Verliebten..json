{"dta.poem.21284": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n  Die Verliebten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr, deren Witz die Sehnsucht \u00fcbt", "tokens": ["Ihr", ",", "de\u00b7ren", "Witz", "die", "Sehn\u00b7sucht", "\u00fcbt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer seufzet, harret, liebt,", "tokens": ["Und", "im\u00b7mer", "seuf\u00b7zet", ",", "har\u00b7ret", ",", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sp\u00e4t erreicht ihr, unbetr\u00fcbt,", "tokens": ["Wie", "sp\u00e4t", "er\u00b7reicht", "ihr", ",", "un\u00b7be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Liebe Freuden!", "tokens": ["Der", "Lie\u00b7be", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Furcht, Knechtschaft, Unruh und Verdacht,", "tokens": ["Furcht", ",", "Knecht\u00b7schaft", ",", "Un\u00b7ruh", "und", "Ver\u00b7dacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der w\u00fcste Tag, die \u00f6de Nacht", "tokens": ["Der", "w\u00fcs\u00b7te", "Tag", ",", "die", "\u00f6\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind, bis die Lieb\u2019 euch gl\u00fccklich macht,", "tokens": ["Sind", ",", "bis", "die", "Lieb'", "euch", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht zu vermeiden.", "tokens": ["Nicht", "zu", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie gro\u00df mu\u00df ihr Vergn\u00fcgen seyn!", "tokens": ["Wie", "gro\u00df", "mu\u00df", "ihr", "Ver\u00b7gn\u00fc\u00b7gen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sehr mu\u00df ihr Genu\u00df erfreun,", "tokens": ["Wie", "sehr", "mu\u00df", "ihr", "Ge\u00b7nu\u00df", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn edle Seelen ihre Pein", "tokens": ["Wenn", "ed\u00b7le", "See\u00b7len", "ih\u00b7re", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So willig leiden!", "tokens": ["So", "wil\u00b7lig", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}