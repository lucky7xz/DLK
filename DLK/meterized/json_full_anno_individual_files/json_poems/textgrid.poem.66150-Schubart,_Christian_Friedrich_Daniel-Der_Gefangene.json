{"textgrid.poem.66150": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gefangner Mann, ein armer Mann!", "tokens": ["Ge\u00b7fang\u00b7ner", "Mann", ",", "ein", "ar\u00b7mer", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs schwarze Eisengitter", "tokens": ["Durchs", "schwar\u00b7ze", "Ei\u00b7sen\u00b7git\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Starr' ich den fernen Himmel an,", "tokens": ["Starr'", "ich", "den", "fer\u00b7nen", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wein' und seufze bitter.", "tokens": ["Und", "wein'", "und", "seuf\u00b7ze", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne, sonst so hell und rund,", "tokens": ["Die", "Son\u00b7ne", ",", "sonst", "so", "hell", "und", "rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut tr\u00fcb' auf mich herunter;", "tokens": ["Schaut", "tr\u00fcb'", "auf", "mich", "her\u00b7un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00f6mmt die braune Abendstund',", "tokens": ["Und", "k\u00f6mmt", "die", "brau\u00b7ne", "A\u00b7bend\u00b7stund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So geht sie blutig unter.", "tokens": ["So", "geht", "sie", "blu\u00b7tig", "un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir ist der Mond so gelb, so bleich,", "tokens": ["Mir", "ist", "der", "Mond", "so", "gelb", ",", "so", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wallt im Witwenschleier;", "tokens": ["Er", "wallt", "im", "Wit\u00b7wensc\u00b7hlei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sterne mir \u2013 sind Fackeln gleich", "tokens": ["Die", "Ster\u00b7ne", "mir", "\u2013", "sind", "Fa\u00b7ckeln", "gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "$(", "VAFIN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei einer Todtenfeier.", "tokens": ["Bei", "ei\u00b7ner", "Tod\u00b7ten\u00b7fei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mag sehen nicht die Bl\u00fcmlein bl\u00fchn,", "tokens": ["Mag", "se\u00b7hen", "nicht", "die", "Bl\u00fcm\u00b7lein", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht f\u00fchlen Lenzeswehen;", "tokens": ["Nicht", "f\u00fch\u00b7len", "Len\u00b7zes\u00b7we\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach! lieber s\u00e4h' ich Rosmarin", "tokens": ["Ach", "!", "lie\u00b7ber", "s\u00e4h'", "ich", "Ros\u00b7ma\u00b7rin"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Duft der Gr\u00e4ber stehen.", "tokens": ["Im", "Duft", "der", "Gr\u00e4\u00b7ber", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vergebens wiegt der Abendhauch", "tokens": ["Ver\u00b7ge\u00b7bens", "wiegt", "der", "A\u00b7bend\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr mich die goldnen Aehren;", "tokens": ["F\u00fcr", "mich", "die", "gold\u00b7nen", "A\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "M\u00f6cht' nur in meinem Felsenbauch", "tokens": ["M\u00f6cht'", "nur", "in", "mei\u00b7nem", "Fel\u00b7sen\u00b7bauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die St\u00fcrme brausen h\u00f6ren.", "tokens": ["Die", "St\u00fcr\u00b7me", "brau\u00b7sen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was hilft mir Thau und Sonnenschein", "tokens": ["Was", "hilft", "mir", "Thau", "und", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Busen einer Rose?", "tokens": ["Im", "Bu\u00b7sen", "ei\u00b7ner", "Ro\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn nichts ist mein, ach! nichts ist mein,", "tokens": ["Denn", "nichts", "ist", "mein", ",", "ach", "!", "nichts", "ist", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "$,", "ITJ", "$.", "PIS", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Muttererdenschoose.", "tokens": ["Im", "Mut\u00b7ter\u00b7er\u00b7den\u00b7scho\u00b7o\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kann nimmer an der Gattin Brust,", "tokens": ["Kann", "nim\u00b7mer", "an", "der", "Gat\u00b7tin", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht an der Kinder Wangen,", "tokens": ["Nicht", "an", "der", "Kin\u00b7der", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Gattenwonne, Vaterlust", "tokens": ["Mit", "Gat\u00b7ten\u00b7won\u00b7ne", ",", "Va\u00b7ter\u00b7lust"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Himmelsthr\u00e4nen hangen.", "tokens": ["In", "Him\u00b7melst\u00b7hr\u00e4\u00b7nen", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gefangner Mann, ein armer Mann!", "tokens": ["Ge\u00b7fang\u00b7ner", "Mann", ",", "ein", "ar\u00b7mer", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern von den Lieben allen,", "tokens": ["Fern", "von", "den", "Lie\u00b7ben", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "PIAT", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mu\u00df ich des Lebens Dornenbahn", "tokens": ["Mu\u00df", "ich", "des", "Le\u00b7bens", "Dor\u00b7nen\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Schauern\u00e4chten wallen.", "tokens": ["In", "Schau\u00b7er\u00b7n\u00e4ch\u00b7ten", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es g\u00e4hnt mich an die Einsamkeit,", "tokens": ["Es", "g\u00e4hnt", "mich", "an", "die", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich w\u00e4lze mich auf Nesseln;", "tokens": ["Ich", "w\u00e4l\u00b7ze", "mich", "auf", "Nes\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und selbst mein Beten wird entweiht", "tokens": ["Und", "selbst", "mein", "Be\u00b7ten", "wird", "ent\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Klirren meiner Fesseln.", "tokens": ["Vom", "Klir\u00b7ren", "mei\u00b7ner", "Fes\u00b7seln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mich dr\u00e4ngt der hohen Freiheit Ruf;", "tokens": ["Mich", "dr\u00e4ngt", "der", "ho\u00b7hen", "Frei\u00b7heit", "Ruf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich f\u00fchl's, da\u00df Gott nur Sklaven", "tokens": ["Ich", "f\u00fchl's", ",", "da\u00df", "Gott", "nur", "Skla\u00b7ven"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Teufel f\u00fcr die Ketten schuf,", "tokens": ["Und", "Teu\u00b7fel", "f\u00fcr", "die", "Ket\u00b7ten", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um sie damit zu strafen.", "tokens": ["Um", "sie", "da\u00b7mit", "zu", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Was hab' ich, Br\u00fcder! euch gethan?", "tokens": ["Was", "hab'", "ich", ",", "Br\u00fc\u00b7der", "!", "euch", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$.", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt doch, und seht mich Armen!", "tokens": ["Kommt", "doch", ",", "und", "seht", "mich", "Ar\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gefangner Mann! ein armer Mann!", "tokens": ["Ge\u00b7fang\u00b7ner", "Mann", "!", "ein", "ar\u00b7mer", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! habt mit mir Erbarmen!", "tokens": ["Ach", "!", "habt", "mit", "mir", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Gefangner Mann, ein armer Mann!", "tokens": ["Ge\u00b7fang\u00b7ner", "Mann", ",", "ein", "ar\u00b7mer", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs schwarze Eisengitter", "tokens": ["Durchs", "schwar\u00b7ze", "Ei\u00b7sen\u00b7git\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Starr' ich den fernen Himmel an,", "tokens": ["Starr'", "ich", "den", "fer\u00b7nen", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wein' und seufze bitter.", "tokens": ["Und", "wein'", "und", "seuf\u00b7ze", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Sonne, sonst so hell und rund,", "tokens": ["Die", "Son\u00b7ne", ",", "sonst", "so", "hell", "und", "rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut tr\u00fcb' auf mich herunter;", "tokens": ["Schaut", "tr\u00fcb'", "auf", "mich", "her\u00b7un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00f6mmt die braune Abendstund',", "tokens": ["Und", "k\u00f6mmt", "die", "brau\u00b7ne", "A\u00b7bend\u00b7stund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So geht sie blutig unter.", "tokens": ["So", "geh