": ["PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach! lieber s\u00e4h' ich Rosmarin", "tokens": ["Ach", "!", "lie\u00b7ber", "s\u00e4h'", "ich", "Ros\u00b7ma\u00b7rin"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Duft der Gr\u00e4ber stehen.", "tokens": ["Im", "Duft", "der", "Gr\u00e4\u00b7ber", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Vergebens wiegt der Abendhauch", "tokens": ["Ver\u00b7ge\u00b7bens", "wiegt", "der", "A\u00b7bend\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr mich die goldnen Aehren;", "tokens": ["F\u00fcr", "mich", "die", "gold\u00b7nen", "A\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "M\u00f6cht' nur in meinem Felsenbauch", "tokens": ["M\u00f6cht'", "nur", "in", "mei\u00b7nem", "Fel\u00b7sen\u00b7bauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die St\u00fcrme brausen h\u00f6ren.", "tokens": ["Die", "St\u00fcr\u00b7me", "brau\u00b7sen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Was hilft mir Thau und Sonnenschein", "tokens": ["Was", "hilft", "mir", "Thau", "und", "Son\u00b7nen\u00