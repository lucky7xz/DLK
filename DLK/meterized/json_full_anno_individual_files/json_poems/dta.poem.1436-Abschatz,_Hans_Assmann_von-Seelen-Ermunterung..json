meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Last der siechen Glieder", "tokens": ["Die", "Last", "der", "sie\u00b7chen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verwahrt das k\u00fchle Grab.", "tokens": ["Ver\u00b7wahrt", "das", "k\u00fch\u00b7le", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Viel lassen sich verblenden", "tokens": ["Viel", "las\u00b7sen", "sich", "ver\u00b7blen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der falschen Hoheit Schein/", "tokens": ["Der", "fal\u00b7schen", "Ho\u00b7heit", "Schein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wollen auff den H\u00e4nden", "tokens": ["Und", "wol\u00b7len", "auff", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Welt getragen seyn.", "tokens": ["Der", "Welt", "ge\u00b7tra\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein sanffter Engel-Wagen", "tokens": ["Ein", "sanff\u00b7ter", "En\u00b7gel\u00b7Wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fchrt mich vor GOttes Thron/", "tokens": ["F\u00fchrt", "mich", "vor", "Got\u00b7tes", "Thr