d", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was nirgend findet Platz/", "tokens": ["Was", "nir\u00b7gend", "fin\u00b7det", "Platz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was keine Zahl kan zehlen/", "tokens": ["Was", "kei\u00b7ne", "Zahl", "kan", "zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott selber ist mein Schatz.", "tokens": ["Gott", "sel\u00b7ber", "ist", "mein", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wohnt ihr in stoltzen Zimmern/", "tokens": ["Wohnt", "ihr", "in", "stolt\u00b7zen", "Zim\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schm\u00fcckt sie mit Marmor aus/", "tokens": ["Schm\u00fcckt", "sie", "mit", "Mar\u00b7mor", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Last auff der Erde schimmern", "tokens": ["Last", "auff", "der", "Er\u00b7de", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ein andres Himmel-Hau\u00df.", "tokens": ["Ein", "and\u00b7res", "Him\u00b7mel\u00b7Hau\u00df", "."], "token_info": ["word", "word", "word", "punct"