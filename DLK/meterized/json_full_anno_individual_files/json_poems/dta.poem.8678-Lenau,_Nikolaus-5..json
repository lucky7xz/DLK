{"dta.poem.8678": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Schon zog vom Wald' ich ferne wieder", "tokens": ["Schon", "zog", "vom", "Wald'", "ich", "fer\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einer steilen Alpenwand;", "tokens": ["Auf", "ei\u00b7ner", "stei\u00b7len", "Al\u00b7pen\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch blickt' ich oft zu ihm hinnieder,", "tokens": ["Doch", "blickt'", "ich", "oft", "zu", "ihm", "hin\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis mir sein lezter Wipfel schwand. \u2014", "tokens": ["Bis", "mir", "sein", "lez\u00b7ter", "Wip\u00b7fel", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da irrten K\u00fch' am Wiesenhange;", "tokens": ["Da", "irr\u00b7ten", "K\u00fch'", "am", "Wie\u00b7sen\u00b7han\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hirte unterm Kieferdach", "tokens": ["Der", "Hir\u00b7te", "un\u00b7term", "Kie\u00b7fer\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hing still bei ihrem Glockenklange", "tokens": ["Hing", "still", "bei", "ih\u00b7rem", "Glo\u00b7cken\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Bilde seines Liebchens nach.", "tokens": ["Dem", "Bil\u00b7de", "sei\u00b7nes", "Lieb\u00b7chens", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}