{"dta.poem.20008": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Todten-Opfer  \n f\u00fcr Karl Gangloff .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Menschheit Seufzer schweigen,", "tokens": ["Der", "Menschheit", "Seuf\u00b7zer", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von Fl\u00f6ten und s\u00fc\u00dfen Geigen", "tokens": ["Von", "Fl\u00f6\u00b7ten", "und", "s\u00fc\u00b7\u00dfen", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ert\u00f6nt ein muntrer Chor,", "tokens": ["Er\u00b7t\u00f6nt", "ein", "mun\u00b7trer", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In freyen Laubgewinden", "tokens": ["In", "frey\u00b7en", "Laub\u00b7ge\u00b7win\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich wieder S\u00e4nger finden,", "tokens": ["Sich", "wie\u00b7der", "S\u00e4n\u00b7ger", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die singen wie zuvor.", "tokens": ["Die", "sin\u00b7gen", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Duftreiche Lilien bl\u00fchen,", "tokens": ["Duf\u00b7trei\u00b7che", "Li\u00b7li\u00b7en", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Melodisch Fl\u00fcsse ziehen", "tokens": ["Me\u00b7lo\u00b7disch", "Fl\u00fcs\u00b7se", "zie\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum freygeword'nen Rhein.", "tokens": ["Zum", "frey\u00b7ge\u00b7word'\u00b7nen", "Rhein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit himmelblauen Wogen", "tokens": ["Mit", "him\u00b7mel\u00b7blau\u00b7en", "Wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kommt jauchzend der gezogen", "tokens": ["Kommt", "jauch\u00b7zend", "der", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Blut und Thr\u00e4nen rein.", "tokens": ["Von", "Blut", "und", "Thr\u00e4\u00b7nen", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die M\u00e4nner, die aus Schlachten", "tokens": ["Die", "M\u00e4n\u00b7ner", ",", "die", "aus", "Schlach\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns Ros' und Lilie brachten,", "tokens": ["Uns", "Ros'", "und", "Li\u00b7lie", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Wunden roth und bleich,", "tokens": ["Durch", "Wun\u00b7den", "roth", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die la\u00df uns w\u00fcrdig preisen,", "tokens": ["Die", "la\u00df", "uns", "w\u00fcr\u00b7dig", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich mit Gesangesweisen,", "tokens": ["Ich", "mit", "Ge\u00b7san\u00b7ges\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du Freund! mit Bildern reich!", "tokens": ["Du", "Freund", "!", "mit", "Bil\u00b7dern", "reich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Traum! \u2014 du junges Leben!", "tokens": ["O", "Traum", "!", "du", "jun\u00b7ges", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "$(", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Bildern hell umgeben,", "tokens": ["Von", "Bil\u00b7dern", "hell", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die deine Kunst erfand.", "tokens": ["Die", "dei\u00b7ne", "Kunst", "er\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Liegst du im stillen Zimmer", "tokens": ["Liegst", "du", "im", "stil\u00b7len", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erbleicht im Sarge, \u2014 nimmer", "tokens": ["Er\u00b7bleicht", "im", "Sar\u00b7ge", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["NN", "APPRART", "NN", "$,", "$(", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "R\u00fchrt sich die theure Hand!", "tokens": ["R\u00fchrt", "sich", "die", "theu\u00b7re", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie k\u00f6nnt' ich so mich tr\u00fcgen!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "so", "mich", "tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bilder und Griffel liegen", "tokens": ["Bil\u00b7der", "und", "Grif\u00b7fel", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Verlassen ja herum!", "tokens": ["Ver\u00b7las\u00b7sen", "ja", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie seyd ihr bleich, ihr Wangen!", "tokens": ["Wie", "seyd", "ihr", "bleich", ",", "ihr", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Lichter! wie vergangen!", "tokens": ["Ihr", "Lich\u00b7ter", "!", "wie", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du Mund! wie kalt und stumm!", "tokens": ["Du", "Mund", "!", "wie", "kalt", "und", "stumm", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "PWAV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Im Tod ist dir erklungen", "tokens": ["Im", "Tod", "ist", "dir", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Lied der Nibelungen,", "tokens": ["Das", "Lied", "der", "Ni\u00b7be\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schwertschlag der Hermannsschlacht.", "tokens": ["Schwert\u00b7schlag", "der", "Her\u00b7manns\u00b7schlacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Drauf hat dir wonnetrunken", "tokens": ["Drauf", "hat", "dir", "won\u00b7ne\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der sel'ge Freund gewunken ", "tokens": ["Der", "sel'\u00b7ge", "Freund", "ge\u00b7wun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sieh! \u2014 es war vollbracht.", "tokens": ["Und", "sieh", "!", "es", "war", "voll\u00b7bracht", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die du hier oft in Bildern", "tokens": ["Die", "du", "hier", "oft", "in", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Versuchtest treu zu schildern", "tokens": ["Ver\u00b7such\u00b7test", "treu", "zu", "schil\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hellen'scher M\u00e4nner Chor,", "tokens": ["Hel\u00b7len'\u00b7scher", "M\u00e4n\u00b7ner", "Chor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Helden aus Herrmannsstreiten,", "tokens": ["Hel\u00b7den", "aus", "Herr\u00b7manns\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Jungfrau'n aus deutschen Zeiten,", "tokens": ["Jung\u00b7frau'n", "aus", "deut\u00b7schen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die tragen dich empor.", "tokens": ["Die", "tra\u00b7gen", "dich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In linden Armen halten", "tokens": ["In", "lin\u00b7den", "Ar\u00b7men", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich g\u00f6ttliche Gestalten,", "tokens": ["Dich", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ahnend du geschaut;", "tokens": ["Die", "ah\u00b7nend", "du", "ge\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wohl sind es deine F\u00fchrer,", "tokens": ["Wohl", "sind", "es", "dei\u00b7ne", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mengs, Raphael und D\u00fcrer,", "tokens": ["Mengs", ",", "Ra\u00b7phael", "und", "D\u00fc\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dir ewig nun vertraut.", "tokens": ["Dir", "e\u00b7wig", "nun", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich doch mu\u00df einsam wallen!", "tokens": ["Ich", "doch", "mu\u00df", "ein\u00b7sam", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Andern! La\u00dft erschallen", "tokens": ["Ihr", "An\u00b7dern", "!", "La\u00dft", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$.", "VVIMP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jubel und Siegsgesang! \u2014", "tokens": ["Ju\u00b7bel", "und", "Siegs\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "O Geist in sel'ger Wonne!", "tokens": ["O", "Geist", "in", "sel'\u00b7ger", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Send' mir aus deiner Sonne", "tokens": ["Send'", "mir", "aus", "dei\u00b7ner", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur einen einz'gen Klang!", "tokens": ["Nur", "ei\u00b7nen", "einz'\u00b7gen", "Klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}