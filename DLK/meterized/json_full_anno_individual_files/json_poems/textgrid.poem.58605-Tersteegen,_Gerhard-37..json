{"textgrid.poem.58605": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hoch, wie tief, wie breit ist unsres Gottes G\u00fcte!", "tokens": ["Wie", "hoch", ",", "wie", "tief", ",", "wie", "breit", "ist", "uns\u00b7res", "Got\u00b7tes", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Meer der Freundlichkeit, was soll ich sagen doch?", "tokens": ["Du", "Meer", "der", "Freund\u00b7lich\u00b7keit", ",", "was", "soll", "ich", "sa\u00b7gen", "doch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PWS", "VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hebst und tr\u00e4gest mich nach Leib und nach Gem\u00fcte,", "tokens": ["Du", "hebst", "und", "tr\u00e4\u00b7gest", "mich", "nach", "Leib", "und", "nach", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du hast es lang getan und tust es immer noch.", "tokens": ["Du", "hast", "es", "lang", "ge\u00b7tan", "und", "tust", "es", "im\u00b7mer", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Mutter liebt ihr Kind, das sie im Leib getragen;", "tokens": ["Die", "Mut\u00b7ter", "liebt", "ihr", "Kind", ",", "das", "sie", "im", "Leib", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du, mehr als Mutterherz, du tr\u00e4gst mich f\u00fcr und f\u00fcr,", "tokens": ["Du", ",", "mehr", "als", "Mut\u00b7ter\u00b7herz", ",", "du", "tr\u00e4gst", "mich", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "KOUS", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob ich im Elend lieg', so h\u00f6r' ich doch dich sagen:", "tokens": ["Ob", "ich", "im", "E\u00b7lend", "lieg'", ",", "so", "h\u00f6r'", "ich", "doch", "dich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u00bbich will erretten dich.\u00ab Wen sollt' ich gleichen dir?", "tokens": ["\u00bb", "ich", "will", "er\u00b7ret\u00b7ten", "dich", ".", "\u00ab", "Wen", "sollt'", "ich", "glei\u00b7chen", "dir", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "P