{"dta.poem.20762": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Mittags, als die Dichterin mit bei dem Dom-  \n Dechant Spiegel zu Halberstadt speiste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir Menschen sind so sehr verschieden:               ", "tokens": ["Wir", "Men\u00b7schen", "sind", "so", "sehr", "ver\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine liebt den stillen Frieden,", "tokens": ["Der", "ei\u00b7ne", "liebt", "den", "stil\u00b7len", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der andre liebt den Ruhm, der dritte liebt den Scherz,", "tokens": ["Der", "and\u00b7re", "liebt", "den", "Ruhm", ",", "der", "drit\u00b7te", "liebt", "den", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und welcher Mensch kann f\u00fcr sein Herz?", "tokens": ["Und", "wel\u00b7cher", "Mensch", "kann", "f\u00fcr", "sein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies hat ihm die Natur gegeben \u2014", "tokens": ["Dies", "hat", "ihm", "die", "Na\u00b7tur", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der beste Vorsatz m\u00fcsse leben!", "tokens": ["Der", "bes\u00b7te", "Vor\u00b7satz", "m\u00fcs\u00b7se", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}