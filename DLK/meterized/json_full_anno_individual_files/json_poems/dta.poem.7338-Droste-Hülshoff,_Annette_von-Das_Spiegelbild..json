{"dta.poem.7338": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das Spiegelbild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Schaust du mich an aus dem Kristall,", "tokens": ["Schaust", "du", "mich", "an", "aus", "dem", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mit deiner Augen Nebelball,", "tokens": ["Mit", "dei\u00b7ner", "Au\u00b7gen", "Ne\u00b7bel\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kometen gleich die im Verbleichen;", "tokens": ["Ko\u00b7me\u00b7ten", "gleich", "die", "im", "Ver\u00b7blei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Z\u00fcgen, worin wunderlich", "tokens": ["Mit", "Z\u00fc\u00b7gen", ",", "wo\u00b7rin", "wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zwei Seelen wie Spione sich", "tokens": ["Zwei", "See\u00b7len", "wie", "Spi\u00b7o\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "KOKOM", "NN", "PRF"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Umschleichen, ja, dann fl\u00fcstre ich:", "tokens": ["Um\u00b7schlei\u00b7chen", ",", "ja", ",", "dann", "fl\u00fcst\u00b7re", "ich", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Phantom, du bist nicht meines Gleichen!", "tokens": ["Phan\u00b7tom", ",", "du", "bist", "nicht", "mei\u00b7nes", "Glei\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist nur entschl\u00fcpft der Tr\u00e4ume Hut,", "tokens": ["Bist", "nur", "ent\u00b7schl\u00fcpft", "der", "Tr\u00e4u\u00b7me", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu eisen mir das warme Blut,", "tokens": ["Zu", "ei\u00b7sen", "mir", "das", "war\u00b7me", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die dunkle Locke mir zu blassen;", "tokens": ["Die", "dunk\u00b7le", "Lo\u00b7cke", "mir", "zu", "blas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch, d\u00e4mmerndes Gesicht,", "tokens": ["Und", "den\u00b7noch", ",", "d\u00e4m\u00b7mern\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drin seltsam spielt ein Doppellicht,", "tokens": ["Drin", "selt\u00b7sam", "spielt", "ein", "Dop\u00b7pel\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tr\u00e4test du vor, ich wei\u00df es nicht,", "tokens": ["Tr\u00e4\u00b7test", "du", "vor", ",", "ich", "wei\u00df", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "W\u00fcrd' ich dich lieben oder hassen?", "tokens": ["W\u00fcrd'", "ich", "dich", "lie\u00b7ben", "o\u00b7der", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu deiner Stirne Herrscherthron,", "tokens": ["Zu", "dei\u00b7ner", "Stir\u00b7ne", "Herr\u00b7scher\u00b7thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo die Gedanken leisten Frohn", "tokens": ["Wo", "die", "Ge\u00b7dan\u00b7ken", "leis\u00b7ten", "Frohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Knechte, w\u00fcrd ich sch\u00fcchtern blicken;", "tokens": ["Wie", "Knech\u00b7te", ",", "w\u00fcrd", "ich", "sch\u00fcch\u00b7tern", "bli\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VAFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch von des Auges kaltem Glast,", "tokens": ["Doch", "von", "des", "Au\u00b7ges", "kal\u00b7tem", "Glast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Voll todten Lichts, gebrochen fast,", "tokens": ["Voll", "tod\u00b7ten", "Lichts", ",", "ge\u00b7bro\u00b7chen", "fast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gespenstig, w\u00fcrd, ein scheuer Gast,", "tokens": ["Ge\u00b7spens\u00b7tig", ",", "w\u00fcrd", ",", "ein", "scheu\u00b7er", "Gast", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weit, weit ich meinen Schemel r\u00fccken.", "tokens": ["Weit", ",", "weit", "ich", "mei\u00b7nen", "Sche\u00b7mel", "r\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und was den Mund umspielt so lind,", "tokens": ["Und", "was", "den", "Mund", "um\u00b7spielt", "so", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So weich und h\u00fclflos wie ein Kind,", "tokens": ["So", "weich", "und", "h\u00fcl\u00b7flos", "wie", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das m\u00f6cht in treue Hut ich bergen;", "tokens": ["Das", "m\u00f6cht", "in", "treu\u00b7e", "Hut", "ich", "ber\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder, wenn er h\u00f6hnend spielt,", "tokens": ["Und", "wie\u00b7der", ",", "wenn", "er", "h\u00f6h\u00b7nend", "spielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie von gespanntem Bogen zielt,", "tokens": ["Wie", "von", "ge\u00b7spann\u00b7tem", "Bo\u00b7gen", "zielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn leis' es durch die Z\u00fcge w\u00fchlt,", "tokens": ["Wenn", "leis'", "es", "durch", "die", "Z\u00fc\u00b7ge", "w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann m\u00f6cht ich fliehen wie vor Schergen.", "tokens": ["Dann", "m\u00f6cht", "ich", "flie\u00b7hen", "wie", "vor", "Scher\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es ist gewi\u00df, du bist nicht Ich,", "tokens": ["Es", "ist", "ge\u00b7wi\u00df", ",", "du", "bist", "nicht", "Ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein fremdes Daseyn, dem ich mich", "tokens": ["Ein", "frem\u00b7des", "Da\u00b7seyn", ",", "dem", "ich", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Moses nahe, unbeschuhet,", "tokens": ["Wie", "Mo\u00b7ses", "na\u00b7he", ",", "un\u00b7be\u00b7schu\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NE", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Kr\u00e4fte die mir nicht bewust,", "tokens": ["Voll", "Kr\u00e4f\u00b7te", "die", "mir", "nicht", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Voll fremden Leides, fremder Lust;", "tokens": ["Voll", "frem\u00b7den", "Lei\u00b7des", ",", "frem\u00b7der", "Lust", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gnade mir Gott, wenn in der Brust", "tokens": ["Gna\u00b7de", "mir", "Gott", ",", "wenn", "in", "der", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Mir schlummernd deine Seele ruhet!", "tokens": ["Mir", "schlum\u00b7mernd", "dei\u00b7ne", "See\u00b7le", "ru\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und dennoch f\u00fchl ich, wie verwandt,", "tokens": ["Und", "den\u00b7noch", "f\u00fchl", "ich", ",", "wie", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinen Schauern mich gebannt,", "tokens": ["Zu", "dei\u00b7nen", "Schau\u00b7ern", "mich", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Liebe mu\u00df der Furcht sich einen.", "tokens": ["Und", "Lie\u00b7be", "mu\u00df", "der", "Furcht", "sich", "ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ART", "NN", "PRF", "ART", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, tr\u00e4test aus Kristalles Rund,", "tokens": ["Ja", ",", "tr\u00e4\u00b7test", "aus", "Kris\u00b7tal\u00b7les", "Rund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Phantom, du lebend auf den Grund,", "tokens": ["Phan\u00b7tom", ",", "du", "le\u00b7bend", "auf", "den", "Grund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur leise zittern w\u00fcrd ich, und", "tokens": ["Nur", "lei\u00b7se", "zit\u00b7tern", "w\u00fcrd", "ich", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VVINF", "VAFIN", "PPER", "$,", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich d\u00fcnkt \u2014 ich w\u00fcrde um dich weinen!", "tokens": ["Mich", "d\u00fcnkt", "ich", "w\u00fcr\u00b7de", "um", "dich", "wei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}