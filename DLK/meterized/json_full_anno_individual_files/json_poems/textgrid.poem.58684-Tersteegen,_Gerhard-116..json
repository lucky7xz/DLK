{"textgrid.poem.58684": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "116.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bin ich so verw\u00fcstet doch, Gott ist so fern dem Geiste,", "tokens": ["Wie", "bin", "ich", "so", "ver\u00b7w\u00fcs\u00b7tet", "doch", ",", "Gott", "ist", "so", "fern", "dem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVFIN", "ADV", "$,", "NN", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--++-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Mein Herz, sein Haus und Heiligtum, voll aller Greuel steht!", "tokens": ["Mein", "Herz", ",", "sein", "Haus", "und", "Hei\u00b7lig\u00b7tum", ",", "voll", "al\u00b7ler", "Greu\u00b7el", "steht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NE", "$,", "ADJD", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Dein wonnevolles Angesicht, mein Gott, mir Beistand leiste,", "tokens": ["Dein", "won\u00b7ne\u00b7vol\u00b7les", "An\u00b7ge\u00b7sicht", ",", "mein", "Gott", ",", "mir", "Bei\u00b7stand", "leis\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Bestrahle meinen d\u00fcstern Grund, du sch\u00f6ne Morgenr\u00f6t',", "tokens": ["Be\u00b7strah\u00b7le", "mei\u00b7nen", "d\u00fcs\u00b7tern", "Grund", ",", "du", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6t'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "La\u00df deine Gnadengegenwart im Geiste wieder sehen,", "tokens": ["La\u00df", "dei\u00b7ne", "Gna\u00b7den\u00b7ge\u00b7gen\u00b7wart", "im", "Geis\u00b7te", "wie\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Erf\u00fclle meiner Seele Grund als deinen Tempel ganz,", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "mei\u00b7ner", "See\u00b7le", "Grund", "als", "dei\u00b7nen", "Tem\u00b7pel", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "KOKOM", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Da wohn und thron nach deiner Lust, da lass' mich vor dir stehen,", "tokens": ["Da", "wohn", "und", "thron", "nach", "dei\u00b7ner", "Lust", ",", "da", "lass'", "mich", "vor", "dir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Bis mich verwandelt in dein Bild dein's Angesichtes Glanz!", "tokens": ["Bis", "mich", "ver\u00b7wan\u00b7delt", "in", "dein", "Bild", "dein's", "An\u00b7ge\u00b7sich\u00b7tes", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "V