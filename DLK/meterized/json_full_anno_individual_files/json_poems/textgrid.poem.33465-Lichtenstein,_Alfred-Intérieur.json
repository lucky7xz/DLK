{"textgrid.poem.33465": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Int\u00e9rieur", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gro\u00dfer Raum \u2013 halbdunkel ... T\u00f6dlich ... Ganz verwirrt ...", "tokens": ["Ein", "gro\u00b7\u00dfer", "Raum", "\u2013", "halb\u00b7dun\u00b7kel", "...", "T\u00f6d\u00b7lich", "...", "Ganz", "ver\u00b7wirrt", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADJD", "$(", "NE", "$(", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aufreizend! ... Z\u00e4rtlich ... Traumhaft ... Nischen, schwere T\u00fcren", "tokens": ["Auf\u00b7rei\u00b7zend", "!", "...", "Z\u00e4rt\u00b7lich", "...", "Traum\u00b7haft", "...", "Ni\u00b7schen", ",", "schwe\u00b7re", "T\u00fc\u00b7ren"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "$.", "$(", "NE", "$(", "NN", "$(", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weite Schatten, die in blaue Winkel f\u00fchren ...", "tokens": ["Und", "wei\u00b7te", "Schat\u00b7ten", ",", "die", "in", "blau\u00b7e", "Win\u00b7kel", "f\u00fch\u00b7ren", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und irgendwo ein Ton, der wie ein Sektglas klirrt.", "tokens": ["Und", "ir\u00b7gend\u00b7wo", "ein", "Ton", ",", "der", "wie", "ein", "Sekt\u00b7glas", "klirrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf schwachem Teppich liegt ein breites Bilderbuch,", "tokens": ["Auf", "schwa\u00b7chem", "Tep\u00b7pich", "liegt", "ein", "brei\u00b7tes", "Bil\u00b7der\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von gr\u00fcnem Deckenlicht verzerrt und \u00fcbertrieben.", "tokens": ["Von", "gr\u00fc\u00b7nem", "De\u00b7cken\u00b7licht", "ver\u00b7zerrt", "und", "\u00fc\u00b7bert\u00b7rie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie \u2013 weiche K\u00e4tzchen \u2013 fromm sich wei\u00dfe Fr\u00e4ulein lieben!", "tokens": ["Wie", "\u2013", "wei\u00b7che", "K\u00e4tz\u00b7chen", "\u2013", "fromm", "sich", "wei\u00b7\u00dfe", "Fr\u00e4u\u00b7lein", "lie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "ADJA", "NN", "$(", "ADJD", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Hintergrund ein Greis und seidnes Taschentuch.", "tokens": ["Vom", "Hin\u00b7ter\u00b7grund", "ein", "Greis", "und", "seid\u00b7nes", "Ta\u00b7schen\u00b7tuch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1