{"dta.poem.6409": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will man von dem Geschmack der C\u00f6rper eigentlich,", "tokens": ["Will", "man", "von", "dem", "Ge\u00b7schmack", "der", "C\u00f6r\u00b7per", "ei\u00b7gent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und von derselben Nang\u2019 ein billig Urtheil f\u00e4llen;", "tokens": ["Und", "von", "der\u00b7sel\u00b7ben", "Nang'", "ein", "bil\u00b7lig", "Ur\u00b7theil", "f\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df man s\u00fc\u00df und bitter sich", "tokens": ["So", "mu\u00df", "man", "s\u00fc\u00df", "und", "bit\u00b7ter", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "KON", "ADJD", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einander nicht entgegen stellen.", "tokens": ["Ein\u00b7an\u00b7der", "nicht", "ent\u00b7ge\u00b7gen", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}