{"textgrid.poem.39417": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Dichter will zur fernen Nachwelt dringen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter will zur fernen Nachwelt dringen,", "tokens": ["Der", "Dich\u00b7ter", "will", "zur", "fer\u00b7nen", "Nach\u00b7welt", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch auf den todten Bl\u00e4ttern schl\u00e4ft sein Lied,", "tokens": ["Doch", "auf", "den", "tod\u00b7ten", "Bl\u00e4t\u00b7tern", "schl\u00e4ft", "sein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis eine seelenvolle Stimm' es weckt", "tokens": ["Bis", "ei\u00b7ne", "see\u00b7len\u00b7vol\u00b7le", "Stimm'", "es", "weckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und freundlich in der H\u00f6rer engem Krei\u00dfe", "tokens": ["Und", "freund\u00b7lich", "in", "der", "H\u00f6\u00b7rer", "en\u00b7gem", "Krei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr ihn um einen stillen Beifall wirbt.", "tokens": ["F\u00fcr", "ihn", "um", "ei\u00b7nen", "stil\u00b7len", "Bei\u00b7fall", "wirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der B\u00fchne Kunst gl\u00e4nzt vor der Mitwelt Augen,", "tokens": ["Der", "B\u00fch\u00b7ne", "Kunst", "gl\u00e4nzt", "vor", "der", "Mit\u00b7welt", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Zauber einer fremden Welt umstrahlen", "tokens": ["Die", "Zau\u00b7ber", "ei\u00b7ner", "frem\u00b7den", "Welt", "um\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Augenblick des Jubels, der Entz\u00fcckung,", "tokens": ["Den", "Au\u00b7gen\u00b7blick", "des", "Ju\u00b7bels", ",", "der", "Ent\u00b7z\u00fc\u00b7ckung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und tausend hingeri\u00df'ne Herzen gl\u00fch'n.", "tokens": ["Und", "tau\u00b7send", "hin\u00b7ge\u00b7ri\u00df'\u00b7ne", "Her\u00b7zen", "gl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "CARD", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Doch ach! sie lebt nur in des K\u00fcnstlers Leben,", "tokens": ["Doch", "ach", "!", "sie", "lebt", "nur", "in", "des", "K\u00fcnst\u00b7lers", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und sein unsterblich Werk stirbt hin mit ihm.", "tokens": ["Und", "sein", "uns\u00b7terb\u00b7lich", "Werk", "stirbt", "hin", "mit", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Nachwelt mu\u00df die sch\u00f6nen Wunder glauben,", "tokens": ["Die", "Nach\u00b7welt", "mu\u00df", "die", "sch\u00f6\u00b7nen", "Wun\u00b7der", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die sie nicht sah; kein Bild, kein schildernd Wort", "tokens": ["Die", "sie", "nicht", "sah", ";", "kein", "Bild", ",", "kein", "schil\u00b7dernd", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$.", "PIAT", "NN", "$,", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vermag des Lebens zarten Hauch zu fa\u00dfen,", "tokens": ["Ver\u00b7mag", "des", "Le\u00b7bens", "zar\u00b7ten", "Hauch", "zu", "fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die m\u00e4cht'ge hochbeseelte Gegenwart.", "tokens": ["Die", "m\u00e4cht'\u00b7ge", "hoch\u00b7be\u00b7seel\u00b7te", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Drum ist des Dichters Pflicht, davon zu zeugen,", "tokens": ["Drum", "ist", "des", "Dich\u00b7ters", "Pflicht", ",", "da\u00b7von", "zu", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN", "$,", "PAV", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Denn er allein entflammt die Phantasie,", "tokens": ["Denn", "er", "al\u00b7lein", "ent\u00b7flammt", "die", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Da\u00df sich in ihr Gestalten frisch bewegen", "tokens": ["Da\u00df", "sich", "in", "ihr", "Ge\u00b7stal\u00b7ten", "frisch", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Von nie geseh'nen Dingen: und so weckt er", "tokens": ["Von", "nie", "ge\u00b7seh'\u00b7nen", "Din\u00b7gen", ":", "und", "so", "weckt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$.", "KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "In fernen ungebornen Menschenaltern", "tokens": ["In", "fer\u00b7nen", "un\u00b7ge\u00b7bor\u00b7nen", "Men\u00b7schen\u00b7al\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Zuschauer noch dem Liebling der Thalia.", "tokens": ["Zu\u00b7schau\u00b7er", "noch", "dem", "Lieb\u00b7ling", "der", "Tha\u00b7lia", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Besch\u00e4mt biet' ich dir diese Lieder an,", "tokens": ["Be\u00b7sch\u00e4mt", "biet'", "ich", "dir", "die\u00b7se", "Lie\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wovon sich keins noch an dein Lob gewagt.", "tokens": ["Wo\u00b7von", "sich", "keins", "noch", "an", "dein", "Lob", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich es leis' und sch\u00fcchtern wohl versuchte,", "tokens": ["Wenn", "ich", "es", "leis'", "und", "sch\u00fcch\u00b7tern", "wohl", "ver\u00b7such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "KON", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nahmst du die Huldigung, die dir geb\u00fchrt,", "tokens": ["Nahmst", "du", "die", "Hul\u00b7di\u00b7gung", ",", "die", "dir", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des abgedrungenen Gef\u00fchls Tribut,", "tokens": ["Des", "ab\u00b7ge\u00b7drun\u00b7ge\u00b7nen", "Ge\u00b7f\u00fchls", "Tri\u00b7but", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wohlwollend auf wie eine freie Gabe.", "tokens": ["Wohl\u00b7wol\u00b7lend", "auf", "wie", "ei\u00b7ne", "frei\u00b7e", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "W\u00e4r' die\u00df ganz deiner w\u00fcrdig mir gelungen,", "tokens": ["W\u00e4r'", "die\u00df", "ganz", "dei\u00b7ner", "w\u00fcr\u00b7dig", "mir", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PPOSAT", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So h\u00e4tt' ich wohl den sch\u00f6nsten Sieg errungen:", "tokens": ["So", "h\u00e4tt'", "ich", "wohl", "den", "sch\u00f6ns\u00b7ten", "Sieg", "er\u00b7run\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn aller Gunst ist sicher das Gedicht,", "tokens": ["Denn", "al\u00b7ler", "Gunst", "ist", "si\u00b7cher", "das", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das deinen Geist in holder Bildung spricht;", "tokens": ["Das", "dei\u00b7nen", "Geist", "in", "hol\u00b7der", "Bil\u00b7dung", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Grazien Geheimni\u00df wird's entfalten,", "tokens": ["Der", "Gra\u00b7zi\u00b7en", "Ge\u00b7heim\u00b7ni\