{"dta.poem.20087": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "O glaub, mein Herz ist nicht erkaltet,", "tokens": ["O", "glaub", ",", "mein", "Herz", "ist", "nicht", "er\u00b7kal\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es gl\u00fcht in ihm so hei\u00df wie je,", "tokens": ["Es", "gl\u00fcht", "in", "ihm", "so", "hei\u00df", "wie", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ihr drin f\u00fcr Winter haltet,", "tokens": ["Und", "was", "ihr", "drin", "f\u00fcr", "Win\u00b7ter", "hal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Schein nur, ist gemalter Schnee.", "tokens": ["Ist", "Schein", "nur", ",", "ist", "ge\u00b7mal\u00b7ter", "Schnee", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch, was in alter Lieb\u2019 ich f\u00fchle,", "tokens": ["Doch", ",", "was", "in", "al\u00b7ter", "Lieb'", "ich", "f\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschlie\u00df ich jetzt in tiefstem Sinn,", "tokens": ["Ver\u00b7schlie\u00df", "ich", "jetzt", "in", "tiefs\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und trag\u2019s nicht f\u00fcrder in\u2019s Gew\u00fchle", "tokens": ["Und", "trag's", "nicht", "f\u00fcr\u00b7der", "in's", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "PTKNEG", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ewig kalten Menschen hin.", "tokens": ["Der", "e\u00b7wig", "kal\u00b7ten", "Men\u00b7schen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bin wie Wein der ausgegohren:", "tokens": ["Ich", "bin", "wie", "Wein", "der", "aus\u00b7ge\u00b7goh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ART", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er sch\u00e4umt nicht l\u00e4nger hin und her,", "tokens": ["Er", "sch\u00e4umt", "nicht", "l\u00e4n\u00b7ger", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch was nach Au\u00dfen er verloren,", "tokens": ["Doch", "was", "nach", "Au\u00b7\u00dfen", "er", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er an ", "tokens": ["Hat", "er", "an"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}}}}}