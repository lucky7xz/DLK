{"dta.poem.937": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wir werden heute nicht zum garten gehen               ", "tokens": ["Wir", "wer\u00b7den", "heu\u00b7te", "nicht", "zum", "gar\u00b7ten", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPRART", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn wie uns manchmal rasch und unerkl&#228;rt               ", "tokens": ["Denn", "wie", "uns", "manch\u00b7mal", "rasch", "und", "un\u00b7er\u00b7kl", "&#228;", "rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "$(", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dies leichte duften oder leise wehen               ", "tokens": ["Dies", "leich\u00b7te", "duf\u00b7ten", "o\u00b7der", "lei\u00b7se", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit lang vergessner freude wieder n&#228;hrt             ", "tokens": ["Mit", "lang", "ver\u00b7gess\u00b7ner", "freu\u00b7de", "wie\u00b7der", "n", "&#228;", "hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADV", "XY", "$(", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "So bringt uns jenes mahnende gespenster               ", "tokens": ["So", "bringt", "uns", "je\u00b7nes", "mah\u00b7nen\u00b7de", "ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und leiden das uns bang und m&#252;de macht &#183;               ", "tokens": ["Und", "lei\u00b7den", "das", "uns", "bang", "und", "m", "&#252;", "de", "macht", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity"], "pos": ["KON", "VVFIN", "ART", "PPER", "ADJD", "KON", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieh unterm baume draussen vor dem fenster               ", "tokens": ["Sieh", "un\u00b7term", "bau\u00b7me", "draus\u00b7sen", "vor", "dem", "fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die vielen leichen nach der winde schlacht             ", "tokens": ["Die", "vie\u00b7len", "lei\u00b7chen", "nach", "der", "win\u00b7de", "schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom thore dessen eisen-lilien rosten               ", "tokens": ["Vom", "tho\u00b7re", "des\u00b7sen", "ei\u00b7sen\u00b7li\u00b7li\u00b7en", "ros\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entfliegen v\u00f6gel zum verdeckten rasen               ", "tokens": ["Ent\u00b7flie\u00b7gen", "v\u00f6\u00b7gel", "zum", "ver\u00b7deck\u00b7ten", "ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und andre trinken frierend auf den pfosten               ", "tokens": ["Und", "and\u00b7re", "trin\u00b7ken", "frie\u00b7rend", "auf", "den", "pfos\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom regen aus den hohlen blumen-vasen.             ", "tokens": ["Vom", "re\u00b7gen", "aus", "den", "hoh\u00b7len", "blu\u00b7men\u00b7va\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}