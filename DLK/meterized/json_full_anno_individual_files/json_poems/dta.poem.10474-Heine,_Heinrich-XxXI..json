{"dta.poem.10474": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Deine weichen Lilienfinger,", "tokens": ["Dei\u00b7ne", "wei\u00b7chen", "Li\u00b7li\u00b7en\u00b7fin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "K\u00f6nnt' ich sie noch einmal k\u00fcssen,", "tokens": ["K\u00f6nnt'", "ich", "sie", "noch", "ein\u00b7mal", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie dr\u00fccken an mein Herz,", "tokens": ["Und", "sie", "dr\u00fc\u00b7cken", "an", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vergehn in stillem Weinen!", "tokens": ["Und", "ver\u00b7gehn", "in", "stil\u00b7lem", "Wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine klaren Veilchenaugen", "tokens": ["Dei\u00b7ne", "kla\u00b7ren", "Veil\u00b7chen\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweben vor mir Tag und Nacht,", "tokens": ["Schwe\u00b7ben", "vor", "mir", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich qu\u00e4lt es: was bedeuten", "tokens": ["Und", "mich", "qu\u00e4lt", "es", ":", "was", "be\u00b7deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "PWS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese s\u00fc\u00dfen, blauen R\u00e4thsel?", "tokens": ["Die\u00b7se", "s\u00fc\u00b7\u00dfen", ",", "blau\u00b7en", "R\u00e4th\u00b7sel", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}