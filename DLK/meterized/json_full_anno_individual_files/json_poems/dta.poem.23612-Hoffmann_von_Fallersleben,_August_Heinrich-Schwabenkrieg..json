{"dta.poem.23612": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schwabenkrieg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Trommel schl\u00e4gt, zum Krieg hinaus", "tokens": ["Die", "Trom\u00b7mel", "schl\u00e4gt", ",", "zum", "Krieg", "hin\u00b7aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Spie\u00dfen, Degen, Flinten!", "tokens": ["Mit", "Spie\u00b7\u00dfen", ",", "De\u00b7gen", ",", "Flin\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcrwahr, es ist ein harter Strau\u00df!", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "ist", "ein", "har\u00b7ter", "Strau\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir ziehn hinaus mit Mann und Maus,", "tokens": ["Wir", "ziehn", "hin\u00b7aus", "mit", "Mann", "und", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und keiner bleibt dahinten.", "tokens": ["Und", "kei\u00b7ner", "bleibt", "da\u00b7hin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als die wilde Schlacht begann,", "tokens": ["Und", "als", "die", "wil\u00b7de", "Schlacht", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sollten wir uns schlagen.", "tokens": ["Da", "soll\u00b7ten", "wir", "uns", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sprach ich: gebt mir meinen Mann \u2014", "tokens": ["Da", "sprach", "ich", ":", "gebt", "mir", "mei\u00b7nen", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was geht mich euer Krieg denn an? \u2014", "tokens": ["Was", "geht", "mich", "eu\u00b7er", "Krieg", "denn", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will mich mit ihm vertragen.", "tokens": ["Will", "mich", "mit", "ihm", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Der Rath war \u00fcberraschend neu", "tokens": ["Der", "Rath", "war", "\u00fc\u00b7berr\u00b7a\u00b7schend", "neu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tapfern wie den Feigen.", "tokens": ["Den", "Tap\u00b7fern", "wie", "den", "Fei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein jeder sprach: bei meiner Treu!", "tokens": ["Ein", "je\u00b7der", "sprach", ":", "bei", "mei\u00b7ner", "Treu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin kein Tieger, bin kein Leu,", "tokens": ["Ich", "bin", "kein", "Tie\u00b7ger", ",", "bin", "kein", "Leu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will mich menschlich zeigen.", "tokens": ["Ich", "will", "mich", "menschlich", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und so auch dachte bald der Feind,", "tokens": ["Und", "so", "auch", "dach\u00b7te", "bald", "der", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er lie\u00df die Fahnen senken:", "tokens": ["Er", "lie\u00df", "die", "Fah\u00b7nen", "sen\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen br\u00fcderlich vereint,", "tokens": ["Wir", "wol\u00b7len", "br\u00fc\u00b7der\u00b7lich", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lang uns noch die Sonne scheint,", "tokens": ["So", "lang", "uns", "noch", "die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An etwas Bessers denken. \u2014", "tokens": ["An", "et\u00b7was", "Bes\u00b7sers", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da zechten wir auf den Vertrag,", "tokens": ["Da", "zech\u00b7ten", "wir", "auf", "den", "Ver\u00b7trag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und sangen Friedenslieder;", "tokens": ["Und", "san\u00b7gen", "Frie\u00b7dens\u00b7lie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als vorbei war das Gelag,", "tokens": ["Und", "als", "vor\u00b7bei", "war", "das", "Ge\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sprach Jeder: ach, wann kommt der Tag,", "tokens": ["Sprach", "Je\u00b7der", ":", "ach", ",", "wann", "kommt", "der", "Tag", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$.", "XY", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann schlagen wir uns wieder!", "tokens": ["Wann", "schla\u00b7gen", "wir", "uns", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}