{"textgrid.poem.50746": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "13", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas ist ein Schreier und ein dummer Prahler,", "tokens": ["\u00bb", "das", "ist", "ein", "Schrei\u00b7er", "und", "ein", "dum\u00b7mer", "Prah\u00b7ler", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein m\u00fcder Drescher auf gedroschnen Halmen,", "tokens": ["Ein", "m\u00fc\u00b7der", "Dre\u00b7scher", "auf", "ge\u00b7drosc\u00b7hnen", "Hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein R\u00e4uchlein mehr in der Emp\u00f6rung Qualmen,", "tokens": ["Ein", "R\u00e4uch\u00b7lein", "mehr", "in", "der", "Em\u00b7p\u00f6\u00b7rung", "Qual\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Vielversprecher, jedoch schlechter Zahler!\u00ab", "tokens": ["Ein", "Viel\u00b7ver\u00b7spre\u00b7cher", ",", "je\u00b7doch", "schlech\u00b7ter", "Zah\u00b7ler", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gemach, gemach, Philistertro\u00df, du kahler!", "tokens": ["Ge\u00b7mach", ",", "ge\u00b7mach", ",", "Phi\u00b7lis\u00b7ter\u00b7tro\u00df", ",", "du", "kah\u00b7ler", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "NE", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht bei dir such noch find ich meine Palmen;", "tokens": ["Nicht", "bei", "dir", "such", "noch", "find", "ich", "mei\u00b7ne", "Pal\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "PRF", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "S\u00e4ng ich gleich David auch die hehrsten Psalmen,", "tokens": ["S\u00e4ng", "ich", "gleich", "Da\u00b7vid", "auch", "die", "hehrs\u00b7ten", "Psal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Sie w\u00fcrden durch dein Lob um soviel schaler!", "tokens": ["Sie", "w\u00fcr\u00b7den", "durch", "dein", "Lob", "um", "so\u00b7viel", "scha\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich geb es zu, ich habe arg geschrieen,", "tokens": ["Ich", "geb", "es", "zu", ",", "ich", "ha\u00b7be", "arg", "ge\u00b7schri\u00b7een", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als tr\u00fcbes Echo von geweihtern T\u00f6nen,", "tokens": ["Als", "tr\u00fc\u00b7bes", 