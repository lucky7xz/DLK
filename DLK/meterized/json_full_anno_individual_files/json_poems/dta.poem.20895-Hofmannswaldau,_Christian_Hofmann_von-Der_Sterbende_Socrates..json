{"dta.poem.20895": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich meint\u2019 ich h\u00e4tte guten Fug/", "tokens": ["Ich", "meint'", "ich", "h\u00e4t\u00b7te", "gu\u00b7ten", "Fug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meinen Meister nun zu sterben/", "tokens": ["Auf", "mei\u00b7nen", "Meis\u00b7ter", "nun", "zu", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich erfuhr/ da\u00df sein Betrug", "tokens": ["Als", "ich", "er\u00b7fuhr", "/", "da\u00df", "sein", "Be\u00b7trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich schlechte K\u00fcnste lie\u00df erwerben.", "tokens": ["Mich", "schlech\u00b7te", "K\u00fcns\u00b7te", "lie\u00df", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sein Geist war gantz und gar verblend/", "tokens": ["Sein", "Geist", "war", "gantz", "und", "gar", "ver\u00b7blend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KON", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit der Finsterni\u00df umwunden/", "tokens": ["Und", "mit", "der", "Fins\u00b7ter\u00b7ni\u00df", "um\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein andre Ursach hab ich funden/", "tokens": ["Kein", "and\u00b7re", "Ur\u00b7sach", "hab", "ich", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als da\u00df ers Himmels-Geister nennt.", "tokens": ["Als", "da\u00df", "ers", "Him\u00b7mels\u00b7Geis\u00b7ter", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein nichtig eingebildtes Wesen/", "tokens": ["Ein", "nich\u00b7tig", "ein\u00b7ge\u00b7bild\u00b7tes", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Lufft und Feuer angethan/", "tokens": ["Mit", "Lufft", "und", "Feu\u00b7er", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin erz\u00f6rnt/ da\u00df ich gelesen/", "tokens": ["Ich", "bin", "er\u00b7z\u00f6rnt", "/", "da\u00df", "ich", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "KOUS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was mich so schlecht erbauen kan.", "tokens": ["Was", "mich", "so", "schlecht", "er\u00b7bau\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}