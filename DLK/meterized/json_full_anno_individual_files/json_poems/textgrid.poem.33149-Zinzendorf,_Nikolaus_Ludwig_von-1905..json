{"textgrid.poem.33149": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1905.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochgelobter Gott und Herr, Gott der kirchenglieder! es dankt dir ein ganzes heer unserer gebr\u00fcder", "tokens": ["Hoch\u00b7ge\u00b7lob\u00b7ter", "Gott", "und", "Herr", ",", "Gott", "der", "kir\u00b7chen\u00b7glie\u00b7der", "!", "es", "dankt", "dir", "ein", "gan\u00b7zes", "heer", "un\u00b7se\u00b7rer", "ge\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+--+--+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "F\u00fcr das liebe gnadenkind weiland Jesum Christum; unsre freuden-thr\u00e4ne rinnt, cum cantamus Istum.", "tokens": ["F\u00fcr", "das", "lie\u00b7be", "gna\u00b7den\u00b7kind", "wei\u00b7land", "Je\u00b7sum", "Chris\u00b7tum", ";", "uns\u00b7re", "freu\u00b7den\u00b7thr\u00e4\u00b7ne", "rinnt", ",", "cum", "can\u00b7ta\u00b7mus", "Is\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NE", "NE", "$.", "PPOSAT", "ADJA", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Seit die Kirch ihr L\u00e4mmelein nicht gesehen hatte, warest du der Creuz-Gemein unverwandter gatte.", "tokens": ["Seit", "die", "Kirch", "ihr", "L\u00e4m\u00b7mel\u00b7ein", "nicht", "ge\u00b7s