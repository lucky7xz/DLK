{"dta.poem.15643": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wenn du willst ein Bild von deiner Freiheit haben,", "tokens": ["Sieh", ",", "wenn", "du", "willst", "ein", "Bild", "von", "dei\u00b7ner", "Frei\u00b7heit", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Menschenwillk\u00fcr kann auf Erden baun und graben.", "tokens": ["Was", "Men\u00b7schen\u00b7will\u00b7k\u00fcr", "kann", "auf", "Er\u00b7den", "baun", "und", "gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "APPR", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Man baut sohoch man will, man gr\u00e4bt sotief man kann,", "tokens": ["Man", "baut", "so\u00b7hoch", "man", "will", ",", "man", "gr\u00e4bt", "so\u00b7ti\u00b7ef", "man", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "VMFIN", "$,", "PIS", "VVFIN", "VVFIN", "PIS", "VMFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Erde Gleichgewicht nimmt keinen Schaden dran.", "tokens": ["Der", "Er\u00b7de", "Gleich\u00b7ge\u00b7wicht", "nimmt", "kei\u00b7nen", "Scha\u00b7den", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So wirkst du v\u00f6llig frei in deinem Wirkungskreise,", "tokens": ["So", "wirkst", "du", "v\u00f6l\u00b7lig", "frei", "in", "dei\u00b7nem", "Wir\u00b7kungs\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bringst den Gang der Welt dadurch nicht aus dem Gleise.", "tokens": ["Und", "bringst", "den", "Gang", "der", "Welt", "da\u00b7durch", "nicht", "aus", "dem", "Glei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PAV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Des K\u00fcnstlers gro\u00dfe Kunst ist dis, da\u00df sich ergebe", "tokens": ["Des", "K\u00fcnst\u00b7lers", "gro\u00b7\u00dfe", "Kunst", "ist", "dis", ",", "da\u00df", "sich", "er\u00b7ge\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PDS", "$,", "KOUS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus soviel Freiheit ein Nothwendigkeitsgewebe.", "tokens": ["Aus", "so\u00b7viel", "Frei\u00b7heit", "ein", "Noth\u00b7wen\u00b7dig\u00b7keits\u00b7ge\u00b7we\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}