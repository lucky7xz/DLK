{"textgrid.poem.40410": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pfahlmann", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dichtqualmende Nebel umfeuchten", "tokens": ["Dicht\u00b7qual\u00b7men\u00b7de", "Ne\u00b7bel", "um\u00b7feuch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein Pfahlbauger\u00fcstwerk im See,", "tokens": ["Ein", "Pfahl\u00b7bau\u00b7ge\u00b7r\u00fcst\u00b7werk", "im", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und fern ob der Waldwildnis leuchten", "tokens": ["Und", "fern", "ob", "der", "Wald\u00b7wild\u00b7nis", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOUS", "ART", "NN", "VVINF"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Alpen in ewigem Schnee.", "tokens": ["Die", "Al\u00b7pen", "in", "e\u00b7wi\u00b7gem", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ein Mann sitzt auf h\u00f6lzernem Stege", "tokens": ["Ein", "Mann", "sitzt", "auf", "h\u00f6l\u00b7zer\u00b7nem", "Ste\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In Felle geh\u00fcllt, denn es zieht;", "tokens": ["In", "Fel\u00b7le", "ge\u00b7h\u00fcllt", ",", "denn", "es", "zieht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er schnipft mit der Feuersteins\u00e4ge", "tokens": ["Er", "schnipft", "mit", "der", "Feu\u00b7ers\u00b7tein\u00b7s\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein Hirschhorn und summelt sein Lied:", "tokens": ["Ein", "Hirschhorn", "und", "sum\u00b7melt", "sein", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "\u00bbda seht mein verschwollen Gesichte", "tokens": ["\u00bb", "da", "seht", "mein", "ver\u00b7schwol\u00b7len", "Ge\u00b7sich\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und seht, wie bei Durchzug und Wind", "tokens": ["Und", "seht", ",", "wie", "bei", "Durch\u00b7zug", "und", "Wind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der Ureurop\u00e4er Geschichte", "tokens": ["Der", "Ur\u00b7eu\u00b7ro\u00b7p\u00e4\u00b7er", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mit Rheuma und Zahnweh beginnt.", "tokens": ["Mit", "Rheu\u00b7ma", "und", "Zahn\u00b7weh", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Zwar klopf' ich mit steinernen Beilen", "tokens": ["Zwar", "klopf'", "ich", "mit", "stei\u00b7ner\u00b7nen", "Bei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und Keulen mir Bahn durch die Welt,", "tokens": ["Und", "Keu\u00b7len", "mir", "Bahn", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch ist ein gem\u00fctlich Verweilen", "tokens": ["Doch", "ist", "ein", "ge\u00b7m\u00fct\u00b7lich", "Ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJD", "NN"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Noch t\u00e4glich in Frage gestellt.", "tokens": ["Noch", "t\u00e4g\u00b7lich", "in", "Fra\u00b7ge", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Im Wald st\u00f6rt das Raubtier mit Schreien", "tokens": ["Im", "Wald", "st\u00f6rt", "das", "Raub\u00b7tier", "mit", "Schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den Schlaf im durchh\u00f6hleten Stamm,", "tokens": ["Den", "Schlaf", "im", "durch\u00b7h\u00f6h\u00b7le\u00b7ten", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und bau' ich mein H\u00fcttlein im Freien,", "tokens": ["Und", "bau'", "ich", "mein", "H\u00fctt\u00b7lein", "im", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So stampft mir's der Urochs zusamm'.", "tokens": ["So", "stampft", "mir's", "der", "U\u00b7rochs", "zu\u00b7samm'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Drum lernt' ich vom biederen Biber", "tokens": ["Drum", "lernt'", "ich", "vom", "bie\u00b7de\u00b7ren", "Bi\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und stelle als Wohnungsbehilf,", "tokens": ["Und", "stel\u00b7le", "als", "Woh\u00b7nungs\u00b7be\u00b7hilf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Je weiter vom Festland je lieber,", "tokens": ["Je", "wei\u00b7ter", "vom", "Fest\u00b7land", "je", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ADV", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Pfahldamm in Seegrund und Schilf.", "tokens": ["Den", "Pfahl\u00b7damm", "in", "See\u00b7grund", "und", "Schilf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Auch hier mu\u00df ich vieles noch meiden,", "tokens": ["Auch", "hier", "mu\u00df", "ich", "vie\u00b7les", "noch", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Was sp\u00e4terer Zeit einst gef\u00e4llt:", "tokens": ["Was", "sp\u00e4\u00b7te\u00b7rer", "Zeit", "einst", "ge\u00b7f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Gern tr\u00fcg' ich ein Schwert an der Seiten", "tokens": ["Gern", "tr\u00fcg'", "ich", "ein", "Schwert", "an", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u2013 Es gibt weder Eisen noch Geld.", "tokens": ["\u2013", "Es", "gibt", "we\u00b7der", "Ei\u00b7sen", "noch", "Geld", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Gern z\u00f6g' ich Gewinn vom Papiere", "tokens": ["Gern", "z\u00f6g'", "ich", "Ge\u00b7winn", "vom", "Pa\u00b7pie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u2013 Noch sind keine B\u00f6rsen gebaut;", "tokens": ["\u2013", "Noch", "sind", "kei\u00b7ne", "B\u00f6r\u00b7sen", "ge\u00b7baut", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gern ging' ich des Abends zum Biere", "tokens": ["Gern", "ging'", "ich", "des", "A\u00b7bends", "zum", "Bie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u2013 Es wird noch keines gebraut.", "tokens": ["\u2013", "Es", "wird", "noch", "kei\u00b7nes", "ge\u00b7braut", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Und denk' ich der Art, wie wir kochen,", "tokens": ["Und", "denk'", "ich", "der", "Art", ",", "wie", "wir", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gesteh' ich selber: 's ist arg.", "tokens": ["Ge\u00b7steh'", "ich", "sel\u00b7ber", ":", "'s", "ist", "arg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir spalten dem Torfschwein die Knochen", "tokens": ["Wir", "spal\u00b7ten", "dem", "Torf\u00b7schwein", "die", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und saugen als Kraftsaft das Mark.", "tokens": ["Und", "sau\u00b7gen", "als", "Kraft\u00b7saft", "das", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Wie kann sich der Geist da schon lenken", "tokens": ["Wie", "kann", "sich", "der", "Geist", "da", "schon", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PRF", "ART", "NN", "ADV", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf h\u00f6h'res Kulturideal?", "tokens": ["Auf", "h\u00f6h'\u00b7res", "Kul\u00b7tu\u00b7ri\u00b7de\u00b7al", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "In all unserm F\u00fchlen und Denken", "tokens": ["In", "all", "un\u00b7serm", "F\u00fch\u00b7len", "und", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Steckt rammeltief Pfahl neben Pfahl.\u00ab", "tokens": ["Steckt", "ram\u00b7mel\u00b7tief", "Pfahl", "ne\u00b7ben", "Pfahl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "VVFIN", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Der Mann sang's mit heiserer Kehle,", "tokens": ["Der", "Mann", "sang's", "mit", "hei\u00b7se\u00b7rer", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da schwoll mit dem Rheuma sein Grimm,", "tokens": ["Da", "schwoll", "mit", "dem", "Rheu\u00b7ma", "sein", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NE", "PPOSAT", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Zwei B\u00e4ren beschlichen die Pf\u00e4hle", "tokens": ["Zwei", "B\u00e4\u00b7ren", "be\u00b7schli\u00b7chen", "die", "Pf\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und schnupperten kletternd nach ihm.", "tokens": ["Und", "schnup\u00b7per\u00b7ten", "klet\u00b7ternd", "nach", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Da schmi\u00df er zum Pfahlk\u00fcchenkehricht", "tokens": ["Da", "schmi\u00df", "er", "zum", "Pfahl\u00b7k\u00fc\u00b7chen\u00b7keh\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beil, Hirschhorn und Trinkkrug von Ton,", "tokens": ["Beil", ",", "Hirschhorn", "und", "Trink\u00b7krug", "von", "Ton", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sprang husch! wie ein Frosch ins Ger\u00f6hricht", "tokens": ["Sprang", "husch", "!", "wie", "ein", "Frosch", "ins", "Ge\u00b7r\u00f6h\u00b7richt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$.", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und schwamm mit Fluchen davon.", "tokens": ["Und", "schwamm", "mit", "Flu\u00b7chen", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Wo einst man die St\u00e4tte errichtet", "tokens": ["Wo", "einst", "man", "die", "St\u00e4t\u00b7te", "er\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Zum keltischen Seeh\u00fcttendorf,", "tokens": ["Zum", "kel\u00b7ti\u00b7schen", "See\u00b7h\u00fct\u00b7ten\u00b7dorf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ruht jetzt eine Fundschicht geschichtet,", "tokens": ["Ruht", "jetzt", "ei\u00b7ne", "Fund\u00b7schicht", "ge\u00b7schich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Tief unter dem Seeschlamm und Torf.", "tokens": ["Tief", "un\u00b7ter", "dem", "See\u00b7schlamm", "und", "Torf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.14": {"line.1": {"text": "Der diesen Gesang schuf zum Singen,", "tokens": ["Der", "die\u00b7sen", "Ge\u00b7sang", "schuf", "zum", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hat selber den Moder durchw\u00fchlt", "tokens": ["Hat", "sel\u00b7ber", "den", "Mo\u00b7der", "durch\u00b7w\u00fchlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und bei den gefundenen Dingen", "tokens": ["Und", "bei", "den", "ge\u00b7fun\u00b7de\u00b7nen", "Din\u00b7