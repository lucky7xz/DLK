{"textgrid.poem.50745": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbist wohl ein Volk, so frei von allen Plagen", "tokens": ["\u00bb", "ist", "wohl", "ein", "Volk", ",", "so", "frei", "von", "al\u00b7len", "Pla\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Die andrer Nationen Erbteil sind \u2013", "tokens": ["\u2013", "Die", "an\u00b7drer", "Na\u00b7ti\u00b7o\u00b7nen", "Erb\u00b7teil", "sind", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein bl\u00fchender, gl\u00fcckselig Heldenkind", "tokens": ["Ein", "bl\u00fc\u00b7hen\u00b7der", ",", "gl\u00fcck\u00b7se\u00b7lig", "Hel\u00b7den\u00b7kind"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJD", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als unser Schweizerv\u00f6lklein zu erfragen?", "tokens": ["Als", "un\u00b7ser", "Schwei\u00b7zer\u00b7v\u00f6l\u00b7klein", "zu", "er\u00b7fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und doch so fiebrisch seine Pulse schlagen!", "tokens": ["Und", "doch", "so", "fie\u00b7brisch", "sei\u00b7ne", "Pul\u00b7se", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr seiner Freiheit reichen Segen blind,", "tokens": ["F\u00fcr", "sei\u00b7ner", "Frei\u00b7heit", "rei\u00b7chen", "Se\u00b7gen", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hascht \u00fcberm\u00fctig es nach eitlem Wind;", "tokens": ["Hascht", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", "es", "nach", "eit\u00b7lem", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wann enden seine undankbaren Klagen?\u00ab", "tokens": ["Wann", "en\u00b7den", "sei\u00b7ne", "un\u00b7dank\u00b7ba\u00b7ren", "Kla\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sprechen, die mit t\u00fcckischem Verlangen", "tokens": ["So", "spre\u00b7chen", ",", "die", "mit", "t\u00fc\u00b7cki\u00b7schem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Tr\u00fcmmerschutt der alten Babel schleichen,", "tokens": ["Im", "Tr\u00fcm\u00b7mer\u00b7schutt", "der", "al\u00b7ten", "Ba\u00b7bel", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geh\u00fcllt in der Vernichtung Leichentuch!", "tokens": ["Ge\u00b7h\u00fcllt", "in", "der", "Ver\u00b7nich\u00b7tung", "Lei\u00b7chen\u00b7tuch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir aber sprechen: \u00bbJa, ihr falschen Schlangen,", "tokens": ["Wir", "a\u00b7ber", "spre\u00b7chen", ":", "\u00bb", "Ja", ",", "ihr", "fal\u00b7schen", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$.", "$(", "PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur euch, nur euch gilt es noch zu erreichen,", "tokens": ["Nur", "euch", ",", "nur", "euch", "gilt", "es", "noch", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und aufgehoben ist der letzte Fluch!\u00ab", "tokens": ["Und", "auf\u00b7ge\u00b7ho\u00b7ben", "ist", "der", "letz\u00b7te", "Fluch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbist wohl ein Volk, so frei von allen Plagen", "tokens": ["\u00bb", "ist", "wohl", "ein", "Volk", ",", "so", "frei", "von", "al\u00b7len", "Pla\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "PIAT", "NN"], "meter": "