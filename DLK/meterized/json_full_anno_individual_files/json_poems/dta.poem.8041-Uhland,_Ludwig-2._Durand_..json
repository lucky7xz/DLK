{"dta.poem.8041": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Durand .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Nach dem hohen Schlo\u00df von Balbi", "tokens": ["Nach", "dem", "ho\u00b7hen", "Schlo\u00df", "von", "Bal\u00b7bi"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht Durand mit seinem Spiele;", "tokens": ["Zieht", "Du\u00b7rand", "mit", "sei\u00b7nem", "Spie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Voll die Brust von s\u00fc\u00dfen Liedern,", "tokens": ["Voll", "die", "Brust", "von", "s\u00fc\u00b7\u00dfen", "Lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Naht er schon dem frohen Ziele.", "tokens": ["Naht", "er", "schon", "dem", "fro\u00b7hen", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort ja wird ein holdes Fr\u00e4ulein,", "tokens": ["Dort", "ja", "wird", "ein", "hol\u00b7des", "Fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Wann die Saiten lieblich rauschen,", "tokens": ["Wann", "die", "Sai\u00b7ten", "lieb\u00b7lich", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Augen senkend, zart ergl\u00fchend,", "tokens": ["Au\u00b7gen", "sen\u00b7kend", ",", "zart", "er\u00b7gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Innig athmend, niederlauschen.", "tokens": ["In\u00b7nig", "ath\u00b7mend", ",", "nie\u00b7der\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In des Hofes Lindenschatten", "tokens": ["In", "des", "Ho\u00b7fes", "Lin\u00b7den\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hat er schon sein Spiel begonnen,", "tokens": ["Hat", "er", "schon", "sein", "Spiel", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Singt er schon mit klarer Stimme", "tokens": ["Singt", "er", "schon", "mit", "kla\u00b7rer", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Was er s\u00fc\u00dfestes ersonnen.", "tokens": ["Was", "er", "s\u00fc\u00b7\u00dfes\u00b7tes", "er\u00b7son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Von dem S\u00f6ller, von den Fenstern", "tokens": ["Von", "dem", "S\u00f6l\u00b7ler", ",", "von", "den", "Fens\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sieht er Blumen freundlich nicken,", "tokens": ["Sieht", "er", "Blu\u00b7men", "freund\u00b7lich", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Doch die Herrin seiner Lieder", "tokens": ["Doch", "die", "Her\u00b7rin", "sei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Kann sein Auge nicht erblicken.", "tokens": ["Kann", "sein", "Au\u00b7ge", "nicht", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und es geht ein Mann vor\u00fcber,", "tokens": ["Und", "es", "geht", "ein", "Mann", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Der sich traurig zu ihm wendet:", "tokens": ["Der", "sich", "trau\u00b7rig", "zu", "ihm", "wen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "\u201est\u00f6re nicht die Ruh der Todten!", "tokens": ["\u201e", "st\u00f6\u00b7re", "nicht", "die", "Ruh", "der", "Tod\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Fr\u00e4ulein Blanka hat vollendet.\u201c", "tokens": ["Fr\u00e4u\u00b7lein", "Blan\u00b7ka", "hat", "voll\u00b7en\u00b7det", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Doch Durand, der junge S\u00e4nger,", "tokens": ["Doch", "Du\u00b7rand", ",", "der", "jun\u00b7ge", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Hat darauf kein Wort gesprochen,", "tokens": ["Hat", "da\u00b7rauf", "kein", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Ach! sein Aug\u2019 ist schon erloschen,", "tokens": ["Ach", "!", "sein", "Aug'", "ist", "schon", "er\u00b7lo\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ach! sein Herz ist schon gebrochen.", "tokens": ["Ach", "!", "sein", "Herz", "ist", "schon", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Dr\u00fcben in der Burgkapelle,", "tokens": ["Dr\u00fc\u00b7ben", "in", "der", "Burg\u00b7ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wo unz\u00e4hl\u2019ge Kerzen gl\u00e4nzen,", "tokens": ["Wo", "un\u00b7z\u00e4hl'\u00b7ge", "Ker\u00b7zen", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Wo das todte Fr\u00e4ulein ruht,", "tokens": ["Wo", "das", "tod\u00b7te", "Fr\u00e4u\u00b7lein", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Hold geschm\u00fcckt mit Blumenkr\u00e4nzen:", "tokens": ["Hold", "ge\u00b7schm\u00fcckt", "mit", "Blu\u00b7men\u00b7kr\u00e4n\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Dort ergreifet alles Volk", "tokens": ["Dort", "er\u00b7grei\u00b7fet", "al\u00b7les", "Volk"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Schreck und Staunen, freudig Beben,", "tokens": ["Schreck", "und", "Stau\u00b7nen", ",", "freu\u00b7dig", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Denn von ihrem Todtenlager", "tokens": ["Denn", "von", "ih\u00b7rem", "Tod\u00b7ten\u00b7la\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Sieht man Blanka sich erheben.", "tokens": ["Sieht", "man", "Blan\u00b7ka", "sich", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NE", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Aus des Scheintods tiefem Schlummer", "tokens": ["Aus", "des", "Schein\u00b7tods", "tie\u00b7fem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Ist sie bl\u00fchend auferstanden,", "tokens": ["Ist", "sie", "bl\u00fc\u00b7hend", "auf\u00b7er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Tritt im Sterbekleid hervor", "tokens": ["Tritt", "im", "Ster\u00b7be\u00b7kleid", "her\u00b7vor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wie in br\u00e4utlichen Gewanden.", "tokens": ["Wie", "in", "br\u00e4ut\u00b7li\u00b7chen", "Ge\u00b7wan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Noch, wie ihr geschehn, nicht wissend,", "tokens": ["Noch", ",", "wie", "ihr", "ge\u00b7schehn", ",", "nicht", "wis\u00b7send", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVPP", "$,", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wie von Tr\u00e4umen noch umschlungen,", "tokens": ["Wie", "von", "Tr\u00e4u\u00b7men", "noch", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Fragt sie z\u00e4rtlich, sehnsuchtsvoll:", "tokens": ["Fragt", "sie", "z\u00e4rt\u00b7lich", ",", "sehn\u00b7suchts\u00b7voll", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "\u201ehat nicht hier Durand gesungen?\u201c", "tokens": ["\u201e", "hat", "nicht", "hier", "Du\u00b7rand", "ge\u00b7sun\u00b7gen", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "ADV", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Ja! gesungen hat Durand,", "tokens": ["Ja", "!", "ge\u00b7sun\u00b7gen", "hat", "Du\u00b7rand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVPP", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Aber nie mehr wird er singen,", "tokens": ["A\u00b7ber", "nie", "mehr", "wird", "er", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Auferweckt hat er die Todte,", "tokens": ["Au\u00b7fer\u00b7weckt", "hat", "er", "die", "Tod\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Ihn wird Niemand wiederbringen.", "tokens": ["Ihn", "wird", "Nie\u00b7mand", "wie\u00b7der\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Schon im Lande der Verkl\u00e4rten", "tokens": ["Schon", "im", "Lan\u00b7de", "der", "Ver\u00b7kl\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Wacht\u2019 er auf und mit Verlangen", "tokens": ["Wacht'", "er", "auf", "und", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Sucht er seine s\u00fc\u00dfe Freundin,", "tokens": ["Sucht", "er", "sei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Freun\u00b7din", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Die er w\u00e4hnt vorangegangen;", "tokens": ["Die", "er", "w\u00e4hnt", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Aller Himmel lichte R\u00e4ume", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "lich\u00b7te", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Sieht er herrlich sich verbreiten;", "tokens": ["Sieht", "er", "herr\u00b7lich", "sich", "ver\u00b7brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Blanka! Blanka! ruft er sehnlich", "tokens": ["Blan\u00b7ka", "!", "Blan\u00b7ka", "!", "ruft", "er", "sehn\u00b7lich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Durch die \u00f6den Seligkeiten.", "tokens": ["Durch", "die", "\u00f6\u00b7den", "Se\u00b7lig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}