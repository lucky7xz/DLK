{"textgrid.poem.56761": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf einer Wiese, der sich hier und dort", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einer Wiese, der sich hier und dort", "tokens": ["Auf", "ei\u00b7ner", "Wie\u00b7se", ",", "der", "sich", "hier", "und", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein reich beschwerter Apfelbaum enthob,", "tokens": ["ein", "reich", "be\u00b7schwer\u00b7ter", "Ap\u00b7fel\u00b7baum", "ent\u00b7hob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erg\u00f6tzten wir, ein H\u00e4uflein Freunde, uns,", "tokens": ["er\u00b7g\u00f6tz\u00b7ten", "wir", ",", "ein", "H\u00e4uf\u00b7lein", "Freun\u00b7de", ",", "uns", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mit gr\u00fcnem Obst uns scherzend zu bekriegen.", "tokens": ["mit", "gr\u00fc\u00b7nem", "Obst", "uns", "scher\u00b7zend", "zu", "be\u00b7krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich lag im Gras, entsandte, deckte mich,", "tokens": ["Ich", "lag", "im", "Gras", ",", "ent\u00b7sand\u00b7te", ",", "deck\u00b7te", "mich", ","], "token_info": ["wo