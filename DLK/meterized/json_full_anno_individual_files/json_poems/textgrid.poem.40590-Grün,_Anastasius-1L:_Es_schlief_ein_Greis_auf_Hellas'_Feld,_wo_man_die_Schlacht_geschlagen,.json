: "iambic.septa"}, "line.2": {"text": "\u00bbdu sel'ge Schaar, wie schl\u00e4fst du s\u00fc\u00df im freien Himmelssaale!", "tokens": ["\u00bb", "du", "sel'\u00b7ge", "Schaar", ",", "wie", "schl\u00e4fst", "du", "s\u00fc\u00df", "im", "frei\u00b7en", "Him\u00b7mels\u00b7saa\u00b7le", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Nicht scheinest du des W\u00fcthrichs Ruf, nicht R\u00e4uberschwert zu f\u00fcrchten;", "tokens": ["Nicht", "schei\u00b7nest", "du", "des", "W\u00fcth\u00b7richs", "Ruf", ",", "nicht", "R\u00e4u\u00b7ber\u00b7schwert", "zu", "f\u00fcrch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "NN", "$,", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "So sicher, traun, und friedlich schl\u00e4ft sich's nur im Land der Freiheit!\u00ab", "tokens": ["So", "si\u00b7cher", ",", "traun", ",", "und", "fried\u00b7lich", "schl\u00e4ft", "sich's", "nur", "im", "Land", "der", "Frei\u00b7heit", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "VVINF", "$,", "KON", "ADJD", "VVFIN", "PIS", "ADV", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.11": {"line.1": {"text": "Und neben ihm, im gr\u00fcnen Gras, da ruhn zwei holde Kinder,", "tokens": ["Und", "ne\u00b7ben", "ihm", ",", "im", "gr\u00fc\u00b7nen", "Gras", ",", "da", "ruhn", "zwei", "hol\u00b7de", "Kin\u00b7der", ","], "token_info": ["word", "word", "w