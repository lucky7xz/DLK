{"textgrid.poem.40590": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schlief ein Greis auf Hellas' Feld, wo man die Schlacht geschlagen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schlief ein Greis auf Hellas' Feld, wo man die Schlacht geschlagen,", "tokens": ["Es", "schlief", "ein", "Greis", "auf", "Hel\u00b7las'", "Feld", ",", "wo", "man", "die", "Schlacht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,", "PWAV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Er schlief wohl an zehn Stunden schon, seit ausgetobt der Schlachtl\u00e4rm,", "tokens": ["Er", "schlief", "wohl", "an", "zehn", "Stun\u00b7den", "schon", ",", "seit", "aus\u00b7ge\u00b7tobt", "der", "Schlacht\u00b7l\u00e4rm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "CARD", "NN", "ADV", "$,", "APPR", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und wer den grauen Schl\u00e4fer sah, seufzt: Friede mit den Todten!", "tokens": ["Und", "wer", "den", "grau\u00b7en", "Schl\u00e4\u00b7fer", "sah", ",", "seufzt", ":", "Frie\u00b7de", "mit", "den", "Tod\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Doch jetzt erhebt der Greis sein Haupt, reibt sich den Schlaf vom Auge.", "tokens": ["Doch", "jetzt", "er\u00b7hebt", "der", "Greis", "sein", "Haupt", ",", "reibt", "sich", "den", "Schlaf", "vom", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Es liegt ein stiller See vor ihm mit purpurrothen Wellen.", "tokens": ["Es", "liegt", "ein", "stil\u00b7ler", "See", "vor", "ihm", "mit", "pur\u00b7pur\u00b7ro\u00b7then", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdu ebner See,\u00ab so lispelt er, \u00bbwie friedlich flie\u00dft dein Wasser,", "tokens": ["\u00bb", "du", "eb\u00b7ner", "See", ",", "\u00ab", "so", "lis\u00b7pelt", "er", ",", "\u00bb", "wie", "fried\u00b7lich", "flie\u00dft", "dein", "Was\u00b7ser", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "$(", "ADV", "VVFIN", "PPER", "$,", "$(", "PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wie gl\u00fchen deine Wellen all' so sch\u00f6n im Morgenrothe!", "tokens": ["Wie", "gl\u00fc\u00b7hen", "dei\u00b7ne", "Wel\u00b7len", "all'", "so", "sch\u00f6n", "im", "Mor\u00b7gen\u00b7ro\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "PIS", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "So hehr ergl\u00e4nzt das Fr\u00fchroth nur im goldnen Land der Freiheit!\u00ab", "tokens": ["So", "hehr", "er\u00b7gl\u00e4nzt", "das", "Fr\u00fch\u00b7roth", "nur", "im", "gold\u00b7nen", "Land", "der", "Frei\u00b7heit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Viel hundert M\u00e4nner lagern rings am Strand des Sees und schlafen.", "tokens": ["Viel", "hun\u00b7dert", "M\u00e4n\u00b7ner", "la\u00b7gern", "rings", "am", "Strand", "des", "Sees", "und", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdu sel'ge Schaar, wie schl\u00e4fst du s\u00fc\u00df im freien Himmelssaale!", "tokens": ["\u00bb", "du", "sel'\u00b7ge", "Schaar", ",", "wie", "schl\u00e4fst", "du", "s\u00fc\u00df", "im", "frei\u00b7en", "Him\u00b7mels\u00b7saa\u00b7le", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Nicht scheinest du des W\u00fcthrichs Ruf, nicht R\u00e4uberschwert zu f\u00fcrchten;", "tokens": ["Nicht", "schei\u00b7nest", "du", "des", "W\u00fcth\u00b7richs", "Ruf", ",", "nicht", "R\u00e4u\u00b7ber\u00b7schwert", "zu", "f\u00fcrch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "NN", "$,", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "So sicher, traun, und friedlich schl\u00e4ft sich's nur im Land der Freiheit!\u00ab", "tokens": ["So", "si\u00b7cher", ",", "traun", ",", "und", "fried\u00b7lich", "schl\u00e4ft", "sich's", "nur", "im", "Land", "der", "Frei\u00b7heit", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "VVINF", "$,", "KON", "ADJD", "VVFIN", "PIS", "ADV", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Und neben ihm, im gr\u00fcnen Gras, da ruhn zwei holde Kinder,", "tokens": ["Und", "ne\u00b7ben", "ihm", ",", "im", "gr\u00fc\u00b7nen", "Gras", ",", "da", "ruhn", "zwei", "hol\u00b7de", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPRART", "ADJA", "NN", "$,", "ADV", "VVFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Zwar regungslos, doch halten sie sich treu und fest umschlungen.", "tokens": ["Zwar", "re\u00b7gungs\u00b7los", ",", "doch", "hal\u00b7ten", "sie", "sich", "treu", "und", "fest", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "\u00bbo sch\u00f6nes, zartes Blumenpaar, umkos't vom Hauch der Liebe!", "tokens": ["\u00bb", "o", "sch\u00f6\u00b7nes", ",", "zar\u00b7tes", "Blu\u00b7men\u00b7paar", ",", "um\u00b7kos't", "vom", "Hauch", "der", "Lie\u00b7be", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Solch s\u00fc\u00dfe, heil'ge Liebe lebt nur in dem Land der Freiheit.\u00ab", "tokens": ["Solch", "s\u00fc\u00b7\u00dfe", ",", "heil'\u00b7ge", "Lie\u00b7be", "lebt", "nur", "in", "dem", "Land", "der", "Frei\u00b7heit", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Es neigt gar mild sich \u00fcber ihn ein lieblich Frauenantlitz;", "tokens": ["Es", "neigt", "gar", "mild", "sich", "\u00fc\u00b7ber", "ihn", "ein", "lieb\u00b7lich", "Frau\u00b7en\u00b7ant\u00b7litz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PRF", "APPR", "PPER", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sein m\u00fcdes Silberhaupt ruht sanft im Schoo\u00df des sch\u00f6nen Weibes", "tokens": ["Sein", "m\u00fc\u00b7des", "Sil\u00b7ber\u00b7haupt", "ruht", "sanft", "im", "Schoo\u00df", "des", "sch\u00f6\u00b7nen", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "\u00bbauf solchen Kissen schl\u00e4ft man nur im sch\u00f6nen Land des Friedens", "tokens": ["\u00bb", "auf", "sol\u00b7chen", "Kis\u00b7sen", "schl\u00e4ft", "man", "nur", "im", "sch\u00f6\u00b7nen", "Land", "des", "Frie\u00b7dens"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIAT", "NN", "VVFIN", "PIS", "ADV", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und solche Engel wachen nur im goldnen Land der Freiheit!\u00ab", "tokens": ["Und", "sol\u00b7che", "En\u00b7gel", "wa\u00b7chen", "nur", "im", "gold\u00b7nen", "Land", "der", "Frei\u00b7heit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Er lispelt's leis und senkt das Haupt und schlie\u00dfet still das Auge,", "tokens": ["Er", "lis\u00b7pelt's", "leis", "und", "senkt", "das", "Haupt", "und", "schlie\u00b7\u00dfet", "still", "das", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und nimmer \u00f6ffnet es der Greis, erhebt nie mehr das Antlitz.", "tokens": ["Und", "nim\u00b7mer", "\u00f6ff\u00b7net", "es", "der", "Greis", ",", "er\u00b7hebt", "nie", "mehr", "das", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "O armer und doch sel'ger Greis, o schlafe fort und tr\u00e4ume!", "tokens": ["O", "ar\u00b7mer", "und", "doch", "sel'\u00b7ger", "Greis", ",", "o", "schla\u00b7fe", "fort", "und", "tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "KON", "ADV", "ADJA", "NN", "$,", "FM", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Erwache nie, da\u00df Keiner dir, was du gesehn, je deute!", "tokens": ["Er\u00b7wa\u00b7che", "nie", ",", "da\u00df", "Kei\u00b7ner", "dir", ",", "was", "du", "ge\u00b7sehn", ",", "je", "deu\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PIS", "PPER", "$,", "PWS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Nicht gl\u00fcht der See vom Fr\u00fchroth, nein, vom Blute deines Volkes!", "tokens": ["Nicht", "gl\u00fcht", "der", "See", "vom", "Fr\u00fch\u00b7roth", ",", "nein", ",", "vom", "Blu\u00b7te", "dei\u00b7nes", "Vol\u00b7kes", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PTKANT", "$,", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Die Schl\u00e4fer \u2013 deine Br\u00fcder sind's \u2013 erwachen nimmer wieder!", "tokens": ["Die", "Schl\u00e4\u00b7fer", "\u2013", "dei\u00b7ne", "Br\u00fc\u00b7der", "sin\u00b7d's", "\u2013", "er\u00b7wa\u00b7chen", "nim\u00b7mer", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPOSAT", "NN", "VAFIN", "$(", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die Kinder \u2013 deine Enkel sind's \u2013 die starben Hungertodes!", "tokens": ["Die", "Kin\u00b7der", "\u2013", "dei\u00b7ne", "En\u00b7kel", "sin\u00b7d's", "\u2013", "die", "star\u00b7ben", "Hun\u00b7ger\u00b7to\u00b7des", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPOSAT", "NN", "VAFIN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Das Frau'nbild \u2013 deine Tochter ist's \u2013 weint \u00fcber deiner Leiche!", "tokens": ["Das", "Frau'\u00b7nbild", "\u2013", "dei\u00b7ne", "Toch\u00b7ter", "ist's", "\u2013", "weint", "\u00fc\u00b7ber", "dei\u00b7ner", "Lei\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPOSAT", "NN", "VAFIN", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Es schlief ein Greis auf Hellas' Feld, wo man die Schlacht geschlagen,", "tokens": ["Es", "schlief", "ein", "Greis", "auf", "Hel\u00b7las'", "Feld", ",", "wo", "man", "die", "Schlacht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,", "PWAV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Er schlief wohl an zehn Stunden schon, seit ausgetobt der Schlachtl\u00e4rm,", "tokens": ["Er", "schlief", "wohl", "an", "zehn", "Stun\u00b7den", "schon", ",", "seit", "aus\u00b7ge\u00b7tobt", "der", "Schlacht\u00b7l\u00e4rm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "CARD", "NN", "ADV", "$,", "APPR", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und wer den grauen Schl\u00e4fer sah, seufzt: Friede mit den Todten!", "tokens": ["Und", "wer", "den", "grau\u00b7en", "Schl\u00e4\u00b7fer", "sah", ",", "seufzt", ":", "Frie\u00b7de", "mit", "den", "Tod\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Doch jetzt erhebt der Greis sein Haupt, reibt sich den Schlaf vom Auge.", "tokens": ["Doch", "jetzt", "er\u00b7hebt", "der", "Greis", "sein", "Haupt", ",", "reibt", "sich", "den", "Schlaf", "vom", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Es liegt ein stiller See vor ihm mit purpurrothen Wellen.", "tokens": ["Es", "liegt", "ein", "stil\u00b7ler", "See", "vor", "ihm", "mit", "pur\u00b7pur\u00b7ro\u00b7then", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "\u00bbdu ebner See,\u00ab so lispelt er, \u00bbwie friedlich flie\u00dft dein Wasser,", "tokens": ["\u00bb", "du", "eb\u0