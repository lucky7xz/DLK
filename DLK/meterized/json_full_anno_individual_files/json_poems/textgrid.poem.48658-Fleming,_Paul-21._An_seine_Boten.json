{"textgrid.poem.48658": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "21. An seine Boten", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht, ihr meine Tr\u00e4nen, geht", "tokens": ["Geht", ",", "ihr", "mei\u00b7ne", "Tr\u00e4\u00b7nen", ",", "geht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "PPER", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und erweichet der ihr Herze,", "tokens": ["und", "er\u00b7wei\u00b7chet", "der", "ihr", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die wie eine Klippe steht,", "tokens": ["die", "wie", "ei\u00b7ne", "Klip\u00b7pe", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "unbewegt von meinem Schmerze,", "tokens": ["un\u00b7be\u00b7wegt", "von", "mei\u00b7nem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die das, was mein Herze bricht,", "tokens": ["die", "das", ",", "was", "mein", "Her\u00b7ze", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "sieht und wills doch sehen nicht!", "tokens": ["sieht", "und", "wills", "doch", "se\u00b7hen", "nicht", "!"], "token_info": ["word", "word", "word", "word", 