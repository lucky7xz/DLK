{"dta.poem.3424": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Luisens Antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl weinen Gottes Engel,", "tokens": ["Wohl", "wei\u00b7nen", "Got\u00b7tes", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn Liebende sich trennen.", "tokens": ["Wenn", "Lie\u00b7ben\u00b7de", "sich", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie werd' ich leben k\u00f6nnen,", "tokens": ["Wie", "werd'", "ich", "le\u00b7ben", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geliebter, ohne dich!", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "oh\u00b7ne", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gestorben allen Freuden,", "tokens": ["Ge\u00b7stor\u00b7ben", "al\u00b7len", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Leb' ich fortan den Leiden,", "tokens": ["Leb'", "ich", "for\u00b7tan", "den", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Und nimmer, J\u00fcngling, nimmer", "tokens": ["Und", "nim\u00b7mer", ",", "J\u00fcng\u00b7ling", ",", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "NN", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergisst Luisa dich.", "tokens": ["Ver\u00b7gisst", "Lu\u00b7i\u00b7sa", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie k\u00f6nnt' ich dein vergessen!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dein", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergessen deines Blickens", "tokens": ["Ver\u00b7ges\u00b7sen", "dei\u00b7nes", "Bli\u00b7ckens"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll feurigen Entz\u00fcckens,", "tokens": ["Voll", "feu\u00b7ri\u00b7gen", "Ent\u00b7z\u00fc\u00b7ckens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll sanften Flehns an mich?", "tokens": ["Voll", "sanf\u00b7ten", "Flehns", "an", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In meines Kummers Dunkel", "tokens": ["In", "mei\u00b7nes", "Kum\u00b7mers", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Strahlt mirs, wie Sternenfunkel.", "tokens": ["Strahlt", "mirs", ",", "wie", "Ster\u00b7nen\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geliebter, nimmer, nimmer", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "nim\u00b7mer", ",", "nim\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADV", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergisst Luisa dich.", "tokens": ["Ver\u00b7gisst", "Lu\u00b7i\u00b7sa", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie k\u00f6nnt' ich dein vergessen!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dein", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Schimmers deiner Wangen,", "tokens": ["Des", "Schim\u00b7mers", "dei\u00b7ner", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ger\u00f6thet von Verlangen,", "tokens": ["Ge\u00b7r\u00f6\u00b7thet", "von", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Inbrunst nass um mich!", "tokens": ["Von", "In\u00b7brunst", "nass", "um", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So nass von heissen Thr\u00e4nen,", "tokens": ["So", "nass", "von", "heis\u00b7sen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So heiss von bangem Sehnen \u2014", "tokens": ["So", "heiss", "von", "ban\u00b7gem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geliebter, nimmer, nimmer", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "nim\u00b7mer", ",", "nim\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADV", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergisst Luisa dich.", "tokens": ["Ver\u00b7gisst", "Lu\u00b7i\u00b7sa", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie k\u00f6nnt' ich dein vergessen!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dein", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergessen hier und dorten,", "tokens": ["Ver\u00b7ges\u00b7sen", "hier", "und", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie mit so s\u00fcssen Worten", "tokens": ["Wie", "mit", "so", "s\u00fcs\u00b7sen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Zauber mich beschlich.", "tokens": ["Dein", "Zau\u00b7ber", "mich", "be\u00b7schlich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch h\u00f6r' ichs um mich flistern", "tokens": ["Noch", "h\u00f6r'", "ichs", "um", "mich", "flis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Stillen und im D\u00fcstern \u2014", "tokens": ["Im", "Stil\u00b7len", "und", "im", "D\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geliebter, nimmer, nimmer", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "nim\u00b7mer", ",", "nim\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADV", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergisst Luisa dich.", "tokens": ["Ver\u00b7gisst", "Lu\u00b7i\u00b7sa", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie k\u00f6nnt' ich dein vergessen!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dein", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergessen je des Klanges,", "tokens": ["Ver\u00b7ges\u00b7sen", "je", "des", "Klan\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So manches Wehgesanges,", "tokens": ["So", "man\u00b7ches", "Weh\u00b7ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dir geklagt um mich!", "tokens": ["Von", "dir", "ge\u00b7klagt", "um", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, unausl\u00f6schlich klingt es", "tokens": ["Ach", ",", "un\u00b7aus\u00b7l\u00f6schlich", "klingt", "es"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In meinen Ohren, singt es", "tokens": ["In", "mei\u00b7nen", "Oh\u00b7ren", ",", "singt", "es"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In meinem Herzen. Nimmer", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", ".", "Nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergisst Luisa dich.", "tokens": ["Ver\u00b7gisst", "Lu\u00b7i\u00b7sa", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie k\u00f6nnt' ich dein vergessen!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dein", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergessen deiner Briefe", "tokens": ["Ver\u00b7ges\u00b7sen", "dei\u00b7ner", "Brie\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll treuer reiner Liebe,", "tokens": ["Voll", "treu\u00b7er", "rei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Gr\u00e4mens, ach, um mich!", "tokens": ["Voll", "Gr\u00e4\u00b7mens", ",", "ach", ",", "um", "mich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ITJ", "$,", "KOUI", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich will sie sorgsam wahren,", "tokens": ["Ich", "will", "sie", "sorg\u00b7sam", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr meinen Sarg sie sparen.", "tokens": ["F\u00fcr", "mei\u00b7nen", "Sarg", "sie", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geliebter, nimmer, nimmer,", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "nim\u00b7mer", ",", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergisst Luisa dich.", "tokens": ["Ver\u00b7gisst", "Lu\u00b7i\u00b7sa", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie k\u00f6nnt' ich dein vergessen!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "dein", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergessen deiner K\u00fcsse!", "tokens": ["Ver\u00b7ges\u00b7sen", "dei\u00b7ner", "K\u00fcs\u00b7se", "!"