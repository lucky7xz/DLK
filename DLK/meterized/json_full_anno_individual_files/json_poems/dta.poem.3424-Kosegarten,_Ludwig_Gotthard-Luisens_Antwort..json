unct"], "pos": ["NE", "VVFIN", "ADV", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verschm\u00e4ht des Stutzers Schmeicheln,", "tokens": ["Ver\u00b7schm\u00e4ht", "des", "Stut\u00b7zers", "Schmei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00f6hnt sein s\u00fcsslich Heucheln,", "tokens": ["Ver\u00b7h\u00f6hnt", "sein", "s\u00fcss\u00b7lich", "Heu\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gedenkt, du Edler, dein;", "tokens": ["Ge\u00b7denkt", ",", "du", "Ed\u00b7ler", ",", "dein", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "NN", "$,", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denkt deines Geistes Adel,", "tokens": ["Denkt", "dei\u00b7nes", "Geis\u00b7tes", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Lieben sonder Tadel,", "tokens": ["Dein", "Lie\u00b7ben", "son\u00b7der", "Ta\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein Herz so treu, so bieder \u2014", "tokens": ["Dein", "Herz", "so", "treu", ",", "so", "bie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und brennt f\u00fcr dich allein.", "tokens": ["Und", "brennt", "f\u00fcr", "dich", "al\u00b7lein", "."], "token_info": ["wo