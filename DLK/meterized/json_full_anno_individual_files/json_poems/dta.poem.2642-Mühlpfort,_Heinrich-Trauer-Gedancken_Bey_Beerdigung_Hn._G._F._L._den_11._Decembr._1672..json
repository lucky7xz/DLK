{"dta.poem.2642": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Gedancken/  \n Bey Beerdigung Hn. G. F. L. den 11.  \n Decembr. 1672.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jjr Hochbek\u00fcmmerten/ die sieden-heisse Thr\u00e4nen/", "tokens": ["Jjr", "Hoch\u00b7be\u00b7k\u00fcm\u00b7mer\u00b7ten", "/", "die", "sie\u00b7den\u00b7heis\u00b7se", "Thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit ihr jetzt den Sarch des liebsten Vaters netzt/", "tokens": ["Wo\u00b7mit", "ihr", "jetzt", "den", "Sarch", "des", "liebs\u00b7ten", "Va\u00b7ters", "netzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seufftzer voller Angst/ das marterreiche Sehnen/", "tokens": ["Die", "Seufft\u00b7zer", "vol\u00b7ler", "Angst", "/", "das", "mar\u00b7ter\u00b7rei\u00b7che", "Seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind Zeugen welch ein Leid den matten Geist verletzt.", "tokens": ["Sind", "Zeu\u00b7gen", "welch", "ein", "Leid", "den", "mat\u00b7ten", "Geist", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PWAT", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eur Alles liegt verblast/ eur Hoffen ist verschwunden/", "tokens": ["Eur", "Al\u00b7les", "liegt", "ver\u00b7blast", "/", "eur", "Hof\u00b7fen", "ist", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "VVFIN", "VVPP", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Eur Helffer/ Rath und Schutz steht euch nicht ferner bey.", "tokens": ["Eur", "Helf\u00b7fer", "/", "Rath", "und", "Schutz", "steht", "euch", "nicht", "fer\u00b7ner", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach kl\u00e4glicher Verlust! Ach tieffe Seelen-Wunden!", "tokens": ["Ach", "kl\u00e4g\u00b7li\u00b7cher", "Ver\u00b7lust", "!", "Ach", "tief\u00b7fe", "See\u00b7len\u00b7Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sagt ob was schmertzlichers noch zu erfinden sey?", "tokens": ["Sagt", "ob", "was", "schmertz\u00b7li\u00b7chers", "noch", "zu", "er\u00b7fin\u00b7den", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PIS", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn sich das Licht der Welt/ der Sonnen g\u00fcldne Kertze", "tokens": ["Wenn", "sich", "das", "Licht", "der", "Welt", "/", "der", "Son\u00b7nen", "g\u00fcld\u00b7ne", "Kert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Finstern\u00fc\u00df verh\u00fcllt/ erschricket Feld und Land:", "tokens": ["In", "Fins\u00b7ter\u00b7n\u00fc\u00df", "ver\u00b7h\u00fcllt", "/", "er\u00b7schri\u00b7cket", "Feld", "und", "Land", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie vielmehr wird ein Kind/ wenn ihm sein Vater-Hertze/", "tokens": ["Wie", "viel\u00b7mehr", "wird", "ein", "Kind", "/", "wenn", "ihm", "sein", "Va\u00b7ter\u00b7Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Lebens-Sonn entweicht/ gesetzt in Trauerstand.", "tokens": ["Des", "Le\u00b7bens\u00b7Sonn", "ent\u00b7weicht", "/", "ge\u00b7setzt", "in", "Trau\u00b7er\u00b7stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir ehren einen Baum/ der Schatten uns gegeben/", "tokens": ["Wir", "eh\u00b7ren", "ei\u00b7nen", "Baum", "/", "der", "Schat\u00b7ten", "uns", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der uns so offt erfrischt mit seiner gr\u00fcnen Nacht/", "tokens": ["Der", "uns", "so", "offt", "er\u00b7frischt", "mit", "sei\u00b7ner", "gr\u00fc\u00b7nen", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mehr noch die jenigen/ so uns zu erst ans Leben/", "tokens": ["Mehr", "noch", "die", "je\u00b7ni\u00b7gen", "/", "so", "uns", "zu", "erst", "ans", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIAT", "$(", "ADV", "PPER", "APPR", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und dann mit Flei\u00df und M\u00fch\u2019 auch auf die Beine bracht.", "tokens": ["Und", "dann", "mit", "Flei\u00df", "und", "M\u00fch'", "auch", "auf", "die", "Bei\u00b7ne", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein gantzer Wald erbebt/ wenn hohe Cedern fallen/", "tokens": ["Ein", "gant\u00b7zer", "Wald", "er\u00b7bebt", "/", "wenn", "ho\u00b7he", "Ce\u00b7dern", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die lange Tanne heult/ die schlancke Birck erblast/", "tokens": ["Die", "lan\u00b7ge", "Tan\u00b7ne", "heult", "/", "die", "schlan\u00b7cke", "Birck", "er\u00b7blast", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und solt ein Angst-Geschrey bey Kindern nicht erschallen/", "tokens": ["Und", "solt", "ein", "Angst\u00b7Ge\u00b7schrey", "bey", "Kin\u00b7dern", "nicht", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wann ihres Vatern Haupt des Todes Sichel fast?", "tokens": ["Wann", "ih\u00b7res", "Va\u00b7tern", "Haupt", "des", "To\u00b7des", "Si\u00b7chel", "fast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "ART", "NN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ja freylich/ ", "tokens": ["Ja", "frey\u00b7lich", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "ADV", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Der Mensch ist nicht von Stahl und Marmel-Stein gebaut.", "tokens": ["Der", "Mensch", "ist", "nicht", "von", "Stahl", "und", "Mar\u00b7mel\u00b7Stein", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch aber mu\u00df auch hier des H\u00f6chsten Wille gelten/", "tokens": ["Doch", "a\u00b7ber", "mu\u00df", "auch", "hier", "des", "H\u00f6chs\u00b7ten", "Wil\u00b7le", "gel\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und da\u00df bey solcher Angst man diesem sich vertraut.", "tokens": ["Und", "da\u00df", "bey", "sol\u00b7cher", "Angst", "man", "die\u00b7sem", "sich", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PIAT", "NN", "PIS", "PDAT", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Legt Schmertzen und Vernunfft auf gleiche Wage-Schalen/", "tokens": ["Legt", "Schmert\u00b7zen", "und", "Ver\u00b7nunfft", "auf", "glei\u00b7che", "Wa\u00b7ge\u00b7Scha\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und hengt den Lauff der Zeit zu einem Ausschlag dran.", "tokens": ["Und", "hengt", "den", "Lauff", "der", "Zeit", "zu", "ei\u00b7nem", "Aus\u00b7schlag", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ists mit den jenigen so die Natur bezahlen/", "tokens": ["Ists", "mit", "den", "je\u00b7ni\u00b7gen", "so", "die", "Na\u00b7tur", "be\u00b7zah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "PIAT", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und zu der Ruhe gehn/ nicht recht und wohl gethan?", "tokens": ["Und", "zu", "der", "Ru\u00b7he", "gehn", "/", "nicht", "recht", "und", "wohl", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$(", "PTKNEG", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn was erwarten wir? Wol schwerlich g\u00fcldne Zeiten/", "tokens": ["Denn", "was", "er\u00b7war\u00b7ten", "wir", "?", "Wol", "schwer\u00b7lich", "g\u00fcld\u00b7ne", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "ADV", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df die Gerechtigkeit vom Himmel wieder k\u00f6mmt/", "tokens": ["Da\u00df", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "vom", "Him\u00b7mel", "wie\u00b7der", "k\u00f6mmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df Tugend/ Zucht und Treu einander stets begleiten/", "tokens": ["Da\u00df", "Tu\u00b7gend", "/", "Zucht", "und", "Treu", "ein\u00b7an\u00b7der", "stets", "be\u00b7glei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df weder ", "tokens": ["Da\u00df", "we\u00b7der"], "token_info": ["word", "word"], "pos": ["KOUS", "KON"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Und was er werben wir mit einem langen Leben?", "tokens": ["Und", "was", "er", "wer\u00b7ben", "wir", "mit", "ei\u00b7nem", "lan\u00b7gen", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Stets neues Hertzeleid/ geh\u00e4uffter Kranckheit Noth/", "tokens": ["Stets", "neu\u00b7es", "Hert\u00b7ze\u00b7leid", "/", "ge\u00b7h\u00e4uff\u00b7ter", "Kran\u00b7ck\u00b7heit", "Noth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.35": {"text": "Bek\u00fcmmern uns so sehr den Athem aufzugeben/", "tokens": ["Be\u00b7k\u00fcm\u00b7mern", "uns", "so", "sehr", "den", "A\u00b7them", "auf\u00b7zu\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da doch in Fleisch und ", "tokens": ["Da", "doch", "in", "Fleisch", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Ein Tagel\u00f6hner seufftzt nach seiner Arbeit Ende/", "tokens": ["Ein", "Ta\u00b7ge\u00b7l\u00f6h\u00b7ner", "seufftzt", "nach", "sei\u00b7ner", "Ar\u00b7beit", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wir aber fliehn das Ziel/ das uns doch ", "tokens": ["Wir", "a\u00b7ber", "fliehn", "das", "Ziel", "/", "das", "uns", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$(", "PRELS", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Es folgt der hellen Sonn\u2019 ihr Kind die Sonnemvende:", "tokens": ["Es", "folgt", "der", "hel\u00b7len", "Sonn'", "ihr", "Kind", "die", "Son\u00b7nem\u00b7ven\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wir nicht dem h\u00f6chsten Glantz/ der uns vom Tod erweckt.", "tokens": ["Wir", "nicht", "dem", "h\u00f6chs\u00b7ten", "Glantz", "/", "der", "uns", "vom", "Tod", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "ADJA", "NN", "$(", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So gar verzaubern uns des Lebens Lust-Sirenen/", "tokens": ["So", "gar", "ver\u00b7zau\u00b7bern", "uns", "des", "Le\u00b7bens", "Lust\u00b7Si\u00b7re\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die Frucht vom Lotos-Baum schmeckt jedem Weltling wol.", "tokens": ["Die", "Frucht", "vom", "Lo\u00b7to\u00b7sBaum", "schmeckt", "je\u00b7dem", "Welt\u00b7ling", "wol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line