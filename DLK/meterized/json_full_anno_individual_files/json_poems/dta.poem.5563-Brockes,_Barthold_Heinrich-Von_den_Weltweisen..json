{"dta.poem.5563": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein andrer sucht in G\u00e4rten, Welt-und Erden", "tokens": ["Ein", "an\u00b7drer", "sucht", "in", "G\u00e4r\u00b7ten", ",", "Welt\u00b7\u00b7und", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "$,", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sterben und gebohren werden,", "tokens": ["Die", "ster\u00b7ben", "und", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In seiner Lehr uns pr\u00e4chtig f\u00fcrzumahlen.", "tokens": ["In", "sei\u00b7ner", "Lehr", "uns", "pr\u00e4ch\u00b7tig", "f\u00fcr\u00b7zu\u00b7mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er meinet, ihre Zahl geh\u2019 \u00fcber alle Zahlen,", "tokens": ["Er", "mei\u00b7net", ",", "ih\u00b7re", "Zahl", "geh'", "\u00fc\u00b7ber", "al\u00b7le", "Zah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da\u00df ein ", "tokens": ["Und", "da\u00df", "ein"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Von einem Sonnen-St\u00e4ubgen-Heer,", "tokens": ["Von", "ei\u00b7nem", "Son\u00b7nen\u00b7St\u00e4ub\u00b7ge\u00b7nHeer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So auch unzehlbar, sie formire.", "tokens": ["So", "auch", "un\u00b7zehl\u00b7bar", ",", "sie", "for\u00b7mi\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "PPER", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}