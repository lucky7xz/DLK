{"dta.poem.9586": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie r\u00fchmte die gegenw\u00e4rtige sch\u00f6nheit  \n einer andern Damen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein! nein! Aurora nein! Filinde ist nicht sch\u00f6n/", "tokens": ["Nein", "!", "nein", "!", "Au\u00b7ro\u00b7ra", "nein", "!", "Fi\u00b7lin\u00b7de", "ist", "nicht", "sch\u00f6n", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PTKANT", "$.", "NE", "PTKANT", "$.", "NN", "VAFIN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kan licht und finstern\u00fc\u00df auch bey einander stehn?", "tokens": ["Kan", "licht", "und", "fins\u00b7ter\u00b7n\u00fc\u00df", "auch", "bey", "ein\u00b7an\u00b7der", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "VVFIN", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie schickt sich ziegel-grau\u00df und perlen doch zusammen?", "tokens": ["Wie", "schickt", "sich", "zie\u00b7gel\u00b7grau\u00df", "und", "per\u00b7len", "doch", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ist ihr todtes feur bey deinen wunder-flammen?", "tokens": ["Was", "ist", "ihr", "tod\u00b7tes", "feur", "bey", "dei\u00b7nen", "wun\u00b7der\u00b7flam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur eine sonne bleibt das auge dioser welt/", "tokens": ["Nur", "ei\u00b7ne", "son\u00b7ne", "bleibt", "das", "au\u00b7ge", "di\u00b7o\u00b7ser", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und nur ein himmel ist/ der allen wolgef\u00e4llt.", "tokens": ["Und", "nur", "ein", "him\u00b7mel", "ist", "/", "der", "al\u00b7len", "wol\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "$(", "ART", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wie dein auge nur das eintzige der erden/", "tokens": ["So", "wie", "dein", "au\u00b7ge", "nur", "das", "eint\u00b7zi\u00b7ge", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "ADV", "ART", "ADJA", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das mit verwunderung hier mu\u00df betrachtet werden:", "tokens": ["Das", "mit", "ver\u00b7wun\u00b7de\u00b7rung", "hier", "mu\u00df", "be\u00b7trach\u00b7tet", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und siehstu endlich ja Filindens angesicht/", "tokens": ["Und", "sieh\u00b7stu", "end\u00b7lich", "ja", "Fi\u00b7lin\u00b7dens", "an\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So lang es vor dir steht/ mit neuer sch\u00f6nheit prangen/", "tokens": ["So", "lang", "es", "vor", "dir", "steht", "/", "mit", "neu\u00b7er", "sch\u00f6n\u00b7heit", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So mu\u00df ihr auge doch von deinen strahlen licht/", "tokens": ["So", "mu\u00df", "ihr", "au\u00b7ge", "doch", "von", "dei\u00b7nen", "strah\u00b7len", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nicht anders als ein stern von seiner sonne/ fangen.", "tokens": ["Nicht", "an\u00b7ders", "als", "ein", "stern", "von", "sei\u00b7ner", "son\u00b7ne", "/", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ART", "ADJA", "APPR", "PPOSAT", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}