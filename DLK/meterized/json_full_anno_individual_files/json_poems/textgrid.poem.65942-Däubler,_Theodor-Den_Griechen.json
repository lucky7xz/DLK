{"textgrid.poem.65942": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Den Griechen", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Inselmeer zertr\u00fcmmert eine Sonne,", "tokens": ["Das", "In\u00b7sel\u00b7meer", "zer\u00b7tr\u00fcm\u00b7mert", "ei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In z\u00e4rtlicher Umblauung, jeden Tag,", "tokens": ["In", "z\u00e4rt\u00b7li\u00b7cher", "Um\u00b7blau\u00b7ung", ",", "je\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Beschenkt mit ihren Splittern Wald und Hag,", "tokens": ["Be\u00b7schenkt", "mit", "ih\u00b7ren", "Split\u00b7tern", "Wald", "und", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Beschert auch uns Verwundertheit und Wonne.", "tokens": ["Be\u00b7schert", "auch", "uns", "Ver\u00b7wun\u00b7der\u00b7theit", "und", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Mann durchgoldet froh die k\u00fchle Tonne", "tokens": ["Der", "Mann", "durch\u00b7gol\u00b7det", "froh", "die", "k\u00fch\u00b7le", "Ton\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "AD