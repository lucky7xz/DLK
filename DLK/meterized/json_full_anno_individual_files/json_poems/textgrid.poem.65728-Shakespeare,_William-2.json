], "pos": ["VMFIN", "PPER", "VVINF", "$(", "PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Tilgt meine Schuld, vertritt im Alter mich,", "tokens": ["Tilgt", "mei\u00b7ne", "Schuld", ",", "ver\u00b7tritt", "im", "Al\u00b7ter", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Weil seine Reize Erben meiner sind\u00ab. \u2013", "tokens": ["Weil", "sei\u00b7ne", "Rei\u00b7ze", "Er\u00b7ben", "mei\u00b7ner", "sind", "\u00ab", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPOSAT", "VAFIN", "$(", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dies ist's, wodurch ein Greis sich neu verj\u00fcngt", "tokens": ["Dies", "ist's", ",", "wo\u00b7durch", "ein", "Greis", "sich", "neu", "ver\u00b7j\u00fcngt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und kaltem Blut die W\u00e4rme wiederbringt.", "tokens": ["Und", "kal\u00b7tem", "Blut", "die", "W\u00e4r\u00b7me", "wie\u00b7der\u00b7bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn vierzig Winter einst dein Haupt umnachten", "tokens": ["Wenn", "vier\u00b7zig", "Win\u00b7ter", "einst", "dein", "Haupt", "um\u00b7nach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "ADV", "PPOSAT", "NN", "VVFIN"], "meter"