{"textgrid.poem.65728": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn vierzig Winter einst dein Haupt umnachten", "tokens": ["Wenn", "vier\u00b7zig", "Win\u00b7ter", "einst", "dein", "Haupt", "um\u00b7nach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tief durchfurchen deiner Sch\u00f6nheit Feld,", "tokens": ["Und", "tief", "durch\u00b7fur\u00b7chen", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann ist dein Jugendflor, wonach wir itzt so trachten,", "tokens": ["Dann", "ist", "dein", "Ju\u00b7gend\u00b7flor", ",", "wo\u00b7nach", "wir", "itzt", "so", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein m\u00fcrbes Kleid, das unbemerkt zerf\u00e4llt.", "tokens": ["Ein", "m\u00fcr\u00b7bes", "Kleid", ",", "das", "un\u00b7be\u00b7merkt", "zer\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein \u00f6des Lob, ein allverzehrend Schm\u00e4hn", "tokens": ["Ein", "\u00f6\u00b7des", "Lob", ",", "ein", "all\u00b7v