{"textgrid.poem.53593": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das erdolchte Heer", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Generale habens gesagt", "tokens": ["Die", "Ge\u00b7ne\u00b7ra\u00b7le", "ha\u00b7bens", "ge\u00b7sagt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "und haben die Heimat angeklagt.", "tokens": ["und", "ha\u00b7ben", "die", "Hei\u00b7mat", "an\u00b7ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Heimat \u2013 hei\u00dft es \u2013 erdolchte das Heer.", "tokens": ["Die", "Hei\u00b7mat", "\u2013", "hei\u00dft", "es", "\u2013", "er\u00b7dolch\u00b7te", "das", "Heer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aber die Heimat litt viel zu sehr!", "tokens": ["A\u00b7ber", "die", "Hei\u00b7mat", "litt", "viel", "zu", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Sie schrie und \u00e4chzte unter der Faust.", "tokens": ["Sie", "schrie", "und", "\u00e4chz\u00b7te", "un\u00b7ter", "der", "Faust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es w\u00fcrgt der Hunger, der Winterwind saust.", "tokens": ["Es", "w\u00fcrgt", "der", "Hun\u00b7ger", ",", "der", "Win\u00b7ter\u00b7wind", "saust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr habt der Heimat erst alles genommen", "tokens": ["Ihr", "habt", "der", "Hei\u00b7mat", "erst", "al\u00b7les", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PIS", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und seid noch besiegt zur\u00fcckgekommen.", "tokens": ["und", "seid", "noch", "be\u00b7siegt", "zu\u00b7r\u00fcck\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Besiegt hat euch euer eigener Wahn.", "tokens": ["Be\u00b7siegt", "hat", "euch", "eu\u00b7er", "ei\u00b7ge\u00b7ner", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dreimal kr\u00e4ht jetzt der biblische Hahn.", "tokens": ["Drei\u00b7mal", "kr\u00e4ht", "jetzt", "der", "bib\u00b7li\u00b7sche", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und nach so viel Fehlern und falschen Taten:", "tokens": ["Und", "nach", "so", "viel", "Feh\u00b7lern", "und", "fal\u00b7schen", "Ta\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "habt ihr nun auch die Heimat verraten.", "tokens": ["habt", "ihr", "nun", "auch", "die", "Hei\u00b7mat", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Heimat, die Frauen, die Schwachen, die Kranken \u2013", "tokens": ["Die", "Hei\u00b7mat", ",", "die", "Frau\u00b7en", ",", "die", "Schwa\u00b7chen", ",", "die", "Kran\u00b7ken", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wir danken, Generale, wir danken!", "tokens": ["Wir", "dan\u00b7ken", ",", "Ge\u00b7ne\u00b7ra\u00b7le", ",", "wir", "dan\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Generale habens gesagt", "tokens": ["Die", "Ge\u00b7ne\u00b7ra\u00b7le", "ha\u00b7bens", "ge\u00b7sagt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "und haben die Heimat angeklagt.", "tokens": ["und", "ha\u00b7ben", "die", "Hei\u00b7mat", "an\u00b7ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": [