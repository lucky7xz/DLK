{"textgrid.poem.25295": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch dunkle Grabzypressen haucht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch dunkle Grabzypressen haucht", "tokens": ["Durch", "dunk\u00b7le", "Grab\u00b7zy\u00b7pres\u00b7sen", "haucht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geheimnisvolles Raunen;", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7les", "Rau\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus wei\u00dfen Fliederdolden taucht", "tokens": ["Aus", "wei\u00b7\u00dfen", "Flie\u00b7der\u00b7dol\u00b7den", "taucht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mond mit scheuem Staunen.", "tokens": ["Der", "Mond", "mit", "scheu\u00b7em", "Stau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sieh, vom frischen Grabe", "tokens": ["Und", "sieh", ",", "vom", "fri\u00b7schen", "Gra\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hebt sich der Marmelstein,", "tokens": ["Hebt", "sich", "der", "Mar\u00b7mel\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die H\u00f6hlung klafft/ ein bleicher Mann", "tokens": ["Die", "H\u00f6h\u00b7lung", "klafft", "/", "ein", "blei\u00b7cher", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ersteht im Silberschein.", "tokens": ["Er\u00b7steht", "im", "Sil\u00b7ber\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An seine wirre Stirne greift", "tokens": ["An", "sei\u00b7ne", "wir\u00b7re", "Stir\u00b7ne", "greift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tote schlummertrunken;", "tokens": ["Der", "To\u00b7te", "schlum\u00b7mer\u00b7trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie sein Blick die Tafel streift,", "tokens": ["Und", "wie", "sein", "Blick", "die", "Ta\u00b7fel", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da stutzt er, bohrt versunken", "tokens": ["Da", "stutzt", "er", ",", "bohrt", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Aug in seine ", "tokens": ["Das", "Aug", "in", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und starrt/ bis an sein Ohr", "tokens": ["Und", "starrt", "/", "bis", "an", "sein", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Hahnenschrei vom Dorfe gellt;", "tokens": ["Ein", "Hah\u00b7nen\u00b7schrei", "vom", "Dor\u00b7fe", "gellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da f\u00e4hrt er j\u00e4h empor.", "tokens": ["Da", "f\u00e4hrt", "er", "j\u00e4h", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zum D\u00f6rflein heimw\u00e4rts will er gehn/", "tokens": ["Zum", "D\u00f6r\u00b7flein", "heim\u00b7w\u00e4rts", "will", "er", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ehedem/ und zaudert", "tokens": ["Wie", "e\u00b7he\u00b7dem", "/", "und", "zau\u00b7dert"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$(", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bleibt am Friedhofzaune stehn,", "tokens": ["Und", "bleibt", "am", "Fried\u00b7hof\u00b7zau\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von fremder Scheu durchschaudert:", "tokens": ["Von", "frem\u00b7der", "Scheu", "durch\u00b7schau\u00b7dert", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbo Pilger, la\u00df, was dr\u00fcben liegt,", "tokens": ["\u00bb", "o", "Pil\u00b7ger", ",", "la\u00df", ",", "was", "dr\u00fc\u00b7ben", "liegt", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "VVIMP", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo sattsam du gegangen!", "tokens": ["Wo", "satt\u00b7sam", "du", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf neuen Pfaden weide", "tokens": ["Auf", "neu\u00b7en", "Pfa\u00b7den", "wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gel\u00e4utertes Verlangen!\u00ab", "tokens": ["Ge\u00b7l\u00e4u\u00b7ter\u00b7tes", "Ver\u00b7lan\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bei B\u00fcschen, H\u00fcgeln, Dorf und Au", "tokens": ["Bei", "B\u00fc\u00b7schen", ",", "H\u00fc\u00b7geln", ",", "Dorf", "und", "Au"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verweilt sein Aug mit Gr\u00fc\u00dfen,", "tokens": ["Ver\u00b7weilt", "sein", "Aug", "mit", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ade! und schwimmt in Tr\u00e4nentau.", "tokens": ["A\u00b7de", "!", "und", "schwimmt", "in", "Tr\u00e4\u00b7nen\u00b7tau", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "VVFIN", "APPR", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und wie er nun dem s\u00fc\u00dfen", "tokens": ["Und", "wie", "er", "nun", "dem", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trostliede lauscht der Nachtigall,", "tokens": ["Trost\u00b7lie\u00b7de", "lauscht", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sucht er eine Gruft", "tokens": ["Da", "sucht", "er", "ei\u00b7ne", "Gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und k\u00fc\u00dft von wei\u00dfer Rose", "tokens": ["Und", "k\u00fc\u00dft", "von", "wei\u00b7\u00dfer", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erinnerungsvollen Duft.", "tokens": ["E\u00b7rin\u00b7ne\u00b7rungs\u00b7vol\u00b7len", "Duft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbzur R\u00fcste, Pilger! Was so schwer", "tokens": ["\u00bb", "zur", "R\u00fcs\u00b7te", ",", "Pil\u00b7ger", "!", "Was", "so", "schwer"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "$,", "NN", "$.", "PWS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir lastet auf dem Herzen,", "tokens": ["Dir", "las\u00b7tet", "auf", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Tu ab von dir! und sch\u00fcrfe leer", "tokens": ["Tu", "ab", "von", "dir", "!", "und", "sch\u00fcr\u00b7fe", "leer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "APPR", "PPER", "$.", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Herz von Schutt und Erzen!", "tokens": ["Dein", "Herz", "von", "Schutt", "und", "Er\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was du gelebet/ Schutt und Erz/", "tokens": ["Was", "du", "ge\u00b7le\u00b7bet", "/", "Schutt", "und", "Erz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei nun gerecht gerichtet", "tokens": ["Sei", "nun", "ge\u00b7recht", "ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und hier auf deiner Tafel,", "tokens": ["Und", "hier", "auf", "dei\u00b7ner", "Ta\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zwei H\u00fcglein, aufgeschichtet!\u00ab", "tokens": ["Zwei", "H\u00fcg\u00b7lein", ",", "auf\u00b7ge\u00b7schich\u00b7tet", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["CARD", "NN", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er wiegt das Haupt in stummem Weh/", "tokens": ["Er", "wiegt", "das", "Haupt", "in", "stum\u00b7mem", "Weh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das gilt dem Schlackenh\u00fcgel.", "tokens": ["Das", "gilt", "dem", "Schla\u00b7cken\u00b7h\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch aus dem andern, rein wie Schnee,", "tokens": ["Doch", "aus", "dem", "an\u00b7dern", ",", "rein", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Formt er zwei Schwanenfl\u00fcgel;", "tokens": ["Formt", "er", "zwei", "Schwa\u00b7nen\u00b7fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die f\u00fcgt er an die Schultern", "tokens": ["Die", "f\u00fcgt", "er", "an", "die", "Schul\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und spannt sie breit und hehr,", "tokens": ["Und", "spannt", "sie", "breit", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {