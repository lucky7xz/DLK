{"dta.poem.1556": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff die gl\u00fcckliche Eroberung der Festung  \n Ofen in Hungarn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.57", "fr:0.42"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Freue dich du edles Land/ ", "tokens": ["Freu\u00b7e", "dich", "du", "ed\u00b7les", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches von den wilden Scythen", "tokens": ["Wel\u00b7ches", "von", "den", "wil\u00b7den", "Scyt\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des T\u00fcrcken strenger Hand", "tokens": ["Und", "des", "T\u00fcr\u00b7cken", "stren\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So viel Qual bi\u00dfher erlitten!", "tokens": ["So", "viel", "Qual", "bi\u00df\u00b7her", "er\u00b7lit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine schwere F\u00e4ssel springen/", "tokens": ["Dei\u00b7ne", "schwe\u00b7re", "F\u00e4s\u00b7sel", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und du kanst gewonnen! singen.", "tokens": ["Und", "du", "kanst", "ge\u0