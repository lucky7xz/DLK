{"textgrid.poem.67504": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fliegt, Ihr Bl\u00e4tter der Blumen, entfliegt zu jenen Gestaden,", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fliegt, Ihr Bl\u00e4tter der Blumen, entfliegt zu jenen Gestaden,", "tokens": ["Fliegt", ",", "Ihr", "Bl\u00e4t\u00b7ter", "der", "Blu\u00b7men", ",", "ent\u00b7fliegt", "zu", "je\u00b7nen", "Ge\u00b7sta\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wo ich im Jugendtraum sprossen Euch Nichtige fand;", "tokens": ["Wo", "ich", "im", "Ju\u00b7gend\u00b7traum", "spros\u00b7sen", "Euch", "Nich\u00b7ti\u00b7ge", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nichtige, bl\u00fchet Ihr noch, so kr\u00e4nzt die Schl\u00e4fe des Freundes,", "tokens": ["Nich\u00b7ti\u00b7ge", ",", "bl\u00fc\u00b7het", "Ihr", "noch", ",", "so", "kr\u00e4nzt", "die", "Schl\u00e4\u00b7fe", "des", "Freun\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Die im Leben ja auch Distel und Rose gekr\u00e4nzt!", "tokens": ["Die", "im", "Le\u00b7ben", "ja", "auch", "Dis\u00b7tel", "und", "Ro\u00b7se", "ge\u00b7kr\u