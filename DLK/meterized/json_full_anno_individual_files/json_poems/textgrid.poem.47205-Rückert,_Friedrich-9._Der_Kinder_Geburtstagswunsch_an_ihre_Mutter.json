{"textgrid.poem.47205": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9. Der Kinder Geburtstagswunsch an ihre Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut kommen deine Vier,", "tokens": ["Heut", "kom\u00b7men", "dei\u00b7ne", "Vier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Um Gl\u00fcck zu w\u00fcnschen dir", "tokens": ["Um", "Gl\u00fcck", "zu", "w\u00fcn\u00b7schen", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "PTKZU", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Tag, der dich gebar.", "tokens": ["Zum", "Tag", ",", "der", "dich", "ge\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sechs waren es vorm Jahr;", "tokens": ["Sechs", "wa\u00b7ren", "es", "vorm", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun fehlt das P\u00e4rchen. Nein!", "tokens": ["Nun", "fehlt", "das", "P\u00e4r\u00b7chen", ".", "Nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es stellet mit sich ein,", "tokens": ["Es", "stel\u00b7let", "mit", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "mea