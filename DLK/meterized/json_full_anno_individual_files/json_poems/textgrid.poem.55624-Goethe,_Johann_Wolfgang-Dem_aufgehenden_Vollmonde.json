{"textgrid.poem.55624": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Dem aufgehenden Vollmonde", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du mich sogleich verlassen?", "tokens": ["Willst", "du", "mich", "sog\u00b7leich", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warst im Augenblick so nah!", "tokens": ["Warst", "im", "Au\u00b7gen\u00b7blick", "so", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich umfinstern Wolkenmassen,", "tokens": ["Dich", "um\u00b7fins\u00b7tern", "Wol\u00b7ken\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nun bist du gar nicht da.", "tokens": ["Und", "nun", "bist", "du", "gar", "nicht", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch du f\u00fchlst, wie ich betr\u00fcbt bin,", "tokens": ["Doch", "du", "f\u00fchlst", ",", "wie", "ich", "be\u00b7tr\u00fcbt", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt dein Rand herauf als Stern!", "tokens": ["Blickt", "dein", "Rand", "her\u00b7auf", "als", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeugest mir, da\u00df ich geliebt bin,", "tokens": ["Zeu\u00b7gest", "mir", ",", "da\u00df", "ich", "ge\u00b7liebt", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sei das Liebchen noch so fern.", "tokens": ["Sei", "das", "Lieb\u00b7chen", "noch", "so", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So hinan denn! hell und heller,", "tokens": ["So", "hi\u00b7nan", "denn", "!", "hell", "und", "hel\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$.", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Reiner Bahn, in voller Pracht!", "tokens": ["Rei\u00b7ner", "Bahn", ",", "in", "vol\u00b7ler", "Pracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00e4gt mein Herz auch schmerzlich schneller,", "tokens": ["Schl\u00e4gt", "mein", "Herz", "auch", "schmerz\u00b7lich", "schnel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberselig ist die Nacht.", "tokens": ["\u00dc\u00b7ber\u00b7se\u00b7lig",