{"textgrid.poem.31161": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du denn immer noch nicht ruhn?", "tokens": ["Willst", "du", "denn", "im\u00b7mer", "noch", "nicht", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du noch immer so viel zu thun?", "tokens": ["Hast", "du", "noch", "im\u00b7mer", "so", "viel", "zu", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00e4ng deine Harfe, mein Herz, an die Weiden,", "tokens": ["H\u00e4ng", "dei\u00b7ne", "Har\u00b7fe", ",", "mein", "Herz", ",", "an", "die", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Lerne dich endlich doch, endlich bescheiden!", "tokens": ["Ler\u00b7ne", "dich", "end\u00b7lich", "doch", ",", "end\u00b7lich", "be\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer noch f\u00fchl ich dich flammen und gl\u00fchn,", "tokens": ["Im\u00b7mer", "noch", "f\u00fchl", "ich", "dich", "flam\u00b7men", "und", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "KON", "ADJD", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wenn dich im Fr\u00fchling die Rosen umbl\u00fchn;", "tokens": ["Wenn", "dich", "im", "Fr\u00fch\u00b7ling", "die", "Ro\u00b7sen", "um\u00b7bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Immer noch sehnst du dich, s\u00fcss wie vor Jahren,", "tokens": ["Im\u00b7mer", "noch", "sehnst", "du", "dich", ",", "s\u00fcss", "wie", "vor", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$,", "ADJD", "KOKOM", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wild nach dem Gl\u00fcck mit den goldenen Haaren,", "tokens": ["Wild", "nach", "dem", "Gl\u00fcck", "mit", "den", "gol\u00b7de\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Schmeichelst es Liebling und Lorelei,", "tokens": ["Schmei\u00b7chelst", "es", "Lieb\u00b7ling", "und", "Lo\u00b7re\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ach, und noch immer fliegt es vorbei!", "tokens": ["Ach", ",", "und", "noch", "im\u00b7mer", "fliegt", "es", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos