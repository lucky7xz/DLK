{"dta.poem.4263": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Cinquille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cinquille wird von mir die Laube mit f\u00fcnf B\u00e4nken,", "tokens": ["Cin\u00b7quil\u00b7le", "wird", "von", "mir", "die", "Lau\u00b7be", "mit", "f\u00fcnf", "B\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPER", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die viele junge B\u00e4um\u2019 in einer R\u00fcnd\u2019 umschr\u00e4nken,", "tokens": ["Die", "vie\u00b7le", "jun\u00b7ge", "B\u00e4um'", "in", "ei\u00b7ner", "R\u00fcnd'", "um\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Fug und Recht genannt. In ihrer Mitte steht", "tokens": ["Mit", "Fug", "und", "Recht", "ge\u00b7nannt", ".", "In", "ih\u00b7rer", "Mit\u00b7te", "steht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein alter Eichen-Baum,", "tokens": ["Ein", "al\u00b7ter", "Ei\u00b7chen\u00b7Baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der fast den ganzen Raum,", "tokens": ["Der", "fast", "den", "gan\u00b7zen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit gr\u00fcnen Schatten, deckt. Ein wenig mehr erh\u00f6het,", "tokens": ["Mit", "gr\u00fc\u00b7nen", "Schat\u00b7ten", ",", "deckt", ".", "Ein", "we\u00b7nig", "mehr", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$.", "ART", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als wie die andern B\u00e4nke liegen,", "tokens": ["Als", "wie", "die", "an\u00b7dern", "B\u00e4n\u00b7ke", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist eine Bank, als wie ein kleiner Thron,", "tokens": ["Ist", "ei\u00b7ne", "Bank", ",", "als", "wie", "ein", "klei\u00b7ner", "Thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wor\u00fcber sich, als wie ein Paviljon,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "sich", ",", "als", "wie", "ein", "Pa\u00b7vil\u00b7jon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Von selbst gewachs\u2019ne Zweige biegen.", "tokens": ["Von", "selbst", "ge\u00b7wachs'\u00b7ne", "Zwei\u00b7ge", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da sie der Lauben-Th\u00fcr", "tokens": ["Da", "sie", "der", "Lau\u00b7ben\u00b7T\u00b7h\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Gerad\u2019 entgegen steht, so giebt sie ihr,", "tokens": ["Ge\u00b7rad'", "ent\u00b7ge\u00b7gen", "steht", ",", "so", "giebt", "sie", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Recht als ein Perspectiv, noch fast die gr\u00f6\u00dfte Zier.", "tokens": ["Recht", "als", "ein", "Per\u00b7spec\u00b7tiv", ",", "noch", "fast", "die", "gr\u00f6\u00df\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sechs, in gerader Linie,", "tokens": ["Sechs", ",", "in", "ge\u00b7ra\u00b7der", "Li\u00b7nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.15": {"text": "Gepflanzte B\u00e4um\u2019, und meist von einer H\u00f6h\u2019,", "tokens": ["Ge\u00b7pflanz\u00b7te", "B\u00e4um'", ",", "und", "meist", "von", "ei\u00b7ner", "H\u00f6h'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Beschr\u00e4nken, an den beyden Seiten,", "tokens": ["Be\u00b7schr\u00e4n\u00b7ken", ",", "an", "den", "bey\u00b7den", "Sei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Den Zugang dergestalt,", "tokens": ["Den", "Zu\u00b7gang", "der\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Da\u00df sie uns, vor den andern in dem Wald,", "tokens": ["Da\u00df", "sie", "uns", ",", "vor", "den", "an\u00b7dern", "in", "dem", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "APPR", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Am fertigsten den Schritt in diese Laube leiten.", "tokens": ["Am", "fer\u00b7tigs\u00b7ten", "den", "Schritt", "in", "die\u00b7se", "Lau\u00b7be", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.20": {"text": "Hier bin ich oft, mit Lust, gesessen,", "tokens": ["Hier", "bin", "ich", "oft", ",", "mit", "Lust", ",", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "APPR", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Hier hab\u2019 ich oft, mit frohem Sinn,", "tokens": ["Hier", "hab'", "ich", "oft", ",", "mit", "fro\u00b7hem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die Sch\u00f6nheit der Natur ermessen,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "der", "Na\u00b7tur", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und wenn ich recht dadurch ger\u00fchrt gewesen bin:", "tokens": ["Und", "wenn", "ich", "recht", "da\u00b7durch", "ge\u00b7r\u00fchrt", "ge\u00b7we\u00b7sen", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PAV", "VVPP", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hab\u2019 ich, GOtt Lob! des Dankens nicht vergessen.", "tokens": ["Hab'", "ich", ",", "Gott", "Lob", "!", "des", "Dan\u00b7kens", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "NN", "$.", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}