{"textgrid.poem.38349": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "I i Imme (Biene.)", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Honigs\u00fc\u00dfe Immelein sich sp\u00e4t und fr\u00fch bem\u00fcht,", "tokens": ["Das", "Ho\u00b7nig\u00b7s\u00fc\u00b7\u00dfe", "Im\u00b7mel\u00b7ein", "sich", "sp\u00e4t", "und", "fr\u00fch", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Es sizt auf allen Bl\u00fcmelein, versuchet alle Bl\u00fcth,", "tokens": ["Es", "sizt", "auf", "al\u00b7len", "Bl\u00fc\u00b7me\u00b7lein", ",", "ver\u00b7su\u00b7chet", "al\u00b7le", "Bl\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Sehr emsig fliegts herummer, tr\u00e4gt ein mit gro\u00dfem Flei\u00df,", "tokens": ["Sehr", "em\u00b7sig", "fliegts", "her\u00b7um\u00b7mer", ",", "tr\u00e4gt", "ein", "mit", "gro\u00b7\u00dfem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sucht den ganzen Sommer, auch f\u00fcr den Winter Spei\u00df.", "tokens": ["Und", "sucht", "den", "gan\u00b7zen", "Som\u00b7mer", ",", "auch", "f\u00fcr", "den", "Win\u00b7ter", "Spei\u00df", "."], "token_info": ["word", "word", "word", "word", "word