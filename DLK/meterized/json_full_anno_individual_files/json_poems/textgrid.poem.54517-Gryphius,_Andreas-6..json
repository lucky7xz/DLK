{"textgrid.poem.54517": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! Seligmacher auf! wach auf von deiner Ruh/", "tokens": ["Auf", "!", "Se\u00b7lig\u00b7ma\u00b7cher", "auf", "!", "wach", "auf", "von", "dei\u00b7ner", "Ruh", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "PTKVZ", "$.", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eile fr\u00fch den Stand der Ehren zu beginnen/", "tokens": ["Und", "ei\u00b7le", "fr\u00fch", "den", "Stand", "der", "Eh\u00b7ren", "zu", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die H\u00f6lle wird nunmehr mit Schrecken deiner innen/", "tokens": ["Die", "H\u00f6l\u00b7le", "wird", "nun\u00b7mehr", "mit", "Schre\u00b7cken", "dei\u00b7ner", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "PPOSAT", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Satan bebt und fleucht/ der Himmel jauchtzt dir zu.", "tokens": ["Der", "Sa\u00b7tan", "bebt", "und", "fleucht", "/", "der", "Him\u00b7mel", "jauchtzt", "dir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$(", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Schlangen Gifft ist hin/ du du bists/ K\u00f6nig/ du/", "tokens": ["Der", "Schlan\u00b7gen", "Gifft", "ist", "hin", "/", "du", "du", "bists", "/", "K\u00f6\u00b7nig", "/", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$(", "PPER", "PPER", "ADV", "$(", "NN", "$(", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sterbend durch den Tod/ dem Tod kan abgewinnen!", "tokens": ["Der", "ster\u00b7bend", "durch", "den", "Tod", "/", "dem", "Tod", "kan", "ab\u00b7ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "$(", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch weil mein' Augen noch mit herben Thr\u00e4nen rinnen/", "tokens": ["Doch", "weil", "mein'", "Au\u00b7gen", "noch", "mit", "her\u00b7ben", "Thr\u00e4\u00b7nen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So zeige doch der Welt wie unbedacht sie thu.", "tokens": ["So", "zei\u00b7ge", "doch", "der", "Welt", "wie", "un\u00b7be\u00b7dacht", "sie", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "KOKOM", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie ist bem\u00fcht dein Grab mit Siegeln zu beschliessen/", "tokens": ["Sie", "ist", "be\u00b7m\u00fcht", "dein", "Grab", "mit", "Sie\u00b7geln", "zu", "be\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da du das feste Thor des Abgrunds hast zurissen/", "tokens": ["Da", "du", "das", "fes\u00b7te", "Thor", "des", "Ab\u00b7grunds", "hast", "zu\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie legt verst\u00e4rckte Wach/ und Waffen um den Stein.", "tokens": ["Sie", "legt", "ver\u00b7st\u00e4rck\u00b7te", "Wach", "/", "und", "Waf\u00b7fen", "um", "den", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$(", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da du dem Starcken doch die Waffen abgenommen/", "tokens": ["Da", "du", "dem", "Star\u00b7cken", "doch", "die", "Waf\u00b7fen", "ab\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wach auf! ich schau dich schon aus deiner Grufft vorkommen/", "tokens": ["Wach", "auf", "!", "ich", "schau", "dich", "schon", "aus", "dei\u00b7ner", "Grufft", "vor\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "po