{"textgrid.poem.26637": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aber schon naht sich auch jetzt, verlangender Wiener, dein Fasching,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber schon naht sich auch jetzt, verlangender Wiener, dein Fasching,", "tokens": ["A\u00b7ber", "schon", "naht", "sich", "auch", "jetzt", ",", "ver\u00b7lan\u00b7gen\u00b7der", "Wie\u00b7ner", ",", "dein", "Fa\u00b7sching", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ADV", "ADV", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Den der gebildete Sinn h\u00f6heren Zwecken vereint.", "tokens": ["Den", "der", "ge\u00b7bil\u00b7de\u00b7te", "Sinn", "h\u00f6\u00b7he\u00b7ren", "Zwe\u00b7cken", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "B\u00e4lle, Redouten zum Wohle der Menschheit. Erhabensten Glanzes,", "tokens": ["B\u00e4l\u00b7le", ",", "Re\u00b7dou\u00b7ten", "zum", "Woh\u00b7le", "der", "Menschheit", ".", "Er\u00b7ha\u00b7bens\u00b7ten", "Glan\u00b7zes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPRART", "NN", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Hell von Orchestern durcht\u00f6nt, schlie\u00dfen die S\u00e4le sich auf.", "tokens": ["Hell", "von", "Or\u00b7ches\u00b7tern", "durch\u00b7t\u00f6nt", ",", "schlie\u00b7\u00dfen", "die", "S\u00e4\u00b7le", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Humanit\u00e4t wird getanzt. Was gilt es nicht Alles zu f\u00f6rdern!", "tokens": ["Hu\u00b7ma\u00b7ni\u00b7t\u00e4t", "wird", "ge\u00b7tanzt", ".", "Was", "gilt", "es", "nicht", "Al\u00b7les", "zu", "f\u00f6r\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$.", "PWS", "VVFIN", "PPER", "PTKNEG", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-++--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "K\u00fcchen, Spit\u00e4ler verlangt, w\u00e4rmende Stuben das Volk.", "tokens": ["K\u00fc\u00b7chen", ",", "Spi\u00b7t\u00e4\u00b7ler", "ver\u00b7langt", ",", "w\u00e4r\u00b7men\u00b7de", "Stu\u00b7ben", "das", "Volk", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVPP", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "L\u00e4chelnd erscheinen besternte Minister; Zierden des Reichsraths,", "tokens": ["L\u00e4\u00b7chelnd", "er\u00b7schei\u00b7nen", "bes\u00b7tern\u00b7te", "Mi\u00b7nis\u00b7ter", ";", "Zier\u00b7den", "des", "Reichs\u00b7raths", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.8": {"text": "Knospende Reden im Haupt, stehen an Pfeiler gelehnt.", "tokens": ["Knos\u00b7pen\u00b7de", "Re\u00b7den", "im", "Haupt", ",", "ste\u00b7hen", "an", "Pfei\u00b7ler", "ge\u00b7