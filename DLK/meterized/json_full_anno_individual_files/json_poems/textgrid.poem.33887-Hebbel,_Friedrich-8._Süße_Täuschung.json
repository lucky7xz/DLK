{"textgrid.poem.33887": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8. S\u00fc\u00dfe T\u00e4uschung", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft, wenn ich bei der Sterne Schein", "tokens": ["Oft", ",", "wenn", "ich", "bei", "der", "Ster\u00b7ne", "Schein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Kirchhof meine Schritte lenke,", "tokens": ["Zum", "Kirch\u00b7hof", "mei\u00b7ne", "Schrit\u00b7te", "len\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mich so tief, so ganz hinein", "tokens": ["Und", "mich", "so", "tief", ",", "so", "ganz", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJD", "$,", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jene sel'ge Zeit versenke,", "tokens": ["In", "je\u00b7ne", "sel'\u00b7ge", "Zeit", "ver\u00b7sen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wir zusammen Hand in Hand", "tokens": ["Wie", "wir", "zu\u00b7sam\u00b7men", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier wandelten in stillem Wehe,", "tokens": ["Hier", "wan\u00b7del\u00b7ten", "in", "stil\u00b7lem", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da ist es mir, als ob das Band", "tokens": ["Da", "ist", "es", "mir", ",", "als", "ob", "das", "Band"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch immer heiter fortbestehe.", "tokens": ["Noch", "im\u00b7mer", "hei\u00b7ter", "fort\u00b7be\u00b7ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir gehen fort und immer fort", "tokens": ["Wir", "ge\u00b7hen", "fort", "und", "im\u00b7mer", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schau'n die Gr\u00e4ber in der Runde,", "tokens": ["Und", "schau'n", "die", "Gr\u00e4\u00b7ber", "in", "der", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast f\u00fcr jegliches ein Wort", "tokens": ["Du", "hast", "f\u00fcr", "jeg\u00b7li\u00b7ches", "ein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprichst es aus mit sanftem Munde,", "tokens": ["Und", "sprichst", "es", "aus", "mit", "sanf\u00b7tem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du sprichst vom fr\u00fchen Schlafengeh'n", "tokens": ["Du", "sprichst", "vom", "fr\u00fc\u00b7hen", "Schla\u00b7fen\u00b7geh'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und von der Eitelkeit der Erde", "tokens": ["Und", "von", "der", "Ei\u00b7tel\u00b7keit", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und von dem gro\u00dfen Wiederseh'n,", "tokens": ["Und", "von", "dem", "gro\u00b7\u00dfen", "Wie\u00b7der\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Gott uns nicht versagen werde.", "tokens": ["Das", "Gott", "uns", "nicht", "ver\u00b7sa\u00b7gen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und kommt zuletzt dein eigen Grab,", "tokens": ["Und", "kommt", "zu\u00b7letzt", "dein", "ei\u00b7gen", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So rufst du aus: wir m\u00fcssen scheiden!", "tokens": ["So", "rufst", "du", "aus", ":", "wir", "m\u00fcs\u00b7sen", "schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER