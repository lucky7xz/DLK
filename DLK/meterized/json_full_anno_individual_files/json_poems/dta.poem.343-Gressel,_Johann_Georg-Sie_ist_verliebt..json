{"dta.poem.343": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie ist verliebt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin verliebt/", "tokens": ["Ich", "bin", "ver\u00b7liebt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und halte stille/", "tokens": ["Und", "hal\u00b7te", "stil\u00b7le", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was mein Schatz mit mir ver\u00fcbt/", "tokens": ["Was", "mein", "Schatz", "mit", "mir", "ver\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist auch mein Wille.", "tokens": ["Ist", "auch", "mein", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dann kan ich mit s\u00fcssen Freuden", "tokens": ["Dann", "kan", "ich", "mit", "s\u00fcs\u00b7sen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Meine heissen Sinnen weiden/", "tokens": ["Mei\u00b7ne", "heis\u00b7sen", "Sin\u00b7nen", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn er mir den ", "tokens": ["Wenn", "er", "mir", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Der mit Wollust truncken macht.", "tokens": ["Der", "mit", "Wol\u00b7lust", "trun\u00b7cken", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00fcsse Lust/ vergn\u00fcgtes Lieben", "tokens": ["S\u00fcs\u00b7se", "Lust", "/", "ver\u00b7gn\u00fcg\u00b7tes", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$(", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist den Menschen vorgeschrieben;", "tokens": ["Ist", "den", "Men\u00b7schen", "vor\u00b7ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Himmel macht den Schlu\u00df/", "tokens": ["Und", "der", "Him\u00b7mel", "macht", "den", "Schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df das Hertze lieben mu\u00df.", "tokens": ["Da\u00df", "das", "Hert\u00b7ze", "lie\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieben speist mit Lust die Seelen/", "tokens": ["Lie\u00b7ben", "speist", "mit", "Lust", "die", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "APPR", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieben ist ein Zucker-Brodt/", "tokens": ["Lie\u00b7ben", "ist", "ein", "Zu\u00b7cke\u00b7rBrodt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieben s\u00fcsset an das Qu\u00e4hlen", "tokens": ["Lie\u00b7ben", "s\u00fcs\u00b7set", "an", "das", "Qu\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und entbittert gar den Tod.", "tokens": ["Und", "ent\u00b7bit\u00b7tert", "gar", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn was kan wol beliebters seyn?", "tokens": ["Denn", "was", "kan", "wol", "be\u00b7lieb\u00b7ters", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ADV", "ADJA", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Als solche Lust/", "tokens": ["Als", "sol\u00b7che", "Lust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die ein verliebtes Hertz", "tokens": ["Die", "ein", "ver\u00b7lieb\u00b7tes", "Hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nimmt sonder Schertz", "tokens": ["Nimmt", "son\u00b7der", "Schertz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von ihren Liebsten ein.", "tokens": ["Von", "ih\u00b7ren", "Liebs\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das Hertz springt in der Brust/", "tokens": ["Das", "Hertz", "springt", "in", "der", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die matten Geister leben/", "tokens": ["Die", "mat\u00b7ten", "Geis\u00b7ter", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn ihnen wird der ", "tokens": ["Wenn", "ih\u00b7nen", "wird", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Mein Hertze lacht/", "tokens": ["Mein", "Hert\u00b7ze", "lacht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Wenn mich mein Liebster winckt;", "tokens": ["Wenn", "mich", "mein", "Liebs\u00b7ter", "winckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich folge ihm zu Bette", "tokens": ["Ich", "fol\u00b7ge", "ihm", "zu", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So bald das Licht versinckt/", "tokens": ["So", "bald", "das", "Licht", "ver\u00b7sinckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Denn lieben wir uns in die Wette.", "tokens": ["Denn", "lie\u00b7ben", "wir", "uns", "in", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}