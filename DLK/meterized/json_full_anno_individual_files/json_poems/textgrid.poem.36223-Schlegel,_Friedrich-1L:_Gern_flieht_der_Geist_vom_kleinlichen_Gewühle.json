{"textgrid.poem.36223": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gern flieht der Geist vom kleinlichen Gew\u00fchle", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gern flieht der Geist vom kleinlichen Gew\u00fchle", "tokens": ["Gern", "flieht", "der", "Geist", "vom", "klein\u00b7li\u00b7chen", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Welt, wo Albernheiten ernsthaft thronen,", "tokens": ["Der", "Welt", ",", "wo", "Al\u00b7bern\u00b7hei\u00b7ten", "ernst\u00b7haft", "thro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf zu des Scherzes heitern Regionen,", "tokens": ["Auf", "zu", "des", "Scher\u00b7zes", "hei\u00b7tern", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verh\u00fcllt in sich die heiligsten Gef\u00fchle.", "tokens": ["Ver\u00b7h\u00fcllt", "in", "sich", "die", "hei\u00b7ligs\u00b7ten", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Umweht ihn einmal \u00c4ther leicht und k\u00fchle,", "tokens": ["Um\u00b7weht", "ihn", "ein\u00b7mal", "\u00c4\u00b7ther", "leicht", "und", "k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So kann er nimmer wieder unten wohnen,", "tokens": ["So", "kann", "er", "nim\u00b7mer", "wie\u00b7der", "un\u00b7ten", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schnell wird jenen Scherz der Ernst belohnen,", "tokens": ["Und", "schnell", "wird", "je\u00b7nen", "Scherz", "der", "Ernst", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PDAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er sich neu im eignen Bilde f\u00fchle.", "tokens": ["Da\u00df", "er", "sich", "neu", "im", "eig\u00b7nen", "Bil\u00b7de", "f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die W\u00fcnsche, die dich hin zur Dichtkunst ziehen,", "tokens": ["Die", "W\u00fcn\u00b7sche", ",", "die", "dich", "hin", "zur", "Dicht\u00b7kunst", "zie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der frohe Ernst, in den du da versankest,", "tokens": ["Der", "fro\u00b7he", "Ernst", ",", "in", "den", "du", "da", "ver\u00b7san\u00b7kest", ","], "token_info": ["word", "