{"textgrid.poem.43751": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nu\u00dfknacker", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nu\u00dfknacker, du machst ein grimmig Gesicht \u2013", "tokens": ["Nu\u00df\u00b7knac\u00b7ker", ",", "du", "machst", "ein", "grim\u00b7mig", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich aber, ich f\u00fcrchte vor dir mich nicht;", "tokens": ["Ich", "a\u00b7ber", ",", "ich", "f\u00fcrch\u00b7te", "vor", "dir", "mich", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VVFIN", "APPR", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich wei\u00df, du meinst es gut mit mir.", "tokens": ["Ich", "wei\u00df", ",", "du", "meinst", "es", "gut", "mit", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum bring' ich meine N\u00fcsse dir.", "tokens": ["Drum", "bring'", "ich", "mei\u00b7ne", "N\u00fcs\u00b7se", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, du bist ein Meister im Knacken:", "tokens": ["Ich", "wei\u00df", ",", "du", "bist", "ein", "Meis\u00b7ter", "im", "Kna\u00b7cken", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du kannst mit deinen dicken Backen", "tokens": ["Du", "kannst", "mit", "dei\u00b7nen", "di\u00b7cken", "Ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gar h\u00fcbsch die harten N\u00fcsse packen", "tokens": ["Gar", "h\u00fcbsch", "die", "har\u00b7ten", "N\u00fcs\u00b7se", "pa\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wei\u00dft sie vortrefflich aufzuknacken.", "tokens": ["Und", "wei\u00dft", "sie", "vor\u00b7treff\u00b7lich", "auf\u00b7zu\u00b7kna\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nu\u00dfknacker, drum bitt' ich, bitt' ich dich,", "tokens": ["Nu\u00df\u00b7knac\u00b7ker", ",", "drum", "bitt'", "ich", ",", "bitt'", "ich", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hast bessere Z\u00e4hn' als ich, Z\u00e4hn' als ich,", "tokens": ["Hast", "bes\u00b7se\u00b7re", "Z\u00e4hn'", "als", "ich", ",", "Z\u00e4hn'", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KOUS", "PPER", "$,", "NN", "KOUS", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "O knacke nur, knacke nur immerzu!", "tokens": ["O", "kna\u00b7cke", "nur", ",", "kna\u00b7cke", "nur", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich will dir zu Ehren", "tokens": ["Ich", "will", "dir", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Kerne verzehren.", "tokens": ["Die", "Ker\u00b7ne", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "O knacke nur, knack knack knack! immerzu!", "tokens": ["O", "kna\u00b7cke", "nur", ",", "knack", "knack", "knack", "!", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "ITJ", "ITJ", "ITJ", "$.", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ei, welch ein braver Kerl bist du!", "tokens": ["Ei", ",", "welch", "ein", "bra\u00b7ver", "Kerl", "bist", "du", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Nu\u00dfknacker, du machst ein grimmig Gesicht \u2013", "tokens": ["Nu\u00df\u00b7knac\u00b7ker", ",", "du", "machst", "ein", "grim\u00b7mig", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "punct", "word", "word",