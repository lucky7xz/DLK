{"dta.poem.925": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "AbENDLIED", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Am Abend, wenn wir auf dunklen Pfaden gehn,", "tokens": ["Am", "A\u00b7bend", ",", "wenn", "wir", "auf", "dunk\u00b7len", "Pfa\u00b7den", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erscheinen unsere bleichen Gestalten vor uns.", "tokens": ["Er\u00b7schei\u00b7nen", "un\u00b7se\u00b7re", "blei\u00b7chen", "Ge\u00b7stal\u00b7ten", "vor", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn uns d\u00fcrstet,", "tokens": ["Wenn", "uns", "d\u00fcrs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Trinken wir die wei\u00dfen Wasser des Teichs,", "tokens": ["Trin\u00b7ken", "wir", "die", "wei\u00b7\u00dfen", "Was\u00b7ser", "des", "Teichs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die S\u00fc\u00dfe unserer traurigen Kindheit.", "tokens": ["Die", "S\u00fc\u00b7\u00dfe", "un\u00b7se\u00b7rer", "trau\u00b7ri\u00b7gen", "Kind\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Erstorbene ruhen wir unterm Hollundergeb\u00fcsch,", "tokens": ["E\u00b7rstor\u00b7be\u00b7ne", "ru\u00b7hen", "wir", "un\u00b7term", "Hol\u00b7lun\u00b7der\u00b7ge\u00b7b\u00fcsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Schaun den grauen M\u00f6ven zu.", "tokens": ["Schaun", "den", "grau\u00b7en", "M\u00f6\u00b7ven", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fr\u00fchlingsgew\u00f6lke steigen \u00fcber die finstere Stadt,", "tokens": ["Fr\u00fch\u00b7lings\u00b7ge\u00b7w\u00f6l\u00b7ke", "stei\u00b7gen", "\u00fc\u00b7ber", "die", "fins\u00b7te\u00b7re", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die der M\u00f6nche edlere Zeiten schweigt.", "tokens": ["Die", "der", "M\u00f6n\u00b7che", "ed\u00b7le\u00b7re", "Zei\u00b7ten", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da ich deine schmalen H\u00e4nde nahm", "tokens": ["Da", "ich", "dei\u00b7ne", "schma\u00b7len", "H\u00e4n\u00b7de", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schlugst du leise die runden Augen auf,", "tokens": ["Schlugst", "du", "lei\u00b7se", "die", "run\u00b7den", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dieses ist lange her.", "tokens": ["Die\u00b7ses", "ist", "lan\u00b7ge", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Doch wenn dunkler Wohllaut die Seele heimsucht,", "tokens": ["Doch", "wenn", "dunk\u00b7ler", "Wohl\u00b7laut", "die", "See\u00b7le", "heim\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erscheinst du Wei\u00dfe in des Freundes herbstlicher", "tokens": ["Er\u00b7scheinst", "du", "Wei\u00b7\u00dfe", "in", "des", "Freun\u00b7des", "herbst\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}