{"dta.poem.3291": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der verlohrne Amor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Amor hat sich j\u00fcngst verlohren;", "tokens": ["A\u00b7mor", "hat", "sich", "j\u00fcngst", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und nun will, die ihn gebohren,", "tokens": ["Und", "nun", "will", ",", "die", "ihn", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Jhren Fl\u00fcchtling wieder k\u00fcssen;", "tokens": ["Ih\u00b7ren", "Fl\u00fccht\u00b7ling", "wie\u00b7der", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und man hat ihn suchen m\u00fcssen.", "tokens": ["Und", "man", "hat", "ihn", "su\u00b7chen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem Schatten dunkler Linden,", "tokens": ["In", "dem", "Schat\u00b7ten", "dunk\u00b7ler", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo wir Dichter Amorn finden;", "tokens": ["Wo", "wir", "Dich\u00b7ter", "A\u00b7morn", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter froher Dichter Myrthen,", "tokens": ["Un\u00b7ter", "fro\u00b7her", "Dich\u00b7ter", "Myr\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den St\u00e4dten, bey den Hirten,", "tokens": ["In", "den", "St\u00e4d\u00b7ten", ",", "bey", "den", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann man nichts von ihm erfragen.", "tokens": ["Kann", "man", "nichts", "von", "ihm", "er\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "M\u00e4dchen! wollt ihr mirs nicht sagen?", "tokens": ["M\u00e4d\u00b7chen", "!", "wollt", "ihr", "mirs", "nicht", "sa\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "NE", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn ihr hegt den Gott der Sorgen:", "tokens": ["Denn", "ihr", "hegt", "den", "Gott", "der", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hat er sich bey euch verborgen?", "tokens": ["Hat", "er", "sich", "bey", "euch", "ver\u00b7bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "In den Rosen eurer Wangen,", "tokens": ["In", "den", "Ro\u00b7sen", "eu\u00b7rer", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die mit frischer Jugend prangen?", "tokens": ["Die", "mit", "fri\u00b7scher", "Ju\u00b7gend", "pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Oder auf den Liljenh\u00fcgeln,", "tokens": ["O\u00b7der", "auf", "den", "Lil\u00b7jen\u00b7h\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wo der Gott mit leisen Fl\u00fcgeln", "tokens": ["Wo", "der", "Gott", "mit", "lei\u00b7sen", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Sich schon \u00f6fters hingestohlen?", "tokens": ["Sich", "schon", "\u00f6f\u00b7ters", "hin\u00b7ge\u00b7stoh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Darf ich suchen und ihn hohlen?", "tokens": ["Darf", "ich", "su\u00b7chen", "und", "ihn", "hoh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "PPER", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}