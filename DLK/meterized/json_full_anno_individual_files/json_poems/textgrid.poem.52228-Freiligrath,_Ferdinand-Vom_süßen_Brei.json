, "measure": "iambic.tetra"}, "line.8": {"text": "So letz' ich euch mit s\u00fc\u00dfem Breie!'", "tokens": ["So", "letz'", "ich", "euch", "mit", "s\u00fc\u00b7\u00dfem", "Brei\u00b7e", "!", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Nun merk': Ich hielt, was ich versprach!", "tokens": ["Nun", "merk'", ":", "Ich", "hielt", ",", "was", "ich", "ver\u00b7sprach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wird sein Wort dem Volke brechen?", "tokens": ["Wer", "wird", "sein", "Wort", "dem", "Vol\u00b7ke", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, heilig sei uns ein Vertrag,", "tokens": ["Nein", ",", "hei\u00b7lig", "sei", "uns", "ein", "Ver\u00b7trag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unumst\u00f6\u00dflich ein Versprechen!", "tokens": ["Und", "un\u00b7um\u00b7st\u00f6\u00df\u00b7lich", "ein", "Ver\u00b7spre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, hat die Schl\u00f6sser, die wir baun,", "tokens": ["Nein", ",", "hat", "die", "Schl\u00f6s\u00b7ser", ",", "die", "wir", "baun", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Schwei\u00df und Blut das Volk gekittet,", "tokens": ["Mit", "Schwei\u00df", "und", "Blut", "das", "Volk", "ge\u00b7kit\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So m\u00f6g' es auch die L\u00f6hnung schaun,", "tokens": ["So", "m\u00f6g'", "es", "auch", "die", "L\u00f6h\u00b7nung", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die nach dem Pakt es sich erbittet!", "tokens": ["Die", "nach", "dem", "Pakt", "es", "sich", "er\u00b7bit\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "O, pr\u00e4chtig war die Gasterei,", "tokens": ["O", ",", "pr\u00e4ch\u00b7tig", "war", "die", "Gas\u00b7te\u00b7rei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als nun die Burg dastand vollendet!", "tokens": ["Als", "nun", "die", "Burg", "das\u00b7tand", "voll\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nie ward zuvor ein s\u00fc\u00dfer Brei", "tokens