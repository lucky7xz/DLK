{"textgrid.poem.52228": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Vom s\u00fc\u00dfen Brei", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist verschwunden wie ein Traum \u2013", "tokens": ["Sie", "ist", "ver\u00b7schwun\u00b7den", "wie", "ein", "Traum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer mag den Grabweg ihr versperren?", "tokens": ["Wer", "mag", "den", "Grab\u00b7weg", "ihr", "ver\u00b7sper\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schwer unterdes auf seinem Flaum,", "tokens": ["Schwer", "un\u00b7ter\u00b7des", "auf", "sei\u00b7nem", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwer ist der Morgenschlaf des Herren.", "tokens": ["Schwer", "ist", "der", "Mor\u00b7gen\u00b7schlaf", "des", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er lallt halbwach: \u00bbDas Volk? das Recht?", "tokens": ["Er", "lallt", "halb\u00b7wach", ":", "\u00bb", "Das", "Volk", "?", "das", "Recht", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was sie nur will? ich m\u00f6cht' es wissen!", "tokens": ["Was", "sie", "nur", "will", "?", "ich", "m\u00f6cht'", "es", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich schlafe diesen Morgen schlecht\u00ab \u2013", "tokens": ["Ich", "schla\u00b7fe", "die\u00b7sen", "Mor\u00b7gen", "schlecht", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADJD", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sinkt zur\u00fcck in seine Kissen.", "tokens": ["Und", "sinkt", "zu\u00b7r\u00fcck", "in", "sei\u00b7ne", "Kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da naht von neuem das Gesicht,", "tokens": ["Da", "naht", "von", "neu\u00b7em", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die letzte Fr\u00fchrast ihm zu st\u00f6ren.", "tokens": ["Die", "letz\u00b7te", "Fr\u00fch\u00b7rast", "ihm", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie tritt zu H\u00e4upten ihm und spricht:", "tokens": ["Sie", "tritt", "zu", "H\u00e4up\u00b7ten", "ihm", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwas du gefragt hast, sollst du h\u00f6ren! \u2013", "tokens": ["\u00bb", "was", "du", "ge\u00b7fragt", "hast", ",", "sollst", "du", "h\u00f6\u00b7ren", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PPER", "VVPP", "VAFIN", "$,", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich baute weiland mir ein Schlo\u00df,", "tokens": ["Ich", "bau\u00b7te", "wei\u00b7land", "mir", "ein", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stolz und in Herrlichkeit zu wohnen!", "tokens": ["Stolz", "und", "in", "Herr\u00b7lich\u00b7keit", "zu", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Aufbaut' ich's mit Vassalentro\u00df \u2013", "tokens": ["Auf\u00b7baut'", "ich's", "mit", "Vas\u00b7sa\u00b7len\u00b7tro\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "NE", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Mein ganzes Dienstvolk mu\u00dfte fronen!", "tokens": ["Mein", "gan\u00b7zes", "Dienst\u00b7volk", "mu\u00df\u00b7te", "fro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlank in die L\u00fcfte stieg der Bau,", "tokens": ["Schlank", "in", "die", "L\u00fcf\u00b7te", "stieg", "der", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlank mit Gew\u00f6lben, Bogen, Gurten!", "tokens": ["Schlank", "mit", "Ge\u00b7w\u00f6l\u00b7ben", ",", "Bo\u00b7gen", ",", "Gur\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aufstieg er, eine pr\u00e4cht'ge Schau,", "tokens": ["Auf\u00b7stieg", "er", ",", "ei\u00b7ne", "pr\u00e4cht'\u00b7ge", "Schau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob auch die Fr\u00f6ner trotzig murrten.", "tokens": ["Ob", "auch", "die", "Fr\u00f6\u00b7ner", "trot\u00b7zig", "murr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da sprach ich: 'Wohl, ich geb' euch Lohn!", "tokens": ["Da", "sprach", "ich", ":", "'", "Wohl", ",", "ich", "geb", "'", "euch", "Lohn", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ADV", "$,", "PPER", "VVFIN", "$(", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So haltet aus denn in der Treue!", "tokens": ["So", "hal\u00b7tet", "aus", "denn", "in", "der", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und endet mit dem Bau die Fron,", "tokens": ["Und", "en\u00b7det", "mit", "dem", "Bau", "die", "Fron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So letz' ich euch mit s\u00fc\u00dfem Breie!'", "tokens": ["So", "letz'", "ich", "euch", "mit", "s\u00fc\u00b7\u00dfem", "Brei\u00b7e", "!", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun merk': Ich hielt, was ich versprach!", "tokens": ["Nun", "merk'", ":", "Ich", "hielt", ",", "was", "ich", "ver\u00b7sprach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wird sein Wort dem Volke brechen?", "tokens": ["Wer", "wird", "sein", "Wort", "dem", "Vol\u00b7ke", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, heilig sei uns ein Vertrag,", "tokens": ["Nein", ",", "hei\u00b7lig", "sei", "uns", "ein", "Ver\u00b7trag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unumst\u00f6\u00dflich ein Versprechen!", "tokens": ["Und", "un\u00b7um\u00b7st\u00f6\u00df\u00b7lich", "ein", "Ver\u00b7spre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, hat die Schl\u00f6sser, die wir baun,", "tokens": ["Nein", ",", "hat", "die", "Schl\u00f6s\u00b7ser", ",", "die", "wir", "baun", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Schwei\u00df und Blut das Volk gekittet,", "tokens": ["Mit", "Schwei\u00df", "und", "Blut", "das", "Volk", "ge\u00b7kit\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So m\u00f6g' es auch die L\u00f6hnung schaun,", "tokens": ["So", "m\u00f6g'", "es", "auch", "die", "L\u00f6h\u00b7nung", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die nach dem Pakt es sich erbittet!", "tokens": ["Die", "nach", "dem", "Pakt", "es", "sich", "er\u00b7bit\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O, pr\u00e4chtig war die Gasterei,", "tokens": ["O", ",", "pr\u00e4ch\u00b7tig", "war", "die", "Gas\u00b7te\u00b7rei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als nun die Burg dastand vollendet!", "tokens": ["Als", "nun", "die", "Burg", "das\u00b7tand", "voll\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nie ward zuvor ein s\u00fc\u00dfer Brei", "tokens": ["Nie", "ward", "zu\u00b7vor", "ein", "s\u00fc\u00b7\u00dfer", "Brei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit volen L\u00f6ffeln so verschwendet!", "tokens": ["Mit", "vo\u00b7len", "L\u00f6f\u00b7feln", "so", "ver\u00b7schwen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und alle Jahr' bei Wein und Brot", "tokens": ["Und", "al\u00b7le", "Jahr'", "bei", "Wein", "und", "Brot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lie\u00df ich den Festtag sich erneuern;", "tokens": ["Lie\u00df", "ich", "den", "Fest\u00b7tag", "sich", "er\u00b7neu\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es mu\u00dft' ihn selbst nach meinem Tod", "tokens": ["Es", "mu\u00dft'", "ihn", "selbst", "nach", "mei\u00b7nem", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die ganze Herrschaft jubelnd feiern.", "tokens": ["Die", "gan\u00b7ze", "Herr\u00b7schaft", "ju\u00b7belnd", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So ward der s\u00fc\u00dfe Brei zum Recht!", "tokens": ["So", "ward", "der", "s\u00fc\u00b7\u00dfe", "Brei", "zum", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verstehst du jetzt mein Reden besser?", "tokens": ["Ver\u00b7stehst", "du", "jetzt", "mein", "Re\u00b7den", "bes\u00b7ser", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Sohn, du und dein Vorgeschlecht,", "tokens": ["O", "Sohn", ",", "du", "und", "dein", "Vor\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr habt erhoben viele Schl\u00f6sser!", "tokens": ["Ihr", "habt", "er\u00b7ho\u00b7ben", "vie\u00b7le", "Schl\u00f6s\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und viele Worte sind gesagt,", "tokens": ["Und", "vie\u00b7le", "Wor\u00b7te", "sind", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die s\u00fc\u00dfen Brei dem Volk verhie\u00dfen \u2013", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "Brei", "dem", "Volk", "ver\u00b7hie\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kannst du dich wundern, wenn es klagt,", "tokens": ["Kannst", "du", "dich", "wun\u00b7dern", ",", "wenn", "es", "klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und endlich Lust hat, zu genie\u00dfen?", "tokens": ["Und", "end\u00b7lich", "Lust", "hat", ",", "zu", "ge\u00b7nie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es gab dir Blut, es gab dir Schwei\u00df,", "tokens": ["Es", "gab", "dir", "Blut", ",", "es", "gab", "dir", "Schwei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wird dir, was es gab, nicht schenken!", "tokens": ["Und", "wird", "dir", ",", "was", "es", "gab", ",", "nicht", "schen\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O, wolle doch des s\u00fc\u00dfen Breis,", "tokens": ["O", ",", "wol\u00b7le", "doch", "des", "s\u00fc\u00b7\u00dfen", "Breis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den du versprochen, bald gedenken!", "tokens": ["Den", "du", "ver\u00b7spro\u00b7chen", ",", "bald", "ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, gib den Brei, den s\u00fc\u00dfen Brei!", "tokens": ["O", ",", "gib", "den", "Brei", ",", "den", "s\u00fc\u00b7\u00dfen", "Brei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer wei\u00df, was wird! rasch fliehn die Stunden!\u00ab", "tokens": ["Wer", "wei\u00df", ",", "was", "wird", "!", "rasch", "fliehn", "die", "Stun\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VAFIN", "$.", "ADJD", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aufwacht der Herr mit j\u00e4hem Schrei,", "tokens": ["Auf\u00b7wacht", "der", "Herr", "mit", "j\u00e4\u00b7hem", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wiederum ist sie verschwunden!", "tokens": ["Und", "wie\u00b7de\u00b7rum", "ist", "sie", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie ist verschwunden wie ein Traum \u2013", "tokens": ["Sie", "ist", "ver\u00b7schwun\u00b7den", "wie", "ein", "Traum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer mag den Grabweg ihr versperren?", "tokens": ["Wer", "mag", "den", "Grab\u00b7weg", "ihr", "ver\u00b7sper\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schwer unterdes auf seinem Flaum,", "tokens": ["Schwer", "un\u00b7ter\u00b7des", "auf", "sei\u00b7nem", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwer ist der Morgenschlaf des Herren.", "tokens": ["Schwer", "ist", "der", "Mor\u00b7gen\u00b7schlaf", "des", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er lallt halbwach: \u00bbDas Volk? das Recht?", "tokens": ["Er", "lallt", "halb\u00b7wach", ":", "\u00bb", "Das", "Volk", "?", "das", "Recht", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was sie nur will? ich m\u00f6cht' es wissen!", "tokens": ["Was", "sie", "nur", "will", "?", "ich", "m\u00f6cht'", "es", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich schlafe diesen Morgen schlecht\u00ab \u2013", "tokens": ["Ich", "schla\u00b7fe", "die\u00b7sen", "Mor\u00b7gen", "schlecht", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADJD", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sinkt zur\u00fcck in seine Kissen.", "tokens": ["Und", "sinkt", "zu\u00b7r\u00fcck", "in", "sei\u00b7ne", "Kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da naht von neuem das Gesicht,", "tokens": ["Da", "naht", "von", "neu\u00b7em", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die letzte Fr\u00fchrast ihm zu st\u00f6ren.", "tokens": ["Die", "letz\u00b7te", "Fr\u00fch\u00b7rast", "ihm", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie tritt zu H\u00e4upten ihm und spricht:", "tokens": ["Sie", "tritt", "zu", "H\u00e4up\u00b7ten", "ihm", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbwas du gefragt hast, sollst du h\u00f6ren! \u2013", "tokens": ["\u00bb", "was", "du", "ge\u00b7fragt", "hast", ",", "sollst", "du", "h\u00f6\u00b7ren", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PPER", "VVPP", "VAFIN", "$,", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich baute weiland mir ein Schlo\u00df,", "tokens": ["Ich", "bau\u00b7te", "wei\u00b7land", "mir", "ein", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stolz und in Herrlichkeit zu wohnen!", "tokens": ["Stolz", "und", "in", "Herr\u00b7lich\u00b7keit", "zu", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Aufbaut' ich's mit Vassalentro\u00df \u2013", "tokens": ["Auf\u00b7baut'", "ich's", "mit", "Vas\u00b7sa\u00b7len\u00b7tro\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "NE", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Mein ganzes Dienstvolk mu\u00dfte fronen!", "tokens": ["Mein", "gan\u00b7zes", "Dienst\u00b7volk", "mu\u00df\u00b7te", "fro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Schlank in die L\u00fcfte stieg der Bau,", "tokens": ["Schlank", "in", "die", "L\u00fcf\u00b7te", "stieg", "der", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlank mit Gew\u00f6lben, Bogen, Gurten!", "tokens": ["Schlank", "mit", "Ge\u00b7w\u00f6l\