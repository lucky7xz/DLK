{"textgrid.poem.49915": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6ttliche Reminiszenz", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorl\u00e4ngst sah ich ein wundersames Bild gemalt,", "tokens": ["Vor\u00b7l\u00e4ngst", "sah", "ich", "ein", "wun\u00b7der\u00b7sa\u00b7mes", "Bild", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Im Kloster der Kart\u00e4user, das ich oft besucht.", "tokens": ["Im", "Klos\u00b7ter", "der", "Kar\u00b7t\u00e4u\u00b7ser", ",", "das", "ich", "oft", "be\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heut, da ich im Gebirge droben einsam ging,", "tokens": ["Heut", ",", "da", "ich", "im", "Ge\u00b7bir\u00b7ge", "dro\u00b7ben", "ein\u00b7sam", "ging", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Umstarrt von wild zerstreuter Felsentr\u00fcmmersaat,", "tokens": ["Um\u00b7starrt", "von", "wild", "zer\u00b7streu\u00b7ter", "Fel\u00b7sen\u00b7tr\u00fcm\u00b7mer\u00b7saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Trat es mit frischen Farben vor die Seele mir.", "tokens": ["Trat", "es", "mit", "fri\u00b7schen", "Far\u00b7ben", "vor", "die", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An j\u00e4her Steinkluft, deren d\u00fcnn begraster Saum,", "tokens": ["An", "j\u00e4\u00b7her", "Stein\u00b7kluft", ",", "de\u00b7ren", "d\u00fcnn", "be\u00b7gras\u00b7ter", "Saum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von zweien Palmen \u00fcberschattet, magre Kost", "tokens": ["Von", "zwei\u00b7en", "Pal\u00b7men", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", ",", "mag\u00b7re", "Kost"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Ziegen beut, den steilauf weidenden am Hang,", "tokens": ["Den", "Zie\u00b7gen", "beut", ",", "den", "stei\u00b7lauf", "wei\u00b7den\u00b7den", "am", "Hang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieht man den Knaben Jesus sitzend auf Gestein;", "tokens": ["Sieht", "man", "den", "Kna\u00b7ben", "Je\u00b7sus", "sit\u00b7zend", "auf", "Ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "NE", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein wei\u00dfes Vlies als Polster ist ihm unterlegt.", "tokens": ["Ein", "wei\u00b7\u00dfes", "Vlies", "als", "Pols\u00b7ter", "ist", "ihm", "un\u00b7ter\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht allzu kindlich deuchte mir das sch\u00f6ne Kind;", "tokens": ["Nicht", "all\u00b7zu", "kind\u00b7lich", "deuch\u00b7te", "mir", "das", "sch\u00f6\u00b7ne", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der hei\u00dfe Sommer, sicherlich sein f\u00fcnfter schon,", "tokens": ["Der", "hei\u00b7\u00dfe", "Som\u00b7mer", ",", "si\u00b7cher\u00b7lich", "sein", "f\u00fcnf\u00b7ter", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "PPOSAT", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat seine Glieder, welche bis zum Knie herab", "tokens": ["Hat", "sei\u00b7ne", "Glie\u00b7der", ",", "wel\u00b7che", "bis", "zum", "Knie", "her\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das gelbe R\u00f6ckchen decket mit dem Purpursaum,", "tokens": ["Das", "gel\u00b7be", "R\u00f6ck\u00b7chen", "de\u00b7cket", "mit", "dem", "Pur\u00b7pur\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hat die gesunden, zarten Wangen sanft gebr\u00e4unt;", "tokens": ["Hat", "die", "ge\u00b7sun\u00b7den", ",", "zar\u00b7ten", "Wan\u00b7gen", "sanft", "ge\u00b7br\u00e4unt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Aus schwarzen Augen leuchtet stille Feuerkraft,", "tokens": ["Aus", "schwar\u00b7zen", "Au\u00b7gen", "leuch\u00b7tet", "stil\u00b7le", "Feu\u00b7er\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Mund jedoch umfremdet unnennbarer Reiz.", "tokens": ["Den", "Mund", "je\u00b7doch", "um\u00b7frem\u00b7det", "un\u00b7nenn\u00b7ba\u00b7rer", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein alter Hirte, freundlich zu dem Kind gebeugt,", "tokens": ["Ein", "al\u00b7ter", "Hir\u00b7te", ",", "freund\u00b7lich", "zu", "dem", "Kind", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gab ihm soeben ein versteinert Meergew\u00e4chs,", "tokens": ["Gab", "ihm", "soe\u00b7ben", "ein", "ver\u00b7stei\u00b7nert", "Meer\u00b7ge\u00b7w\u00e4chs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "VVPP", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Seltsam gestaltet, in die Hand zum Zeitvertreib.", "tokens": ["Selt\u00b7sam", "ge\u00b7stal\u00b7tet", ",", "in", "die", "Hand", "zum", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Der Knabe hat das Wunderding beschaut, und jetzt,", "tokens": ["Der", "Kna\u00b7be", "hat", "das", "Wun\u00b7der\u00b7ding", "be\u00b7schaut", ",", "und", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gleichsam betroffen, spannet sich der weite Blick,", "tokens": ["Gleich\u00b7sam", "be\u00b7trof\u00b7fen", ",", "span\u00b7net", "sich", "der", "wei\u00b7te", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Entgegen dir, doch wirklich ohne Gegenstand,", "tokens": ["Ent\u00b7ge\u00b7gen", "dir", ",", "doch", "wirk\u00b7lich", "oh\u00b7ne", "Ge\u00b7gen\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durchdringend ewge Zeitenfernen, grenzenlos:", "tokens": ["Durch\u00b7drin\u00b7gend", "ew\u00b7ge", "Zei\u00b7ten\u00b7fer\u00b7nen", ",", "gren\u00b7zen\u00b7los", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als wittre durch die \u00fcberw\u00f6lkte Stirn ein Blitz", "tokens": ["Als", "witt\u00b7re", "durch", "die", "\u00fc\u00b7berw\u00b7\u00f6lk\u00b7te", "Stirn", "ein", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Gottheit, ein Erinnern, das im gleichen Nu", "tokens": ["Der", "Got\u00b7theit", ",", "ein", "E\u00b7rin\u00b7nern", ",", "das", "im", "glei\u00b7chen", "Nu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Erloschen sein wird; und das welterschaffende,", "tokens": ["Er\u00b7lo\u00b7schen", "sein", "wird", ";", "und", "das", "wel\u00b7ter\u00b7schaf\u00b7fen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "VAFIN", "$.", "KON", "ART", "ADJA", "$,"], "meter": "-+-++--+-+--", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Das Wort von Anfang, als ein spielend Erdenkind", "tokens": ["Das", "Wort", "von", "An\u00b7fang", ",", "als", "ein", "spie\u00b7lend", "Er\u00b7den\u00b7kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KOUS", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit L\u00e4cheln zeigt's unwissend dir sein eigen Werk.", "tokens": ["Mit", "L\u00e4\u00b7cheln", "zeigt's", "un\u00b7wis\u00b7send", "dir", "sein", "ei\u00b7gen", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vorl\u00e4ngst sah ich ein wundersames Bild gemalt,", "tokens": ["Vor\u00b7l\u00e4ngst", "sah", "ich", "ein", "wun\u00b7der\u00b7sa\u00b7mes", "Bild", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Im Kloster der Kart\u00e4user, das ich oft besucht.", "tokens": ["Im", "Klos\u00b7ter", "der", "Kar\u00b7t\u00e4u\u00b7ser", ",", "das", "ich", "oft", "be\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heut, da ich im Gebirge droben einsam ging,", "tokens": ["Heut", ",", "da", "ich", "im", "Ge\u00b7bir\u00b7ge", "dro\u00b7ben", "ein\u00b7sam", "ging", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Umstarrt von wild zerstreuter Felsentr\u00fcmmersaat,", "tokens": ["Um\u00b7starrt", "von", "wild", "zer\u00b7streu\u00b7ter", "Fel\u00b7sen\u00b7tr\u00fcm\u00b7mer\u00b7saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Trat es mit frischen Farben vor die Seele mir.",