{"dta.poem.21866": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxII,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sieger hohe Pracht/ der Tichter Ange-", "tokens": ["Der", "Sie\u00b7ger", "ho\u00b7he", "Pracht", "/", "der", "Tich\u00b7ter", "An\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Jungfern Ruhm und Lust nahm mich", "tokens": ["der", "Jung\u00b7fern", "Ruhm", "und", "Lust", "nahm", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wiewol erging mirs dar! Mein Amor kahm", "tokens": ["Wie\u00b7wol", "er\u00b7ging", "mirs", "dar", "!", "Mein", "A\u00b7mor", "kahm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "PTKVZ", "$.", "PPOSAT", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und stach mich tapfer an. Du Gl\u00fckk ver-", "tokens": ["und", "stach", "mich", "tap\u00b7fer", "an", ".", "Du", "Gl\u00fckk", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PPER", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "erteilst mir alle Lust/ und meine Lust-gefehrden", "tokens": ["er\u00b7teilst", "mir", "al\u00b7le", "Lust", "/", "und", "mei\u00b7ne", "Lust\u00b7ge\u00b7fehr\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "verhindern/ da\u00df ich nicht der Lust kan f\u00e4-", "tokens": ["ver\u00b7hin\u00b7dern", "/", "da\u00df", "ich", "nicht", "der", "Lust", "kan", "f\u00e4"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVINF", "$(", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}