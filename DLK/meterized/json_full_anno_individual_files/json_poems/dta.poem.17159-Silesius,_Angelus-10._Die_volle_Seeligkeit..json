{"dta.poem.17159": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "10.  Die volle Seeligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch hat eher nicht vollkommne Seeligkeit;", "tokens": ["Der", "Mensch", "hat", "e\u00b7her", "nicht", "voll\u00b7komm\u00b7ne", "See\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00df da\u00df die Einheit hat verschlukt die Anderheit.", "tokens": ["Bi\u00df", "da\u00df", "die", "Ein\u00b7heit", "hat", "ver\u00b7schlukt", "die", "An\u00b7der\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VAFIN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}