 "letz\u00b7lich", "wirst", "er\u00b7kal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "so soll dich zu allen Zeiten", "tokens": ["so", "soll", "dich", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "meine Klio auch begleiten.", "tokens": ["mei\u00b7ne", "Klio", "auch", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.18": {"line.1": {"text": "Trautes Paar der Pierinnen,", "tokens": ["Trau\u00b7tes", "Paar", "der", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "das nicht nur ein F\u00f6bus liebt,", "tokens": ["das", "nicht", "nur", "ein", "F\u00f6\u00b7bus", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dem sich iede Charis giebt,", "tokens": ["dem", "sich", "ie\u00b7de", "Cha\u00b7ris", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "hohen Blutes, hoher Sinnen,", "tokens": ["ho\u00b7hen", "Blu\u00b7tes", ",", "ho\u00b7her", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"t