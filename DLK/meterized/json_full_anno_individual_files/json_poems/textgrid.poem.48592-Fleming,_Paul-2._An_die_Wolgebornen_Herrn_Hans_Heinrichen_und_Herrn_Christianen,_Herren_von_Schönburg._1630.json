{"textgrid.poem.48592": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. An die Wolgebornen Herrn Hans Heinrichen und Herrn Christianen, Herren von Sch\u00f6nburg. 1630", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trautes Paar der Pierinnen,", "tokens": ["Trau\u00b7tes", "Paar", "der", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "das nicht nur ein F\u00f6bus liebt,", "tokens": ["das", "nicht", "nur", "ein", "F\u00f6\u00b7bus", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dem sich iede Charis giebt,", "tokens": ["dem", "sich", "ie\u00b7de", "Cha\u00b7ris", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "hohen Blutes, hoher Sinnen,", "tokens": ["ho\u00b7hen", "Blu\u00b7tes", ",", "ho\u00b7her", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Paar an Ehren, Paar an Tugend,", "tokens": ["Paar", "an", "Eh\u00b7ren", ",", "Paar", "an", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Paar einander gleich an Jugend?", "tokens": ["Paar", "ein\u00b7an\u00b7der", "gleich", "an", "Ju\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll denn meine Klio singen,", "tokens": ["Soll", "denn", "mei\u00b7ne", "Klio", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Klio, meine Bauer-Magd,", "tokens": ["Klio", ",", "mei\u00b7ne", "Bau\u00b7e\u00b7rMagd", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "dich, von dem Apollo sagt,", "tokens": ["dich", ",", "von", "dem", "A\u00b7pol\u00b7lo", "sagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "da\u00df die Saiten nicht recht klingen,", "tokens": ["da\u00df", "die", "Sai\u00b7ten", "nicht", "recht", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn er will zu deinen Ehren", "tokens": ["wenn", "er", "will", "zu", "dei\u00b7nen", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ein Get\u00f6ne lassen h\u00f6ren?", "tokens": ["ein", "Ge\u00b7t\u00f6\u00b7ne", "las\u00b7sen", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch schallt es noch fein in P\u00fcschen,", "tokens": ["Doch", "schallt", "es", "noch", "fein", "in", "P\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wenn die frische Fyllis t\u00f6nt,", "tokens": ["wenn", "die", "fri\u00b7sche", "Fyl\u00b7lis", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "fr\u00fche, wenn sich Titan sehnt", "tokens": ["fr\u00fc\u00b7he", ",", "wenn", "sich", "Ti\u00b7tan", "sehnt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "$,", "KOUS", "PRF", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "aus den Wassern vorzuwischen,", "tokens": ["aus", "den", "Was\u00b7sern", "vor\u00b7zu\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn sie singet von dem Herzen,", "tokens": ["wenn", "sie", "sin\u00b7get", "von", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "das ihr Mopsus gab mit Scherzen.", "tokens": ["das", "ihr", "Mop\u00b7sus", "gab", "mit", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meine Klio, meine Liebe,", "tokens": ["Mei\u00b7ne", "Klio", ",", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "lasse dich nichts fechten an,", "tokens": ["las\u00b7se", "dich", "nichts", "fech\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ob hier F\u00f6bus selbst nichts kan!", "tokens": ["ob", "hier", "F\u00f6\u00b7bus", "selbst", "nichts", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "ADV", "PIS", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stell dein Antlitz nur nicht tr\u00fcbe,", "tokens": ["Stell", "dein", "Ant\u00b7litz", "nur", "nicht", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": " . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}, "line.6": {"text": " . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct"