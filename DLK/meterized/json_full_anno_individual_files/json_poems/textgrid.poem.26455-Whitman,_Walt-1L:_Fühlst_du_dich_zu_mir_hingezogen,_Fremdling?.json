{"textgrid.poem.26455": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fchlst du dich zu mir hingezogen, Fremdling?", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchlst du dich zu mir hingezogen, Fremdling?", "tokens": ["F\u00fchlst", "du", "dich", "zu", "mir", "hin\u00b7ge\u00b7zo\u00b7gen", ",", "Fremd\u00b7ling", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ich warne dich, nimm dich vor mir in acht! denn ich bin sicherlich ganz anders als du denkst.", "tokens": ["Ich", "war\u00b7ne", "dich", ",", "nimm", "dich", "vor", "mir", "in", "acht", "!", "denn", "ich", "bin", "si\u00b7cher\u00b7lich", "ganz", "an\u00b7ders", "als", "du", "denkst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "APPR", "PPER", "APPR", "CARD", "$.", "KON", "PPER", "VAFIN", "PRF", "ADV", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Meinst du in mir dein Ideal zu finden?", "tokens": ["Meinst", "du", "in", "mir", "dein", "I\u00b7deal", "zu", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meinst du, es w\u00e4re so leicht, mich zum Geliebten zu gewinnen?", "tokens": ["Meinst", "du", ",", "es", "w\u00e4\u00b7re", "so", "leicht", ",", "mich", "zum", "Ge\u00b7lieb\u00b7ten", "zu", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Meinst du, meine Freundschaft br\u00e4chte dir ungest\u00f6rte Befriedigung?", "tokens": ["Meinst", "du", ",", "mei\u00b7ne", "Freund\u00b7schaft", "br\u00e4ch\u00b7te", "dir", "un\u00b7ge\u00b7st\u00f6r\u00b7te", "Be\u00b7frie\u00b7di\u00b7gung", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Meinst du, ich sei verl\u00e4\u00dflich und treu?", "tokens": ["Meinst", "du", ",", "ich", "sei", "ver\u00b7l\u00e4\u00df\u00b7lich", "und", "treu", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Blickst du nicht tiefer als auf diese glatte Fassade, dieses milde, leutselige Betragen?", "tokens": ["Blickst", "du", "nicht", "tie\u00b7fer", "als", "auf", "die\u00b7se", "glat\u00b7te", "Fas\u00b7sa\u00b7de", ",", "die\u00b7ses", "mil\u00b7de", ",", "leut\u00b7se\u00b7li\u00b7ge", "Be\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "KOKOM", "APPR", "PDAT", "ADJA", "NN", "$,", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+--+-+-+-+-+--+-", "measure": "dactylic.tetra.plus"}, "line.8": {"text": "Bildest du dir ein, du k\u00e4mest auf sicherem Boden zu einem wirklich heldenhaften Mann?", "tokens": ["Bil\u00b7dest", "du", "dir", "ein", ",", "du", "k\u00e4\u00b7mest", "auf", "si\u00b7che\u00b7rem", "Bo\u00b7den", "zu", "ei\u00b7nem", "wirk\u00b7lich", "hel\u00b7den\u00b7haf\u00b7ten", "Mann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Kommt dir nie der Gedanke, o Tr\u00e4umer, da\u00df alles am Ende nur Maya sein kann, nur Schein?", "tokens": ["Kommt", "dir", "nie", "der", "Ge\u00b7dan\u00b7ke", ",", "o", "Tr\u00e4u\u00b7mer", ",", "da\u00df", "al\u00b7les", "am", "En\u00b7de", "nur", "Ma\u00b7ya", "sein", "kann", ",", "nur", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,", "FM", "NN", "$,", "KOUS", "PIS", "APPRART", "NN", "ADV", "NE", "VAINF", "VMFIN", "$,", "ADV", "NN", "$."], "meter": "--+--+--+--+--+-+-+-+-+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "F\u00fchlst du dich zu mir hingezogen, Fremdling?", "tokens": ["F\u00fchlst", "du", "dich", "zu", "mir", "hin\u00b7ge\u00b7zo\u00b7gen", ",", "Fremd\u00b7ling", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ich warne dich, nimm dich vor mir in acht! denn ich bin sicherlich ganz anders als du denkst.", "token