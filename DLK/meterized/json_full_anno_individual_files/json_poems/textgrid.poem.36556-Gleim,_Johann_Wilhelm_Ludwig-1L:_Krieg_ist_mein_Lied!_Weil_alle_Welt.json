{"textgrid.poem.36556": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Krieg ist mein Lied! Weil alle Welt", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Krieg ist mein Lied! Weil alle Welt", "tokens": ["Krieg", "ist", "mein", "Lied", "!", "Weil", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "KOUS", "PIAT", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Krieg will, so sei es Krieg!", "tokens": ["Krieg", "will", ",", "so", "sei", "es", "Krieg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Berlin sei Sparta! Preu\u00dfens Held", "tokens": ["Ber\u00b7lin", "sei", "Spar\u00b7ta", "!", "Preu\u00b7\u00dfens", "Held"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "NE", "$.", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gekr\u00f6nt mit Ruhm und Sieg!", "tokens": ["Ge\u00b7kr\u00f6nt", "mit", "Ruhm", "und", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gern will ich seine Thaten thun;", "tokens": ["Gern", "will", "ich", "sei\u00b7ne", "Tha\u00b7ten", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leier in der Hand,", "tokens": ["Die", "Lei\u00b7er", "in", "der", "Hand", "