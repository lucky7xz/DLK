 ",", "ein", "tapf\u00b7rer", "Gre\u00b7na\u00b7di\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Was acht' ich es, wenn \u00fcber mir", "tokens": ["Was", "acht'", "ich", "es", ",", "wenn", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kanonendonner br\u00fcllt?", "tokens": ["Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", "br\u00fcllt", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Held fall' ich; noch sterbend droht", "tokens": ["Ein", "Held", "fall'", "ich", ";", "noch", "ster\u00b7bend", "droht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein S\u00e4bel in der Hand!", "tokens": ["Mein", "S\u00e4\u00b7bel", "in", "der", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unsterblich macht der Helden Tod,", "tokens": ["U\u00b7nsterb\u00b7lich", "macht", "der", "Hel\u00b7den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der Tod f\u00fcrs Vaterland!", "tokens": ["Der", "Tod", "f\u00fcrs", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auch k\u00f6mmt man aus der Welt davon,", "tokens": ["Auch", "k\u00f6mmt", "man", "aus", "der", "Welt", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinder wie der Blitz;", "tokens": ["Ge\u00b7schwin\u00b7der", "wie", "der", "Blitz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer ihn stirbt, bek\u00f6mmt zum Lohn,", "tokens": ["Und", "wer", "ihn", "stirbt", ",", "be\u00b7k\u00f6mmt", "zum", "Lohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Himmel hohen Sitz!", "tokens": ["Im", "Him\u00b7mel", "ho\u00b7hen", "Sitz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn aber ich, als solch ein Held,", "tokens": ["Wenn", "a\u00b7ber", "ich", ",", "als", "solch", "ein", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,", "KOUS", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Mars, nicht sterben soll,", "tokens": ["Dir", ",", "Mars", ",", "nicht", "ster\u00b7ben", "soll", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PP