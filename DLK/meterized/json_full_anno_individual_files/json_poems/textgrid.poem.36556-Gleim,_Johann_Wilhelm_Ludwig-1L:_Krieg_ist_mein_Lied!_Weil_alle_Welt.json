n_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Wenn aber ich, als solch ein Held,", "tokens": ["Wenn", "a\u00b7ber", "ich", ",", "als", "solch", "ein", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,", "KOUS", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Mars, nicht sterben soll,", "tokens": ["Dir", ",", "Mars", ",", "nicht", "ster\u00b7ben", "soll", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht gl\u00e4nzen soll im Sternenzelt:", "tokens": ["Nicht", "gl\u00e4n\u00b7zen", "soll", "im", "Ster\u00b7nen\u00b7zelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So leb' ich dem Apoll!", "tokens": ["So", "leb'", "ich", "dem", "A\u00b7poll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.18": {"line.1": {"text": "So werd' aus ", "tokens": ["So", "werd'", "aus"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Dem Schutz, der Ruhm des Staats:", "tokens": ["Dem", "Schutz", ",", "der", "Ruhm", "des", "Staats", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lern' er deutscher Sprache Zier,", "tokens": ["So", "lern'", "er", "deut\u00b7scher", "Spra\u00b7che", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und werde sein Horaz.", "tokens": ["Und", "wer\u00b7de", "sein", "Ho\u00b7raz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.19": {"line.1": {"text": "Dann singe Gott und ", "tokens": ["Dann", "sin\u00b7ge", "Gott", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nichts kleiners, stolzes Lied!", "tokens": ["Nichts", "klei\u00b7ners", ",", "stol\u00b7zes", "Lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Adler gleich erhebe dich,", "tokens": ["Dem", "Ad\u00b7ler", "gleich", "er\u00b7he\u00b7be", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der in die Sonne sieht!", "tokens": ["Der", "in", "die", "Son\u00b7ne", "sieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "\u2013 \u2013 ", "tokens": ["\u2013", "\u2013"], "token_info": ["punct", "punct"], "pos": ["$(", "$("]}}}}}