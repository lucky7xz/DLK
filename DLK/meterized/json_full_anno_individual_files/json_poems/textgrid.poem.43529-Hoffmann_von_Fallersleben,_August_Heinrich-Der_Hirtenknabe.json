{"textgrid.poem.43529": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hirtenknabe", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eben wann der Morgen graut,", "tokens": ["E\u00b7ben", "wann", "der", "Mor\u00b7gen", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch in aller Fr\u00fche", "tokens": ["Noch", "in", "al\u00b7ler", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Treib' ich t\u00e4glich auf die Alp,", "tokens": ["Treib'", "ich", "t\u00e4g\u00b7lich", "auf", "die", "Alp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich meine K\u00fche.", "tokens": ["T\u00e4g\u00b7lich", "mei\u00b7ne", "K\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und die V\u00f6gel singen dann", "tokens": ["Und", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir die sch\u00f6nsten Lieder,", "tokens": ["Mir", "die", "sch\u00f6ns\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"t