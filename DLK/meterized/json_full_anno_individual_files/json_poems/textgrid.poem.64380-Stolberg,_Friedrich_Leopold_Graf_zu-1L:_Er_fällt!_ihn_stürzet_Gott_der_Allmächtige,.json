{"textgrid.poem.64380": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er f\u00e4llt! ihn st\u00fcrzet Gott der Allm\u00e4chtige,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er f\u00e4llt! ihn st\u00fcrzet Gott der Allm\u00e4chtige,", "tokens": ["Er", "f\u00e4llt", "!", "ihn", "st\u00fcr\u00b7zet", "Gott", "der", "All\u00b7m\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der auf der Wage, welche Tyrannen w\u00e4gt", "tokens": ["Der", "auf", "der", "Wa\u00b7ge", ",", "wel\u00b7che", "Ty\u00b7ran\u00b7nen", "w\u00e4gt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Landesv\u00e4ter, mit umw\u00f6lkter", "tokens": ["Und", "Lan\u00b7des\u00b7v\u00e4\u00b7ter", ",", "mit", "um\u00b7w\u00f6lk\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rechte den Frevelnden wog und leicht fand.", "tokens": ["Rech\u00b7te", "den", "Fre\u00b7veln\u00b7den", "wog", "und", "leicht", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Er f\u00e4llt! Vielleicht schon st\u00e4ubet der Schnee vom Huf", "tokens": ["Er", "f\u00e4llt", "!", "Viel\u00b7leicht", "schon", "st\u00e4u\u00b7bet", "der", "Schnee", "vom", "Huf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der schnellen Boten, welche, \u00bbverworfen sei", "tokens": ["Der", "schnel\u00b7len", "Bo\u00b7ten", ",", "wel\u00b7che", ",", "\u00bb", "ver\u00b7wor\u00b7fen", "sei"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "$(", "VVPP", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Volke der von Gott Verworfne,\u00ab", "tokens": ["Vom", "Vol\u00b7ke", "der", "von", "Gott", "Ver\u00b7worf\u00b7ne", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "APPR", "NN", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Melden dem Aufgang und Niedergange.", "tokens": ["Mel\u00b7den", "dem", "Auf\u00b7gang", "und", "Nie\u00b7der\u00b7gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "O gebt die Ehre Gott dem Allm\u00e4chtigen!", "tokens": ["O", "gebt", "die", "Eh\u00b7re", "Gott", "dem", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor seinem Hauch erstarrten in Nordens Scho\u00df", "tokens": ["Vor", "sei\u00b7nem", "Hauch", "er\u00b7starr\u00b7ten", "in", "Nor\u00b7dens", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die R\u00e4uberscharen, fluchten sterbend", "tokens": ["Die", "R\u00e4u\u00b7ber\u00b7scha\u00b7ren", ",", "fluch\u00b7ten", "ster\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem, der allein wie ein Dieb entschl\u00fcpfte.", "tokens": ["Dem", ",", "der", "al\u00b7lein", "wie", "ein", "Dieb", "ent\u00b7schl\u00fcpf\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Entrinnen lie\u00df ihn Gott der Allweise, weil", "tokens": ["Ent\u00b7rin\u00b7nen", "lie\u00df", "ihn", "Gott", "der", "All\u00b7wei\u00b7se", ",", "weil"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "NN", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein Ma\u00df nicht voll war! lie\u00df ihn den Taumelkelch", "tokens": ["Sein", "Ma\u00df", "nicht", "voll", "war", "!", "lie\u00df", "ihn", "den", "Tau\u00b7mel\u00b7kelch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADJD", "VAFIN", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bis zu den Hefen leeren! Schwindelnd", "tokens": ["Bis", "zu", "den", "He\u00b7fen", "lee\u00b7ren", "!", "Schwin\u00b7delnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VVINF", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00fcrfte mit ihm das beth\u00f6rte Volk noch.", "tokens": ["Schl\u00fcrf\u00b7te", "mit", "ihm", "das", "be\u00b7th\u00f6r\u00b7te", "Volk", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und neue Scharen schwirrten, wie Hornisse", "tokens": ["Und", "neu\u00b7e", "Scha\u00b7ren", "schwirr\u00b7ten", ",", "wie", "Hor\u00b7nis\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "PWAV", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Sommersglut durch nordische Kiefern ziehn,", "tokens": ["In", "Som\u00b7mers\u00b7glut", "durch", "nor\u00b7di\u00b7sche", "Kie\u00b7fern", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gereizt und zahllos; noch verma\u00df sich", "tokens": ["Ge\u00b7reizt", "und", "zahl\u00b7los", ";", "noch", "ver\u00b7ma\u00df", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KON", "ADJD", "$.", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stolzer Entscheidung der eitle Korse.", "tokens": ["Stol\u00b7zer", "Ent\u00b7schei\u00b7dung", "der", "eit\u00b7le", "Kor\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Entschieden aber hatte der Alte schon", "tokens": ["Ent\u00b7schie\u00b7den", "a\u00b7ber", "hat\u00b7te", "der", "Al\u00b7te", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Rat der W\u00e4chter; hatte der Weisheit Rat", "tokens": ["Im", "Rat", "der", "W\u00e4ch\u00b7ter", ";", "hat\u00b7te", "der", "Weis\u00b7heit", "Rat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NE", "$.", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Landesv\u00e4tern, und der Eintracht", "tokens": ["Den", "Lan\u00b7des\u00b7v\u00e4\u00b7tern", ",", "und", "der", "Ein\u00b7tracht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sinn, und der Demut, ins Herz gehauchet.", "tokens": ["Sinn", ",", "und", "der", "De\u00b7mut", ",", "ins", "Herz", "ge\u00b7hau\u00b7chet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Die Demut schaut gen Himmel, und geht einher", "tokens": ["Die", "De\u00b7mut", "schaut", "gen", "Him\u00b7mel", ",", "und", "geht", "ein\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit leisem Fu\u00df, doch unter der Wandelnden", "tokens": ["Mit", "lei\u00b7sem", "Fu\u00df", ",", "doch", "un\u00b7ter", "der", "Wan\u00b7deln\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erbebt der Abgrund, seine G\u00f6tter", "tokens": ["Er\u00b7bebt", "der", "Ab\u00b7grund", ",", "sei\u00b7ne", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fahren geschreckt von den ehrnen Thronen!", "tokens": ["Fah\u00b7ren", "ge\u00b7schreckt", "von", "den", "ehr\u00b7nen", "Thro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Nicht so der Erde G\u00f6tze von gestern her:", "tokens": ["Nicht", "so", "der", "Er\u00b7de", "G\u00f6t\u00b7ze", "von", "ge\u00b7stern", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er log noch Siege, tr\u00e4umte noch Herrschaft, zog", "tokens": ["Er", "log", "noch", "Sie\u00b7ge", ",", "tr\u00e4um\u00b7te", "noch", "Herr\u00b7schaft", ",", "zog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "VVFIN", "ADV", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit \u00dcbermacht heran; da st\u00fcrzten", "tokens": ["Mit", "\u00dc\u00b7ber\u00b7macht", "he\u00b7ran", ";", "da", "st\u00fcrz\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hin vor den Helden die R\u00e4uberscharen", "tokens": ["Hin", "vor", "den", "Hel\u00b7den", "die", "R\u00e4u\u00b7ber\u00b7scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Wie Wetter Gottes \u2013 siehe, noch rollt im Thal", "tokens": ["Wie", "Wet\u00b7ter", "Got\u00b7tes", "\u2013", "sie\u00b7he", ",", "noch", "rollt", "im", "Thal"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "$(", "VVIMP", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Donner, und schon z\u00fccket der neue Blitz \u2013", "tokens": ["Der", "Don\u00b7ner", ",", "und", "schon", "z\u00fc\u00b7cket", "der", "neu\u00b7e", "Blitz", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So folgten Sieg auf Sieg! Die Deutschen", "tokens": ["So", "folg\u00b7ten", "Sieg", "auf", "Sieg", "!", "Die", "Deut\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlten sich Helden in Kraft des H\u00f6chsten!", "tokens": ["F\u00fchl\u00b7ten", "sich", "Hel\u00b7den", "in", "Kraft", "des", "H\u00f6chs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Manch zartes Weiblein hatte mit Heldenschwert", "tokens": ["Manch", "zar\u00b7tes", "Weib\u00b7lein", "hat\u00b7te", "mit", "Hel\u00b7den\u00b7schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Mann geg\u00fcrtet, M\u00fctter die S\u00f6hne: \u00bbGeht", "tokens": ["Den", "Mann", "ge\u00b7g\u00fcr\u00b7tet", ",", "M\u00fct\u00b7ter", "die", "S\u00f6h\u00b7ne", ":", "\u00bb", "Geht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "VVPP", "$,", "NN", "ART", "NN", "$.", "$(", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Gott! Es gilt die Sache Gottes!", "tokens": ["Mit", "Gott", "!", "Es", "gilt", "die", "Sa\u00b7che", "Got\u00b7tes", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vaterland gilt es, Altar und Freiheit!\u00ab", "tokens": ["Va\u00b7ter\u00b7land", "gilt", "es", ",", "Al\u00b7tar", "und", "Frei\u00b7heit", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Ihm sei die Ehre, Dank Ihm und Lobgesang", "tokens": ["Ihm", "sei", "die", "Eh\u00b7re", ",", "Dank", "Ihm", "und", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PPER", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und frommen Lebens besserer Hymnus Ihm,", "tokens": ["Und", "from\u00b7men", "Le\u00b7bens", "bes\u00b7se\u00b7rer", "Hym\u00b7nus", "Ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Allbarmherzigen! Er st\u00fcrzet", "tokens": ["Dem", "All\u00b7barm\u00b7her\u00b7zi\u00b7gen", "!", "Er", "st\u00fcr\u00b7zet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Stolze vom Thron, und erh\u00f6ht die Demut.", "tokens": ["Stol\u00b7ze", "vom", "Thron", ",", "und", "er\u00b7h\u00f6ht", "die", "De\u00b7mut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPRART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.12": {"line.1": {"text": "Er f\u00e4llt! ihn st\u00fcrzet Gott der Allm\u00e4chtige,", "tokens": ["Er", "f\u00e4llt", "!", "ihn", "st\u00fcr\u00b7zet", "Gott", "der", "All\u00b7m\u00e4ch\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der auf der Wage, welche Tyrannen w\u00e4gt", "tokens": ["Der", "auf", "der", "Wa\u00b7ge", ",", "wel\u00b7che", "Ty\u00b7ran\u00b7nen", "w\u00e4gt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Landesv\u00e4ter, mit umw\u00f6lkter", "tokens": ["Und", "Lan\u00b7des\u00b7v\u00e4\u00b7ter", ",", "mit", "um\u00b7w\u00f6lk\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measu