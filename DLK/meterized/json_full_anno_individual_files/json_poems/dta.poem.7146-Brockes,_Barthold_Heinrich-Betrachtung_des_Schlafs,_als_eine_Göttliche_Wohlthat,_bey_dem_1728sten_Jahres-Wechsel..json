{"dta.poem.7146": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nur dergleichen Traum vielleicht nach dieser Zeit,", "tokens": ["Wenn", "nur", "derg\u00b7lei\u00b7chen", "Traum", "viel\u00b7leicht", "nach", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "NN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verruchter S\u00fcnder Seelen schreckte,", "tokens": ["Ver\u00b7ruch\u00b7ter", "S\u00fcn\u00b7der", "See\u00b7len", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis an den j\u00fcngsten Tag,", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vermeint ihr, da\u00df darinn nicht eine Straffe steckte?", "tokens": ["Ver\u00b7meint", "ihr", ",", "da\u00df", "da\u00b7rinn", "nicht", "ei\u00b7ne", "Straf\u00b7fe", "steck\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PAV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie d\u00fcrfft\u2019 es um dich stehen,", "tokens": ["Wie", "d\u00fcrfft'", "es", "um", "dich", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann nicht, wie hier, durch unsre Sinnlichkeit,", "tokens": ["Wann", "nicht", ",", "wie", "hier", ",", "durch", "uns\u00b7re", "Sinn\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PWAV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Bilder, die uns nicht gefallen, nicht vergehen?", "tokens": ["Die", "Bil\u00b7der", ",", "die", "uns", "nicht", "ge\u00b7fal\u00b7len", ",", "nicht", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann nicht, wie hier auf dieser Welt,", "tokens": ["Wann", "nicht", ",", "wie", "hier", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PWAV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da, was uns etwan nicht gef\u00e4llt,", "tokens": ["Da", ",", "was", "uns", "et\u00b7wan", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich durch die Sinnlichkeit l\u00e4sst aus dem Sinne schlagen;", "tokens": ["Sich", "durch", "die", "Sinn\u00b7lich\u00b7keit", "l\u00e4sst", "aus", "dem", "Sin\u00b7ne", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und die erschrecklichsten Gedancken sich verjagen:", "tokens": ["Und", "die", "er\u00b7schreck\u00b7lichs\u00b7ten", "Ge\u00b7dan\u00b7cken", "sich", "ver\u00b7ja\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nein, unaufh\u00f6rlich dir von deinem Leben", "tokens": ["Nein", ",", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "dir", "von", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das scheu\u00dflichste f\u00fcr Augen d\u00fcrffte schweben?", "tokens": ["Das", "scheu\u00df\u00b7lichs\u00b7te", "f\u00fcr", "Au\u00b7gen", "d\u00fcrff\u00b7te", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Es scheinet selbst zur Straff ein solches Qu\u00e4len,", "tokens": ["Es", "schei\u00b7net", "selbst", "zur", "Straff", "ein", "sol\u00b7ches", "Qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Recht eigentlich dem Wesen unsrer Seelen,", "tokens": ["Recht", "ei\u00b7gent\u00b7lich", "dem", "We\u00b7sen", "uns\u00b7rer", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und ihrer Art gem\u00e4s,", "tokens": ["Und", "ih\u00b7rer", "Art", "ge\u00b7m\u00e4s", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "So lang sie sonder Leib, im Dencken auch zu leiden;", "tokens": ["So", "lang", "sie", "son\u00b7der", "Leib", ",", "im", "Den\u00b7cken", "auch", "zu", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJA", "NN", "$,", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und dies ist schrecklich gnug.", "tokens": ["Und", "dies", "ist", "schreck\u00b7lich", "gnug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}