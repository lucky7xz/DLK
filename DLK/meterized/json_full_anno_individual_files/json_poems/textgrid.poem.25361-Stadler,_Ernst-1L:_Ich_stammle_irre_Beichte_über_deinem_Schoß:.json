{"textgrid.poem.25361": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stammle irre Beichte \u00fcber deinem Scho\u00df:", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stammle irre Beichte \u00fcber deinem Scho\u00df:", "tokens": ["Ich", "stamm\u00b7le", "ir\u00b7re", "Beich\u00b7te", "\u00fc\u00b7ber", "dei\u00b7nem", "Scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Madonna, mach' mich meiner Qualen los.", "tokens": ["Ma\u00b7don\u00b7na", ",", "mach'", "mich", "mei\u00b7ner", "Qua\u00b7len", "los", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du, deren Weh die Liebe nie verlie\u00df,", "tokens": ["Du", ",", "de\u00b7ren", "Weh", "die", "Lie\u00b7be", "nie", "ver\u00b7lie\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In deren Leib man sieben Schwerter stie\u00df,", "tokens": ["In", "de\u00b7ren", "Leib", "man", "sie\u00b7ben", "Schwer\u00b7ter", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die l\u00e4chelnd man zur Marterbank gezerrt \u2013", "tokens": ["Die", "l\u00e4\u00b7chelnd", "man", "zur", "Mar\u00b7ter\u00b7bank", "ge\u00b7zerrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIS", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O sieh, noch bin ich ganz nicht aufgesperrt,", "tokens": ["O", "sieh", ",", "noch", "bin", "ich", "ganz", "nicht", "auf\u00b7ge\u00b7sperrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch f\u00fchl' ich, wie mir Ha\u00df zur Kehle steigt,", "tokens": ["Noch", "f\u00fchl'", "ich", ",", "wie", "mir", "Ha\u00df", "zur", "Keh\u00b7le", "steigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und vielem bin ich fern und ungeneigt.", "tokens": ["Und", "vie\u00b7lem", "bin", "ich", "fern", "und", "un\u00b7ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O la\u00df die H\u00e4rte, die mich engt, zergehn,", "tokens": ["O", "la\u00df", "die", "H\u00e4r\u00b7te", ",", "die", "mich", "engt", ",", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nur Tor mich sein, durch das die Bilder gehn,", "tokens": ["Nur", "Tor", "mich", "sein", ",", "durch", "das", "die", "Bil\u