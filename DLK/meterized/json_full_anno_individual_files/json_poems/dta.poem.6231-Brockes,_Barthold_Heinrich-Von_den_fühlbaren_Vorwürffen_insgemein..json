{"dta.poem.6231": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie fast gantz ausgel\u00f6scht. Es gl\u00fchet, brennt und strahlet", "tokens": ["Sie", "fast", "gantz", "aus\u00b7ge\u00b7l\u00f6scht", ".", "Es", "gl\u00fc\u00b7het", ",", "brennt", "und", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$.", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unterirdsche Gluht, und macht die Felsen springen,", "tokens": ["Die", "un\u00b7ter\u00b7ird\u00b7sche", "Gluht", ",", "und", "macht", "die", "Fel\u00b7sen", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Erde \u00f6ffnet sich, um Felsen zu verschlingen.", "tokens": ["Die", "Er\u00b7de", "\u00f6ff\u00b7net", "sich", ",", "um", "Fel\u00b7sen", "zu", "ver\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch \u00f6ffnet sie sich auch, und l\u00e4sset uns zu gut,", "tokens": ["Doch", "\u00f6ff\u00b7net", "sie", "sich", "auch", ",", "und", "l\u00e4s\u00b7set", "uns", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch zarte L\u00f6cherchen, von ihrer innerm Gluht", "tokens": ["Durch", "zar\u00b7te", "L\u00f6\u00b7cher\u00b7chen", ",", "von", "ih\u00b7rer", "in\u00b7nerm", "Gluht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Lebens-Kr\u00e4ffte dringen,", "tokens": ["Die", "Le\u00b7bens\u00b7Kr\u00e4ff\u00b7te", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So \u00fcberall den Geist des Wachsthums bringen.", "tokens": ["So", "\u00fc\u00b7be\u00b7rall", "den", "Geist", "des", "Wach\u00b7sthums", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Rings um sie her kan man die Lufft-Gesicht\u2019", "tokens": ["Rings", "um", "sie", "her", "kan", "man", "die", "Lufft\u00b7Ge\u00b7sicht'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "APZR", "VMFIN", "PIS", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Entstehen sehen", "tokens": ["Ent\u00b7ste\u00b7hen", "se\u00b7hen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und vergehen.", "tokens": ["Und", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Schnee, Regen, Nebel, Dufft, der Jris buntes Licht,", "tokens": ["Schnee", ",", "Re\u00b7gen", ",", "Ne\u00b7bel", ",", "Dufft", ",", "der", "Jris", "bun\u00b7tes", "Licht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-++-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Blitz, Donner, Hagel-Schaur, der reiffen Felder Pest.", "tokens": ["Blitz", ",", "Don\u00b7ner", ",", "Ha\u00b7gel\u00b7Schaur", ",", "der", "reif\u00b7fen", "Fel\u00b7der", "Pest", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was die Natur uns nun von Aendrung sehen l\u00e4sst,", "tokens": ["Was", "die", "Na\u00b7tur", "uns", "nun", "von", "A\u00b7en\u00b7drung", "se\u00b7hen", "l\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "So vielerley ver\u00e4ndertes Bewegen,", "tokens": ["So", "vie\u00b7ler\u00b7ley", "ver\u00b7\u00e4n\u00b7der\u00b7tes", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Sind die Ver\u00e4ndrungen uns f\u00e4hig zu erregen.", "tokens": ["Sind", "die", "Ver\u00b7\u00e4n\u00b7drun\u00b7gen", "uns", "f\u00e4\u00b7hig", "zu", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}