{"dta.poem.20619": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Vorberichtender  \n Lebenslauf  \n der Dichterin  \n Anna Louise Karschin,  \n geb. D\u00fcrbach .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Freunde von den Wissenschaften!", "tokens": ["Ihr", "Freun\u00b7de", "von", "den", "Wis\u00b7sen\u00b7schaf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr kamet, mich zu sehn, von der Ihr viel geh\u00f6rt.", "tokens": ["Ihr", "ka\u00b7met", ",", "mich", "zu", "sehn", ",", "von", "der", "Ihr", "viel", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr saht die D\u00fcrftigkeit. \u2014 Ich wurde nie belehrt,", "tokens": ["Ihr", "saht", "die", "D\u00fcrf\u00b7tig\u00b7keit", ".", "Ich", "wur\u00b7de", "nie", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und keine Regel bleibt mir im Ged\u00e4chtni\u00df haften,", "tokens": ["Und", "kei\u00b7ne", "Re\u00b7gel", "bleibt", "mir", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "haf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bin nur von Natur, der zweiten Sch\u00f6pferin,", "tokens": ["Ich", "bin", "nur", "von", "Na\u00b7tur", ",", "der", "zwei\u00b7ten", "Sch\u00f6p\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von ihr allein nur bin ich was ich bin.", "tokens": ["Von", "ihr", "al\u00b7lein", "nur", "bin", "ich", "was", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "VAFIN", "PPER", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vier Kinder st\u00f6hren mich; doch das Ger\u00e4usch von Kindern,", "tokens": ["Vier", "Kin\u00b7der", "st\u00f6h\u00b7ren", "mich", ";", "doch", "das", "Ge\u00b7r\u00e4usch", "von", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$.", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kann nicht den Trieb in mir und nicht das Feuer mindern.", "tokens": ["Kann", "nicht", "den", "Trieb", "in", "mir", "und", "nicht", "das", "Feu\u00b7er", "min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "APPR", "PPER", "KON", "PTKNEG", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Gl\u00fcck ist klein, doch gro\u00df genug f\u00fcr mich,", "tokens": ["Mein", "Gl\u00fcck", "ist", "klein", ",", "doch", "gro\u00df", "ge\u00b7nug", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und im Gesang ist mir der Gram nicht hinderlich.", "tokens": ["Und", "im", "Ge\u00b7sang", "ist", "mir", "der", "Gram", "nicht", "hin\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr Freunde, die Ihr Euch die gro\u00dfe M\u00fche nahmet,", "tokens": ["Ihr", "Freun\u00b7de", ",", "die", "Ihr", "Euch", "die", "gro\u00b7\u00dfe", "M\u00fc\u00b7he", "nah\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und mich so niedres Weib zu sehn nach Glogau kamet,", "tokens": ["Und", "mich", "so", "nie\u00b7dres", "Weib", "zu", "sehn", "nach", "Glo\u00b7gau", "ka\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Euch geb\u2019 ein solches Gl\u00fcck freundschaftlich das Geleit,", "tokens": ["Euch", "geb'", "ein", "sol\u00b7ches", "Gl\u00fcck", "freund\u00b7schaft\u00b7lich", "das", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Als Euer Herz verdient und Eure Redlichkeit,", "tokens": ["Als", "Eu\u00b7er", "Herz", "ver\u00b7dient", "und", "Eu\u00b7re", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die ich aus Euren Augen kenne,", "tokens": ["Die", "ich", "aus", "Eu\u00b7ren", "Au\u00b7gen", "ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und die ich mich bereit zu Euren Diensten nenne", "tokens": ["Und", "die", "ich", "mich", "be\u00b7reit", "zu", "Eu\u00b7ren", "Diens\u00b7ten", "nen\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELS", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}