{"textgrid.poem.39488": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bewundert nur die feingeschnitzten G\u00f6tzen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bewundert nur die feingeschnitzten G\u00f6tzen,", "tokens": ["Be\u00b7wun\u00b7dert", "nur", "die", "fein\u00b7ge\u00b7schnitz\u00b7ten", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und la\u00dft als Meister, F\u00fchrer, Freund uns Goethen:", "tokens": ["Und", "la\u00dft", "als", "Meis\u00b7ter", ",", "F\u00fch\u00b7rer", ",", "Freund", "uns", "Goe\u00b7then", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "KOUS", "NN", "$,", "NN", "$,", "NN", "PPER", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Euch wird nach seines Geistes Morgenr\u00f6then", "tokens": ["Euch", "wird", "nach", "sei\u00b7nes", "Geis\u00b7tes", "Mor\u00b7gen\u00b7r\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Apollos goldner Tag nicht mit erg\u00f6tzen.", "tokens": ["A\u00b7pol\u00b7los", "gold\u00b7ner", "Tag", "nicht", "mit", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKNEG", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der lockt kein frisches Gr\u00fcn aus d\u00fcrren Kl\u00f6tzen,", "tokens": ["Der", "lockt", "kein", "fri\u00b7sc