{"dta.poem.5951": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die verschiedlichen Ver\u00e4nderungen nun", "tokens": ["Durch", "die", "ver\u00b7schied\u00b7li\u00b7chen", "Ver\u00b7\u00e4n\u00b7de\u00b7run\u00b7gen", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Der zarten Lufft und Himmels-Lufft, die sich", "tokens": ["Der", "zar\u00b7ten", "Lufft", "und", "Him\u00b7mels\u00b7Lufft", ",", "die", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um manchen Mittel-Punct rings-um zusammen thun,", "tokens": ["Um", "man\u00b7chen", "Mit\u00b7tel\u00b7Punct", "rings\u00b7um", "zu\u00b7sam\u00b7men", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind Wirbel zugericht, die auch gemeinschafftlich,", "tokens": ["Sind", "Wir\u00b7bel", "zu\u00b7ge\u00b7richt", ",", "die", "auch", "ge\u00b7mein\u00b7schafft\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie sie, beweget sind; da jeder dann im Creyse", "tokens": ["Wie", "sie", ",", "be\u00b7we\u00b7get", "sind", ";", "da", "je\u00b7der", "dann", "im", "Crey\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "VAFIN", "$.", "KOUS", "PIS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Unglaublich schnell sich dreht, auf gantz verschiedne Weise.", "tokens": ["Un\u00b7glaub\u00b7lich", "schnell", "sich", "dreht", ",", "auf", "gantz", "ver\u00b7schied\u00b7ne", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PRF", "VVFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}