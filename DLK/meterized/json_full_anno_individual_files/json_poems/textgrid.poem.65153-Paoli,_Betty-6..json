"Es haben ihr die Sassenagh", "tokens": ["Es", "ha\u00b7ben", "ihr", "die", "Sas\u00b7se\u00b7nagh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Stirling ihren Sohn erschossen. \u2013 \u2013", "tokens": ["Zu", "Stir\u00b7ling", "ih\u00b7ren", "Sohn", "er\u00b7schos\u00b7sen", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer durch die stille Haide geht", "tokens": ["Wer", "durch", "die", "stil\u00b7le", "Hai\u00b7de", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sieht sie kauern auf der Erde,", "tokens": ["Und", "sieht", "sie", "kau\u00b7ern", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der murmelt wohl ein fromm Gebet,", "tokens": ["Der", "mur\u00b7melt", "wohl", "ein", "fromm", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ihrem Herzen Friede werde.", "tokens": ["Da\u00df", "ih\u00b7rem", "Her\u00b7zen", "Frie\u00b7de", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch dunkel loht's aus ihrem Blick:", "tokens": ["Doch", "dun\u00b7kel", "loht's", "aus", "ih\u00b7rem", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Leben nicht und nicht im Sterben!", "tokens": ["Im", "Le\u00b7ben", "nicht", "und", "nicht", "im", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "KON", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er war mein Stolz, er war mein Gl\u00fcck,", "tokens": ["Er", "war", "mein", "Stolz", ",", "er", "war", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ich \u2013 ich stie\u00df ihn in's Verderben!", "tokens": ["Und", "ich", "\u2013", "ich", "stie\u00df", "ihn", "in's", "Ver\u00b7der\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}