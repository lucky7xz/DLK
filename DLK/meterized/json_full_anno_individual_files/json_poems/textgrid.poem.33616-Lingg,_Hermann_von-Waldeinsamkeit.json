{"textgrid.poem.33616": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Waldeinsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6tlich schimmern durchs Tannengr\u00fcn", "tokens": ["R\u00f6t\u00b7lich", "schim\u00b7mern", "durchs", "Tan\u00b7nen\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ragende St\u00e4mme der F\u00f6hren;", "tokens": ["Ra\u00b7gen\u00b7de", "St\u00e4m\u00b7me", "der", "F\u00f6h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Fern die Berge, die duftigen, gl\u00fchn,", "tokens": ["Fern", "die", "Ber\u00b7ge", ",", "die", "duf\u00b7ti\u00b7gen", ",", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In den L\u00fcften l\u00e4\u00dft sich h\u00f6ren", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "l\u00e4\u00dft", "sich", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eines Falken heller Schrei,", "tokens": ["Ei\u00b7nes", "Fal\u00b7ken", "hel\u00b7ler", "Schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und summende Bienen schw\u00e4rmen vorbei.", "tokens": ["Und", "sum\u00b7men\u00b7de", "Bie\u00b7nen", "schw\u00e4r\u00b7men", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Horch, was donnert und st\u00f6rt den Traum", "tokens": ["Horch", ",", "was", "don\u00b7nert", "und", "st\u00f6rt", "den", "Traum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In der Friedensstille mitten?", "tokens": ["In", "der", "Frie\u00b7dens\u00b7stil\u00b7le", "mit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es fiel des Waldes h\u00f6chster Baum,", "tokens": ["Es", "fiel", "des", "Wal\u00b7des", "h\u00f6chs\u00b7ter", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Axt hat ihn durchschnitten \u2013", "tokens": ["Die", "Axt", "hat", "ihn", "durch\u00b7schnit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dr\u00fcben aber am Bergeshang", "tokens": ["Dr\u00fc\u00b7ben", "a\u00b7ber", "am", "Ber\u00b7ges\u00b7hang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Da schallt des Hirten froher Gesang.", "tokens": ["Da", "schallt", "des", "Hir\u00b7ten", "fro\u00b7her", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": 