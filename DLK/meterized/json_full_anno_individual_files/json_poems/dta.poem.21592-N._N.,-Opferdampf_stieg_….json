{"dta.poem.21592": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Opferdampf stieg \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Opferdampf stieg von der befleckten Erde               ", "tokens": ["Op\u00b7fer\u00b7dampf", "stieg", "von", "der", "be\u00b7fleck\u00b7ten", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wahrlich niemals herrlicher auf zum Himmel,", "tokens": ["Wahr\u00b7lich", "nie\u00b7mals", "herr\u00b7li\u00b7cher", "auf", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Denn, da dein Wort M\u00e4rtyrerblut besiegelt,", "tokens": ["Denn", ",", "da", "dein", "Wort", "M\u00e4r\u00b7ty\u00b7rer\u00b7blut", "be\u00b7sie\u00b7gelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Heilige Wahrheit,", "tokens": ["Hei\u00b7li\u00b7ge", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Da der Pfaff sich \u2014 Pfaffe zugleich und Henker \u2014", "tokens": ["Da", "der", "Pfaff", "sich", "Pfaf\u00b7fe", "zu\u00b7gleich", "und", "Hen\u00b7ker"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "PRF", "$(", "NN", "ADV", "KON", "NN", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "An der Gluthqual denkender Menschen letzte,", "tokens": ["An", "der", "Gluth\u00b7qual", "den\u00b7ken\u00b7der", "Men\u00b7schen", "letz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Da im Rauch sein Blick und des Ketzers Blick wie", "tokens": ["Da", "im", "Rauch", "sein", "Blick", "und", "des", "Ket\u00b7zers", "Blick", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "KON", "ART", "NN", "NN", "KOKOM"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dolche sich kreuzten. \u2014", "tokens": ["Dol\u00b7che", "sich", "kreuz\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "VVFIN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Jene Gluth, entfacht in dem Schoo\u00df des Dunkels,", "tokens": ["Je\u00b7ne", "Gluth", ",", "ent\u00b7facht", "in", "dem", "Schoo\u00df", "des", "Dun\u00b7kels", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ueberraschend siegreich den Bezirk der Scheite,", "tokens": ["Ue\u00b7ber\u00b7ra\u00b7schend", "sieg\u00b7reich", "den", "Be\u00b7zirk", "der", "Schei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ward des Dunkels Feind \u2014 und der Strom der Zeiten", "tokens": ["Ward", "des", "Dun\u00b7kels", "Feind", "und", "der", "Strom", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$(", "KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wird sie nie l\u00f6schen.", "tokens": ["Wird", "sie", "nie", "l\u00f6\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nein! Sie gl\u00fcht! Und w\u00e4r\u2019s in den fernsten Tagen, \u2014", "tokens": ["Nein", "!", "Sie", "gl\u00fcht", "!", "Und", "w\u00e4r's", "in", "den", "ferns\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "$.", "KON", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Asche wird die finstere Tempelhalle,", "tokens": ["A\u00b7sche", "wird", "die", "fins\u00b7te\u00b7re", "Tem\u00b7pel\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Drin geknechtet seufzet der Geist der Menschheit,", "tokens": ["Drin", "ge\u00b7knech\u00b7tet", "seuf\u00b7zet", "der", "Geist", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Hegend und reifend", "tokens": ["He\u00b7gend", "und", "rei\u00b7fend"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Eine Saat, die sp\u00e4tere Enkel ernten,", "tokens": ["Ei\u00b7ne", "Saat", ",", "die", "sp\u00e4\u00b7te\u00b7re", "En\u00b7kel", "ern\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Fa\u00dft sie Herzen, die sie entflammt zu reden \u2014", "tokens": ["Fa\u00dft", "sie", "Her\u00b7zen", ",", "die", "sie", "ent\u00b7flammt", "zu", "re\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "VVPP", "PTKZU", "VVINF", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ihre Macht verk\u00fcndigend hat sie meine", "tokens": ["Ih\u00b7re", "Macht", "ver\u00b7k\u00fcn\u00b7di\u00b7gend", "hat", "sie", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "PPER", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Lieder befeuert.", "tokens": ["Lie\u00b7der", "be\u00b7feu\u00b7ert", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}