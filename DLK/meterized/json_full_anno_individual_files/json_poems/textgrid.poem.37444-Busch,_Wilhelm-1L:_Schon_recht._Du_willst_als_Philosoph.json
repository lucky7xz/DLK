{"textgrid.poem.37444": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon recht. Du willst als Philosoph", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon recht. Du willst als Philosoph", "tokens": ["Schon", "recht", ".", "Du", "willst", "als", "Phi\u00b7lo\u00b7soph"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "PPER", "VMFIN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wahrheit dir gewinnen;", "tokens": ["Die", "Wahr\u00b7heit", "dir", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du machst mit Worten ihr den Hof,", "tokens": ["Du", "machst", "mit", "Wor\u00b7ten", "ihr", "den", "Hof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um so sie einzuspinnen.", "tokens": ["Um", "so", "sie", "ein\u00b7zu\u00b7spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur sage nicht, da\u00df zwischen dir", "tokens": ["Nur", "sa\u00b7ge", "nicht", ",", "da\u00df", "zwi\u00b7schen", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihr schon alles richtig.", "tokens": ["Und", "ihr", "schon", "al\u00b7les", "rich\u00b7tig", "."], "token_info": ["word", "wo