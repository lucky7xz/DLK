{"dta.poem.13993": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Zaubernetz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Zelter sollst Du prangen,", "tokens": ["Auf", "dem", "Zel\u00b7ter", "sollst", "Du", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die sch\u00f6nen Glieder schlanke", "tokens": ["Um", "die", "sch\u00f6\u00b7nen", "Glie\u00b7der", "schlan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seide, himmelblau, gespannet,", "tokens": ["Sei\u00b7de", ",", "him\u00b7mel\u00b7blau", ",", "ge\u00b7span\u00b7net", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein s\u00fc\u00dfgeschm\u00fcckter Knabe.", "tokens": ["Als", "ein", "s\u00fc\u00df\u00b7ge\u00b7schm\u00fcck\u00b7ter", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}