{"dta.poem.9972": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich vergiesse niemahls z\u00e4hren/ und mach andre wohl betr\u00fcbt/", "tokens": ["Ich", "ver\u00b7gies\u00b7se", "nie\u00b7mahls", "z\u00e4h\u00b7ren", "/", "und", "mach", "and\u00b7re", "wohl", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "$(", "KON", "VVFIN", "PIS", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Niemand wird von mir geschlagen/ dennoch mir man schl\u00e4ge", "tokens": ["Nie\u00b7mand", "wird", "von", "mir", "ge\u00b7schla\u00b7gen", "/", "den\u00b7noch", "mir", "man", "schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "VVPP", "$(", "ADV", "PPER", "PIS", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "glebt:", "tokens": ["glebt", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Jtzund hei\u00dft mich einer gut/ b\u00f6se mich der andre nennet/", "tokens": ["Jt\u00b7zund", "hei\u00dft", "mich", "ei\u00b7ner", "gut", "/", "b\u00f6\u00b7se", "mich", "der", "and\u00b7re", "nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJD", "$(", "VVFIN", "PPER", "ART", "PIS", "VVFIN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Wenn ich lang genung gedient/ werd ich endlich gar verbren\u0303et.", "tokens": ["Wenn", "ich", "lang", "ge\u00b7nung", "ge\u00b7dient", "/", "werd", "ich", "end\u00b7lich", "gar", "ver\u00b7bre\u00f1et", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VVPP", "$(", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}}}}