{"textgrid.poem.24029": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Traum-Exegese", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umte, da\u00df ich Adam w\u00e4r,", "tokens": ["Mir", "tr\u00e4um\u00b7te", ",", "da\u00df", "ich", "A\u00b7dam", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Adam im Paradiese,", "tokens": ["A\u00b7dam", "im", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich gr\u00fcbelte so f\u00fcr mich her", "tokens": ["Ich", "gr\u00fc\u00b7bel\u00b7te", "so", "f\u00fcr", "mich", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einer bunten Wiese.", "tokens": ["Auf", "ei\u00b7ner", "bun\u00b7ten", "Wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da unter einem Apfelbaum,", "tokens": ["Da", "un\u00b7ter", "ei\u00b7nem", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah meine Frau ich liegen;", "tokens": ["Sah", "mei\u00b7ne", "Frau", "ich", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schlief, und \u00fcber ihrem Traum", "tokens": ["Sie", "schlief", ",", "und", "\u00fc\u00b7ber", "ih\u00b7rem", "Traum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Th\u00e4t sich die Schlange wiegen.", "tokens": ["Th\u00e4t", "sich", "die", "Schlan\u00b7ge", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die sprach, mit Schrecken h\u00f6rt ichs: I\u00df!", "tokens": ["Die", "sprach", ",", "mit", "Schre\u00b7cken", "h\u00f6rt", "ichs", ":", "I\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "NN", "VVFIN", "PIS", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist doch vom Apfelbaume!", "tokens": ["Ist", "doch", "vom", "Ap\u00b7fel\u00b7bau\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drauf: Wenn michs l\u00fcstet, ei gewi\u00df!", "tokens": ["Drauf", ":", "Wenn", "michs", "l\u00fcs\u00b7tet", ",", "ei", "ge\u00b7wi\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$.", "KOUS", "PIS", "VVFIN", "$,", "ITJ", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach meine Frau im Traume.", "tokens": ["Sprach", "mei\u00b7ne", "Frau", "im", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Schlange, lauernd: Aber Er,", "tokens": ["Die", "Schlan\u00b7ge", ",", "lau\u00b7ernd", ":", "A\u00b7ber", "Er", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$.", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat es euch verboten ...", "tokens": ["Er", "hat", "es", "euch", "ver\u00b7bo\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Frau: Verboten? Wer?", "tokens": ["Und", "mei\u00b7ne", "Frau", ":", "Ver\u00b7bo\u00b7ten", "?", "Wer", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$.", "NN", "$.", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mach, hol mir einen roten!", "tokens": ["Mach", ",", "hol", "mir", "ei\u00b7nen", "ro\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Schlange wand sich schnell hinauf,", "tokens": ["Die", "Schlan\u00b7ge", "wand", "sich", "schnell", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warf einen roten runter.", "tokens": ["Warf", "ei\u00b7nen", "ro\u00b7ten", "run\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da that mein Lieb die Augen auf,", "tokens": ["Da", "that", "mein", "Lieb", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bi\u00df in den Apfel munter.", "tokens": ["Bi\u00df", "in", "den", "Ap\u00b7fel", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Um Gott! rief ich, nun ist es aus,", "tokens": ["Um", "Gott", "!", "rief", "ich", ",", "nun", "ist", "es", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$.", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir werden ausgewiesen!", "tokens": ["Wir", "wer\u00b7den", "aus\u00b7ge\u00b7wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sah auch schon im schnellsten Braus", "tokens": ["Und", "sah", "auch", "schon", "im", "schnells\u00b7ten", "Braus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herab den Engel schie\u00dfen.", "tokens": ["Her\u00b7ab", "den", "En\u00b7gel", "schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er stand in gro\u00dfer Gloria", "tokens": ["Er", "stand", "in", "gro\u00b7\u00dfer", "Glo\u00b7ria"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ein K\u00fcrisserposten", "tokens": ["Wie", "ein", "K\u00fc\u00b7ris\u00b7ser\u00b7pos\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor meiner Frau; wie die ihn sah,", "tokens": ["Vor", "mei\u00b7ner", "Frau", ";", "wie", "die", "ihn", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PWAV", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lacht sie: Du, willst du kosten?", "tokens": ["Lacht", "sie", ":", "Du", ",", "willst", "du", "kos\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nein, sprach der Engel Gabriel,", "tokens": ["Nein", ",", "sprach", "der", "En\u00b7gel", "Ga\u00b7bri\u00b7el", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin nicht da zum Essen;", "tokens": ["Ich", "bin", "nicht", "da", "zum", "Es\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Oh liebe Frau, du leichte Seel!", "tokens": ["Oh", "lie\u00b7be", "Frau", ",", "du", "leich\u00b7te", "Seel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du denn ganz vergessen?..", "tokens": ["Hast", "du", "denn", "ganz", "ver\u00b7ges\u00b7sen", "?", ".."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und wieder lachend meine Frau:", "tokens": ["Und", "wie\u00b7der", "la\u00b7chend", "mei\u00b7ne", "Frau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein guter Gottesbote,", "tokens": ["Mein", "gu\u00b7ter", "Got\u00b7tes\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur keine Angst! Ich wei\u00df genau", "tokens": ["Nur", "kei\u00b7ne", "Angst", "!", "Ich", "wei\u00df", "ge\u00b7nau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die g\u00f6ttlichen Verbote.", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7chen", "Ver\u00b7bo\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbund dennoch?! Ach, was soll ich ihm", "tokens": ["\u00bb", "und", "den\u00b7noch", "?!", "Ach", ",", "was", "soll", "ich", "ihm"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADV", "$.", "ITJ", "$,", "PWS", "VMFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von soviel Trotze sagen,", "tokens": ["Von", "so\u00b7viel", "Trot\u00b7ze", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um den die tausend Seraphim", "tokens": ["Um", "den", "die", "tau\u00b7send", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die goldnen Fl\u00fcgel schlagen?!\u00ab", "tokens": ["Die", "gold\u00b7nen", "Fl\u00fc\u00b7gel", "schla\u00b7gen", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u203asag unserm strengeguten Herrn,", "tokens": ["\u203a", "sag", "un\u00b7serm", "stren\u00b7ge\u00b7gu\u00b7ten", "Herrn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich th\u00e4t ihn herzlich lieben", "tokens": ["Ich", "th\u00e4t", "ihn", "herz\u00b7lich", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4r im Paradiese gern,", "tokens": ["Und", "w\u00e4r", "im", "Pa\u00b7ra\u00b7die\u00b7se", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Herzen gern geblieben.", "tokens": ["Von", "Her\u00b7zen", "gern", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Doch d\u00fcrfte nichts verboten sein.", "tokens": ["Doch", "d\u00fcrf\u00b7te", "nichts", "ver\u00b7bo\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das w\u00e4r ein grausam Spielen,", "tokens": ["Das", "w\u00e4r", "ein", "grau\u00b7sam", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sollt ewig ich tagaus tagein", "tokens": ["Sollt", "e\u00b7wig", "ich", "ta\u00b7gaus", "ta\u00b7gein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPER", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Gottes Aepfeln schielen.", "tokens": ["Nach", "Got\u00b7tes", "A\u00b7e\u00b7pfeln", "schie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Hab lieber drum gegessen schnell", "tokens": ["Hab", "lie\u00b7ber", "drum", "ge\u00b7ges\u00b7sen", "schnell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PAV", "VVPP", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und warte des Gerichtes.\u2039", "tokens": ["Und", "war\u00b7te", "des", "Ge\u00b7rich\u00b7tes", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Abflog der Engel Gabriel", "tokens": ["Ab\u00b7flog", "der", "En\u00b7gel", "Ga\u00b7bri\u00b7el"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NE", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mi\u00dfmutigen Gesichtes.", "tokens": ["Mi\u00df\u00b7mu\u00b7ti\u00b7gen", "Ge\u00b7sich\u00b7tes", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Kaum, da\u00df ich: Aber Frau! gebarmt,", "tokens": ["Kaum", ",", "da\u00df", "ich", ":", "A\u00b7ber", "Frau", "!", "ge\u00b7barmt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "$.", "KON", "NN", "$.", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam er in sanftem Schweben", "tokens": ["Kam", "er", "in", "sanf\u00b7tem", "Schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zur\u00fcck, hat fl\u00fcgelnd sie umarmt,", "tokens": ["Zu\u00b7r\u00fcck", ",", "hat", "fl\u00fc\u00b7gelnd", "sie", "um\u00b7armt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "VAFIN", "VVPP", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr einen Ku\u00df gegeben.", "tokens": ["Ihr", "ei\u00b7nen", "Ku\u00df", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und hoch vom Himmel sang es hell,", "tokens": ["Und", "hoch", "vom", "Him\u00b7mel", "sang", "es", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar lieblich anzuh\u00f6ren,", "tokens": ["Gar", "lieb\u00b7lich", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu Fl\u00f6te, Geig und Violoncell", "tokens": ["Zu", "Fl\u00f6\u00b7te", ",", "Geig", "und", "Vi\u00b7o\u00b7lon\u00b7cell"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In heitren Engelsch\u00f6ren:", "tokens": ["In", "hei\u00b7tren", "En\u00b7gel\u00b7sch\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Weil du so gut das Paradies", "tokens": ["Weil", "du", "so", "gut", "das", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gottes Herz verstanden,", "tokens": ["Und", "Got\u00b7tes", "Herz", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Geschahs, da\u00df er dich k\u00fcssen lie\u00df", "tokens": ["Ge\u00b7schahs", ",", "da\u00df", "er", "dich", "k\u00fcs\u00b7sen", "lie\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch seinen Abgesandten.", "tokens": ["Durch", "sei\u00b7nen", "Ab\u00b7ge\u00b7sand\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ich aber ward mit manchem Hieb", "tokens": ["Ich", "a\u00b7ber", "ward", "mit", "man\u00b7chem", "Hieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getrieben vor die Pforte,", "tokens": ["Ge\u00b7trie\u00b7ben", "vor", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dar\u00fcber flammenz\u00fcgig schrieb", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "flam\u00b7men\u00b7z\u00fc\u00b7gig", "schrieb"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herr Gabriel die Worte:", "tokens": ["Herr", "Ga\u00b7bri\u00b7el", "die", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Wer immer m\u00f6cht und nimmer wagt,", "tokens": ["Wer", "im\u00b7mer", "m\u00f6cht", "und", "nim\u00b7mer", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnell herzhaft zu genie\u00dfen,", "tokens": ["Schnell", "herz\u00b7haft", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei feierlich davon gejagt", "tokens": ["Sei", "fei\u00b7er\u00b7lich", "da\u00b7von", "ge\u00b7jagt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "PAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Gottes Paradiesen.", "tokens": ["Aus", "Got\u00b7tes", "Pa\u00b7ra\u00b7die\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Mir tr\u00e4umte, da\u00df ich Adam w\u00e4r,", "tokens": ["Mir", "tr\u00e4um\u00b7te", ",", "da\u00df", "ich", "A\u00b7dam", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Adam im Paradiese,", "tokens": ["A\u00b7dam", "im", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich gr\u00fcbelte so f\u00fcr mich her", "tokens": ["Ich", "gr\u00fc\u00b7bel\u00b7te", "so", "f\u00fcr", "mich", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einer bunten Wiese.", "tokens": ["Auf", "ei\u00b7ner", "bun\u00b7ten", "Wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Da unter einem Apfelbaum,", "tokens": ["Da", "un\u00b7ter", "ei\u00b7nem", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah meine Frau ich liegen;", "tokens": ["Sah", "mei\u00b7ne", "Frau", "ich", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schlief, und \u00fcber ihrem Traum", "tokens": ["Sie", "schlief", ",", "und", "\u00fc\u00b7