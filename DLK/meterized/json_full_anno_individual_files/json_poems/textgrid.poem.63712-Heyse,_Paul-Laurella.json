{"textgrid.poem.63712": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Laurella", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist noch wild, du bist noch scheu,", "tokens": ["Du", "bist", "noch", "wild", ",", "du", "bist", "noch", "scheu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur von der Mutter gez\u00e4hmt,", "tokens": ["Nur", "von", "der", "Mut\u00b7ter", "ge\u00b7z\u00e4hmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Du wei\u00dft noch nicht, wie s\u00fc\u00df es sei,", "tokens": ["Du", "wei\u00dft", "noch", "nicht", ",", "wie", "s\u00fc\u00df", "es", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Menschen entz\u00fcckt und gr\u00e4mt.", "tokens": ["Was", "Men\u00b7schen", "ent\u00b7z\u00fcckt", "und", "gr\u00e4mt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Du l\u00e4ssest dein Haar in die Stirne wehn", "tokens": ["Du", "l\u00e4s\u00b7sest", "dein", "Haar", "in", "die", "Stir\u00b7ne", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF"