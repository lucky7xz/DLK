{"textgrid.poem.30201": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drey hundert sechtzig f\u00fcnffe sind Tage von dem Jahre;", "tokens": ["Drey", "hun\u00b7dert", "secht\u00b7zig", "f\u00fcnf\u00b7fe", "sind", "Ta\u00b7ge", "von", "dem", "Jah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "CARD", "CARD", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wann siebenmal de\u00df Tages der Frome f\u00e4llig ware,", "tokens": ["Wann", "sie\u00b7ben\u00b7mal", "de\u00df", "Ta\u00b7ges", "der", "Fro\u00b7me", "f\u00e4l\u00b7lig", "wa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+--+---+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was meint man, was f\u00fcr Summen der S\u00fcnden werde spinnen", "tokens": ["Was", "meint", "man", ",", "was", "f\u00fcr", "Sum\u00b7men", "der", "S\u00fcn\u00b7den", "wer\u00b7de", "spin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIS", "$,", "PRELS", "APPR", "NN", "ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der B\u00f6se, der stets frevelt mit Worten, Wercken, Sinnen!", "tokens": ["Der", "B\u00f6\u00b7se", ",", "der", "stets", "fre\u00b7velt", "mit", "Wor\u00b7ten", ",", "Wer\u00b7cken", ",", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Was meint man, was f\u00fcr Zahlen zu letzte dieser zehlet,", "tokens": ["Was", "meint", "man", ",", "was", "f\u00fcr", "Zah\u00b7len", "zu", "letz\u00b7te", "die\u00b7ser", "zeh\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "$,", "PRELS", "APPR", "NN", "APPR", "ADJA", "PDS", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der sechtzig, siebtzig Jahre fast augenblicklich fehlet!", "tokens": ["Der", "secht\u00b7zig", ",", "siebt\u00b7zig", "Jah\u00b7re", "fast", "au\u00b7gen\u00b7blick\u00b7lich", "feh\u00b7let", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "CARD", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Es bleibt dabey, ihr Menschen, da\u00df Gott an euch nichts finde,", "tokens": ["Es", "bleibt", "da\u00b7bey", ",", "ihr", "Men\u00b7schen", ",", "da\u00df", "Gott", "an", "euch", "nichts", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PPOSAT", "NN", "$,", "KOUS", "NN", "APPR", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Was er nicht selbsten gibet, als S\u00fcnde, S\u00fcnde, S\u00fcnde!", "tokens": ["Was", "er", "nicht", "selbs\u00b7ten", "gi\u00b7bet", ",", "als", "S\u00fcn\u00b7de", ",", "S\u00fcn\u00b7de", ",", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "wo