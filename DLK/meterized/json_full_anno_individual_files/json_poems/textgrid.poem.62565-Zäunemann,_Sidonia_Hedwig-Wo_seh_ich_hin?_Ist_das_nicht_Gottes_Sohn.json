"PPER", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "ADJD", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das schn\u00f6de Welt-Get\u00fcmmel", "tokens": ["Das", "schn\u00f6\u00b7de", "Welt\u00b7Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit meinen Augen sehn.", "tokens": ["Mit", "mei\u00b7nen", "Au\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm! f\u00fchre mich zum Himmel!", "tokens": ["Komm", "!", "f\u00fch\u00b7re", "mich", "zum", "Him\u00b7mel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach la\u00df es bald geschehn!", "tokens": ["Ach", "la\u00df", "es", "bald", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Hier ist gut seyn, drum sehn ich mich dahin;", "tokens": ["Hier", "ist", "gut", "seyn", ",", "drum", "sehn", "ich", "mich", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VAINF", "$,", "PAV", "VVFIN", "PPER", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn dieses ist, O Jesu! der Gewinn,", "tokens": ["Denn", "die\u00b7ses", "ist", ",", "O", "Je\u00b7su", "!", "der", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,", "NE", "NE", "$.", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den du mir aufbehalten;", "tokens": ["Den", "du", "mir", "auf\u00b7be\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier ist der Sonnen Pracht,", "tokens": ["Hier", "ist", "der", "Son\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die, wenn ich werd erkalten,", "tokens": ["Die", ",", "wenn", "ich", "werd", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir Jesus zugedacht.", "tokens": ["Mir", "Je\u00b7sus", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Allein, so lang ich hier noch wallen soll,", "tokens": ["Al\u00b7lein", ",", "so", "lang", "ich", "hier", "noch", "wal\u00b7len", "soll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So mache mich der Himmels-Freude voll;", "tokens": ["So", "ma\u00b7che", "mich", "der", "Him\u00b7mels\u00b7Freu\u00b7de", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich frage nichts nach Gallen,", "tokens": ["Ich", "fra\u00b7ge", "nichts", "nach", "Gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Noth und Ungemach:", "tokens": ["Nach", "Noth", "und", "Un\u00b7ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich folge dir in allen,", "tokens": ["Ich", "fol\u00b7ge", "dir", "in", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Heyland Jesu! nach.", "tokens": ["Mein", "Hey\u00b7land", "Je\u00b7su", "!", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$.", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Gieb mir verkl\u00e4rt der Unschuld weises Kleid.", "tokens": ["Gieb", "mir", "ver\u00b7kl\u00e4rt", "der", "Un\u00b7schuld", "wei\u00b7ses", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich zweifle nicht, du bist darzu bereit;", "tokens": ["Ich", "zweif\u00b7le", "nicht", ",", "du", "bist", "dar\u00b7zu", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du kanst mirs nicht versagen.", "tokens": ["Du", "kanst", "mirs", "nicht", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich h\u00fclle mich darein,", "tokens": ["Ich", "h\u00fcl\u00b7le", "mich", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und will es prangend tragen,", "tokens": ["Und", "will", "es", "pran\u00b7gend", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bi\u00df ich werd bey dir seyn.", "tokens": ["Bi\u00df", "ich", "werd", "bey", "dir", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ach nimm mein Herz zu deiner H\u00fctten ein,", "tokens": ["Ach", "nimm", "mein", "Herz", "zu", "dei\u00b7ner", "H\u00fct\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wilst ja gern in solchen H\u00e4usern seyn.", "tokens": ["Du", "wilst", "ja", "gern", "in", "sol\u00b7chen", "H\u00e4u\u00b7sern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nimm Jesu! meine Seele", "tokens": ["Nimm", "Je\u00b7su", "!", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Thabors H\u00fcgel an,", "tokens": ["Vor", "Tha\u00b7bors", "H\u00fc\u00b7gel", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich mich nicht mehr qu\u00e4le,", "tokens": ["Da\u00df", "ich", "mich", "nicht", "mehr", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ewig freuen kan.", "tokens": ["Und", "e\u00b7wig", "freu\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}