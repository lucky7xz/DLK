{"textgrid.poem.59886": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbendlich wird es mir zuwider,", "tokens": ["\u00bb", "end\u00b7lich", "wird", "es", "mir", "zu\u00b7wi\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses ew'ge Minnespiel!", "tokens": ["Die\u00b7ses", "ew'\u00b7ge", "Min\u00b7ne\u00b7spiel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer hallen deine Lieder", "tokens": ["Im\u00b7mer", "hal\u00b7len", "dei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur von Wein und Liebe wider,", "tokens": ["Nur", "von", "Wein", "und", "Lie\u00b7be", "wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was zuviel ist, ist zuviel!\u00ab", "tokens": ["Was", "zu\u00b7viel", "ist", ",", "ist", "zu\u00b7viel", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "PIS", "VAFIN", "$,", "VAFIN", "PIS", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u2013 Kannst du Besseres mir geben?", "tokens": ["\u2013", "Kannst", "du", "Bes\u00b7se\u00b7res", "mir", "ge\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeige mir den Weg, das Ziel;", "tokens": ["Zei\u00b7ge", "mir", "den", "Weg", ",", "das", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gut, wei\u00df ich, ist all mein Streben,", "tokens": ["Gut", ",", "wei\u00df", "ich", ",", "ist", "all", "mein", "Stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in diesem Jammerleben", "tokens": ["Und", "in", "die\u00b7sem", "Jam\u00b7mer\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist des Guten nie zuviel! \u2013", "tokens": ["Ist", "des", "Gu\u00b7ten", "nie", "zu\u00b7viel", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PIS", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbendlich wird es mir zuwider,", "tokens": ["\u00bb", "end\u00b7lich", "wird", "es", "mir", "zu\u00b7wi\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses ew'ge Minnespiel!", "tokens": ["Die\u00b7ses", "ew'\u00b7ge", "Min\u00b7ne\u00b7spiel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure