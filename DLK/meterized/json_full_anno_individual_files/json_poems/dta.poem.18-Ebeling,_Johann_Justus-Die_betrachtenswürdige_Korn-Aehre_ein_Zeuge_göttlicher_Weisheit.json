{"dta.poem.18": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n betrachtensw\u00fcrdige Korn-Aehre  \n ein Zeuge  \n  g\u00f6ttlicher Weisheit", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich der Aehren lieblich Wallen,", "tokens": ["Wenn", "ich", "der", "A\u00b7eh\u00b7ren", "lieb\u00b7lich", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auf den gereiften Feldern seh,", "tokens": ["Auf", "den", "ge\u00b7reif\u00b7ten", "Fel\u00b7dern", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie hier wanken, da sanft fallen", "tokens": ["Wie", "sie", "hier", "wan\u00b7ken", ",", "da", "sanft", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "$,", "KOUS", "ADJD", "VVINF"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wie sie sich lenken in die H\u00f6h;", "tokens": ["Wie", "sie", "sich", "len\u00b7ken", "in", "die", "H\u00f6h", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sehe ich auf den Gefilden,", "tokens": ["So", "se\u00b7he", "ich", "auf", "den", "Ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ein ausgebreitet Seegens-Meer,", "tokens": ["Ein", "aus\u00b7ge\u00b7brei\u00b7tet", "See\u00b7gens\u00b7Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und solche trokne Wellen bilden,", "tokens": ["Und", "sol\u00b7che", "trok\u00b7ne", "Wel\u00b7len", "bil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Darin sich spiegelt ", "tokens": ["Da\u00b7rin", "sich", "spie\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PRF", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Woraus die Wunderg\u00fcte quillet,", "tokens": ["Wo\u00b7raus", "die", "Wun\u00b7der\u00b7g\u00fc\u00b7te", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die unser Herz mit Lust erf\u00fcllet:", "tokens": ["Die", "un\u00b7ser", "Herz", "mit", "Lust", "er\u00b7f\u00fcl\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn auf den schlanken, regen R\u00f6hren,", "tokens": ["Wenn", "auf", "den", "schlan\u00b7ken", ",", "re\u00b7gen", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00e4upter die von Korn gedr\u00fckt,", "tokens": ["Die", "H\u00e4up\u00b7ter", "die", "von", "Korn", "ge\u00b7dr\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Wind bewegt, zu Boden kehren;", "tokens": ["Von", "Wind", "be\u00b7wegt", ",", "zu", "Bo\u00b7den", "keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So scheints als wenn sich jede b\u00fckt;", "tokens": ["So", "scheints", "als", "wenn", "sich", "je\u00b7de", "b\u00fckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "KOUS", "PRF", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als wenn sie bei dem Niedersinken,", "tokens": ["Als", "wenn", "sie", "bei", "dem", "Nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Menschen, die vor\u00fcber gehn,", "tokens": ["Die", "Men\u00b7schen", ",", "die", "vor\u00b7\u00fc\u00b7ber", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit ihren Zeigefinger winken,", "tokens": ["Mit", "ih\u00b7ren", "Zei\u00b7ge\u00b7fin\u00b7ger", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Erden-Mutter anzusehn,", "tokens": ["Die", "Er\u00b7den\u00b7Mut\u00b7ter", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Woraus sie mit dem Korn entspriessen,", "tokens": ["Wo\u00b7raus", "sie", "mit", "dem", "Korn", "ent\u00b7spries\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Menschen theils und Vieh geniessen.", "tokens": ["Das", "Men\u00b7schen", "theils", "und", "Vieh", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie zeigen uns den Schoos der Erden,", "tokens": ["Sie", "zei\u00b7gen", "uns", "den", "Schoos", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Daraus ihr Halm, das F\u00fclle-Horn,", "tokens": ["Da\u00b7raus", "ihr", "Halm", ",", "das", "F\u00fcl\u00b7le\u00b7Horn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sie zugleich gebohren werden,", "tokens": ["Und", "sie", "zu\u00b7gleich", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren n\u00e4hrnden Wunderkorn:", "tokens": ["Mit", "ih\u00b7ren", "n\u00e4hrn\u00b7den", "Wun\u00b7der\u00b7korn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit wir aber nicht ged\u00e4chten,", "tokens": ["Da\u00b7mit", "wir", "a\u00b7ber", "nicht", "ge\u00b7d\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wenn sie aus der Erd allein,", "tokens": ["Als", "wenn", "sie", "aus", "der", "Erd", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Seegensreiche Nahrung br\u00e4chten;", "tokens": ["Die", "See\u00b7gens\u00b7rei\u00b7che", "Nah\u00b7rung", "br\u00e4ch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So richten sie sich insgemein,", "tokens": ["So", "rich\u00b7ten", "sie", "sich", "ins\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit ihren wallenden Gewimmel,", "tokens": ["Mit", "ih\u00b7ren", "wal\u00b7len\u00b7den", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch wiederum gesteift zum Himmel.", "tokens": ["Auch", "wie\u00b7de\u00b7rum", "ge\u00b7steift", "zum", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie lehren mit gestrekten Spizzen,", "tokens": ["Sie", "leh\u00b7ren", "mit", "ge\u00b7strek\u00b7ten", "Spiz\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Himmel stamme ihr Gedein;", "tokens": ["Vom", "Him\u00b7mel", "stam\u00b7me", "ihr", "Ge\u00b7dein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der H\u00f6chsten Obhut ihr Besch\u00fczzen,", "tokens": ["Der", "H\u00f6chs\u00b7ten", "Ob\u00b7hut", "ihr", "Be\u00b7sch\u00fcz\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mu\u00df ihnen stat des Schirmdachs seyn.", "tokens": ["Mu\u00df", "ih\u00b7nen", "stat", "des", "Schirm\u00b7dachs", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Einflus von des Himmels Milde,", "tokens": ["Der", "Ein\u00b7flus", "von", "des", "Him\u00b7mels", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wolken Thau und Regen-Gu\u00df,", "tokens": ["Der", "Wol\u00b7ken", "Thau", "und", "Re\u00b7gen\u00b7Gu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bekr\u00f6ne sie auf dem Gefilde", "tokens": ["Be\u00b7kr\u00f6\u00b7ne", "sie", "auf", "dem", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Mit Seegensreichen Ueberflus;", "tokens": ["Mit", "See\u00b7gens\u00b7rei\u00b7chen", "Ue\u00b7ber\u00b7flus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und aus der Sonn, des Lichtes Bronnen,", "tokens": ["Und", "aus", "der", "Sonn", ",", "des", "Lich\u00b7tes", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sey ihre Treibekraft geronnen.", "tokens": ["Sey", "ih\u00b7re", "Trei\u00b7be\u00b7kraft", "ge\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie zeugen von dem h\u00f6chsten Wesen,", "tokens": ["Sie", "zeu\u00b7gen", "von", "dem", "h\u00f6chs\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sie und alle Ding gemacht,", "tokens": ["Der", "sie", "und", "al\u00b7le", "Ding", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und geben uns auch klar zu lesen,", "tokens": ["Und", "ge\u00b7ben", "uns", "auch", "klar", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df der sie nur herf\u00fcrgebracht,", "tokens": ["Da\u00df", "der", "sie", "nur", "her\u00b7f\u00fcr\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der weise sey in seinem Walten,", "tokens": ["Der", "wei\u00b7se", "sey", "in", "sei\u00b7nem", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der alles k\u00fcnstlich auferbaut,", "tokens": ["Der", "al\u00b7les", "k\u00fcnst\u00b7lich", "au\u00b7fer\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der kl\u00fcglich sey in den Anstalten,", "tokens": ["Der", "kl\u00fcg\u00b7lich", "sey", "in", "den", "An\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die seine Vorsicht \u00fcberschaut:", "tokens": ["Die", "sei\u00b7ne", "Vor\u00b7sicht", "\u00fc\u00b7bersc\u00b7haut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und aus dem ersten zubereiten,", "tokens": ["Und", "aus", "dem", "ers\u00b7ten", "zu\u00b7be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gewust den Entzwek herzuleiten.", "tokens": ["Ge\u00b7wust", "den", "Ent\u00b7zwek", "her\u00b7zu\u00b7lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie herrlich ist der Bau der Aehren,", "tokens": ["Wie", "herr\u00b7lich", "ist", "der", "Bau", "der", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie weislich ihre Einrichtung!", "tokens": ["Wie", "weis\u00b7lich", "ih\u00b7re", "Ein\u00b7rich\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Blik, der kan uns dieses lehren,", "tokens": ["Ein", "Blik", ",", "der", "kan", "uns", "die\u00b7ses", "leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur heiligen Verwunderung.", "tokens": ["Zur", "hei\u00b7li\u00b7gen", "Ver\u00b7wun\u00b7de\u00b7rung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Wie sch\u00f6n ist dieses Kunstgeh\u00e4use,", "tokens": ["Wie", "sch\u00f6n", "ist", "die\u00b7ses", "Kunst\u00b7ge\u00b7h\u00e4u\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus zarten H\u00fclsen-Stof formirt,", "tokens": ["Aus", "zar\u00b7ten",