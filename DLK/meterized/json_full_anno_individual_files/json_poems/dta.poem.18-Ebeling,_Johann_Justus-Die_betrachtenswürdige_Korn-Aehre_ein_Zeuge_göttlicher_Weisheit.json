", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wohin wir unser Augenmerk,", "tokens": ["Wo\u00b7hin", "wir", "un\u00b7ser", "Au\u00b7gen\u00b7merk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Jm Reiche der Natur hinwenden,", "tokens": ["Jm", "Rei\u00b7che", "der", "Na\u00b7tur", "hin\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sind Wunder da, von deinen H\u00e4nden.", "tokens": ["Sind", "Wun\u00b7der", "da", ",", "von", "dei\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "La\u00df mich an denen Kreaturen,", "tokens": ["La\u00df", "mich", "an", "de\u00b7nen", "Kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PRELS", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die uns der Felder Schanplaz zeigt,", "tokens": ["Die", "uns", "der", "Fel\u00b7der", "Schan\u00b7plaz", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bemerken deiner Weisheits Spuren,", "tokens": ["Be\u00b7mer\u00b7ken", "dei\u00b7ner", "Weis\u00b7heits", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+