{"textgrid.poem.49315": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "An Meister H\u00e4mmerling", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wunder-Wunder-Wunderding!", "tokens": ["Wun\u00b7der\u00b7Wun\u00b7der\u00b7Wun\u00b7der\u00b7ding", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Meister H\u00e4mmerling", "tokens": ["Un\u00b7ser", "Meis\u00b7ter", "H\u00e4m\u00b7mer\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt doch gar zu grobe Possen:", "tokens": ["Treibt", "doch", "gar", "zu", "gro\u00b7be", "Pos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Daphnis mu\u00df ihm sein geschossen", "tokens": ["Daph\u00b7nis", "mu\u00df", "ihm", "sein", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zweifelsfrei mit Liebespfeilen,", "tokens": ["Zwei\u00b7fels\u00b7frei", "mit", "Lie\u00b7be\u00b7spfei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil er durch der Liebe Scherz", "tokens": ["Weil", "er", "durch", "der", "Lie\u00b7be", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00f6nne sein getreues Herz", "tokens": ["K\u00f6n\u00b7ne", "sein", "ge\u00b7treu\u00b7es", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter so viel Nymphen theilen.", "tokens": ["Un\u00b7ter", "so", "viel", "Nym\u00b7phen", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja, die\u00df kan nicht anders sein!", "tokens": ["Ja", ",", "die\u00df", "kan", "nicht", "an\u00b7ders", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VMFIN", "PTKNEG", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Daphnis ist durch Liebespein", "tokens": ["Daph\u00b7nis", "ist", "durch", "Lie\u00b7be\u00b7spein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "An dem linken Ohr entz\u00fcndet;", "tokens": ["An", "dem", "lin\u00b7ken", "Ohr", "ent\u00b7z\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "H\u00e4mmerling hat das ergr\u00fcndet,", "tokens": ["H\u00e4m\u00b7mer\u00b7ling", "hat", "das", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "H\u00e4mmerling, das Haubt der Narren,", "tokens": ["H\u00e4m\u00b7mer\u00b7ling", ",", "das", "Haubt", "der", "Nar\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Der so gar verstehet nicht,", "tokens": ["Der", "so", "gar", "ver\u00b7ste\u00b7het", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Was nur hei\u00df' ein Kunstgedicht,", "tokens": ["Was", "nur", "hei\u00df'", "ein", "Kunst\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Wil doch immer mit drein schnarren.", "tokens": ["Wil", "doch", "im\u00b7mer", "mit", "drein", "schnar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "H\u00e4mmerling der redet wahr!", "tokens": ["H\u00e4m\u00b7mer\u00b7ling", "der", "re\u00b7det", "wahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Solten nicht ein zwanzig Par", "tokens": ["Sol\u00b7ten", "nicht", "ein", "zwan\u00b7zig", "Par"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "CARD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Der begabten Sch\u00e4ferinnen", "tokens": ["Der", "be\u00b7gab\u00b7ten", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ihren Daphnis lieb gewinnen,", "tokens": ["Ih\u00b7ren", "Daph\u00b7nis", "lieb", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Der sie niemals zwar gesehen?", "tokens": ["Der", "sie", "nie\u00b7mals", "zwar", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "G\u00f6nnet ihnen doch den Preis,", "tokens": ["G\u00f6n\u00b7net", "ih\u00b7nen", "doch", "den", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Weil er ihre Namen wei\u00df,", "tokens": ["Weil", "er", "ih\u00b7re", "Na\u00b7men", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Welch' in vielen B\u00fcchern stehen.", "tokens": ["Welch'", "in", "vie\u00b7len", "B\u00fc\u00b7chern", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Fillis komt aus Frankreich her,", "tokens": ["Fil\u00b7lis", "komt", "aus", "Fran\u00b7kreich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Perlemund weit \u00fcbers Meer,", "tokens": ["Per\u00b7le\u00b7mund", "weit", "\u00fc\u00b7bers", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Florabell' aus welschen Landen,", "tokens": ["Flo\u00b7ra\u00b7bell'", "aus", "wel\u00b7schen", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Galathe ist da gestanden,", "tokens": ["Ga\u00b7la\u00b7the", "ist", "da", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wo Diana pflag zu baden,", "tokens": ["Wo", "Di\u00b7a\u00b7na", "pflag", "zu", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Rosimind' ist spanisch gar,", "tokens": ["Ro\u00b7si\u00b7mind'", "ist", "spa\u00b7nisch", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Lilliet hat hundert Jahr", "tokens": ["Lil\u00b7liet", "hat", "hun\u00b7dert", "Jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "CARD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Und wol mehr auf sich geladen.", "tokens": ["Und", "wol", "mehr", "auf", "sich", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "W\u00e4r' es nicht ein feines St\u00fcck,", "tokens": ["W\u00e4r'", "es", "nicht", "ein", "fei\u00b7nes", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Sein Gewissen, Ehr' und Gl\u00fcck", "tokens": ["Sein", "Ge\u00b7wis\u00b7sen", ",", "Ehr'", "und", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "So gar liederlich verscherzen?", "tokens": ["So", "gar", "lie\u00b7der\u00b7lich", "ver\u00b7scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Nein, man nimt die\u00df mehr zu Herzen,", "tokens": ["Nein", ",", "man", "nimt", "die\u00df", "mehr", "zu", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "PDS", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Als die Venusnarren pflegen.", "tokens": ["Als", "die", "Ve\u00b7nus\u00b7nar\u00b7ren", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Namen sind es und nichts mehr.", "tokens": ["Na\u00b7men", "sind", "es", "und", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Daphnis suchet Kunst und Lehr'", "tokens": ["Daph\u00b7nis", "su\u00b7chet", "Kunst", "und", "Lehr'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Aus der Sprachen Grund zu legen.", "tokens": ["Aus", "der", "Spra\u00b7chen", "Grund", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Ronsard und der Theophil", "tokens": ["Ron\u00b7sard", "und", "der", "Theo\u00b7phil"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "F\u00fchrten ihn zu diesem Ziel,", "tokens": ["F\u00fchr\u00b7ten", "ihn", "zu", "die\u00b7sem", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Und Petrarch hat ihm gewiesen,", "tokens": ["Und", "Pe\u00b7trarch", "hat", "ihm", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Wie die Tugend wird gepriesen.", "tokens": ["Wie", "die", "Tu\u00b7gend", "wird", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Hat er nun die Sch\u00e4ferinnen", "tokens": ["Hat", "er", "nun", "die", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Schon ger\u00fchmet? Ei, wolan,", "tokens": ["Schon", "ge\u00b7r\u00fch\u00b7met", "?", "Ei", ",", "wo\u00b7lan", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "NN", "$,", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Tugend trieb ihn, welche kan", "tokens": ["Tu\u00b7gend", "trieb", "ihn", ",", "wel\u00b7che", "kan"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAT", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Auch ein steinern Herz gewinnen.", "tokens": ["Auch", "ein", "stei\u00b7nern", "Herz", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Wunder-Wunder-Wunderding,", "tokens": ["Wun\u00b7der\u00b7Wun\u00b7der\u00b7Wun\u00b7der\u00b7ding", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Da\u00df der Meister H\u00e4mmerling,", "tokens": ["Da\u00df", "der", "Meis\u00b7ter", "H\u00e4m\u00b7mer\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Der sonst wolbekante Hase,", "tokens": ["Der", "sonst", "wol\u00b7be\u00b7kan\u00b7te", "Ha\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "Geht davon mit einer Nase", "tokens": ["Geht", "da\u00b7von", "mit", "ei\u00b7ner", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "L\u00e4nger als des Daphnis Pr\u00fcgel.", "tokens": ["L\u00e4n\u00b7ger", "als", "des", "Daph\u00b7nis", "Pr\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "So recht! Nunmehr wirds geschehn,", "tokens": ["So", "recht", "!", "Nun\u00b7mehr", "wirds", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.55": {"text": "Daphnis Lieder werden stehn", "tokens": ["Daph\u00b7nis", "Lie\u00b7der", "wer\u00b7den", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Ewig auf der Musen H\u00fcgel.", "tokens": ["E\u00b7wig", "auf", "der", "Mu\u00b7sen", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wunder-Wunder-Wunderding!", "tokens": ["Wun\u00b7der\u00b7Wun\u00b7der\u00b7Wun\u00b7der\u00b7ding", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Meister H\u00e4mmerling", "tokens": ["Un\u00b7ser", "Meis\u00b7ter", "H\u00e4m\u00b7mer\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibt doch gar zu grobe Possen:", "tokens": ["Treibt", "doch", "gar", "zu", "gro\u00b7be", "Pos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Daphnis mu\u00df ihm sein geschossen", "tokens": ["Daph\u00b7nis", "mu\u00df", "ihm", "sein", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zweifelsfrei mit Liebespfeilen,", "tokens": ["Zwei\u00b7fels\u00b7frei", "mit", "Lie\u00b7be\u00b7spfei\u00b7len", ","], "token_info":