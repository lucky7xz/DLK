{"textgrid.poem.43802": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommt der Vogel Federlos", "tokens": ["Es", "kommt", "der", "Vo\u00b7gel", "Fe\u00b7der\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus hoher Luft gezogen,", "tokens": ["Aus", "ho\u00b7her", "Luft", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ist auf B\u00e4umchen Bl\u00e4tterlos", "tokens": ["Und", "ist", "auf", "B\u00e4um\u00b7chen", "Bl\u00e4t\u00b7ter\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz munter hingeflogen.", "tokens": ["Ganz", "mun\u00b7ter", "hin\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sitzt der Vogel Federlos", "tokens": ["Da", "sitzt", "der", "Vo\u00b7gel", "Fe\u00b7der\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchlt sich recht geborgen,", "tokens": ["Und", "f\u00fchlt", "sich", "recht", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und denkt: hier hast du Ruh und Rast.", "tokens": ["Und", "denkt", ":", "hier", "hast", "du", "Ruh", "und", "Rast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie aber geht's ihm morgen?", "tokens": ["Wie", "a\u00b7ber", "geht's", "ihm", "mor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am andern Morgen hat sich gleich", "tokens": ["Am", "an\u00b7dern", "Mor\u00b7gen", "hat", "sich", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Mundlos hergeschwungen", "tokens": ["Frau", "Mund\u00b7los", "her\u00b7ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hat den Vogel Federlos", "tokens": ["Und", "hat", "den", "Vo\u00b7gel", "Fe\u00b7der\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Haut und Haar verschlungen.", "tokens": ["Mit", "Haut", "und", "Haar", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun rathe, wer da rathen kann!", "tokens": ["Nun", "ra\u00b7the", ",", "wer", "da", "ra\u00b7then", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "V