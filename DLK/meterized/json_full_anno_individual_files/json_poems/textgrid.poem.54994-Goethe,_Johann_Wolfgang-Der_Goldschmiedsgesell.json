nct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach, denk ich, wann, und wieder, wann", "tokens": ["Ach", ",", "denk", "ich", ",", "wann", ",", "und", "wie\u00b7der", ",", "wann"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "PWAV", "$,", "KON", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist solch ein Ring f\u00fcr K\u00e4thchen?", "tokens": ["Ist", "solch", "ein", "Ring", "f\u00fcr", "K\u00e4th\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und tut sie erst die Schaltern auf,", "tokens": ["Und", "tut", "sie", "erst", "die", "Schal\u00b7tern", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kommt das ganze St\u00e4dtchen", "tokens": ["Da", "kommt", "das", "gan\u00b7ze", "St\u00e4dt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und feilscht und wirbt mit hellem Hauf", "tokens": ["Und", "feilscht", "und", "wirbt", "mit", "hel\u00b7lem", "Hauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ums Allerlei im L\u00e4dchen.", "tokens": ["Ums", "Al\u00b7ler\u00b7lei", "im", "L\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPRART", "NN", "$."], 