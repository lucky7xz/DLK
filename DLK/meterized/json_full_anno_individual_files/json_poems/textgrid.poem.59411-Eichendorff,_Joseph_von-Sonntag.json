{"textgrid.poem.59411": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntag", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht war kaum verbl\u00fchet,", "tokens": ["Die", "Nacht", "war", "kaum", "ver\u00b7bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur eine Lerche sang", "tokens": ["Nur", "ei\u00b7ne", "Ler\u00b7che", "sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die stille Luft entlang.", "tokens": ["Die", "stil\u00b7le", "Luft", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wen gr\u00fc\u00dft sie schon so fr\u00fche?", "tokens": ["Wen", "gr\u00fc\u00dft", "sie", "schon", "so", "fr\u00fc\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und drau\u00dfen in dem Garten", "tokens": ["Und", "drau\u00b7\u00dfen", "in", "dem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die B\u00e4ume \u00fcbers Haus", "tokens": ["Die", "B\u00e4u\u00b7me", "\u00fc\u00b7bers", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sahn weit ins Land hinaus,", "tokens": ["Sahn", "weit", "ins", "Land", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als ob sie wen erwarten.", "tokens": ["Als", "ob", "sie", "wen", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In festlichen Gewanden", "tokens": ["In", "fest\u00b7li\u00b7chen", "Ge\u00b7wan\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie eine Kinderschar,", "tokens": ["Wie", "ei\u00b7ne", "Kin\u00b7der\u00b7schar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tauperlen in dem Haar,", "tokens": ["Tau\u00b7per\u00b7len", "in", "dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Blumen alle standen.", "tokens": ["Die", "Blu\u00b7men", "al\u00b7le", "stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich dacht: ihr kleinen Br\u00e4ute,", "tokens": ["Ich", "dacht", ":", "ihr", "klei\u00b7nen", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was schm\u00fcckt ihr euch so sehr? \u2013", "tokens": ["Was", "schm\u00fcckt", "ihr", "euch", "so", "sehr", "?", "\u2013"], "token_inf