{"textgrid.poem.25334": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Totenfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In weichem Wiegen schaukelte die Brandung", "tokens": ["In", "wei\u00b7chem", "Wie\u00b7gen", "schau\u00b7kel\u00b7te", "die", "Bran\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die K\u00f6nigsbarke, die", "tokens": ["Die", "K\u00f6\u00b7nigs\u00b7bar\u00b7ke", ",", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Flor getaucht hinaussah auf die See.", "tokens": ["In", "Flor", "ge\u00b7taucht", "hin\u00b7aus\u00b7sah", "auf", "die", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Ufer standen", "tokens": ["Am", "U\u00b7fer", "stan\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Asen all und sahen wortlos, wie", "tokens": ["Die", "A\u00b7sen", "all", "und", "sa\u00b7hen", "wort\u00b7los", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PIAT", "KON", "VVFIN", "ADJD", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Abendglut in Baldurs Locken weinte.", "tokens": ["Die", "A\u00b7bend\u00b7glut", "in", "Bal\u00b7durs", "Lo\u00b7cken", "wein\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie Rosenduften schwamm's durch goldne L\u00fcfte,", "tokens": ["Wie", "Ro\u00b7sen\u00b7duf\u00b7ten", "schwam\u00b7m's", "durch", "gold\u00b7ne", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Das sonnenuntergangs, wenn alle Blumenkelche", "tokens": ["Das", "son\u00b7nen\u00b7un\u00b7ter\u00b7gangs", ",", "wenn", "al\u00b7le", "Blu\u00b7men\u00b7kel\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr Sehnen gl\u00fchender in den Abend gie\u00dfen,", "tokens": ["Ihr", "Seh\u00b7nen", "gl\u00fc\u00b7hen\u00b7der", "in", "den", "A\u00b7bend", "gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Der Sommerwind auf Sammetfl\u00fcgeln stahl.", "tokens": ["Der", "Som\u00b7mer\u00b7wind", "auf", "Sam\u00b7met\u00b7fl\u00fc\u00b7geln", "stahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auflodernd knirscht die Barke in die Flut.", "tokens": ["Auf\u00b7lo\u00b7dernd", "knirscht", "die", "Bar\u00b7ke", "in", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Stumm standen", "tokens": ["Stumm", "stan\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die Asen \u2013 bleich ... Und sahen", "tokens": ["Die", "A\u00b7sen", "\u2013", "bleich", "...", "Und", "sa\u00b7hen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "ADJD", "$(", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aufs Meer hinaus, wo fern", "tokens": ["Aufs", "Meer", "hin\u00b7aus", ",", "wo", "fern"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "APZR", "$,", "PWAV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Glut verleuchtete. Und es war,", "tokens": ["Die", "Glut", "ver\u00b7leuch\u00b7te\u00b7te", ".", "Und", "es", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "PPER", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als wiche weit durch goldne Abendthore", "tokens": ["Als", "wi\u00b7che", "weit", "durch", "gold\u00b7ne", "A\u00b7bend\u00b7tho\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr Gl\u00fcck und ihre Jugend in die Nacht ...", "tokens": ["Ihr", "Gl\u00fcck", "und", "ih\u00b7re", "Ju\u00b7gend", "in", "die", "Nacht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In weichem Wiegen schaukelte die Brandung", "tokens": ["In", "wei\u00b7chem", "Wie\u00b7gen", "schau\u00b7kel\u0