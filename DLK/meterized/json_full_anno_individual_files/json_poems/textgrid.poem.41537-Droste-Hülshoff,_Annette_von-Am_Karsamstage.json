{"textgrid.poem.41537": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Karsamstage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tiefes, \u00f6des Schweigen,", "tokens": ["Tie\u00b7fes", ",", "\u00f6\u00b7des", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die ganze Erd' wie tot!", "tokens": ["Die", "gan\u00b7ze", "Erd'", "wie", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lerchen ohne Lieder steigen,", "tokens": ["Die", "Ler\u00b7chen", "oh\u00b7ne", "Lie\u00b7der", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne ohne Morgenrot.", "tokens": ["Die", "Son\u00b7ne", "oh\u00b7ne", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf die Welt sich legt", "tokens": ["Auf", "die", "Welt", "sich", "legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der Himmel matt und schwer,", "tokens": ["Der", "Him\u00b7mel", "matt", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Starr und unbewegt,", "tokens": ["Starr", "und", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Wie ein gefrornes Meer.", "tokens": ["Wie", "ein", "ge\u00b7fror\u00b7nes", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, erhalt' uns!", "tokens": ["O", "Herr", ",", "er\u00b7halt'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Meereswogen brechen,", "tokens": ["Mee\u00b7res\u00b7wo\u00b7gen", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sie toben sonder Schall;", "tokens": ["Sie", "to\u00b7ben", "son\u00b7der", "Schall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur die Menschenkinder sprechen,", "tokens": ["Nur", "die", "Men\u00b7schen\u00b7kin\u00b7der", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch schaurig schweigt der Widerhall.", "tokens": ["Doch", "schau\u00b7rig", "schweigt", "der", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie versteinet steht", "tokens": ["Wie", "ver\u00b7stei\u00b7net", "steht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der \u00c4ther um uns her;", "tokens": ["Der", "\u00c4\u00b7ther", "um", "uns", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dringt wohl kein Gebet", "tokens": ["Dringt", "wohl", "kein", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Durch ihn zum Himmel mehr.", "tokens": ["Durch", "ihn", "zum", "Him\u00b7mel", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, erhalt'uns!", "tokens": ["O", "Herr", ",", "er\u00b7halt'\u00b7uns", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "S\u00fcnden sind geschehen,", "tokens": ["S\u00fcn\u00b7den", "sind", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fcr jedes Wort zu gro\u00df,", "tokens": ["F\u00fcr", "je\u00b7des", "Wort", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df die Erde m\u00fc\u00dft' vergehen,", "tokens": ["Da\u00df", "die", "Er\u00b7de", "m\u00fc\u00dft'", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcg' sie nicht Jesu Leib im Scho\u00df.", "tokens": ["Tr\u00fcg'", "sie", "nicht", "Je\u00b7su", "Leib", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch im Tod voll Huld", "tokens": ["Noch", "im", "Tod", "voll", "Huld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Erh\u00e4lt sein Leib die Welt,", "tokens": ["Er\u00b7h\u00e4lt", "sein", "Leib", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df in ihrer Schuld", "tokens": ["Da\u00df", "in", "ih\u00b7rer", "Schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Sie nicht zu Staub zerf\u00e4llt.", "tokens": ["Sie", "nicht", "zu", "Staub", "zer\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, verschon' uns!", "tokens": ["O", "Herr", ",", "ver\u00b7schon'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Jesus liegt im Grabe,", "tokens": ["Je\u00b7sus", "liegt", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Im Grabe liegt mein Gott!", "tokens": ["Im", "Gra\u00b7be", "liegt", "mein", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich von Gedanken habe,", "tokens": ["Was", "ich", "von", "Ge\u00b7dan\u00b7ken", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist doch dagegen nur ein Spott.", "tokens": ["Ist", "doch", "da\u00b7ge\u00b7gen", "nur", "ein", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kennt in Ewigkeit", "tokens": ["Kennt", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Kein Jesus mehr die Welt?", "tokens": ["Kein", "Je\u00b7sus", "mehr", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Keiner der verzeiht,", "tokens": ["Kei\u00b7ner", "der", "ver\u00b7zeiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und keiner der erh\u00e4lt?", "tokens": ["Und", "kei\u00b7ner", "der", "er\u00b7h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, errett' uns!", "tokens": ["O", "Herr", ",", "er\u00b7rett'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ach, auf jene Frommen,", "tokens": ["Ach", ",", "auf", "je\u00b7ne", "From\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die seines Heils geharrt,", "tokens": ["Die", "sei\u00b7nes", "Heils", "ge\u00b7harrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist die Glorie gekommen", "tokens": ["Ist", "die", "Glo\u00b7rie", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit seiner s\u00fc\u00dfen Gegenwart.", "tokens": ["Mit", "sei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Harrten seiner Huld:", "tokens": ["Harr\u00b7ten", "sei\u00b7ner", "Huld", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vergangenheit die Zeit,", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "die", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gegenwart Geduld,", "tokens": ["Ge\u00b7gen\u00b7wart", "Ge\u00b7duld", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Zukunft die Ewigkeit.", "tokens": ["Zu\u00b7kunft", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "O Herr, erl\u00f6s' uns!", "tokens": ["O", "Herr", ",", "er\u00b7l\u00f6s'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Lange, lange Zeiten", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In Glauben und Vertraun,", "tokens": ["In", "Glau\u00b7ben", "und", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch die unbekannten Weiten", "tokens": ["Durch", "die", "un\u00b7be\u00b7kann\u00b7ten", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach unbekanntem Heil sie schaun.", "tokens": ["Nach", "un\u00b7be\u00b7kann\u00b7tem", "Heil", "sie", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dachten sich so viel,", "tokens": ["Dach\u00b7ten", "sich", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Viel Seligkeit und Pracht.", "tokens": ["Viel", "Se\u00b7lig\u00b7keit", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach, es war wie Spiel,", "tokens": ["Ach", ",", "es", "war", "wie", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Von Kindern ausgedacht.", "tokens": ["Von", "Kin\u00b7dern", "aus\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, befrei' uns!", "tokens": ["O", "Herr", ",", "be\u00b7frei'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Herr, ich kann nicht sprechen", "tokens": ["Herr", ",", "ich", "kann", "nicht", "spre\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vor deinem Angesicht!", "tokens": ["Vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df die ganze Sch\u00f6pfung brechen,", "tokens": ["La\u00df", "die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen Tag ertr\u00e4gt sie nicht.", "tokens": ["Die\u00b7sen", "Tag", "er\u00b7tr\u00e4gt", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, was naht so schwer,", "tokens": ["Ach", ",", "was", "naht", "so", "schwer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ist es die ew'ge Nacht,", "tokens": ["Ist", "es", "die", "ew'\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Ist's ein Sonnenmeer,", "tokens": ["Ist's", "ein", "Son\u00b7nen\u00b7meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In tausend Strahlen Pracht?", "tokens": ["In", "tau\u00b7send", "Strah\u00b7len", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, erhalt' uns!", "tokens": ["O", "Herr", ",", "er\u00b7halt'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Tiefes, \u00f6des Schweigen,", "tokens": ["Tie\u00b7fes", ",", "\u00f6\u00b7des", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die ganze Erd' wie tot!", "tokens": ["Die", "gan\u00b7ze", "Erd'", "wie", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lerchen ohne Lieder steigen,", "tokens": ["Die", "Ler\u00b7chen", "oh\u00b7ne", "Lie\u00b7der", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne ohne Morgenrot.", "tokens": ["Die", "Son\u00b7ne", "oh\u00b7ne", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf die Welt sich legt", "tokens": ["Auf", "die", "Welt", "sich", "legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der Himmel matt und schwer,", "tokens": ["Der", "Him\u00b7mel", "matt", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Starr und unbewegt,", "tokens": ["Starr", "und", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Wie ein gefrornes Meer.", "tokens": ["Wie", "ein", "ge\u00b7fror\u00b7nes", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, erhalt' uns!", "tokens": ["O", "Herr", ",", "er\u00b7halt'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Meereswogen brechen,", "tokens": ["Mee\u00b7res\u00b7wo\u00b7gen", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sie toben sonder Schall;", "tokens": ["Sie", "to\u00b7ben", "son\u00b7der", "Schall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur die Menschenkinder sprechen,", "tokens": ["Nur", "die", "Men\u00b7schen\u00b7kin\u00b7der", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch schaurig schweigt der Widerhall.", "tokens": ["Doch", "schau\u00b7rig", "schweigt", "der", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie versteinet steht", "tokens": ["Wie", "ver\u00b7stei\u00b7net", "steht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVPP", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der \u00c4ther um uns her;", "tokens": ["Der", "\u00c4\u00b7ther", "um", "uns", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dringt wohl kein Gebet", "tokens": ["Dringt", "wohl", "kein", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Durch ihn zum Himmel mehr.", "tokens": ["Durch", "ihn", "zum", "Him\u00b7mel", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, erhalt'uns!", "tokens": ["O", "Herr", ",", "er\u00b7halt'\u00b7uns", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "$."], "meter": "-+---", "measure": "dactylic.init"}}, "stanza.10": {"line.1": {"text": "S\u00fcnden sind geschehen,", "tokens": ["S\u00fcn\u00b7den", "sind", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fcr jedes Wort zu gro\u00df,", "tokens": ["F\u00fcr", "je\u00b7des", "Wort", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df die Erde m\u00fc\u00dft' vergehen,", "tokens": ["Da\u00df", "die", "Er\u00b7de", "m\u00fc\u00dft'", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00fcg' sie nicht Jesu Leib im Scho\u00df.", "tokens": ["Tr\u00fcg'", "sie", "nicht", "Je\u00b7su", "Leib", "im", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch im Tod voll Huld", "tokens": ["Noch", "im", "Tod", "voll", "Huld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Erh\u00e4lt sein Leib die Welt,", "tokens": ["Er\u00b7h\u00e4lt", "sein", "Leib", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df in ihrer Schuld", "tokens": ["Da\u00df", "in", "ih\u00b7rer", "Schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Sie nicht zu Staub zerf\u00e4llt.", "tokens": ["Sie", "nicht", "zu", "Staub", "zer\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, verschon' uns!", "tokens": ["O", "Herr", ",", "ver\u00b7schon'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Jesus liegt im Grabe,", "tokens": ["Je\u00b7sus", "liegt", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Im Grabe liegt mein Gott!", "tokens": ["Im", "Gra\u00b7be", "liegt", "mein", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich von Gedanken habe,", "tokens": ["Was", "ich", "von", "Ge\u00b7dan\u00b7ken", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist doch dagegen nur ein Spott.", "tokens": ["Ist", "doch", "da\u00b7ge\u00b7gen", "nur", "ein", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kennt in Ewigkeit", "tokens": ["Kennt", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Kein Jesus mehr die Welt?", "tokens": ["Kein", "Je\u00b7sus", "mehr", "die", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Keiner der verzeiht,", "tokens": ["Kei\u00b7ner", "der", "ver\u00b7zeiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und keiner der erh\u00e4lt?", "tokens": ["Und", "kei\u00b7ner", "der", "er\u00b7h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, errett' uns!", "tokens": ["O", "Herr", ",", "er\u00b7rett'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Ach, auf jene Frommen,", "tokens": ["Ach", ",", "auf", "je\u00b7ne", "From\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die seines Heils geharrt,", "tokens": ["Die", "sei\u00b7nes", "Heils", "ge\u00b7harrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist die Glorie gekommen", "tokens": ["Ist", "die", "Glo\u00b7rie", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit seiner s\u00fc\u00dfen Gegenwart.", "tokens": ["Mit", "sei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Harrten seiner Huld:", "tokens": ["Harr\u00b7ten", "sei\u00b7ner", "Huld", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vergangenheit die Zeit,", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "die", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gegenwart Geduld,", "tokens": ["Ge\u00b7gen\u00b7wart", "Ge\u00b7duld", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Zukunft die Ewigkeit.", "tokens": ["Zu\u00b7kunft", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "O Herr, erl\u00f6s' uns!", "tokens": ["O", "Herr", ",", "er\u00b7l\u00f6s'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Lange, lange Zeiten", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In Glauben und Vertraun,", "tokens": ["In", "Glau\u00b7ben", "und", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch die unbekannten Weiten", "tokens": ["Durch", "die", "un\u00b7be\u00b7kann\u00b7ten", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach unbekanntem Heil sie schaun.", "tokens": ["Nach", "un\u00b7be\u00b7kann\u00b7tem", "Heil", "sie", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dachten sich so viel,", "tokens": ["Dach\u00b7ten", "sich", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Viel Seligkeit und Pracht.", "tokens": ["Viel", "Se\u00b7lig\u00b7keit", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach, es war wie Spiel,", "tokens": ["Ach", ",", "es", "war", "wie", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Von Kindern ausgedacht.", "tokens": ["Von", "Kin\u00b7dern", "aus\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, befrei' uns!", "tokens": ["O", "Herr", ",", "be\u00b7frei'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Herr, ich kann nicht sprechen", "tokens": ["Herr", ",", "ich", "kann", "nicht", "spre\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vor deinem Angesicht!", "tokens": ["Vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df die ganze Sch\u00f6pfung brechen,", "tokens": ["La\u00df", "die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen Tag ertr\u00e4gt sie nicht.", "tokens": ["Die\u00b7sen", "Tag", "er\u00b7tr\u00e4gt", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, was naht so schwer,", "tokens": ["Ach", ",", "was", "naht", "so", "schwer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ist es die ew'ge Nacht,", "tokens": ["Ist", "es", "die", "ew'\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Ist's ein Sonnenmeer,", "tokens": ["Ist's", "ein", "Son\u00b7nen\u00b7meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "In tausend Strahlen Pracht?", "tokens": ["In", "tau\u00b7send", "Strah\u00b7len", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "O Herr, erhalt' uns!", "tokens": ["O", "Herr", ",", "er\u00b7halt'", "uns", "!"], "token_info": ["word", "word",