{"dta.poem.6372": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des holden Welschlands Bauwerck macht,", "tokens": ["Des", "hol\u00b7den", "Wel\u00b7schlands", "Bau\u00b7werck", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df uns die Lufft in schw\u00fclen Sommer k\u00fchlet,", "tokens": ["Da\u00df", "uns", "die", "Lufft", "in", "schw\u00fc\u00b7len", "Som\u00b7mer", "k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indem man sie zur\u00fccke prallen f\u00fchlet", "tokens": ["In\u00b7dem", "man", "sie", "zu\u00b7r\u00fc\u00b7cke", "pral\u00b7len", "f\u00fch\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von einer glatten Fl\u00e4ch\u2019. In unsrer Schl\u00f6sser Pracht,", "tokens": ["Von", "ei\u00b7ner", "glat\u00b7ten", "Fl\u00e4ch'", ".", "In", "uns\u00b7rer", "Schl\u00f6s\u00b7ser", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn Marmor uns die Lufft im Sommer r\u00fcckwerts schicket,", "tokens": ["Wenn", "Mar\u00b7mor", "uns", "die", "Lufft", "im", "Som\u00b7mer", "r\u00fcck\u00b7werts", "schi\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kommt sie gerad auf uns, wodurch sie uns erquicket.", "tokens": ["Kommt", "sie", "ge\u00b7rad", "auf", "uns", ",", "wo\u00b7durch", "sie", "uns", "er\u00b7quic\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}