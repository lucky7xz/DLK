{"dta.poem.14110": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fccksritter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch ich geb' mir keine M\u00fch:", "tokens": ["Doch", "ich", "geb'", "mir", "kei\u00b7ne", "M\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201ehe, noch eine her!\u201c", "tokens": ["\u201e", "he", ",", "noch", "ei\u00b7ne", "her", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$,", "ADV", "ART", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Kehr' den R\u00fccken gegen Sie,", "tokens": ["Kehr'", "den", "R\u00fc\u00b7cken", "ge\u00b7gen", "Sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df' hoch leben die und die \u2014", "tokens": ["La\u00df'", "hoch", "le\u00b7ben", "die", "und", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "VVFIN", "ART", "KON", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das verdrie\u00dft Sie sehr.", "tokens": ["Das", "ver\u00b7drie\u00dft", "Sie", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}