{"textgrid.poem.66149": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der gl\u00fcckliche Ehemann", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so gl\u00fccklich, bin so froh;", "tokens": ["Ich", "bin", "so", "gl\u00fcck\u00b7lich", ",", "bin", "so", "froh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Weiblein darf ich lieben,", "tokens": ["Ein", "Weib\u00b7lein", "darf", "ich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ganz, wie einst K\u00f6nig Salomo", "tokens": ["Ganz", ",", "wie", "einst", "K\u00f6\u00b7nig", "Sa\u00b7lo\u00b7mo"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADV", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein liebstes Weib beschrieben.", "tokens": ["Sein", "liebs\u00b7tes", "Weib", "be\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie r\u00fcstig ist sie sp\u00e4t und fr\u00fch!", "tokens": ["Wie", "r\u00fcs\u00b7tig", "ist", "sie", "sp\u00e4t", "und", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In goldner Morgenstunde", "tokens": ["In", "gold\u00b7ner", "Mor\u00b7gen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weckt sie mich mit der Melodie:", "tokens": ["Weckt", "sie", "mich", "mit", "der", "Me\u00b7lo\u00b7die", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ich hab' den Engel dann und wann", "tokens": ["Ich", "hab'", "den", "En\u00b7gel", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Stillen knieen sehen.", "tokens": ["Im", "Stil\u00b7len", "kni\u00b7e\u00b7en", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+----+-", "measure": "dactylic.init"}, "line.3": {"text": "Da h\u00f6rt' ich sie f\u00fcr ihren Mann", "tokens": ["Da", "h\u00f6rt'", "ich", "sie", "f\u00fcr", "ih\u00b7ren", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Kinder flehen.", "tokens": ["Und", "ih\u00b7re", "Kin\u00b7der", "fle\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Bibelbuche liest sie gern,", "tokens": ["Im", "Bi\u00b7bel\u00b7bu\u00b7che", "liest", "sie", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei jeder sch\u00f6nen Stelle", "tokens": ["Bei", "je\u00b7der", "sch\u00f6\u00b7nen", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird meines Weibchens Augenstern", "tokens": ["Wird", "mei\u00b7nes", "Weib\u00b7chens", "Au\u00b7gens\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Von frommen Z\u00e4hren helle.", "tokens": ["Von", "from\u00b7men", "Z\u00e4h\u00b7ren", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann rennt so frisch das gute Kind", "tokens": ["Dann", "rennt", "so", "frisch", "das", "gu\u00b7te", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Hause hin und wieder,", "tokens": ["Im", "Hau\u00b7se", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Befiehlt; und h\u00e4lt doch das Gesind'", "tokens": ["Be\u00b7fiehlt", ";", "und", "h\u00e4lt", "doch", "das", "Ge\u00b7sind'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Schwestern und f\u00fcr Br\u00fcder.", "tokens": ["F\u00fcr", "Schwes\u00b7tern", "und", "f\u00fcr", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dem Vieh gebricht sein Futter nie,", "tokens": ["Dem", "Vieh", "ge\u00b7bricht", "sein", "Fut\u00b7ter", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie flattert ihr entgegen", "tokens": ["Wie", "flat\u00b7tert", "ihr", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Hof das bunte Federvieh", "tokens": ["Im", "Hof", "das", "bun\u00b7te", "Fe\u00b7der\u00b7vieh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und pickt den goldnen Regen.", "tokens": ["Und", "pickt", "den", "gold\u00b7nen", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Als Mutter erst \u2013 da solltet ihr", "tokens": ["Als", "Mut\u00b7ter", "erst", "\u2013", "da", "soll\u00b7tet", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "$(", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies Herzensweiblein kennen.", "tokens": ["Dies", "Her\u00b7zens\u00b7wei\u00b7blein", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schw\u00f6r' euch, ihr w\u00fcrdet sie mit mir", "tokens": ["Schw\u00f6r'", "euch", ",", "ihr", "w\u00fcr\u00b7det", "sie", "mit", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der M\u00fctter Muster nennen.", "tokens": ["Der", "M\u00fct\u00b7ter", "Mus\u00b7ter", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie lehrt die treue Mutter nicht", "tokens": ["Wie", "lehrt", "die", "treu\u00b7e", "Mut\u00b7ter", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den T\u00f6chtern und den S\u00f6hnen,", "tokens": ["Den", "T\u00f6ch\u00b7tern", "und", "den", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur Fertigkeit in jeder Pflicht", "tokens": ["Zur", "Fer\u00b7tig\u00b7keit", "in", "je\u00b7der", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich zeitig zu gew\u00f6hnen!", "tokens": ["Sich", "zei\u00b7tig", "zu", "ge\u00b7w\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dann setzt sie, wie das Bild der ", "tokens": ["Dann", "setzt", "sie", ",", "wie", "das", "Bild", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich still an meine Seite.", "tokens": ["Sich", "still", "an", "mei\u00b7ne", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6r' dem Tanz der Spindel zu", "tokens": ["Ich", "h\u00f6r'", "dem", "Tanz", "der", "Spin\u00b7del", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit inniglicher Freude.", "tokens": ["Mit", "in\u00b7nig\u00b7li\u00b7cher", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.10": {"line.1": {"text": "Wie wird die Arbeit mir so leicht!", "tokens": ["Wie", "wird", "die", "Ar\u00b7beit", "mir", "so", "leicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es streichelt mich die Liebe,", "tokens": ["Es", "strei\u00b7chelt", "mich", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sieht sie oft meine Stirne feucht", "tokens": ["Sieht", "sie", "oft", "mei\u00b7ne", "Stir\u00b7ne", "feucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Augen tr\u00fcbe.", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ihr Fr\u00fchlingsl\u00e4cheln im Gesicht", "tokens": ["Ihr", "Fr\u00fch\u00b7lings\u00b7l\u00e4\u00b7cheln", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lehrt mich des Lebens Plagen,", "tokens": ["Lehrt", "mich", "des", "Le\u00b7bens", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lehrt Zentner, wie ein Lothgewicht,", "tokens": ["Lehrt", "Zent\u00b7ner", ",", "wie", "ein", "Loth\u00b7ge\u00b7wicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich Gl\u00fccklichen ertragen.", "tokens": ["Mich", "Gl\u00fcck\u00b7li\u00b7chen", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie sorgt f\u00fcr mein gesundes Mahl;", "tokens": ["Sie", "sorgt", "f\u00fcr", "mein", "ge\u00b7sun\u00b7des", "Mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und reicht mir, will ich trinken,", "tokens": ["Und", "reicht", "mir", ",", "will", "ich", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit L\u00e4cheln selber den Pokal,", "tokens": ["Mit", "L\u00e4\u00b7cheln", "sel\u00b7ber", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drin goldne Tropfen blinken.", "tokens": ["Drin", "gold\u00b7ne", "Trop\u00b7fen", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Des Himmels Pracht, der Auen Zier,", "tokens": ["Des", "Him\u00b7mels", "Pracht", ",", "der", "Au\u00b7en", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das spiegelnde Gew\u00e4sser,", "tokens": ["Das", "spie\u00b7geln\u00b7de", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du holdes Weib, gefallen mir", "tokens": ["Du", "hol\u00b7des", "Weib", ",", "ge\u00b7fal\u00b7len", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVPP", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deiner Seite besser.", "tokens": ["An", "dei\u00b7ner", "Sei\u00b7te", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Drum steigt mein Dank zum Himmel hin,", "tokens": ["Drum", "steigt", "mein", "Dank", "zum", "Him\u00b7mel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Thr\u00e4nen mir entbeben,", "tokens": ["Da\u00df", "Thr\u00e4\u00b7nen", "mir", "ent\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil Gott zur Lebensf\u00fchrerin", "tokens": ["Weil", "Gott", "zur", "Le\u00b7bens\u00b7f\u00fch\u00b7re\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir solch ein Weib gegeben.", "tokens": ["Mir", "solch", "ein", "Weib", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Mit ihr la\u00df mich durchs Erdenthal,", "tokens": ["Mit", "ihr", "la\u00df", "mich", "durchs", "Er\u00b7dent\u00b7hal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVIMP", "PPER", "APPRART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Du Gott der Liebe, wallen;", "tokens": ["Du", "Gott", "der", "Lie\u00b7be", ",", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihr in deines Thrones Strahl", "tokens": ["Mit", "ihr", "in", "dei\u00b7nes", "Thro\u00b7nes", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einst dankend niederfallen.", "tokens": ["Einst", "dan\u00b7kend", "nie\u00b7der\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"