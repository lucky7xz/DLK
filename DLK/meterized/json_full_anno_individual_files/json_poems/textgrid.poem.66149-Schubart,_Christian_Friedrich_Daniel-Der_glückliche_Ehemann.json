iambic.tetra"}, "line.2": {"text": "Allm\u00e4chtiger, gepriesen!", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ger", ",", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}