en ertragen.", "tokens": ["Mich", "Gl\u00fcck\u00b7li\u00b7chen", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Sie sorgt f\u00fcr mein gesundes Mahl;", "tokens": ["Sie", "sorgt", "f\u00fcr", "mein", "ge\u00b7sun\u00b7des", "Mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und reicht mir, will ich trinken,", "tokens": ["Und", "reicht", "mir", ",", "will", "ich", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit L\u00e4cheln selber den Pokal,", "tokens": ["Mit", "L\u00e4\u00b7cheln", "sel\u00b7ber", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drin goldne Tropfen blinken.", "tokens": ["Drin", "gold\u00b7ne", "Trop\u00b7fen", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Des Himmels Pracht, der Auen Zier,", "tokens": ["Des", "Him\u00b7mels", "Pracht", ",", "der", "Au\u00b7en", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das spiegelnde Gew\u00e4sser,", "tokens": ["Das", "spie\u00b7geln\u00b7de", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du holdes Weib, gefallen mir", "tokens": ["Du", "hol\u00b7des", "Weib", ",", "ge\u00b7fal\u00b7len", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVPP", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deiner Seite besser.", "tokens": ["An", "dei\u00b7ner", "Sei\u00b7te", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Drum steigt mein Dank zum Himmel hin,", "tokens": ["Drum", "steigt", "mein", "Dank", "zum", "Him\u00b7mel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Thr\u00e4nen mir entbeben,", "tokens": ["Da\u00df", "Thr\u00e4\u00b7nen", "mir", "ent\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil Gott zur Lebensf\u00fchrerin", "tokens": ["Weil", "Gott", "zur", "Le\u00b7bens\u00b7f\u00fch\u00b7re\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir solch ein Weib gegeben.", "tokens": ["Mir", "solch", "ein", "Weib", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Mit ihr la\u00df mich durchs Erdenthal,", "tokens": ["Mit", "ihr", "la\u00df", "mich", "durchs", "Er\