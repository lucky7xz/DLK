{"textgrid.poem.48564": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Auf des Hochedlen Herrn Peter Kru\u00dfbiorn, K\u00f6nigl. Schwedischen Residenten in der Moskow u.s.w., seines erstgebornen S\u00f6hnleins Absterben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmel auf und zu den Engeln,", "tokens": ["Him\u00b7mel", "auf", "und", "zu", "den", "En\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da man wei\u00df von keinen M\u00e4ngeln,", "tokens": ["da", "man", "wei\u00df", "von", "kei\u00b7nen", "M\u00e4n\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmel auf und selbst zu Gott", "tokens": ["Him\u00b7mel", "auf", "und", "selbst", "zu", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ist der reine Geist geflogen;", "tokens": ["ist", "der", "rei\u00b7ne", "Geist", "ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da, da wird er aufgezogen,", "tokens": ["da", ",", "da", "wird", "er", "auf\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da, da i\u00dft er Engelbrot.", "tokens": ["da", ",", "da", "i\u00dft", "er", "En\u00b7gel\u00b7brot", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00dfe Seele, soll ich klagen", "tokens": ["S\u00fc\u00b7\u00dfe", "See\u00b7le", ",", "soll", "ich", "kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mit deinen Eltern sagen:", "tokens": ["und", "mit", "dei\u00b7nen", "El\u00b7tern", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bballzu zeitlich, kurzer Gast!?\u00ab", "tokens": ["\u00bb", "all\u00b7zu", "zeit\u00b7lich", ",", "kur\u00b7zer", "Gast", "!?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PTKA", "ADJD", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein! Selbst sie auch m\u00fcssen sprechen,", "tokens": ["Nein", "!", "Selbst", "sie", "auch", "m\u00fcs\u00b7sen", "spre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "doch mit reichen Tr\u00e4nenb\u00e4chen:", "tokens": ["doch", "mit", "rei\u00b7chen", "Tr\u00e4\u00b7nen\u00b7b\u00e4\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbwol dir, da\u00df du Ruhe hast!\u00ab", "tokens": ["\u00bb", "wol", "dir", ",", "da\u00df", "du", "Ru\u00b7he", "hast", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Edel' Eltern, seids gest\u00e4ndig,", "tokens": ["E\u00b7del'", "El\u00b7tern", ",", "seids", "ge\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nichts ist unser eigenh\u00e4ndig,", "tokens": ["nichts", "ist", "un\u00b7ser", "ei\u00b7gen\u00b7h\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "alles Unser' ist geborgt;", "tokens": ["al\u00b7les", "Un\u00b7ser'", "ist", "ge\u00b7borgt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bevoraus die s\u00fc\u00dfen Erben", "tokens": ["be\u00b7vo\u00b7raus", "die", "s\u00fc\u00b7\u00dfen", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "hei\u00dft der H\u00f6chste zeitlich sterben,", "tokens": ["hei\u00dft", "der", "H\u00f6chs\u00b7te", "zeit\u00b7lich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df sie besser sein versorgt.", "tokens": ["da\u00df", "sie", "bes\u00b7ser", "sein", "ver\u00b7sorgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gott, der wechselt stetigs abe,", "tokens": ["Gott", ",", "der", "wech\u00b7selt", "ste\u00b7tigs", "a\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "untermenget Raub und Gabe,", "tokens": ["un\u00b7ter\u00b7men\u00b7get", "Raub", "und", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "schenket, da\u00df er nehmen kan.", "tokens": ["schen\u00b7ket", ",", "da\u00df", "er", "neh\u00b7men", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klaget, was ihr k\u00f6nnet klagen!", "tokens": ["Kla\u00b7get", ",", "was", "ihr", "k\u00f6n\u00b7net", "kla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch m\u00fc\u00dft ihr endlich sagen:", "tokens": ["Den\u00b7noch", "m\u00fc\u00dft", "ihr", "end\u00b7lich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "es ist Alles wol getan!", "tokens": ["es", "ist", "Al\u00b7les", "wol", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00dft den h\u00f6chsten Vater walten!", "tokens": ["La\u00dft", "den", "h\u00f6chs\u00b7ten", "Va\u00b7ter", "wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der so lang' hat Haus gehalten,", "tokens": ["Der", "so", "lang'", "hat", "Haus", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der wird nun verderben Nichts.", "tokens": ["der", "wird", "nun", "ver\u00b7der\u00b7ben", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Christen lassen Christum handeln,", "tokens": ["Chris\u00b7ten", "las\u00b7sen", "Chris\u00b7tum", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "folgen, wie er vor will wandeln;", "tokens": ["fol\u00b7gen", ",", "wie", "er", "vor", "will", "wan\u00b7deln", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "PWAV", "PPER", "APPR", "VMFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nur ein Heide der versprichts.", "tokens": ["nur", "ein", "Hei\u00b7de", "der", "ver\u00b7sprichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Vorgeschickt ist unverloren.", "tokens": ["Vor\u00b7ge\u00b7schickt", "ist", "un\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er hat einen Ort erkoren,", "tokens": ["Er", "hat", "ei\u00b7nen", "Ort", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "da er bleibt und ihr solt hin.", "tokens": ["da", "er", "bleibt", "und", "ihr", "solt", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "PPER", "VMFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lasset denn nach eurem Schatze", "tokens": ["Las\u00b7set", "denn", "nach", "eu\u00b7rem", "Schat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und nach seinem edlen Platze", "tokens": ["und", "nach", "sei\u00b7nem", "ed\u00b7len", "Plat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "euren Mut und Herzen ziehn!", "tokens": ["eu\u00b7ren", "Mut", "und", "Her\u00b7zen", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tausent sind ihr, die dem Leben", "tokens": ["Tau\u00b7sent", "sind", "ihr", ",", "die", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gerne wolten Urlaub geben;", "tokens": ["ger\u00b7ne", "wol\u00b7ten", "Ur\u00b7laub", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "tausent Ander' stehn in Not,", "tokens": ["tau\u00b7sent", "An\u00b7der'", "stehn", "in", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df sie nicht zu bald verderben.", "tokens": ["da\u00df", "sie", "nicht", "zu", "bald", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PTKA", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eurer Sohn hat sollen sterben,", "tokens": ["Eu\u00b7rer", "Sohn", "hat", "sol\u00b7len", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "eh' er w\u00fcndscht und scheut den Tod.", "tokens": ["eh'", "er", "w\u00fcnd\u00b7scht", "und", "scheut", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00dcbel uns betagten S\u00fcndern!", "tokens": ["\u00dc\u00b7bel", "uns", "be\u00b7tag\u00b7ten", "S\u00fcn\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie wol geschicht den Kindern,", "tokens": ["O", "wie", "wol", "ge\u00b7schicht", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die bald sagen gute Nacht", "tokens": ["die", "bald", "sa\u00b7gen", "gu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "ADJA", "NN"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "und sind unbesorgt der Sachen,", "tokens": ["und", "sind", "un\u00b7be\u00b7sorgt", "der", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die uns Alten bange machen", "tokens": ["die", "uns", "Al\u00b7ten", "ban\u00b7ge", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fcr des letzten Richters Macht!", "tokens": ["f\u00fcr", "des", "letz\u00b7ten", "Rich\u00b7ters", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn das Wesen aller Sachen", "tokens": ["Wenn", "das", "We\u00b7sen", "al\u00b7ler", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in der letzten Glut wird krachen,", "tokens": ["in", "der", "letz\u00b7ten", "Glut", "wird", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles tot und nichts mehr sein,", "tokens": ["Al\u00b7les", "tot", "und", "nichts", "mehr", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "PIS", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "denn so soll ein neues Leben", "tokens": ["denn", "so", "soll", "ein", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "in verj\u00fcngter Erden weben,", "tokens": ["in", "ver\u00b7j\u00fcng\u00b7ter", "Er\u00b7den", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da uns Gott will setzen ein.", "tokens": ["da", "uns", "Gott", "will", "set\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "R\u00fccket zu, ihr lieben Sternen,", "tokens": ["R\u00fc\u00b7cket", "zu", ",", "ihr", "lie\u00b7ben", "Ster\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zeigt es, zeigt es uns von fernen,", "tokens": ["zeigt", "es", ",", "zeigt", "es", "uns", "von", "fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "zeigts uns stets das edle Bild!", "tokens": ["zeigts", "uns", "stets", "das", "ed\u00b7le", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Gebeinlein soll hienieden", "tokens": ["Sein", "Ge\u00b7bein\u00b7lein", "soll", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "um sich haben eitel Frieden,", "tokens": ["um", "sich", "ha\u00b7ben", "ei\u00b7tel", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "VAFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ganz in Blumen eingeh\u00fcllt.", "tokens": ["ganz", "in", "Blu\u00b7men", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Himmel auf und zu den Engeln,", "tokens": ["Him\u00b7mel", "auf", "und", "zu", "den", "En\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da man wei\u00df von keinen M\u00e4ngeln,", "tokens": ["da", "man", "wei\u00df", "von", "kei\u00b7nen", "M\u00e4n\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmel auf und selbst zu Gott", "tokens": ["Him\u00b7mel", "auf", "und", "selbst", "zu", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ist der reine Geist geflogen;", "tokens": ["ist", "der", "rei\u00b7ne", "Geist", "ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da, da wird er aufgezogen,", "tokens": ["da", ",", "da", "wird", "er", "auf\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da, da i\u00dft er Engelbrot.", "tokens": ["da", ",", "da", "i\u00dft", "er", "En\u00b7gel\u00b7brot", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "S\u00fc\u00dfe Seele, soll ich klagen", "tokens": ["S\u00fc\u00b7\u00dfe", "See\u00b7le", ",", "soll", "ich", "kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mit deinen Eltern sagen:", "tokens": ["und", "mit", "dei\u00b7nen", "El\u00b7tern", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bballzu zeitlich, kurzer Gast!?\u00ab", "tokens": ["\u00bb", "all\u00b7zu", "zeit\u00b7lich", ",", "kur\u00b7zer", "Gast", "!?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PTKA", "ADJD", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein! Selbst sie auch m\u00fcssen sprechen,", "tokens": ["Nein", "!", "Selbst", "sie", "auch", "m\u00fcs\u00b7sen", "spre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "doch mit reichen Tr\u00e4nenb\u00e4chen:", "tokens": ["doch", "mit", "rei\u00b7chen", "Tr\u00e4\u00b7nen\u00b7b\u00e4\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00bbwol dir, da\u00df du Ruhe hast!\u00ab", "tokens": ["\u00bb", "wol", "dir", ",", "da\u00df", "du", "Ru\u00b7he", "hast", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Edel' Eltern, seids gest\u00e4ndig,", "tokens": ["E\u00b7del'", "El\u00b7tern", ",", "seids", "ge\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nichts ist unser eigenh\u00e4ndig,", "tokens": ["nichts", "ist", "un\u00b7ser", "ei\u00b7gen\u00b7h\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "alles Unser' ist geborgt;", "tokens": ["al\u00b7les", "Un\u00b7ser'", "ist", "ge\u00b7borgt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "bevoraus die s\u00fc\u00dfen Erben", "tokens": ["be\u00b7vo\u00b7raus", "die", "s\u00fc\u00b7\u00dfen", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "hei\u00dft der H\u00f6chste zeitlich sterben,", "tokens": ["hei\u00dft", "der", "H\u00f6chs\u00b7te", "zeit\u00b7lich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df sie besser sein versorgt.", "tokens": ["da\u00df", "sie", "bes\u00b7ser", "sein", "ver\u00b7sorgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Gott, der wechselt stetigs abe,", "tokens": ["Gott", ",", "der", "wech\u00b7selt", "ste\u00b7tigs", "a\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "untermenget Raub und Gabe,", "tokens": ["un\u00b7ter\u00b7men\u00b7get", "Raub", "und", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "schenket, da\u00df er nehmen kan.", "tokens": ["schen\u00b7ket", ",", "da\u00df", "er", "neh\u00b7men", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klaget, was ihr k\u00f6nnet klagen!", "tokens": ["Kla\u00b7get", ",", "was", "ihr", "k\u00f6n\u00b7net", "kla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch m\u00fc\u00dft ihr endlich sagen:", "tokens": ["Den\u00b7noch", "m\u00fc\u00dft", "ihr", "end\u00b7lich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "es ist Alles wol getan!", "tokens": ["es", "ist", "Al\u00b7les", "wol", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "La\u00dft den h\u00f6chsten Vater walten!", "tokens": ["La\u00dft", "den", "h\u00f6chs\u00b7ten", "Va\u00b7ter", "wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der so lang' hat Haus gehalten,", "tokens": ["Der", "so", "lang'", "hat", "Haus", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der wird nun verderben Nichts.", "tokens": ["der", "wird", "nun", "ver\u00b7der\u00b7ben", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Christen lassen Christum handeln,", "tokens": ["Chris\u00b7ten", "las\u00b7sen", "Chris\u00b7tum", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "folgen, wie er vor will wandeln;", "tokens": ["fol\u00b7gen", ",", "wie", "er", "vor", "will", "wan\u00b7deln", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "PWAV", "PPER", "APPR", "VMFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nur ein Heide der versprichts.", "tokens": ["nur", "ein", "Hei\u00b7de", "der", "ver\u00b7sprichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Vorgeschickt ist unverloren.", "tokens": ["Vor\u00b7ge\u00b7schickt", "ist", "un\u00b7ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er hat einen Ort erkoren,", "tokens": ["Er", "hat", "ei\u00b7nen", "Ort", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "da er bleibt und ihr solt hin.", "tokens": ["da", "er", "bleibt", "und", "ihr", "solt", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "PPER", "VMFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lasset denn nach eurem Schatze", "tokens": ["Las\u00b7set", "denn", "nach", "eu\u00b7rem", "Schat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und nach seinem edlen Platze", "tokens": ["und", "nach", "sei\u00b7nem", "ed\u00b7len", "Plat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "euren Mut und Herzen ziehn!", "tokens": ["eu\u00b7ren", "Mut", "und", "Her\u00b7zen", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Tausent sind ihr, die dem Leben", "tokens": ["Tau\u00b7sent", "sind", "ihr", ",", "die", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gerne wolten Urlaub geben;", "tokens": ["ger\u00b7ne", "wol\u00b7ten", "Ur\u00b7laub", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "tausent Ander' stehn in Not,", "tokens": ["tau\u00b7sent", "An\u00b7der'", "stehn", "in", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df sie nicht zu bald verderben.", "tokens": ["da\u00df", "sie", "nicht", "zu", "bald", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PTKA", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eurer Sohn hat sollen sterben,", "tokens": ["Eu\u00b7rer", "Sohn", "hat", "sol\u00b7len", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "eh' er w\u00fcndscht und scheut den Tod.", "tokens": ["eh'", "er", "w\u00fcnd\u00b7scht", "und", "scheut", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "\u00dcbel uns betagten S\u00fcndern!", "tokens": ["\u00dc\u00b7bel", "uns", "be\u00b7tag\u00b7ten", "S\u00fcn\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie wol geschicht den Kindern,", "tokens": ["O", "wie", "wol", "ge\u00b7schicht", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "die bald sagen gute Nacht", "tokens": ["die", "bald", "sa\u00b7gen", "gu\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "ADJA", "NN"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "und sind unbesorgt der Sachen,", "tokens": ["und", "sind", "un\u00b7be\u00b7sorgt", "der", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die uns Alten bange machen", "tokens": ["die", "uns", "Al\u00b7ten", "ban\u00b7ge", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "f\u00fcr des letzten Richters Macht!", "tokens": ["f\u00fcr", "des", "letz\u00b7ten", "Rich\u00b7ters", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Wenn das Wesen aller Sachen", "tokens": ["Wenn", "das", "We\u00b7sen", "al\u00b7ler", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in der letzten Glut wird krachen,", "tokens": ["in", "der", "letz\u00b7ten", "Glut", "wird", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles tot und nichts mehr sein,", "tokens": ["Al\u00b7les", "tot", "und", "nichts", "mehr", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "PIS", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "denn so soll ein neues Leben", "tokens": ["denn", "so", "soll", "ein", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "in verj\u00fcngter Erden weben,", "tokens": ["in", "ver\u00b7j\u00fcng\u00b7ter", "Er\u00b7den", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da uns Gott will setzen ein.", "tokens": ["da", "uns", "Gott", "will", "set\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "R\u00fccket zu, ihr lieben Sternen,", "tokens": ["R\u00fc\u00b7cket", "zu", ",", "ihr", "lie\u00b7ben", "Ster\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "zeigt es, zeigt es uns von fernen,", "tokens": ["zeigt", "es", ",", "zeigt", "es", "uns", "von", "fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "zeigts uns stets das edle Bild!", "tokens": ["zeigts", "uns", "stets", "das", "ed\u00b7le", "Bild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Gebeinlein soll hienieden", "tokens": ["Sein", "Ge\u00b7bein\u00b7lein", "soll", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "um sich haben eitel Frieden,", "tokens": ["um", "sich", "ha\u00b7ben", "ei\u00b7tel", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "VAFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ganz in Blumen eingeh\u00fcllt.", "tokens": ["ganz", "in", "Blu\u00b7men", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}