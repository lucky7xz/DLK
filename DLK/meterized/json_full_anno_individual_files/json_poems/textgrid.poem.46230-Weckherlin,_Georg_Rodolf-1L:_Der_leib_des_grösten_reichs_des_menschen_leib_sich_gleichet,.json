{"textgrid.poem.46230": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der leib des gr\u00f6sten reichs des menschen leib sich gleichet,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der leib des gr\u00f6sten reichs des menschen leib sich gleichet,", "tokens": ["Der", "leib", "des", "gr\u00f6s\u00b7ten", "reichs", "des", "men\u00b7schen", "leib", "sich", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "in beeden sihet man, wie durch m\u00fcssigkeit schand,", "tokens": ["in", "bee\u00b7den", "si\u00b7het", "man", ",", "wie", "durch", "m\u00fcs\u00b7sig\u00b7keit", "schand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PIS", "$,", "PWAV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+----+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "durch schand uneinigkeit, durch zertrennung aufstand,", "tokens": ["durch", "schand", "un\u00b7ei\u00b7nig\u00b7keit", ",", "durch", "zer\u00b7tren\u00b7nung", "auf\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADJD", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "durch entp\u00f6rung schwachheit, durch schwachheit der tod schleichet;", "tokens": ["durch", "ent\u00b7p\u00f6\u00b7rung", "schwach\u00b7heit", ",", "durch", "schwach\u00b7heit", "der", "tod", "schlei\u00b7chet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", 