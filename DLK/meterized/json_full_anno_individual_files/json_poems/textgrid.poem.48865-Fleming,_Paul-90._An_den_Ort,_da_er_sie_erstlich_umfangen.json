{"textgrid.poem.48865": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "90. An den Ort, da er sie erstlich umfangen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es m\u00fcss' ein ewger Lenz mit steten Favoninnen", "tokens": ["Es", "m\u00fcss'", "ein", "ew\u00b7ger", "Lenz", "mit", "ste\u00b7ten", "Fa\u00b7vo\u00b7nin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "auf dein gep\u00fcschtes H\u00e4upt und blumicht Antlitz wehn,", "tokens": ["auf", "dein", "ge\u00b7p\u00fcschtes", "H\u00e4upt", "und", "blu\u00b7micht", "Ant\u00b7litz", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "die reichbetaute Brust den Perlen gleiche stehn", "tokens": ["die", "reich\u00b7be\u00b7tau\u00b7te", "Brust", "den", "Per\u00b7len", "glei\u00b7che", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und deine feuchte Scho\u00df mit kalten Br\u00fcnnen rinnen.", "tokens": ["und", "dei\u00b7ne", "feuch\u00b7te", "Scho\u00df", "mit", "kal\u00b7ten", "Br\u00fcn\u00b7nen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So sollen j\u00e4rlich auch hier meine Kastalinnen", "tokens": ["So", "sol\u00b7len", "j\u00e4r\u00b7lich", "auch", "hier", "mei\u00b7ne", "Kas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit Zweigen vom Parnass' um dein Gehege gehn,", "tokens": ["mit", "Zwei\u00b7gen", "vom", "Par\u00b7nass'", "um", "dein", "Ge\u00b7he\u00b7ge", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und deine Treflichkeit durch solch ein Lied erh\u00f6hn,", "tokens": ["und", "dei\u00b7ne", "Tref\u00b7lich\u00b7keit", "durch", "solch", "ein", "Lied", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das du nur w\u00fcrdig bist. Und die uns lieb gewinnen,", "tokens": ["das", "du", "nur", "w\u00fcr\u00b7dig", "bist", ".", "Und", "die", "uns", "lieb", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAFIN", "$.", "KON", "ART", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "die Hamadryaden, die sollen dir f\u00fcr Schaden,", "tokens": ["die", "Ha\u00b7mad\u00b7ry\u00b7a\u00b7den", ",", "die", "sol\u00b7len", "dir", "f\u00fcr", "Scha\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "po