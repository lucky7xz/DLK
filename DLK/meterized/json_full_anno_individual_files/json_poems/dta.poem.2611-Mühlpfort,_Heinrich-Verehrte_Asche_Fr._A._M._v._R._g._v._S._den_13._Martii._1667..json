{"dta.poem.2611": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Verehrte Asche/  \n  Fr. A. M. v. R. g. v. S. den 13. Martii.  1667.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Leser la\u00df dir nicht vor meinen Zeilen grauen/", "tokens": ["Mein", "Le\u00b7ser", "la\u00df", "dir", "nicht", "vor", "mei\u00b7nen", "Zei\u00b7len", "grau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die voller Asche sind und reden nur von Staub/", "tokens": ["Die", "vol\u00b7ler", "A\u00b7sche", "sind", "und", "re\u00b7den", "nur", "von", "Staub", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KON", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du wirst den Inbegriff der Eitelkeiten schauen/", "tokens": ["Du", "wirst", "den", "In\u00b7be\u00b7griff", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie nichts mehr \u00fcbrig ist als dieser Aschen-Raub.", "tokens": ["Wie", "nichts", "mehr", "\u00fcb\u00b7rig", "ist", "als", "die\u00b7ser", "A\u00b7schen\u00b7Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADJD", "VAFIN", "KOKOM", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In eitlen Augen sind es wol betr\u00fcbte Blicke/", "tokens": ["In", "eit\u00b7len", "Au\u00b7gen", "sind", "es", "wol", "be\u00b7tr\u00fcb\u00b7te", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df von der Herrligkeit des Menschen di\u00df nur bleibt/", "tokens": ["Da\u00df", "von", "der", "Herr\u00b7lig\u00b7keit", "des", "Men\u00b7schen", "di\u00df", "nur", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "PDS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch weil sie GOtt erwehlt/ und braucht zum Opferst\u00fccke/", "tokens": ["Doch", "weil", "sie", "Gott", "er\u00b7wehlt", "/", "und", "braucht", "zum", "Op\u00b7fer\u00b7st\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVPP", "$(", "KON", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja gar des Himmels Erb\u2019 in solcher uns verschreibt:", "tokens": ["Ja", "gar", "des", "Him\u00b7mels", "Erb'", "in", "sol\u00b7cher", "uns", "ver\u00b7schreibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "NN", "APPR", "PIAT", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So kan ein feurig Geist auch hier Vergn\u00fcgung finden/", "tokens": ["So", "kan", "ein", "feu\u00b7rig", "Geist", "auch", "hier", "Ver\u00b7gn\u00fc\u00b7gung", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJD", "NN", "ADV", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn er sich Adlern gleich schwingt zu der Sternen H\u00f6h\u2019/", "tokens": ["Wenn", "er", "sich", "Ad\u00b7lern", "gleich", "schwingt", "zu", "der", "Ster\u00b7nen", "H\u00f6h'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Und wei\u00df durch wahre Spur de\u00df Glaubens zuergr\u00fcnden/", "tokens": ["Und", "wei\u00df", "durch", "wah\u00b7re", "Spur", "de\u00df", "Glau\u00b7bens", "zu\u00b7er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df der verweste Leib in Asche nicht vergeh\u2019.", "tokens": ["Da\u00df", "der", "ver\u00b7wes\u00b7te", "Leib", "in", "A\u00b7sche", "nicht", "ver\u00b7geh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier ist sie ein Entwurff der irrdischen Gebrechen/", "tokens": ["Hier", "ist", "sie", "ein", "Ent\u00b7wurff", "der", "irr\u00b7di\u00b7schen", "Ge\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und dort ein Saame der zu Schmuck und Klarheit bl\u00fcht;", "tokens": ["Und", "dort", "ein", "Saa\u00b7me", "der", "zu", "Schmuck", "und", "Klar\u00b7heit", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn GOttes Richt-Tag wird das grosse Urtheil sprechen/", "tokens": ["Wenn", "Got\u00b7tes", "Richt\u00b7Tag", "wird", "das", "gros\u00b7se", "Ur\u00b7theil", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gewi\u00df/ da\u00df sie mehr sch\u00f6n als alle Blumen sieht.", "tokens": ["Ge\u00b7wi\u00df", "/", "da\u00df", "sie", "mehr", "sch\u00f6n", "als", "al\u00b7le", "Blu\u00b7men", "sieht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PPER", "ADV", "ADJD", "KOKOM", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die tieffste Wei\u00dfheit kan ein Aschen-Hauff uns zeigen/", "tokens": ["Die", "tieffs\u00b7te", "Wei\u00df\u00b7heit", "kan", "ein", "A\u00b7schen\u00b7Hauff", "uns", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein morscher Todten-Kopff ist eine hohe Schul.", "tokens": ["Ein", "mor\u00b7scher", "Tod\u00b7ten\u00b7Kopff", "ist", "ei\u00b7ne", "ho\u00b7he", "Schul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn sich das Leimen Hau\u00df mu\u00df zu der Erden neigen/", "tokens": ["Wenn", "sich", "das", "Lei\u00b7men", "Hau\u00df", "mu\u00df", "zu", "der", "Er\u00b7den", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So steht doch einst der Leib verkl\u00e4rt f\u00fcrs H\u00f6chsten Stul.", "tokens": ["So", "steht", "doch", "einst", "der", "Leib", "ver\u00b7kl\u00e4rt", "f\u00fcrs", "H\u00f6chs\u00b7ten", "Stul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Heyden glaubten selbst was G\u00f6ttlichs von der Aschen/", "tokens": ["Die", "Hey\u00b7den", "glaub\u00b7ten", "selbst", "was", "G\u00f6tt\u00b7lichs", "von", "der", "A\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PWS", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bevor das g\u00fcldne Rom hielt seine K\u00e4yser werth;", "tokens": ["Be\u00b7vor", "das", "g\u00fcld\u00b7ne", "Rom", "hielt", "sei\u00b7ne", "K\u00e4y\u00b7ser", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn wenn sie sie genug mit Thr\u00e4nen abgewaschen/", "tokens": ["Denn", "wenn", "sie", "sie", "ge\u00b7nug", "mit", "Thr\u00e4\u00b7nen", "ab\u00b7ge\u00b7wa\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Fast\u2019 ein vergold Geschirr di\u00df was noch unverzehrt.", "tokens": ["Fast'", "ein", "ver\u00b7gold", "Ge\u00b7schirr", "di\u00df", "was", "noch", "un\u00b7ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "PDS", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich will vom G\u00f6tzendienst der neuen Welt nichts sagen/", "tokens": ["Ich", "will", "vom", "G\u00f6t\u00b7zen\u00b7dienst", "der", "neu\u00b7en", "Welt", "nichts", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Di\u00df ist ja wunderns voll was Pegu aufgebracht:", "tokens": ["Di\u00df", "ist", "ja", "wun\u00b7derns", "voll", "was", "Pe\u00b7gu", "auf\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "ADJD", "PWS", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So bald sein Priester wird/ ein Rolim/ hingetragen/", "tokens": ["So", "bald", "sein", "Pries\u00b7ter", "wird", "/", "ein", "Ro\u00b7lim", "/", "hin\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VAFIN", "$(", "ART", "NE", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So ist ein Traur-Gezelt aufs herrlichste gemacht;", "tokens": ["So", "ist", "ein", "Traur\u00b7Ge\u00b7zelt", "aufs", "herr\u00b7lichs\u00b7te", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Leiche steht umzirckt vom K\u00f6nig\u2019 und den Seinen/", "tokens": ["Die", "Lei\u00b7che", "steht", "um\u00b7zirckt", "vom", "K\u00f6\u00b7nig'", "und", "den", "Sei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPRART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bi\u00df den gesalbten Leib die Flamme hat verzehrt/", "tokens": ["Bi\u00df", "den", "ge\u00b7salb\u00b7ten", "Leib", "die", "Flam\u00b7me", "hat", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Alsdenn bey Seiten-Spiel/ und bey der Grepos weinen/", "tokens": ["Als\u00b7denn", "bey", "Sei\u00b7ten\u00b7Spiel", "/", "und", "bey", "der", "Gre\u00b7pos", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$(", "KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hebt die geheiligt\u2019 Asch\u2019 ihr K\u00f6nig auf und schwehrt.", "tokens": ["Hebt", "die", "ge\u00b7hei\u00b7ligt'", "Asch'", "ihr", "K\u00f6\u00b7nig", "auf", "und", "schwehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Man frag\u2019 in China nur die witzigen Bramipen/", "tokens": ["Man", "frag'", "in", "Chi\u00b7na", "nur", "die", "wit\u00b7zi\u00b7gen", "Bra\u00b7mi\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NE", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.34": {"text": "Was vor ein Heiligthum verbrandte C\u00f6rper seyn.", "tokens": ["Was", "vor", "ein", "Hei\u00b7lig\u00b7thum", "ver\u00b7brand\u00b7te", "C\u00f6r\u00b7per", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Fing nicht auch Jsrael dem Moloch an zu dienen/", "tokens": ["Fing", "nicht", "auch", "Js\u00b7rael", "dem", "Mo\u00b7loch", "an", "zu", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "NE", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.36": {"text": "Als in dem Aschen-Thal di\u00df Ungeheur brach ein?", "tokens": ["Als", "in", "dem", "A\u00b7schen\u00b7Thal", "di\u00df", "Un\u00b7ge\u00b7heur", "brach", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PDS", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nun dieser Aschen-dienst gleicht mit der H\u00f6llen Kohlen/", "tokens": ["Nun", "die\u00b7ser", "A\u00b7schen\u00b7dienst", "gleicht", "mit", "der", "H\u00f6l\u00b7len", "Koh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und geht nicht unsern Leib noch die Verwesung an.", "tokens": ["Und", "geht", "nicht", "un\u00b7sern", "Leib", "noch", "die", "Ver\u00b7we\u00b7sung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wir k\u00f6nnen bessern Trost au\u00df erster Kirche holen/", "tokens": ["Wir", "k\u00f6n\u00b7nen", "bes\u00b7sern", "Trost", "au\u00df", "ers\u00b7ter", "Kir\u00b7che", "ho\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da sie mit einem Ku\u00df die Asche beygethan;", "tokens": ["Da", "sie", "mit", "ei\u00b7nem", "Ku\u00df", "die", "A\u00b7sche", "bey\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und diese Pflicht beruht\u2019 auf gutem Angedencken;", "tokens": ["Und", "die\u00b7se", "Pflicht", "be\u00b7ruh