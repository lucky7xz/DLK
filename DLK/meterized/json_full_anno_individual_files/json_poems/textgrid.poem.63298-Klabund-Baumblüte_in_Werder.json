{"textgrid.poem.63298": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Baumbl\u00fcte in Werder", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tante Klara ist schon um ein Uhr mittags besinnungslos betrunken.", "tokens": ["Tan\u00b7te", "Kla\u00b7ra", "ist", "schon", "um", "ein", "Uhr", "mit\u00b7tags", "be\u00b7sin\u00b7nungs\u00b7los", "be\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ihr Satinkleid ist geplatzt. Sie sitzt im m\u00e4rkischen Sand und schluchzt.", "tokens": ["Ihr", "Sa\u00b7tin\u00b7kleid", "ist", "ge\u00b7platzt", ".", "Sie", "sitzt", "im", "m\u00e4r\u00b7ki\u00b7schen", "Sand", "und", "schluchzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der Johannisbeerwein hat's in sich. Alles jubelt und juchzt", "tokens": ["Der", "Jo\u00b7han\u00b7nis\u00b7beer\u00b7wein", "hat's", "in", "sich", ".", "Al\u00b7les", "ju\u00b7belt", "und", "juchzt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PRF", "$.", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Und schwankt wie auf der Havel die wei\u00dfen Dschunken.", "tokens": ["Und", "schwankt", "wie", "auf", "der", "Ha\u00b7vel", "die", "wei\u00b7\u00dfen", "Dschun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "ART", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Waldteufel knarren, und M\u00e4dchenaugen gl\u00fchn.", "tokens": ["Wald\u00b7teu\u00b7fel", "knar\u00b7ren", ",", "und", "M\u00e4d\u00b7chen\u00b7au\u00b7gen", "gl\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "NN", "VVINF", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mutta, Mutta kiek ma die Boombl\u00fcte.", "tokens": ["Mut\u00b7ta", ",", "Mut\u00b7ta", "kiek", "ma", "die", "Boom\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NE", "NE", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ach du liebe G\u00fcte \u2013", "tokens": ["Ach", "du", "lie\u00b7be", "G\u00fc\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die Bl\u00fcten sind alle erfroren. Ein einsamer Kirschbaum versucht zu bl\u00fchn.", "tokens": ["Die", "Bl\u00fc\u00b7ten", "sind", "al\u00b7le", "er\u00b7fro\u00b7ren", ".", "Ein", "ein\u00b7sa\u00b7mer", "Kirschbaum", "ver\u00b7sucht", "zu", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVINF", "$.", "ART", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Eisige Winde wehn. In den Kuten balgt und sielt", "tokens": ["Ei\u00b7si\u00b7ge", "Win\u00b7de", "wehn", ".", "In", "den", "Ku\u00b7ten", "balgt", "und", "sielt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "$.", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sich ein Kinderhaufen. Der Lenz ist da: ert\u00f6nt es von Seele zu Seele.", "tokens": ["Sich", "ein", "Kin\u00b7der\u00b7hau\u00b7fen", ".", "Der", "Lenz", "ist", "da", ":", "er\u00b7t\u00f6nt", "es", "von", "See\u00b7le", "zu", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "$.", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "--+-+--+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ein schon melierter Herr berappt f\u00fcr seine Tele,", "tokens": ["Ein", "schon", "me\u00b7lier\u00b7ter", "Herr", "be\u00b7rappt", "f\u00fcr", "sei\u00b7ne", "Te\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ein Kinderbein f\u00fcr ein Britzer Knoblinchen hielt.", "tokens": ["Die", "ein", "Kin\u00b7der\u00b7bein", "f\u00fcr", "ein", "Brit\u00b7zer", "Kno\u00b7blin\u00b7chen", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Vater spielt auf der Bismarckh\u00f6he mit sich selber Skat und haut", "tokens": ["Va\u00b7ter", "spielt", "auf", "der", "Bis\u00b7marck\u00b7h\u00f6\u00b7he", "mit", "sich", "sel\u00b7ber", "Skat", "und", "haut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "APPR", "PRF", "ADV", "NN", "KON", "VVFIN"], "meter": "+----+-+-+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Alle Tr\u00fcmpfe auf den Tisch, unbeirrt um das Wogen und Treiben der Menge.", "tokens": ["Al\u00b7le", "Tr\u00fcmp\u00b7fe", "auf", "den", "Tisch", ",", "un\u00b7be\u00b7irrt", "um", "das", "Wo\u00b7gen", "und", "Trei\u00b7ben", "der", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+--+--+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Braut und Br\u00e4utigam verlieren sich im Gedr\u00e4nge,", "tokens": ["Braut", "und", "Br\u00e4u\u00b7ti\u00b7gam", "ver\u00b7lie\u00b7ren", "sich", "im", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Ach, wie mancher erwacht am n\u00e4chsten Morgen", "tokens": ["Ach", ",", "wie", "man\u00b7cher", "er\u00b7wacht", "am", "n\u00e4chs\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "mit einer ihm bis dato unbekannten Braut.", "tokens": ["mit", "ei\u00b7ner", "ihm", "bis", "da\u00b7to", "un\u00b7be\u00b7kann\u00b7ten", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mutter Natur, wie gro\u00df ist deiner Erfindungen Pracht!", "tokens": ["Mut\u00b7ter", "Na\u00b7tur", ",", "wie", "gro\u00df", "ist", "dei\u00b7ner", "Er\u00b7fin\u00b7dun\u00b7gen", "Pracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Vor lauter Staub sieht man die Erde nicht.", "tokens": ["Vor", "lau\u00b7ter", "Staub", "sieht", "man", "die", "Er\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tief geladen, mit Klumpen von Menschen beladen, sticht", "tokens": ["Tief", "ge\u00b7la\u00b7den", ",", "mit", "Klum\u00b7pen", "von", "Men\u00b7schen", "be\u00b7la\u00b7den", ",", "sticht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVPP", "$,", "APPR", "NN", "APPR", "NN", "VVPP", "$,", "VVFIN"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein Haveldampfer in See. Schon d\u00e4mmert es. \u00dcber den F\u00f6hren erscheint", "tokens": ["Ein", "Ha\u00b7vel\u00b7damp\u00b7fer", "in", "See", ".", "Schon", "d\u00e4m\u00b7mert", "es", ".", "\u00dc\u00b7ber", "den", "F\u00f6h\u00b7ren", "er\u00b7scheint"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ADV", "VVFIN", "PPER", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+--+--+--+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line