{"dta.poem.11650": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Charis und Penia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Frei, losgebunden ruht ein jedes Glied;", "tokens": ["Frei", ",", "los\u00b7ge\u00b7bun\u00b7den", "ruht", "ein", "je\u00b7des", "Glied", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIZU", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nur bei den Kn\u00f6cheln schmiegen sich die F\u00fc\u00dfe,", "tokens": ["Nur", "bei", "den", "Kn\u00f6\u00b7cheln", "schmie\u00b7gen", "sich", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das rothe Paar der Stiefeln, um einander,", "tokens": ["Das", "ro\u00b7the", "Paar", "der", "Stie\u00b7feln", ",", "um", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "KOUI", "PRF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Bl\u00fcthenknopfe des Granatbaums gleich,", "tokens": ["Dem", "Bl\u00fc\u00b7then\u00b7knop\u00b7fe", "des", "Gra\u00b7nat\u00b7baums", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der eben aufzubrechen Willens ist;", "tokens": ["Der", "e\u00b7ben", "auf\u00b7zu\u00b7bre\u00b7chen", "Wil\u00b7lens", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es scheinen seine F\u00fc\u00dfe wie zum Tanz", "tokens": ["Es", "schei\u00b7nen", "sei\u00b7ne", "F\u00fc\u00b7\u00dfe", "wie", "zum", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In jedem Augenblicke sich zu \u00f6ffnen.", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7bli\u00b7cke", "sich", "zu", "\u00f6ff\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}