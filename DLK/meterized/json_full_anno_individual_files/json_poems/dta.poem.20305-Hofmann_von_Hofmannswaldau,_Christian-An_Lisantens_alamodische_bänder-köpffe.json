{"dta.poem.20305": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Lisantens alamodische  \n b\u00e4nder-k\u00f6pffe", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du \u00e4nderst nur umsonst die bunten b\u00e4nder-k\u00f6pffe/", "tokens": ["Du", "\u00e4n\u00b7derst", "nur", "um\u00b7sonst", "die", "bun\u00b7ten", "b\u00e4n\u00b7der\u00b7k\u00f6pf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lang dein alter kopff noch die ver\u00e4ndrung scheut.", "tokens": ["So", "lang", "dein", "al\u00b7ter", "kopff", "noch", "die", "ver\u00b7\u00e4n\u00b7drung", "scheut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sag mir/ was hilfft dich doch dergleichen eitelkeit?", "tokens": ["Sag", "mir", "/", "was", "hilfft", "dich", "doch", "derg\u00b7lei\u00b7chen", "ei\u00b7tel\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PWS", "VVFIN", "PPER", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was n\u00fctzen deinem haar die diamanten kn\u00f6pffe?", "tokens": ["Was", "n\u00fct\u00b7zen", "dei\u00b7nem", "haar", "die", "di\u00b7a\u00b7man\u00b7ten", "kn\u00f6pf\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was schm\u00fccken deinen kopff die umgeflochtnen z\u00f6pffe?", "tokens": ["Was", "schm\u00fc\u00b7cken", "dei\u00b7nen", "kopff", "die", "um\u00b7ge\u00b7flocht\u00b7nen", "z\u00f6pf\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach w\u00e4rst du doch vielmehr mit tugend angekleidt/", "tokens": ["Ach", "w\u00e4rst", "du", "doch", "viel\u00b7mehr", "mit", "tu\u00b7gend", "an\u00b7ge\u00b7kleidt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und von dergleichen tand und aberwitz befreyt/", "tokens": ["Und", "von", "derg\u00b7lei\u00b7chen", "tand", "und", "a\u00b7ber\u00b7witz", "be\u00b7freyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "KON", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der dich so fest bestrickt/ du th\u00f6richtes gesch\u00f6pffe/", "tokens": ["Der", "dich", "so", "fest", "be\u00b7strickt", "/", "du", "th\u00f6\u00b7rich\u00b7tes", "ge\u00b7sch\u00f6pf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Leg ab den harten kopff/ und setz die sanfftmuth drauff/", "tokens": ["Leg", "ab", "den", "har\u00b7ten", "kopff", "/", "und", "setz", "die", "sanfft\u00b7muth", "drauff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wirff weg die heucheley/ und nimm die warheit auff;", "tokens": ["Wirff", "weg", "die", "heu\u00b7che\u00b7ley", "/", "und", "nimm", "die", "war\u00b7heit", "auff", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$(", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Tausch um den leichten sinn vor ein best\u00e4ndigs wesen/", "tokens": ["Tausch", "um", "den", "leich\u00b7ten", "sinn", "vor", "ein", "be\u00b7st\u00e4n\u00b7digs", "we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verla\u00df den stoltzen geist/ und nimm die demuth an;", "tokens": ["Ver\u00b7la\u00df", "den", "stolt\u00b7zen", "geist", "/", "und", "nimm", "die", "de\u00b7muth", "an", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$(", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df ist der rechte kopff/ den liebt ein jederman.", "tokens": ["Di\u00df", "ist", "der", "rech\u00b7te", "kopff", "/", "den", "liebt", "ein", "je\u00b7der\u00b7man", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "VVFIN", "ART", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "O da\u00df du ihm nicht l\u00e4ngst zu deinem schmuck erlesen!", "tokens": ["O", "da\u00df", "du", "ihm", "nicht", "l\u00e4ngst", "zu", "dei\u00b7nem", "schmuck", "er\u00b7le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}