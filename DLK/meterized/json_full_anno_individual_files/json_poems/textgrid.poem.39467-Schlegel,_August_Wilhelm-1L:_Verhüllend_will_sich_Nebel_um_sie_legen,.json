{"textgrid.poem.39467": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verh\u00fcllend will sich Nebel um sie legen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verh\u00fcllend will sich Nebel um sie legen,", "tokens": ["Ver\u00b7h\u00fcl\u00b7lend", "will", "sich", "Ne\u00b7bel", "um", "sie", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "PRF", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch bleibt vom Nacken nieder zu den Sohlen", "tokens": ["Doch", "bleibt", "vom", "Na\u00b7cken", "nie\u00b7der", "zu", "den", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der zarte Bau der Glieder unverhohlen,", "tokens": ["Der", "zar\u00b7te", "Bau", "der", "Glie\u00b7der", "un\u00b7ver\u00b7hoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Sch\u00f6n'res noch err\u00e4th der Blick verwegen.", "tokens": ["Und", "Sch\u00f6n'\u00b7res", "noch", "er\u00b7r\u00e4\u00b7th", "der", "Blick", "ver\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Entz\u00fccken scheint sich durch sie hin zu regen,", "tokens": ["Ent\u00b7z\u00fc\u00b7cken", "scheint", "sich", "durch", "sie", "