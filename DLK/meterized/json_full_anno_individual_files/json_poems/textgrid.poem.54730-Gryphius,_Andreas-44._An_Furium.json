: "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe dich sehr oft sehn betteln umbher gehn/", "tokens": ["Ich", "ha\u00b7be", "dich", "sehr", "oft", "sehn", "bet\u00b7teln", "um\u00b7bher", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und umb ein St\u00fccke Brod f\u00fcr meiner Th\u00fcren stehn.", "tokens": ["Und", "umb", "ein", "St\u00fc\u00b7cke", "Brod", "f\u00fcr", "mei\u00b7ner", "Th\u00fc\u00b7ren", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bi\u00df dich das freche Weib die ", "tokens": ["Bi\u00df", "dich", "das", "fre\u00b7che", "Weib", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die mit geringer M\u00fch ist zu vil Reichthumb kommen.", "tokens": ["Die", "mit", "ge\u00b7rin\u00b7ger", "M\u00fch", "ist", "zu", "vil", "Reicht\u00b7humb", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als sie der schnelle Tod ( man wei\u00df nicht wie) versehrt;", "tokens": ["Als", "sie", "der", "schnel\u00b7le", "Tod", "(", "man", "wei\u00df", "nicht", "wie", ")", "ver\u00b7sehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "