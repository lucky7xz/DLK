{"dta.poem.79": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als man einem Frauen-Zimmer einge  \n  Galanterien  \u00fcbersandte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dort Cleopatra, das Wunder ihrer Zeit/", "tokens": ["Wenn", "dort", "Cleo\u00b7pat\u00b7ra", ",", "das", "Wun\u00b7der", "ih\u00b7rer", "Zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "$,", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den feurigen ", "tokens": ["Den", "feu\u00b7ri\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und dieser solche Huld mit Dancke will erkennen;", "tokens": ["Und", "die\u00b7ser", "sol\u00b7che", "Huld", "mit", "Dan\u00b7cke", "will", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "PIAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df ein K\u00f6nigreich sie seine G\u00f6ttinn nennen.", "tokens": ["So", "mu\u00df", "ein", "K\u00f6\u00b7nig\u00b7reich", "sie", "sei\u00b7ne", "G\u00f6t\u00b7tinn", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gantz ", "tokens": ["Gantz"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Man siehet \u00fcberall viel tausend Opffer brennen/", "tokens": ["Man", "sie\u00b7het", "\u00fc\u00b7be\u00b7rall", "viel", "tau\u00b7send", "Opf\u00b7fer", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nichts als der Todt soll ihn von ihrer Liebe trennen.", "tokens": ["Nichts", "als", "der", "Todt", "soll", "ihn", "von", "ih\u00b7rer", "Lie\u00b7be", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich/ der ich gleiche Gunst von eurer Huld empfangen/", "tokens": ["Ich", "/", "der", "ich", "glei\u00b7che", "Gunst", "von", "eu\u00b7rer", "Huld", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Will/ ob ich schon nicht kan mit Kron und Scepter prangen/", "tokens": ["Will", "/", "ob", "ich", "schon", "nicht", "kan", "mit", "Kron", "und", "Scep\u00b7ter", "pran\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und kein ", "tokens": ["Und", "kein"], "token_info": ["word", "word"], "pos": ["KON", "PIAT"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Mein Hertze will ich euch zu einem Tempel schencken/", "tokens": ["Mein", "Hert\u00b7ze", "will", "ich", "euch", "zu", "ei\u00b7nem", "Tem\u00b7pel", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So an der Probe Gold/ an Treue Demant Stein/", "tokens": ["So", "an", "der", "Pro\u00b7be", "Gold", "/", "an", "Treu\u00b7e", "De\u00b7mant", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$(", "APPR", "NN", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nebst etwas Weniges zu einem Angedencken.", "tokens": ["Nebst", "et\u00b7was", "We\u00b7ni\u00b7ges", "zu", "ei\u00b7nem", "An\u00b7ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}