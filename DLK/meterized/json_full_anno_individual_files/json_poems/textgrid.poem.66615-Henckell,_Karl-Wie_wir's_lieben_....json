{"textgrid.poem.66615": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wie wir's lieben ...", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon will der liebe Morgenschein,", "tokens": ["Schon", "will", "der", "lie\u00b7be", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "indes die V\u00f6glein singen,", "tokens": ["in\u00b7des", "die", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir in die Kemenat' herein", "tokens": ["Mir", "in", "die", "Ke\u00b7me\u00b7nat'", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit s\u00fc\u00dfem L\u00e4cheln springen.", "tokens": ["mit", "s\u00fc\u00b7\u00dfem", "L\u00e4\u00b7cheln", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der du in diesem stillen Tal", "tokens": ["Der", "du", "in", "die\u00b7sem", "stil\u00b7len", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mich oft geweckt am Morgen,", "tokens": ["mich", "oft", "ge\u00b7weckt", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sei mir gegr\u00fc\u00dft, mein holder Strahl,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "mein", "hol\u00b7der", "Strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "du lichter Tod der Sorgen!", "tokens": ["du", "lich\u00b7ter", "Tod", "der", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Aufdringlich plagt die Finsternu\u00df", "tokens": ["Auf\u00b7dring\u00b7lich", "plagt", "die", "Fins\u00b7ter\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "verworrner Seelenk\u00e4mpfe,", "tokens": ["ver\u00b7worr\u00b7ner", "See\u00b7len\u00b7k\u00e4mp\u00b7fe", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und wie der Qualm Johannen Hu\u00df", "tokens": ["Und", "wie", "der", "Qualm", "Jo\u00b7han\u00b7nen", "Hu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "umbraun mich Nebeld\u00e4mpfe:", "tokens": ["um\u00b7braun", "mich", "Ne\u00b7bel\u00b7d\u00e4mp\u00b7fe", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die gro\u00dfe Eitelkeit der Welt,", "tokens": ["Die", "gro\u00b7\u00dfe", "Ei\u00b7tel\u00b7keit", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "die Roheit, Dummheit, L\u00fcge,", "tokens": ["die", "Ro\u00b7heit", ",", "Dumm\u00b7heit", ",", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und die mich stets am Grips noch h\u00e4lt,", "tokens": ["Und", "die", "mich", "stets", "am", "Grips", "noch", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "der eignen Schwachheit R\u00fcge;", "tokens": ["der", "eig\u00b7nen", "Schwach\u00b7heit", "R\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Der Zweifel an berufner Kraft,", "tokens": ["Der", "Zwei\u00b7fel", "an", "be\u00b7ruf\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mi\u00dftraun in stolze Sendung,", "tokens": ["Mi\u00df\u00b7traun", "in", "stol\u00b7ze", "Sen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die ungestillte Leidenschaft,", "tokens": ["Die", "un\u00b7ge\u00b7still\u00b7te", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "die Sehnsucht nach Vollendung.", "tokens": ["die", "Sehn\u00b7sucht", "nach", "Vol\u00b7len\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.21": {"text": "Ein ganzes B\u00fcndel von Ideen,", "tokens": ["Ein", "gan\u00b7zes", "B\u00fcn\u00b7del", "von", "I\u00b7deen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "ein wunderbarer Krempel,", "tokens": ["ein", "wun\u00b7der\u00b7ba\u00b7rer", "Krem\u00b7pel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ach, k\u00f6nnt' ich aus mir selber gehn,", "tokens": ["Ach", ",", "k\u00f6nnt'", "ich", "aus", "mir", "sel\u00b7ber", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "ich schmiss' ihn aus dem Tempel.", "tokens": ["ich", "schmiss'", "ihn", "aus", "dem", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Doch da ich mal Karl Henckell bin", "tokens": ["Doch", "da", "ich", "mal", "Karl", "Hen\u00b7ckell", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "NE", "NE", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "und leider nicht Hans Meier,", "tokens": ["und", "lei\u00b7der", "nicht", "Hans", "Mei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "So f\u00fchr' ich meinen Extrasinn", "tokens": ["So", "f\u00fchr'", "ich", "mei\u00b7nen", "Ext\u00b7ra\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "und meine Extraleier.", "tokens": ["und", "mei\u00b7ne", "Ext\u00b7ra\u00b7lei\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "So leb' ich in den Tag hinein", "tokens": ["So", "leb'", "ich", "in", "den", "Tag", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "und liebe gute Leute", "tokens": ["und", "lie\u00b7be", "gu\u00b7te", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Und setze \u00fcber Stock und Stein,", "tokens": ["Und", "set\u00b7ze", "\u00fc\u00b7ber", "Stock", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "bellt hinterdrein die Meute.", "tokens": ["bellt", "hin\u00b7ter\u00b7drein", "die", "Meu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Herrn Drill und S\u00f6hne lieb' ich nicht,", "tokens": ["Herrn", "Drill", "und", "S\u00f6h\u00b7ne", "lieb'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "der Kaiser ist mir schnuppe,", "tokens": ["der", "Kai\u00b7ser", "ist", "mir", "schnup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Des tollsten Rackers Angesicht", "tokens": ["Des", "tolls\u00b7ten", "Ra\u00b7ckers", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "ist sch\u00f6ner als 'ne Puppe.", "tokens": ["ist", "sch\u00f6\u00b7ner", "als", "'ne", "Pup\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Ja, lieber ist mir noch der Lump,", "tokens": ["Ja", ",", "lie\u00b7ber", "ist", "mir", "noch", "der", "Lump", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "verreckend hinterm Zaune,", "tokens": ["ver\u00b7re\u00b7ckend", "hin\u00b7term", "Zau\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Der Kunde lebt auf Schicksals Pump", "tokens": ["Der", "Kun\u00b7de", "lebt", "auf", "Schick\u00b7sals", "Pump"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "und tanzt nach seiner Laune,", "tokens": ["und", "tanzt", "nach", "sei\u00b7ner", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Und wenn du recht nat\u00fcrlich hopst,", "tokens": ["Und", "wenn", "du", "recht", "na\u00b7t\u00fcr\u00b7lich", "hopst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "bist du mir zehnmal lieber,", "tokens": ["bist", "du", "mir", "zehn\u00b7mal", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Als wenn du dich in Fischbein stoppst,", "tokens": ["Als", "wenn", "du", "dich", "in", "Fischbein", "stoppst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "das Zwangskorsett dar\u00fcber ...", "tokens": ["das", "Zwangs\u00b7kor\u00b7sett", "da\u00b7r\u00fc\u00b7ber", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Das Leben wird vom Tod erstickt,", "tokens": ["Das", "Le\u00b7ben", "wird", "vom", "Tod", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "drein wir uns selber schn\u00fcren,", "tokens": ["drein", "wir", "uns", "sel\u00b7ber", "schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.47": {"text": "Moral, sie heuchelt ihr Verdikt \u2013", "tokens": ["Mo\u00b7ral", ",", "sie", "heu\u00b7chelt", "ihr", "Ver\u00b7dikt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "nur ja nicht daran r\u00fchren!", "tokens": ["nur", "ja", "nicht", "da\u00b7ran", "r\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schon will der liebe Morgenschein,", "tokens": ["Schon", "will", "der", "lie\u00b7be", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "indes die V\u00f6glein singen,", "tokens": ["in\u00b7des", "die", "V\u00f6\u00b7glein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir in die Kemenat' herein", "tokens": ["Mir", "in", "die", "Ke\u00b7me\u00b7nat'", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit s\u00fc\u00dfem L\u00e4cheln springen.", "tokens": ["mit", "s\u00fc\u00b7\u00dfem", "L\u00e4\u00b7cheln", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der du in diesem stillen Tal", "tokens": ["Der", "du", "in", "die\u00b7sem", "stil\u00b7len", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PDAT", "ADJA", "NN"], "m