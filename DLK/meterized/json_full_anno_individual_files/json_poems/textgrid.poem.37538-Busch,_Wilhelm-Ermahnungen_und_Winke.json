{"textgrid.poem.37538": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ermahnungen und Winke", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie lieblich, o wie schicklich,", "tokens": ["O", "wie", "lieb\u00b7lich", ",", "o", "wie", "schick\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$,", "FM", "KOKOM", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sozusagen herzerquicklich,", "tokens": ["So\u00b7zu\u00b7sa\u00b7gen", "herz\u00b7er\u00b7quick\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es doch f\u00fcr eine Gegend,", "tokens": ["Ist", "es", "doch", "f\u00fcr", "ei\u00b7ne", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn zwei Leute, die verm\u00f6gend,", "tokens": ["Wenn", "zwei", "Leu\u00b7te", ",", "die", "ver\u00b7m\u00f6\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Au\u00dferdem mit sich zufrieden,", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "mit", "sich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PRF", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Aber von Geschlecht verschieden,", "tokens": ["A\u00b7ber", "von", "Ge\u00b7schlecht", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn nun diese, sag ich, ihre", "tokens": ["Wenn", "nun", "die\u00b7se", ",", "sag", "ich", ",", "ih\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "PDAT", "$,", "VVFIN", "PPER", "$,", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dazu n\u00f6tigen Papiere", "tokens": ["Da\u00b7zu", "n\u00f6\u00b7ti\u00b7gen", "Pa\u00b7pie\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So wie auch die Haushaltsachen", "tokens": ["So", "wie", "auch", "die", "Haus\u00b7halt\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Endlich mal in Ordnung machen", "tokens": ["End\u00b7lich", "mal", "in", "Ord\u00b7nung", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und in Ehren und beizeiten", "tokens": ["Und", "in", "Eh\u00b7ren", "und", "bei\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hin zum Standesamte schreiten,", "tokens": ["Hin", "zum", "Stan\u00b7des\u00b7am\u00b7te", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie es denen, welche lieben,", "tokens": ["Wie", "es", "de\u00b7nen", ",", "wel\u00b7che", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Vom Gesetze vorgeschrieben,", "tokens": ["Vom", "Ge\u00b7set\u00b7ze", "vor\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Dann ruft jeder freudiglich:", "tokens": ["Dann", "ruft", "je\u00b7der", "freu\u00b7dig\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "\u00bbgott sei Dank, sie haben sich!\u00ab", "tokens": ["\u00bb", "gott", "sei", "Dank", ",", "sie", "ha\u00b7ben", "sich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "NN", "$,", "PPER", "VAFIN", "PRF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Da\u00df es hierzu aber endlich", "tokens": ["Da\u00df", "es", "hier\u00b7zu", "a\u00b7ber", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Kommen mu\u00df, ist selbstverst\u00e4ndlich. \u2013", "tokens": ["Kom\u00b7men", "mu\u00df", ",", "ist", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Oder liebt man Pf\u00e4nderspiele?", "tokens": ["O\u00b7der", "liebt", "man", "Pf\u00e4n\u00b7der\u00b7spie\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "So was l\u00e4\u00dft den Weisen k\u00fchle.", "tokens": ["So", "was", "l\u00e4\u00dft", "den", "Wei\u00b7sen", "k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Oder sch\u00e4tzt man Tanz und Reigen?", "tokens": ["O\u00b7der", "sch\u00e4tzt", "man", "Tanz", "und", "Rei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Von Symbolen la\u00dft uns schweigen.", "tokens": ["Von", "Sym\u00b7bo\u00b7len", "la\u00dft", "uns", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Oder will man unter Rosen", "tokens": ["O\u00b7der", "will", "man", "un\u00b7ter", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Innig miteinander kosen? \u2013", "tokens": ["In\u00b7nig", "mi\u00b7tein\u00b7an\u00b7der", "ko\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Dies hat freilich seinen Reiz;", "tokens": ["Dies", "hat", "frei\u00b7lich", "sei\u00b7nen", "Reiz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Aber elterlicherseits", "tokens": ["A\u00b7ber", "el\u00b7ter\u00b7li\u00b7cher\u00b7seits"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "St\u00f6\u00dft man leicht auf so gewisse", "tokens": ["St\u00f6\u00dft", "man", "leicht", "auf", "so", "ge\u00b7wis\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "ADV", "ADJA"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "Unbequeme Hindernisse,", "tokens": ["Un\u00b7be\u00b7que\u00b7me", "Hin\u00b7der\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Und man hat, um sie zu heben,", "tokens": ["Und", "man", "hat", ",", "um", "sie", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Als verlobt sich kundzugeben. \u2013", "tokens": ["Als", "ver\u00b7lobt", "sich", "kund\u00b7zu\u00b7ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Das ist allerdings was Sch\u00f6nes;", "tokens": ["Das", "ist", "al\u00b7ler\u00b7dings", "was", "Sch\u00f6\u00b7nes", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PWS", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Dennoch mangelt dies und jenes.", "tokens": ["Den\u00b7noch", "man\u00b7gelt", "dies", "und", "je\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "KON", "PDS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Traulich im Familienkreise", "tokens": ["Trau\u00b7lich", "im", "Fa\u00b7mi\u00b7li\u00b7en\u00b7krei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Sitzt man da und fl\u00fcstert leise,", "tokens": ["Sitzt", "man", "da", "und", "fl\u00fcs\u00b7tert", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Dr\u00fcckt die Daumen, k\u00fc\u00dft und plaudert,", "tokens": ["Dr\u00fcckt", "die", "Dau\u00b7men", ",", "k\u00fc\u00dft", "und", "plau\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Zehne schl\u00e4gt's, indes man zaudert,", "tokens": ["Zeh\u00b7ne", "schl\u00e4gt's", ",", "in\u00b7des", "man", "zau\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Mutter strickt und Vater g\u00e4hnt,", "tokens": ["Mut\u00b7ter", "strickt", "und", "Va\u00b7ter", "g\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Und, eh man was B\u00f6ses w\u00e4hnt,", "tokens": ["Und", ",", "eh", "man", "was", "B\u00f6\u00b7ses", "w\u00e4hnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PWS", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Hei\u00dft es: \u00bbGute Nacht, bis morgen!\u00ab \u2013", "tokens": ["Hei\u00dft", "es", ":", "\u00bb", "Gu\u00b7te", "Nacht", ",", "bis", "mor\u00b7gen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "$,", "ADV", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Tief im Paletot verborgen,", "tokens": ["Tief", "im", "Pa\u00b7le\u00b7tot", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Durch die schwarzen, nassen Gassen,", "tokens": ["Durch", "die", "schwar\u00b7zen", ",", "nas\u00b7sen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Die fast jeder Mensch verlassen,", "tokens": ["Die", "fast", "je\u00b7der", "Mensch", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Strebt man unmutsvoll nach Hause", "tokens": ["Strebt", "man", "un\u00b7muts\u00b7voll", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "In die alte, kalte Klause,", "tokens": ["In", "die", "al\u00b7te", ",", "kal\u00b7te", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "W\u00fchlt ins Bett sich tief und tiefer,", "tokens": ["W\u00fchlt", "ins", "Bett", "sich", "tief", "und", "tie\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Schnatteratt! so macht der Kiefer,", "tokens": ["Schnat\u00b7te\u00b7ratt", "!", "so", "macht", "der", "Kie\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Und so etwa gegen eine", "tokens": ["Und", "so", "et\u00b7wa", "ge\u00b7gen", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.48": {"text": "Kriegt man endlich warme Beine.", "tokens": ["Kriegt", "man", "end\u00b7lich", "war\u00b7me", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Kurz, Verstand sowie Empfindung", "tokens": ["Kurz", ",", "Ver\u00b7stand", "so\u00b7wie", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Dringt auf ehliche Verbindung. \u2013", "tokens": ["Dringt", "auf", "eh\u00b7li\u00b7che", "Ver\u00b7bin\u00b7dung", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Dann wird's aber auch gem\u00fctlich.", "tokens": ["Dann", "wird's", "a\u00b7ber", "auch", "ge\u00b7m\u00fct\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "T\u00e4glich, st\u00fcndlich und min\u00fctlich", "tokens": ["T\u00e4g\u00b7lich", ",", "st\u00fcnd\u00b7lich", "und", "mi\u00b7n\u00fct\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Darf man nun vereint zu zween", "tokens": ["Darf", "man", "nun", "ver\u00b7eint", "zu", "zween"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "VVPP", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Arm in Arm spazierengehn!", "tokens": ["Arm", "in", "Arm", "spa\u00b7zie\u00b7ren\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Ja, was irgend sch\u00f6n und lieblich,", "tokens": ["Ja", ",", "was", "ir\u00b7gend", "sch\u00f6n", "und", "lieb\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Segensreich und landes\u00fcblich", "tokens": ["Se\u00b7gens\u00b7reich", "und", "lan\u00b7des\u00b7\u00fcb\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Und ein gutes Herz ergetzt,", "tokens": ["Und", "ein", "gu\u00b7tes", "Herz", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Pr\u00fcft, erf\u00e4hrt und hat man jetzt.", "tokens": ["Pr\u00fcft", ",", "er\u00b7f\u00e4hrt", "und", "hat", "man", "jetzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VAFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O wie lieblich, o wie schicklich,", "tokens": ["O", "wie", "lieb\u00b7lich", ",", "o", "wie", "schick\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$,", "FM", "KOKOM", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sozusagen herzerquicklich,", "tokens": ["So\u00b7zu\u00b7sa\u00b7gen", "herz\u00b7er\u00b7quick\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es doch f\u00fcr eine Gegend,", "tokens": ["Ist", "es", "doch", "f\u00fcr", "ei\u00b7ne", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn zwei Leute, die verm\u00f6gend,", "tokens": ["Wenn", "zwei", "Leu\u00b7te", ",", "die", "ver\u00b7m\u00f6\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Au\u00dferdem mit sich zufrieden,", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "mit", "sich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PRF", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Aber von Geschlecht verschieden,", "tokens": ["A\u00b7ber", "von", "Ge\u00b7schlecht", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn nun diese, sag ich, ihre", "tokens": ["Wenn", "nun", "die\u00b7se", ",", "sag", "ich", ",", "ih\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "PDAT", "$,", "VVFIN", "PPER", "$,", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dazu n\u00f6tigen Papiere", "tokens": ["Da\u00b7zu", "n\u00f6\u00b7ti\u00b7gen", "Pa\u00b7pie\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So wie auch die Haushaltsachen", "tokens": ["So", "wie", "auch", "die", "Haus\u00b7halt\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Endlich mal in Ordnung machen", "tokens": ["End\u00b7lich", "mal", "in", "Ord\u00b7nung", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und in Ehren und beizeiten", "tokens": ["Und", "in", "Eh\u00b7ren", "und", "bei\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hin zum Standesamte schreiten,", "tokens": ["Hin", "zum", "Stan\u00b7des\u00b7am\u00b7te", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie es denen, welche lieben,", "tokens": ["Wie", "es", "de\u00b7nen", ",", "wel\u00b7che", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Vom Gesetze vorgeschrieben,", "tokens": ["Vom", "Ge\u00b7set\u00b7ze", "vor\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Dann ruft jeder freudiglich:", "tokens": ["Dann", "ruft", "je\u00b7der", "freu\u00b7dig\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "\u00bbgott sei Dank, sie haben sich!\u00ab", "tokens": ["\u00bb", "gott", "sei", "Dank", ",", "sie", "ha\u00b7ben", "sich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "NN", "$,", "PPER", "VAFIN", "PRF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Da\u00df es hierzu aber endlich", "tokens": ["Da\u00df", "es", "hier\u00b7zu", "a\u00b7ber", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Kommen mu\u00df, ist selbstverst\u00e4ndlich. \u2013", "tokens": ["Kom\u00b7men", "mu\u00df", ",", "ist", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Oder liebt man Pf\u00e4nderspiele?", "tokens": ["O\u00b7der", "liebt", "man", "Pf\u00e4n\u00b7der\u00b7spie\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "So was l\u00e4\u00dft den Weisen k\u00fchle.", "tokens": ["So", "was", "l\u00e4\u00dft", "den", "Wei\u00b7sen", "k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Oder sch\u00e4tzt man Tanz und Reigen?", "tokens": ["O\u00b7der", "sch\u00e4tzt", "man", "Tanz", "und", "Rei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Von Symbolen la\u00dft uns schweigen.", "tokens": ["Von", "Sym\u00b7bo\u00b7len", "la\u00dft", "uns", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Oder will man unter Rosen", "tokens": ["O\u00b7der", "will", "man", "un\u00b7ter", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Innig miteinander kosen? \u2013", "tokens": ["In\u00b7nig", "mi\u00b7tein\u00b7an\u00b7der", "ko\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Dies hat freilich seinen Reiz;", "tokens": ["Dies", "hat", "frei\u00b7lich", "sei\u00b7nen", "Reiz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Aber elterlicherseits", "tokens": ["A\u00b7ber", "el\u00b7ter\u00b7li\u00b7cher\u00b7seits"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "St\u00f6\u00dft man leicht auf so gewisse", "tokens": ["St\u00f6\u00dft", "man", "leicht", "auf", "so", "ge\u00b7wis\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "ADV", "ADJA"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "Unbequeme Hindernisse,", "tokens": ["Un\u00b7be\u00b7que\u00b7me", "Hin\u00b7der\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Und man hat, um sie zu heben,", "tokens": ["Und", "man", "hat", ",", "um", "sie", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Als verlobt sich kundzugeben. \u2013", "tokens": ["Als", "ver\u00b7lobt", "sich", "kund\u00b7zu\u00b7ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Das ist allerdings was Sch\u00f6nes;", "tokens": ["Das", "ist", "al\u00b7ler\u00b7dings", "was", "Sch\u00f6\u00b7nes", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PWS", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Dennoch mangelt dies und jenes.", "tokens": ["Den\u00b7noch", "man\u00b7gelt", "dies", "und", "je\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "KON", "PDS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Traulich im Familienkreise", "tokens": ["Trau\u00b7lich", "im", "Fa\u00b7mi\u00b7li\u00b7en\u00b7krei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Sitzt man da und fl\u00fcstert leise,", "tokens": ["Sitzt", "man", "da", "und", "fl\u00fcs\u00b7tert", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Dr\u00fcckt die Daumen, k\u00fc\u00dft und plaudert,", "tokens": ["Dr\u00fcckt", "die", "Dau\u00b7men", ",", "k\u00fc\u00dft", "und", "plau\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Zehne schl\u00e4gt's, indes man zaudert,", "tokens": ["Zeh\u00b7ne", "schl\u00e4gt's", ",", "in\u00b7des", "man", "zau\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Mutter strickt und Vater g\u00e4hnt,", "tokens": ["Mut\u00b7ter", "strickt", "und", "Va\u00b7ter", "g\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Und, eh man was B\u00f6ses w\u00e4hnt,", "tokens": ["Und", ",", "eh", "man", "was", "B\u00f6\u00b7ses", "w\u00e4hnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PWS", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Hei\u00dft es: \u00bbGute Nacht, bis morgen!\u00ab \u2013", "tokens": ["Hei\u00dft", "es", ":", "\u00bb", "Gu\u00b7te", "Nacht", ",", "bis", "mor\u00b7gen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "$,", "ADV", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Tief im Paletot verborgen,", "tokens": ["Tief", "im", "Pa\u00b7le\u00b7tot", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Durch die schwarzen, nassen Gassen,", "tokens": ["Durch", "die", "schwar\u00b7zen", ",", "nas\u00b7sen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Die fast jeder Mensch verlassen,", "tokens": ["Die", "fast", "je\u00b7der", "Mensch", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Strebt man unmutsvoll nach Hause", "tokens": ["Strebt", "man", "un\u00b7muts\u00b7voll", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "In die alte, kalte Klause,", "tokens": ["In", "die", "al\u00b7te", ",", "kal\u00b7te", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "W\u00fchlt ins Bett sich tief und tiefer,", "tokens": ["W\u00fchlt", "ins", "Bett", "sich", "tief", "und", "tie\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Schnatteratt! so macht der Kiefer,", "tokens": ["Schnat\u00b7te\u00b7ratt", "!", "so", "macht", "der", "Kie\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Und so etwa gegen eine", "tokens": ["Und", "so", "et\u00b7wa", "ge\u00b7gen", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ART"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.48": {"text": "Kriegt man endlich warme Beine.", "tokens": ["Kriegt", "man", "end\u00b7lich", "war\u00b7me", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Kurz, Verstand sowie Empfindung", "tokens": ["Kurz", ",", "Ver\u00b7stand", "so\u00b7wie", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Dringt auf ehliche Verbindung. \u2013", "tokens": ["Dringt", "auf", "eh\u00b7li\u00b7che", "Ver\u00b7bin\u00b7dung", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Dann wird's aber auch gem\u00fctlich.", "tokens": ["Dann", "wird's", "a\u00b7ber", "auch", "ge\u00b7m\u00fct\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "T\u00e4glich, st\u00fcndlich und min\u00fctlich", "tokens": ["T\u00e4g\u00b7lich", ",", "st\u00fcnd\u00b7lich", "und", "mi\u00b7n\u00fct\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Darf man nun vereint zu zween", "tokens": ["Darf", "man", "nun", "ver\u00b7eint", "zu", "zween"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "VVPP", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Arm in Arm spazierengehn!", "tokens": ["Arm", "in", "Arm", "spa\u00b7zie\u00b7ren\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Ja, was irgend sch\u00f6n und lieblich,", "tokens": ["Ja", ",", "was", "ir\u00b7gend", "sch\u00f6n", "und", "lieb\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Segensreich und landes\u00fcblich", "tokens": ["Se\u00b7gens\u00b7reich", "und", "lan\u00b7des\u00b7\u00fcb\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Und ein gutes Herz ergetzt,", "tokens": ["Und", "ein", "gu\u00b7tes", "Herz", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Pr\u00fcft, erf\u00e4hrt und hat man jetzt.", "tokens": ["Pr\u00fcft", ",", "er\u00b7f\u00e4hrt", "und", "hat", "man", "jetzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VAFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}