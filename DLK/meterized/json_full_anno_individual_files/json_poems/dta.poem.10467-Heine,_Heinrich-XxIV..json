{"dta.poem.10467": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ungl\u00fccksel'ger Atlas! eine Welt,", "tokens": ["Ich", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ger", "At\u00b7las", "!", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ganze Welt der Schmerzen mu\u00df ich tragen,", "tokens": ["Die", "gan\u00b7ze", "Welt", "der", "Schmer\u00b7zen", "mu\u00df", "ich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich trage Unertr\u00e4gliches, und brechen", "tokens": ["Ich", "tra\u00b7ge", "Un\u00b7er\u00b7tr\u00e4g\u00b7li\u00b7ches", ",", "und", "bre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Will mir das Herz im Leibe.", "tokens": ["Will", "mir", "das", "Herz", "im", "Lei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du stolzes Herz! du hast es ja gewollt,", "tokens": ["Du", "stol\u00b7zes", "Herz", "!", "du", "hast", "es", "ja", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VMPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wolltest gl\u00fccklich seyn, unendlich gl\u00fccklich", "tokens": ["Du", "woll\u00b7test", "gl\u00fcck\u00b7lich", "seyn", ",", "un\u00b7end\u00b7lich", "gl\u00fcck\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oder unendlich elend, stolzes Herz,", "tokens": ["O\u00b7der", "un\u00b7end\u00b7lich", "e\u00b7lend", ",", "stol\u00b7zes", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jetzo bist du elend.", "tokens": ["Und", "jet\u00b7zo", "bist", "du", "e\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}