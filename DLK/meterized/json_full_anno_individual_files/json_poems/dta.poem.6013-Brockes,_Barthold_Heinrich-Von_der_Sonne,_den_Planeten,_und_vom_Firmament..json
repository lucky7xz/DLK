{"dta.poem.6013": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch Plato, den man insgemein", "tokens": ["Auch", "Pla\u00b7to", ",", "den", "man", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NE", "$,", "PRELS", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den G\u00f6ttlichen benahmt, stimmt hiermit \u00fcberein.", "tokens": ["Den", "G\u00f6tt\u00b7li\u00b7chen", "be\u00b7nahmt", ",", "stimmt", "hier\u00b7mit", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Wie er, bevor er starb, die Ordnung dieser Welt", "tokens": ["Wie", "er", ",", "be\u00b7vor", "er", "starb", ",", "die", "Ord\u00b7nung", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch einmahl \u00fcbersah, erkannt\u2019 er diese Wahrheit.", "tokens": ["Noch", "ein\u00b7mahl", "\u00fc\u00b7ber\u00b7sah", ",", "er\u00b7kannt'", "er", "die\u00b7se", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er folgt Pythagoras und seiner Lehre Klarheit.", "tokens": ["Er", "folgt", "Py\u00b7tha\u00b7go\u00b7ras", "und", "sei\u00b7ner", "Leh\u00b7re", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der regen Erde Lauff blieb bey ihm fest gestellt.", "tokens": ["Der", "re\u00b7gen", "Er\u00b7de", "Lauff", "blieb", "bey", "ihm", "fest", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}