os": ["PRF", "APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,", "KOUS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er schon mit Riesenkraft die St\u00e4rkere bezwungen,", "tokens": ["Er", "schon", "mit", "Rie\u00b7sen\u00b7kraft", "die", "St\u00e4r\u00b7ke\u00b7re", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es freut die Mutter sich und f\u00fchlt", "tokens": ["Es", "freut", "die", "Mut\u00b7ter", "sich", "und", "f\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In ihres Sohnes Lust sich doppelt s\u00fc\u00df durchdrungen \u2013:", "tokens": ["In", "ih\u00b7res", "Soh\u00b7nes", "Lust", "sich", "dop\u00b7pelt", "s\u00fc\u00df", "durch\u00b7drun\u00b7gen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PRF", "ADJD", "ADJD", "VVPP", "$(", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So stand vor dir einst, gro\u00dfe Here,", "tokens": ["So", "stand", "vor", "dir", "einst", ",", "gro\u00b7\u00dfe", "He\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In sel'gen Anblick tief entz\u00fcckt,", "tokens": ["In", "sel'\u00b7gen", "An\u00b7blick", "tief", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die himmlisch l\u00e4chelnd Cythere,", "tokens": ["Die", "himm\u00b7lisch", "l\u00e4\u00b7chelnd", "Cy\u00b7the\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.12": {"text": "Da sie mit ihrem Zauberg\u00fcrtel dich geschm\u00fcckt", "tokens": ["Da", "sie", "mit", "ih\u00b7rem", "Zau\u00b7ber\u00b7g\u00fcr\u00b7tel", "dich", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zum Wunderbild f\u00fcr Erd' und Meere,", "tokens": ["Zum", "Wun\u00b7der\u00b7bild", "f\u00fcr", "Erd'", "und", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zur Sch\u00f6nsten, die Olympus je erblickt!", "tokens": ["Zur", "Sch\u00f6ns\u00b7ten", ",", "die", "O\u00b7lym\u00b7pus", "je", "er\u00b7blickt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sie h\u00e4ngt an dich das Wonnesiegel", "tokens": ["Sie", "h\u00e4ngt", "an", "dich", "das", "Won\u00b7ne\u00b7sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "All' ihrer Reize, allen Glanz,", "tokens": ["All'", "ih\u00b7rer", "Rei\u00b7ze", ",", "al\u00b7len", "Glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"