], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der mir sonst die \u00f6de Welt verkl\u00e4rt.", "tokens": ["Der", "mir", "sonst", "die", "\u00f6\u00b7de", "Welt", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach jetzt d\u00fcnkt sie leer mir und verblichen,", "tokens": ["Ach", "jetzt", "d\u00fcnkt", "sie", "leer", "mir", "und", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht mehr ist sie meiner W\u00fcnsche werth.", "tokens": ["Nicht", "mehr", "ist", "sie", "mei\u00b7ner", "W\u00fcn\u00b7sche", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Trennung rief in eine weite Ferne", "tokens": ["Tren\u00b7nung", "rief", "in", "ei\u00b7ne", "wei\u00b7te", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Meines Lebens einz'ges, h\u00f6chstes Gl\u00fcck,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "einz'\u00b7ges", ",", "h\u00f6chs\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Seitdem traten meiner Hoffnung Sterne", "tokens": ["Seit\u00b7dem", "tra\u00b7ten", "mei\u00b7ner", "Hoff\u00b7nung",