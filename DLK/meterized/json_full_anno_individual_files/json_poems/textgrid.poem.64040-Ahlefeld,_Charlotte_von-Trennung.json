{"textgrid.poem.64040": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Trennung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maienl\u00fcste wehen durch die Haine,", "tokens": ["Mai\u00b7en\u00b7l\u00fcs\u00b7te", "we\u00b7hen", "durch", "die", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bl\u00fcthen brechen aus dem Keim hervor;", "tokens": ["Bl\u00fc\u00b7then", "bre\u00b7chen", "aus", "dem", "Keim", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In der Sonne m\u00fctterlichem Scheine", "tokens": ["In", "der", "Son\u00b7ne", "m\u00fct\u00b7ter\u00b7li\u00b7chem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Richtet sich die Pflanzenwelt empor.", "tokens": ["Rich\u00b7tet", "sich", "die", "Pflan\u00b7zen\u00b7welt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "V\u00f6gel singen in den gr\u00fcnen Zweigen,", "tokens": ["V\u00f6\u00b7gel", "sin\u00b7gen", "in", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "K\u00e4fer schweben freudig hin und her \u2013", "tokens": ["K\u00e4\u00b7fer", "schwe\u00b7ben", "freu\u00b7dig", "hin", "und", "her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "KON", "ADV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Doch aus mir will nicht der Winter weichen,", "tokens": ["Doch", "aus", "mir", "will", "nicht", "der", "Win\u00b7ter", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und das Herz ist mir erstarrt und schwer.", "tokens": ["Und", "das", "Herz", "ist", "mir", "er\u00b7starrt", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sonst begr\u00fcsst' ich gern das rege Leben,", "tokens": ["Sonst", "be\u00b7gr\u00fcsst'", "ich", "gern", "das", "re\u00b7ge", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das im Lenz sich jugendlich erneut,", "tokens": ["Das", "im", "Lenz", "sich", "ju\u00b7gend\u00b7lich", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PRF", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit ahnungsvollem, s\u00fcssem Beben,", "tokens": ["Und", "mit", "ah\u00b7nungs\u00b7vol\u00b7lem", ",", "s\u00fcs\u00b7sem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcllte mich des Jahres Erstlingszeit.", "tokens": ["F\u00fcll\u00b7te", "mich", "des", "Jah\u00b7res", "Erst\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber jetzt verdunklen bittre Thr\u00e4nen", "tokens": ["A\u00b7ber", "jetzt", "ver\u00b7dunk\u00b7len", "bitt\u00b7re", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mir die frische, l\u00e4chelnde Natur,", "tokens": ["Mir", "die", "fri\u00b7sche", ",", "l\u00e4\u00b7cheln\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und des Busens ungestilltes Sehnen,", "tokens": ["Und", "des", "Bu\u00b7sens", "un\u00b7ge\u00b7still\u00b7tes", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zeigt mir Bilder dumpfer Schwermuth nur.", "tokens": ["Zeigt", "mir", "Bil\u00b7der", "dum\u00b7pfer", "Schwer\u00b7muth", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Denn der Zauber ist von mir gewichen,", "tokens": ["Denn", "der", "Zau\u00b7ber", "ist", "von", "mir", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der mir sonst die \u00f6de Welt verkl\u00e4rt.", "tokens": ["Der", "mir", "sonst", "die", "\u00f6\u00b7de", "Welt", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach jetzt d\u00fcnkt sie leer mir und verblichen,", "tokens": ["Ach", "jetzt", "d\u00fcnkt", "sie", "leer", "mir", "und", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht mehr ist sie meiner W\u00fcnsche werth.", "tokens": ["Nicht", "mehr", "ist", "sie", "mei\u00b7ner", "W\u00fcn\u00b7sche", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Trennung rief in eine weite Ferne", "tokens": ["Tren\u00b7nung", "rief", "in", "ei\u00b7ne", "wei\u00b7te", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Meines Lebens einz'ges, h\u00f6chstes Gl\u00fcck,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "einz'\u00b7ges", ",", "h\u00f6chs\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Seitdem traten meiner Hoffnung Sterne", "tokens": ["Seit\u00b7dem", "tra\u00b7ten", "mei\u00b7ner", "Hoff\u00b7nung", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In des Kummers finstre Nacht zur\u00fcck.", "tokens": ["In", "des", "Kum\u00b7mers", "finst\u00b7re", "Nacht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn uns schwere Tr\u00e4ume oft umfangen,", "tokens": ["Wenn", "uns", "schwe\u00b7re", "Tr\u00e4u\u00b7me", "oft", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Muth und Kraft uns l\u00e4hmend in der Brust,", "tokens": ["Muth", "und", "Kraft", "uns", "l\u00e4h\u00b7mend", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Stillt der Morgen das ertr\u00e4umte Bangen", "tokens": ["Stillt", "der", "Mor\u00b7gen", "das", "er\u00b7tr\u00e4um\u00b7te", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit des fr\u00f6hlichen Erwachens Lust;", "tokens": ["Mit", "des", "fr\u00f6h\u00b7li\u00b7chen", "Er\u00b7wa\u00b7chens", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und wir l\u00e4cheln \u00fcber die Gefahren,", "tokens": ["Und", "wir", "l\u00e4\u00b7cheln", "\u00fc\u00b7ber", "die", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aufgeth\u00fcrmt von schwarzer Fantasie.", "tokens": ["Auf\u00b7ge\u00b7th\u00fcrmt", "von", "schwar\u00b7zer", "Fan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Alle Schrecken, welche wir erfahren,", "tokens": ["Al\u00b7le", "Schre\u00b7cken", ",", "wel\u00b7che", "wir", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Fliehn vor der Besinnung Harmonie.", "tokens": ["Fliehn", "vor", "der", "Be\u00b7sin\u00b7nung", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "M\u00f6cht' auch mir dereinst der Morgen gl\u00fchen,", "tokens": ["M\u00f6cht'", "auch", "mir", "de\u00b7reinst", "der", "Mor\u00b7gen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der mich weckt aus \u00f6der Trennung Traum.", "tokens": ["Der", "mich", "weckt", "aus", "\u00f6\u00b7der", "Tren\u00b7nung", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann erst wird der Fr\u00fchling mir erbl\u00fchen,", "tokens": ["Dann", "erst", "wird", "der", "Fr\u00fch\u00b7ling", "mir", "er\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Mild und sonnig in des Daseyns Raum.", "tokens": ["Mild", "und", "son\u00b7nig", "in", "des", "Da\u00b7seyns", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Doch bis dahin h\u00fcllt ein matter Schleier", "tokens": ["Doch", "bis", "da\u00b7hin", "h\u00fcllt", "ein", "mat\u00b7ter", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Jeden Reiz der Erde f\u00fcr mich ein,", "tokens": ["Je\u00b7den", "Reiz", "der", "Er\u00b7de", "f\u00fcr", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und es strahlt mir nur des Lenzes Feier", "tokens": ["Und", "es", "strahlt", "mir", "nur", "des", "Len\u00b7zes", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In des Wiedersehens Himmelsschein.", "tokens": ["In", "des", "Wie\u00b7der\u00b7se\u00b7hens", "Him\u00b7mels\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Maienl\u00fcste wehen durch die Haine,", "tokens": ["Mai\u00b7en\u00b7l\u00fcs\u00b7te", "we\u00b7hen", "durch", "die", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bl\u00fcthen brechen aus dem Keim hervor;", "tokens": ["Bl\u00fc\u00b7then", "bre\u00b7chen", "aus", "dem", "Keim", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In der Sonne m\u00fctterlichem Scheine", "tokens": ["In", "der", "Son\u00b7ne", "m\u00fct\u00b7ter\u00b7li\u00b7chem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Richtet sich die Pflanzenwelt empor.", "tokens": ["Rich\u00b7tet", "sich", "die", "Pflan\u00b7zen\u00b7welt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "V\u00f6gel singen in den gr\u00fcnen Zweigen,", "tokens": ["V\u00f6\u00b7gel", "sin\u00b7gen", "in", "den", "gr\u00fc\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "K\u00e4fer schweben freudig hin und her \u2013", "tokens": ["K\u00e4\u00b7fer", "schwe\u00b7ben", "freu\u00b7dig", "hin"