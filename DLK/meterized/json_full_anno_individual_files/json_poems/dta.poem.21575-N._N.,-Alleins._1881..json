{"dta.poem.21575": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Alleins.  \n 1881.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht flie\u00dft in Tag und Tag in Nacht,", "tokens": ["Nacht", "flie\u00dft", "in", "Tag", "und", "Tag", "in", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bach zum Strom, der Strom zum Meer \u2014", "tokens": ["Der", "Bach", "zum", "Strom", ",", "der", "Strom", "zum", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Tod zerrinnt des Lebens Pracht,", "tokens": ["In", "Tod", "zer\u00b7rinnt", "des", "Le\u00b7bens", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Tod zeugt Leben licht und hehr.", "tokens": ["Und", "Tod", "zeugt", "Le\u00b7ben", "licht", "und", "hehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und jeder Geist, der br\u00fcnstig strebt,", "tokens": ["Und", "je\u00b7der", "Geist", ",", "der", "br\u00fcns\u00b7tig", "strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dringt wie ein Quell in alle Welt, \u2014", "tokens": ["Dringt", "wie", "ein", "Quell", "in", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PIAT", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was du erlebst, hab\u2019 ich erlebt,", "tokens": ["Was", "du", "er\u00b7lebst", ",", "hab'", "ich", "er\u00b7lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was mich erhellt, hat dich erhellt.", "tokens": ["Was", "mich", "er\u00b7hellt", ",", "hat", "dich", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "All\u2019 sind wir eines Baums Getrieb,", "tokens": ["All'", "sind", "wir", "ei\u00b7nes", "Baums", "Ge\u00b7trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob Zweig, ob Ast, ob Mark, ob Blatt \u2014", "tokens": ["Ob", "Zweig", ",", "ob", "Ast", ",", "ob", "Mark", ",", "ob", "Blatt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,", "KOUS", "NN", "$,", "KOUS", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich hat Natur uns Alle lieb,", "tokens": ["Gleich", "hat", "Na\u00b7tur", "uns", "Al\u00b7le", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie unser Aller Ruhestatt.", "tokens": ["Sie", "un\u00b7ser", "Al\u00b7ler", "Ru\u00b7he\u00b7statt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}