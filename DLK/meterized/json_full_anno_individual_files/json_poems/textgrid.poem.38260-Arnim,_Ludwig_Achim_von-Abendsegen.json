{"textgrid.poem.38260": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Abendsegen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag hat seinen Schmuck auf heute weggethan,", "tokens": ["Der", "Tag", "hat", "sei\u00b7nen", "Schmuck", "auf", "heu\u00b7te", "weg\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ziehet nun die Nacht die braunen Kleider an;", "tokens": ["Es", "zie\u00b7het", "nun", "die", "Nacht", "die", "brau\u00b7nen", "Klei\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deckt die Welt in angenehmer Ruh", "tokens": ["Und", "deckt", "die", "Welt", "in", "an\u00b7ge\u00b7neh\u00b7mer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihren Schatten zu.", "tokens": ["Mit", "ih\u00b7ren", "Schat\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohlan ich suche nun auch meine Lagerstadt,", "tokens": ["Wo\u00b7hlan", "ich", "su\u00b7che", "nun", "auch", "mei\u00b7ne", "La\u00b7ger\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worauf der m\u00fcde Leib sich zu erquicken hat;", "tokens": ["Wo\u00b7rauf", "der", "m\u00fc\u00b7de", "Leib", "sich", "zu", "er\u00b7qui\u00b7cken", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wo der Geist geruhig und vergn\u00fcgt", "tokens": ["Und", "wo", "der", "Geist", "ge\u00b7ru\u00b7hig", "und", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "KON", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In s\u00fc\u00dfer Stille liegt.", "tokens": ["In", "s\u00fc\u00b7\u00dfer", "Stil\u00b7le", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein gut Gewissen wird mein Abendsegen seyn,", "tokens": ["Ein", "gut", "Ge\u00b7wis\u00b7sen", "wird", "mein", "A\u00b7bend\u00b7se\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Unschuld machet mich von aller Falschheit rein,", "tokens": ["Die", "Un\u00b7schuld", "ma\u00b7chet", "mich", "von", "al\u00b7ler", "Falschheit", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Herz ist treu, wer anders von mir spricht,", "tokens": ["Mein", "Herz", "ist", "treu", ",", "wer", "an\u00b7ders", "von", "mir", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der kennet mich noch nicht.", "tokens": ["Der", "ken\u00b7net", "mich", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So kleide dich nun aus, mein ungebundner Sinn,", "tokens": ["So", "klei\u00b7de", "dich", "nun", "aus", ",", "mein", "un\u00b7ge\u00b7bund\u00b7ner", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch dich leg ich vergn\u00fcgt die Sorgenkleider hin;", "tokens": ["Durch", "dich", "leg", "ich", "ver\u00b7gn\u00fcgt", "die", "Sor\u00b7gen\u00b7klei\u00b7der", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die Brust ist frey, die Kummer und Verdru\u00df", "tokens": ["Die", "Brust", "ist", "frey", ",", "die", "Kum\u00b7mer", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bei andern qu\u00e4len mu\u00df.", "tokens": ["Bei", "an\u00b7dern", "qu\u00e4\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein froh Gem\u00fcthe soll mein saubres Nachtzeug seyn,", "tokens": ["Ein", "froh", "Ge\u00b7m\u00fc\u00b7the", "soll", "mein", "sau\u00b7bres", "Nacht\u00b7zeug", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In solchen schlaf ich sanft und ohne Schwermuth ein;", "tokens": ["In", "sol\u00b7chen", "schlaf", "ich", "sanft", "und", "oh\u00b7ne", "Schwer\u00b7muth", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "ADJD", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und machte mir auch was Melancholey,", "tokens": ["Und", "mach\u00b7te", "mir", "auch", "was", "Me\u00b7lan\u00b7cho\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PWS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So schwebt sie doch vorbey.", "tokens": ["So", "schwebt", "sie", "doch", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Himmel wacht bei mir, sein Auge das mich kennt,", "tokens": ["Der", "Him\u00b7mel", "wacht", "bei", "mir", ",", "sein", "Au\u00b7ge", "das", "mich", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df mir die Lampe seyn, die mir zum Troste brennt;", "tokens": ["Mu\u00df", "mir", "die", "Lam\u00b7pe", "seyn", ",", "die", "mir", "zum", "Tros\u00b7te", "brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil das Oel der Gnade nie gebricht,", "tokens": ["Und", "weil", "das", "O\u00b7el", "der", "Gna\u00b7de", "nie", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ach so verl\u00f6scht sie nicht.", "tokens": ["Ach", "so", "ver\u00b7l\u00f6scht", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die s\u00fc\u00dfre Hoffnung ist auf meinen Dienst bereit,", "tokens": ["Die", "s\u00fc\u00df\u00b7re", "Hoff\u00b7nung", "ist", "auf", "mei\u00b7nen", "Dienst", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die lauter Rosen mir zum Ruhebette streut;", "tokens": ["Die", "lau\u00b7ter", "Ro\u00b7sen", "mir", "zum", "Ru\u00b7he\u00b7bet\u00b7te", "streut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die Geduld deckt mich mit Myrthen zu,", "tokens": ["Und", "die", "Ge\u00b7duld", "deckt", "mich", "mit", "Myr\u00b7then", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So sch\u00f6n ist meine Ruh.", "tokens": ["So", "sch\u00f6n", "ist", "mei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Zum Schlafgesellen nehm ich die Vergn\u00fcgung an,", "tokens": ["Zum", "Schlaf\u00b7ge\u00b7sel\u00b7len", "nehm", "ich", "die", "Ver\u00b7gn\u00fc\u00b7gung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dr\u00fcck ich an mein Herz, so fest ich immer kann,", "tokens": ["Die", "dr\u00fcck", "ich", "an", "mein", "Herz", ",", "so", "fest", "ich", "im\u00b7mer", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man schl\u00e4ft, wenn so ein Schaz in Armen liegt,", "tokens": ["Man", "schl\u00e4ft", ",", "wenn", "so", "ein", "Schaz", "in", "Ar\u00b7men", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unm\u00f6glich mi\u00dfvergn\u00fcgt.", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "mi\u00df\u00b7ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und treibt ihr Tr\u00e4ume ja ein Sinnenspiel mit mir,", "tokens": ["Und", "treibt", "ihr", "Tr\u00e4u\u00b7me", "ja", "ein", "Sin\u00b7nen\u00b7spiel", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So stellt in s\u00fc\u00dfer Ruh mir meine Freundinn f\u00fcr;", "tokens": ["So", "stellt", "in", "s\u00fc\u00b7\u00dfer", "Ruh", "mir", "mei\u00b7ne", "Freun\u00b7dinn", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vielleicht wird das, was jetzt ein Schatten ist,", "tokens": ["Viel\u00b7leicht", "wird", "das", ",", "was", "jetzt", "ein", "Schat\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch in der That gek\u00fc\u00dft.", "tokens": ["Noch", "in", "der", "That", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nun dir befehl ich mich, du angenehme Nacht,", "tokens": ["Nun", "dir", "be\u00b7fehl", "ich", "mich", ",", "du", "an\u00b7ge\u00b7neh\u00b7me", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn das Morgengold am fr\u00fchen Himmel lacht,", "tokens": ["Und", "wenn", "das", "Mor\u00b7gen\u00b7gold", "am", "fr\u00fc\u00b7hen", "Him\u00b7mel", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So werde doch dem Herzen das geschenkt,", "tokens": ["So", "wer\u00b7de", "doch", "dem", "Her\u00b7zen", "das", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Worauf es schlafend denkt.", "tokens": ["Wo\u00b7rauf", "es", "schla\u00b7fend", "denkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Tag hat seinen Schmuck auf heute weggethan,", "tokens": ["Der", "Tag", "hat", "sei\u00b7nen", "Schmuck", "auf", "heu\u00b7te", "weg\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ziehet nun die Nacht die braunen Kleider an;", "tokens": ["Es", "zie\u00b7het", "nun", "die", "Nacht", "die", "brau\u00b7nen", "Klei\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deckt die Welt in angenehmer Ruh", "tokens": ["Und", "deckt", "die", "Welt", "in", "an\u00b7ge\u00b7neh\u00b7mer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihren Schatten zu.", "tokens": ["Mit", "ih\u00b7ren", "Schat\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wohlan ich suche nun auch meine Lagerstadt,", "tokens": ["Wo\u00b7hlan", "ich", "su\u00b7che", "nun", "auch", "mei\u00b7ne", "La\u00b7ger\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worauf der m\u00fcde Leib sich zu erquicken hat;", "tokens": ["Wo\u00b7rauf", "der", "m\u00fc\u00b7de", "Leib", "sich", "zu", "er\u00b7qui\u00b7cken", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wo der Geist geruhig und vergn\u00fcgt", "tokens": ["Und", "wo", "der", "Geist", "ge\u00b7ru\u00b7hig", "und", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART"