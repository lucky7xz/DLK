{"textgrid.poem.50871": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die alten Jungfern bleichen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die alten Jungfern bleichen", "tokens": ["Die", "al\u00b7ten", "Jung\u00b7fern", "blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Tuch am Sternenschein,", "tokens": ["Ihr", "Tuch", "am", "Ster\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wird dann ohnegleichen", "tokens": ["Da", "wird", "dann", "oh\u00b7ne\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Linnen zart und rein.", "tokens": ["Das", "Lin\u00b7nen", "zart", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei Tage an der Sonnen", "tokens": ["Bei", "Ta\u00b7ge", "an", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tut's fast den Augen weh;", "tokens": ["Tut's", "fast", "den", "Au\u00b7gen", "weh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im Himmel wird's gesponnen,", "tokens": ["Im", "Him\u00b7mel", "wird's", "ge\u00b7sp