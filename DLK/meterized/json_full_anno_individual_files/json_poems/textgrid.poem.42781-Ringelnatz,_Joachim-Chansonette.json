{"textgrid.poem.42781": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Chansonette", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War ein echter Prinz und hat Warzen im Bett.", "tokens": ["War", "ein", "ech\u00b7ter", "Prinz", "und", "hat", "War\u00b7zen", "im", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "KON", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kniete vor jeder Schleife.", "tokens": ["Und", "knie\u00b7te", "vor", "je\u00b7der", "Schlei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vaters Leiche lag auf dem B\u00fcgelbrett", "tokens": ["Va\u00b7ters", "Lei\u00b7che", "lag", "auf", "dem", "B\u00fc\u00b7gel\u00b7brett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und roch nach Genever und Seife.", "tokens": ["Und", "roch", "nach", "Ge\u00b7ne\u00b7ver", "und", "Sei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn der Pfaffe unter meine R\u00f6cke schielt,", "tokens": ["Wenn", "der", "Pfaf\u00b7fe", "un\u00b7ter", "mei\u00b7ne", "R\u00f6\u00b7cke", "schielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$