{"dta.poem.3062": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun nachgerade bin ich dieses Daseyns satt,", "tokens": ["Nun", "nach\u00b7ge\u00b7ra\u00b7de", "bin", "ich", "die\u00b7ses", "Da\u00b7seyns", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des engen, das den Geist solang umrungen hat.", "tokens": ["Des", "en\u00b7gen", ",", "das", "den", "Geist", "so\u00b7lang", "um\u00b7run\u00b7gen", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN", "VMFIN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und mich begeben m\u00f6cht' ich auf Entdeckungsreisen,", "tokens": ["Und", "mich", "be\u00b7ge\u00b7ben", "m\u00f6cht'", "ich", "auf", "Ent\u00b7de\u00b7ckungs\u00b7rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch in Welttheilen nicht, noch auch in Sternenkreisen.", "tokens": ["Doch", "in", "Welt\u00b7thei\u00b7len", "nicht", ",", "noch", "auch", "in", "Ster\u00b7nen\u00b7krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKNEG", "$,", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn Weltentheile sind nur Theile dieser Welt,", "tokens": ["Denn", "Wel\u00b7ten\u00b7thei\u00b7le", "sind", "nur", "Thei\u00b7le", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch nur Zeit und Raum umspannt das Sternenzelt.", "tokens": ["Und", "auch", "nur", "Zeit", "und", "Raum", "um\u00b7spannt", "das", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In einer Welt, o Geist, worin die Zeit zum Nu,", "tokens": ["In", "ei\u00b7ner", "Welt", ",", "o", "Geist", ",", "wo\u00b7rin", "die", "Zeit", "zum", "Nu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "FM", "NN", "$,", "PWAV", "ART", "NN", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Raum zum Punkte wird, zu kreisen l\u00fcstest du.", "tokens": ["Der", "Raum", "zum", "Punk\u00b7te", "wird", ",", "zu", "krei\u00b7sen", "l\u00fcs\u00b7test", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In Gottes Geisterwelt zu kreisen l\u00fcstest du,", "tokens": ["In", "Got\u00b7tes", "Geis\u00b7ter\u00b7welt", "zu", "krei\u00b7sen", "l\u00fcs\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Gottes Geisterwelt zu reisen r\u00fcstest du.", "tokens": ["In", "Got\u00b7tes", "Geis\u00b7ter\u00b7welt", "zu", "rei\u00b7sen", "r\u00fcs\u00b7test", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was ist die R\u00fcstung denn dahin und Vorbereitung?", "tokens": ["Was", "ist", "die", "R\u00fcs\u00b7tung", "denn", "da\u00b7hin", "und", "Vor\u00b7be\u00b7rei\u00b7tung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "PAV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erharren in Geduld Fahrwind und Segelspreitung.", "tokens": ["Er\u00b7har\u00b7ren", "in", "Ge\u00b7duld", "Fahr\u00b7wind", "und", "Se\u00b7gel\u00b7sprei\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wo ist die Himmelsluft, vor deren Hauch erbl\u00fcht", "tokens": ["Wo", "ist", "die", "Him\u00b7mel\u00b7sluft", ",", "vor", "de\u00b7ren", "Hauch", "er\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "APPR", "PRELAT", "NN", "VVPP"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Das Segel, das gewelkt umflattert mein Gem\u00fct?", "tokens": ["Das", "Se\u00b7gel", ",", "das", "ge\u00b7welkt", "um\u00b7flat\u00b7tert", "mein", "Ge\u00b7m\u00fct", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Derselben harre du, und sammle kein Gewicht", "tokens": ["Der\u00b7sel\u00b7ben", "har\u00b7re", "du", ",", "und", "samm\u00b7le", "kein", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Reise, sammle dich! die Reis' entgeht dir nicht.", "tokens": ["Zur", "Rei\u00b7se", ",", "samm\u00b7le", "dich", "!", "die", "Reis'", "ent\u00b7geht", "dir", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ganz sammle nur, mein Geist, dich in Vergeistigung!", "tokens": ["Ganz", "samm\u00b7le", "nur", ",", "mein", "Geist", ",", "dich", "in", "Ver\u00b7geis\u00b7ti\u00b7gung", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Reis' entgeht dir nicht, wann du bist Geist genung.", "tokens": ["Die", "Reis'", "ent\u00b7geht", "dir", "nicht", ",", "wann", "du", "bist", "Geist", "ge\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}