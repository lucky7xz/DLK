{"textgrid.poem.57381": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Graun der Mitternacht schliefst mich nicht ein,", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graun der Mitternacht schliefst mich nicht ein,", "tokens": ["Graun", "der", "Mit\u00b7ter\u00b7nacht", "schliefst", "mich", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr Verstummen nicht; auch ist, in dem Namen der heiligen,", "tokens": ["Ihr", "Ver\u00b7stum\u00b7men", "nicht", ";", "auch", "ist", ",", "in", "dem", "Na\u00b7men", "der", "hei\u00b7li\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "$.", "ADV", "VAFIN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+---+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Freyheit, j\u00fcngst kein Mord geschehn; dennoch ist mir", "tokens": ["Frey\u00b7heit", ",", "j\u00fcngst", "kein", "Mord", "ge\u00b7schehn", ";", "den\u00b7noch", "ist", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PIAT", "NN", "VVPP", "$.", "ADV", "VAFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ernst die ganze Seele.", "tokens": ["Ernst", "die", "gan\u00b7ze", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Liebliches Wehn uns\u00e4uselt mich;", "tokens": ["Li