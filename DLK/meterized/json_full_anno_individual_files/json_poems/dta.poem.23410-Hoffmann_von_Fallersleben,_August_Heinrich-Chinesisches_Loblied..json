{"dta.poem.23410": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Chinesisches Loblied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stehende Heere m\u00fcssen wir haben,", "tokens": ["Ste\u00b7hen\u00b7de", "Hee\u00b7re", "m\u00fcs\u00b7sen", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Stehende Heer' im himmlischen Reich.", "tokens": ["Ste\u00b7hen\u00b7de", "Heer'", "im", "himm\u00b7li\u00b7schen", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "W\u00e4r' es nicht wahrlich Jammer und Schade,", "tokens": ["W\u00e4r'", "es", "nicht", "wahr\u00b7lich", "Jam\u00b7mer", "und", "Scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "NN", "KON", "ADJD", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wenn wir nicht h\u00e4tten manchmal Parade,", "tokens": ["Wenn", "wir", "nicht", "h\u00e4t\u00b7ten", "manch\u00b7mal", "Pa\u00b7ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wenn wir nicht h\u00f6rten den Zapfenstreich?", "tokens": ["Wenn", "wir", "nicht", "h\u00f6r\u00b7ten", "den", "Zap\u00b7fen\u00b7streich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Stehende Heere m\u00fcssen wir haben,", "tokens": ["Ste\u00b7hen\u00b7de", "Hee\u00b7re", "m\u00fcs\u00b7sen", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Stehende Heer' im himmlischen Reich.", "tokens": ["Ste\u00b7hen\u00b7de", "Heer'", "im", "himm\u00b7li\u00b7schen", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Stehende Heere m\u00fcssen wir haben,", "tokens": ["Ste\u00b7hen\u00b7de", "Hee\u00b7re", "m\u00fcs\u00b7sen", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Weil sie in Umlauf bringen das Geld:", "tokens": ["Weil", "sie", "in", "Um\u00b7lauf", "brin\u00b7gen", "das", "Geld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wo die Soldaten zechen und zehren", "tokens": ["Wo", "die", "Sol\u00b7da\u00b7ten", "ze\u00b7chen", "und", "zeh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mu\u00df sich der Handel und Wandel vermehren,", "tokens": ["Mu\u00df", "sich", "der", "Han\u00b7del", "und", "Wan\u00b7del", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Und es verdienet dann alle Welt.", "tokens": ["Und", "es", "ver\u00b7die\u00b7net", "dann", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "------+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Stehende Heere m\u00fcssen wir haben,", "tokens": ["Ste\u00b7hen\u00b7de", "Hee\u00b7re", "m\u00fcs\u00b7sen", "wir", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Weil sie in Umlauf bringen das Geld.", "tokens": ["Weil", "sie", "in", "Um\u00b7lauf", "brin\u00b7gen", "das", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Stehende Heere m\u00fcssen wir haben;", "tokens": ["Ste\u00b7hen\u00b7de", "Hee\u00b7re", "m\u00fcs\u00b7sen", "wir", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wo sie bestehen, bestehen auch wir.", "tokens": ["Wo", "sie", "be\u00b7ste\u00b7hen", ",", "be\u00b7ste\u00b7hen", "auch", "wir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wenn wir die stehenden Heere nicht wollten,", "tokens": ["Wenn", "wir", "die", "ste\u00b7hen\u00b7den", "Hee\u00b7re", "nicht", "woll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "W\u00fc\u00dften die Junker nicht was sie sollten,", "tokens": ["W\u00fc\u00df\u00b7ten", "die", "Jun\u00b7ker", "nicht", "was", "sie", "soll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PWS", "PPER", "VMFIN", "$,"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ach! und die meisten verschmachteten schier.", "tokens": ["Ach", "!", "und", "die", "meis\u00b7ten", "ver\u00b7schmach\u00b7te\u00b7ten", "schier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "ART", "PIAT", "ADJA", "NN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Stehende Heere m\u00fcssen wir haben;", "tokens": ["Ste\u00b7hen\u00b7de", "Hee\u00b7re", "m\u00fcs\u00b7sen", "wir", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wo sie bestehen, bestehen auch wir.", "tokens": ["Wo", "sie", "be\u00b7ste\u00b7hen", ",", "be\u00b7ste\u00b7hen", "auch", "wir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}