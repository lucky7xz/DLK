{"dta.poem.8459": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Huldigung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Will ich fr\u00f6hnen", "tokens": ["Will", "ich", "fr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Bis an meinen Tod,", "tokens": ["Bis", "an", "mei\u00b7nen", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mit Gesangesweisen.", "tokens": ["Mit", "Ge\u00b7san\u00b7ges\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bis an meinen Tod", "tokens": ["Bis", "an", "mei\u00b7nen", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Eure Tugend preisen.", "tokens": ["Eu\u00b7re", "Tu\u00b7gend", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr, o Guten,", "tokens": ["Ihr", ",", "o", "Gu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wohlgemuten,", "tokens": ["Wohl\u00b7ge\u00b7mu\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Macht das Leben s\u00fcss,", "tokens": ["Macht", "das", "Le\u00b7ben", "s\u00fcss", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Macht den Mann zum Engel,", "tokens": ["Macht", "den", "Mann", "zum", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und zum Paradies", "tokens": ["Und", "zum", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Eine Welt voll M\u00e4ngel.", "tokens": ["Ei\u00b7ne", "Welt", "voll", "M\u00e4n\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wer die S\u00fcsse", "tokens": ["Wer", "die", "S\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Treuer K\u00fcsse", "tokens": ["Treu\u00b7er", "K\u00fcs\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nicht gekostet hat,", "tokens": ["Nicht", "ge\u00b7kos\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Irret wie verloren", "tokens": ["Ir\u00b7ret", "wie", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KOKOM", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf dem Lebenspfad,", "tokens": ["Auf", "dem", "Le\u00b7ben\u00b7spfad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Ist noch ungeboren.", "tokens": ["Ist", "noch", "un\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wer die S\u00fcsse", "tokens": ["Wer", "die", "S\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Treuer K\u00fcsse", "tokens": ["Treu\u00b7er", "K\u00fcs\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schon gekostet hat,", "tokens": ["Schon", "ge\u00b7kos\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gl\u00e4nzt von Himmelsscheine;", "tokens": ["Gl\u00e4nzt", "von", "Him\u00b7mels\u00b7schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wo sein Fuss sich naht,", "tokens": ["Wo", "sein", "Fuss", "sich", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bl\u00fchen Rosenhaine.", "tokens": ["Bl\u00fc\u00b7hen", "Ro\u00b7sen\u00b7hai\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}