{"textgrid.poem.33732": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "14. Urzeit", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein D\u00e4mmrungsfalter, kaum entschl\u00fcpft den Puppen", "tokens": ["Ein", "D\u00e4mm\u00b7rungs\u00b7fal\u00b7ter", ",", "kaum", "ent\u00b7schl\u00fcpft", "den", "Pup\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Stein- und Pflanzenreichs, sieh, da vertraute", "tokens": ["Des", "Stein", "und", "Pflan\u00b7zen\u00b7reichs", ",", "sieh", ",", "da", "ver\u00b7trau\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "$,", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die junge Tierwelt, als ihr Morgen graute,", "tokens": ["Die", "jun\u00b7ge", "Tier\u00b7welt", ",", "als", "ihr", "Mor\u00b7gen", "grau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Fl\u00fcgeln sich, noch ganz in harten Schuppen.", "tokens": ["Den", "Fl\u00fc\u00b7geln", "sich", ",", "noch", "ganz", "in", "har\u00b7ten", "Schup\u00b7pen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch stoben Rauch der Berge nackte Kuppen,", "tokens": ["Noch", "sto\u00b7ben", "Rauch", "der", "Ber\u00b7ge", "nack\u00b7te", "Kup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie die Wasserflut allm\u00e4hlich staute,", "tokens": ["Und", "wie", "die", "Was\u00b7ser\u00b7flut", "all\u00b7m\u00e4h\u00b7lich", "stau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schwang es sich empor, gez\u00e4hnt, und schaute", "tokens": ["So", "schwang", "es", "sich", "em\u00b7por", ",", "ge\u00b7z\u00e4hnt", ",", "und", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "VVPP", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begierig aus nach gr\u00fcnen Inselgruppen.", "tokens": ["Be\u00b7gie\u00b7rig", "aus", "nach", "gr\u00fc\u00b7nen", "In\u00b7sel\u00b7grup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da freute jedes Unget\u00fcm, und kreischend", "tokens": ["Da", "freu\u00b7te", "je\u00b7des", "Un\u00b7ge\u00b7t\u00fcm", ",", "und", "krei\u00b7schend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In aller Scheu\u00dflichkeit, sich seines Fanges,", "tokens": ["In", "al\u00b7ler", "Scheu\u00df\u00b7lich\u00b7keit", ",", "sich", "sei\u00b7nes", "Fan\u00b7ges", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den gleich abscheulich wilden Feind zerfleischend.", "tokens": ["Den", "gleich", "ab\u00b7scheu\u00b7lich", "wil\u00b7den", "Feind", "zer\u00b7flei\u00b7schend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "In tr\u00fcber Mondnacht heulte da sein banges", "tokens": ["In", "tr\u00fc\u00b7ber", "Mond\u00b7nacht", "heul\u00b7te", "da", "sein", "ban\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschrei die Brut, den Beuteteil erheischend,", "tokens": ["Ge\u00b7schrei", "die", "Brut", ",", "den", "Beu\u00b7te\u00b7teil", "er\u00b7hei\u00b7schend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Ahnungsgraun des eignen Unterganges.", "tokens": ["Im", "Ah\u00b7nungs\u00b7graun", "des", "eig\u00b7nen", "Un\u00b7ter\u00b7gan\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein D\u00e4mmrungsfalter, kaum entschl\u00fcpft den Puppen", "tokens": ["Ein", "D\u00e4mm\u00b7rungs\u00b7fal\u00b7ter", ",", "kaum", "ent\u00b7schl\u00fcpft", "den", "Pup\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-