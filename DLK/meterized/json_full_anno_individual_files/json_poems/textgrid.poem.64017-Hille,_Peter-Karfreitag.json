{"textgrid.poem.64017": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Karfreitag", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Karfreitags Krone. Heldenk\u00f6nig! Einsames Haupt.", "tokens": ["Kar\u00b7frei\u00b7tags", "Kro\u00b7ne", ".", "Hel\u00b7den\u00b7k\u00f6\u00b7nig", "!", "Ein\u00b7sa\u00b7mes", "Haupt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Versto\u00dfen. Erheben", "tokens": ["Ver\u00b7sto\u00b7\u00dfen", ".", "Er\u00b7he\u00b7ben"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die feige Flucht verdammender H\u00e4nde.", "tokens": ["Die", "fei\u00b7ge", "Flucht", "ver\u00b7dam\u00b7men\u00b7der", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein suchender f\u00fchrender Quell.", "tokens": ["Ein", "su\u00b7chen\u00b7der", "f\u00fch\u00b7ren\u00b7der", "Quell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wenn ich erh\u00f6ht sein werde, will ich alle zu mir ziehen.", "tokens": ["Wenn", "ich", "er\u00b7h\u00f6ht", "sein", "wer\u00b7de", ",", "will", "ich", "al\u00b7le", "zu", "mir", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAINF", "VAFIN", "$,", "VMFIN", "PPER", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Und die Welt, die schwere Welt, die leichtsinnschwere Welt,", "tokens": ["Und", "die", "Welt", ",", "die", "schwe\u00b7re", "Welt", ",", "die", "leicht\u00b7sinn\u00b7schwe\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Fast schon oben, rei\u00dft ab, eine Wunde rei\u00dft auf,", "tokens": ["Fast", "schon", "o\u00b7ben", ",", "rei\u00dft", "ab", ",", "ei\u00b7ne", "Wun\u00b7de", "rei\u00dft", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.8": {"text": "Der Seele, Wunde des Leibes, Wunde des Todes:", "tokens": ["Der", "See\u00b7le", ",", "Wun\u00b7de", "des", "Lei\u00b7bes", ",", "Wun\u00b7de", "des", "To\u00b7des", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Vater verzeihe ihnen, sie wissen nicht, was sie tun.", "tokens": ["Va\u00b7ter", "ver\u00b7zei\u00b7he", "ih\u00b7nen", ",", "sie", "wis\u00b7sen", "nicht", ",", "was", "sie", "tun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+--+-+--+-++-+", "measure": "iambic.septa.invert"}, "line.10": {"text": "Zum