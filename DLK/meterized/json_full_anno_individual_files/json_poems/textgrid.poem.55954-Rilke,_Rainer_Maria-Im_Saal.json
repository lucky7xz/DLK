{"textgrid.poem.55954": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Im Saal", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sind sie alle um uns, diese Herrn", "tokens": ["Wie", "sind", "sie", "al\u00b7le", "um", "uns", ",", "die\u00b7se", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PIS", "APPR", "PPER", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in Kammerherrentrachten und Jabots,", "tokens": ["in", "Kam\u00b7mer\u00b7her\u00b7ren\u00b7trach\u00b7ten", "und", "Ja\u00b7bots", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "wie eine Nacht um ihren Ordensstern", "tokens": ["wie", "ei\u00b7ne", "Nacht", "um", "ih\u00b7ren", "Or\u00b7denss\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sich immer mehr verdunkelnd, r\u00fccksichtslos,", "tokens": ["sich", "im\u00b7mer", "mehr", "ver\u00b7dun\u00b7kelnd", ",", "r\u00fcck\u00b7sichts\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und diese Damen, zart, fragile, doch gro\u00df", "tokens": ["und", "die\u00b7se", "Da\u00b7men", ",", "zart", ",", "fra\u00b7gi\u00b7le", ",", "doch", "gro\u00df"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "ADJD", "$,", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "von ihren Kleidern, eine Hand im Schoo\u00df,", "tokens": ["von", "ih\u00b7ren", "Klei\u00b7dern", ",", "ei\u00b7ne", "Hand", "im", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "klein wie ein Halsband f\u00fcr den Bologneser;", "tokens": ["klein", "wie", "ein", "Hals\u00b7band", "f\u00fcr", "den", "Bo\u00b7log\u00b7ne\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "++-+-+-+---", "measure": "unknown.measure.penta"}, "line.8": {"text": "wie sind sie da um jeden: um den Leser,", "tokens": ["wie", "sind", "sie", "da", "um", "je\u00b7den", ":", "um", "den", "Le\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "um den Betrachter dieser Bibelots,", "tokens": ["um", "den", "Be\u00b7trach\u00b7ter", "die\u00b7ser", "Bi\u00b7be\u00b7lots", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "darunter manches ihnen noch geh\u00f6rt.", "tokens": ["da\u00b7run\u00b7ter", "man\u00b7ches", "ih\u00b7nen", "noch", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie lassen, voller Takt, uns ungest\u00f6rt", "tokens": ["Sie", "las\u00b7sen", ",", "vol\u00b7ler", "Takt", ",", "uns", "un\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$,", "ADJA", "NN", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das Leben leben wie wir es begreifen", "tokens": ["das", "Le\u00b7ben", "le\u00b7ben", "wie", "wir", "es", "be\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und wie sie's nicht verstehn. Sie wollten bl\u00fchn,", "tokens": ["und", "wie", "sie's", "nicht", "ver\u00b7stehn", ".", "Sie", "woll\u00b7ten", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PTKNEG", "VVINF", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und bl\u00fchn ist sch\u00f6n sein; doch wir wollen reifen,", "tokens": ["und", "bl\u00fchn", "ist", "sch\u00f6n", "sein", ";", "doch", "wir", "wol\u00b7len", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADJD", "VAINF", "$.", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und das hei\u00dft dunkel sein und sich bem\u00fchn.", "tokens": ["und", "das", "hei\u00dft", "dun\u00b7kel", "sein", "und", "sich", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "VAINF", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie sind sie alle um uns, diese Herrn", "tokens