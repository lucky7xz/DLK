{"textgrid.poem.52716": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war die Schlacht geschlagen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war die Schlacht geschlagen,", "tokens": ["Es", "war", "die", "Schlacht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die letzte Schlacht im Krieg,", "tokens": ["Die", "letz\u00b7te", "Schlacht", "im", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es trugen unsre Fahnen", "tokens": ["Es", "tru\u00b7gen", "uns\u00b7re", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Feindesland den Sieg.", "tokens": ["In", "Fein\u00b7des\u00b7land", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich war der erste beste", "tokens": ["Ich", "war", "der", "ers\u00b7te", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Freiwillige vor der Schanz,", "tokens": ["Frei\u00b7wil\u00b7li\u00b7ge", "vor", "der", "Schanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die erste beste Kugel", "tokens": ["Die", "ers\u00b7te", "bes\u00b7te", "Ku\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerri\u00df den Arm mir ganz.", "tokens": ["Zer\u00b7ri\u00df", "den", "Arm", "mir", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich lag so schwer darnieder,", "tokens": ["Ich", "lag", "so", "schwer", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So fern von Liebchens Th\u00fcr,", "tokens": ["So", "fern", "von", "Lieb\u00b7chens", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eines sch\u00f6nen Abends", "tokens": ["Und", "ei\u00b7nes", "sch\u00f6\u00b7nen", "A\u00b7bends"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00e4r ich gestorben schier.", "tokens": ["W\u00e4r", "ich", "ge\u00b7stor\u00b7ben", "schier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als davon die Kunde", "tokens": ["Und", "als", "da\u00b7von", "die", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gedrungen in ihr Haus,", "tokens": ["Ge\u00b7drun\u00b7gen", "in", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da brach mein Schatz vor Schmerzen", "tokens": ["Da", "brach", "mein", "Schatz", "vor", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Blut und Thr\u00e4nen aus.", "tokens": ["In", "Blut", "und", "Thr\u00e4\u00b7nen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr ward im tiefsten Herzen", "tokens": ["Ihr", "ward", "im", "tiefs\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bang, so wild und weh:", "tokens": ["So", "bang", ",", "so", "wild", "und", "weh", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob ich ihn habe verloren,", "tokens": ["Ob", "ich", "ihn", "ha\u00b7be", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob ich ihn wiederseh?", "tokens": ["Ob", "ich", "ihn", "wie\u00b7der\u00b7seh", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So sank sie hin aufs Lager,", "tokens": ["So", "sank", "sie", "hin", "aufs", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sang sie leise f\u00fcr sich,", "tokens": ["So", "sang", "sie", "lei\u00b7se", "f\u00fcr", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PRF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So schrie die sch\u00f6ne Jungfrau", "tokens": ["So", "schrie", "die", "sch\u00f6\u00b7ne", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In ihrem Jammer um mich.", "tokens": ["In", "ih\u00b7rem", "Jam\u00b7mer", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "O Gott, er ist gestorben!", "tokens": ["O", "Gott", ",", "er", "ist", "ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Himmel, er ist todt!", "tokens": ["O", "Him\u00b7mel", ",", "er", "ist", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah sein Herz zerrissen", "tokens": ["Ich", "sah", "sein", "Herz", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Traum, war blutigroth!", "tokens": ["Im", "Traum", ",", "war", "blu\u00b7ti\u00b7groth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da ward es in ihrer Seele", "tokens": ["Da", "ward", "es", "in", "ih\u00b7rer", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gar feierlich, gar still,", "tokens": ["Gar", "fei\u00b7er\u00b7lich", ",", "gar", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ward der Guten zu Muthe", "tokens": ["Da", "ward", "der", "Gu\u00b7ten", "zu", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Einer, die sterben will.", "tokens": ["Wie", "Ei\u00b7ner", ",", "die", "ster\u00b7ben", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie glaubte zu zerflie\u00dfen", "tokens": ["Sie", "glaub\u00b7te", "zu", "zer\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins weite herrliche All,", "tokens": ["Ins", "wei\u00b7te", "herr\u00b7li\u00b7che", "All", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aus lichten Fernen vernahm sie", "tokens": ["Aus", "lich\u00b7ten", "Fer\u00b7nen", "ver\u00b7nahm", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unendlich s\u00fc\u00dfen Schall.", "tokens": ["Un\u00b7end\u00b7lich", "s\u00fc\u00b7\u00dfen", "Schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie faltete die H\u00e4nde", "tokens": ["Sie", "fal\u00b7te\u00b7te", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf ihrer Brust, sie sang", "tokens": ["Auf", "ih\u00b7rer", "Brust", ",", "sie", "sang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihrer Engelsstimme", "tokens": ["Mit", "ih\u00b7rer", "En\u00b7gels\u00b7stim\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unendlich s\u00fc\u00dfen Klang.", "tokens": ["Un\u00b7end\u00b7lich", "s\u00fc\u00b7\u00dfen", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbwie wird mir, ach, auf einmal?", "tokens": ["\u00bb", "wie", "wird", "mir", ",", "ach", ",", "auf", "ein\u00b7mal", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "$,", "ITJ", "$,", "APPR", "ADV", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "So wohl, so leicht, so frei!", "tokens": ["So", "wohl", ",", "so", "leicht", ",", "so", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ob zu seliger Wonne", "tokens": ["Als", "ob", "zu", "se\u00b7li\u00b7ger", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich ganz genesen sei!", "tokens": ["Ich", "ganz", "ge\u00b7ne\u00b7sen", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich f\u00fchle mich so gl\u00fccklich,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "so", "gl\u00fcck\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie nie ich gl\u00fccklich war,", "tokens": ["Wie", "nie", "ich", "gl\u00fcck\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich fasset ein Entz\u00fccken,", "tokens": ["Mich", "fas\u00b7set", "ein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entz\u00fccken wunderbar.", "tokens": ["Ent\u00b7z\u00fc\u00b7cken", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ich f\u00fchle mich, ich f\u00fchle", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", ",", "ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ihm, ach ihm vereint,", "tokens": ["Mit", "ihm", ",", "ach", "ihm", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "XY", "PPER", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich hab ihn wiedergewonnen,", "tokens": ["Ich", "hab", "ihn", "wie\u00b7der\u00b7ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um den ich hei\u00df geweint!\u00ab", "tokens": ["Um", "den", "ich", "hei\u00df", "ge\u00b7weint", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Du lagst in Fiebertr\u00e4umen", "tokens": ["Du", "lagst", "in", "Fie\u00b7ber\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So krank und fern von mir,", "tokens": ["So", "krank", "und", "fern", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eines sch\u00f6nen Abends", "tokens": ["Und", "ei\u00b7nes", "sch\u00f6\u00b7nen", "A\u00b7bends"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00e4rst du gestorben schier.", "tokens": ["W\u00e4rst", "du", "ge\u00b7stor\u00b7ben", "schier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.15": {"line.1": {"text": "Und kaum war dunkle Kunde", "tokens": ["Und", "kaum", "war", "dunk\u00b7le", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gedrungen in mein Haus,", "tokens": ["Ge\u00b7drun\u00b7gen", "in", "mein", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hielt es mich nicht l\u00e4nger,", "tokens": ["Da", "hielt", "es", "mich", "nicht", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da trieb es mich hinaus!", "tokens": ["Da", "trieb", "es", "mich", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APZR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "In deine lieben Arme,", "tokens": ["In", "dei\u00b7ne", "lie\u00b7ben", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An deinen lieben Mund \u2013", "tokens": ["An", "dei\u00b7nen", "lie\u00b7ben", "Mund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der starke Geist, der Wille", "tokens": ["Der", "star\u00b7ke", "Geist", ",", "der", "Wil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie machten mich gesund.", "tokens": ["Sie", "mach\u00b7ten", "mich", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "O Liebchen, theures Leben,", "tokens": ["O", "Lieb\u00b7chen", ",", "theu\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Freudenthr\u00e4nen stehn", "tokens": ["Die", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "stehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In meinen Augen, denk ich", "tokens": ["In", "mei\u00b7nen", "Au\u00b7gen", ",", "denk", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An jenes \u2013 Wiedersehn!", "tokens": ["An", "je\u00b7nes", "\u2013", "Wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "$(", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Es war die Schlacht geschlagen,", "tokens": ["Es", "war", "die", "Schlacht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die letzte Schlacht im Krieg,", "tokens": ["Die", "letz\u00b7te", "Schlacht", "im", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es trugen unsre Fahnen", "tokens": ["Es", "tru\u00b7gen", "uns\u00b7re", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Feindesland den Sieg.", "tokens": ["In", "Fein\u00b7des\u00b7land", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Ich war der erste beste", "tokens": ["Ich", "war", "der", "ers\u00b7te", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Freiwillige vor der Schanz,", "tokens": ["Frei\u00b7wil\u00b7li\u00b7ge", "vor", "der", "Schanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die erste beste Kugel", "tokens": ["Die", "ers\u00b7te", "bes\u00b7te", "Ku\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerri\u00df den Arm mir ganz.", "tokens": ["Zer\u00b7ri\u00df", "den", "Arm", "mir", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Ich lag so schwer darnieder,", "tokens": ["Ich", "lag", "so", "schwer", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So fern von Liebchens Th\u00fcr,", "tokens": ["So", "fern", "von", "Lieb\u00b7chens", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eines sch\u00f6nen Abends", "tokens": ["Und", "ei\u00b7nes", "sch\u00f6\u00b7nen", "A\u00b7bends"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00e4r ich gestorben schier.", "tokens": ["W\u00e4r", "ich", "ge\u00b7stor\u00b7ben", "schier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Und als davon die Kunde", "tokens": ["Und", "als", "da\u00b7von", "die", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gedrungen in ihr Haus,", "tokens": ["Ge\u00b7drun\u00b7gen", "in", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da brach mein Schatz vor Schmerzen", "tokens": ["Da", "brach", "mein", "Schatz", "vor", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Blut und Thr\u00e4nen aus.", "tokens": ["In", "Blut", "und", "Thr\u00e4\u00b7nen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Ihr ward im tiefsten Herzen", "tokens": ["Ihr", "ward", "im", "tiefs\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bang, so wild und weh:", "tokens": ["So", "bang", ",", "so", "wild", "und", "weh", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob ich ihn habe verloren,", "tokens": ["Ob", "ich", "ihn", "ha\u00b7be", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob ich ihn wiederseh?", "tokens": ["Ob", "ich", "ihn", "wie\u00b7der\u00b7seh", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "So sank sie hin aufs Lager,", "tokens": ["So", "sank", "sie", "hin", "aufs", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sang sie leise f\u00fcr sich,", "tokens": ["So", "sang", "sie", "lei\u00b7se", "f\u00fcr", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PRF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So schrie die sch\u00f6ne Jungfrau", "tokens": ["So", "schrie", "die", "sch\u00f6\u00b7ne", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "In ihrem Jammer um mich.", "tokens": ["In", "ih\u00b7rem", "Jam\u00b7mer", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.24": {"line.1": {"text": "O Gott, er ist gestorben!", "tokens": ["O", "Gott", ",", "er", "ist", "ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Himmel, er ist todt!", "tokens": ["O", "Him\u00b7mel", ",", "er", "ist", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah sein Herz zerrissen", "tokens": ["Ich", "sah", "sein", "Herz", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Traum, war blutigroth!", "tokens": ["Im", "Traum", ",", "war", "blu\u00b7ti\u00b7groth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Da ward es in ihrer Seele", "tokens": ["Da", "ward", "es", "in", "ih\u00b7rer", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gar feierlich, gar still,", "tokens": ["Gar", "fei\u00b7er\u00b7lich", ",", "gar", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ward der Guten zu Muthe", "tokens": ["Da", "ward", "der", "Gu\u00b7ten", "zu", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Einer, die sterben will.", "tokens": ["Wie", "Ei\u00b7ner", ",", "die", "ster\u00b7ben", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.26": {"line.1": {"text": "Sie glaubte zu zerflie\u00dfen", "tokens": ["Sie", "glaub\u00b7te", "zu", "zer\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins weite herrliche All,", "tokens": ["Ins", "wei\u00b7te", "herr\u00b7li\u00b7che", "All", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aus lichten Fernen vernahm sie", "tokens": ["Aus", "lich\u00b7ten", "Fer\u00b7nen", "ver\u00b7nahm", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unendlich s\u00fc\u00dfen Schall.", "tokens": ["Un\u00b7end\u00b7lich", "s\u00fc\u00b7\u00dfen", "Schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Sie faltete die H\u00e4nde", "tokens": ["Sie", "fal\u00b7te\u00b7te", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf ihrer Brust, sie sang", "tokens": ["Auf", "ih\u00b7rer", "Brust", ",", "sie", "sang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihrer Engelsstimme", "tokens": ["Mit", "ih\u00b7rer", "En\u00b7gels\u00b7stim\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unendlich s\u00fc\u00dfen Klang.", "tokens": ["Un\u00b7end\u00b7lich", "s\u00fc\u00b7\u00dfen", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "\u00bbwie wird mir, ach, auf einmal?", "tokens": ["\u00bb", "wie", "wird", "mir", ",", "ach", ",", "auf", "ein\u00b7mal", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "$,", "ITJ", "$,", "APPR", "ADV", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "So wohl, so leicht, so frei!", "tokens": ["So", "wohl", ",", "so", "leicht", ",", "so", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ob zu seliger Wonne", "tokens": ["Als", "ob", "zu", "se\u00b7li\u00b7ger", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich ganz genesen sei!", "tokens": ["Ich", "ganz", "ge\u00b7ne\u00b7sen", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Ich f\u00fchle mich so gl\u00fccklich,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "so", "gl\u00fcck\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie nie ich gl\u00fccklich war,", "tokens": ["Wie", "nie", "ich", "gl\u00fcck\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich fasset ein Entz\u00fccken,", "tokens": ["Mich", "fas\u00b7set", "ein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entz\u00fccken wunderbar.", "tokens": ["Ent\u00b7z\u00fc\u00b7cken", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Ich f\u00fchle mich, ich f\u00fchle", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", ",", "ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ihm, ach ihm vereint,", "tokens": ["Mit", "ihm", ",", "ach", "ihm", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "XY", "PPER", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich hab ihn wiedergewonnen,", "tokens": ["Ich", "hab", "ihn", "wie\u00b7der\u00b7ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um den ich hei\u00df geweint!\u00ab", "tokens": ["Um", "den", "ich", "hei\u00df", "ge\u00b7weint", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Du lagst in Fiebertr\u00e4umen", "tokens": ["Du", "lagst", "in", "Fie\u00b7ber\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So krank und fern von mir,", "tokens": ["So", "krank", "und", "fern", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und eines sch\u00f6nen Abends", "tokens": ["Und", "ei\u00b7nes", "sch\u00f6\u00b7nen", "A\u00b7bends"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00e4rst du gestorben schier.", "tokens": ["W\u00e4rst", "du", "ge\u00b7stor\u00b7ben", "schier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.32": {"line.1": {"text": "Und kaum war dunkle Kunde", "tokens": ["Und", "kaum", "war", "dunk\u00b7le", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gedrungen in mein Haus,", "tokens": ["Ge\u00b7drun\u00b7gen", "in", "mein", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hielt es mich nicht l\u00e4nger,", "tokens": ["Da", "hielt", "es", "mich", "nicht", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da trieb es mich hinaus!", "tokens": ["Da", "trieb", "es", "mich", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APZR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "In deine lieben Arme,", "tokens": ["In", "dei\u00b7ne", "lie\u00b7ben", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An deinen lieben Mund \u2013", "tokens": ["An", "dei\u00b7nen", "lie\u00b7ben", "Mund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der starke Geist, der Wille", "tokens": ["Der", "star\u00b7ke", "Geist", ",", "der", "Wil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie machten mich gesund.", "tokens": ["Sie", "mach\u00b7ten", "mich", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.34": {"line.1": {"text": "O Liebchen, theures Leben,", "tokens": ["O", "Lieb\u00b7chen", ",", "theu\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Freudenthr\u00e4nen stehn", "tokens": ["Die", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "stehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In meinen Augen, denk ich", "tokens": ["In", "mei\u00b7nen", "Au\u00b7gen", ",", "denk", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An jenes \u2013 Wiedersehn!", "tokens": ["An", "je\u00b7nes", "\u2013", "Wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "$(", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}