{"dta.poem.3933": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "23) Schreiben an einen Freund,  \n  den er zum Heyrathen/ im Nahmen eines andern/  \n anmahnen muste.   Oder:  \n Der G\u00f6ttin  Venus  Steuer-Stube.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe hegt das Steuer-Amt,", "tokens": ["Die", "Lie\u00b7be", "hegt", "das", "Steu\u00b7er\u00b7Amt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das von der G\u00f6ttin ", "tokens": ["Das", "von", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und ihrem g\u00fctig sch\u00f6nen Wollen", "tokens": ["Und", "ih\u00b7rem", "g\u00fc\u00b7tig", "sch\u00f6\u00b7nen", "Wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df man den Abtrag redlich zollen.", "tokens": ["Mu\u00df", "man", "den", "Ab\u00b7trag", "red\u00b7lich", "zol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Steuer-St\u00fcbgen ist geputzt,", "tokens": ["Das", "Steu\u00b7er\u00b7St\u00fcb\u00b7gen", "ist", "ge\u00b7putzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So, da\u00df ", "tokens": ["So", ",", "da\u00df"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die Anmuth wohnt in ihrem Zimmer", "tokens": ["Die", "An\u00b7muth", "wohnt", "in", "ih\u00b7rem", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit hellsten Glantz, und Pracht, und Schimmer,", "tokens": ["Mit", "hells\u00b7ten", "Glantz", ",", "und", "Pracht", ",", "und", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ja ihre grosse Rechen-Kunst", "tokens": ["Ja", "ih\u00b7re", "gros\u00b7se", "Re\u00b7chen\u00b7Kunst"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Verdienet gantz besondre Gunst.", "tokens": ["Ver\u00b7die\u00b7net", "gantz", "be\u00b7sond\u00b7re", "Gunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie hegt kein ", "tokens": ["Sie", "hegt", "kein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Sie wei\u00df die Rechnung schon zu f\u00fchren,", "tokens": ["Sie", "wei\u00df", "die", "Rech\u00b7nung", "schon", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Nach dreyen vierthel Jahren raus,", "tokens": ["Nach", "drey\u00b7en", "vier\u00b7thel", "Jah\u00b7ren", "raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Denn sie verfehlet keine Gasse,", "tokens": ["Denn", "sie", "ver\u00b7feh\u00b7let", "kei\u00b7ne", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Vom Neumarck bis zur Ritter-Strasse.", "tokens": ["Vom", "Neu\u00b7marck", "bis", "zur", "Rit\u00b7ter\u00b7Stras\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So bald sie in das Aemtgen kam,", "tokens": ["So", "bald", "sie", "in", "das", "A\u00b7emt\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "So kriegte sie die Fleisch-Einnahm.", "tokens": ["So", "krieg\u00b7te", "sie", "die", "FleischEin\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Sie eiffert alles abzuf\u00fchren,", "tokens": ["Sie", "eif\u00b7fert", "al\u00b7les", "ab\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Um keinen Creutzer zu verliehren,", "tokens": ["Um", "kei\u00b7nen", "Creut\u00b7zer", "zu", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Auch auf ein Haar mu\u00df alles seyn,", "tokens": ["Auch", "auf", "ein", "Haar", "mu\u00df", "al\u00b7les", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sie schreibt mit eignen H\u00e4nden ein;", "tokens": ["Sie", "schreibt", "mit", "eig\u00b7nen", "H\u00e4n\u00b7den", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Gesetzt, da\u00df einer was versehen,", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "ei\u00b7ner", "was", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PIS", "PWS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "So mu\u00df er in den Schuld-Thurm gehen,", "tokens": ["So", "mu\u00df", "er", "in", "den", "Schuld\u00b7Thurm", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wo nicht, so tr\u00e4get er davon", "tokens": ["Wo", "nicht", ",", "so", "tr\u00e4\u00b7get", "er", "da\u00b7von"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die sch\u00e4rffste ", "tokens": ["Die", "sch\u00e4rffs\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Herr Peter mit dem langen Degen", "tokens": ["Herr", "Pe\u00b7ter", "mit", "dem", "lan\u00b7gen", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wird sich zu ihn auf Schulden legen,", "tokens": ["Wird", "sich", "zu", "ihn", "auf", "Schul\u00b7den", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und da verliehrt er, wenn er dumm/", "tokens": ["Und", "da", "ver\u00b7liehrt", "er", ",", "wenn", "er", "dumm", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sein gantzes ", "tokens": ["Sein", "gant\u00b7zes"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Herr Bruder, ", "tokens": ["Herr", "Bru\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.32": {"text": "Wo nicht, so mu\u00df sie ", "tokens": ["Wo", "nicht", ",", "so", "mu\u00df", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VMFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "Schon drey\u00dfig Jahre, wie bekannt,", "tokens": ["Schon", "drey\u00b7\u00dfig", "Jah\u00b7re", ",", "wie", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Lebst du im Junggesellen-Stand,", "tokens": ["Lebst", "du", "im", "Jung\u00b7ge\u00b7sel\u00b7len\u00b7Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die G\u00f6ttin wird es nimmer leiden,", "tokens": ["Die", "G\u00f6t\u00b7tin", "wird", "es", "nim\u00b7mer", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Drum folge ihrem Ruff beyzeiten,", "tokens": ["Drum", "fol\u00b7ge", "ih\u00b7rem", "Ruff", "bey\u00b7zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Mit Schaden ist ihr nichts gedient,", "tokens": ["Mit", "Scha\u00b7den", "ist", "ihr", "nichts", "ge\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Erwehle dir ein sch\u00f6nes Kind.", "tokens": ["Er\u00b7weh\u00b7le", "dir", "ein", "sch\u00f6\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Geh und erlege ihre Steuer,", "tokens": ["Geh", "und", "er\u00b7le\u00b7ge", "ih\u00b7re", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Wo m\u00f6glich, liebster Freund, noch heuer,", "tokens": ["Wo", "m\u00f6g\u00b7lich", ",", "liebs\u00b7ter", "Freund", ",", "noch", "heu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Denn hast du ihre Gunst und Huld.", "tokens": ["Denn", "hast", "du", "ih\u00b7re", "Gunst", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Freund, stecke dich in keine Schuld,", "tokens": ["Freund", ",", "ste\u00b7cke", "dich", "in", "kei\u00b7ne", "Schuld", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Entrichte ", "tokens": ["Ent\u00b7rich\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.44": {"text": "Sey k\u00fcnfftig nicht mehr Junggeselle.", "tokens": ["Sey", "k\u00fcnff\u00b7tig", "nicht", "mehr", "Jung\u00b7ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}