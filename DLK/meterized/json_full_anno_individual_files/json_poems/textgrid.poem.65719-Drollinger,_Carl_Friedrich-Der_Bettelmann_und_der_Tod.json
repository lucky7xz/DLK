{"textgrid.poem.65719": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Bettelmann und der Tod", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bettelmann warf seine Kr\u00fccke", "tokens": ["Ein", "Bet\u00b7tel\u00b7mann", "warf", "sei\u00b7ne", "Kr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Unmuhts in den tiefen Rhein,", "tokens": ["Voll", "Un\u00b7muhts", "in", "den", "tie\u00b7fen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach, erz\u00f6rnt auf sein Geschicke,", "tokens": ["Und", "sprach", ",", "er\u00b7z\u00f6rnt", "auf", "sein", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Tod, verk\u00fcrze meine Pein!", "tokens": ["O", "Tod", ",", "ver\u00b7k\u00fcr\u00b7ze", "mei\u00b7ne", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tod erschien ihm aus Erbarmen.", "tokens": ["Der", "Tod", "er\u00b7schien", "ihm", "aus", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ey, sprach der Bettler, bist du hier.", "tokens": ["Ey", ",", "sprach", "der", "Bett\u00b7ler", ",", "bist", "du", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Trost und Stab entfiel mir Armen,", "tokens": ["Mein", "Trost", "und", "Stab", "ent\u00b7fiel", "mir", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach schwimm ihm nach, und hohl ihn mir.", "tokens": ["Ach", "schwimm", "ihm", "nach", ",", "und", "hohl", "ihn", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Bettelmann warf seine Kr\u00fccke", "tokens": ["Ein", "Bet\u00b7tel\u00b7mann", "warf", "sei\u00b7ne", "Kr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Unmuhts in den tiefen Rhein,", "tokens": ["Voll", "Un\u00b7muhts", "in", "den", "tie\u00b7fen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach, erz\u00f6rnt auf sein Geschicke,", "tokens": ["Und", "sprach", ",", "er\u00b7z\u00f6rnt", "auf", "sein", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"]