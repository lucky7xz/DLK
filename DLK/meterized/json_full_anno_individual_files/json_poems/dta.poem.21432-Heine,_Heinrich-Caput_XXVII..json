{"dta.poem.21432": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schon knospet die Jugend, welche versteht", "tokens": ["Schon", "knos\u00b7pet", "die", "Ju\u00b7gend", ",", "wel\u00b7che", "ver\u00b7steht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Dichters Stolz und G\u00fcte,", "tokens": ["Des", "Dich\u00b7ters", "Stolz", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sich an seinem Herzen w\u00e4rmt,", "tokens": ["Und", "sich", "an", "sei\u00b7nem", "Her\u00b7zen", "w\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An seinem Sonnengem\u00fcthe.", "tokens": ["An", "sei\u00b7nem", "Son\u00b7nen\u00b7ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist liebend wie das Licht,", "tokens": ["Mein", "Herz", "ist", "lie\u00b7bend", "wie", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rein und keusch wie das Feuer;", "tokens": ["Und", "rein", "und", "keusch", "wie", "das", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die edelsten Grazien haben gestimmt", "tokens": ["Die", "e\u00b7dels\u00b7ten", "Gra\u00b7zi\u00b7en", "ha\u00b7ben", "ge\u00b7stimmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Saiten meiner Leyer.", "tokens": ["Die", "Sai\u00b7ten", "mei\u00b7ner", "Le\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Es ist dieselbe Leyer, die einst", "tokens": ["Es", "ist", "die\u00b7sel\u00b7be", "Le\u00b7yer", ",", "die", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Vater lie\u00df ert\u00f6nen,", "tokens": ["Mein", "Va\u00b7ter", "lie\u00df", "er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der selige Herr Aristophanes,", "tokens": ["Der", "se\u00b7li\u00b7ge", "Herr", "A\u00b7ris\u00b7to\u00b7pha\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Der Liebling der Kam\u00f6nen.", "tokens": ["Der", "Lieb\u00b7ling", "der", "Ka\u00b7m\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist die Leyer, worauf er einst", "tokens": ["Es", "ist", "die", "Le\u00b7yer", ",", "wo\u00b7rauf", "er", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Paisteteros besungen,", "tokens": ["Den", "Pais\u00b7te\u00b7te\u00b7ros", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der um die Basileia gefreyt,", "tokens": ["Der", "um", "die", "Ba\u00b7si\u00b7lei\u00b7a", "ge\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit ihr sich emporgeschwungen.", "tokens": ["Mit", "ihr", "sich", "em\u00b7por\u00b7ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}