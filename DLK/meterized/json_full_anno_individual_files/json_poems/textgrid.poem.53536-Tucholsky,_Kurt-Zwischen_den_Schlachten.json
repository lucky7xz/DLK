{"textgrid.poem.53536": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Zwischen den Schlachten", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leidige Politika!", "tokens": ["Lei\u00b7di\u00b7ge", "Po\u00b7li\u00b7ti\u00b7ka", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Clementine, s\u00fc\u00dfer Fetzen!", "tokens": ["Cle\u00b7men\u00b7ti\u00b7ne", ",", "s\u00fc\u00b7\u00dfer", "Fet\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich mich an dir ergetzen \u2013", "tokens": ["La\u00df", "mich", "mich", "an", "dir", "er\u00b7get\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "bin so wild, seit ich dich sah,", "tokens": ["bin", "so", "wild", ",", "seit", "ich", "dich", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Venus Amathusia!", "tokens": ["Ve\u00b7nus", "A\u00b7mat\u00b7hu\u00b7sia", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "M\u00e4dchen mit dem kleinen Ohr,", "tokens": ["M\u00e4d\u00b7chen", "mit", "dem", "klei\u00b7nen", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "mit den ma\u00dfvoll fetten Beinen,", "tokens": ["mit", "den", "ma\u00df\u00b7voll", "fet\u00b7ten", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sieh vor Lust mich leise weinen,", "tokens": ["sieh", "vor", "Lust", "mich", "lei\u00b7se", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ein verliebter hei\u00dfer Tor . . .", "tokens": ["ein", "ver\u00b7lieb\u00b7ter", "hei\u00b7\u00dfer", "Tor", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hogarth nennt dies Bild: Before.", "tokens": ["Ho\u00b7garth", "nennt", "dies", "Bild", ":", "Be\u00b7fo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber eine Nacht darauf?", "tokens": ["A\u00b7ber", "ei\u00b7ne", "Nacht", "da\u00b7rauf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigt dein Troubadour und schlaft er?", "tokens": ["Schweigt", "dein", "Trou\u00b7ba\u00b7dour", "und", "schlaft", "er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hogarth nennt dies Bildchen: After.", "tokens": ["Ho\u00b7garth", "nennt", "dies", "Bild\u00b7chen", ":", "Af\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh, das ist der Welten Lauf \u2013", "tokens": ["Sieh", ",", "das", "ist", "der", "Wel\u00b7ten", "Lauf", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "hebst du die Gef\u00fchle auf?", "tokens": ["hebst", "du", "die", "Ge\u00b7f\u00fch\u00b7le", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald bin ich dir wieder nah.", "tokens": ["Bald", "bin", "ich", "dir", "wie\u00b7der", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau, ich kann nur manchmal l\u00fcgen.", "tokens": ["Schau", ",", "ich", "kann", "nur", "manch\u00b7mal", "l\u00fc\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du tusts stets in vollen Z\u00fcgen.", "tokens": ["Du", "tusts", "stets", "in", "vol\u00b7len", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df dir nur an mir gen\u00fcgen", "tokens": ["La\u00df", "dir", "n