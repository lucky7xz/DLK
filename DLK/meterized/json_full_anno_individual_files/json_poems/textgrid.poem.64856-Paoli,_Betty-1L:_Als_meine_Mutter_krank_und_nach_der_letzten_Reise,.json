{"textgrid.poem.64856": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als meine Mutter krank und nach der letzten Reise,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als meine Mutter krank und nach der letzten Reise,", "tokens": ["Als", "mei\u00b7ne", "Mut\u00b7ter", "krank", "und", "nach", "der", "letz\u00b7ten", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ward ver\u00e4ndert viel auf mannigfache Weise.", "tokens": ["Da", "ward", "ver\u00b7\u00e4n\u00b7dert", "viel", "auf", "man\u00b7nig\u00b7fa\u00b7che", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zuerst befahl der Arzt, die Blumen wegzutragen,", "tokens": ["Zu\u00b7erst", "be\u00b7fahl", "der", "Arzt", ",", "die", "Blu\u00b7men", "weg\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gerne sie gepflegt in fr\u00fchern bessern Tagen.", "tokens": ["Die", "ger\u00b7ne", "sie", "ge\u00b7pflegt", "in", "fr\u00fc\u00b7hern", "bes\u00b7sern", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVPP", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann ward dem Tageslicht der Eingang auch verwehrt \u2013", "tokens": ["Dann", "ward", "dem", "Ta\u00b7ges\u00b7licht", "der", "Ein\u00b7gang", "auch", "ver\u00b7wehrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es hie\u00df, damit die Ruh der Kranken ungest\u00f6rt.", "tokens": ["Es", "hie\u00df", ",", "da\u00b7mit", "die", "Ruh", "der", "Kran\u00b7ken", "un\u00b7ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und als der Priester kam, die Hostie ihr zu reichen,", "tokens": ["Und", "als", "der", "Pries\u00b7ter", "kam", ",", "die", "Hos\u00b7tie", "ihr", "zu", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da mu\u00dfte selbst ihr Kind aus ihrem Zimmer weichen.", "tokens": ["Da", "mu\u00df\u00b7te", "selbst", "ihr", "Kind", "aus", "ih\u00b7rem", "Zim\u00b7mer", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So, losgerissen l\u00e4ngst, und l\u00e4ngst schon im Entschweben", "tokens": ["So", ",", "los\u00b7ge\u00b7