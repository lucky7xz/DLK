{"textgrid.poem.36181": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was sind der Volkskraft Sehnen? Was die Adern", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sind der Volkskraft Sehnen? Was die Adern", "tokens": ["Was", "sind", "der", "Volks\u00b7kraft", "Seh\u00b7nen", "?", "Was", "die", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$.", "PWS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des B\u00fcrgerthums? Was dessen Band und Kranz?", "tokens": ["Des", "B\u00fcr\u00b7ger\u00b7thums", "?", "Was", "des\u00b7sen", "Band", "und", "Kranz", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "PDS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sind es die Mauern? Sinds die starren Quadern?", "tokens": ["Sind", "es", "die", "Mau\u00b7ern", "?", "Sinds", "die", "star\u00b7ren", "Qua\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ists der Pall\u00e4ste Prunk? der M\u00fcnster Glanz?", "tokens": ["Ists", "der", "Pal\u00b7l\u00e4s\u00b7te", "Prunk", "?", "der", "M\u00fcns\u00b7ter", "Glanz", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein, Treu' und Glaub' ists, Eintracht und Vertrauen,", "tokens": ["Nein", ",", "Treu'", "und", "Glaub'", "ists", ",", "Ein\u00b7tracht", "und", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "VAFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der sch\u00f6ne Sinn, der keine Opfer scheu't;", "tokens": ["Der", "sch\u00f6\u00b7ne", "Sinn", ",", "der", "kei\u00b7ne", "Op\u00b7fer", "scheu't", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Jugend frischer Muth, die strenge Zucht der Frauen,", "tokens": ["Der", "Ju\u00b7gend", "fri\u00b7scher", "Muth", ",", "die", "stren\u00b7ge", "Zucht", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Alten heit'rer Ernst, der sich des Jenseits freut!", "tokens": ["Der", "Al\u00b7ten", "heit'\u00b7rer", "Ernst", ",", "der", "sich", "des", "Jen\u00b7seits", "freut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wohl uns! Nicht gar verschwand aus uns'rer Mitte", "tokens": ["Wohl", "uns", "!", "Nicht", "gar", "ver\u00b7schwand", "aus", "un\u00b7s'\u00b7rer", "Mit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "PTKNEG", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Tugend, die uns von den V\u00e4tern kam:", "tokens": ["Die", "Tu\u00b7gend", ",", "die", "uns", "von", "den", "V\u00e4\u00b7tern", "kam", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht gar verloren ging der Ahnen Sitte;", "tokens": ["Nicht", "gar", "ver\u00b7lo\u00b7ren", "ging", "der", "Ah\u00b7nen", "Sit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht gar zur Fabel ward uns Zucht und Scham.", "tokens": ["Nicht", "gar", "zur", "Fa\u00b7bel", "ward", "uns", "Zucht", "und", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch mangeln nicht die M\u00e4nner unserm Kreise,", "tokens": ["Noch", "man\u00b7geln", "nicht", "die", "M\u00e4n\u00b7ner", "un\u00b7serm", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In deren Brust das heil'ge Feuer brennt;", "tokens": ["In", "de\u00b7ren", "Brust", "das", "heil'\u00b7ge", "Feu\u00b7er", "brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch gr\u00fcnen da und dort die ehrenwerthen Greise,", "tokens": ["Noch", "gr\u00fc\u00b7nen", "da", "und", "dort", "die", "eh\u00b7ren\u00b7wert\u00b7hen", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die achtend ihre Zeit, preisend die Nachwelt nennt.", "tokens": ["Die", "ach\u00b7tend", "ih\u00b7re", "Zeit", ",", "prei\u00b7send", "die", "Nach\u00b7welt", "nennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "PPOSAT", "NN", "$,", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Zu solchen z\u00e4hlt, Heil Ihm! auch unser ", "tokens": ["Zu", "sol\u00b7chen", "z\u00e4hlt", ",", "Heil", "Ihm", "!", "auch", "un\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "$,", "NN", "PPER", "$.", "ADV", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der G'rade, Brave, Wackre, W\u00fcrdige;", "tokens": ["Der", "G'\u00b7ra\u00b7de", ",", "Bra\u00b7ve", ",", "Wack\u00b7re", ",", "W\u00fcr\u00b7di\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Mann von biedrem Sinn und lautrer Seele,", "tokens": ["Der", "Mann", "von", "bied\u00b7rem", "Sinn", "und", "laut\u00b7rer", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der, wie er sprach, auch dacht' und handelte;", "tokens": ["Der", ",", "wie", "er", "sprach", ",", "auch", "dacht'", "und", "han\u00b7del\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er, der durch funfzig arbeitsvolle Jahre", "tokens": ["Er", ",", "der", "durch", "funf\u00b7zig", "ar\u00b7beits\u00b7vol\u00b7le", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Kraft und Treue des Berufs gepflegt,", "tokens": ["Mit", "Kraft", "und", "Treu\u00b7e", "des", "Be\u00b7rufs", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem trotz des Alters Frost, und trotz dem Reif der Haare,", "tokens": ["Dem", "trotz", "des", "Al\u00b7ters", "Frost", ",", "und", "trotz", "dem", "Reif", "der", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein vaterl\u00e4ndisch Herz im warmen Busen schl\u00e4gt.", "tokens": ["Ein", "va\u00b7ter\u00b7l\u00e4n\u00b7disch", "Herz", "im", "war\u00b7men", "Bu\u00b7sen", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihm sprudelt noch der reiche Born der Kr\u00e4fte,", "tokens": ["Ihm", "spru\u00b7delt", "noch", "der", "rei\u00b7che", "Born", "der", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Ihn gest\u00e4rkt, wo mindre Kraft erlag.", "tokens": ["Der", "Ihn", "ge\u00b7st\u00e4rkt", ",", "wo", "mind\u00b7re", "Kraft", "er\u00b7lag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er pflegt, wie sonst, der l\u00f6blichen Gesch\u00e4fte", "tokens": ["Er", "pflegt", ",", "wie", "sonst", ",", "der", "l\u00f6b\u00b7li\u00b7chen", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit jugendlichem Eifer Tag vyr Tag.", "tokens": ["Mit", "ju\u00b7gend\u00b7li\u00b7chem", "Ei\u00b7fer", "Tag", "vyr", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schon mehr denn einmal ward um Ihn erneuert", "tokens": ["Schon", "mehr", "denn", "ein\u00b7mal", "ward", "um", "Ihn", "er\u00b7neu\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Sterblichen hinf\u00e4lliges Geschlecht.", "tokens": ["Der", "Sterb\u00b7li\u00b7chen", "hin\u00b7f\u00e4l\u00b7li\u00b7ges", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Licht, was jenen schwand, gl\u00e4nzt Ihm noch unverschleiert;", "tokens": ["Das", "Licht", ",", "was", "je\u00b7nen", "schwand", ",", "gl\u00e4nzt", "Ihm", "noch", "un\u00b7ver\u00b7schlei\u00b7ert", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PDS", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie ruhn! Er wirkt und schafft rastlos und ungeschw\u00e4cht.", "tokens": ["Sie", "ruhn", "!", "Er", "wirkt", "und", "schafft", "rast\u00b7los", "und", "un\u00b7ge\u00b7schw\u00e4cht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sey uns gegr\u00fc\u00dft dann zu dem Doppelfeste,", "tokens": ["Sey", "uns", "ge\u00b7gr\u00fc\u00dft", "dann", "zu", "dem", "Dop\u00b7pel\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wenigen des Schicksals Gunst gew\u00e4hrt!", "tokens": ["Das", "We\u00b7ni\u00b7gen", "des", "Schick\u00b7sals", "Gunst", "ge\u00b7w\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir bringen Dir das Liebst' und Sch\u00f6nst' und Beste,", "tokens": ["Wir", "brin\u00b7gen", "Dir", "das", "Liebst'", "und", "Sch\u00f6nst'", "und", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was uns der Gott zu Trost und Lust beschert:", "tokens": ["Was", "uns", "der", "Gott", "zu", "Trost", "und", "Lust", "be\u00b7schert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein herzlich Lied, ein Deutsches H\u00e4ndedr\u00fccken,", "tokens": ["Ein", "herz\u00b7lich", "Lied", ",", "ein", "Deut\u00b7sches", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gl\u00fcckw\u00fcnsche, die nicht hohler, leerer Trug,", "tokens": ["Gl\u00fcck\u00b7w\u00fcn\u00b7sche", ",", "die", "nicht", "hoh\u00b7ler", ",", "lee\u00b7rer", "Trug", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Augen, die ger\u00fchrt f\u00fcr Dich gen Himmel blicken,", "tokens": ["Und", "Au\u00b7gen", ",", "die", "ge\u00b7r\u00fchrt", "f\u00fcr", "Dich", "gen", "Him\u00b7mel", "bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "VVPP", "APPR", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Herzen sonder Falsch und Lippen sonder Lug.", "tokens": ["Und", "Her\u00b7zen", "son\u00b7der", "Falsch", "und", "Lip\u00b7pen", "son\u00b7der", "Lug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mag sch\u00fctzend \u00fcber Dir auch k\u00fcnftig walten", "tokens": ["Mag", "sch\u00fct\u00b7zend", "\u00fc\u00b7ber", "Dir", "auch", "k\u00fcnf\u00b7tig", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "APPR", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Macht, die liebend Dich bisher gesch\u00fctzt!", "tokens": ["Die", "Macht", ",", "die", "lie\u00b7bend", "Dich", "bis\u00b7her", "ge\u00b7sch\u00fctzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mag sp\u00e4t und lange noch Dich aufrecht halten", "tokens": ["Mag", "sp\u00e4t", "und", "lan\u00b7ge", "noch", "Dich", "auf\u00b7recht", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "KON", "ADV", "ADV", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Arm, der h\u00fclfreich Dich bis itzt gest\u00fctzt!", "tokens": ["Der", "Arm", ",", "der", "h\u00fclf\u00b7reich", "Dich", "bis", "itzt", "ge\u00b7st\u00fctzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mag Deines Lebenstages Sonne sinken,", "tokens": ["Mag", "Dei\u00b7nes", "Le\u00b7bens\u00b7ta\u00b7ges", "Son\u00b7ne", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie sie gestiegen, ruhig, roth und rund!", "tokens": ["Wie", "sie", "ge\u00b7stie\u00b7gen", ",", "ru\u00b7hig", ",", "roth", "und", "rund", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Magst Du des Daseyns Wein aus g\u00fcldnem Becher trinken,", "tokens": ["Magst", "Du", "des", "Da\u00b7seyns", "Wein", "aus", "g\u00fcld\u00b7nem", "Be\u00b7cher", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nach wackrer Zecher Art, rein aus bis auf den Grund!", "tokens": ["Nach", "wack\u00b7rer", "Ze\u00b7cher", "Art", ",", "rein", "aus", "bis", "auf", "den", "Grund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "ADJD", "APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "In Frieden, Bester, magst Du niederwandeln", "tokens": ["In", "Frie\u00b7den", ",", "Bes\u00b7ter", ",", "magst", "Du", "nie\u00b7der\u00b7wan\u00b7deln"], "token_info": ["word", "word"