{"textgrid.poem.55017": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Beherzigung", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, was soll der Mensch verlangen?", "tokens": ["Ach", ",", "was", "soll", "der", "Mensch", "ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es besser, ruhig bleiben?", "tokens": ["Ist", "es", "bes\u00b7ser", ",", "ru\u00b7hig", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klammernd fest sich anzuhangen?", "tokens": ["Klam\u00b7mernd", "fest", "sich", "an\u00b7zu\u00b7han\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist es besser, sich zu treiben?", "tokens": ["Ist", "es", "bes\u00b7ser", ",", "sich", "zu", "trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll er sich ein H\u00e4uschen bauen?", "tokens": ["Soll", "er", "sich", "ein", "H\u00e4usc\u00b7hen", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll er unter Zelten leben?", "tokens": ["Soll", "er", "