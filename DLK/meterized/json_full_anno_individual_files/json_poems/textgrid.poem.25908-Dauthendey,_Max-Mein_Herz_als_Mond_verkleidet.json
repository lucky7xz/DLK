{"textgrid.poem.25908": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mein Herz als Mond verkleidet", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchr' im Schlaf an deine Wangen,", "tokens": ["R\u00fchr'", "im", "Schlaf", "an", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hangen Tropfen an den Kissen,", "tokens": ["Han\u00b7gen", "Trop\u00b7fen", "an", "den", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du und ich allein nur wissen:", "tokens": ["Du", "und", "ich", "al\u00b7lein", "nur", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Sehnen hat vereint", "tokens": ["Un\u00b7ser", "Seh\u00b7nen", "hat", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hei\u00df sich in den Schlaf geweint.", "tokens": ["Hei\u00df", "sich", "in", "den", "Schlaf", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, mein Herz wie's liebt und leidet!", "tokens": ["Ach", ",", "mein", "Herz", "wie's", "liebt", "und", "lei\u00b7det", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "NE", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sp\u00fcr es leis als Mond verkleidet", "tokens": ["Sp\u00fcr", "es", "leis", "als", "Mond", "ver\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "KOKOM", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df an deiner T\u00fcr.", "tokens": ["Wei\u00df", "an", "dei\u00b7ner", "T\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sehnsucht mu\u00df mit hellen H\u00e4nden", "tokens": ["Sehn\u00b7sucht", "mu\u00df", "mit", "hel\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch im Schlaf dein Zimmer blenden,", "tokens": ["Noch", "im", "Schlaf", "dein", "Zim\u00b7mer", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die blanken Scheiben schicken", "tokens": ["Und", "die", "blan\u00b7ken", "Schei\u00b7ben", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blicke, die tags dunkel bleiben;", "tokens": ["Bli\u00b7cke", ",", "die", "tags", "dun\u00b7kel", "blei\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo sie ungesehen fielen,", "tokens": ["Wo", "sie", "un\u00b7ge\u00b7se\u00b7hen", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steigen Lichter aus den Dielen.", "tokens": ["Stei\u00b7gen", "Lich\u00b7ter", "aus", "den", "Die\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schweigen m\u00fcssen Uhr und Zeit,", "tokens": ["Schwei\u00b7gen", "m\u00fcs\u00b7sen", "Uhr", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehnsucht spielt auf blauen Geigen,", "tokens": ["Sehn\u00b7sucht", "spielt", "auf", "blau\u00b7en", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie einst auf M\u00e4rzenauen", "tokens": ["Und", "wie", "einst", "auf", "M\u00e4r\u00b7ze\u00b7nau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Werden Balken in den R\u00e4umen", "tokens": ["Wer\u00b7den", "Bal\u00b7ken", "in", "den", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wieder k\u00fchn zu Knospenb\u00e4umen.", "tokens": ["Wie\u00b7der", "k\u00fchn", "zu", "Knos\u00b7pen\u00b7b\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auch taut im Mond wie Eis", "tokens": ["Und", "auch", "taut", "im", "Mond", "wie", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "KOKOM", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Lautlos deines Spiegels Glas,", "tokens": ["Laut\u00b7los", "dei\u00b7nes", "Spie\u00b7gels", "Glas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Will mir Heimlichkeiten zeigen,", "tokens": ["Will", "mir", "Heim\u00b7lich\u00b7kei\u00b7ten", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die der Spiegel nie verga\u00df,", "tokens": ["Die", "der", "Spie\u00b7gel", "nie", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Er, der z\u00e4rtliche Vertraute,", "tokens": ["Er", ",", "der", "z\u00e4rt\u00b7li\u00b7che", "Ver\u00b7trau\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Der nur lebt von deinen Augen", "tokens": ["Der", "nur", "lebt", "von", "dei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und in deine Sehnsucht schaute.", "tokens": ["Und", "in", "dei\u00b7ne", "Sehn\u