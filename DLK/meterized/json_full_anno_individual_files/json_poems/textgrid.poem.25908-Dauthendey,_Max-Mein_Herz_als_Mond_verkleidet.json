ens": ["Auf", "dem", "ro\u00b7ten", "Dach", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6tet sich der Mond gelassen;", "tokens": ["T\u00f6\u00b7tet", "sich", "der", "Mond", "ge\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wer ahnt in lauten Gassen,", "tokens": ["Und", "wer", "ahnt", "in", "lau\u00b7ten", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Da\u00df, wo Sehnsucht hingestellt,", "tokens": ["Da\u00df", ",", "wo", "Sehn\u00b7sucht", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich noch nachts das Pflaster hellt,", "tokens": ["Sich", "noch", "nachts", "das", "Pflas\u00b7ter", "hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mein Herz, als Mond verkleidet,", "tokens": ["Und", "mein", "Herz", ",", "als", "Mond", "ver\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4chtlich blinde W\u00fcnsche weidet.", "tokens": ["N\u00e4cht\u00b7lich", "blin\u00b7de", "W\u00fcn\u00b7sche", "wei\u00b7det", "."], "token_info": ["word"