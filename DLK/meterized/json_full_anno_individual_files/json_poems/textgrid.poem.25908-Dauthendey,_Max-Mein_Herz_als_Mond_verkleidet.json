nd wie Eis", "tokens": ["Und", "auch", "taut", "im", "Mond", "wie", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "KOKOM", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Lautlos deines Spiegels Glas,", "tokens": ["Laut\u00b7los", "dei\u00b7nes", "Spie\u00b7gels", "Glas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Will mir Heimlichkeiten zeigen,", "tokens": ["Will", "mir", "Heim\u00b7lich\u00b7kei\u00b7ten", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die der Spiegel nie verga\u00df,", "tokens": ["Die", "der", "Spie\u00b7gel", "nie", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Er, der z\u00e4rtliche Vertraute,", "tokens": ["Er", ",", "der", "z\u00e4rt\u00b7li\u00b7che", "Ver\u00b7trau\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Der nur lebt von deinen Augen", "tokens": ["Der", "nur", "lebt", "von", "dei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und in deine Sehnsucht schaute.", "tokens": ["Und", "in", "dei\u00b7ne", "Sehn\u00b7sucht", "schau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Dicht an deinen wei\u00dfen Wangen", "tokens": ["Dicht", "an", "dei\u00b7nen", "wei\u00b7\u00dfen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich deinen Atem fangen.", "tokens": ["Will", "ich", "dei\u00b7nen", "A\u00b7tem", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Scham mir nicht gestand,", "tokens": ["Was", "die", "Scham", "mir", "nicht", "ge\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fc\u00df ich aus dem Schlaf der kleinen, zagen, zahmen Hand.", "tokens": ["K\u00fc\u00df", "ich", "aus", "dem", "Schlaf", "der", "klei\u00b7nen", ",", "za\u00b7gen", ",", "zah\u00b7men", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.12": {"line.1": {"text": "R\u00f6tet Morgen sich im Land,", "tokens": ["R\u00f6\u00b7tet", "Mor\u00b7gen", "sich", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem roten Dach der Welt", "tokens": ["Auf", "dem", "ro\u00b7ten", "Dach", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6tet sich der Mond gelassen;", "tokens": ["T\u00f6\u00b7tet", "sich", "der", "Mond", "ge\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wer ahnt in lauten Gassen,", "tokens": ["Und", "wer", "ahnt", "in", "lau\u00b7ten", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Da\u00df, wo Sehnsucht hingestellt,", "tokens": ["Da\u00df", ",", "wo", "Sehn\u00b7sucht", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich noch nachts das Pflaster hellt,", "tokens": ["Sich", "noch", "nachts", "das", "Pflas\u00b7ter", "hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mein Herz, als Mond verkleidet,", "tokens": ["Und", "mein", "Herz", ",", "als", "Mond", "ver\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4chtlich blinde W\u00fcnsche weidet.", "tokens": ["N\u00e4cht\u00b7lich", "blin\u00b7de", "W\u00fcn\u00b7sche", "wei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}