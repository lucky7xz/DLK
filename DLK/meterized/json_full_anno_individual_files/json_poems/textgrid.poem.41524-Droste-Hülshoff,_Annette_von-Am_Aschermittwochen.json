"text": "Ich fahr' zur\u00fcck", "tokens": ["Ich", "fahr'", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vor dem, was ich einst bleibe,", "tokens": ["Vor", "dem", ",", "was", "ich", "einst", "blei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und werd' es selber noch,", "tokens": ["Und", "werd'", "es", "sel\u00b7ber", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wei\u00df es schon,", "tokens": ["Und", "wei\u00df", "es", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und trag' es selber doch", "tokens": ["Und", "trag'", "es", "sel\u00b7ber", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zu bitterm Hohn", "tokens": ["Zu", "bit\u00b7term", "Hohn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Im eignen Leibe!", "tokens": ["Im", "eig\u00b7nen", "Lei\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "F\u00fchl' ich des Pulses Schlag", "tokens": ["F\u00fchl'", "ich", "des", "Pul\u00b7ses", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In meiner Hand,", "tokens": ["In", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wor\u00fcber sinn' ich nach?", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "sinn'", "ich", "nach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O leerer Tand:", "tokens": ["O", "lee\u00b7rer", "Tand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ob ich gesunde!", "tokens": ["Ob", "ich", "ge\u00b7sun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und denke nicht bet\u00f6rt,", "tokens": ["Und", "den\u00b7ke", "nicht", "be\u00b7t\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df f\u00fcr und f\u00fcr", "tokens": ["Da\u00df", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "KON", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ein jeder Pulsschlag zehrt", "tokens": ["Ein", "je\u00b7der", "Puls\u00b7schlag", "zehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Am Leben mir,", "tokens": ["Am", "Le\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schl\u00e4gt Todeswunde!", "tokens": ["Schl\u00e4gt", "To\u00b7des\u00b7wun\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Du schn\u00f6der K\u00f6rper, der", "tokens": ["Du", "schn\u00f6\u00b7der", "K\u00f6r\u00b7per", ",", "der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Mich oft verf\u00fchrt,", "tokens": ["Mich", "oft", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit Welt und S\u00fcnde schwer", "tokens": ["Mit", "Welt", "und", "S\u00fcn\u00b7de", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz ger\u00fchrt,", "tokens": ["Mein", "Herz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Noch hast du Leben!", "tokens": ["Noch", "hast", "du", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bald liegst du starr wie Eis,", "tokens": ["Bald", "liegst", "du", "starr", "wie", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der W\u00fcrmer Spott,", "tokens": ["Der", "W\u00fcr\u00b7mer", "Spott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Den Elementen preis;", "tokens": ["Den", "E\u00b7le\u00b7men\u00b7ten", "preis", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "O, m\u00f6ge Gott", "tokens": ["O", ",", "m\u00f6\u00b7ge", "Gott"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "VMFIN", "NN"], "meter": "---+", "measure": "unknown.measure.single"}, "line.10": {"text": "Die Seele heben!", "tokens": ["Die", "See\u00b7le", "he\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}