{"textgrid.poem.41524": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Aschermittwochen", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf meiner Stirn dies Kreuz", "tokens": ["Auf", "mei\u00b7ner", "Stirn", "dies", "Kreuz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PDS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von Asche grau!", "tokens": ["Von", "A\u00b7sche", "grau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O schn\u00f6der Lebensreiz,", "tokens": ["O", "schn\u00f6\u00b7der", "Le\u00b7bens\u00b7reiz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie bist du schlau,", "tokens": ["Wie", "bist", "du", "schlau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Uns zu betr\u00fcgen!", "tokens": ["Uns", "zu", "be\u00b7tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit Farben hell und bunt,", "tokens": ["Mit", "Far\u00b7ben", "hell", "und", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit wei\u00df und rot,", "tokens": ["Mit", "wei\u00df", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Deckst du des Moders Grund,", "tokens": ["Deckst", "du", "des", "Mo\u00b7ders", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dann k\u00f6mmt der Tod", "tokens": ["Dann", "k\u00f6mmt", "der", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und straft dich L\u00fcgen!", "tokens": ["Und", "straft", "dich", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und wer es nicht bedacht", "tokens": ["Und", "wer", "es", "nicht", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und wohl gewu\u00dft,", "tokens": ["Und", "wohl", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sein Leben hingelacht", "tokens": ["Sein", "Le\u00b7ben", "hin\u00b7ge\u00b7lacht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In eitler Lust,", "tokens": ["In", "eit\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der mu\u00df dann weinen,", "tokens": ["Der", "mu\u00df", "dann", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Er achtet nicht was lieb,", "tokens": ["Er", "ach\u00b7tet", "nicht", "was", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und was ihm wert,", "tokens": ["Und", "was", "ihm", "wert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Das flieht ihn wie ein Dieb,", "tokens": ["Das", "flieht", "ihn", "wie", "ein", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "F\u00e4llt ab zu Erd'", "tokens": ["F\u00e4llt", "ab", "zu", "Erd'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und zu Gebeinen.", "tokens": ["Und", "zu", "Ge\u00b7bei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Was schm\u00fcckt sich denn so hold", "tokens": ["Was", "schm\u00fcckt", "sich", "denn", "so", "hold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In bunter Seid'?", "tokens": ["In", "bun\u00b7ter", "Seid'", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was tritt einher in Gold", "tokens": ["Was", "tritt", "ein\u00b7her", "in", "Gold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Perlgeschmeid'?", "tokens": ["Und", "Perl\u00b7ge\u00b7schmeid'", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O Herr, ich hasche", "tokens": ["O", "Herr", ",", "ich", "ha\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nach allem was nicht gut,", "tokens": ["Nach", "al\u00b7lem", "was", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nach Wahn und Traum,", "tokens": ["Nach", "Wahn", "und", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und h\u00e4nge Erd' und Blut", "tokens": ["Und", "h\u00e4n\u00b7ge", "Erd'", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und Meeresschaum", "tokens": ["Und", "Mee\u00b7res\u00b7schaum"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Um bunte Asche!", "tokens": ["Um", "bun\u00b7te", "A\u00b7sche", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Was wird so hei\u00df geliebt?", "tokens": ["Was", "wird", "so", "hei\u00df", "ge\u00b7liebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was legt in Band,", "tokens": ["Was", "legt", "in", "Band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ob's gleich nur Schmerzen gibt,", "tokens": ["Ob's", "gleich", "nur", "Schmer\u00b7zen", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sinn und Verstand?", "tokens": ["Sinn", "und", "Ver\u00b7stand", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "O Herr verzeihe!", "tokens": ["O", "Herr", "ver\u00b7zei\u00b7he", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Seele minnt man nicht,", "tokens": ["Die", "See\u00b7le", "minnt", "man", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die edle Braut,", "tokens": ["Die", "ed\u00b7le", "Braut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und wagt um ein Gesicht,", "tokens": ["Und", "wagt", "um", "ein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Aus Staub gebaut,", "tokens": ["Aus", "Staub", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Die ew'ge Reue!", "tokens": ["Die", "ew'\u00b7ge", "Reu\u00b7e", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Stellt ein Geripp' sich dar", "tokens": ["Stellt", "ein", "Ge\u00b7ripp'", "sich", "dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor meinem Blick,", "tokens": ["Vor", "mei\u00b7nem", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So str\u00e4ubt sich mir das Haar,", "tokens": ["So", "str\u00e4ubt", "sich", "mir", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich fahr' zur\u00fcck", "tokens": ["Ich", "fahr'", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vor dem, was ich einst bleibe,", "tokens": ["Vor", "dem", ",", "was", "ich", "einst", "blei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und werd' es selber noch,", "tokens": ["Und", "werd'", "es", "sel\u00b7ber", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wei\u00df es schon,", "tokens": ["Und", "wei\u00df", "es", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und trag' es selber doch", "tokens": ["Und", "trag'", "es", "sel\u00b7ber", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zu bitterm Hohn", "tokens": ["Zu", "bit\u00b7term", "Hohn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Im eignen Leibe!", "tokens": ["Im", "eig\u00b7nen", "Lei\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "F\u00fchl' ich des Pulses Schlag", "tokens": ["F\u00fchl'", "ich", "des", "Pul\u00b7ses", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In meiner Hand,", "tokens": ["In", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wor\u00fcber sinn' ich nach?", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "sinn'", "ich", "nach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O leerer Tand:", "tokens": ["O", "lee\u00b7rer", "Tand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ob ich gesunde!", "tokens": ["Ob", "ich", "ge\u00b7sun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und denke nicht bet\u00f6rt,", "tokens": ["Und", "den\u00b7ke", "nicht", "be\u00b7t\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df f\u00fcr und f\u00fcr", "tokens": ["Da\u00df", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "KON", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ein jeder Pulsschlag zehrt", "tokens": ["Ein", "je\u00b7der", "Puls\u00b7schlag", "zehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Am Leben mir,", "tokens": ["Am", "Le\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schl\u00e4gt Todeswunde!", "tokens": ["Schl\u00e4gt", "To\u00b7des\u00b7wun\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Du schn\u00f6der K\u00f6rper, der", "tokens": ["Du", "schn\u00f6\u00b7der", "K\u00f6r\u00b7per", ",", "der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Mich oft verf\u00fchrt,", "tokens": ["Mich", "oft", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit Welt und S\u00fcnde schwer", "tokens": ["Mit", "Welt", "und", "S\u00fcn\u00b7de", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz ger\u00fchrt,", "tokens": ["Mein", "Herz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Noch hast du Leben!", "tokens": ["Noch", "hast", "du", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bald liegst du starr wie Eis,", "tokens": ["Bald", "liegst", "du", "starr", "wie", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der W\u00fcrmer Spott,", "tokens": ["Der", "W\u00fcr\u00b7mer", "Spott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Den Elementen preis;", "tokens": ["Den", "E\u00b7le\u00b7men\u00b7ten", "preis", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "O, m\u00f6ge Gott", "tokens": ["O", ",", "m\u00f6\u00b7ge", "Gott"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "VMFIN", "NN"], "meter": "---+", "measure": "unknown.measure.single"}, "line.10": {"text": "Die Seele heben!", "tokens": ["Die", "See\u00b7le", "he\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Auf meiner Stirn dies Kreuz", "tokens": ["Auf", "mei\u00b7ner", "Stirn", "dies", "Kreuz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PDS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von Asche grau!", "tokens": ["Von", "A\u00b7sche", "grau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O schn\u00f6der Lebensreiz,", "tokens": ["O", "schn\u00f6\u00b7der", "Le\u00b7bens\u00b7reiz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie bist du schlau,", "tokens": ["Wie", "bist", "du", "schlau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Uns zu betr\u00fcgen!", "tokens": ["Uns", "zu", "be\u00b7tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit Farben hell und bunt,", "tokens": ["Mit", "Far\u00b7ben", "hell", "und", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit wei\u00df und rot,", "tokens": ["Mit", "wei\u00df", "und", "rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Deckst du des Moders Grund,", "tokens": ["Deckst", "du", "des", "Mo\u00b7ders", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dann k\u00f6mmt der Tod", "tokens": ["Dann", "k\u00f6mmt", "der", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und straft dich L\u00fcgen!", "tokens": ["Und", "straft", "dich", "L\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Und wer es nicht bedacht", "tokens": ["Und", "wer", "es", "nicht", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und wohl gewu\u00dft,", "tokens": ["Und", "wohl", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sein Leben hingelacht", "tokens": ["Sein", "Le\u00b7ben", "hin\u00b7ge\u00b7lacht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In eitler Lust,", "tokens": ["In", "eit\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der mu\u00df dann weinen,", "tokens": ["Der", "mu\u00df", "dann", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Er achtet nicht was lieb,", "tokens": ["Er", "ach\u00b7tet", "nicht", "was", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und was ihm wert,", "tokens": ["Und", "was", "ihm", "wert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Das flieht ihn wie ein Dieb,", "tokens": ["Das", "flieht", "ihn", "wie", "ein", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "F\u00e4llt ab zu Erd'", "tokens": ["F\u00e4llt", "ab", "zu", "Erd'"], "token_