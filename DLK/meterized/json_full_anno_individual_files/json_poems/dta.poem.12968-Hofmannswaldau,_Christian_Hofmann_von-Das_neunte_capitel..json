{"dta.poem.12968": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das neunte capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gehorsam leben ist weit sichrer, als befehlen:", "tokens": ["Ge\u00b7hor\u00b7sam", "le\u00b7ben", "ist", "weit", "sich\u00b7rer", ",", "als", "be\u00b7feh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VAFIN", "ADJD", "ADJA", "$,", "KOUS", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn unterth\u00e4nig seyn ist kein geringes gut;", "tokens": ["Denn", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "seyn", "ist", "kein", "ge\u00b7rin\u00b7ges", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "VAFIN", "PIAT", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wiewol die wenigsten nach dieser regel wehlen,", "tokens": ["Wie\u00b7wol", "die", "we\u00b7nigs\u00b7ten", "nach", "die\u00b7ser", "re\u00b7gel", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Weil das gehorsams-joch dem fleische bange thut.", "tokens": ["Weil", "das", "ge\u00b7hor\u00b7sams\u00b7joch", "dem", "flei\u00b7sche", "ban\u00b7ge", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum dient der gr\u00f6ste theil aus noth und nicht aus liebe;", "tokens": ["Drum", "dient", "der", "gr\u00f6s\u00b7te", "theil", "aus", "noth", "und", "nicht", "aus", "lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "PTKNEG", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch wer die sanffte last nicht ohne murren tr\u00e4gt,", "tokens": ["Doch", "wer", "die", "sanff\u00b7te", "last", "nicht", "oh\u00b7ne", "mur\u00b7ren", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "VVFIN", "PTKNEG", "APPR", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der hat ihm, wenn er sich schon vor verdru\u00df begr\u00fcbe,", "tokens": ["Der", "hat", "ihm", ",", "wenn", "er", "sich", "schon", "vor", "ver\u00b7dru\u00df", "be\u00b7gr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nur eine gr\u00f6\u00dfre b\u00fcrd auf seinen hals gelegt.", "tokens": ["Nur", "ei\u00b7ne", "gr\u00f6\u00df\u00b7re", "b\u00fcrd", "auf", "sei\u00b7nen", "hals", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum lerne durch den Geist des fleisches hoffart stillen,", "tokens": ["Drum", "ler\u00b7ne", "durch", "den", "Geist", "des", "flei\u00b7sches", "hof\u00b7fart", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sonst \u00fcbergiebst du dich der \u00e4rgsten tyranney;", "tokens": ["Sonst", "\u00fc\u00b7berg\u00b7iebst", "du", "dich", "der", "\u00e4rgs\u00b7ten", "ty\u00b7ran\u00b7ney", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn wo das hertze nicht sich um des H\u00f6chsten willen", "tokens": ["Denn", "wo", "das", "hert\u00b7ze", "nicht", "sich", "um", "des", "H\u00f6chs\u00b7ten", "wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PTKNEG", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Freym\u00fcthig unterwirfft, so wirst du niemals frey.", "tokens": ["Frey\u00b7m\u00fct\u00b7hig", "un\u00b7ter\u00b7wirfft", ",", "so", "wirst", "du", "nie\u00b7mals", "frey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Glaubst du es aber nicht, so lerne mit verlierung", "tokens": ["Glaubst", "du", "es", "a\u00b7ber", "nicht", ",", "so", "ler\u00b7ne", "mit", "ver\u00b7lie\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der allzutheuren zeit, was hier dein auge liest;", "tokens": ["Der", "all\u00b7zu\u00b7theu\u00b7ren", "zeit", ",", "was", "hier", "dein", "au\u00b7ge", "liest", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und sage mir alsdenn, ob ausser der regierung,", "tokens": ["Und", "sa\u00b7ge", "mir", "als\u00b7denn", ",", "ob", "aus\u00b7ser", "der", "re\u00b7gie\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die uns in Christo f\u00fchrt, die ruh zu finden ist.", "tokens": ["Die", "uns", "in", "Chris\u00b7to", "f\u00fchrt", ",", "die", "ruh", "zu", "fin\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der eingebildte wahn hat ihrer viel betrogen,", "tokens": ["Der", "ein\u00b7ge\u00b7bild\u00b7te", "wahn", "hat", "ih\u00b7rer", "viel", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn die zufriedenheit liegt an dem orte nicht:", "tokens": ["Denn", "die", "zu\u00b7frie\u00b7den\u00b7heit", "liegt", "an", "dem", "or\u00b7te", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein tugendhaffter mann ruht zwischen well und wogen,", "tokens": ["Ein", "tu\u00b7gend\u00b7haff\u00b7ter", "mann", "ruht", "zwi\u00b7schen", "well", "und", "wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wenn einem thoren auch in hafen ruh gebricht.", "tokens": ["Wenn", "ei\u00b7nem", "tho\u00b7ren", "auch", "in", "ha\u00b7fen", "ruh", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es ist wol, leyder! wahr, da\u00df ieder seinen willen", "tokens": ["Es", "ist", "wol", ",", "ley\u00b7der", "!", "wahr", ",", "da\u00df", "ie\u00b7der", "sei\u00b7nen", "wil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADV", "$.", "ADJD", "$,", "KOUS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und eigensinn allein vor seinen herrn erkennt;", "tokens": ["Und", "ei\u00b7gen\u00b7sinn", "al\u00b7lein", "vor", "sei\u00b7nen", "herrn", "er\u00b7kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was die begierde sagt, das mu\u00df die hand erf\u00fcllen,", "tokens": ["Was", "die", "be\u00b7gier\u00b7de", "sagt", ",", "das", "mu\u00df", "die", "hand", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PDS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wer das strafen will, der wird ein feind genennt.", "tokens": ["Und", "wer", "das", "stra\u00b7fen", "will", ",", "der", "wird", "ein", "feind", "ge\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "VVINF", "VMFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch wo wir GOttes ehr\u2019 und unsre wohlfahrt suchen,", "tokens": ["Doch", "wo", "wir", "Got\u00b7tes", "ehr'", "und", "uns\u00b7re", "wohl\u00b7fahrt", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VVFIN", "KON", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So mu\u00df die eigenheit nicht unser abgott seyn.", "tokens": ["So", "mu\u00df", "die", "ei\u00b7gen\u00b7heit", "nicht", "un\u00b7ser", "ab\u00b7gott", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ach lerne, lieber mensch! den eigensinn verfluchen,", "tokens": ["Ach", "ler\u00b7ne", ",", "lie\u00b7ber", "mensch", "!", "den", "ei\u00b7gen\u00b7sinn", "ver\u00b7flu\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ADV", "ADJD", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und r\u00e4ume, was du kanst, des friedens wege ein.", "tokens": ["Und", "r\u00e4u\u00b7me", ",", "was", "du", "kanst", ",", "des", "frie\u00b7dens", "we\u00b7ge", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn wo ist solch ein mensch, der alles ausstudiret,", "tokens": ["Denn", "wo", "ist", "solch", "ein", "mensch", ",", "der", "al\u00b7les", "aus\u00b7stu\u00b7di\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PIAT", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Und der in dieser welt nichts mehr zu lernen hat?", "tokens": ["Und", "der", "in", "die\u00b7ser", "welt", "nichts", "mehr", "zu", "ler\u00b7nen", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PDAT", "NN", "PIS", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer ihm alleine glaubt, wird insgemein verf\u00fchret:", "tokens": ["Wer", "ihm", "al\u00b7lei\u00b7ne", "glaubt", ",", "wird", "ins\u00b7ge\u00b7mein", "ver\u00b7f\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wer sicher gehen will, sucht auch bey andern rath.", "tokens": ["Wer", "si\u00b7cher", "ge\u00b7hen", "will", ",", "sucht", "auch", "bey", "an\u00b7dern", "rath", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "$,", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ist deine meynung gut, und dein gehorsam hertze", "tokens": ["Ist", "dei\u00b7ne", "mey\u00b7nung", "gut", ",", "und", "dein", "ge\u00b7hor\u00b7sam", "hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,", "KON", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Schl\u00e4gt sie aus Gottes rath und antrieb in den wind,", "tokens": ["Schl\u00e4gt", "sie", "aus", "Got\u00b7tes", "rath", "und", "an\u00b7trieb", "in", "den", "wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So glaube, da\u00df dein geist nach \u00fcberstandnem schmertze", "tokens": ["So", "glau\u00b7be", ",", "da\u00df", "dein", "geist", "nach", "\u00fc\u00b7bers\u00b7tand\u00b7nem", "schmert\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vor das vermeynte gut das h\u00f6chste gut gewinnt.", "tokens": ["Vor", "das", "ver\u00b7meyn\u00b7te", "gut", "das", "h\u00f6chs\u00b7te", "gut", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJD", "ART", "ADJA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wer andern rath ertheilt, geht lange nicht so sicher,", "tokens": ["Wer", "an\u00b7dern", "rath", "er\u00b7theilt", ",", "geht", "lan\u00b7ge", "nicht", "so", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVPP", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als der sich rathen l\u00e4st, wie die erfahrung zeigt.", "tokens": ["Als", "der", "sich", "ra\u00b7then", "l\u00e4st", ",", "wie", "die", "er\u00b7fah\u00b7rung", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "VVINF", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Zucht und gehorsam macht viel kl\u00fcger, als viel b\u00fccher,", "tokens": ["Zucht", "und", "ge\u00b7hor\u00b7sam", "macht", "viel", "kl\u00fc\u00b7ger", ",", "als", "viel", "b\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "PIAT", "ADJA", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.40": {"text": "Weil Gott den niedrigen nichts heilsames verschweigt.", "tokens": ["Weil", "Gott", "den", "nied\u00b7ri\u00b7gen", "nichts", "heil\u00b7sa\u00b7mes", "ver\u00b7schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "PIS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Drum folg und lerne dich in den gehorsam schicken:", "tokens": ["Drum", "folg", "und", "ler\u00b7ne", "dich", "in", "den", "ge\u00b7hor\u00b7sam", "schi\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "VVFIN", "PRF", "APPR", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Gew\u00f6hne die vernunfft zur unterth\u00e4nigkeit,", "tokens": ["Ge\u00b7w\u00f6h\u00b7ne", "die", "ver\u00b7nunfft", "zur", "un\u00b7ter\u00b7th\u00e4\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und la\u00df ben eigensinn dir nicht das ziel verr\u00fccken,", "tokens": ["Und", "la\u00df", "ben", "ei\u00b7gen\u00b7sinn", "dir", "nicht", "das", "ziel", "ver\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADJA", "NN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Den frechen eigensinn, den uns der HErr verbeut.", "tokens": ["Den", "fre\u00b7chen", "ei\u00b7gen\u00b7sinn", ",", "den", "uns", "der", "Herr", "ver\u00b7beut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Man mag bisweilen wol gar gute schl\u00fcsse fassen;", "tokens": ["Man", "mag", "bis\u00b7wei\u00b7len", "wol", "gar", "gu\u00b7te", "schl\u00fcs\u00b7se", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Doch widerspenstig seyn, und als ein st\u00f6rrig holtz", "tokens": ["Doch", "wi\u00b7der\u00b7spens\u00b7tig", "seyn", ",", "und", "als", "ein", "st\u00f6r\u00b7rig", "holtz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAINF", "$,", "KON", "KOUS", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Von treuen f\u00fchrern sich zu nichts bewegen lassen", "tokens": ["Von", "treu\u00b7en", "f\u00fch\u00b7rern", "sich", "zu", "nichts", "be\u00b7we\u00b7gen", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "PRF", "APPR", "PIS", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Hei\u00dft bey verst\u00e4ndigen nur unverstand und stoltz.", "tokens": ["Hei\u00dft", "bey", "ver\u00b7st\u00e4n\u00b7di\u00b7gen", "nur", "un\u00b7ver\u00b7stand", "und", "stoltz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}