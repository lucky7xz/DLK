{"dta.poem.23703": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Bacchus und Cithere.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.71", "sv:0.14", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich trinken oder k&#252;ssen?", "tokens": ["Soll", "ich", "trin\u00b7ken", "o\u00b7der", "k", "&#252;", "ssen", "?"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "NE", "$(", "VVINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hier winkt Bacchus, dort Cithere.", "tokens": ["Hier", "winkt", "Bac\u00b7chus", ",", "dort", "Cit\u00b7he\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beide winken, beide l\u00e4cheln.", "tokens": ["Bei\u00b7de", "win\u00b7ken", ",", "bei\u00b7de", "l\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Baechus mit gesetzten Minen,", "tokens": ["Bae\u00b7chus", "mit", "ge\u00b7setz\u00b7ten", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Cithere mit verliebten.", "tokens": ["Und", "Cit\u00b7he\u00b7re", "mit", "ver\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Baechus zeigt mir seine Reben,", "tokens": ["Bae\u00b7chus", "zeigt", "mir", "sei\u00b7ne", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seht, sie sinken, schwer von Trauben!", "tokens": ["Seht", ",", "sie", "sin\u00b7ken", ",", "schwer", "von", "Trau\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVINF", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber seht nur, dort im Schatten,", "tokens": ["A\u00b7ber", "seht", "nur", ",", "dort", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dort im Schatten, unter Reben,", "tokens": ["Dort", "im", "Schat\u00b7ten", ",", "un\u00b7ter", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Liegt ein M\u00e4dchen lang gestrekket!", "tokens": ["Liegt", "ein", "M\u00e4d\u00b7chen", "lang", "ge\u00b7strek\u00b7ket", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Seht, es schl\u00e4ft, es l\u00e4chelt schlafend,", "tokens": ["Seht", ",", "es", "schl\u00e4ft", ",", "es", "l\u00e4\u00b7chelt", "schla\u00b7fend", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und es l\u00e4chelte Cithere", "tokens": ["Und", "es", "l\u00e4\u00b7chel\u00b7te", "Cit\u00b7he\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.13": {"text": "Nicht so reitzend, als sie winkte.", "tokens": ["Nicht", "so", "reit\u00b7zend", ",", "als", "sie", "wink\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "O wie s\u00fc\u00df mag es nicht schlummern!", "tokens": ["O", "wie", "s\u00fc\u00df", "mag", "es", "nicht", "schlum\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "O wie reitzend liegt das M\u00e4dchen!", "tokens": ["O", "wie", "reit\u00b7zend", "liegt", "das", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Um den weissen regen Busen,", "tokens": ["Um", "den", "weis\u00b7sen", "re\u00b7gen", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Hangen schwarze reife Trauben,", "tokens": ["Han\u00b7gen", "schwar\u00b7ze", "rei\u00b7fe", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und es gl\u00e4nzen um den Lokken,", "tokens": ["Und", "es", "gl\u00e4n\u00b7zen", "um", "den", "Lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Um den Rabenschwartzen Lokken,", "tokens": ["Um", "den", "Ra\u00b7ben\u00b7schwart\u00b7zen", "Lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Goldne Blumen in dem Schatten.", "tokens": ["Gold\u00b7ne", "Blu\u00b7men", "in", "dem", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Weingott, winke nur nicht l\u00e4nger;", "tokens": ["Wein\u00b7gott", ",", "win\u00b7ke", "nur", "nicht", "l\u00e4n\u00b7ger", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Denn ich mu\u00df erst, bei dem M\u00e4dchen,", "tokens": ["Denn", "ich", "mu\u00df", "erst", ",", "bei", "dem", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Unter deinen Trauben schlummern.", "tokens": ["Un\u00b7ter", "dei\u00b7nen", "Trau\u00b7ben", "schlum\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}