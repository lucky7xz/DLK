{"dta.poem.11314": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der Doris beliebtem kinde.  \n  G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein kleines kind h\u00e4ngt allem manns-volck an,", "tokens": ["Dein", "klei\u00b7nes", "kind", "h\u00e4ngt", "al\u00b7lem", "manns\u00b7volck", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und du verwunderst dich de\u00dfwegen;", "tokens": ["Und", "du", "ver\u00b7wun\u00b7derst", "dich", "de\u00df\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein ich wei\u00df, woran es ihm gelegen:", "tokens": ["Al\u00b7lein", "ich", "wei\u00df", ",", "wo\u00b7ran", "es", "ihm", "ge\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es suchet, ob es auch den vater finden kan.", "tokens": ["Es", "su\u00b7chet", ",", "ob", "es", "auch", "den", "va\u00b7ter", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}