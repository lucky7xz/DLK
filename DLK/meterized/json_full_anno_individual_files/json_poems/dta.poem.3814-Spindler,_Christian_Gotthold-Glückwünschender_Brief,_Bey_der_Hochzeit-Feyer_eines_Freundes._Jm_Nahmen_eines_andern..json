{"dta.poem.3814": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschender Brief,  \n Bey der  Hochzeit-Feyer  eines Freundes. Jm Nahmen  \n eines andern.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "So soll ich denn, wie ich vernommen,", "tokens": ["So", "soll", "ich", "denn", ",", "wie", "ich", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deiner Hochzeit-Feyer kommen,", "tokens": ["Zu", "dei\u00b7ner", "Hoch\u00b7zeit\u00b7Fey\u00b7er", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein es mangelt mir die Zeit,", "tokens": ["Al\u00b7lein", "es", "man\u00b7gelt", "mir", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und noch dazu ist es zu weit.", "tokens": ["Und", "noch", "da\u00b7zu", "ist", "es", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Du weist, ich zehle alle Stunden,", "tokens": ["Du", "weist", ",", "ich", "zeh\u00b7le", "al\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du weist es schon, wie ich gebunden,", "tokens": ["Du", "weist", "es", "schon", ",", "wie", "ich", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du weist/ wie mein Verh\u00e4ngni\u00df spricht:", "tokens": ["Du", "weist", "/", "wie", "mein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du must, du kanst, du solst auch nicht.", "tokens": ["Du", "must", ",", "du", "kanst", ",", "du", "solst", "auch", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch kommt ein Blatt vor mich gegangen,", "tokens": ["Doch", "kommt", "ein", "Blatt", "vor", "mich", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Du wirst es, als den Gast, empfangen,", "tokens": ["Du", "wirst", "es", ",", "als", "den", "Gast", ",", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und wenn es kommt, so mache Platz;", "tokens": ["Und", "wenn", "es", "kommt", ",", "so", "ma\u00b7che", "Platz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Drauf gieb es deinem liebsten Schatz,", "tokens": ["Drauf", "gieb", "es", "dei\u00b7nem", "liebs\u00b7ten", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Denn ihr soll es zu Ehren singen,", "tokens": ["Denn", "ihr", "soll", "es", "zu", "Eh\u00b7ren", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und dir die fr\u00f6msten W\u00fcnsche bringen.", "tokens": ["Und", "dir", "die", "fr\u00f6ms\u00b7ten", "W\u00fcn\u00b7sche", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wie wird es nicht so sehnlich sehn", "tokens": ["Wie", "wird", "es", "nicht", "so", "sehn\u00b7lich", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Euch Beyde in das Bette gehn.", "tokens": ["Euch", "Bey\u00b7de", "in", "das", "Bet\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und schlagen eurer Liebe Flammen", "tokens": ["Und", "schla\u00b7gen", "eu\u00b7rer", "Lie\u00b7be", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auch etwa lichterloh zusammen,", "tokens": ["Auch", "et\u00b7wa", "lich\u00b7ter\u00b7loh", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wenn nun ihr Strahl das finstre bricht,", "tokens": ["Wenn", "nun", "ihr", "Strahl", "das", "finst\u00b7re", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Verbrennt das arme Ding nur nicht,", "tokens": ["Ver\u00b7brennt", "das", "ar\u00b7me", "Ding", "nur", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Es wird euch Beyden nicht viel schaden,", "tokens": ["Es", "wird", "euch", "Bey\u00b7den", "nicht", "viel", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Denn weil es stumm, kans nicht verrathen.", "tokens": ["Denn", "weil", "es", "stumm", ",", "kans", "nicht", "ver\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$,", "VMFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Denn wisset: es ist wie sein Mann,", "tokens": ["Denn", "wis\u00b7set", ":", "es", "ist", "wie", "sein", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Der da zu allen schweigen kan.", "tokens": ["Der", "da", "zu", "al\u00b7len", "schwei\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So viel als wir hier Lerchen zehlen,", "tokens": ["So", "viel", "als", "wir", "hier", "Ler\u00b7chen", "zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So viel als uns Philister qu\u00e4len,", "tokens": ["So", "viel", "als", "uns", "Phi\u00b7lis\u00b7ter", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "So viel im ", "tokens": ["So", "viel", "im"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "So viel als Garten-Nympffen seyn,", "tokens": ["So", "viel", "als", "Gar\u00b7ten\u00b7Nym\u00b7pf\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "So viel die Messe Menschen heget,", "tokens": ["So", "viel", "die", "Mes\u00b7se", "Men\u00b7schen", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "So viel man Gelder niederleget,", "tokens": ["So", "viel", "man", "Gel\u00b7der", "nie\u00b7der\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "So viel in ", "tokens": ["So", "viel", "in"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.32": {"text": "So viel in ", "tokens": ["So", "viel", "in"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "So viel man da an Kr\u00fcgen leeret,", "tokens": ["So", "viel", "man", "da", "an", "Kr\u00fc\u00b7gen", "lee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "So viel der Pursche Geld verzehret,", "tokens": ["So", "viel", "der", "Pur\u00b7sche", "Geld", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "So viele W\u00e4gen in dem ", "tokens": ["So", "vie\u00b7le", "W\u00e4\u00b7gen", "in", "dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Geliebter Hertzens-Freund, so viel", "tokens": ["Ge\u00b7lieb\u00b7ter", "Hert\u00b7zens\u00b7Freund", ",", "so", "viel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "W\u00fcnscht dir mein Blatt zu deinem Feste,", "tokens": ["W\u00fcnscht", "dir", "mein", "Blatt", "zu", "dei\u00b7nem", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und jedem deiner Hochzeit-G\u00e4ste.", "tokens": ["Und", "je\u00b7dem", "dei\u00b7ner", "Hoch\u00b7zeit\u00b7G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Der Storch vom Aplischen Pallast", "tokens": ["Der", "Storch", "vom", "Ap\u00b7li\u00b7schen", "Pal\u00b7last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Hat sich den festen Schlu\u00df gefa\u00dft,", "tokens": ["Hat", "sich", "den", "fes\u00b7ten", "Schlu\u00df", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Er will es redlich mit dir meinen,", "tokens": ["Er", "will", "es", "red\u00b7lich", "mit", "dir", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und bey dem Hochzeit-Fest erscheinen.", "tokens": ["Und", "bey", "dem", "Hoch\u00b7zeit\u00b7Fest", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Er bringt den rechten Hochzeit-Lohn,", "tokens": ["Er", "bringt", "den", "rech\u00b7ten", "Hoch\u00b7zeit\u00b7Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Freund! eine Tochter oder Sohn.", "tokens": ["Freund", "!", "ei\u00b7ne", "Toch\u00b7ter", "o\u00b7der", "Sohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.45": {"text": "Sein Fl\u00fcgel wei\u00df sehr hoch zu steigen,", "tokens": ["Sein", "Fl\u00fc\u00b7gel", "wei\u00df", "sehr", "hoch", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Und listig in das Nest zu schleichen,", "tokens": ["Und", "lis\u00b7tig", "in", "das", "Nest", "zu", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Versorge ihn nur nach Geb\u00fchr,", "tokens": ["Ver\u00b7sor\u00b7ge", "ihn", "nur", "nach", "Ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Er ist dir gar ein artig Thier.", "tokens": ["Er", "ist", "dir", "gar", "ein", "ar\u00b7tig", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "So erndte mit dem ", "tokens": ["So", "ernd\u00b7te", "mit", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.50": {"text": "Die allersch\u00f6nsten Hochzeit-Bl\u00fcmgen.", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Hoch\u00b7zeit\u00b7B\u00b7l\u00fcm\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Lebt gl\u00fccklich in der neuen Eh,", "tokens": ["Lebt", "gl\u00fcck\u00b7lich", "in", "der", "neu\u00b7en", "Eh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "So flieht das unvergn\u00fcgte Weh,", "tokens": ["So", "flieht", "das", "un\u00b7ver\u00b7gn\u00fcg\u00b7te", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "So wird auch balde in der Wiegen", "tokens": ["So", "wird", "auch", "bal\u00b7de", "in", "der", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Ein kleiner lieber ", "tokens": ["Ein", "klei\u00b7ner", "lie\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}}}}