{"textgrid.poem.25121": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Italien", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu ihr, zu der die Gletscherb\u00e4che", "tokens": ["Zu", "ihr", ",", "zu", "der", "die", "Glet\u00b7scher\u00b7b\u00e4\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "S\u00fcdw\u00e4rts hinunterjauchzen,", "tokens": ["S\u00fcd\u00b7w\u00e4rts", "hin\u00b7un\u00b7ter\u00b7jauch\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Noch einmal wend' ich den Blick.", "tokens": ["Noch", "ein\u00b7mal", "wend'", "ich", "den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wie unter der nordischen Eichen Dom", "tokens": ["Wie", "un\u00b7ter", "der", "nor\u00b7di\u00b7schen", "Ei\u00b7chen", "Dom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ihre Riesenschwester Germania,", "tokens": ["Ih\u00b7re", "Rie\u00b7sen\u00b7schwes\u00b7ter", "Ger\u00b7ma\u00b7nia", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "So unter Lorbeerwipfeln", "tokens": ["So", "un\u00b7ter", "Lor\u00b7beer\u00b7wip\u00b7feln"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "H\u00e4lt Italien die Siegesfeier.", "tokens": ["H\u00e4lt", "I\u00b7ta\u00b7li\u00b7en", "die", "Sie\u00b7ge\u00b7sfei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ein magischer Ring", "tokens": ["Ein", "ma\u00b7gi\u00b7scher", "Ring"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Hat eure Geschicke, ihr L\u00e4nder,", "tokens": ["Hat", "eu\u00b7re", "Ge\u00b7schi\u00b7cke", ",", "ihr", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Aneinander gebunden \u2013", "tokens": ["An\u00b7ein\u00b7an\u00b7der", "ge\u00b7bun\u00b7den", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Zu eurem Unheil, o wie lange!", "tokens": ["Zu", "eu\u00b7rem", "Un\u00b7heil", ",", "o", "wie", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit ihres Himmels schmachtendem Blau,", "tokens": ["Mit", "ih\u00b7res", "Him\u00b7mels", "schmach\u00b7ten\u00b7dem", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Ihrer Goldfruchthaine Duft und Glanz", "tokens": ["Ih\u00b7rer", "Gold\u00b7frucht\u00b7hai\u00b7ne", "Duft", "und", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Lockte die Zauberin des S\u00fcdens", "tokens": ["Lock\u00b7te", "die", "Zau\u00b7be\u00b7rin", "des", "S\u00fc\u00b7dens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Deutschlands F\u00fcrsten und V\u00f6lker", "tokens": ["Deutschlands", "F\u00fcrs\u00b7ten", "und", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "In ihre Armidag\u00e4rten,", "tokens": ["In", "ih\u00b7re", "Ar\u00b7mi\u00b7da\u00b7g\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Da\u00df sie bei Brunnenrieseln", "tokens": ["Da\u00df", "sie", "bei", "Brun\u00b7nen\u00b7rie\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Unter Myrtengeb\u00fcsch und leuchtenden Marmorbildern", "tokens": ["Un\u00b7ter", "Myr\u00b7ten\u00b7ge\u00b7b\u00fcsch", "und", "leuch\u00b7ten\u00b7den", "Mar\u00b7mor\u00b7bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Nicht ihres Reiches und Volks mehr gedachten.", "tokens": ["Nicht", "ih\u00b7res", "Rei\u00b7ches", "und", "Volks", "mehr", "ge\u00b7dach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Dann aus Wollusttr\u00e4umen der Nacht", "tokens": ["Dann", "aus", "Wol\u00b7lust\u00b7tr\u00e4u\u00b7men", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Fuhren sie auf;", "tokens": ["Fuh\u00b7ren", "sie", "auf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "An den eisernen Panzer", "tokens": ["An", "den", "ei\u00b7ser\u00b7nen", "Pan\u00b7zer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Pochte ihr Herz in Begier,", "tokens": ["Poch\u00b7te", "ihr", "Herz", "in", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.24": {"text": "Ueber das Land der G\u00f6tter zu herrschen;", "tokens": ["Ue\u00b7ber", "das", "Land", "der", "G\u00f6t\u00b7ter", "zu", "herr\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Es zuckte das Schwert aus der Scheide,", "tokens": ["Es", "zuck\u00b7te", "das", "Schwert", "aus", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.26": {"text": "Und hochauf schlug die Flamme des Kampfes;", "tokens": ["Und", "hoch\u00b7auf", "schlug", "die", "Flam\u00b7me", "des", "Kamp\u00b7fes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "St\u00e4dte loderten und erstanden neu", "tokens": ["St\u00e4d\u00b7te", "lo\u00b7der\u00b7ten", "und", "er\u00b7stan\u00b7den", "neu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.28": {"text": "Zum Rachekrieg aus der Asche;", "tokens": ["Zum", "Ra\u00b7che\u00b7krieg", "aus", "der", "A\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Von Gift gew\u00fcrgt", "tokens": ["Von", "Gift", "ge\u00b7w\u00fcrgt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Sank der gr\u00f6\u00dfte der Kaiser", "tokens": ["Sank", "der", "gr\u00f6\u00df\u00b7te", "der", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.31": {"text": "Bleich auf den fieberatmenden Boden;", "tokens": ["Bleich", "auf", "den", "fie\u00b7be\u00b7rat\u00b7men\u00b7den", "Bo\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Selbst die Bande des Bluts", "tokens": ["Selbst", "die", "Ban\u00b7de", "des", "Bluts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "L\u00f6ste der Ha\u00df,", "tokens": ["L\u00f6s\u00b7te", "der", "Ha\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.34": {"text": "Ganze Geschlechter von Italiens S\u00f6hnen", "tokens": ["Gan\u00b7ze", "Ge\u00b7schlech\u00b7ter", "von", "I\u00b7ta\u00b7li\u00b7ens", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NE", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.35": {"text": "Niederw\u00e4lzte die mordende Schlacht;", "tokens": ["Nie\u00b7der\u00b7w\u00e4lz\u00b7te", "die", "mor\u00b7den\u00b7de", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.36": {"text": "Und als verhallt der Schwertschlag,", "tokens": ["Und", "als", "ver\u00b7hallt", "der", "Schwert\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Der Siegesruf und die Totenklage,", "tokens": ["Der", "Sie\u00b7ges\u00b7ruf", "und", "die", "To\u00b7ten\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Ersch\u00f6pft, ohnm\u00e4chtig lagt ihr beide,", "tokens": ["Er\u00b7sch\u00f6pft", ",", "ohn\u00b7m\u00e4ch\u00b7tig", "lagt", "ihr", "bei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Ein Hohn und Spott dem Fremden.", "tokens": ["Ein", "Hohn", "und", "Spott", "dem", "Frem\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sei denn, wie einst zum Verderben,", "tokens": ["Sei", "denn", ",", "wie", "einst", "zum", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "ADV", "APPRART", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Nun euch zum Heil, eu'r Schicksal", "tokens": ["Nun", "euch", "zum", "Heil", ",", "eu'r", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Unaufl\u00f6slich verbunden,", "tokens": ["Un\u00b7auf\u00b7l\u00f6s\u00b7lich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und, wie in ", "tokens": ["Und", ",", "wie", "in"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Eu'r Auferstehungsfest ihr feiert,", "tokens": ["Eu'r", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7fest", "ihr", "fei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So schreitet Arm in Arm", "tokens": ["So", "schrei\u00b7tet", "Arm", "in", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der gr\u00f6\u00dfern Zukunft entgegen.", "tokens": ["Der", "gr\u00f6\u00b7\u00dfern", "Zu\u00b7kunft", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "me