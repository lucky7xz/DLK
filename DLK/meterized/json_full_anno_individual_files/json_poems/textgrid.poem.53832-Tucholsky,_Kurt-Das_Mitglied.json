{"textgrid.poem.53832": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das Mitglied", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In mein' Verein bin ich hineingetreten,", "tokens": ["In", "mein'", "Ver\u00b7ein", "bin", "ich", "hin\u00b7ein\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "weil mich ein alter Freund darum gebeten,", "tokens": ["weil", "mich", "ein", "al\u00b7ter", "Freund", "da\u00b7rum", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich war allein.", "tokens": ["ich", "war", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Jetzt bin ich Mitglied, Kamerad, Kollege \u2013", "tokens": ["Jetzt", "bin", "ich", "Mit\u00b7glied", ",", "Ka\u00b7me\u00b7rad", ",", "Kol\u00b7le\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "das kleine Band, das ich ins Knopfloch lege,", "tokens": ["das", "klei\u00b7ne", "Band", ",", "das", "ich", "ins", "Kno\u00b7pfloch", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "ist der Verein.", "tokens": ["ist", "der", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wir haben einen Vorstandspr\u00e4sidenten", "tokens": ["Wir", "ha\u00b7ben", "ei\u00b7nen", "Vor\u00b7stand\u00b7spr\u00e4\u00b7si\u00b7den\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und einen Kassenwart und Referenten", "tokens": ["und", "ei\u00b7nen", "Kas\u00b7sen\u00b7wart", "und", "Re\u00b7fe\u00b7ren\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und obendrein", "tokens": ["und", "o\u00b7ben\u00b7drein"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "den m\u00e4chtigen Krach der oppositionellen", "tokens": ["den", "m\u00e4ch\u00b7ti\u00b7gen", "Krach", "der", "op\u00b7po\u00b7si\u00b7ti\u00b7o\u00b7nel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Minorit\u00e4t, doch die wird glatt zerschellen", "tokens": ["Mi\u00b7no\u00b7ri\u00b7t\u00e4t", ",", "doch", "die", "wird", "glatt", "zer\u00b7schel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ART", "VAFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "in mein' Verein.", "tokens": ["in", "mein'", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich bin Verwaltungsbeirat seit drei Wochen.", "tokens": ["Ich", "bin", "Ver\u00b7wal\u00b7tungs\u00b7bei\u00b7rat", "seit", "drei", "Wo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will ja nicht auf meine W\u00fcrde pochen \u2013", "tokens": ["Ich", "will", "ja", "nicht", "auf", "mei\u00b7ne", "W\u00fcr\u00b7de", "po\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich bild mir gar nichts ein . . .", "tokens": ["ich", "bild", "mir", "gar", "nichts", "ein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "NN", "PPER", "ADV", "PIS", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und doch ist das Gef\u00fchl so sch\u00f6n, zu wissen:", "tokens": ["Und", "doch", "ist", "das", "Ge\u00b7f\u00fchl", "so", "sch\u00f6n", ",", "zu", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "sie k\u00f6nnen mich ja gar nicht missen", "tokens": ["sie", "k\u00f6n\u00b7nen", "mich", "ja", "gar", "nicht", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "in mein' Verein.", "tokens": ["in", "mein'", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Da drau\u00dfen bin ich nur ein armes Luder.", "tokens": ["Da", "drau\u00b7\u00dfen", "bin", "ich", "nur", "ein", "ar\u00b7mes", "Lu\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier bin ich ich \u2013 und Mann und Bundesbruder", "tokens": ["Hier", "bin", "ich", "ich", "\u2013", "und", "Mann", "und", "Bun\u00b7des\u00b7bru\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$(", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in vollen Reihn.", "tokens": ["in", "vol\u00b7len", "Reihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Hoch \u00fcber uns, da schweben die Statuten.", "tokens": ["Hoch", "\u00fc\u00b7ber", "uns", ",", "da", "schwe\u00b7ben", "die", "Sta\u00b7tu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Abendstunden schwinden wie Minuten", "tokens": ["Die", "A\u00b7bends\u00b7tun\u00b7den", "schwin\u00b7den", "wie", "Mi\u00b7nu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "in mein' Verein.", "tokens": ["in", "mein'", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "In mein' Verein werd ich erst richtig munter.", "tokens": ["In", "mein'", "Ver\u00b7ein", "werd", "ich", "erst", "rich\u00b7tig", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf die, wo nicht drin sind, seh ich hinunter \u2013", "tokens": ["Auf", "die", ",", "wo", "nicht", "drin", "sind", ",", "seh", "ich", "hin\u00b7un\u00b7ter", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWAV", "PTKNEG", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "was kann mit denen sein?", "tokens": ["was", "kann", "mit", "de\u00b7nen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PRELS", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Stolz weht die Fahne, die wir mutig tragen.", "tokens": ["Stolz", "weht", "die", "Fah\u00b7ne", ",", "die", "wir", "mu\u00b7tig", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Auf mich k\u00f6nn' Sie ja ruhig \u00bbOchse\u00ab sagen,", "tokens": ["Auf", "mich", "k\u00f6nn'", "Sie", "ja", "ru\u00b7hig", "\u00bb", "O\u00b7chse", "\u00ab", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADV", "ADJD", "$(", "NN", "$(", "VVINF", "$,"], "meter": "+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "da werd ich mich bestimmt nicht erst verteidigen.", "tokens": ["da", "werd", "ich", "mich", "be\u00b7stimmt", "nicht", "erst", "ver\u00b7tei\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wenn Sie mich als Mitglied so beleidigen . . . !", "tokens": ["Doch", "wenn", "Sie", "mich", "als", "Mit\u00b7glied", "so", "be\u00b7lei\u00b7di\u00b7gen", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "KOUS", "NN", "ADV", "VVFIN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann steigt mein deutscher Gruppenstolz!", "tokens": ["Dann", "steigt", "mein", "deut\u00b7scher", "Grup\u00b7pen\u00b7stolz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hoch Stolze-Schrey! Freiheit! Gut Holz!", "tokens": ["Hoch", "Stol\u00b7ze\u00b7Schrey", "!", "Frei\u00b7heit", "!", "Gut", "Holz", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "NN", "$.", "ADJD", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Hier lebe ich.", "tokens": ["Hier", "le\u00b7be", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und will auch einst begraben sein", "tokens": ["Und", "will", "auch", "einst", "be\u00b7gra\u00b7ben", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "in mein' Verein.", "tokens": ["in", "mein'", "Ver\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "In mein' Verein bin ich hineingetreten,", "tokens": ["In", "mein'", "Ver\u00b7ein", "bin", "ich", "hin\u00b7ein\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "weil mich ein alter Freund darum gebeten,", "tokens": ["weil", "mich", "ein", "al\u00b7ter", "Freund", "da\u00b7rum", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich war allein.", "tokens": ["ich", "war", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Jetzt bin ich Mitglied, Kamerad, Kollege \u2013", "tokens": ["Jetzt", "bin", "ich", "Mit\u00b7glied", ",", "Ka\u00b7me\u00b7rad", ",", "Kol\u00b7le\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "das kleine Band, das ich ins Knopfloch lege,", "tokens": ["das", "klei\u00b7ne", "Band", ",", "das", "ich", "ins", "Kno\u00b7pfloch", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word