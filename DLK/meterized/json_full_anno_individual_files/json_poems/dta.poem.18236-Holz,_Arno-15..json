{"dta.poem.18236": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer ist's, der meine Wange br\u00e4unt,", "tokens": ["Der", "Som\u00b7mer", "ist's", ",", "der", "mei\u00b7ne", "Wan\u00b7ge", "br\u00e4unt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und meine Lenzsaat steht noch ungeschnitten.", "tokens": ["Und", "mei\u00b7ne", "Lenz\u00b7saat", "steht", "noch", "un\u00b7ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch, der erste Fr\u00fchschmelz ist dahin,", "tokens": ["Und", "doch", ",", "der", "ers\u00b7te", "Fr\u00fch\u00b7schmelz", "ist", "da\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "ADJA", "NN", "VAFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz ward dunkel, d\u00fcster ward mein Sinn,", "tokens": ["Mein", "Herz", "ward", "dun\u00b7kel", ",", "d\u00fcs\u00b7ter", "ward", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn ach, wer viel geliebt, hat viel gelitten!", "tokens": ["Denn", "ach", ",", "wer", "viel", "ge\u00b7liebt", ",", "hat", "viel", "ge\u00b7lit\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWS", "ADV", "VVPP", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, ", "tokens": ["Ich", "wei\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "In mir ruft's faustisch schon: Vorbei! Vorbei!", "tokens": ["In", "mir", "ruft's", "faus\u00b7tisch", "schon", ":", "Vor\u00b7bei", "!", "Vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJD", "ADV", "$.", "ADV", "$.", "ADV", "$."], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Nur wenig noch will meinem Herzen taugen:", "tokens": ["Nur", "we\u00b7nig", "noch", "will", "mei\u00b7nem", "Her\u00b7zen", "tau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Blumenduft, ein ferner Glockenklang,", "tokens": ["Ein", "Blu\u00b7men\u00b7duft", ",", "ein", "fer\u00b7ner", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Vogelruf, ein Sonnenuntergang", "tokens": ["Ein", "Vo\u00b7gel\u00b7ruf", ",", "ein", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und dann und wann ein Blick in Kinderaugen.", "tokens": ["Und", "dann", "und", "wann", "ein", "Blick", "in", "Kin\u00b7der\u00b7au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}