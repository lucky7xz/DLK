{"textgrid.poem.34530": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vertr\u00e4umt in seine Wunderwelt der Thoren,", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vertr\u00e4umt in seine Wunderwelt der Thoren,", "tokens": ["Ver\u00b7tr\u00e4umt", "in", "sei\u00b7ne", "Wun\u00b7der\u00b7welt", "der", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "w\u00e4hnt sich Pierrot in tiefen Wald vertrieben,", "tokens": ["w\u00e4hnt", "sich", "Pier\u00b7rot", "in", "tie\u00b7fen", "Wald", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "verlassen und verfehmt von seinen Lieben,", "tokens": ["ver\u00b7las\u00b7sen", "und", "ver\u00b7fehmt", "von", "sei\u00b7nen", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "auf ewig in die Einsamkeit verloren.", "tokens": ["auf", "e\u00b7wig", "in", "die", "Ein\u00b7sam\u00b7keit", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu Hass und Leiden glaubt er sich geboren,", "tokens": ["Zu", "Hass", "und", "Lei\u00b7den", "glaubt", "er", "sich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "w