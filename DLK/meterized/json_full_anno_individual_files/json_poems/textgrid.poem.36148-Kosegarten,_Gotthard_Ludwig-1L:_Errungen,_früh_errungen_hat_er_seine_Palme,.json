{"textgrid.poem.36148": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Errungen, fr\u00fch errungen hat er seine Palme,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Errungen, fr\u00fch errungen hat er seine Palme,", "tokens": ["Er\u00b7run\u00b7gen", ",", "fr\u00fch", "er\u00b7run\u00b7gen", "hat", "er", "sei\u00b7ne", "Pal\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der theure K\u00e4mpfer, und sein Rettungstag", "tokens": ["Der", "theu\u00b7re", "K\u00e4mp\u00b7fer", ",", "und", "sein", "Ret\u00b7tungs\u00b7tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist fr\u00fch ged\u00e4mmert und in Jubelpsalme", "tokens": ["Ist", "fr\u00fch", "ge\u00b7d\u00e4m\u00b7mert", "und", "in", "Ju\u00b7be\u00b7lpsal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVPP", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwandelt all' sein Erdenach!", "tokens": ["Ver\u00b7wan\u00b7delt", "all'", "sein", "Er\u00b7de\u00b7nach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus seinem Glanzpfad, seinem Regenbogenkleide,", "tokens": ["Aus", "sei\u00b7nem", "Glanz\u00b7pfad", ",", "sei\u00b7nem", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit freud- und wehmuthsanftgemischtem Blick,", "tokens": ["Mit", "freu\u00b7d", "und", "weh\u00b7muths\u00b7anft\u00b7ge\u00b7mischtem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schaut nun der Sch\u00f6nverkl\u00e4rte auf euch beide,", "tokens": ["Schaut", "nun", "der", "Sch\u00f6n\u00b7ver\u00b7kl\u00e4r\u00b7te", "auf", "euch", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die er hienieden liebt', zur\u00fcck. \u2013", "tokens": ["Die", "er", "hien\u00b7ie\u00b7den", "liebt'", ",", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er liebte dich, o J\u00fcngling, liebte deine Seele,", "tokens": ["Er", "lieb\u00b7te", "dich", ",", "o", "J\u00fcng\u00b7ling", ",", "lieb\u00b7te", "dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00e4nkte deinen Durst nach Wissenschaft,", "tokens": ["Und", "tr\u00e4nk\u00b7te", "dei\u00b7nen", "Durst", "nach", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und rief das Kraftwort tief in deine Seele:", "tokens": ["Und", "rief", "das", "Kraft\u00b7wort", "tief", "in", "dei\u00b7ne", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbsey, J\u00fcngling, fromm und tugendhaft!\u00ab", "tokens": ["\u00bb", "sey", ",", "J\u00fcng\u00b7ling", ",", "fromm", "und", "tu\u00b7gend\u00b7haft", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er liebte dich, o M\u00e4dchen! Wie der Engel einer,", "tokens": ["Er", "lieb\u00b7te", "dich", ",", "o", "M\u00e4d\u00b7chen", "!", "Wie", "der", "En\u00b7gel", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "PWAV", "ART", "NN", "ART", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gott dem Erdenwaller zugesellt,", "tokens": ["Die", "Gott", "dem", "Er\u00b7den\u00b7wal\u00b7ler", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So liebt' er dich, so schwang er deine Seele", "tokens": ["So", "liebt'", "er", "dich", ",", "so", "schwang", "er", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erdabw\u00e4rts zu der bessern Welt. \u2013", "tokens": ["Erd\u00b7ab\u00b7w\u00e4rts", "zu", "der", "bes\u00b7sern", "Welt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr habt ihn auch geliebt! Ihr saht voll Ahndungsschauer \u2013", "tokens": ["Ihr", "habt", "ihn", "auch", "ge\u00b7liebt", "!", "Ihr", "saht", "voll", "Ahn\u00b7dungs\u00b7schau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zween Monden sind's \u2013 den Guten von euch gehn", "tokens": ["Zween", "Mon\u00b7den", "sin\u00b7d's", "\u2013", "den", "Gu\u00b7ten", "von", "euch", "gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "$(", "ART", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und, nun er hin ist, hab' ich eure Trauer", "tokens": ["Und", ",", "nun", "er", "hin", "ist", ",", "hab'", "ich", "eu\u00b7re", "Trau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "PPER", "ADV", "VAFIN", "$,", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eure Thr\u00e4nen flie\u00dfen sehn. \u2013", "tokens": ["Und", "eu\u00b7re", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "sehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n ist's und menschlich, um den Todten Gottes klagen,", "tokens": ["Sch\u00f6n", "ist's", "und", "menschlich", ",", "um", "den", "Tod\u00b7ten", "Got\u00b7tes", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "$,", "KOUI", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und tr\u00f6stend ist die Thr\u00e4ne, die ihn ehrt.", "tokens": ["Und", "tr\u00f6s\u00b7tend", "ist", "die", "Thr\u00e4\u00b7ne", ",", "die", "ihn", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, Vielgeliebte, denkt bei euren Klagen:", "tokens": ["Doch", ",", "Viel\u00b7ge\u00b7lieb\u00b7te", ",", "denkt", "bei", "eu\u00b7ren", "Kla\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er selbst ist nie beklagenswerth.", "tokens": ["Er", "selbst", "ist", "nie", "be\u00b7kla\u00b7gens\u00b7werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Begl\u00fcckt, begl\u00fcckt ist er, und gl\u00e4nzt im Siegerkranze,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "be\u00b7gl\u00fcckt", "ist", "er", ",", "und", "gl\u00e4nzt", "im", "Sie\u00b7ger\u00b7kran\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VAFIN", "PPER", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den ihm die j\u00fcngste Saronstochter wand,", "tokens": ["Den", "ihm", "die", "j\u00fcngs\u00b7te", "Sa\u00b7ron\u00b7stoch\u00b7ter", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schwingt die Palm', und jauchzt, das Aug' voll Glanze,", "tokens": ["Und", "schwingt", "die", "Palm'", ",", "und", "jauchzt", ",", "das", "Aug'", "voll", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er den gro\u00dfen Sieg bestand.", "tokens": ["Da\u00df", "er", "den", "gro\u00b7\u00dfen", "Sieg", "be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihn ewig lieben, sein, auch nach versiegter Z\u00e4hre,", "tokens": ["Ihn", "e\u00b7wig", "lie\u00b7ben", ",", "sein", ",", "auch", "nach", "ver\u00b7sieg\u00b7ter", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,", "VAINF", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit hei\u00dfem Danksturm eingedenk zu seyn,", "tokens": ["Mit", "hei\u00b7\u00dfem", "Dank\u00b7sturm", "ein\u00b7ge\u00b7denk", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist Pflicht, ist mehr dem Hingeschiednen Ehre,", "tokens": ["Ist", "Pflicht", ",", "ist", "mehr", "dem", "Hin\u00b7ge\u00b7schied\u00b7nen", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Marmorschrift auf seinem Stein \u2013", "tokens": ["Als", "Mar\u00b7mor\u00b7schrift", "auf", "sei\u00b7nem", "Stein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und euch, welch' Ruhm, welch' Trost in jenem Hochgedanken:", "tokens": ["Und", "euch", ",", "welch'", "Ruhm", ",", "welch'", "Trost", "in", "je\u00b7nem", "Hoch\u00b7ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWAT", "NN", "$,", "PWAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der uns als Freund, als Lehrer, liebete,", "tokens": ["Der", "uns", "als", "Freund", ",", "als", "Leh\u00b7rer", ",", "lie\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der trinkt den Quell nun, den die V\u00e4ter tranken,", "tokens": ["Der", "trinkt", "den", "Quell", "nun", ",", "den", "die", "V\u00e4\u00b7ter", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und blickt auf uns aus seiner H\u00f6h'.", "tokens": ["Und", "blickt", "auf", "uns", "aus", "sei\u00b7ner", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O Freund, o Freundinn, wenn im wilden Sturmget\u00fcmmel", "tokens": ["O", "Freund", ",", "o", "Freun\u00b7dinn", ",", "wenn", "im", "wil\u00b7den", "Sturm\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch Jugendlust durch Nerv' und Adern z\u00fcckt,", "tokens": ["Euch", "Ju\u00b7gend\u00b7lust", "durch", "Ner\u00b7v'", "und", "A\u00b7dern", "z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So denkt, wie euer Freund aus seinem Himmel", "tokens": ["So", "denkt", ",", "wie", "eu\u00b7er", "Freund", "aus", "sei\u00b7nem", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mitleidig auf euch niederblickt.", "tokens": ["Mit\u00b7lei\u00b7dig", "auf", "euch", "nie\u00b7der\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ringt aber euer Geist im sch\u00f6nen Tugendstreite", "tokens": ["Ringt", "a\u00b7ber", "eu\u00b7er", "Geist", "im", "sch\u00f6\u00b7nen", "Tu\u00b7gend\u00b7strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Geiste eures Hingegangnen nach,", "tokens": ["Dem", "Geis\u00b7te", "eu\u00b7res", "Hin\u00b7ge\u00b7gang\u00b7nen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So denkt: dem Schlachttag folgt ein Tag der Beute,", "tokens": ["So", "denkt", ":", "dem", "Schlacht\u00b7tag", "folgt", "ein", "Tag", "der", "Beu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Rast dem schw\u00fclen Arbeittag.", "tokens": ["Und", "Rast", "dem", "schw\u00fc\u00b7len", "Ar\u00b7beit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Denkt: Gottes Saat erstirbt auf Hoffnung; ihrer harret", "tokens": ["Denkt", ":", "Got\u00b7tes", "Saat", "er\u00b7stirbt", "auf", "Hoff\u00b7nung", ";", "ih\u00b7rer", "har\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "NN", "NN", "VVFIN", "APPR", "NN", "$.", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Tag der Ernte, dunkelsch\u00f6n und gro\u00df.", "tokens": ["Ein", "Tag", "der", "Ern\u00b7te", ",", "dun\u00b7kel\u00b7sch\u00f6n", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da windet sich der Staub in Staub verscharret", "tokens": ["Da", "win\u00b7det", "sich", "der", "Staub", "in", "Staub", "ver\u00b7schar\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf seines Herrn Posaunruf los.", "tokens": ["Auf", "sei\u00b7nes", "Herrn", "Po\u00b7saun\u00b7ruf", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Rings rauscht das rege Feld vom Auferstehn. Es gl\u00e4nzen", "tokens": ["Rings", "rauscht", "das", "re\u00b7ge", "Feld", "vom", "Auf\u00b7er\u00b7stehn", ".", "Es", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sicheln, und die Schnitter jauchzen laut,", "tokens": ["Die", "Si\u00b7cheln", ",", "und", "die", "Schnit\u00b7ter", "jauch\u00b7zen", "laut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und freudig stehn in goldnen Erntekr\u00e4nzen,", "tokens": ["Und", "freu\u00b7dig", "stehn", "in", "gold\u00b7nen", "Ern\u00b7te\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die hier das Feld des Herrn gebaut.", "tokens": ["Die", "hier", "das", "Feld", "des", "Herrn", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Dann st\u00fcrzen wir mit euch, die sich hier uns vertrauten,", "tokens": ["Dann", "st\u00fcr\u00b7zen", "wir", "mit", "euch", ",", "die", "sich", "hier", "uns", "ver\u00b7trau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PRELS", "PRF", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Stuhl des gro\u00dfen Ernteherrn und schrei'n:", "tokens": ["Zum", "Stuhl", "des", "gro\u00b7\u00dfen", "Ern\u00b7te\u00b7herrn", "und", "schrei'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbsieh hier die Garben, Herr, die wir dir bauten!", "tokens": ["\u00bb", "sieh", "hier", "die", "Gar\u00b7ben", ",", "Herr", ",", "die", "wir", "dir", "bau\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "ART", "NN", "$,", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nimm sie in deine Scheuern ein!!\u00ab", "tokens": ["Nimm", "sie", "in", "dei\u00b7ne", "Scheu\u00b7ern", "ein", "!!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Errungen, fr\u00fch errungen hat er seine Palme,", "tokens": ["Er\u00b7run\u00b7gen", ",", "fr\u00fch", "er\u00b7run\u00b7gen", "hat", "er", "sei\u00b7ne", "Pal\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der theure K\u00e4mpfer, und sein Rettungstag", "tokens": ["Der", "theu\u00b7re", "K\u00e4mp\u00b7fer", ",", "und", "sein", "Ret\u00b7tungs\u00b7tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist fr\u00fch ged\u00e4mmert und in Jubelpsalme", "tokens": ["Ist", "fr\u00fch", "ge\u00b7d\u00e4m\u00b7mert", "und", "in", "Ju\u00b7be\u00b7lpsal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVPP", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwandelt all' sein Erdenach!", "tokens": ["Ver\u00b7wan\u00b7delt", "all'", "sein", "Er\u00b7de\u00b7nach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Aus seinem Glanzpfad, seinem Regenbogenkleide,", "tokens": ["Aus", "sei\u00b7nem", "Glanz\u00b7pfad", ",", "sei\u00b7nem", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit freud- und wehmuthsanftgemischtem Blick,", "tokens": ["Mit", "freu\u00b7d", "und", "weh\u00b7muths\u00b7anft\u00b7ge\u00b7mischtem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schaut nun der Sch\u00f6nverkl\u00e4rte auf euch beide,", "tokens": ["Schaut", "nun", "der", "Sch\u00f6n\u00b7ver\u00b7kl\u00e4r\u00b7te", "auf", "euch", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die er hienieden liebt', zur\u00fcck. \u2013", "tokens": ["Die", "er", "hien\u00b7ie\u00b7den", "liebt'", ",", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Er liebte dich, o J\u00fcngling, liebte deine Seele,", "tokens": ["Er", "lieb\u00b7te", "dich", ",", "o", "J\u00fcng\u00b7ling", ",", "lieb\u00b7te", "dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00e4nkte deinen Durst nach Wissenschaft,", "tokens": ["Und", "tr\u00e4nk\u00b7te", "dei\u00b7nen", "Durst", "nach", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und rief das Kraftwort tief in deine Seele:", "tokens": ["Und", "rief", "das", "Kraft\u00b7wort", "tief", "in", "dei\u00b7ne", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbsey, J\u00fcngling, fromm und tugendhaft!\u00ab", "tokens": ["\u00bb", "sey", ",", "J\u00fcng\u00b7ling", ",", "fromm", "und", "tu\u00b7gend\u00b7haft", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Er liebte dich, o M\u00e4dchen! Wie der Engel einer,", "tokens": ["Er", "lieb\u00b7te", "dich", ",", "o", "M\u00e4d\u00b7chen", "!", "Wie", "der", "En\u00b7gel", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "PWAV", "ART", "NN", "ART", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gott dem Erdenwaller zugesellt,", "tokens": ["Die", "Gott", "dem", "Er\u00b7den\u00b7wal\u00b7ler", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So liebt' er dich, so schwang er deine Seele", "tokens": ["So", "liebt'", "er", "dich", ",", "so", "schwang", "er", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erdabw\u00e4rts zu der bessern Welt. \u2013", "tokens": ["Erd\u00b7ab\u00b7w\u00e4rts", "zu", "der", "bes\u00b7sern", "Welt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Ihr habt ihn auch geliebt! Ihr saht voll Ahndungsschauer \u2013", "tokens": ["Ihr", "habt", "ihn", "auch", "ge\u00b7liebt", "!", "Ihr", "saht", "voll", "Ahn\u00b7dungs\u00b7schau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zween Monden sind's \u2013 den Guten von euch gehn", "tokens": ["Zween", "Mon\u00b7den", "sin\u00b7d's", "\u2013", "den", "Gu\u00b7ten", "von", "euch", "gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "NE", "$(", "ART", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und, nun er hin ist, hab' ich eure Trauer", "tokens": ["Und", ",", "nun", "er", "hin", "ist", ",", "hab'", "ich", "eu\u00b7re", "Trau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "PPER", "ADV", "VAFIN", "$,", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eure Thr\u00e4nen flie\u00dfen sehn. \u2013", "tokens": ["Und", "eu\u00b7re", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "sehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Sch\u00f6n ist's und menschlich, um den Todten Gottes klagen,", "tokens": ["Sch\u00f6n", "ist's", "und", "menschlich", ",", "um", "den", "Tod\u00b7ten", "Got\u00b7tes", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "$,", "KOUI", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und tr\u00f6stend ist die Thr\u00e4ne, die ihn ehrt.", "tokens": ["Und", "tr\u00f6s\u00b7tend", "ist", "die", "Thr\u00e4\u00b7ne", ",", "die", "ihn", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, Vielgeliebte, denkt bei euren Klagen:", "tokens": ["Doch", ",", "Viel\u00b7ge\u00b7lieb\u00b7te", ",", "denkt", "bei", "eu\u00b7ren", "Kla\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er selbst ist nie beklagenswerth.", "tokens": ["Er", "selbst", "ist", "nie", "be\u00b7kla\u00b7gens\u00b7werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Begl\u00fcckt, begl\u00fcckt ist er, und gl\u00e4nzt im Siegerkranze,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "be\u00b7gl\u00fcckt", "ist", "er", ",", "und", "gl\u00e4nzt", "im", "Sie\u00b7ger\u00b7kran\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VAFIN", "PPER", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den ihm die j\u00fcngste Saronstochter wand,", "tokens": ["Den", "ihm", "die", "j\u00fcngs\u00b7te", "Sa\u00b7ron\u00b7stoch\u00b7ter", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schwingt die Palm', und jauchzt, das Aug' voll Glanze,", "tokens": ["Und", "schwingt", "die", "Palm'", ",", "und", "jauchzt", ",", "das", "Aug'", "voll", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er den gro\u00dfen Sieg bestand.", "tokens": ["Da\u00df", "er", "den", "gro\u00b7\u00dfen", "Sieg", "be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Ihn ewig lieben, sein, auch nach versiegter Z\u00e4hre,", "tokens": ["Ihn", "e\u00b7wig", "lie\u00b7ben", ",", "sein", ",", "auch", "nach", "ver\u00b7sieg\u00b7ter", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,", "VAINF", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit hei\u00dfem Danksturm eingedenk zu seyn,", "tokens": ["Mit", "hei\u00b7\u00dfem", "Dank\u00b7sturm", "ein\u00b7ge\u00b7denk", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist Pflicht, ist mehr dem Hingeschiednen Ehre,", "tokens": ["Ist", "Pflicht", ",", "ist", "mehr", "dem", "Hin\u00b7ge\u00b7schied\u00b7nen", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Marmorschrift auf seinem Stein \u2013", "tokens": ["Als", "Mar\u00b7mor\u00b7schrift", "auf", "sei\u00b7nem", "Stein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Und euch, welch' Ruhm, welch' Trost in jenem Hochgedanken:", "tokens": ["Und", "euch", ",", "welch'", "Ruhm", ",", "welch'", "Trost", "in", "je\u00b7nem", "Hoch\u00b7ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWAT", "NN", "$,", "PWAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der uns als Freund, als Lehrer, liebete,", "tokens": ["Der", "uns", "als", "Freund", ",", "als", "Leh\u00b7rer", ",", "lie\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der trinkt den Quell nun, den die V\u00e4ter tranken,", "tokens": ["Der", "trinkt", "den", "Quell", "nun", ",", "den", "die", "V\u00e4\u00b7ter", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und blickt auf uns aus seiner H\u00f6h'.", "tokens": ["Und", "blickt", "auf", "uns", "aus", "sei\u00b7ner", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "O Freund, o Freundinn, wenn im wilden Sturmget\u00fcmmel", "tokens": ["O", "Freund", ",", "o", "Freun\u00b7dinn", ",", "wenn", "im", "wil\u00b7den", "Sturm\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch Jugendlust durch Nerv' und Adern z\u00fcckt,", "tokens": ["Euch", "Ju\u00b7gend\u00b7lust", "durch", "Ner\u00b7v'", "und", "A\u00b7dern", "z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So denkt, wie euer Freund aus seinem Himmel", "tokens": ["So", "denkt", ",", "wie", "eu\u00b7er", "Freund", "aus", "sei\u00b7nem", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mitleidig auf euch niederblickt.", "tokens": ["Mit\u00b7lei\u00b7dig", "auf", "euch", "nie\u00b7der\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Ringt aber euer Geist im sch\u00f6nen Tugendstreite", "tokens": ["Ringt", "a\u00b7ber", "eu\u00b7er", "Geist", "im", "sch\u00f6\u00b7nen", "Tu\u00b7gend\u00b7strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Geiste eures Hingegangnen nach,", "tokens": ["Dem", "Geis\u00b7te", "eu\u00b7res", "Hin\u00b7ge\u00b7gang\u00b7nen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So denkt: dem Schlachttag folgt ein Tag der Beute,", "tokens": ["So", "denkt", ":", "dem", "Schlacht\u00b7tag", "folgt", "ein", "Tag", "der", "Beu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Rast dem schw\u00fclen Arbeittag.", "tokens": ["Und", "Rast", "dem", "schw\u00fc\u00b7len", "Ar\u00b7beit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Denkt: Gottes Saat erstirbt auf Hoffnung; ihrer harret", "tokens": ["Denkt", ":", "Got\u00b7tes", "Saat", "er\u00b7stirbt", "auf", "Hoff\u00b7nung", ";", "ih\u00b7rer", "har\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "NN", "NN", "VVFIN", "APPR", "NN", "$.", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Tag der Ernte, dunkelsch\u00f6n und gro\u00df.", "tokens": ["Ein", "Tag", "der", "Ern\u00b7te", ",", "dun\u00b7kel\u00b7sch\u00f6n", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da windet sich der Staub in Staub verscharret", "tokens": ["Da", "win\u00b7det", "sich", "der", "Staub", "in", "Staub", "ver\u00b7schar\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf seines Herrn Posaunruf los.", "tokens": ["Auf", "sei\u00b7nes", "Herrn", "Po\u00b7saun\u00b7ruf", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Rings rauscht das rege Feld vom Auferstehn. Es gl\u00e4nzen", "tokens": ["Rings", "rauscht", "das", "re\u00b7ge", "Feld", "vom", "Auf\u00b7er\u00b7stehn", ".", "Es", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sicheln, und die Schnitter jauchzen laut,", "tokens": ["Die", "Si\u00b7cheln", ",", "und", "die", "Schnit\u00b7ter", "jauch\u00b7zen", "laut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und freudig stehn in goldnen Erntekr\u00e4nzen,", "tokens": ["Und", "freu\u00b7dig", "stehn", "in", "gold\u00b7nen", "Ern\u00b7te\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die hier das Feld des Herrn gebaut.", "tokens": ["Die", "hier", "das", "Feld", "des", "Herrn", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.28": {"line.1": {"text": "Dann st\u00fcrzen wir mit euch, die sich hier uns vertrauten,", "tokens": ["Dann", "st\u00fcr\u00b7zen", "wir", "mit", "euch", ",", "die", "sich", "hier", "uns", "ver\u00b7trau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PRELS", "PRF", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Stuhl des gro\u00dfen Ernteherrn und schrei'n:", "tokens": ["Zum", "Stuhl", "des", "gro\u00b7\u00dfen", "Ern\u00b7te\u00b7herrn", "und", "schrei'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbsieh hier die Garben, Herr, die wir dir bauten!", "tokens": ["\u00bb", "sieh", "hier", "die", "Gar\u00b7ben", ",", "Herr", ",", "die", "wir", "dir", "bau\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "ART", "NN", "$,", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nimm sie in deine Scheuern ein!!\u00ab", "tokens": ["Nimm", "sie", "in", "dei\u00b7ne", "Scheu\u00b7ern", "ein", "!!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}