{"textgrid.poem.52906": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwarum denn nur in allen Sachen", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwarum denn nur in allen Sachen", "tokens": ["\u00bb", "wa\u00b7rum", "denn", "nur", "in", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den unzufrieden Tadler machen?", "tokens": ["Den", "un\u00b7zu\u00b7frie\u00b7den", "Tad\u00b7ler", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was spielst Du, nimmer-m\u00fcder Krittler,", "tokens": ["Was", "spielst", "Du", ",", "nim\u00b7mer\u00b7m\u00fc\u00b7der", "Kritt\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht lieber freundlich den Vermittler?", "tokens": ["Nicht", "lie\u00b7ber", "freund\u00b7lich", "den", "Ver\u00b7mitt\u00b7ler", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Sinn besieht mit rechtem Willen", "tokens": ["Dein", "Sinn", "be\u00b7sieht", "mit", "rech\u00b7tem", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt durch schwarzgef\u00e4rbte Brillen,", "tokens": ["Die", "Welt", "durch", "schwarz\u00b7ge\u00b7f\u00e4rb\u00b7te", "Bril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in Kritik, in Wunsch und Klage", "tokens": ["Und", "in", "Kri\u00b7tik", ",", "in", "Wunsch", "und", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vertr\u00e4umst Du Deine besten Tage.", "tokens": ["Ver\u00b7tr\u00e4umst", "Du", "Dei\u00b7ne", "bes\u00b7ten", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du wirst durch Predigen und Schimpfen", "tokens": ["Du", "wirst", "durch", "Pre\u00b7di\u00b7gen", "und", "Schimp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Mi\u00dfmut in die Menschen impfen,", "tokens": ["Nur", "Mi\u00df\u00b7mut", "in", "die", "Men\u00b7schen", "imp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch macht Dein wildes L\u00e4stern", "tokens": ["Und", "den\u00b7noch", "macht", "Dein", "wil\u00b7des", "L\u00e4s\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das tr\u00e4ge Heute nicht zum Gestern.", "tokens": ["Das", "tr\u00e4\u00b7ge", "Heu\u00b7te", "nicht", "zum", "Ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Du kannst das Rad der Zeit nicht drehen,", "tokens": ["Du", "kannst", "das", "Rad", "der", "Zeit", "nicht", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird im alten Gleise gehen,", "tokens": ["Es", "wird", "im", "al\u00b7ten", "Glei\u00b7se", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wort befreit die Erde nimmer,", "tokens": ["Das", "Wort", "be\u00b7freit", "die", "Er\u00b7de", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es macht nur schlimme Dinge schlimmer.", "tokens": ["Es", "macht", "nur", "schlim\u00b7me", "Din\u00b7ge", "schlim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Genie\u00df doch wie die andren tuen,", "tokens": ["Ge\u00b7nie\u00df", "doch", "wie", "die", "an\u00b7dren", "tu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die weise dort im Schatten ruhen,", "tokens": ["Die", "wei\u00b7se", "dort", "im", "Schat\u00b7ten", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und statt die Macht keck zu bestreiten,", "tokens": ["Und", "statt", "die", "Macht", "keck", "zu", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Such schlau an ihr emporzugleiten.", "tokens": ["Such", "schlau", "an", "ihr", "em\u00b7por\u00b7zu\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was k\u00fcmmern Dich die freien Pressen,", "tokens": ["Was", "k\u00fcm\u00b7mern", "Dich", "die", "frei\u00b7en", "Pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du zu trinken hast, zu essen?", "tokens": ["Wenn", "du", "zu", "trin\u00b7ken", "hast", ",", "zu", "es\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was das allgemeine Beste,", "tokens": ["Und", "was", "das", "all\u00b7ge\u00b7mei\u00b7ne", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Du behaglich sitz'st im Neste?", "tokens": ["Wenn", "Du", "be\u00b7hag\u00b7lich", "sitz'st", "im", "Nes\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh zu, wie hoch's die Klugen treiben,", "tokens": ["Sieh", "zu", ",", "wie", "hoch's", "die", "Klu\u00b7gen", "trei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Willst Du am Boden ewig bleiben?", "tokens": ["Willst", "Du", "am", "Bo\u00b7den", "e\u00b7wig", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast die Kraft, nun brauch sie richtig", "tokens": ["Du", "hast", "die", "Kraft", ",", "nun", "brauch", "sie", "rich\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mach Dein Pfund durch Wucher wichtig!\u00ab", "tokens": ["Und", "mach", "Dein", "Pfund", "durch", "Wu\u00b7cher", "wich\u00b7tig", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u2013 Und h\u00e4tten so wie Du gedacht", "tokens": ["\u2013", "Und", "h\u00e4t\u00b7ten", "so", "wie", "Du", "ge\u00b7dacht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VAFIN", "ADV", "KOKOM", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die unsre V\u00e4ter sind,", "tokens": ["Die", "uns\u00b7re", "V\u00e4\u00b7ter", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00e4r's im Land noch immer Nacht", "tokens": ["So", "w\u00e4r's", "im", "Land", "noch", "im\u00b7mer", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wir noch immer blind.", "tokens": ["Und", "wir", "noch", "im\u00b7mer", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wohl ist es schwach und arm mein Wort,", "tokens": ["Wohl", "ist", "es", "schwach", "und", "arm", "mein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich nur Dichter bin,", "tokens": ["Weil", "ich", "nur", "Dich\u00b7ter", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch tr\u00e4gt's vielleicht ein L\u00fcftchen fort,", "tokens": ["Doch", "tr\u00e4gt's", "viel\u00b7leicht", "ein", "L\u00fcft\u00b7chen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wei\u00df wie und wohin?", "tokens": ["Wer", "wei\u00df", "wie", "und", "wo\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "KON", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es gleicht dem dunklen Samenkorn,", "tokens": ["Es", "gleicht", "dem", "dunk\u00b7len", "Sa\u00b7men\u00b7korn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du kennst das alte Bild:", "tokens": ["Du", "kennst", "das", "al\u00b7te", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eins f\u00e4llt in Busch und Stein und Dorn,", "tokens": ["Eins", "f\u00e4llt", "in", "Busch", "und", "Stein", "und", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eins in ein Fruchtgefild.", "tokens": ["Eins", "in", "ein", "Frucht\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Vielleicht bl\u00fcht \u00fcber Tag und Jahr,", "tokens": ["Viel\u00b7leicht", "bl\u00fcht", "\u00fc\u00b7ber", "Tag", "und", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn l\u00e4ngst der S\u00e4'mann tot,", "tokens": ["Wenn", "l\u00e4ngst", "der", "S\u00e4'\u00b7mann", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf steilen Felsen wunderbar", "tokens": ["Auf", "stei\u00b7len", "Fel\u00b7sen", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bl\u00fcmlein wei\u00df und rot.", "tokens": ["Ein", "Bl\u00fcm\u00b7lein", "wei\u00df", "und", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Fr\u00fchling kommt schon \u00fcber Nacht,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", "schon", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ziehn erst die Schwalben um;", "tokens": ["Ziehn", "erst", "die", "Schwal\u00b7ben", "um", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Weil eine keinen Sommer macht,", "tokens": ["Weil", "ei\u00b7ne", "kei\u00b7nen", "Som\u00b7mer", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum sei sie noch nicht stumm.", "tokens": ["Drum", "sei", "sie", "noch", "nicht", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und wenn ich nicht, wie Ihr es wollt,", "tokens": ["Und", "wenn", "ich", "nicht", ",", "wie", "Ihr", "es", "wollt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch lobe mit Geschrei, \u2013", "tokens": ["Euch", "lo\u00b7be", "mit", "Ge\u00b7schrei", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ei nun! ich singe nicht um Gold", "tokens": ["Ei", "nun", "!", "ich", "sin\u00b7ge", "nicht", "um", "Gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin kein Papagei.", "tokens": ["Und", "bin", "kein", "Pa\u00b7pa\u00b7gei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ihr mietet Euch des Zeugs genug", "tokens": ["Ihr", "mie\u00b7tet", "Euch", "des", "Zeugs", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcr Euch sind sie all,", "tokens": ["Und", "f\u00fcr", "Euch", "sind", "sie", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "PIAT", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So la\u00dft der Lerche ihren Flug,", "tokens": ["So", "la\u00dft", "der", "Ler\u00b7che", "ih\u00b7ren", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Lied der Nachtigall.", "tokens": ["Ihr", "Lied", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Nach Hohem steht mir nicht der Sinn,", "tokens": ["Nach", "Ho\u00b7hem", "steht", "mir", "nicht", "der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Ihr es meint, Ihr Herrn,", "tokens": ["Wie", "Ihr", "es", "meint", ",", "Ihr", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach Sternen streb' ich freilich hin,", "tokens": ["Nach", "Ster\u00b7nen", "streb'", "ich", "frei\u00b7lich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nicht nach einem Stern.", "tokens": ["Doch", "nicht", "nach", "ei\u00b7nem", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Mit Euch genie\u00dfen mag ich nicht,", "tokens": ["Mit", "Euch", "ge\u00b7nie\u00b7\u00dfen", "mag", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr weint ja nicht mit mir,", "tokens": ["Ihr", "weint", "ja", "nicht", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was das Herz entzwei mir bricht,", "tokens": ["Und", "was", "das", "Herz", "ent\u00b7zwei", "mir", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! dazu l\u00e4chelt Ihr.", "tokens": ["Ach", "!", "da\u00b7zu", "l\u00e4\u00b7chelt", "Ihr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Da\u00df ich die Welt nicht anders seh,", "tokens": ["Da\u00df", "ich", "die", "Welt", "nicht", "an\u00b7ders", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie \u2013 durch Euch! \u2013 sie ward,", "tokens": ["Als", "wie", "\u2013", "durch", "Euch", "!", "\u2013", "sie", "ward", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "$(", "APPR", "PPER", "$.", "$(", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Glaubt mir, das tut Euch minder weh,", "tokens": ["Glaubt", "mir", ",", "das", "tut", "Euch", "min\u00b7der", "weh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als mir und meiner Art.", "tokens": ["Als", "mir", "und", "mei\u00b7ner", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Geh du die Wege deiner Pflicht,", "tokens": ["Geh", "du", "die", "We\u00b7ge", "dei\u00b7ner", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich die meinen geh';", "tokens": ["Weil", "ich", "die", "mei\u00b7nen", "geh'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hadre mit dir wahrlich nicht,", "tokens": ["Ich", "had\u00b7re", "mit", "dir", "wahr\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und damit, Mann, ade!", "tokens": ["Und", "da\u00b7mit", ",", "Mann", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "$,", "NN", "$,", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.19": {"line.1": {"text": "\u00bbwarum denn nur in allen Sachen", "tokens": ["\u00bb", "wa\u00b7rum", "denn", "nur", "in", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den unzufrieden Tadler machen?", "tokens": ["Den", "un\u00b7zu\u00b7frie\