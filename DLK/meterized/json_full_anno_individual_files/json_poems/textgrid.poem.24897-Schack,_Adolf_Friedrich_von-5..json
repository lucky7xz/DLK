{"textgrid.poem.24897": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erloschen ist der Stern von Jemen,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "der", "Stern", "von", "Je\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerst\u00f6rt die Welt, die er beschien;", "tokens": ["Zer\u00b7st\u00f6rt", "die", "Welt", ",", "die", "er", "be\u00b7schien", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts blieb zur\u00fcck als bleiche Schemen,", "tokens": ["Nichts", "blieb", "zu\u00b7r\u00fcck", "als", "blei\u00b7che", "Sche\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die n\u00e4chtlich um die Tr\u00fcmmer ziehn.", "tokens": ["Die", "n\u00e4cht\u00b7lich", "um", "die", "Tr\u00fcm\u00b7mer", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergebens, da\u00df ihr nach dem Volke,", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "da\u00df", "ihr", "nach", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor dem die Erde bebte, fragt;", "tokens": ["Vor", "dem", "die", "Er\u00b7de", "beb\u00b7te", ",", "fragt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie nach dem Sturm die letzte Wolke", "tokens": ["Wie", "nach", "dem", "Sturm", "die", "letz\u00b7te", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlassen durch den Himmel jagt,", "tokens": ["Ver\u00b7las\u00b7sen", "durch", "den", "Him\u00b7mel", "jagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So, wo im scheitelrechten Brande", "tokens": ["So", ",", "wo", "im", "schei\u00b7tel\u00b7rech\u00b7ten", "Bran\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonne alles Leben dorrt,", "tokens": ["Der", "Son\u00b7ne", "al\u00b7les", "Le\u00b7ben", "dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Irrt es in Maghribs weh'ndem Sande", "tokens": ["Irrt", "es", "in", "Ma\u00b7ghribs", "weh'n\u00b7dem", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Unstet dahin von Ort zu Ort.", "tokens": ["Un\u00b7stet", "da\u00b7hin", "von", "Ort", "zu", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Blickt hin, wo zitternd die Gazellen", "tokens": ["Blickt", "hin", ",", "wo", "zit\u00b7ternd", "die", "Ga\u00b7zel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schakal fliehn, der heiser bellt!", "tokens": ["Den", "Scha\u00b7kal", "fliehn", ",", "der", "hei\u00b7ser", "bellt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hei\u00df schl\u00e4gt die W\u00fcste ihre Wellen,", "tokens": ["Hei\u00df", "schl\u00e4gt", "die", "W\u00fcs\u00b7te", "ih\u00b7re", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Hauch des Samums klappt das Zelt;", "tokens": ["Im", "Hauch", "des", "Sa\u00b7mums", "klappt", "das", "Zelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gekauert auf die d\u00fcrre Erde,", "tokens": ["Ge\u00b7kau\u00b7ert", "auf", "die", "d\u00fcr\u00b7re", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gebr\u00e4unt der Nacken und der Arm,", "tokens": ["Ge\u00b7br\u00e4unt", "der", "Na\u00b7cken", "und", "der", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt \u2013 um ihn her die magre Herde \u2013", "tokens": ["Liegt", "\u2013", "um", "ihn", "her", "die", "mag\u00b7re", "Her\u00b7de", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUI", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Halbnackt der Beduinenschwarm.", "tokens": ["Halb\u00b7nackt", "der", "Be\u00b7du\u00b7i\u00b7nen\u00b7schwarm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nichts nennt er sein als das Gestr\u00fcppe", "tokens": ["Nichts", "nennt", "er", "sein", "als", "das", "Ge\u00b7str\u00fcp\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "VAINF", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des kahlen Bodens, das ihn n\u00e4hrt,", "tokens": ["Des", "kah\u00b7len", "Bo\u00b7dens", ",", "das", "ihn", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr seine Schafe eine Krippe,", "tokens": ["F\u00fcr", "sei\u00b7ne", "Scha\u00b7fe", "ei\u00b7ne", "Krip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Stein f\u00fcr seinen Feuerherd.", "tokens": ["Den", "Stein", "f\u00fcr", "sei\u00b7nen", "Feu\u00b7er\u00b7herd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Oed' ist der Geist den W\u00fcstenkindern,", "tokens": ["O\u00b7ed'", "ist", "der", "Geist", "den", "W\u00fcs\u00b7ten\u00b7kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So wie die Erde um sie her;", "tokens": ["So", "wie", "die", "Er\u00b7de", "um", "sie", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat, um ihre Pein zu lindern,", "tokens": ["Es", "hat", ",", "um", "ih\u00b7re", "Pein", "zu", "lin\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Auge keine Thr\u00e4nen mehr.", "tokens": ["Ihr", "Au\u00b7ge", "kei\u00b7ne", "Thr\u00e4\u00b7nen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Einmal im Jahr nur, wenn die Horden", "tokens": ["Ein\u00b7mal", "im", "Jahr", "nur", ",", "wenn", "die", "Hor\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Am Abend vor den Zelten stehn", "tokens": ["Am", "A\u00b7bend", "vor", "den", "Zel\u00b7ten", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber sich zum fernen Norden", "tokens": ["Und", "\u00fc\u00b7ber", "sich", "zum", "fer\u00b7nen", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kranichheere fliegen sehn:", "tokens": ["Die", "Kra\u00b7nich\u00b7hee\u00b7re", "flie\u00b7gen", "sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dann quillt von ihren Lippen leise", "tokens": ["Dann", "quillt", "von", "ih\u00b7ren", "Lip\u00b7pen", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Seufzer, ihre Thr\u00e4ne rinnt,", "tokens": ["Ein", "Seuf\u00b7zer", ",", "ih\u00b7re", "Thr\u00e4\u00b7ne", "rinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der J\u00fcngling sinkt ans Herz dem Greise,", "tokens": ["Der", "J\u00fcng\u00b7ling", "sinkt", "ans", "Herz", "dem", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter hebt empor das Kind;", "tokens": ["Die", "Mut\u00b7ter", "hebt", "em\u00b7por", "das", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und schwermutvoll in stillem Harme", "tokens": ["Und", "schwer\u00b7mut\u00b7voll", "in", "stil\u00b7lem", "Har\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sehn sie dem flieh'nden Zuge nach;", "tokens": ["Sehn", "sie", "dem", "flieh'n\u00b7den", "Zu\u00b7ge", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Himmel breiten sie die Arme,", "tokens": ["Zum", "Him\u00b7mel", "brei\u00b7ten", "sie", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Mund zu Munde fliegt ein Ach!", "tokens": ["Von", "Mund", "zu", "Mun\u00b7de", "fliegt", "ein", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbgr\u00fc\u00dft, V\u00f6gel,\u00ab \u2013 rufen sie \u2013 \u00bbdie sch\u00f6ne", "tokens": ["\u00bb", "gr\u00fc\u00dft", ",", "V\u00f6\u00b7gel", ",", "\u00ab", "\u2013", "ru\u00b7fen", "sie", "\u2013", "\u00bb", "die", "sch\u00f6\u00b7ne"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "$(", "$(", "VVFIN", "PPER", "$(", "$(", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Granada, unsrer V\u00e4ter Gl\u00fcck!", "tokens": ["Gra\u00b7na\u00b7da", ",", "uns\u00b7rer", "V\u00e4\u00b7ter", "Gl\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach ihr, der Mutter, schaun die S\u00f6hne", "tokens": ["Nach", "ihr", ",", "der", "Mut\u00b7ter", ",", "schaun", "die", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sehnsuchtvollem Blick zur\u00fcck.", "tokens": ["Mit", "sehn\u00b7sucht\u00b7vol\u00b7lem", "Blick", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "O einmal nur, den wir besessen,", "tokens": ["O", "ein\u00b7mal", "nur", ",", "den", "wir", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den teuren Boden wiedersehn,", "tokens": ["Den", "teu\u00b7ren", "Bo\u00b7den", "wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn k\u00fcssen und mit Thr\u00e4nen n\u00e4ssen \u2013", "tokens": ["Ihn", "k\u00fcs\u00b7sen", "und", "mit", "Thr\u00e4\u00b7nen", "n\u00e4s\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann m\u00f6chten wir zu Grabe gehn.", "tokens": ["Dann", "m\u00f6ch\u00b7ten", "wir", "zu", "Gra\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Zum Eintritt ladet noch die Schwelle", "tokens": ["Zum", "Ein\u00b7tritt", "la\u00b7det", "noch", "die", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Hauses, das uns einst geh\u00f6rt;", "tokens": ["Des", "Hau\u00b7ses", ",", "das", "uns", "einst", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Hofe rauscht die alte Quelle;", "tokens": ["Im", "Ho\u00b7fe", "rauscht", "die", "al\u00b7te", "Quel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Feuer knistert noch am Herd.", "tokens": ["Das", "Feu\u00b7er", "knis\u00b7tert", "noch", "am", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die Schl\u00fcssel zu der Eltern Th\u00fcren", "tokens": ["Die", "Schl\u00fcs\u00b7sel", "zu", "der", "El\u00b7tern", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-