tetra"}, "line.4": {"text": "Uns weiter durch die W\u00fcste ziehn!\u00ab", "tokens": ["Uns", "wei\u00b7ter", "durch", "die", "W\u00fcs\u00b7te", "ziehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Erloschen ist der Stern von Jemen,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "der", "Stern", "von", "Je\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerst\u00f6rt die Welt, die er beschien;", "tokens": ["Zer\u00b7st\u00f6rt", "die", "Welt", ",", "die", "er", "be\u00b7schien", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts blieb zur\u00fcck als bleiche Schemen,", "tokens": ["Nichts", "blieb", "zu\u00b7r\u00fcck", "als", "blei\u00b7che", "Sche\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die n\u00e4chtlich um die Tr\u00fcmmer ziehn.", "tokens": ["Die", "n\u00e4cht\u00b7lich", "um", "die", "Tr\u00fcm\u00b7mer", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Vergebens, da\u00df ihr nach dem Volke,", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "da\u00df", "ihr", "nach", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", 