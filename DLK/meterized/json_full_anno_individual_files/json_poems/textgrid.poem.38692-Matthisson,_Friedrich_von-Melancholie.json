{"textgrid.poem.38692": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Melancholie", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall klagt bang' im Bl\u00fctenschatten", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "klagt", "bang'", "im", "Bl\u00fc\u00b7ten\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie um den Liebling die verla\u00dfne Braut;", "tokens": ["Wie", "um", "den", "Lieb\u00b7ling", "die", "ver\u00b7la\u00df\u00b7ne", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Abendstern blickt auf die Veilchenmatten,", "tokens": ["Der", "A\u00b7bends\u00b7tern", "blickt", "auf", "die", "Veil\u00b7chen\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bla\u00df wie der Schmerz auf Sarkofage schaut;", "tokens": ["Bla\u00df", "wie", "der", "Schmerz", "auf", "Sar\u00b7ko\u00b7fa\u00b7ge", "schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ein Trauerflor scheint ob dem See zu wallen;", "tokens": ["Ein", "Trau\u00b7er\u00b7flor", "scheint", "ob", "dem", "See", "zu", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word",