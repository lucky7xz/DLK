{"dta.poem.1250": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lange wilt du noch mit deinen Sternen prangen?", "tokens": ["Wie", "lan\u00b7ge", "wilt", "du", "noch", "mit", "dei\u00b7nen", "Ster\u00b7nen", "pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie lange soll mir noch der Mond verdr\u00fc\u00dflich seyn?", "tokens": ["Wie", "lan\u00b7ge", "soll", "mir", "noch", "der", "Mond", "ver\u00b7dr\u00fc\u00df\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zeuch/ bitt ich/ braune Nacht den tuncklen Schatten ein:", "tokens": ["Zeuch", "/", "bitt", "ich", "/", "brau\u00b7ne", "Nacht", "den", "tunck\u00b7len", "Schat\u00b7ten", "ein", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "$(", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich k\u00f6nt/ und w\u00e4restu ein Jahr/ nicht mehr verlangen.", "tokens": ["Mich", "k\u00f6nt", "/", "und", "w\u00e4\u00b7res\u00b7tu", "ein", "Jahr", "/", "nicht", "mehr", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VAFIN", "ART", "NN", "$(", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die/ welche meinen Geist vor langer Zeit gefangen/", "tokens": ["Die", "/", "wel\u00b7che", "mei\u00b7nen", "Geist", "vor", "lan\u00b7ger", "Zeit", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAT", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die/ welche mehr bezwingt/ um H\u00fclff und Trost zu schreyn/", "tokens": ["Die", "/", "wel\u00b7che", "mehr", "be\u00b7zwingt", "/", "um", "H\u00fclff", "und", "Trost", "zu", "schreyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "ADV", "VVFIN", "$(", "APPR", "NN", "KON", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als des Cupido Pfeil durch ihrer Augen Schein", "tokens": ["Als", "des", "Cu\u00b7pi\u00b7do", "Pfeil", "durch", "ih\u00b7rer", "Au\u00b7gen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NE", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Soll mir zu einem Ku\u00df erlauben ihre Wangen.", "tokens": ["Soll", "mir", "zu", "ei\u00b7nem", "Ku\u00df", "er\u00b7lau\u00b7ben", "ih\u00b7re", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hat sie nicht gestern mir beym Scheiden zugesagt", "tokens": ["Hat", "sie", "nicht", "ge\u00b7stern", "mir", "beym", "Schei\u00b7den", "zu\u00b7ge\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PPER", "APPRART", "NN", "VVPP"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Mit ihrer Marmol-Hand/ so bald es wieder tagt/", "tokens": ["Mit", "ih\u00b7rer", "Mar\u00b7mol\u00b7Hand", "/", "so", "bald", "es", "wie\u00b7der", "tagt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So soll ich meinen Wunsch von ihr erf\u00fcllet finden?", "tokens": ["So", "soll", "ich", "mei\u00b7nen", "Wunsch", "von", "ihr", "er\u00b7f\u00fcl\u00b7let", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch/ was verlier ich Zeit? Du weist von Gnade nicht:", "tokens": ["Doch", "/", "was", "ver\u00b7lier", "ich", "Zeit", "?", "Du", "weist", "von", "Gna\u00b7de", "nicht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "VVFIN", "PPER", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nacht/ ich geh unges\u00e4umt zu meiner Roselinden:", "tokens": ["Nacht", "/", "ich", "geh", "un\u00b7ge\u00b7s\u00e4umt", "zu", "mei\u00b7ner", "Ro\u00b7se\u00b7lin\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.14": {"text": "Ihr Auge machet dir zu Trotze Tag und Licht.", "tokens": ["Ihr", "Au\u00b7ge", "ma\u00b7chet", "dir", "zu", "Trot\u00b7ze", "Tag", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}