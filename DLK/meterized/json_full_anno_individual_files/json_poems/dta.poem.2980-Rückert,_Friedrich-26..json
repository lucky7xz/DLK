{"dta.poem.2980": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Europ\u00e4er, wenn du einen Weg dir bahnen", "tokens": ["Mein", "Eu\u00b7ro\u00b7p\u00e4\u00b7er", ",", "wenn", "du", "ei\u00b7nen", "Weg", "dir", "bah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Achtung willst und Anerkennung beim Brahmanen;", "tokens": ["Zur", "Ach\u00b7tung", "willst", "und", "An\u00b7er\u00b7ken\u00b7nung", "beim", "Brah\u00b7ma\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mu\u00dft du von deinen Vorurteilen erst dich heilen,", "tokens": ["Mu\u00dft", "du", "von", "dei\u00b7nen", "Vor\u00b7ur\u00b7tei\u00b7len", "erst", "dich", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Ansto\u00df nehmen nicht an seinen Vorurteilen.", "tokens": ["Und", "An\u00b7sto\u00df", "neh\u00b7men", "nicht", "an", "sei\u00b7nen", "Vor\u00b7ur\u00b7tei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht das ihm heil'ge Rind mu\u00dft du zur Malzeit schlachten,", "tokens": ["Nicht", "das", "ihm", "heil'\u00b7ge", "Rind", "mu\u00dft", "du", "zur", "Mal\u00b7zeit", "schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "PPER", "ADJA", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er nicht soll ein Thier ein rei\u00dfendes dich achten.", "tokens": ["Wenn", "er", "nicht", "soll", "ein", "Thier", "ein", "rei\u00b7\u00dfen\u00b7des", "dich", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "ART", "NN", "ART", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht duften darf dein Mund von Rauschgetr\u00e4nkes Duft,", "tokens": ["Nicht", "duf\u00b7ten", "darf", "dein", "Mund", "von", "Rauschge\u00b7tr\u00e4n\u00b7kes", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Damit nicht schon dein Hauch verunreint seine Luft.", "tokens": ["Da\u00b7mit", "nicht", "schon", "dein", "Hauch", "ver\u00b7un\u00b7reint", "sei\u00b7ne", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ADV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wird dann Unm\u00e4\u00dfigkeit vom Himmelstrich gerochen,", "tokens": ["Wird", "dann", "Un\u00b7m\u00e4\u00b7\u00dfig\u00b7keit", "vom", "Him\u00b7mel\u00b7strich", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Von seinen G\u00f6ttern glaubt er dir den Tod gesprochen.", "tokens": ["Von", "sei\u00b7nen", "G\u00f6t\u00b7tern", "glaubt", "er", "dir", "den", "Tod", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und du, jemehr dir all die G\u00f6tter sind ein Spott,", "tokens": ["Und", "du", ",", "je\u00b7mehr", "dir", "all", "die", "G\u00f6t\u00b7ter", "sind", "ein", "Spott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "PPER", "PIAT", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je weniger bekehrst du ihn zum einen Gott.", "tokens": ["Je", "we\u00b7ni\u00b7ger", "be\u00b7kehrst", "du", "ihn", "zum", "ei\u00b7nen", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}