{"textgrid.poem.41765": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wei\u00dft, wie ich in meiner Kindheit Tagen,", "tokens": ["Du", "wei\u00dft", ",", "wie", "ich", "in", "mei\u00b7ner", "Kind\u00b7heit", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wie ein M\u00e4rchen traumdurchwebt verronnen,", "tokens": ["Die", "wie", "ein", "M\u00e4r\u00b7chen", "traum\u00b7durch\u00b7webt", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein hohes Bild den Dichtern abgewonnen,", "tokens": ["Ein", "ho\u00b7hes", "Bild", "den", "Dich\u00b7tern", "ab\u00b7ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mich erquickt mit ihren Heldensagen.", "tokens": ["Die", "mich", "er\u00b7quickt", "mit", "ih\u00b7ren", "Hel\u00b7den\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Ritter, der die Laute bald geschlagen,", "tokens": ["Ein", "Rit\u00b7ter", ",", "der", "die", "Lau\u00b7te", "bald", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bald das Schwert gef\u00fchrt, k\u00fchn und besonnen,", "tokens": ["Und", "bald", "das", "Schwert", "ge\u00b7f\u00fchrt", ",", "k\u00fchn", "und", "be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$,", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit goldnem Haar und blauer Augen Bronnen \u2013", "tokens": ["Mit", "gold\u00b7nem", "Haar", "und", "blau\u00b7er", "Au\u00b7gen", "Bron\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es war ", "tokens": ["Es", "war"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Wie ich Dich sah \u2013 da stand es vor mir wieder,", "tokens": ["Wie", "ich", "Dich", "sah", "\u2013", "da", "stand", "es", "vor", "mir", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwirklicht waren die Heroen-Lieder,", "tokens": ["Ver\u00b7wirk\u00b7licht", "wa\u00b7ren", "die", "He\u00b7ro\u00b7en\u00b7Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ich als Spiel der Phantasie verklagt.", "tokens": ["Die", "ich", "als", "Spiel", "der", "Phan\u00b7ta\u00b7sie", "ver\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Fast sank die stolze Jungfrau vor Dir nieder,", "tokens": ["Fast", "sank", "die", "stol\u00b7ze", "Jung\u00b7frau", "vor", "Dir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und da\u00df Du selbst ihr Deine Lieb' gesagt,", "tokens": ["Und", "da\u00df", "Du", "selbst", "ihr", "Dei\u00b7ne", "Lieb'", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das hatte sie zu denken nie gewagt!", "tokens": ["Das", "hat\u00b7te", "sie", "zu", "den\u00b7ken", "nie", "ge\u00b7wagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du wei\u00dft, wie ich in meiner Kindheit Tagen,", "tokens": ["Du", "wei\u00dft", ",", "wie", "ich", "in", "mei\u00b7ner", "Kind\u00b7heit", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wie ein M\u00e4rchen traumdurchwebt verronnen,", "tokens": ["Die", "wie", "ein", "M\u00e4r\u00b7chen", "traum\u00b7durch\u00b7w