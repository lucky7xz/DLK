{"dta.poem.5827": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nun ein C\u00f6rper, der sich reget,", "tokens": ["Wenn", "nun", "ein", "C\u00f6r\u00b7per", ",", "der", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht so viel Kr\u00e4ffte heget,", "tokens": ["Nicht", "so", "viel", "Kr\u00e4ff\u00b7te", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In dem geraden Lauff zu bleiben,", "tokens": ["In", "dem", "ge\u00b7ra\u00b7den", "Lauff", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als einer, den er trifft, Krafft hat zu widerstehn;", "tokens": ["Als", "ei\u00b7ner", ",", "den", "er", "trifft", ",", "Krafft", "hat", "zu", "wi\u00b7der\u00b7stehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird der erste gleich abprallend abwerts gehn.", "tokens": ["So", "wird", "der", "ers\u00b7te", "gleich", "ab\u00b7pral\u00b7lend", "ab\u00b7werts", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch ist er m\u00e4chtiger; wird er sich vorwerts treiben,", "tokens": ["Doch", "ist", "er", "m\u00e4ch\u00b7ti\u00b7ger", ";", "wird", "er", "sich", "vor\u00b7werts", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "$.", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wohin er Anfangs gieng:", "tokens": ["Wo\u00b7hin", "er", "An\u00b7fangs", "gieng", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Schw\u00e4chere wird dann durch ihn mit fortgerissen,", "tokens": ["Der", "Schw\u00e4\u00b7che\u00b7re", "wird", "dann", "durch", "ihn", "mit", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch mu\u00df der Starcke das von seiner St\u00e4rcke missen,", "tokens": ["Doch", "mu\u00df", "der", "Star\u00b7cke", "das", "von", "sei\u00b7ner", "St\u00e4r\u00b7cke", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was jener erst von ihm empfing.", "tokens": ["Was", "je\u00b7ner", "erst", "von", "ihm", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}