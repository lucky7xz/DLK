{"textgrid.poem.56233": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Zur Unzeit", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte, wie gerne, dich herzen,", "tokens": ["Ich", "woll\u00b7te", ",", "wie", "ger\u00b7ne", ",", "dich", "her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "ADV", "$,", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dich wiegen in meinem Arm,", "tokens": ["Dich", "wie\u00b7gen", "in", "mei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dich dr\u00fccken an meinem Herzen,", "tokens": ["Dich", "dr\u00fc\u00b7cken", "an", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dich hegen so traut und so warm.", "tokens": ["Dich", "he\u00b7gen", "so", "traut", "und", "so", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Man verscheuchet mit Rauch die Fliegen,", "tokens": ["Man", "ver\u00b7scheu\u00b7chet", "mit", "Rauch", "die", "Flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit Verdrie\u00dflichkeit wohl den Mann;", "tokens": ["Mit", "Ver\u00b7drie\u00df\u00b7lich\u00b7keit", "wohl", "den", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und wollt ich an dich mich schmiegen,", "tokens": ["Und", "wollt", "ich", "an", "dich", "mich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich t\u00e4te nicht weise daran.", "tokens": ["Ich", "t\u00e4\u00b7te", "nicht", "wei\u00b7se", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVFIN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Wohl zieht vom strengen Norden", "tokens": ["Wohl", "zieht", "vom", "stren\u00b7gen", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein tr\u00fcbes Gew\u00f6lk herauf,", "tokens": ["Ein", "tr\u00fc\u00b7bes", "Ge\u00b7w\u00f6lk", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich bin ganz stille geworden,", "tokens": ["Ich", "bin", "ganz", "stil\u00b7le", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "VAPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich schlage die Augen nicht auf.", "tokens": ["Ich", "schla\u00b7ge", "die", "Au\u00b7gen", "nicht", "auf", "."], "token_info": ["word", "word", "word