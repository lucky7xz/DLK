], "pos": ["ADJA", "NN", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sei solches Schaun nun T\u00e4uschung oder Wahrheit", "tokens": ["Sei", "sol\u00b7ches", "Schaun", "nun", "T\u00e4u\u00b7schung", "o\u00b7der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "(wollt nur, was euch das Innre sagt, hier meinen),", "tokens": ["(", "wollt", "nur", ",", "was", "euch", "das", "Inn\u00b7re", "sagt", ",", "hier", "mei\u00b7nen", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "ADV", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "PPOSAT", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ward mir doch k\u00fcrzlich ", "tokens": ["Ward", "mir", "doch", "k\u00fcrz\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ich sa\u00df in einer stillen Mondennacht", "tokens": ["Ich", "sa\u00df", "in", "ei\u00b7ner", "stil\u00b7len", "Mon\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einsam auf jenen alten Burgruinen,", "tokens": ["Ein\u00b7sam", "auf", "je\u00b7nen", "al\u00b7ten", "Bur\u00b7gru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Beleuchtet von des Sternenhimmels Pracht,", "tokens": ["Be\u00b7leuch\u00b7tet", "von", "des", 