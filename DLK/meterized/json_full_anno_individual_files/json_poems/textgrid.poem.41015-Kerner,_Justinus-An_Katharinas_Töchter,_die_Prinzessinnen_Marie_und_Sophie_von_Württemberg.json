, "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und selig drauf erstanden in den G\u00e4rten,", "tokens": ["Und", "se\u00b7lig", "drauf", "er\u00b7stan\u00b7den", "in", "den", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Wo ihre Mutter wallt, der Lenz nie endet.", "tokens": ["Wo", "ih\u00b7re", "Mut\u00b7ter", "wallt", ",", "der", "Lenz", "nie", "en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Dir, der sich nie von jener Sel'gen wendet", "tokens": ["Dir", ",", "der", "sich", "nie", "von", "je\u00b7ner", "Sel'\u00b7gen", "wen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PRF", "ADV", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "(und der Gedanke hat gemacht uns dreister),", "tokens": ["(", "und", "der", "Ge\u00b7dan\u00b7ke", "hat", "ge\u00b7macht", "uns", "dreis\u00b7ter", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ART", "NN", "VAFIN", "VVPP", "PPER", "ADJA", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Erscheinen wir unsel'ge Blumengeister", "tokens": ["Er\u00b7schei\u00b7nen", "wir", "un\u00b7sel'\u00b7ge", "Blu\u00b7men\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Und bitten schmerzvoll dich: sei du der Meister,", "tokens": ["Und", "bit\u00b7ten", "schmerz\u00b7voll", "dich", ":", "sei", "du", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "$.", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Der uns in ein Gef\u00e4\u00df gebannt versendet", "tokens": ["Der", "uns", "in", "ein", "Ge\u00b7f\u00e4\u00df", "ge\u00b7bannt", "ver\u00b7sen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Dahin, wohin wir, ach! so sehnlich streben!\u00ab", "tokens": ["Da\u00b7hin", ",", "wo\u00b7hin", "wir", ",", "ach", "!", "so", "sehn\u00b7lich", "stre\u00b7ben", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "$,", "ITJ", "$.", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Also ert\u00f6nt' es, und die Blumen schweben,", "tokens": ["Al\u00b7so", "er\u00b7t\u00f6nt'", "es", ",", "und", "die", "Blu\u00b7men", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Schatten, farblos dunkles Geisterleben,", "tokens": ["Die", "Schat\u00b7ten", ",", "farb\u00b7los", "dunk\u00b7les", "Geis\u00b7ter\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Recht Blumengeister, nach mir von den H\u00f6hen", "tokens": ["Recht", "Blu\u00b7men\u00b7geis\u00b7ter", ",", "nach", "mir", "von", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Tal, da\u00df ich sie banne. \u2013 ", "tokens": ["Zu", "Tal", ",", "da\u00df", "ich", "sie", "ban\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bin ich gefolgt. M\u00f6cht' nun f\u00fcr sie erflehen,", "tokens": ["Bin", "ich", "ge\u00b7folgt", ".", "M\u00f6cht'", "nun", "f\u00fcr", "sie", "er\u00b7fle\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr Lieblichen! ein freundliches Empfangen,", "tokens": ["Ihr", "Lieb\u00b7li\u00b7chen", "!", "ein", "freund\u00b7li\u00b7ches", "Emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ihr die Armen l\u00f6set von den Schmerzen,", "tokens": ["Da\u00df", "ihr", "die", "Ar\u00b7men", "l\u00f6\u00b7set", "von", "den", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sie nicht starben j\u00fcngst an euren Herzen!", "tokens": ["Da\u00df", "sie", "nicht", "star\u00b7ben", "j\u00fcngst", "an", "eu\u00b7ren", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df sie sind ungesehn von euch vergangen!", "tokens": ["Da\u00df", "sie", "sind", "un\u00b7ge\u00b7sehn", "von", "euch", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Oft nach dem Tod erst wird erkannt die Liebe,", "tokens": ["Oft", "nach", "dem", "Tod", "erst", "wird", "er\u00b7kannt", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ihr Lieblichen! o sprechet: \u00bbBlickt nicht tr\u00fcbe,", "tokens": ["Ihr", "Lieb\u00b7li\u00b7chen", "!", "o", "spre\u00b7chet", ":", "\u00bb", "Blickt", "nicht", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "FM", "VVFIN", "$.", "$(", "VVFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ihr Blumenschatten! seid uns ja willkommen", "tokens": ["Ihr", "Blu\u00b7men\u00b7schat\u00b7ten", "!", "seid", "uns", "ja", "will\u00b7kom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mit L\u00e4cheln aufgenommen!", "tokens": ["Mit", "L\u00e4\u00b7cheln", "auf\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Oft nach dem Tod erst wird erkannt die Liebe!\u00ab", "tokens": ["Oft", "nach", "dem", "Tod", "erst", "wird", "er\u00b7kannt", "die", "Lie\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VAFIN", "VVPP", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}