{"textgrid.poem.41015": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An Katharinas T\u00f6chter, die Prinzessinnen Marie und Sophie von W\u00fcrttemberg", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sage geht, ich kann's euch nicht verbergen,", "tokens": ["Die", "Sa\u00b7ge", "geht", ",", "ich", "kann's", "euch", "nicht", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df man in jenem Haus, dem stillen, kleinen,", "tokens": ["Da\u00df", "man", "in", "je\u00b7nem", "Haus", ",", "dem", "stil\u00b7len", ",", "klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So ihr geschaut an Weinsbergs Rebenbergen,", "tokens": ["So", "ihr", "ge\u00b7schaut", "an", "Weins\u00b7bergs", "Re\u00b7ben\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verstorbner Menschen Schatten sah erscheinen.", "tokens": ["Ver\u00b7storb\u00b7ner", "Men\u00b7schen", "Schat\u00b7ten", "sah", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sei solches Schaun nun T\u00e4uschung oder Wahrheit", "tokens": ["Sei", "sol\u00b7ches", "Schaun", "nun", "T\u00e4u\u00b7schung", "o\u00b7der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "(wollt nur, was euch das Innre sagt, hier meinen),", "tokens": ["(", "wollt", "nur", ",", "was", "euch", "das", "Inn\u00b7re", "sagt", ",", "hier", "mei\u00b7nen", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "ADV", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "PPOSAT", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ward mir doch k\u00fcrzlich ", "tokens": ["Ward", "mir", "doch", "k\u00fcrz\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich sa\u00df in einer stillen Mondennacht", "tokens": ["Ich", "sa\u00df", "in", "ei\u00b7ner", "stil\u00b7len", "Mon\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Einsam auf jenen alten Burgruinen,", "tokens": ["Ein\u00b7sam", "auf", "je\u00b7nen", "al\u00b7ten", "Bur\u00b7gru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Beleuchtet von des Sternenhimmels Pracht,", "tokens": ["Be\u00b7leuch\u00b7tet", "von", "des", "Ster\u00b7nen\u00b7him\u00b7mels", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nachdenkend euch, und wie ihr mir erschienen", "tokens": ["Nach\u00b7den\u00b7kend", "euch", ",", "und", "wie", "ihr", "mir", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KON", "PWAV", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als Gl\u00fcckliche, um die ein Sel'ges wacht,", "tokens": ["Als", "Gl\u00fcck\u00b7li\u00b7che", ",", "um", "die", "ein", "Sel'\u00b7ges", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem noch viel Selige als Engel dienen \u2013", "tokens": ["Dem", "noch", "viel", "Se\u00b7li\u00b7ge", "als", "En\u00b7gel", "die\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Mutter war's, an die ich tief gedacht,", "tokens": ["Die", "Mut\u00b7ter", "wa\u00b7r's", ",", "an", "die", "ich", "tief", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da kamen durch die monderhellte Luft", "tokens": ["Da", "ka\u00b7men", "durch", "die", "mon\u00b7der\u00b7hell\u00b7te", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "(von Menschenschatten wohl h\u00f6rt man's oft sagen)", "tokens": ["(", "von", "Men\u00b7schen\u00b7schat\u00b7ten", "wohl", "h\u00f6rt", "man's", "oft", "sa\u00b7gen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ADV", "VVFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Jetzt Blumenschatten, schwarz und ohne Duft,", "tokens": ["Jetzt", "Blu\u00b7men\u00b7schat\u00b7ten", ",", "schwarz", "und", "oh\u00b7ne", "Duft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Zu mir im Geisterreigen hergeschwebet.", "tokens": ["Zu", "mir", "im", "Geis\u00b7ter\u00b7rei\u00b7gen", "her\u00b7ge\u00b7schwe\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein Westhauch durch die Totenstille bebet,", "tokens": ["Ein", "West\u00b7hauch", "durch", "die", "To\u00b7tens\u00b7til\u00b7le", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und schmerzreich, wie entstiegen einer Gruft,", "tokens": ["Und", "schmer\u00b7zreich", ",", "wie", "ent\u00b7stie\u00b7gen", "ei\u00b7ner", "Gruft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "H\u00f6r' ich's ", "tokens": ["H\u00f6r'", "ich's"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "\u00bbwelch herbe Schickung haben wir erlebet!", "tokens": ["\u00bb", "welch", "her\u00b7be", "Schi\u00b7ckung", "ha\u00b7ben", "wir", "er\u00b7le\u00b7bet", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAT", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Den vollen Kr\u00e4nzen, weh! sind wir entfallen,", "tokens": ["Den", "vol\u00b7len", "Kr\u00e4n\u00b7zen", ",", "weh", "!", "sind", "wir", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKVZ", "$.", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die man f\u00fcr sie, die Lieblichen, gebunden", "tokens": ["Die", "man", "f\u00fcr", "sie", ",", "die", "Lieb\u00b7li\u00b7chen", ",", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PIS", "APPR", "PPER", "$,", "ART", "NN", "$,", "VVPP"], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.18": {"text": "Im Neckartale in den holden Stunden,", "tokens": ["Im", "Ne\u00b7ckar\u00b7ta\u00b7le", "in", "den", "hol\u00b7den", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Als sie erschienen Blumenengel allen.", "tokens": ["Als", "sie", "er\u00b7schie\u00b7nen", "Blu\u00b7me\u00b7nen\u00b7gel", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Weh! wir Unseligen sind nicht gekommen", "tokens": ["Weh", "!", "wir", "Un\u00b7se\u00b7li\u00b7gen", "sind", "nicht", "ge\u00b7kom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "NN", "VAFIN", "PTKNEG", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "An ihre Brust und nicht in ihre H\u00e4nde,", "tokens": ["An", "ih\u00b7re", "Brust", "und", "nicht", "in", "ih\u00b7re", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die staub'ge Erde hat uns hingenommen;", "tokens": ["Die", "staub'\u00b7ge", "Er\u00b7de", "hat", "uns", "hin\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und s\u00fcndhaft fluchten wir da unserm Ende.", "tokens": ["Und", "s\u00fcnd\u00b7haft", "fluch\u00b7ten", "wir", "da", "un\u00b7serm", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Nun irren wir rastlos, als schwarze Schatten", "tokens": ["Nun", "ir\u00b7ren", "wir", "rast\u00b7los", ",", "als", "schwar\u00b7ze", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Gehoben in die L\u00fcfte von der Erden,", "tokens": ["Ge\u00b7ho\u00b7ben", "in", "die", "L\u00fcf\u00b7te", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Bis da\u00df auch wir das Ziel, das jene hatten,", "tokens": ["Bis", "da\u00df", "auch", "wir", "das", "Ziel", ",", "das", "je\u00b7ne", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ADV", "PPER", "ART", "NN", "$,", "PRELS", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die gl\u00fccklicher als wir, erreichen werden.", "tokens": ["Die", "gl\u00fcck\u00b7li\u00b7cher", "als", "wir", ",", "er\u00b7rei\u00b7chen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "PPER", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Hin treibt es uns wohl ohne Duft und Farben,", "tokens": ["Hin", "treibt", "es", "uns", "wohl", "oh\u00b7ne", "Duft", "und", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Unsel'ge, hin zu jenen Liebewerten,", "tokens": ["Un\u00b7sel'\u00b7ge", ",", "hin", "zu", "je\u00b7nen", "Lie\u00b7be\u00b7wer\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.30": {"text": "An deren Busen unsre Schwestern starben,", "tokens": ["An", "de\u00b7ren", "Bu\u00b7sen", "uns\u00b7re", "Schwes\u00b7tern", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und selig drauf erstanden in den G\u00e4rten,", "tokens": ["Und", "se\u00b7lig", "drauf", "er\u00b7stan\u00b7den", "in", "den", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Wo ihre Mutter wallt, der Lenz nie endet.", "tokens": ["Wo", "ih\u00b7re", "Mut\u00b7ter", "wallt", ",", "der", "Lenz", "nie", "en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Dir, der sich nie von jener Sel'gen wendet", "tokens": ["Dir", ",", "der", "sich", "nie", "von", "je\u00b7ner", "Sel'\u00b7gen", "wen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PRF", "ADV", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "(und der Gedanke hat gemacht uns dreister),", "tokens": ["(", "und", "der", "Ge\u00b7dan\u00b7ke", "hat", "ge\u00b7macht", "uns", "dreis\u00b7ter", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ART", "NN", "VAFIN", "VVPP", "PPER", "ADJA", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Erscheinen wir unsel'ge Blumengeister", "tokens": ["Er\u00b7schei\u00b7nen", "wir", "un\u00b7sel'\u00b7ge", "Blu\u00b7men\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Und bitten schmerzvoll dich: sei du der Meister,", "tokens": ["Und", "bit\u00b7ten", "schmerz\u00b7voll", "dich", ":", "sei", "du", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "$.", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Der uns in ein Gef\u00e4\u00df gebannt versendet", "tokens": ["Der", "uns", "in", "ein", "Ge\u00b7f\u00e4\u00df", "ge\u00b7bannt", "ver\u00b7sen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Dahin, wohin wir, ach! so sehnlich streben!\u00ab", "tokens": ["Da\u00b7hin", ",", "wo\u00b7hin", "wir", ",", "ach", "!", "so", "sehn\u00b7lich", "stre\u00b7ben", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "$,", "ITJ", "$.", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Also ert\u00f6nt' es, und die Blumen schweben,", "tokens": ["Al\u00b7so", "er\u00b7t\u00f6nt'", "es", ",", "und", "die", "Blu\u00b7men", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Schatten, farblos dunkles Geisterleben,", "tokens": ["Die", "Schat\u00b7ten", ",", "farb\u00b7los", "dunk\u00b7les", "Geis\u00b7ter\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Recht Blumengeister, nach mir von den H\u00f6hen", "tokens": ["Recht", "Blu\u00b7men\u00b7geis\