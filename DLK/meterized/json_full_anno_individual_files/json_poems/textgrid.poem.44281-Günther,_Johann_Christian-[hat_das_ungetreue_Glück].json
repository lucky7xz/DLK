{"textgrid.poem.44281": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[hat das ungetreue Gl\u00fcck]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat das ungetreue Gl\u00fcck", "tokens": ["Hat", "das", "un\u00b7ge\u00b7treu\u00b7e", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich auf meinen Kopf verschworen?", "tokens": ["Sich", "auf", "mei\u00b7nen", "Kopf", "ver\u00b7schwo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat mich denn das Misgeschick", "tokens": ["Hat", "mich", "denn", "das", "Mis\u00b7ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets zum Fangeball erkohren?", "tokens": ["Stets", "zum", "Fan\u00b7ge\u00b7ball", "er\u00b7koh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey so w\u00fcntsch ich tausendmahl,", "tokens": ["Ey", "so", "w\u00fcnt\u00b7sch", "ich", "tau\u00b7send\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lieber meinen Geist zu la\u00dfen,", "tokens": ["Lie\u00b7ber", "mei\u00b7nen", "Geist", "zu", "la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als in einer solchen Qual", "tokens": ["Als", "in", "ei\u00b7ner", "sol\u00b7chen", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich aus \u00dcberdru\u00df zu ha\u00dfen.", "tokens": ["Mich", "aus", "\u00dc\u00b7berd\u00b7ru\u00df", "zu", "ha\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4st sichs gleich bisweilen an,", "tokens": ["L\u00e4st", "sichs", "gleich", "bis\u00b7wei\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als wenn alles herrlich schiene,", "tokens": ["Als", "wenn", "al\u00b7les", "herr\u00b7lich", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ist auf der Gl\u00fcckesbahn", "tokens": ["Und", "ist", "auf", "der", "Gl\u00fc\u00b7ckes\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alles wohl geb\u00e4hnt und gr\u00fcne,", "tokens": ["Al\u00b7les", "wohl", "ge\u00b7b\u00e4hnt", "und", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, so k\u00f6mmt doch allzu oft", "tokens": ["Ach", ",", "so", "k\u00f6mmt", "doch", "all\u00b7zu", "oft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "PTKA", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein erz\u00f6rntes Ungl\u00fcckswetter", "tokens": ["Ein", "er\u00b7z\u00f6rn\u00b7tes", "Un\u00b7gl\u00fccks\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und zerschl\u00e4gt mir unverhoft", "tokens": ["Und", "zer\u00b7schl\u00e4gt", "mir", "un\u00b7ver\u00b7hoft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die gehoften Lorbeerbl\u00e4tter.", "tokens": ["Die", "ge\u00b7hof\u00b7ten", "Lor\u00b7beer\u00b7bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie, wenn zu der Sommerszeit", "tokens": ["Wie", ",", "wenn", "zu", "der", "Som\u00b7mers\u00b7zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Phoebus uns zu schmeicheln pfleget,", "tokens": ["Phoe\u00b7bus", "uns", "zu", "schmei\u00b7cheln", "pfle\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Juppiter ein dunckles Kleid", "tokens": ["Jup\u00b7pi\u00b7ter", "ein", "dunck\u00b7les", "Kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Um die blauen Schultern leget,", "tokens": ["Um", "die", "blau\u00b7en", "Schul\u00b7tern", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Also machts das Gl\u00fcck mit mir,", "tokens": ["Al\u00b7so", "machts", "das", "Gl\u00fcck", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Erstlich reicht es s\u00fc\u00dfe Freuden,", "tokens": ["Erst\u00b7lich", "reicht", "es", "s\u00fc\u00b7\u00dfe", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kurz darauf mu\u00df ich daf\u00fcr", "tokens": ["Kurz", "da\u00b7rauf", "mu\u00df", "ich", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PAV", "VMFIN", "PPER", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen schweren Donner leiden.", "tokens": ["Ei\u00b7nen", "schwe\u00b7ren", "Don\u00b7ner", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier erscheint ein falscher Freund", "tokens": ["Hier", "er\u00b7scheint", "ein", "fal\u00b7scher", "Freund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und bestellt mir Fall und Neze,", "tokens": ["Und", "be\u00b7stellt", "mir", "Fall", "und", "Ne\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort betr\u00fcbet mich ein Feind,", "tokens": ["Dort", "be\u00b7tr\u00fc\u00b7bet", "mich", "ein", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ich zwar nicht wichtig sch\u00e4ze,", "tokens": ["Den", "ich", "zwar", "nicht", "wich\u00b7tig", "sch\u00e4\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch hernach erfahren mu\u00df,", "tokens": ["Doch", "her\u00b7nach", "er\u00b7fah\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da\u00df die allerkleinsten Fliegen", "tokens": ["Da\u00df", "die", "al\u00b7ler\u00b7kleins\u00b7ten", "Flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich nicht nur an unsern Fu\u00df,", "tokens": ["Sich", "nicht", "nur", "an", "un\u00b7sern", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sondern auch ans Haupt verf\u00fcgen.", "tokens": ["Son\u00b7dern", "auch", "ans", "Haupt", "ver\u00b7f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denen ich viel zugetraut,", "tokens": ["De\u00b7nen", "ich", "viel", "zu\u00b7ge\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Diese la\u00dfen mich jezt stecken,", "tokens": ["Die\u00b7se", "la\u00b7\u00dfen", "mich", "jezt", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor welchen mir gegraut,", "tokens": ["Und", "vor", "wel\u00b7chen", "mir", "ge\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PWAT", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese la\u00dfen sich erwecken", "tokens": ["Die\u00b7se", "la\u00b7\u00dfen", "sich", "er\u00b7we\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ertheilen Rath und That,", "tokens": ["Und", "er\u00b7thei\u00b7len", "Rath", "und", "That", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den mir nicht ein andrer giebet,", "tokens": ["Den", "mir", "nicht", "ein", "an\u00b7drer", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der nur leere Worte hat", "tokens": ["Der", "nur", "lee\u00b7re", "Wor\u00b7te", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und um seinen Vortheil liebet.", "tokens": ["Und", "um", "sei\u00b7nen", "Vor\u00b7theil", "lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Falsches Gl\u00fccke, sieh auf mich,", "tokens": ["Fal\u00b7sches", "Gl\u00fc\u00b7cke", ",", "sieh", "auf", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwinge nicht stets dein Gefieder,", "tokens": ["Schwin\u00b7ge", "nicht", "stets", "dein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steh einmahl und la\u00dfe dich", "tokens": ["Steh", "ein\u00b7mahl", "und", "la\u00b7\u00dfe", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der glatten Kugel nieder!", "tokens": ["Von", "der", "glat\u00b7ten", "Ku\u00b7gel", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleib doch nur ein wenig stehn!", "tokens": ["Bleib", "doch", "nur", "ein", "we\u00b7nig", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Halt, halt an, du must mich k\u00fc\u00dfen;", "tokens": ["Halt", ",", "halt", "an", ",", "du", "must", "mich", "k\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Halt, ich las dich eh nicht gehn,", "tokens": ["Halt", ",", "ich", "las", "dich", "eh", "nicht", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "KOUS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis du mich der Noth entri\u00dfen.", "tokens": ["Bis", "du", "mich", "der", "Noth", "ent\u00b7ri\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hat das ungetreue Gl\u00fcck", "tokens": ["Hat", "das", "un\u00b7ge\u00b7treu\u00b7e", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich auf meinen Kopf verschworen?", "tokens": ["Sich", "auf", "mei\u00b7nen", "Kopf", "ver\u00b7schwo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat mich denn das Misgeschick", "tokens": ["Hat", "mich", "denn", "das", "Mis\u00b7ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets zum Fangeball erkohren?", "tokens": ["Stets", "zum", "Fan\u00b7ge\u00b7ball", "er\u00b7koh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey so w\u00fcntsch ich tausendmahl,", "tokens": ["Ey", "so", "w\u00fcnt\u00b7sch", "ich", "tau\u00b7send\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lieber meinen Geist zu la\u00dfen,", "tokens": ["Lie\u00b7ber", "mei\u00b7nen", "Geist", "zu", "la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als in einer solchen Qual", "tokens": ["Als", "in", "ei\u00b7ner", "sol\u00b7chen", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich aus \u00dcberdru\u00df zu ha\u00dfen.", "tokens": ["Mich", "aus", "\u00dc\u00b7berd\u00b7ru\u00df", "zu", "ha\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "L\u00e4st sichs gleich bisweilen an,", "tokens": ["L\u00e4st", "sichs", "gleich", "bis\u00b7wei\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als wenn alles herrlich schiene,", "tokens": ["Als", "wenn", "al\u00b7les", "herr\u00b7lich", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ist auf der Gl\u00fcckesbahn", "tokens": ["Und", "ist", "auf", "der", "Gl\u00fc\u00b7ckes\u00b7bahn"], "token_info": ["word", "word", "word", "wor