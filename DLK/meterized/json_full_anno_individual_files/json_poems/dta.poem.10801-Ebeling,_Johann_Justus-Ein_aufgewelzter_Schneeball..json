{"dta.poem.10801": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ein aufgewelzter Schneeball.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man siehet, wenn zur Winters-Zeit,", "tokens": ["Man", "sie\u00b7het", ",", "wenn", "zur", "Win\u00b7ter\u00b7sZeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erdenball mit Schnee bestreut,", "tokens": ["Der", "Er\u00b7den\u00b7ball", "mit", "Schnee", "be\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sich die Jugend dran vergn\u00fcget,", "tokens": ["Wie", "sich", "die", "Ju\u00b7gend", "dran", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf den Gassen h\u00e4ufig lieget.", "tokens": ["Und", "auf", "den", "Gas\u00b7sen", "h\u00e4u\u00b7fig", "lie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie scheuen Frost und K\u00e4lte nicht,", "tokens": ["Sie", "scheu\u00b7en", "Frost", "und", "K\u00e4l\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ihnen gleich das Sonnenlicht", "tokens": ["Wenn", "ih\u00b7nen", "gleich", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht scheint; ihr innres Feur vergehet,", "tokens": ["Nicht", "scheint", ";", "ihr", "inn\u00b7res", "Feur", "ver\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vornemlich wenn der Nord recht wehet:", "tokens": ["Vor\u00b7nem\u00b7lich", "wenn", "der", "Nord", "recht", "we\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und dennoch spielen sie im Schnee,", "tokens": ["Und", "den\u00b7noch", "spie\u00b7len", "sie", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bis endlich ein recht krimmelnd Weh,", "tokens": ["Bis", "end\u00b7lich", "ein", "recht", "krim\u00b7melnd", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Durch ihre kalte Finger schleichet,", "tokens": ["Durch", "ih\u00b7re", "kal\u00b7te", "Fin\u00b7ger", "schlei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und das noch feurge Herz erweichet.", "tokens": ["Und", "das", "noch", "feur\u00b7ge", "Herz", "er\u00b7wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Vornemlich ist es lustig sch\u00f6n,", "tokens": ["Vor\u00b7nem\u00b7lich", "ist", "es", "lus\u00b7tig", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "An einem Berge anzusehn,", "tokens": ["An", "ei\u00b7nem", "Ber\u00b7ge", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "All wo der Schnee wenn er nicht schmelzet,", "tokens": ["All", "wo", "der", "Schnee", "wenn", "er", "nicht", "schmel\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PWAV", "ART", "NN", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Jm Rollen leicht wird aufgewelzet.", "tokens": ["Jm", "Rol\u00b7len", "leicht", "wird", "auf\u00b7ge\u00b7wel\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Da rollen sie von einem Ort,", "tokens": ["Da", "rol\u00b7len", "sie", "von", "ei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den kleinen Klump stets weiter fort,", "tokens": ["Den", "klei\u00b7nen", "Klump", "stets", "wei\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und in dem fortgewelzten Wallen,", "tokens": ["Und", "in", "dem", "fort\u00b7ge\u00b7welz\u00b7ten", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Vergr\u00f6ssert sich des Schnees Ballen;", "tokens": ["Ver\u00b7gr\u00f6s\u00b7sert", "sich", "des", "Schnees", "Bal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Ich sahe einstens dies Gew\u00fchl,", "tokens": ["Ich", "sa\u00b7he", "eins\u00b7tens", "dies", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Mit Lust an, als ein Kinderspiel:", "tokens": ["Mit", "Lust", "an", ",", "als", "ein", "Kin\u00b7der\u00b7spiel", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und dachte: was kan nicht entstehen,", "tokens": ["Und", "dach\u00b7te", ":", "was", "kan", "nicht", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "Wenn Dinge immer weiter gehen.", "tokens": ["Wenn", "Din\u00b7ge", "im\u00b7mer", "wei\u00b7ter", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Dies Kinderspiel, das stellte mir,", "tokens": ["Dies", "Kin\u00b7der\u00b7spiel", ",", "das", "stell\u00b7te", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Manch Lehrbild im Gedanken f\u00fcr:", "tokens": ["Manch", "Lehr\u00b7bild", "im", "Ge\u00b7dan\u00b7ken", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich konnte an dem klebricht Wesen", "tokens": ["Ich", "konn\u00b7te", "an", "dem", "kleb\u00b7richt", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Des Schnees, diese Warheit lesen:", "tokens": ["Des", "Schnees", ",", "die\u00b7se", "War\u00b7heit", "le\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Ein Ding von einer gleichen Art,", "tokens": ["Ein", "Ding", "von", "ei\u00b7ner", "glei\u00b7chen", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Wird mit dem andern leicht verpaart;", "tokens": ["Wird", "mit", "dem", "an\u00b7dern", "leicht", "ver\u00b7paart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Es wachsen leicht die losen Hauffen,", "tokens": ["Es", "wach\u00b7sen", "leicht", "die", "lo\u00b7sen", "Hauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Die eh mans meint, zusammen lauffen.", "tokens": ["Die", "eh", "mans", "meint", ",", "zu\u00b7sam\u00b7men", "lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "KOUS", "PIS", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ein Mensch der zieht den andern an,", "tokens": ["Ein", "Mensch", "der", "zieht", "den", "an\u00b7dern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Auf einer schl\u00fcpfeig glatten Bahn:", "tokens": ["Auf", "ei\u00b7ner", "schl\u00fcp\u00b7feig", "glat\u00b7ten", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und kaum wird einer da erblikker,", "tokens": ["Und", "kaum", "wird", "ei\u00b7ner", "da", "er\u00b7blik\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "So wird er auch so gleich bestrikker.", "tokens": ["So", "wird", "er", "auch", "so", "gleich", "be\u00b7strik\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Die Kinder rollten in dem Lauf,", "tokens": ["Die", "Kin\u00b7der", "roll\u00b7ten", "in", "dem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Den Schneeball immer gr\u00f6sser auf;", "tokens": ["Den", "Schnee\u00b7ball", "im\u00b7mer", "gr\u00f6s\u00b7ser", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ich dachte: ", "tokens": ["Ich", "dach\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Die immer neuen Zuwachs kriegen,", "tokens": ["Die", "im\u00b7mer", "neu\u00b7en", "Zu\u00b7wachs", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Je weiter man durch das Ger\u00fccht,", "tokens": ["Je", "wei\u00b7ter", "man", "durch", "das", "Ge\u00b7r\u00fccht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Von einer falschen Sache spricht;", "tokens": ["Von", "ei\u00b7ner", "fal\u00b7schen", "Sa\u00b7che", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Je mehr wird sie mit Schein verbessert,", "tokens": ["Je", "mehr", "wird", "sie", "mit", "Schein", "ver\u00b7bes\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.44": {"text": "Und fast von Mund zu Mund vergr\u00f6ssert.", "tokens": ["Und", "fast", "von", "Mund", "zu", "Mund", "ver\u00b7gr\u00f6s\u00b7sert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Der Klumpe ward zulezt sehr gro\u00df,", "tokens": ["Der", "Klum\u00b7pe", "ward", "zu\u00b7lezt", "sehr", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Und ein recht ungeheurer Klos,", "tokens": ["Und", "ein", "recht", "un\u00b7ge\u00b7heu\u00b7rer", "Klos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Er wolte endlich nicht mehr wandern:", "tokens": ["Er", "wol\u00b7te", "end\u00b7lich", "nicht", "mehr", "wan\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Drum rief ein Knabe zu dem andern,", "tokens": ["Drum", "rief", "ein", "Kna\u00b7be", "zu", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Komm hilf mir da\u00df er komm zum Lauf,", "tokens": ["Komm", "hilf", "mir", "da\u00df", "er", "komm", "zum", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVIMP", "PPER", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "So th\u00fcrmt sich ein Geb\u00fcrge auf.", "tokens": ["So", "th\u00fcrmt", "sich", "ein", "Ge\u00b7b\u00fcr\u00b7ge", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Er kam, sie stiessen alle Beide,", "tokens": ["Er", "kam", ",", "sie", "sties\u00b7sen", "al\u00b7le", "Bei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Jedoch es ward die eitle Freude,", "tokens": ["Je\u00b7doch", "es", "ward", "die", "eit\u00b7le", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Durch einem Zufal drauf gest\u00f6hrt.", "tokens": ["Durch", "ei\u00b7nem", "Zu\u00b7fal", "drauf", "ge\u00b7st\u00f6hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Die Lust in Weinen bald verkehrt.", "tokens": ["Die", "Lust", "in", "Wei\u00b7nen", "bald", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Indem sie solchen st\u00e4rker rollten,", "tokens": ["In\u00b7dem", "sie", "sol\u00b7chen", "st\u00e4r\u00b7ker", "roll\u00b7ten", 