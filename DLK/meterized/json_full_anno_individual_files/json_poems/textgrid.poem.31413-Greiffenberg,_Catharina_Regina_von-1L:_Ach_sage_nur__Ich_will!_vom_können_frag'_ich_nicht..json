{"textgrid.poem.31413": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach sage nur / Ich will! vom k\u00f6nnen frag' ich nicht.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach sage nur / Ich will! vom k\u00f6nnen frag' ich nicht.", "tokens": ["Ach", "sa\u00b7ge", "nur", "/", "Ich", "will", "!", "vom", "k\u00f6n\u00b7nen", "frag'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "$.", "APPRART", "ADJA", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann Berg-versetzen dir / wie mir das W\u00fcrffelwerffen.", "tokens": ["Dann", "Ber\u00b7gver\u00b7set\u00b7zen", "dir", "/", "wie", "mir", "das", "W\u00fcrf\u00b7fel\u00b7wer\u00b7ffen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "$(", "PWAV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Zum Meer- austrucknen wirst du nur ein wort bed\u00f6rffen.", "tokens": ["Zum", "Meer", "aus\u00b7truck\u00b7nen", "wirst", "du", "nur", "ein", "wort", "be\u00b7d\u00f6rf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "VVINF", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh / als im Augenblick / was du gebietst / geschicht.", "tokens": ["Eh", "/", "als", "im", "Au\u00b7gen\u00b7blick", "/", "was", "du", "ge\u00b7bietst", "/", "ge\u00b7schicht", "."], "token_info": ["word", "punct", "word