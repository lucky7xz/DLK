{"dta.poem.11112": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine wind-m\u00fchle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schau hier ein fliegend hau\u00df, das niemahls stille steht,", "tokens": ["Schau", "hier", "ein", "flie\u00b7gend", "hau\u00df", ",", "das", "nie\u00b7mahls", "stil\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJD", "NN", "$,", "PRELS", "ADV", "ADJA", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und dennoch auch niemahls von seiner stelle geht.", "tokens": ["Und", "den\u00b7noch", "auch", "nie\u00b7mahls", "von", "sei\u00b7ner", "stel\u00b7le", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Schau eine werckstatt an, die in den l\u00fcfften schwebet,", "tokens": ["Schau", "ei\u00b7ne", "wercks\u00b7tatt", "an", ",", "die", "in", "den", "l\u00fcff\u00b7ten", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die andere ern\u00e4hrt, und selbst vom winde lebet.", "tokens": ["Die", "an\u00b7de\u00b7re", "er\u00b7n\u00e4hrt", ",", "und", "selbst", "vom", "win\u00b7de", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "KON", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}