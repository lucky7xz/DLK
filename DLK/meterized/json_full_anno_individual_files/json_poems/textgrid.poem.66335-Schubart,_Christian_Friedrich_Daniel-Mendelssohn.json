{"textgrid.poem.66335": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Mendelssohn", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Da strahlt' ein Geist des Himmels zu der Seele", "tokens": ["Da", "strahlt'", "ein", "Geist", "des", "Him\u00b7mels", "zu", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Weisen. \u00bbKomm, und folge mir!\u00ab", "tokens": ["Des", "Wei\u00b7sen", ".", "\u00bb", "Komm", ",", "und", "fol\u00b7ge", "mir", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "VVFIN", "$,", "KON", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Silberglockennachhall sprach's der Geist.", "tokens": ["Wie", "Sil\u00b7ber\u00b7glo\u00b7cken\u00b7nach\u00b7hall", "sprach's", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er f\u00fchrte dann die Seele durch des Todes", "tokens": ["Er", "f\u00fchr\u00b7te", "dann", "die", "See\u00b7le", "durch", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Grauenvolles Nachtthal, wies der staunenden", "tokens": ["Grau\u00b7en\u00b7vol\u00b7les", "Nachtt\u00b7hal", ",", "wies", "der", "stau\u00b7nen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.7": {"text": "Bei seines Krystallstabes Schimmer die Zerst\u00f6rung", "tokens": ["Bei", "sei\u00b7nes", "Krys\u00b7tall\u00b7sta\u00b7bes", "Schim\u00b7mer", "die", "Zer\u00b7st\u00f6\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Wo die Verwesung nagt, Verzweiflung br\u00fcllt,", "tokens": ["Wo", "die", "Ver\u00b7we\u00b7sung", "nagt", ",", "Ver\u00b7zwei\u00b7flung", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wo gelbe B\u00e4che aus den Ritzen schwarzer", "tokens": ["Wo", "gel\u00b7be", "B\u00e4\u00b7che", "aus", "den", "Rit\u00b7zen", "schwar\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mit Moos bewachsner Felsen sch\u00e4umend st\u00fcrzen;", "tokens": ["Mit", "Moos", "be\u00b7wachs\u00b7ner", "Fel\u00b7sen", "sch\u00e4u\u00b7mend", "st\u00fcr\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wo aus der Menschensch\u00e4del hohlem Auge", "tokens": ["Wo", "aus", "der", "Men\u00b7schen\u00b7sch\u00e4\u00b7del", "hoh\u00b7lem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Otter z\u00fcngelt, wo gefleckte Kr\u00f6ten", "tokens": ["Die", "Ot\u00b7ter", "z\u00fcn\u00b7gelt", ",", "wo", "ge\u00b7fleck\u00b7te", "Kr\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sich gurgelnd bl\u00e4hn bei Menschen\u00e4sern.", "tokens": ["Sich", "gur\u00b7gelnd", "bl\u00e4hn", "bei", "Men\u00b7sche\u00b7n\u00e4\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwer wird einmal zerst\u00f6ren das Gekl\u00fcft", "tokens": ["\u00bb", "wer", "wird", "ein\u00b7mal", "zer\u00b7st\u00f6\u00b7ren", "das", "Ge\u00b7kl\u00fcft"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll Jammer?\u00ab athmet ", "tokens": ["Voll", "Jam\u00b7mer", "?", "\u00ab", "ath\u00b7met"], "token_info": ["word", "word", "punct", "punct", "word"], "pos": ["ADJD", "NN", "$.", "$(", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "\u00bbder ", "tokens": ["\u00bb", "der"], "token_info": ["punct", "word"], "pos": ["$(", "ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der H\u00f6lle und des Todes,\u00ab sprach der Engel;", "tokens": ["Der", "H\u00f6l\u00b7le", "und", "des", "To\u00b7des", ",", "\u00ab", "sprach", "der", "En\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchrte dann des Weisen Seele durch die R\u00e4ume", "tokens": ["F\u00fchr\u00b7te", "dann", "des", "Wei\u00b7sen", "See\u00b7le", "durch", "die", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Des Himmels.", "tokens": ["Des", "Him\u00b7mels", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Staunend sah der Denker", "tokens": ["Stau\u00b7nend", "sah", "der", "Den\u00b7ker"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In der Sch\u00f6pfung Weite Raums genug", "tokens": ["In", "der", "Sch\u00f6p\u00b7fung", "Wei\u00b7te", "Raums", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr alle Wesen, Welten, Geister,", "tokens": ["F\u00fcr", "al\u00b7le", "We\u00b7sen", ",", "Wel\u00b7ten", ",", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich drinn zu w\u00e4lzen. Gottes N\u00e4he", "tokens": ["Sich", "drinn", "zu", "w\u00e4l\u00b7zen", ".", "Got\u00b7tes", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADV", "PTKZU", "VVINF", "$.", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durchschauert' ihn. \u00bbDu bist Jehovah!\u00ab", "tokens": ["Durch\u00b7schau\u00b7ert'", "ihn", ".", "\u00bb", "Du", "bist", "Je\u00b7ho\u00b7vah", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "NE", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sprach die Seele, k\u00fc\u00dft' der Rechten", "tokens": ["Sprach", "die", "See\u00b7le", ",", "k\u00fc\u00dft'", "der", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aufgehobne, lichtbestr\u00f6mte Finger.", "tokens": ["Auf\u00b7ge\u00b7hob\u00b7ne", ",", "licht\u00b7be\u00b7str\u00f6m\u00b7te", "Fin\u00b7ger", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "\u00bbdu bist Jehovah! Hab' so oft gef\u00fchlt", "tokens": ["\u00bb", "du", "bist", "Je\u00b7ho\u00b7vah", "!", "Hab'", "so", "oft", "ge\u00b7f\u00fchlt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "NE", "$.", "NN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Im niedern Erdenthale diese gro\u00dfe Ahnung.\u00ab", "tokens": ["Im", "nie\u00b7dern", "Er\u00b7dent\u00b7ha\u00b7le", "die\u00b7se", "gro\u00b7\u00dfe", "Ah\u00b7nung", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So lispelt ", "tokens": ["So", "lis\u00b7pelt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Sie kamen vor des Himmels Sonnenpforte,", "tokens": ["Sie", "ka\u00b7men", "vor", "des", "Him\u00b7mels", "Son\u00b7nen\u00b7pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Eloa \u00f6ffnet sie. Der F\u00fchrende", "tokens": ["E\u00b7loa", "\u00f6ff\u00b7net", "sie", ".", "Der", "F\u00fch\u00b7ren\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "ART", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Und der Gef\u00fchrte traten schauernd hinein.", "tokens": ["Und", "der", "Ge\u00b7f\u00fchr\u00b7te", "tra\u00b7ten", "schau\u00b7ernd", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Ein Menschensohn, der Sch\u00f6nheit und der Gr\u00f6\u00dfe,", "tokens": ["Ein", "Men\u00b7schen\u00b7sohn", ",", "der", "Sch\u00f6n\u00b7heit", "und", "der", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der reinsten G\u00fcte Urbild stand vor ", "tokens": ["Der", "reins\u00b7ten", "G\u00fc\u00b7te", "Ur\u00b7bild", "stand", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "\u00bbaus meinem Stamme bist du nach dem Fleische,\u00ab", "tokens": ["\u00bb", "aus", "mei\u00b7nem", "Stam\u00b7me", "bist", "du", "nach", "dem", "Flei\u00b7sche", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Sprach eine Stimm', der Liebe Wiederhall.", "tokens": ["Sprach", "ei\u00b7ne", "Stimm'", ",", "der", "Lie\u00b7be", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbich kenne dich, bist aus dem Volke, dessen", "tokens": ["\u00bb", "ich", "ken\u00b7ne", "dich", ",", "bist", "aus", "dem", "Vol\u00b7ke", ",", "des\u00b7sen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "VAFIN", "APPR", "ART", "NN", "$,", "PRELAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ha\u00df ans Kreuz mich schlug. Du hast auf Erden", "tokens": ["Ha\u00df", "ans", "Kreuz", "mich", "schlug", ".", "Du", "hast", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Mich nie gekannt, doch nie gel\u00e4stert.", "tokens": ["Mich", "nie", "ge\u00b7kannt", ",", "doch", "nie", "ge\u00b7l\u00e4s\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Drum w\u00e4hlt' ich dich aus Tausenden,", "tokens": ["Drum", "w\u00e4hlt'", "ich", "dich", "aus", "Tau\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Um fr\u00fcher dir zu sagen: Ich bin dein Bruder!", "tokens": ["Um", "fr\u00fc\u00b7her", "dir", "zu", "sa\u00b7gen", ":", "Ich", "bin", "dein", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPER", "PTKZU", "VVINF", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.23": {"text": "Bin Jesus Christus! Bin dein Bruder!", "tokens": ["Bin", "Je\u00b7sus", "Chris\u00b7tus", "!", "Bin", "dein", "Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nach Wahrheit lechztest du, komm, fall an meine Brust.", "tokens": ["Nach", "Wahr\u00b7heit", "lechz\u00b7test", "du", ",", "komm", ",", "fall", "an", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "VVFIN", "$,", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.25": {"text": "Hier findst du sie! Nach Sch\u00f6nheit strebtest du;", "tokens": ["Hier", "findst", "du", "sie", "!", "Nach", "Sch\u00f6n\u00b7heit", "streb\u00b7test", "du", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$.", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Sieh hier der Sch\u00f6nheit h\u00f6chstes Ideal.", "tokens": ["Sieh", "hier", "der", "Sch\u00f6n\u00b7heit", "h\u00f6chs\u00b7tes", "I\u00b7deal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Nach ew'gem Heil und Leben rangest du;", "tokens": ["Nach", "ew'\u00b7gem", "Heil", "und", "Le\u00b7ben", "ran\u00b7gest", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Komm! ew'ges Heil und Leben geb' ich dir!", "tokens": ["Komm", "!", "ew'\u00b7ges", "Heil", "und", "Le\u00b7ben", "geb'", "ich", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJA", "NN", "KON", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Als du des Abfalls Greuelfolgen sahst", "tokens": ["Als", "du", "des", "Ab\u00b7falls", "Greu\u00b7el\u00b7fol\u00b7gen", "sahst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Dort im Scheol, im Todtenbeingekl\u00fcft,", "tokens": ["Dort", "im", "Sche\u00b7ol", ",", "im", "Tod\u00b7ten\u00b7bein\u00b7ge\u00b7kl\u00fcft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Da weinte deine Seel'; o weine nimmer,", "tokens": ["Da", "wein\u00b7te", "dei\u00b7ne", "Seel'", ";", "o", "wei\u00b7ne", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", 