{"textgrid.poem.55173": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Versuchung", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reichte die sch\u00e4dliche Frucht einst Mutter Eva dem Gatten,", "tokens": ["Reich\u00b7te", "die", "sch\u00e4d\u00b7li\u00b7che", "Frucht", "einst", "Mut\u00b7ter", "E\u00b7va", "dem", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "NN", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ach! vom t\u00f6richten Bi\u00df kr\u00e4nkelt das ganze Geschlecht.", "tokens": ["Ach", "!", "vom", "t\u00f6\u00b7rich\u00b7ten", "Bi\u00df", "kr\u00e4n\u00b7kelt", "das", "gan\u00b7ze", "Ge\u00b7schlecht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun vom heiligen Leibe, der Seelen speiset und heilet,", "tokens": ["Nun", "vom", "hei\u00b7li\u00b7gen", "Lei\u00b7be", ",", "der", "See\u00b7len", "spei\u00b7set", "und", "hei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Kostest du, Lydia, fromm, liebliches b\u00fc\u00dfendes Kind!", "tokens": ["Kos\u00b7test", "du", ",", "Ly\u00b7dia", ",", "fromm", ",", "lieb\u00b7li\u00b7ches", "b\u00fc\u00b7\u00dfen\u00b7des", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "ADJD", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Darum schick ich dir eilig die Frucht voll irdischer S\u00fc\u00dfe,", "tokens": ["Da\u00b7rum", "schick", "ich", "dir", "ei\u00b7lig", "die", "Frucht", "voll", "ir\u00b7di\u00b7scher", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADJD", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Da\u00df der Himmel dich nicht deinem Geliebten entzieh.", "tokens": ["Da\u00df", "der", "Him\u00b7mel", "dich", "nicht", "dei\u00b7nem", "Ge\u00b7lieb\u00b7ten", "ent\u00b7zieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKNEG", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Reichte die sch\u00e4dliche Frucht einst Mutter Eva dem Gatten,", "tokens": ["Reich\u00b7te", "die", "sch\u00e4d\u00b7li\u00b7che", "Frucht", "einst", "Mut\u00b7ter", "E\u00b7va", "dem", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "NN", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ach! vom t\u00f6richten Bi\u00df kr\u00e4nkelt das ganze Geschlecht.", "tokens": ["Ach", "!", "vom", "t\u00f6\u00b7rich\u00b7ten", "Bi\u00df", "kr\u00e4n\u00b7kelt", "das", "gan\u00b7ze", "Ge\u00b7schlecht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter