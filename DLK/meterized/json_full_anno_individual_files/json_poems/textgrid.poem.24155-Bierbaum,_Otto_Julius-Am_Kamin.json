{"textgrid.poem.24155": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Am Kamin", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen bl\u00e4st der Wind und fegt", "tokens": ["Drau\u00b7\u00dfen", "bl\u00e4st", "der", "Wind", "und", "fegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flocken an die Fensterscheiben,", "tokens": ["Flo\u00b7cken", "an", "die", "Fens\u00b7ter\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcrrisch patrouilliert der Mond", "tokens": ["M\u00fcr\u00b7risch", "pat\u00b7rou\u00b7il\u00b7liert", "der", "Mond"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hinter dicken Wolkenw\u00e4llen.", "tokens": ["Hin\u00b7ter", "di\u00b7cken", "Wol\u00b7ken\u00b7w\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Kamin sitz ich und st\u00fctze", "tokens": ["Am", "Ka\u00b7min", "sitz", "ich", "und", "st\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVIMP", "PPER", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meine F\u00fc\u00dfe auf das Gitter,", "tokens": ["Mei\u00b7ne", "F\u00fc\u00b7\u00dfe", "auf", "das", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich starre in die Gluten,", "tokens": ["Und", "ich", "star\u00b7re", "in", "die", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das hei\u00dfe, helle Sterben.", "tokens": ["In", "das", "hei\u00b7\u00dfe", ",", "hel\u00b7le", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie die Flammenzungen zucken,", "tokens": ["Wie", "die", "Flam\u00b7men\u00b7zun\u00b7gen", "zu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese roten Schlangenzungen;", "tokens": ["Die\u00b7se", "ro\u00b7ten", "Schlan\u00b7gen\u00b7zun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kleine blaue Flackerfl\u00e4mmchen", "tokens": ["Klei\u00b7ne", "blau\u00b7e", "Fla\u00b7cker\u00b7fl\u00e4mm\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beben wie erschrockene Seelen,", "tokens": ["Be\u00b7ben", "wie", "er\u00b7schro\u00b7cke\u00b7ne", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Und glutgoldene Flammenschwerter", "tokens": ["Und", "glut\u00b7gol\u00b7de\u00b7ne", "Flam\u00b7men\u00b7schwer\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Sto\u00dfen unabl\u00e4ssig blitzend", "tokens": ["Sto\u00b7\u00dfen", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", "blit\u00b7zend"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In die leere Luft.", "tokens": ["In", "die", "lee\u00b7re", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hinter mir auf eichenem Tische", "tokens": ["Hin\u00b7ter", "mir", "auf", "ei\u00b7che\u00b7nem", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Singt der Samovar sein leises", "tokens": ["Singt", "der", "Sa\u00b7mo\u00b7var", "sein", "lei\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seufzerlied, auf dem Gesimse", "tokens": ["Seuf\u00b7zer\u00b7lied", ",", "auf", "dem", "Ge\u00b7sim\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Kamins tickt silbert\u00f6nig", "tokens": ["Des", "Ka\u00b7mins", "tickt", "sil\u00b7ber\u00b7t\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Pend\u00fcle; wie in Aengsten", "tokens": ["Die", "Pen\u00b7d\u00fc\u00b7le", ";", "wie", "in", "A\u00b7engs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KOKOM", "APPR", "NE"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Fegt die goldene Pendelscheibe", "tokens": ["Fegt", "die", "gol\u00b7de\u00b7ne", "Pen\u00b7del\u00b7schei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Hin und her.", "tokens": ["Hin", "und", "her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Sinkt mir auf die Brust der Kopf,", "tokens": ["Sinkt", "mir", "auf", "die", "Brust", "der", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bebts im Herzen mir wie Traum:", "tokens": ["Bebts", "im", "Her\u00b7zen", "mir", "wie", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbmai und Bl\u00fcten, Mai und Bl\u00fcten,", "tokens": ["\u00bb", "mai", "und", "Bl\u00fc\u00b7ten", ",", "Mai", "und", "Bl\u00fc\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erster Sang der Nachtigallen,", "tokens": ["Ers\u00b7ter", "Sang", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwischen duftenden Syringen", "tokens": ["Zwi\u00b7schen", "duf\u00b7ten\u00b7den", "Sy\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben wir die Nacht durchk\u00fc\u00dft \u2013\u00ab", "tokens": ["Ha\u00b7ben", "wir", "die", "Nacht", "durch\u00b7k\u00fc\u00dft", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Haben ... wir ... die Nacht ... durchk\u00fc\u00dft ...", "tokens": ["Ha\u00b7ben", "...", "wir", "...", "die", "Nacht", "...", "durch\u00b7k\u00fc\u00dft", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$(", "PPER", "$(", "ART", "NN", "$(", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aus dem tiefsten Herzen tauchen", "tokens": ["Aus", "dem", "tiefs\u00b7ten", "Her\u00b7zen", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir die Verse wie ein Tr\u00e4umen, \u2013", "tokens": ["Mir", "die", "Ver\u00b7se", "wie", "ein", "Tr\u00e4u\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "KOKOM", "ART", "NN", "$,", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Aber glaub ich diesem Traume?", "tokens": ["A\u00b7ber", "glaub", "ich", "die\u00b7sem", "Trau\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "War es denn, das warme Leben", "tokens": ["War", "es", "denn", ",", "das", "war\u00b7me", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den hei\u00dfen, nahen Lippen?", "tokens": ["Mit", "den", "hei\u00b7\u00dfen", ",", "na\u00b7hen", "Lip\u00b7pen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "War es denn?", "tokens": ["War", "es", "denn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Es ist in mein Herz gefrostet,", "tokens": ["Es", "ist", "in", "mein", "Herz", "ge\u00b7fros\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Hartes Eis, hell wie Erfahrung,", "tokens": ["Har\u00b7tes", "Eis", ",", "hell", "wie", "Er\u00b7fah\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Undurchdringlich starre Kruste,", "tokens": ["Un\u00b7durch\u00b7dring\u00b7lich", "star\u00b7re", "Krus\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die kein Hoffen mehr durchbricht;", "tokens": ["Die", "kein", "Hof\u00b7fen", "mehr", "durch\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schnee ist auf mein Haupt gefallen,", "tokens": ["Schnee", "ist", "auf", "mein", "Haupt", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schnee, den keine Sonne schmelzen,", "tokens": ["Schnee", ",", "den", "kei\u00b7ne", "Son\u00b7ne", "schmel\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Den kein Lenz verjagen wird.", "tokens": ["Den", "kein", "Lenz", "ver\u00b7ja\u00b7gen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kalt und leer und stumm und farblos", "tokens": ["Kalt", "und", "leer", "und", "stumm", "und", "farb\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist die ganze Welt mir worden,", "tokens": ["Ist", "die", "gan\u00b7ze", "Welt", "mir", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seit ich ihres Herzens W\u00e4rme", "tokens": ["Seit", "ich", "ih\u00b7res", "Her\u00b7zens", "W\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht an meiner Brust mehr f\u00fchle,", "tokens": ["Nicht", "an", "mei\u00b7ner", "Brust", "mehr", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Seit mir ihres Herzens F\u00fclle", "tokens": ["Seit", "mir", "ih\u00b7res", "Her\u00b7zens", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Nicht mehr lebt in tiefer Liebe,", "tokens": ["Nicht", "mehr", "lebt", "in", "tie\u00b7fer", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Seit ihr Mund verstummt,", "tokens": ["Seit", "ihr", "Mund", "ver\u00b7stummt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Der so innig sprach,", "tokens": ["Der", "so", "in\u00b7nig", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Seit ihr blaues Auge", "tokens": ["Seit", "ihr", "blau\u00b7es", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Stier im Tode brach.", "tokens": ["Stier", "im", "To\u00b7de", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "In den Flammen nur ist Leben,", "tokens": ["In", "den", "Flam\u00b7men", "nur", "ist", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dies Leben ist das hei\u00dfe,", "tokens": ["Und", "dies", "Le\u00b7ben", "ist", "das", "hei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jache, ungest\u00fcme Sterben.", "tokens": ["Ja\u00b7che", ",", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Drau\u00dfen bl\u00e4st der Wind und fegt", "tokens": ["Drau\u00b7\u00dfen", "bl\u00e4st", "der", "Wind", "und", "fegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flocken an die Fensterscheiben,", "tokens": ["Flo\u00b7cken", "an", "die", "Fens\u00b7ter\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcrrisch patrouilliert der Mond", "tokens": ["M\u00fcr\u00b7risch", "pat\u00b7rou\u00b7il\u00b7liert", "der", "Mond"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hinter dicken Wolkenw\u00e4llen.", "tokens": ["Hin\u00b7ter", "di\u00b7cken", "Wol\u00b7ken\u00b7w\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Am Kamin sitz ich und st\u00fctze", "tokens": ["Am", "Ka\u00b7min", 