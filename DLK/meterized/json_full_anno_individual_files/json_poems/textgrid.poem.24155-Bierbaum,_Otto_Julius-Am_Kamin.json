: "Stier im Tode brach.", "tokens": ["Stier", "im", "To\u00b7de", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "In den Flammen nur ist Leben,", "tokens": ["In", "den", "Flam\u00b7men", "nur", "ist", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dies Leben ist das hei\u00dfe,", "tokens": ["Und", "dies", "Le\u00b7ben", "ist", "das", "hei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jache, ungest\u00fcme Sterben.", "tokens": ["Ja\u00b7che", ",", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}