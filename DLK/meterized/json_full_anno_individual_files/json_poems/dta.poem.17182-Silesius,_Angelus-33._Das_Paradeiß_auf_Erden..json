{"dta.poem.17182": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "33.  Das Paradei\u00df auf Erden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du suchst das Paradei\u00df/ und w\u00fcntschest hin zukommen/", "tokens": ["Du", "suchst", "das", "Pa\u00b7ra\u00b7dei\u00df", "/", "und", "w\u00fcnt\u00b7schest", "hin", "zu\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo du von allem Lei\u00df und Unfried bist entnommen.", "tokens": ["Wo", "du", "von", "al\u00b7lem", "Lei\u00df", "und", "Un\u00b7fried", "bist", "ent\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIS", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Befriedige dein Hertz/ und mach es Rein und wei\u00df:", "tokens": ["Be\u00b7frie\u00b7di\u00b7ge", "dein", "Hertz", "/", "und", "mach", "es", "Rein", "und", "wei\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bistu selbst noch hier dasselbe Paradei\u00df.", "tokens": ["So", "bis\u00b7tu", "selbst", "noch", "hier", "das\u00b7sel\u00b7be", "Pa\u00b7ra\u00b7dei\u00df", "."], "token_info": ["w