{"textgrid.poem.52246": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So seh' im Geist ein trutzig Kriegsgeschwader", "tokens": ["So", "seh'", "im", "Geist", "ein", "trut\u00b7zig", "Kriegs\u00b7ge\u00b7schwa\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich Wacht sie halten, festiglich und stete,", "tokens": ["Ich", "Wacht", "sie", "hal\u00b7ten", ",", "fes\u00b7tig\u00b7lich", "und", "ste\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "VVINF", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo weiland nur des Ewers Wimpel wehte,", "tokens": ["Wo", "wei\u00b7land", "nur", "des", "E\u00b7wers", "Wim\u00b7pel", "weh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Buxtehuder etwa oder Stader;", "tokens": ["Ein", "Bux\u00b7te\u00b7hu\u00b7der", "et\u00b7wa", "o\u00b7der", "Sta\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da naht der Feind, und mit ihm naht der Hader!", "tokens": ["Da", "naht", "der", "Feind", ",", "und", "mit", "ihm", "naht", "der", "Ha\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufzischt gen Himmel die Signalrakete,", "tokens": ["Auf\u00b7zischt", "gen", "Him\u00b7mel", "die", "Sig\u00b7nal\u00b7ra\u00b7ke\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Trommel w\u00fctet, und an die Lafette", "tokens": ["Die", "Trom\u00b7mel", "w\u00fc\u00b7tet", ",", "und", "an", "die", "La\u00b7fet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schlachtatmend tritt das r\u00fcst'ge Volk der Lader!", "tokens": ["Schlach\u00b7tat\u00b7mend", "tritt", "das", "r\u00fcst'\u00b7ge", "Volk", "der", "La\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Sprachrohr heischt: da birst mit tausend Sch\u00fcssen", "tokens": ["Das", "Sprach\u00b7rohr", "heischt", ":", "da", "birst", "mit", "tau\u00b7send", "Sch\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Flammengru\u00df aus den metallnen L\u00e4ufen;", "tokens": ["Ihr", "Flam\u00b7men\u00b7gru\u00df", "aus", "den", "me\u00b7tall\u00b7nen", "L\u00e4u\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN