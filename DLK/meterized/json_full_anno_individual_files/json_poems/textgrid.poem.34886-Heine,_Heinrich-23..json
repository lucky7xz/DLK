{"textgrid.poem.34886": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ich so ganz vorz\u00fcglich blitze,", "tokens": ["Weil", "ich", "so", "ganz", "vor\u00b7z\u00fcg\u00b7lich", "blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Glaubt ihr, da\u00df ich nicht donnern k\u00f6nnt!", "tokens": ["Glaubt", "ihr", ",", "da\u00df", "ich", "nicht", "don\u00b7nern", "k\u00f6nnt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr irrt euch sehr, denn ich besitze", "tokens": ["Ihr", "irrt", "euch", "sehr", ",", "denn", "ich", "be\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleichfalls f\u00fcrs Donnern ein Talent.", "tokens": ["Gleich\u00b7falls", "f\u00fcrs", "Don\u00b7nern", "ein", "Ta\u00b7lent", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Es wird sich grausenhaft bew\u00e4hren,", "tokens": ["Es", "wird", "sich", "grau\u00b7sen\u00b7haft", "be\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-",