{"dta.poem.7534": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kommst du in fremde Welt, so siehst du fremden Baum,", "tokens": ["Kommst", "du", "in", "frem\u00b7de", "Welt", ",", "so", "siehst", "du", "frem\u00b7den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fremd Antlitz, fremd Gethier, dich schreckt der fremde Raum.", "tokens": ["Fremd", "Ant\u00b7litz", ",", "fremd", "Ge\u00b7thier", ",", "dich", "schreckt", "der", "frem\u00b7de", "Raum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch sieh den Boden an, er ist vom selben Steine,", "tokens": ["Doch", "sieh", "den", "Bo\u00b7den", "an", ",", "er", "ist", "vom", "sel\u00b7ben", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sieh das Wasser auch, es ist vom selben Scheine.", "tokens": ["Und", "sieh", "das", "Was\u00b7ser", "auch", ",", "es", "ist", "vom", "sel\u00b7ben", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADV", "$,", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann sieh zum Himmel auf, es sind dieselben Sterne;", "tokens": ["Dann", "sieh", "zum", "Him\u00b7mel", "auf", ",", "es", "sind", "die\u00b7sel\u00b7ben", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so im fremden Raum dich heimisch finden lerne.", "tokens": ["Und", "so", "im", "frem\u00b7den", "Raum", "dich", "hei\u00b7misch", "fin\u00b7den", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Sterne helfen dir, das Wasser und die Erde,", "tokens": ["Die", "Ster\u00b7ne", "hel\u00b7fen", "dir", ",", "das", "Was\u00b7ser", "und", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df unfremd Baum und Thier und auch der Mensch dir werde.", "tokens": ["Da\u00df", "un\u00b7fremd", "Baum", "und", "Thier", "und", "auch", "der", "Mensch", "dir", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "KON", "NN", "KON", "ADV", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Befreundet wirst du leicht mit fremdester Natur,", "tokens": ["Be\u00b7freun\u00b7det", "wirst", "du", "leicht", "mit", "frem\u00b7des\u00b7ter", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am l\u00e4ngsten bleibt der Mensch dem Menschen fremde nur.", "tokens": ["Am", "l\u00e4ngs\u00b7ten", "bleibt", "der", "Mensch", "dem", "Men\u00b7schen", "frem\u00b7de", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und erst der Himmel mu\u00df erkl\u00e4ren und die Erde", "tokens": ["Und", "erst", "der", "Him\u00b7mel", "mu\u00df", "er\u00b7kl\u00e4\u00b7ren", "und", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir deines Bruders ganz entfremdete Geberde.", "tokens": ["Dir", "dei\u00b7nes", "Bru\u00b7ders", "ganz", "ent\u00b7frem\u00b7de\u00b7te", "Ge\u00b7ber\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}