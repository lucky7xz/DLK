{"textgrid.poem.49757": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist neulich in Gnesen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist neulich in Gnesen", "tokens": ["Das", "ist", "neu\u00b7lich", "in", "Gne\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wieder bei einem Feste gewesen,", "tokens": ["Wie\u00b7der", "bei", "ei\u00b7nem", "Fes\u00b7te", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo man so nebenbei erfuhr:", "tokens": ["Wo", "man", "so", "ne\u00b7ben\u00b7bei", "er\u00b7fuhr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tja. Kultur, und ei! ei!", "tokens": ["Tja", ".", "Kul\u00b7tur", ",", "und", "ei", "!", "ei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "KON", "NE", "$.", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da\u00df es auch die Freiheit sei.", "tokens": ["Da\u00df", "es", "auch", "die", "Frei\u00b7heit", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier\u00fcber sage ich zun\u00e4chst:", "tokens": ["Hier\u00b7\u00fc\u00b7ber", "sa\u00b7ge", "ich", "zu\u00b7n\u00e4chst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dieses Diktum ist allerh\u00f6chst.", "tokens": ["Die\u00b7ses", "Dik\u00b7tum", "ist", "al\u00b7ler\u00b7h\u00f6chst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Dar\u00fcber sind wir uns alle klar.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "sind", "wir", "uns", "al\u00b7le", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "PIS", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Zweitens: ist es aber auch wahr?", "tokens": ["Zwei\u00b7tens", ":", "ist", "es", "a\u00b7ber", "auch", "wahr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VAFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Das hei\u00dft: ob es auch g\u00e4nzlich stimmt,", "tokens": ["Das", "hei\u00dft", ":", "ob", "es", "auch", "g\u00e4nz\u00b7lich", "stimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Indem es von so hoher Stelle kimmt?", "tokens": ["In\u00b7dem", "es", "von", "so", "ho\u00b7her", "Stel\u00b7le", "kimmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Untersuchen wir mit tiefstem Respekt,", "tokens": ["Un\u00b7ter\u00b7su\u00b7chen", "wir", "mit", "tiefs\u00b7tem", "Res\u00b7pekt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Ob das Diktum auch fleckt und kleckt.", "tokens": ["Ob", "das", "Dik\u00b7tum", "auch", "fleckt", "und", "kleckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Also: \u00bbDeutschtum ist Kultur.\u00ab", "tokens": ["Al\u00b7so", ":", "\u00bb", "Deutschtum", "ist", "Kul\u00b7tur", ".", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "$(", "NE", "VAFIN", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Sch\u00f6n! Fragt sich nur,", "tokens": ["Sch\u00f6n", "!", "Fragt", "sich", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Was f\u00fcr eine.", "tokens": ["Was", "f\u00fcr", "ei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "So zum Beispiel ist es keine,", "tokens": ["So", "zum", "Bei\u00b7spiel", "ist", "es", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wenn man ihr erstes Element", "tokens": ["Wenn", "man", "ihr", "ers\u00b7tes", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mitunter und h\u00e4ufig so ganz verkennt,", "tokens": ["Mi\u00b7tun\u00b7ter", "und", "h\u00e4u\u00b7fig", "so", "ganz", "ver\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Ich meine, wenn man die Kunst", "tokens": ["Ich", "mei\u00b7ne", ",", "wenn", "man", "die", "Kunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Recht gotteserb\u00e4rmlich verhunzt.", "tokens": ["Recht", "got\u00b7te\u00b7ser\u00b7b\u00e4rm\u00b7lich", "ver\u00b7hunzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.22": {"text": "Und \u2013 pardong! \u2013 in einem Land,", "tokens": ["Und", "\u2013", "par\u00b7dong", "!", "\u2013", "in", "ei\u00b7nem", "Land", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "$.", "$(", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Wo blo\u00df der Pfaffe und Leutenant", "tokens": ["Wo", "blo\u00df", "der", "Pfaf\u00b7fe", "und", "Leu\u00b7ten\u00b7ant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Ein ungest\u00f6rtes Behagen genie\u00dfen,", "tokens": ["Ein", "un\u00b7ge\u00b7st\u00f6r\u00b7tes", "Be\u00b7ha\u00b7gen", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Kann nicht viele Kultur ersprie\u00dfen.", "tokens": ["Kann", "nicht", "vie\u00b7le", "Kul\u00b7tur", "er\u00b7sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PIAT", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Freilich \u2013 in einer Monarchie,", "tokens": ["Frei\u00b7lich", "\u2013", "in", "ei\u00b7ner", "Mon\u00b7ar\u00b7chie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Zugegeben, da braucht man sie.", "tokens": ["Zu\u00b7ge\u00b7ge\u00b7ben", ",", "da", "braucht", "man", "sie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVFIN", "PIS", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "Der liebe Gott und Gefreitenkn\u00f6pfe,", "tokens": ["Der", "lie\u00b7be", "Gott", "und", "Ge\u00b7frei\u00b7ten\u00b7kn\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Schandarmen, Richter und gute Gesch\u00f6pfe", "tokens": ["Schan\u00b7dar\u00b7men", ",", "Rich\u00b7ter", "und", "gu\u00b7te", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Bilden die monarchische Institution.", "tokens": ["Bil\u00b7den", "die", "mon\u00b7ar\u00b7chi\u00b7sche", "Ins\u00b7ti\u00b7tu\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Und st\u00fctzen den Thron.", "tokens": ["Und", "st\u00fct\u00b7zen", "den", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.32": {"text": "Aber \u2013 Kultur?! So unterweil", "tokens": ["A\u00b7ber", "\u2013", "Kul\u00b7tur", "?!", "So", "un\u00b7ter\u00b7weil"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$(", "NN", "$.", "ADV", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Kommt's mir vor wie das Gegenteil.", "tokens": ["Kommt's", "mir", "vor", "wie", "das", "Ge\u00b7gen\u00b7teil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und die Sittlichkeitsriecher im sch\u00f6nen Bunde,", "tokens": ["Und", "die", "Sitt\u00b7lich\u00b7keits\u00b7rie\u00b7cher", "im", "sch\u00f6\u00b7nen", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.35": {"text": "Die auf der Stra\u00dfe wie rote Hunde", "tokens": ["Die", "auf", "der", "Stra\u00b7\u00dfe", "wie", "ro\u00b7te", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Hinter dem Geschlechtlichen jagen,", "tokens": ["Hin\u00b7ter", "dem", "Ge\u00b7schlecht\u00b7li\u00b7chen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.37": {"text": "Und es hinterdrein verklagen, \u2013", "tokens": ["Und", "es", "hin\u00b7ter\u00b7drein", "ver\u00b7kla\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Dieses Gesindel und auch der Staat,", "tokens": ["Die\u00b7ses", "Ge\u00b7sin\u00b7del", "und", "auch", "der", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Der es gepflegt und gef\u00f6rdert hat \u2013", "tokens": ["Der", "es", "ge\u00b7pflegt", "und", "ge\u00b7f\u00f6r\u00b7dert", "hat", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "VVPP", "VAFIN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.40": {"text": "Wirklich Kultur bedeuten sie?", "tokens": ["Wirk\u00b7lich", "Kul\u00b7tur", "be\u00b7deu\u00b7ten", "sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Die Mucker, Schweine und Compagnie?", "tokens": ["Die", "Mu\u00b7cker", ",", "Schwei\u00b7ne", "und", "Com\u00b7pag\u00b7nie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Und eine Regierung, die solches liebt,", "tokens": ["Und", "ei\u00b7ne", "Re\u00b7gie\u00b7rung", ",", "die", "sol\u00b7ches", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.43": {"text": "Da\u00df sie den Russen die Schergen gibt,", "tokens": ["Da\u00df", "sie", "den", "Rus\u00b7sen", "die", "Scher\u00b7gen", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "Die ist Kultur??! \u2013 Na, mir ist's recht,", "tokens": ["Die", "ist", "Kul\u00b7tur", "??!", "\u2013", "Na", ",", "mir", "ist's", "recht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$.", "$(", "ITJ", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Aber was ich bemerken m\u00f6cht',", "tokens": ["A\u00b7ber", "was", "ich", "be\u00b7mer\u00b7ken", "m\u00f6cht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Da erlaub' ich mir wirklich schon", "tokens": ["Da", "er\u00b7laub'", "ich", "mir", "wirk\u00b7lich", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.47": {"text": "Ganz untert\u00e4nigst einmal zu fragen:", "tokens": ["Ganz", "un\u00b7ter\u00b7t\u00e4\u00b7nigst", "ein\u00b7mal", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "\u00dcbt nicht in Stra\u00dfburg in diesen Tagen", "tokens": ["\u00dcbt", "nicht", "in", "Stra\u00df\u00b7burg", "in", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "NE", "APPR", "PDAT", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.49": {"text": "Die Dummheit wieder den alten Zwang?", "tokens": ["Die", "Dumm\u00b7heit", "wie\u00b7der", "den", "al\u00b7ten", "Zwang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.50": {"text": "Und wenn 's ihr in Deutschland so wohl gelang,", "tokens": ["Und", "wenn", "'s", "ihr", "in", "Deutschland", "so", "wohl", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPR", "NE", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.51": {"text": "In Frankreich hat man sie abgeschafft.", "tokens": ["In", "Fran\u00b7kreich", "hat", "man", "sie", "ab\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.52": {"text": "Wir aber werden verdummt, verpfafft,", "tokens": ["Wir", "a\u00b7ber", "wer\u00b7den", "ver\u00b7dummt", ",", "ver\u00b7pfafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.53": {"text": "Und r\u00fcckw\u00e4rts, r\u00fcckw\u00e4rts weist die Spur.", "tokens": ["Und", "r\u00fcck\u00b7w\u00e4rts", ",", "r\u00fcck\u00b7w\u00e4rts", "weist", "die", "Spur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Vielleicht ist 's fr\u00fcher so gewesen", "tokens": ["Viel\u00b7leicht", "ist", "'s", "fr\u00fc\u00b7her", "so", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word