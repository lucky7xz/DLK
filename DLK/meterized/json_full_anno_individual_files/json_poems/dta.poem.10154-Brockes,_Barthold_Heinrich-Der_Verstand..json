{"dta.poem.10154": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Verstand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Offt hab' ich bey mir \u00fcberleget,", "tokens": ["Offt", "hab'", "ich", "bey", "mir", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was der Verstand doch eigentlich,", "tokens": ["Was", "der", "Ver\u00b7stand", "doch", "ei\u00b7gent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch die Menschheit sich", "tokens": ["Wo\u00b7durch", "die", "Menschheit", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Auf eine andre Art, als wie ein Vieh, betr\u00e4get.", "tokens": ["Auf", "ei\u00b7ne", "and\u00b7re", "Art", ",", "als", "wie", "ein", "Vieh", ",", "be\u00b7tr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich stelle denn, nach langem dencken, mir", "tokens": ["Ich", "stel\u00b7le", "denn", ",", "nach", "lan\u00b7gem", "den\u00b7cken", ",", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "VVINF", "$,", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denselbigen nicht anders f\u00fcr,", "tokens": ["Den\u00b7sel\u00b7bi\u00b7gen", "nicht", "an\u00b7ders", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als etwa wie ein reines Licht,", "tokens": ["Als", "et\u00b7wa", "wie", "ein", "rei\u00b7nes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das im Gehirn, als seinem Sitz, vereinet,", "tokens": ["Das", "im", "Ge\u00b7hirn", ",", "als", "sei\u00b7nem", "Sitz", ",", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von wannen es mit regen Strahlen bricht,", "tokens": ["Von", "wan\u00b7nen", "es", "mit", "re\u00b7gen", "Strah\u00b7len", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und unsern gantzen Leib durchscheinet:", "tokens": ["Und", "un\u00b7sern", "gant\u00b7zen", "Leib", "durch\u00b7schei\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da denn, wenn die\u00df in Ruh, und ungehemmt, geschicht,", "tokens": ["Da", "denn", ",", "wenn", "die\u00df", "in", "Ruh", ",", "und", "un\u00b7ge\u00b7hemmt", ",", "ge\u00b7schicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PDS", "APPR", "NN", "$,", "KON", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein heitrer Zustand, eine Stille,", "tokens": ["Ein", "hei\u00b7trer", "Zu\u00b7stand", ",", "ei\u00b7ne", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vergn\u00fcgung und Gelassenheit", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "und", "Ge\u00b7las\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In uns mit Lust entsteht. In muntrer Fr\u00f6hlichkeit", "tokens": ["In", "uns", "mit", "Lust", "ent\u00b7steht", ".", "In", "mun\u00b7trer", "Fr\u00f6h\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schwimmt gleichsam unser froher Wille,", "tokens": ["Schwimmt", "gleich\u00b7sam", "un\u00b7ser", "fro\u00b7her", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zum GOttes-Dienst, zur Danckbarkeit", "tokens": ["Zum", "Got\u00b7tes\u00b7Dienst", ",", "zur", "Dan\u00b7ck\u00b7bar\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zur N\u00e4chsten-Lieb\u2019 und Tugenden bereit.", "tokens": ["Zur", "N\u00e4chs\u00b7ten\u00b7Lieb'", "und", "Tu\u00b7gen\u00b7den", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wann aber, durch der Leidenschaften Dufft", "tokens": ["Wann", "a\u00b7ber", ",", "durch", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Dufft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Sonnen Licht) das Licht der Seele", "tokens": ["Der", "Son\u00b7nen", "Licht", ")", "das", "Licht", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Benebelt und gehemmt, da nemlich die Can\u00e4le", "tokens": ["Be\u00b7ne\u00b7belt", "und", "ge\u00b7hemmt", ",", "da", "nem\u00b7lich", "die", "Ca\u00b7n\u00e4\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Jm C\u00f6rper sich verstopfft, so da\u00df die reine Glut", "tokens": ["Jm", "C\u00f6r\u00b7per", "sich", "ver\u00b7stopfft", ",", "so", "da\u00df", "die", "rei\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PRF", "VVPP", "$,", "ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die sonst in uns die vielen Wunder thut,", "tokens": ["Die", "sonst", "in", "uns", "die", "vie\u00b7len", "Wun\u00b7der", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch die Materie den Durchgang nicht zu finden", "tokens": ["Durch", "die", "Ma\u00b7te\u00b7rie", "den", "Durch\u00b7gang", "nicht", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Vermag noch f\u00e4hig ist; wird unser Wesen gleich", "tokens": ["Ver\u00b7mag", "noch", "f\u00e4\u00b7hig", "ist", ";", "wird", "un\u00b7ser", "We\u00b7sen", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "VAFIN", "$.", "VAFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verdunckelt, und dadurch in solchen Stand gesetzt,", "tokens": ["Ver\u00b7dun\u00b7ckelt", ",", "und", "da\u00b7durch", "in", "sol\u00b7chen", "Stand", "ge\u00b7setzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PAV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ihn, was viehisch ist, allein ergetzt.", "tokens": ["Da\u00df", "ihn", ",", "was", "vie\u00b7hisch", "ist", ",", "al\u00b7lein", "er\u00b7getzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ja wie sich in der L\u00fcffte Neich", "tokens": ["Ja", "wie", "sich", "in", "der", "L\u00fcff\u00b7te", "Neich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KON", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Offt Schwefel-D\u00fcfft\u2019 in sich so fest verbinden,", "tokens": ["Offt", "Schwe\u00b7fel\u00b7D\u00fcfft'", "in", "sich", "so", "fest", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df sie so gar in Blitz und Donner sich entz\u00fcnden,", "tokens": ["Da\u00df", "sie", "so", "gar", "in", "Blitz", "und", "Don\u00b7ner", "sich", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und sehr gef\u00e4hrlich sind; so raset die Begier,", "tokens": ["Und", "sehr", "ge\u00b7f\u00e4hr\u00b7lich", "sind", ";", "so", "ra\u00b7set", "die", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn sie zu sehr geh\u00e4ufft, mit solcher wilden Wuht:", "tokens": ["Wenn", "sie", "zu", "sehr", "ge\u00b7h\u00e4ufft", ",", "mit", "sol\u00b7cher", "wil\u00b7den", "Wuht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADV", "VVPP", "$,", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als wie kaum in der Lufft, Blitz, Sturm und Donner", "tokens": ["Als", "wie", "kaum", "in", "der", "Lufft", ",", "Blitz", ",", "Sturm", "und", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ADV", "APPR", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie mancher Erd-Strich auch morastig, schweflicht ist,", "tokens": ["Wie", "man\u00b7cher", "Erd\u00b7Strich", "auch", "mo\u00b7ras\u00b7tig", ",", "schwef\u00b7licht", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "ADJD", "$,", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und folglich Dunst in grosser Menge", "tokens": ["Und", "folg\u00b7lich", "Dunst", "in", "gros\u00b7ser", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor vielen andern zeugt; so trifft man C\u00f6rper an,", "tokens": ["Vor", "vie\u00b7len", "an\u00b7dern", "zeugt", ";", "so", "trifft", "man", "C\u00f6r\u00b7per", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Leidenschaften offt sich recht als im Gedr\u00e4nge", "tokens": ["Wo", "Lei\u00b7den\u00b7schaf\u00b7ten", "offt", "sich", "recht", "als", "im", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "PRF", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erzeugen und erh\u00f6hn: Das Licht der Seele kann", "tokens": ["Er\u00b7zeu\u00b7gen", "und", "er\u00b7h\u00f6hn", ":", "Das", "Licht", "der", "See\u00b7le", "kann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVINF", "$.", "ART", "NN", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den dicken Schwall nicht trennen: bis zu spat,", "tokens": ["Den", "di\u00b7cken", "Schwall", "nicht", "tren\u00b7nen", ":", "bis", "zu", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$.", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn es gest\u00fcrmt, geblitzt, gedonnert hat,", "tokens": ["Wenn", "es", "ge\u00b7st\u00fcrmt", ",", "ge\u00b7blitzt", ",", "ge\u00b7don\u00b7nert", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es, wie ein \u00f6des Feld, wo alles \u00fcmgekehrt,", "tokens": ["Es", ",", "wie", "ein", "\u00f6\u00b7des", "Feld", ",", "wo", "al\u00b7les", "\u00fcm\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die H\u00e4user abgedeckt, die reiffe Saat versehrt,", "tokens": ["Die", "H\u00e4u\u00b7ser", "ab\u00b7ge\u00b7deckt", ",", "die", "reif\u00b7fe", "Saat", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die B\u00e4um\u2019 entlaubt, der G\u00e4rten Pracht verheert,", "tokens": ["Die", "B\u00e4um'", "ent\u00b7laubt", ",", "der", "G\u00e4r\u00b7ten", "Pracht", "ver\u00b7heert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In seinen Grentzen schaut. Da es den Schaden offt", "tokens": ["In", "sei\u00b7nen", "Grent\u00b7zen", "schaut", ".", "Da", "es", "den", "Scha\u00b7den", "offt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "KOUS", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit Neu zu bessern hofft:", "tokens": ["Mit", "Neu", "zu", "bes\u00b7sern", "hofft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der aber mehrentheils un\u00fcberwindlich bleibet.", "tokens": ["Der", "a\u00b7ber", "meh\u00b7ren\u00b7theils", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach lasst uns denn mit Ernst dahin uns doch bestreben,", "tokens": ["Ach", "lasst", "uns", "denn", "mit", "Ernst", "da\u00b7hin", "uns", "doch", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "NE", "PAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-