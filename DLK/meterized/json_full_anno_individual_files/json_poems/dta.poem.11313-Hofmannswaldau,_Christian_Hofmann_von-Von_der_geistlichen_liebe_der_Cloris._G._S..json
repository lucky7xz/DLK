{"dta.poem.11313": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der geistlichen liebe der Cloris.  \n  G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Cloris l\u00e4\u00dft die hofe-leute fahren,", "tokens": ["Die", "Clo\u00b7ris", "l\u00e4\u00dft", "die", "ho\u00b7fe\u00b7leu\u00b7te", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4\u00dft die pfaffen nur in ihre kammer ein.", "tokens": ["Und", "l\u00e4\u00dft", "die", "pfaf\u00b7fen", "nur", "in", "ih\u00b7re", "kam\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit die flammen noch, so vormahls weltlich waren,", "tokens": ["Da\u00b7mit", "die", "flam\u00b7men", "noch", ",", "so", "vor\u00b7mahls", "welt\u00b7lich", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "VVFIN", "ADV", "$,", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor ihrem ende geistlich seyn.", "tokens": ["Vor", "ih\u00b7rem", "en\u00b7de", "geist\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}