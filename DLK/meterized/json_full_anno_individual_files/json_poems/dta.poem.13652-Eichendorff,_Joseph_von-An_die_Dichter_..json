{"dta.poem.13652": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O la\u00dft' unedle M\u00fche fahren,", "tokens": ["O", "la\u00dft'", "u\u00b7ned\u00b7le", "M\u00fc\u00b7he", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O klingelt, glei\u00dft und schielet nicht", "tokens": ["O", "klin\u00b7gelt", ",", "glei\u00dft", "und", "schie\u00b7let", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Licht und Gnad' so ihr erfahren,", "tokens": ["Mit", "Licht", "und", "Gnad'", "so", "ihr", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur S\u00fcnde macht ihr das Gedicht!", "tokens": ["Zur", "S\u00fcn\u00b7de", "macht", "ihr", "das", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}