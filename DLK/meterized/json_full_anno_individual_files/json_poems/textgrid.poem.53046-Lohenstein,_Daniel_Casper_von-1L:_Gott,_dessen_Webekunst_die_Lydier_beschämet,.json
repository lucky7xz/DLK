{"textgrid.poem.53046": {"metadata": {"author": {"name": "Lohenstein, Daniel Casper von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott, dessen Webekunst die Lydier besch\u00e4met,", "genre": "verse", "period": "N.A.", "pub_year": 1659, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, dessen Webekunst die Lydier besch\u00e4met,", "tokens": ["Gott", ",", "des\u00b7sen", "We\u00b7be\u00b7kunst", "die", "Ly\u00b7di\u00b7er", "be\u00b7sch\u00e4\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor der Minervens Hand nur Spinnenweben macht,", "tokens": ["Vor", "der", "Mi\u00b7ner\u00b7vens", "Hand", "nur", "Spin\u00b7nen\u00b7we\u00b7ben", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Hat, als er 's Himmels Tuch mit Sternen \u00fcbers\u00e4met,", "tokens": ["Hat", ",", "als", "er", "'s", "Him\u00b7mels", "Tuch", "mit", "Ster\u00b7nen", "\u00fc\u00b7ber\u00b7s\u00e4\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PPER", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Zwar solche Bildungen in sein Geweb' gebracht,", "tokens": ["Zwar", "sol\u00b7che", "Bil\u00b7dun\u00b7gen", "in", "sein", "Ge\u00b7web'", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die kein Ph\u00f6nizier hat jemals nachgesticket,", "tokens": ["Die", "kein", "Ph\u00f6\u00b7ni\u00b7zier", "hat", "je\u00b7mals", "nach\u00b7ge\u00b7sti\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Die kein Ch\u00e4ld\u00e4er kennt, kein Zeuxis malen kann;", "tokens": ["Die", "kein", "Ch\u00e4l\u00b7d\u00e4\u00b7er", "kennt", ",", "kein", "Zeu\u00b7xis", "ma\u00b7len", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er hat auch das Gewand der Erde so geschm\u00fccket,", "tokens": ["Er", "hat", "auch", "das", "Ge\u00b7wand", "der", "Er\u00b7de", "so", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df Augen und Vernunft es sehn erstarrend an.", "tokens": ["Da\u00df", "Au\u00b7gen", "und", "Ver\u00b7nunft", "es", "sehn", "er\u00b7star\u00b7rend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein sein Meisterst\u00fcck \u00fcbt unsers K\u00fcnstlers G\u00fcte,", "tokens": ["Al\u00b7lein", "sein", "Meis\u00b7ter\u00b7st\u00fcck", "\u00fcbt", "un\u00b7sers", "K\u00fcnst\u00b7lers", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn er im Mutterleib' uns Menschen webet aus,", "tokens": ["Wenn", "er", "im", "Mut\u00b7ter\u00b7leib'", "uns", "Men\u00b7schen", "we\u00b7bet", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn er im sch\u00f6nen Leib wirkt ein verschmitzt Gem\u00fcthe", "tokens": ["Wenn", "er", "im", "sch\u00f6\u00b7nen", "Leib", "wirkt", "ein", "ver\u00b7schmitzt", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "ART", "VVPP", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und die so gro\u00dfe Welt bringt in dies kleine Haus.", "tokens": ["Und", "die", "so", "gro\u00b7\u00dfe", "Welt", "bringt", "in", "dies", "klei\u00b7ne", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "VVFIN", "APPR", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Adern F\u00e4den und die B\u00e4nder der Gebeine,", "tokens": ["Der", "A\u00b7dern", "F\u00e4\u00b7den", "und", "die", "B\u00e4n\u00b7der", "der", "Ge\u00b7bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des Fleisches zartes Garn, der Umschlag unsrer Haut", "tokens": ["Des", "Flei\u00b7sches", "zar\u00b7tes", "Garn", ",", "der", "Um\u00b7schlag", "uns\u00b7rer", "Haut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.15": {"text": "Sind, wie Tapezerei voll Perl' und Edelsteine,", "tokens": ["Sind", ",", "wie", "Ta\u00b7pe\u00b7ze\u00b7rei", "voll", "Perl'", "und", "E\u00b7del\u00b7stei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vereinbart, da\u00df Gott selbst Lust an dem Werke schaut.", "tokens": ["Ver\u00b7ein\u00b7bart", ",", "da\u00df", "Gott", "selbst", "Lust", "an", "dem", "Wer\u00b7ke", "schaut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "ADV", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-++-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "An diesem Teppiche des sch\u00f6nen Leibes findet", "tokens": ["An", "die\u00b7sem", "Tep\u00b7pi\u00b7che", "des", "sch\u00f6\u00b7nen", "Lei\u00b7bes", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.18": {"text": "Der Mensch Nichts, was von ihm noch auszumachen sei;", "tokens": ["Der", "Mensch", "Nichts", ",", "was", "von", "ihm", "noch", "aus\u00b7zu\u00b7ma\u00b7chen", "sei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "PRELS", "APPR", "PPER", "ADV", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn was nach der Geburt den Gliedern noch erwindet,", "tokens": ["Denn", "was", "nach", "der", "Ge\u00b7burt", "den", "Glie\u00b7dern", "noch", "er\u00b7win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das setzt Zeit und Natur, des Sch\u00f6pfers Dienstmagd, bei.", "tokens": ["Das", "setzt", "Zeit", "und", "Na\u00b7tur", ",", "des", "Sch\u00f6p\u00b7fers", "Dienst\u00b7magd", ",", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "$,", "ART", "NN", "NN", "$,", "PTKVZ", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Allein dem andern Blatt an diesem Kunstgewebe,", "tokens": ["Al\u00b7lein", "dem", "an\u00b7dern", "Blatt", "an", "die\u00b7sem", "Kunst\u00b7ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Seele, pflanzet Gott nur das Verm\u00f6gen ein,", "tokens": ["Der", "See\u00b7le", ",", "pflan\u00b7zet", "Gott", "nur", "das", "Ver\u00b7m\u00f6\u00b7gen", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df sie durch eigne M\u00fch' sich Werth und G\u00fcte gebe,", "tokens": ["Da\u00df", "sie", "durch", "eig\u00b7ne", "M\u00fch'", "sich", "Werth", "und", "G\u00fc\u00b7te", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und hei\u00dft des Menschen Geist selbst seiner Sch\u00f6pfer sein.", "tokens": ["Und", "hei\u00dft", "des", "Men\u00b7schen", "Geist", "selbst", "sei\u00b7ner", "Sch\u00f6p\u00b7fer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Damit der Mensch auch wei\u00df, was er f\u00fcr Bilder sticken", "tokens": ["Da\u00b7mit", "der", "Mensch", "auch", "wei\u00df", ",", "was", "er", "f\u00fcr", "Bil\u00b7der", "sti\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "$,", "PWS", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Soll in das ihm von Gott so sch\u00f6n gewebte Tuch,", "tokens": ["Soll", "in", "das", "ihm", "von", "Gott", "so", "sch\u00f6n", "ge\u00b7web\u00b7te", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PRELS", "PPER", "APPR", "NN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So l\u00e4\u00dft er die Vernunft mit ihren Augen blicken", "tokens": ["So", "l\u00e4\u00dft", "er", "die", "Ver\u00b7nunft", "mit", "ih\u00b7ren", "Au\u00b7gen", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "In 'n Spiegel seines Worts, in der Natur ihr Buch.", "tokens": ["In", "'n", "Spie\u00b7gel", "sei\u00b7nes", "Worts", ",", "in", "der", "Na\u00b7tur", "ihr", "Buch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.29": {"text": "In beiden aber steht Gott selber abgemalet,", "tokens": ["In", "bei\u00b7den", "a\u00b7ber", "steht", "Gott", "sel\u00b7ber", "ab\u00b7ge\u00b7ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADV", "VVFIN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nach dem sein Ebenbild, der Mensch, sich bilden soll.", "tokens": ["Nach", "dem", "sein", "E\u00b7ben\u00b7bild", ",", "der", "Mensch", ",", "sich", "bil\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer mit dem Schatten nun nur dieses Bildes prahlet,", "tokens": ["Wer", "mit", "dem", "Schat\u00b7ten", "nun", "nur", "die\u00b7ses", "Bil\u00b7des", "prah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In ein vollkommen Werk, gef\u00e4llt dem H\u00f6chsten wohl.", "tokens": ["In", "ein", "voll\u00b7kom\u00b7men", "Werk", ",", "ge\u00b7f\u00e4llt", "dem", "H\u00f6chs\u00b7ten", "wohl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der gro\u00dfe Gott verschm\u00e4ht Apellens gro\u00dfe K\u00fcnste,", "tokens": ["Der", "gro\u00b7\u00dfe", "Gott", "ver\u00b7schm\u00e4ht", "A\u00b7pel\u00b7lens", "gro\u00b7\u00dfe", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das Thun des Phidias, Gold, Marmor, Elfenbein", "tokens": ["Das", "Thun", "des", "Phi\u00b7di\u00b7as", ",", "Gold", ",", "Mar\u00b7mor", ",", "El\u00b7fen\u00b7bein"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und das in Purpurblut getauchte Wurmgespinnste,", "tokens": ["Und", "das", "in", "Pur\u00b7pur\u00b7blut", "ge\u00b7tauch\u00b7te", "Wurm\u00b7ge\u00b7spinns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Weil er in 's Menschen Geist nur will gepr\u00e4get sein.", "tokens": ["Weil", "er", "in", "'s", "Men\u00b7schen", "Geist", "nur", "will", "ge\u00b7pr\u00e4\u00b7get", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "NN", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.37": {"text": "Den aber pr\u00e4gt der Mensch, wenn er die Tugend \u00fcbet,", "tokens": ["Den", "a\u00b7ber", "pr\u00e4gt", "der", "Mensch", ",", "wenn", "er", "die", "Tu\u00b7gend", "\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Durch Andacht sich zu Gott zu schwingen ist bem\u00fcht,", "tokens": ["Durch", "An\u00b7dacht", "sich", "zu", "Gott", "zu", "schwin\u00b7gen", "ist", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nach Wissenschaften strebt, die Weisheit herzlich liebet;", "tokens": ["Nach", "Wis\u00b7sen\u00b7schaf\u00b7ten", "strebt", ",", "die", "Weis\u00b7heit", "herz\u00b7lich", "lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Denn dies macht zwischen Mensch und Vieh den Unterschied.", "tokens": ["Denn", "dies", "macht", "zwi\u00b7schen", "Mensch", "und", "Vieh", "den", "Un\u00b7ter\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.41": {"text": "Durch dieses kann ein Mensch ein Halbgott hier auf Erden,", "tokens": ["Durch", "die\u00b7ses", "kann", "ein", "Mensch", "ein", "Halb\u00b7gott", "hier", "auf", "Er\u00