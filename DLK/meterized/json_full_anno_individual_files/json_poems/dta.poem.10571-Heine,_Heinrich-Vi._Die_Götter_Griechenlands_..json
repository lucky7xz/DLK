{"dta.poem.10571": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Die G\u00f6tter Griechenlands .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Vollbl\u00fchender Mond! In deinem Licht,", "tokens": ["Voll\u00b7bl\u00fc\u00b7hen\u00b7der", "Mond", "!", "In", "dei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie flie\u00dfendes Gold, ergl\u00e4nzt das Meer;", "tokens": ["Wie", "flie\u00b7\u00dfen\u00b7des", "Gold", ",", "er\u00b7gl\u00e4nzt", "das", "Meer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Tagesklarheit, doch d\u00e4mm'rig verzaubert,", "tokens": ["Wie", "Ta\u00b7ges\u00b7klar\u00b7heit", ",", "doch", "d\u00e4m\u00b7m'\u00b7rig", "ver\u00b7zau\u00b7bert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Liegt's \u00fcber der weiten Strandesfl\u00e4che;", "tokens": ["Liegt's", "\u00fc\u00b7ber", "der", "wei\u00b7ten", "Stran\u00b7des\u00b7fl\u00e4\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und am hellblau'n sternlosen Himmel", "tokens": ["Und", "am", "hell\u00b7blau'n", "stern\u00b7lo\u00b7sen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "ADJA", "NN"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.6": {"text": "Schweben die wei\u00dfen Wolken,", "tokens": ["Schwe\u00b7ben", "die", "wei\u00b7\u00dfen", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wie kolossale G\u00f6tterbilder", "tokens": ["Wie", "ko\u00b7los\u00b7sa\u00b7le", "G\u00f6t\u00b7ter\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von leuchtendem Marmor.", "tokens": ["Von", "leuch\u00b7ten\u00b7dem", "Mar\u00b7mor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Nein, nimmermehr, das sind keine Wolken!", "tokens": ["Nein", ",", "nim\u00b7mer\u00b7mehr", ",", "das", "sind", "kei\u00b7ne", "Wol\u00b7ken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$,", "PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das sind sie selber, die G\u00f6tter von Hellas,", "tokens": ["Das", "sind", "sie", "sel\u00b7ber", ",", "die", "G\u00f6t\u00b7ter", "von", "Hel\u00b7las", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die einst so freudig die Welt beherrschten,", "tokens": ["Die", "einst", "so", "freu\u00b7dig", "die", "Welt", "be\u00b7herrschten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch jetzt, verdr\u00e4ngt und verstorben,", "tokens": ["Doch", "jetzt", ",", "ver\u00b7dr\u00e4ngt", "und", "ver\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als ungeheure Gespenster dahinziehn", "tokens": ["Als", "un\u00b7ge\u00b7heu\u00b7re", "Ge\u00b7spens\u00b7ter", "da\u00b7hin\u00b7ziehn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Am mittern\u00e4chtlichen Himmel", "tokens": ["Am", "mit\u00b7ter\u00b7n\u00e4cht\u00b7li\u00b7chen", "Him\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}