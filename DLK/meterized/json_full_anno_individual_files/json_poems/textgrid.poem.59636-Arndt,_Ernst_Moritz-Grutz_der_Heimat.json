{"textgrid.poem.59636": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Grutz der Heimat", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Felder, s\u00fc\u00dfe Haine,", "tokens": ["Ge\u00b7lieb\u00b7te", "Fel\u00b7der", ",", "s\u00fc\u00b7\u00dfe", "Hai\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bin ich endlich wieder da,", "tokens": ["So", "bin", "ich", "end\u00b7lich", "wie\u00b7der", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich als Kind beim Sternenscheine", "tokens": ["Wo", "ich", "als", "Kind", "beim", "Ster\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "KOUS", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So oft die Engel wandeln sah,", "tokens": ["So", "oft", "die", "En\u00b7gel", "wan\u00b7deln", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo mir aus himmlischen Geschichten", "tokens": ["Wo", "mir", "aus", "himm\u00b7li\u00b7schen", "Ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Himmel diese Erde schien,", "tokens": ["Ein", "Him\u00b7mel", "die\u00b7se", "Er\u00b7de", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Freuden wimmelnd und Gedichten,", "tokens": ["Von", "Freu\u00b7den", "wim\u00b7melnd", "und", "Ge\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Adams Eden lieb und gr\u00fcn?", "tokens": ["Wie", "A\u00b7dams", "E\u00b7den", "lieb", "und", "gr\u00fcn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So seh' ich dich, mein Schoritz, wieder,", "tokens": ["So", "seh'", "ich", "dich", ",", "mein", "Scho\u00b7ritz", ",", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo mir das Meer mit dunkelm Klang", "tokens": ["Wo", "mir", "das", "Meer", "mit", "dun\u00b7kelm", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ahnungsvollen Wunderlieder", "tokens": ["Die", "ah\u00b7nungs\u00b7vol\u00b7len", "Wun\u00b7der\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zukunft um die Wiege sang?", "tokens": ["Der", "Zu\u00b7kunft", "um", "die", "Wie\u00b7ge", "sang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kann ich wieder dich begr\u00fc\u00dfen,", "tokens": ["So", "kann", "ich", "wie\u00b7der", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Dumsevitz, du trauter Ort?", "tokens": ["Mein", "Dum\u00b7se\u00b7vitz", ",", "du", "trau\u00b7ter", "Ort", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So traut, da\u00df meine Tr\u00e4nen flie\u00dfen,", "tokens": ["So", "traut", ",", "da\u00df", "mei\u00b7ne", "Tr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meine Lippe wei\u00df kein Wort?", "tokens": ["Und", "mei\u00b7ne", "Lip\u00b7pe", "wei\u00df", "kein", "Wort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie vieles mu\u00df ich nicht bedenken,", "tokens": ["Wie", "vie\u00b7les", "mu\u00df", "ich", "nicht", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn euch ich also wiederseh'?", "tokens": ["Wenn", "euch", "ich", "al\u00b7so", "wie\u00b7der\u00b7seh'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin sich meine Schritte lenken,", "tokens": ["Wo\u00b7hin", "sich", "mei\u00b7ne", "Schrit\u00b7te", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tut alles mir so lieb, so weh,", "tokens": ["Tut", "al\u00b7les", "mir", "so", "lieb", ",", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "ADV", "ADJD", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An jeden Baum, an jede Quelle", "tokens": ["An", "je\u00b7den", "Baum", ",", "an", "je\u00b7de", "Quel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00e4ngt liebend die Erinnrung sich,", "tokens": ["H\u00e4ngt", "lie\u00b7bend", "die", "E\u00b7rinn\u00b7rung", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jedes Bl\u00e4ttchen, jede Welle", "tokens": ["Und", "je\u00b7des", "Bl\u00e4tt\u00b7chen", ",", "je\u00b7de", "Wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Fragt freundlich: Wandrer, kennst du mich?", "tokens": ["Fragt", "freund\u00b7lich", ":", "Wand\u00b7rer", ",", "kennst", "du", "mich", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "NN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und diese leise Kinderfrage", "tokens": ["Und", "die\u00b7se", "lei\u00b7se", "Kin\u00b7der\u00b7fra\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4llt wie ein Stein mir auf das Herz,", "tokens": ["F\u00e4llt", "wie", "ein", "Stein", "mir", "auf", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In stiller R\u00fcckflut ferner Tage", "tokens": ["In", "stil\u00b7ler", "R\u00fcck\u00b7flut", "fer\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt inhaltschwer ein ernster Scherz,", "tokens": ["Kommt", "in\u00b7halt\u00b7schwer", "ein", "erns\u00b7ter", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und zwischen Weinen, zwischen Lachen", "tokens": ["Und", "zwi\u00b7schen", "Wei\u00b7nen", ",", "zwi\u00b7schen", "La\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wehmut endlich m\u00e4chtig siegt:", "tokens": ["Die", "Weh\u00b7mut", "end\u00b7lich", "m\u00e4ch\u00b7tig", "siegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es l\u00e4\u00dft sich nicht zum Spa\u00dfe machen,", "tokens": ["Es", "l\u00e4\u00dft", "sich", "nicht", "zum", "Spa\u00b7\u00dfe", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Worin ein ganzes Leben liegt.", "tokens": ["Wo\u00b7rin", "ein", "gan\u00b7zes", "Le\u00b7ben", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sind einst nicht hier auch sie getreten", "tokens": ["Sind", "einst", "nicht", "hier", "auch", "sie", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Jugendkraft und Freudigkeit,", "tokens": ["In", "Ju\u00b7gend\u00b7kraft", "und", "Freu\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die jetzt f\u00fcr mich im Himmel beten", "tokens": ["Die", "jetzt", "f\u00fcr", "mich", "im", "Him\u00b7mel", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcber Erdenlust und Leid?", "tokens": ["Hoch", "\u00fc\u00b7ber", "Er\u00b7den\u00b7lust", "und", "Leid", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Habt ihr mich hier nicht eingesegnet", "tokens": ["Habt", "ihr", "mich", "hier", "nicht", "ein\u00b7ge\u00b7seg\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcrs Leben, Eltern fromm und treu,", "tokens": ["F\u00fcrs", "Le\u00b7ben", ",", "El\u00b7tern", "fromm", "und", "treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Lieb' auf mich herabgeregnet,", "tokens": ["Und", "Lieb'", "auf", "mich", "her\u00b7ab\u00b7ge\u00b7reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie's Bl\u00fcten regnet in dem Mai?", "tokens": ["Wie's", "Bl\u00fc\u00b7ten", "reg\u00b7net", "in", "dem", "Mai", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was ward aus euren frommen Sorgen?", "tokens": ["Was", "ward", "aus", "eu\u00b7ren", "from\u00b7men", "Sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was trug die treue Liebe ein?", "tokens": ["Was", "trug", "die", "treu\u00b7e", "Lie\u00b7be", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reicht wohl an jenen sch\u00f6nen Morgen", "tokens": ["Reicht", "wohl", "an", "je\u00b7nen", "sch\u00f6\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebens voller Mittagschein?", "tokens": ["Des", "Le\u00b7bens", "vol\u00b7ler", "Mit\u00b7tag\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "M\u00f6gt ihr von euren lichten H\u00f6hen,", "tokens": ["M\u00f6gt", "ihr", "von", "eu\u00b7ren", "lich\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo nichts mehr zwischen Schatten schwebt,", "tokens": ["Wo", "nichts", "mehr", "zwi\u00b7schen", "Schat\u00b7ten", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch auf den Wandrer niedersehen,", "tokens": ["Noch", "auf", "den", "Wand\u00b7rer", "nie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der unten hei\u00df im Staube strebt?", "tokens": ["Der", "un\u00b7ten", "hei\u00df", "im", "Stau\u00b7be", "strebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie kommt er aus der weiten Ferne", "tokens": ["Wie", "kommt", "er", "aus", "der", "wei\u00b7ten", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seiner Kindheit Feld zur\u00fcck?", "tokens": ["Auf", "sei\u00b7ner", "Kind\u00b7heit", "Feld", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schaut noch zum Spiegel sel'ger Sterne", "tokens": ["Schaut", "noch", "zum", "Spie\u00b7gel", "sel'\u00b7ger", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein heitrer Spiegel auf sein Blick?", "tokens": ["Ein", "hei\u00b7trer", "Spie\u00b7gel", "auf", "sein", "Blick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und spielt er noch mit reinen H\u00e4nden", "tokens": ["Und", "spielt", "er", "noch", "mit", "rei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das s\u00fc\u00dfe Kinderblumenspiel?", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Kin\u00b7der\u00b7blu\u00b7men\u00b7spiel", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach! Abw\u00e4rts mu\u00df er sich hier wenden \u2013", "tokens": ["Ach", "!", "Ab\u00b7w\u00e4rts", "mu\u00df", "er", "sich", "hier", "wen\u00b7den", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", 