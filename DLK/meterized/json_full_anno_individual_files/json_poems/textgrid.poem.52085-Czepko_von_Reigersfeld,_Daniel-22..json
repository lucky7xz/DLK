{"textgrid.poem.52085": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sol der Krieg bestehn, wenn ieder Tag und Nacht,", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", ",", "wenn", "ie\u00b7der", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$,", "KOUS", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff nichts, als G\u00fctter ist, auff nichts, als Geld bedacht:", "tokens": ["Auff", "nichts", ",", "als", "G\u00fct\u00b7ter", "ist", ",", "auff", "nichts", ",", "als", "Geld", "be\u00b7dacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "NN", "VAFIN", "$,", "APPR", "PIS", "$,", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man die V\u00f6lcker f\u00fchrt nur armes Volck zu qu\u00e4len,", "tokens": ["Wo", "man", "die", "V\u00f6l\u00b7cker", "f\u00fchrt", "nur", "ar\u00b7mes", "Volck", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur auff und ab zu ziehn, nur Pferd und K\u00fch' zu stehlen.", "tokens": ["Nur", "auff", "und", "ab", "zu", "ziehn", ",", "nur", "Pferd", "und", "K\u00fch'", "zu", "steh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KON", "PTKVZ", "PTKZU", "VVINF", "$,", "ADV", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sol der Krieg bestehn? Wann man zu Felde zeucht,", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", "?", "Wann", "man", "zu", "Fel\u00b7de", "zeucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$.", "PWAV", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eh' ein Schu\u00df geschicht, frisch vor dem Feinde fleucht:", "tokens": ["Und", "eh'", "ein", "Schu\u00df", "ge\u00b7schicht", ",", "frisch", "vor", "dem", "Fein\u00b7de", "fleucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man, ob gleich kein Stall, kein Bansam zu verschlie\u00dfen,", "tokens": ["Wo", "man", ",", "ob", "gleich", "kein", "Stall", ",", "kein", "Ban\u00b7sam", "zu", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "KOUS", "ADV", "PIAT", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der zu entlegen sey nichts wil vom Feinde wi\u00dfen.", "tokens": ["Der", "zu", "ent\u00b7le\u00b7gen", "sey", "nichts", "wil", "vom", "Fein\u00b7de", "wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "VAFIN", "PIS", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie sol der Krieg bestehn? wann man Gewehr und Schwerd", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", "?", "wann", "man", "Ge\u00b7wehr", "und", "Schwerd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$.", "PWAV", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff Unterthanen nur und nicht auff Feinde kehrt:", "tokens": ["Auff", "Un\u00b7ter\u00b7tha\u00b7nen", "nur", "und", "nicht", "auff", "Fein\u00b7de", "kehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man gefangen nihmt, was da verlohrn das Leben,", "tokens": ["Wo", "man", "ge\u00b7fan\u00b7gen", "nihmt", ",", "was", "da", "ver\u00b7lohrn", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und diese t\u00f6dten wil, die alles hergegeben.", "tokens": ["Und", "die\u00b7se", "t\u00f6d\u00b7ten", "wil", ",", "die", "al\u00b7les", "her\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "VMFIN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie sol der Krieg bestehn? wann selbst ein Feldherr nicht,", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", "?", "wann", "selbst", "ein", "Feld\u00b7herr", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$.", "PWAV", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Obersten darff traun, Sie ihrer Knechte Pflicht:", "tokens": ["Dem", "O\u00b7bers\u00b7ten", "darff", "traun", ",", "Sie", "ih\u00b7rer", "Knech\u00b7te", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "VVINF", "$,", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wo auff den Sammel Platz gesetzte Trouppen r\u00fccken,", "tokens": ["Wo", "auff", "den", "Sam\u00b7mel", "Platz", "ge\u00b7setz\u00b7te", "Troup\u00b7pen", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die (doch auff Beuthen nicht) sie drauff nach Beuten schicken.", "tokens": ["Die", "(", "doch", "auff", "Beu\u00b7then", "nicht", ")", "sie", "drauff", "nach", "Beu\u00b7ten", "schi\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "APPR", "NN", "PTKNEG", "$(", "PPER", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie sol der Krieg bestehn? wann man, was ihn ern\u00e4hrt,", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", "?", "wann", "man", ",", "was", "ihn", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$.", "PWAV", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihn erh\u00e4lt und f\u00fchrt, verurscht, verterbt, verheert:", "tokens": ["Was", "ihn", "er\u00b7h\u00e4lt", "und", "f\u00fchrt", ",", "ver\u00b7urscht", ",", "ver\u00b7terbt", ",", "ver\u00b7heert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man hergegen kan ins Feindes L\u00e4ger schauen,", "tokens": ["Wo", "man", "her\u00b7ge\u00b7gen", "kan", "ins", "Fein\u00b7des", "L\u00e4\u00b7ger", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie iedes mu\u00df das Land zu seinem Frommen bauen.", "tokens": ["Wie", "ie\u00b7des", "mu\u00df", "das", "Land", "zu", "sei\u00b7nem", "From\u00b7men", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie sol der Krieg bestehn? wann Haus und Magen leer,", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", "?", "wann", "Haus", "und", "Ma\u00b7gen", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$.", "PWAV", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nichts mehr \u00fcbrig bleibt als Hunger und Beschwer:", "tokens": ["Und", "nichts", "mehr", "\u00fcb\u00b7rig", "bleibt", "als", "Hun\u00b7ger", "und", "Be\u00b7schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADJD", "VVFIN", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man viel Meilen mu\u00df um unsre Gegend reisen,", "tokens": ["Wo", "man", "viel", "Mei\u00b7len", "mu\u00df", "um", "uns\u00b7re", "Ge\u00b7gend", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh' als man einen Pflug kan Wandersleuten weisen.", "tokens": ["Eh'", "als", "man", "ei\u00b7nen", "Pflug", "kan", "Wan\u00b7ders\u00b7leu\u00b7ten", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PIS", "ART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie sol der Krieg bestehn? wann nicht mehr Hertz und Brust", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", "?", "wann", "nicht", "mehr", "Hertz", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$.", "PWAV", "PTKNEG", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein deutscher Eyfer brennt, zu sehn des Feindes Post:", "tokens": ["Ein", "deut\u00b7scher", "Ey\u00b7fer", "brennt", ",", "zu", "sehn", "des", "Fein\u00b7des", "Post", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo mehr kein Vorsatz ist den Frieden einzuf\u00fchren:", "tokens": ["Wo", "mehr", "kein", "Vor\u00b7satz", "ist", "den", "Frie\u00b7den", "ein\u00b7zu\u00b7f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VAFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und H\u00e4nde Palmen Zweig und Hertzen Drommeln r\u00fchren.", "tokens": ["Und", "H\u00e4n\u00b7de", "Pal\u00b7men", "Zweig", "und", "Hert\u00b7zen", "Drom\u00b7meln", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "NN", "KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ihr Herren, wo der Krieg sol lange so bestehn,", "tokens": ["Ihr", "Her\u00b7ren", ",", "wo", "der", "Krieg", "sol", "lan\u00b7ge", "so", "be\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werdet ihr mit uns, bef\u00fcrcht ich, untergehn:", "tokens": ["So", "wer\u00b7det", "ihr", "mit", "uns", ",", "be\u00b7f\u00fcrcht", "ich", ",", "un\u00b7ter\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "PPER", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann Herren ohne Land und L\u00e4nder ohne Leute,", "tokens": ["Dann", "Her\u00b7ren", "oh\u00b7ne", "Land", "und", "L\u00e4n\u00b7der", "oh\u00b7ne", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Leut' ohn Haus und Hoff sind solcher Kriege Beute.", "tokens": ["Und", "Leut'", "ohn", "Haus", "und", "Hoff", "sind", "sol\u00b7cher", "Krie\u00b7ge", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "VVFIN", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie sol der Krieg bestehn, wenn ieder Tag und Nacht,", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", ",", "wenn", "ie\u00b7der", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$,", "KOUS", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff nichts, als G\u00fctter ist, auff nichts, als Geld bedacht:", "tokens": ["Auff", "nichts", ",", "als", "G\u00fct\u00b7ter", "ist", ",", "auff", "nichts", ",", "als", "Geld", "be\u00b7dacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "NN", "VAFIN", "$,", "APPR", "PIS", "$,", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man die V\u00f6lcker f\u00fchrt nur armes Volck zu qu\u00e4len,", "tokens": ["Wo", "man", "die", "V\u00f6l\u00b7cker", "f\u00fchrt", "nur", "ar\u00b7mes", "Volck", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur auff und ab zu ziehn, nur Pferd und K\u00fch' zu stehlen.", "tokens": ["Nur", "auff", "und", "ab", "zu", "ziehn", ",", "nur", "Pferd", "und", "K\u00fch'", "zu", "steh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KON", "PTKVZ", "PTKZU", "VVINF", "$,", "ADV", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wie sol der Krieg bestehn? Wann man zu Felde zeucht,", "tokens": ["Wie", "sol", "der", "Krieg", "be\u00b7stehn", "?", "Wann", "man", "zu", "Fel\u00b7de", "zeucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "VVINF", "$.", "PWAV", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eh' ein Schu\u00df geschicht, frisch vor dem Feinde fleucht:", "tokens": ["Und", "eh'", "ein", "Schu\u00df", "ge\u00b7schicht", ",", "frisch", "vor", "dem", "Fein\u00b7de", "fleucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,", "ADJD", "APPR", "ART", "NN", "VVFI