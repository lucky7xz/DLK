{"textgrid.poem.44083": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Last mich doch nur in der Still", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Last mich doch nur in der Still", "tokens": ["Last", "mich", "doch", "nur", "in", "der", "Still"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ohne Licht und Zeugen weinen,", "tokens": ["Oh\u00b7ne", "Licht", "und", "Zeu\u00b7gen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil der Himmel gar nicht will,", "tokens": ["Weil", "der", "Him\u00b7mel", "gar", "nicht", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mir be\u00dfre Tage scheinen;", "tokens": ["Da\u00df", "mir", "be\u00df\u00b7re", "Ta\u00b7ge", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Bek\u00fcmmern\u00fc\u00df der Brust", "tokens": ["Das", "Be\u00b7k\u00fcm\u00b7mer\u00b7n\u00fc\u00df", "der", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird durch Mitleid nicht zur Lust.", "tokens": ["Wird", "durch", "Mit\u00b7leid", "nicht", "zur", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meines Lebens schwerer Lauf", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "schwe\u00b7rer", "Lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist vorwahr so kurz als b\u00f6se;", "tokens": ["Ist", "vor\u00b7wahr", "so", "kurz", "als", "b\u00f6\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh ich gleich mit Sehnsucht auf,", "tokens": ["Seh", "ich", "gleich", "mit", "Sehn\u00b7sucht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob und wer mich bald erl\u00f6se,", "tokens": ["Ob", "und", "wer", "mich", "bald", "er\u00b7l\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh ich gleichwohl allemahl", "tokens": ["Seh", "ich", "gleich\u00b7wohl", "al\u00b7le\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor den Stern den Donnerstrahl.", "tokens": ["Vor", "den", "Stern", "den", "Don\u00b7ner\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht verzweifeln ist ein Werck", "tokens": ["Nicht", "ver\u00b7zwei\u00b7feln", "ist", "ein", "Werck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Derer, die noch m\u00e4\u00dfig tragen.", "tokens": ["De\u00b7rer", ",", "die", "noch", "m\u00e4\u00b7\u00dfig", "tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier ein Abgrund, dort ein Berg,", "tokens": ["Hier", "ein", "Ab\u00b7grund", ",", "dort", "ein", "Berg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Abends Jammer, morgens Klagen,", "tokens": ["A\u00b7bends", "Jam\u00b7mer", ",", "mor\u00b7gens", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Also wechselt bis ins Grab", "tokens": ["Al\u00b7so", "wech\u00b7selt", "bis", "ins", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Elend stets mit Elend ab.", "tokens": ["E\u00b7lend", "stets", "mit", "E\u00b7lend", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seufzer sind mein Zeitvertreib,", "tokens": ["Seuf\u00b7zer", "sind", "mein", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Brodt und Trunck mischt Asch und Thr\u00e4nen;", "tokens": ["Brodt", "und", "Trunck", "mischt", "Asch", "und", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Creuz und Schwachheit biegt den Leib,", "tokens": ["Creuz", "und", "Schwach\u00b7heit", "biegt", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Seele lechst mit Sehnen", "tokens": ["Und", "die", "See\u00b7le", "lechst", "mit", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein matt und durstig Reh", "tokens": ["Wie", "ein", "matt", "und", "durs\u00b7tig", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "KON", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der H\u00fclf aus Salems H\u00f6h.", "tokens": ["Nach", "der", "H\u00fclf", "aus", "Sa\u00b7lems", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Freunde weichen wie das Laub,", "tokens": ["Freun\u00b7de", "wei\u00b7chen", "wie", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches Wind und Herbst verjagen;", "tokens": ["Wel\u00b7ches", "Wind", "und", "Herbst", "ver\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feinde treten mich in Staub,", "tokens": ["Fein\u00b7de", "tre\u00b7ten", "mich", "in", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neider spotten meiner Klagen,", "tokens": ["Nei\u00b7der", "spot\u00b7ten", "mei\u00b7ner", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles lacht und flieht von mir,", "tokens": ["Al\u00b7les", "lacht", "und", "flieht", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur die Unruh bleibet hier.", "tokens": ["Nur", "die", "Un\u00b7ruh", "blei\u00b7bet", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach wie schrey ich, ach wie viel", "tokens": ["Ach", "wie", "schrey", "ich", ",", "ach", "wie", "viel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJD", "PPER", "$,", "ADV", "KOKOM", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden mir der langen N\u00e4chte!", "tokens": ["Wer\u00b7den", "mir", "der", "lan\u00b7gen", "N\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht die Hofnung gar kein Ziel,", "tokens": ["Sieht", "die", "Hof\u00b7nung", "gar", "kein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie sich erholen m\u00f6chte?", "tokens": ["Da\u00df", "sie", "sich", "er\u00b7ho\u00b7len", "m\u00f6ch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll, o Gott, denn meine Pein", "tokens": ["Soll", ",", "o", "Gott", ",", "denn", "mei\u00b7ne", "Pein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$,", "FM", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie dein Eifer ewig seyn?", "tokens": ["Wie", "dein", "Ei\u00b7fer", "e\u00b7wig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch was \u00fcberf\u00e4llt mein Herz", "tokens": ["Doch", "was", "\u00fc\u00b7berf\u00b7\u00e4llt", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor ein innerlicher Frieden?", "tokens": ["Vor", "ein", "in\u00b7ner\u00b7li\u00b7cher", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wo ist denn schon der Schmerz?", "tokens": ["O", "wo", "ist", "denn", "schon", "der", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich etwan gar verschieden?", "tokens": ["Bin", "ich", "et\u00b7wan", "gar", "ver\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder giebt ein Traumgesicht", "tokens": ["O\u00b7der", "giebt", "ein", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir nur Schatten vor das Licht?", "tokens": ["Mir", "nur", "Schat\u00b7ten", "vor", "das", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Herr, verzeih der Ungedult,", "tokens": ["Herr", ",", "ver\u00b7zeih", "der", "Un\u00b7ge\u00b7dult", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn jezt seh ich deine St\u00e4rcke,", "tokens": ["Denn", "jezt", "seh", "ich", "dei\u00b7ne", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die gro\u00dfe Vaterhuld", "tokens": ["Und", "die", "gro\u00b7\u00dfe", "Va\u00b7ter\u00b7huld"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird an mir zum Wunderwercke", "tokens": ["Wird", "an", "mir", "zum", "Wun\u00b7der\u00b7wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und erquickt mich in der That", "tokens": ["Und", "er\u00b7quickt", "mich", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Thau die welcke Saat.", "tokens": ["Wie", "der", "Thau", "die", "wel\u00b7cke", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "S\u00fcnden, greift mich grausam an,", "tokens": ["S\u00fcn\u00b7den", ",", "greift", "mich", "grau\u00b7sam", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sorgen, kr\u00e4nckt mein schwach Gem\u00fcthe!", "tokens": ["Sor\u00b7gen", ",", "kr\u00e4nckt", "mein", "schwach", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich verbei\u00dfe, was ich kan.", "tokens": ["Ich", "ver\u00b7bei\u00b7\u00dfe", ",", "was", "ich", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feinde raset, Misgunst w\u00fcte!", "tokens": ["Fein\u00b7de", "ra\u00b7set", ",", "Mis\u00b7gunst", "w\u00fc\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, mein Glauben und dein Wort", "tokens": ["Herr", ",", "mein", "Glau\u00b7ben", "und", "dein", "Wort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00e4rckt mich hier und h\u00e4lt mich dort.", "tokens": ["St\u00e4rckt", "mich", "hier", "und", "h\u00e4lt", "mich", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Last mich doch nur in der Still", "tokens": ["Last", "mich", "doch", "nur", "in", "der", "Still"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ohne Licht und Zeugen weinen,", "tokens": ["Oh\u00b7ne", "Licht", "und", "Zeu\u00b7gen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil der Himmel gar nicht will,", "tokens": ["Weil", "der", "Him\u00b7mel", "gar", "nicht", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mir be\u00dfre Tage scheinen;", "tokens": ["Da\u00df", "mir", "be\u00df\u00b7re", "Ta\u00b7ge", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Bek\u00fcmmern\u00fc\u00df der Brust", "tokens": ["Das", "Be\u00b7k\u00fcm\u00b7mer\u00b7n\u00fc\u00df", "der", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird durch Mitleid nicht zur Lust.", "tokens": ["Wird", "durch", "Mit\u00b7leid", "nicht", "zur", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Meines Lebens schwerer Lauf", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "schwe\u00b7rer", "Lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist vorwahr so kurz als b\u00f6se;", "tokens": ["Ist", "vor\u00b7wahr", "so", "kurz", "als", "b\u00f6\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh ich gleich mit Sehnsucht auf,", "tokens": ["Seh", "ich", "gleich", "mit", "Sehn\u00b7sucht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob und wer mich bald erl\u00f6se,", "tokens": ["Ob", "und", "wer", "mich", "bald", "er\u00b7l\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh ich gleichwohl allemahl", "tokens": ["Seh", "ich", "gleich\u00b7wohl", "al\u00b7le\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor den Stern den Donnerstrahl.", "tokens": ["Vor", "den", "Stern", "den", "Don\u00b7ner\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nicht verzweifeln ist ein Werck", "tokens": ["Nicht", "ver\u00b7zwei\u00b7feln", "ist", "ein", "Werck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Derer, die noch m\u00e4\u00dfig tragen.", "tokens": ["De\u00b7rer", ",", "die", "noch", "m\u00e4\u00b7\u00dfig", "tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier ein Abgrund, dort ein Berg,", "tokens": ["Hier", "ein", "Ab\u00b7grund", ",", "dort", "ein", "Berg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Abends Jammer, morgens Klagen,", "tokens": ["A\u00b7bends", "Jam\u00b7mer", ",", "mor\u00b7gens", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Also wechselt bis ins Grab", "tokens": ["Al\u00b7so", "wech\u00b7selt", "bis", "ins", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Elend stets mit Elend ab.", "tokens": ["E\u00b7lend", "stets", "mit", "E\u00b7lend", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Seufzer sind mein Zeitvertreib,", "tokens": ["Seuf\u00b7zer", "sind", "mein", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Brodt und Trunck mischt Asch und Thr\u00e4nen;", "tokens": ["Brodt", "und", "Trunck", "mischt", "Asch", "und", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Creuz und Schwachheit biegt den Leib,", "tokens": ["Creuz", "und", "Schwach\u00b7heit", "biegt", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Seele lechst mit Sehnen", "tokens": ["Und", "die", "See\u00b7le", "lechst", "mit", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein matt und durstig Reh", "tokens": ["Wie", "ein", "matt", "und", "durs\u00b7tig", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "KON", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach der H\u00fclf aus Salems H\u00f6h.", "tokens": ["Nach", "der", "H\u00fclf", "aus", "Sa\u00b7lems", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Freunde weichen wie das Laub,", "tokens": ["Freun\u00b7de", "wei\u00b7chen", "wie", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches Wind und Herbst verjagen;", "tokens": ["Wel\u00b7ches", "Wind", "und", "Herbst", "ver\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feinde treten mich in Staub,", "tokens": ["Fein\u00b7de", "tre\u00b7ten", "mich", "in", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neider spotten meiner Klagen,", "tokens": ["Nei\u00b7der", "spot\u00b7ten", "mei\u00b7ner", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles lacht und flieht von mir,", "tokens": ["Al\u00b7les", "lacht", "und", "flieht", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur die Unruh bleibet hier.", "tokens": ["Nur", "die", "Un\u00b7ruh", "blei\u00b7bet", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ach wie schrey ich, ach wie viel", "tokens": ["Ach", "wie", "schrey", "ich", ",", "ach", "wie", "viel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJD", "PPER", "$,", "ADV", "KOKOM", "PIAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden mir der langen N\u00e4chte!", "tokens": ["Wer\u00b7den", "mir", "der", "lan\u00b7gen", "N\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht die Hofnung gar kein Ziel,", "tokens": ["Sieht", "die", "Hof\u00b7nung", "gar", "kein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie sich erholen m\u00f6chte?", "tokens": ["Da\u00df", "sie", "sich", "er\u00b7ho\u00b7len", "m\u00f6ch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll, o Gott, denn meine Pein", "tokens": ["Soll", ",", "o", "Gott", ",", "denn", "mei\u00b7ne", "Pein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "$,", "FM", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie dein Eifer ewig seyn?", "tokens": ["Wie", "dein", "Ei\u00b7fer", "e\u00b7wig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Doch was \u00fcberf\u00e4llt mein Herz", "tokens": ["Doch", "was", "\u00fc\u00b7berf\u00b7\u00e4llt", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor ein innerlicher Frieden?", "tokens": ["Vor", "ein", "in\u00b7ner\u00b7li\u00b7cher", "Frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wo ist denn schon der Schmerz?", "tokens": ["O", "wo", "ist", "denn", "schon", "der", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich etwan gar verschieden?", "tokens": ["Bin", "ich", "et\u00b7wan", "gar", "ver\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder giebt ein Traumgesicht", "tokens": ["O\u00b7der", "giebt", "ein", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir nur Schatten vor das Licht?", "tokens": ["Mir", "nur", "Schat\u00b7ten", "vor", "das", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Herr, verzeih der Ungedult,", "tokens": ["Herr", ",", "ver\u00b7zeih", "der", "Un\u00b7ge\u00b7dult", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn jezt seh ich deine St\u00e4rcke,", "tokens": ["Denn", "jezt", "seh", "ich", "dei\u00b7ne", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die gro\u00dfe Vaterhuld", "tokens": ["Und", "die", "gro\u00b7\u00dfe", "Va\u00b7ter\u00b7huld"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird an mir zum Wunderwercke", "tokens": ["Wird", "an", "mir", "zum", "Wun\u00b7der\u00b7wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und erquickt mich in der That", "tokens": ["Und", "er\u00b7quickt", "mich", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Thau die welcke Saat.", "tokens": ["Wie", "der", "Thau", "die", "wel\u00b7cke", "Saat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "S\u00fcnden, greift mich grausam an,", "tokens": ["S\u00fcn\u00b7den", ",", "greift", "mich", "grau\u00b7sam", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sorgen, kr\u00e4nckt mein schwach Gem\u00fcthe!", "tokens": ["Sor\u00b7gen", ",", "kr\u00e4nckt", "mein", "schwach", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich verbei\u00dfe, was ich kan.", "tokens": ["Ich", "ver\u00b7bei\u00b7\u00dfe", ",", "was", "ich", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Feinde raset, Misgunst w\u00fcte!", "tokens": ["Fein\u00b7de", "ra\u00b7set", ",", "Mis\u00b7gunst", "w\u00fc\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, mein Glauben und dein Wort", "tokens": ["Herr", ",", "mein", "Glau\u00b7ben", "und", "dein", "Wort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00e4rckt mich hier und h\u00e4lt mich dort.", "tokens": ["St\u00e4rckt", "mich", "hier", "und", "h\u00e4lt", "mich", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}