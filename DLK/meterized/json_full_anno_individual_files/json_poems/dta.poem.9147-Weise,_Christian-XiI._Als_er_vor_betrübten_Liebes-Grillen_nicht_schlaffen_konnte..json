{"dta.poem.9147": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n Als er vor betr\u00fcbten Liebes-Grillen nicht  \n schlaffen konnte.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Jtzt ruht und schl\u00e4fft die gantze welt", "tokens": ["Jtzt", "ruht", "und", "schl\u00e4fft", "die", "gant\u00b7ze", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was see und feld", "tokens": ["Was", "see", "und", "feld"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In den verliebten armen h\u00e4lt.", "tokens": ["In", "den", "ver\u00b7lieb\u00b7ten", "ar\u00b7men", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ich empfinde keine ruh/", "tokens": ["Nur", "ich", "emp\u00b7fin\u00b7de", "kei\u00b7ne", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und bringe nicht ein auge zu.", "tokens": ["Und", "brin\u00b7ge", "nicht", "ein", "au\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann weil der tag zu ende geht/", "tokens": ["Dann", "weil", "der", "tag", "zu", "en\u00b7de", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eh meine sonne vor mir steht/", "tokens": ["Eh", "mei\u00b7ne", "son\u00b7ne", "vor", "mir", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So bricht der abend auch herein/", "tokens": ["So", "bricht", "der", "a\u00b7bend", "auch", "her\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und mu\u00df gedoppelt finster seyn.", "tokens": ["Und", "mu\u00df", "ge\u00b7dop\u00b7pelt", "fins\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich habe lange nacht", "tokens": ["Ich", "ha\u00b7be", "lan\u00b7ge", "nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Umsonst gewacht/", "tokens": ["Um\u00b7sonst", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und meinem schmertzen nachgedacht/", "tokens": ["Und", "mei\u00b7nem", "schmert\u00b7zen", "nach\u00b7ge\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wiewol je mehr ich dencken mu\u00df/", "tokens": ["Wie\u00b7wol", "je", "mehr", "ich", "den\u00b7cken", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Je mehr empfind ich \u00fcberdru\u00df/", "tokens": ["Je", "mehr", "emp\u00b7find", "ich", "\u00fc\u00b7berd\u00b7ru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil niemand die erw\u00fcnschte bahn", "tokens": ["Weil", "nie\u00b7mand", "die", "er\u00b7w\u00fcnschte", "bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Zu meiner hoffnung finden kan/", "tokens": ["Zu", "mei\u00b7ner", "hoff\u00b7nung", "fin\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dannoch hab ich diesen trieb", "tokens": ["Und", "dan\u00b7noch", "hab", "ich", "die\u00b7sen", "trieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der traurigen gedancken lieb.", "tokens": ["Der", "trau\u00b7ri\u00b7gen", "ge\u00b7dan\u00b7cken", "lieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ich bin verliebt in meine pein/", "tokens": ["Ich", "bin", "ver\u00b7liebt", "in", "mei\u00b7ne", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nicht allein", "tokens": ["Und", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In ihren hochgesch\u00e4tzten schein:", "tokens": ["In", "ih\u00b7ren", "hoch\u00b7ge\u00b7sch\u00e4tz\u00b7ten", "schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn wann der schlaff ein sanfftes ziel", "tokens": ["Denn", "wann", "der", "schlaff", "ein", "sanff\u00b7tes", "ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In meinen sorgen setzen will/", "tokens": ["In", "mei\u00b7nen", "sor\u00b7gen", "set\u00b7zen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So stellt sich meine mattigkeit", "tokens": ["So", "stellt", "sich", "mei\u00b7ne", "mat\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch immer in den gegenstreit/", "tokens": ["Noch", "im\u00b7mer", "in", "den", "ge\u00b7gen\u00b7streit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bi\u00df sich ein traum ins hertze spielt/", "tokens": ["Bi\u00df", "sich", "ein", "traum", "ins", "hert\u00b7ze", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df er mir die gedancken stielt.", "tokens": ["Da\u00df", "er", "mir", "die", "ge\u00b7dan\u00b7cken", "stielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. In diesem schatten k\u00f6mmet mir", "tokens": ["In", "die\u00b7sem", "schat\u00b7ten", "k\u00f6m\u00b7met", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die liebste zier", "tokens": ["Die", "liebs\u00b7te", "zier"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In ihrer h\u00f6chsten sch\u00f6nheit f\u00fcr/", "tokens": ["In", "ih\u00b7rer", "h\u00f6chs\u00b7ten", "sch\u00f6n\u00b7heit", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stralt mein hertz dermassen an/", "tokens": ["Und", "stralt", "mein", "hertz", "der\u00b7mas\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PDS", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich es kaum vertragen kan;", "tokens": ["Da\u00df", "ich", "es", "kaum", "ver\u00b7tra\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jedoch wann ich ein gleiches spiel", "tokens": ["Je\u00b7doch", "wann", "ich", "ein", "glei\u00b7ches", "spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit meinen armen machen will/", "tokens": ["Mit", "mei\u00b7nen", "ar\u00b7men", "ma\u00b7chen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So greiff ich an die kalte wand/", "tokens": ["So", "greiff", "ich", "an", "die", "kal\u00b7te", "wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und sie entwischt mir aus der hand.", "tokens": ["Und", "sie", "ent\u00b7wischt", "mir", "aus", "der", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Jtzt sehn ich mich nach meiner ruh/", "tokens": ["Jtzt", "sehn", "ich", "mich", "nach", "mei\u00b7ner", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schliesse nu", "tokens": ["Und", "schlies\u00b7se", "nu"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die augen vor mir selber zu/", "tokens": ["Die", "au\u00b7gen", "vor", "mir", "sel\u00b7ber", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Komm wieder/ komm du s\u00fcsser traum/", "tokens": ["Komm", "wie\u00b7der", "/", "komm", "du", "s\u00fcs\u00b7ser", "traum", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mache meiner wollust raum!", "tokens": ["Und", "ma\u00b7che", "mei\u00b7ner", "wol\u00b7lust", "raum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn wird der kummer nicht gestillt", "tokens": ["Denn", "wird", "der", "kum\u00b7mer", "nicht", "ge\u00b7stillt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch ein erdichtes schatten-bild/", "tokens": ["Durch", "ein", "er\u00b7dich\u00b7tes", "schat\u00b7ten\u00b7bild", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So weicht auff eine kleine zeit", "tokens": ["So", "weicht", "auff", "ei\u00b7ne", "klei\u00b7ne", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zum minsten die empfindligkeit.", "tokens": ["Zum", "mins\u00b7ten", "die", "emp\u00b7find\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ach aber ach es geht nicht an!", "tokens": ["Ach", "a\u00b7ber", "ach", "es", "geht", "nicht", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich daran", "tokens": ["Da\u00df", "ich", "da\u00b7ran"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein rechtes labsal haben kan.", "tokens": ["Ein", "rech\u00b7tes", "lab\u00b7sal", "ha\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein schmertzen geht mir viel zu nah/", "tokens": ["Mein", "schmert\u00b7zen", "geht", "mir", "viel", "zu", "nah", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach w\u00e4re nur der morgen da/", "tokens": ["Ach", "w\u00e4\u00b7re", "nur", "der", "mor\u00b7gen", "da", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "ADV", "ART", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vielleicht ist di\u00df der liebe tag", "tokens": ["Viel\u00b7leicht", "ist", "di\u00df", "der", "lie\u00b7be", "tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PDS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da ich mich wieder r\u00fchmen mag/", "tokens": ["Da", "ich", "mich", "wie\u00b7der", "r\u00fch\u00b7men", "mag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df eine die mich sonst betr\u00fcbt/", "tokens": ["Da\u00df", "ei\u00b7ne", "die", "mich", "sonst", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRELS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mir ruh und leben wieder giebt", "tokens": ["Mir", "ruh", "und", "le\u00b7ben", "wie\u00b7der", "giebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}