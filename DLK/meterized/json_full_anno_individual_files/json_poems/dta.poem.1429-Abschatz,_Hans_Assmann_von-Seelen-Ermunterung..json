{"dta.poem.1429": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seelen-Ermunterung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.71", "nl:0.28"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Seel Ewig/ es ist Zeit von hinnen zu gedencken/", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "von", "hin\u00b7nen", "zu", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "APPR", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was klebst du an der Welt verg\u00e4llter Eitelkeit/", "tokens": ["Was", "klebst", "du", "an", "der", "Welt", "ver\u00b7g\u00e4ll\u00b7ter", "Ei\u00b7tel\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr bester Zeitvertreib ist ein Verderb der Zeit/", "tokens": ["Ihr", "bes\u00b7ter", "Zeit\u00b7ver\u00b7treib", "ist", "ein", "Ver\u00b7derb", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie kan dir nichts/ als Reu und Gall/ auff Honig schencken.", "tokens": ["Sie", "kan", "dir", "nichts", "/", "als", "Reu", "und", "Gall", "/", "auff", "Ho\u00b7nig", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "$(", "KOUS", "NE", "KON", "NE", "$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seel Ewig es ist Zeit von hinnen zu gedencken.", "tokens": ["Seel", "E\u00b7wig", "es", "ist", "Zeit", "von", "hin\u00b7nen", "zu", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VAFIN", "NN", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Seel Ewig/ es ist Zeit die Erde zu verachten!", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "die", "Er\u00b7de", "zu", "ver\u00b7ach\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "ART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was n\u00fctzet dir bey GOtt der Erden-G\u00f6tter Gunst?", "tokens": ["Was", "n\u00fct\u00b7zet", "dir", "bey", "Gott", "der", "Er\u00b7den\u00b7G\u00f6t\u00b7ter", "Gunst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Hoheit dieser Welt ist Schatten/ Rauch und Dunst:", "tokens": ["Die", "Ho\u00b7heit", "die\u00b7ser", "Welt", "ist", "Schat\u00b7ten", "/", "Rauch", "und", "Dunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie mu\u00df der edle Geist bey Dienst der Ehr-Sucht schmach-", "tokens": ["Wie", "mu\u00df", "der", "ed\u00b7le", "Geist", "bey", "Dienst", "der", "Ehr\u00b7Sucht", "schmach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "APPR", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seel Ewig/ es ist Zeit die Erde zu verachten!", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "die", "Er\u00b7de", "zu", "ver\u00b7ach\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "ART", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Seel Ewig/ es ist Zeit/ was fl\u00fcchtig ist/ zu fliehen/", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "/", "was", "fl\u00fcch\u00b7tig", "ist", "/", "zu", "flie\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "$(", "PWS", "ADJD", "VAFIN", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie dein verg\u00e4nglich Gutt/ so w\u00e4chst der S\u00fcnden Zahl/", "tokens": ["Wie", "dein", "ver\u00b7g\u00e4ng\u00b7lich", "Gutt", "/", "so", "w\u00e4chst", "der", "S\u00fcn\u00b7den", "Zahl", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJD", "ADJD", "$(", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein eigner Uberflu\u00df versalzt dir offt die Wahl", "tokens": ["Dein", "eig\u00b7ner", "U\u00b7ber\u00b7flu\u00df", "ver\u00b7salzt", "dir", "offt", "die", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Sorge/ f\u00fcr Verlust und Lust noch mehr zu ziehen/", "tokens": ["Mit", "Sor\u00b7ge", "/", "f\u00fcr", "Ver\u00b7lust", "und", "Lust", "noch", "mehr", "zu", "zie\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "NN", "KON", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seel Ewig/ es ist Zeit/ was fl\u00fcchtig ist/ zu fliehen.", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "/", "was", "fl\u00fcch\u00b7tig", "ist", "/", "zu", "flie\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "$(", "PWS", "ADJD", "VAFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Seel Ewig/ es ist Zeit von hinnen zu gedencken/", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "von", "hin\u00b7nen", "zu", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "APPR", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott mahnet dich hierzu durch manch Erinnern an/", "tokens": ["Gott", "mah\u00b7net", "dich", "hier\u00b7zu", "durch", "manch", "E\u00b7rin\u00b7nern", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PAV", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du siehest/ h\u00f6rst und f\u00fchlst was dich bewegen kan/", "tokens": ["Du", "sie\u00b7hest", "/", "h\u00f6rst", "und", "f\u00fchlst", "was", "dich", "be\u00b7we\u00b7gen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "PWS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey Zeiten Hertz und Fu\u00df vom Eitlen abzulencken/", "tokens": ["Bey", "Zei\u00b7ten", "Hertz", "und", "Fu\u00df", "vom", "Eit\u00b7len", "ab\u00b7zu\u00b7len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "APPRART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seel Ewig/ es ist Zeit von hinnen zu gedencken.", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "von", "hin\u00b7nen", "zu", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Seel Ewig/ es ist Zeit von hinnen zu gedencken/", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "von", "hin\u00b7nen", "zu", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "APPR", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Jahre gehen hin/ die Kr\u00e4ffte lassen nach/", "tokens": ["Die", "Jah\u00b7re", "ge\u00b7hen", "hin", "/", "die", "Kr\u00e4ff\u00b7te", "las\u00b7sen", "nach", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie bald wohl k\u00f6mmt der Tag/ der dich ins Schlaff-Gemach", "tokens": ["Wie", "bald", "wohl", "k\u00f6mmt", "der", "Tag", "/", "der", "dich", "ins", "Schlaff\u00b7Ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "ART", "NN", "$(", "PRELS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der k\u00fchlen Erde (GOtt gieb selig!) soll versencken/", "tokens": ["Der", "k\u00fch\u00b7len", "Er\u00b7de", "(", "Gott", "gieb", "se\u00b7lig", "!", ")", "soll", "ver\u00b7sen\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "VVFIN", "ADJD", "$.", "$(", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seel Ewig/ es ist Zeit an Himmel zu gedencken.", "tokens": ["Seel", "E\u00b7wig", "/", "es", "ist", "Zeit", "an", "Him\u00b7mel", "zu", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PPER", "VAFIN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}