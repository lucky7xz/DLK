en;", "tokens": ["Dich", ",", "Va\u00b7ter", ",", "las\u00b7sen", "wal\u00b7ten", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Dein zarter Wink uns stets regier',", "tokens": ["Dein", "zar\u00b7ter", "Wink", "uns", "stets", "re\u00b7gier'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ohn' S\u00e4en und ohn' Sorgen,", "tokens": ["Ohn'", "S\u00e4\u00b7en", "und", "ohn'", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Ohn' gestern und ohn' morgen,", "tokens": ["Ohn'", "ge\u00b7stern", "und", "ohn'", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADV", "ADV", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "Zu sterben uns, zu feiern dir!", "tokens": ["Zu", "ster\u00b7ben", "uns", ",", "zu", "fei\u00b7ern", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "$,", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In diesem Jubeljahre", "tokens": ["In", "die\u00b7sem", "Ju\u00b7bel\u00b7jah\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verleihe Gott das Wahre", "tokens": ["Ver\u00b7lei\u00b7he", "Gott", "das", "Wah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "l