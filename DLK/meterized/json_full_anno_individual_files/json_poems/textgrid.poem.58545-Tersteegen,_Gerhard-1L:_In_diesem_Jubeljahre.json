{"textgrid.poem.58545": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diesem Jubeljahre", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem Jubeljahre", "tokens": ["In", "die\u00b7sem", "Ju\u00b7bel\u00b7jah\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verleihe Gott das Wahre", "tokens": ["Ver\u00b7lei\u00b7he", "Gott", "das", "Wah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mach' ein End' der Sklaverei!", "tokens": ["Und", "mach'", "ein", "End'", "der", "Skla\u00b7ve\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Herren, unserm rechten,", "tokens": ["Dem", "Her\u00b7ren", ",", "un\u00b7serm", "rech\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu ewig will'gen Knechten", "tokens": ["Zu", "e\u00b7wig", "will'\u00b7gen", "Knech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Ohr am Pfost durchbohret sei,", "tokens": ["Das", "Ohr", "am", "Pfost", "durch\u00b7boh\u00b7ret", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Knechten, die daneben", "to