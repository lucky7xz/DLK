{"dta.poem.3589": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Espenzweigelein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt mir ein Espenzweigelein               ", "tokens": ["H\u00e4tt", "mir", "ein", "E\u00b7spen\u00b7zwei\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gebogen zu der Erden;", "tokens": ["Ge\u00b7bo\u00b7gen", "zu", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den liebsten Bulen, den ich hab,", "tokens": ["Den", "liebs\u00b7ten", "Bu\u00b7len", ",", "den", "ich", "hab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ist mir leider allzuferne.", "tokens": ["Der", "ist", "mir", "lei\u00b7der", "all\u00b7zu\u00b7fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er ist mir doch zu ferne nicht,", "tokens": ["Er", "ist", "mir", "doch", "zu", "fer\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei ihm hab ich geschlafen;", "tokens": ["Bei", "ihm", "hab", "ich", "ge\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Von rothem Gold ein Fingerlein", "tokens": ["Von", "ro\u00b7them", "Gold", "ein", "Fin\u00b7ger\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab ich in seinem Bett gelassen.", "tokens": ["Hab", "ich", "in", "sei\u00b7nem", "Bett", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und da ichs da gelassen hab,", "tokens": ["Und", "da", "ichs", "da", "ge\u00b7las\u00b7sen", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ichs auch wieder bekommen;", "tokens": ["Will", "ichs", "auch", "wie\u00b7der", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und thun, als ob ichs bei mir h\u00e4tt,", "tokens": ["Und", "thun", ",", "als", "ob", "ichs", "bei", "mir", "h\u00e4tt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOKOM", "KOUS", "PIS", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4r mir keinmal genommen.", "tokens": ["Und", "w\u00e4r", "mir", "kein\u00b7mal", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja zwischen Berg und tiefe Thal", "tokens": ["Ja", "zwi\u00b7schen", "Berg", "und", "tie\u00b7fe", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da geht ein enge Stra\u00dfe:", "tokens": ["Da", "geht", "ein", "en\u00b7ge", "Stra\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer seinen Buhl nicht haben will,", "tokens": ["Wer", "sei\u00b7nen", "Buhl", "nicht", "ha\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der soll ihn allzeit fahren lassen.", "tokens": ["Der", "soll", "ihn", "all\u00b7zeit", "fah\u00b7ren", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Scheid dich nit Herzensd\u00f6ckelein,", "tokens": ["Scheid", "dich", "nit", "Her\u00b7zens\u00b7d\u00f6\u00b7ckel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von dir will ich nit weichen;", "tokens": ["Von", "dir", "will", "ich", "nit", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hab Andre lieber nit als mich,", "tokens": ["Hab", "And\u00b7re", "lie\u00b7ber", "nit", "als", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "PTKNEG", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Reich findt man nit dein's Gleichen.", "tokens": ["Im", "Reich", "findt", "man", "nit", "dein's", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}