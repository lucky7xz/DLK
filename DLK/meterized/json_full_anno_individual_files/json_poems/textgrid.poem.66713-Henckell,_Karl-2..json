{"textgrid.poem.66713": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und noch ein andres Wort steht mir am Tor.", "tokens": ["Und", "noch", "ein", "and\u00b7res", "Wort", "steht", "mir", "am", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Wort: \u00bb", "tokens": ["Das", "Wort", ":", "\u00bb"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wen je die Schauer seliger Andacht fa\u00dften", "tokens": ["Wen", "je", "die", "Schau\u00b7er", "se\u00b7li\u00b7ger", "An\u00b7dacht", "fa\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In H\u00f6hen, wo sich Schwere fern verlor;", "tokens": ["In", "H\u00f6\u00b7hen", ",", "wo", "sich", "Schwe\u00b7re", "fern", "ver\u00b7lor", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PRF", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer einmal nur gelauscht dem reinern Chor,", "tokens": ["Wer", "ein\u00b7mal", "nur", "ge\u00b7lauscht", "dem", "rei\u00b7nern", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ihn erl\u00f6st von rohen Lebenslasten,", "tokens": ["Der", "ihn", "er\u00b7l\u00f6st", "von", "ro\u00b7hen", "Le\u00b7bens\u00b7las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ihn entr\u00fcckt dem weihelosen Hasten", "tokens": ["Der", "ihn", "ent\u00b7r\u00fcckt", "dem", "wei\u00b7he\u00b7lo\u00b7sen", "Has\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus grauem Dunst in Goldgew\u00f6lk empor;", "tokens": ["Aus", "grau\u00b7em", "Dunst", "in", "Gold\u00b7ge\u00b7w\u00f6lk", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer von der Quelle wei\u00df, die nie versandet,", "tokens": ["Wer", "von", "der", "Quel\u00b7le", "wei\u00df", ",", "die", "nie", "ver\u00b7san\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil ihre Tiefen unergr\u00fcndlich sind;", "tokens": ["Weil", "ih\u00b7re", "Tie\u00b7fen", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer, ob an Jahren Greis, an Seele Kind,", "tokens": ["Wer", ",", "ob", "an", "Jah\u00b7ren", "Greis", ",", "an", "See\u00b7le", "Kind", ","], "token_info": ["wo