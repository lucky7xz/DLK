{"textgrid.poem.65897": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Hades", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Haupt im Hintergrund der hei\u00dfen Hauche", "tokens": ["Das", "Haupt", "im", "Hin\u00b7ter\u00b7grund", "der", "hei\u00b7\u00dfen", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bef\u00e4llt Besinnung an der Hestia Herd:", "tokens": ["Be\u00b7f\u00e4llt", "Be\u00b7sin\u00b7nung", "an", "der", "Hes\u00b7tia", "Herd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 Wer bin ich? \u2013 Gott \u2013 den nie das Opfer ehrt?", "tokens": ["\u2013", "Wer", "bin", "ich", "?", "\u2013", "Gott", "\u2013", "den", "nie", "das", "Op\u00b7fer", "ehrt", "?"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$.", "$(", "NN", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gefangener und Herr im Reich der Rauche!", "tokens": ["Ge\u00b7fan\u00b7ge\u00b7ner", "und", "Herr", "im", "Reich", "der", "Rau\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Mensch! Die gute Glut im Erdenbauche", "tokens": ["O", "Mensch", "!", "Die", "gu\u00b7te", "Glut", "im", "Er\u00b7den\u00b7bau\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bleibt heilig offenbart, weil unversehrt \u2013", "tokens": ["Bleibt", "hei\u00b7lig", "of\u00b7fen\u00b7bart", ",", "weil", "un\u00b7ver\u00b7sehrt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "$,", "KOUS", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin der Wachsame, der Ernten mehrt,", "tokens": ["Ich", "bin", "der", "Wach\u00b7sa\u00b7me", ",", "der", "Ern\u00b7ten", "mehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sogar Geschmack aus Bacchos' Funkel-Schlauche!", "tokens": ["So\u00b7gar", "Ge\u00b7schmack", "aus", "Bac\u00b7chos'", "Fun\u00b7kel\u00b7Schlau\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Blut wird eine Sonnentochter ehen,", "tokens": ["Mein", "Blut", "wird", "ei\u00b7ne", "Son\u00b7nen\u00b7toch\u00b7ter", "e\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit euch, Wo-Wandelnden, sei ich verwandt:", "tokens": ["Mit", "euch", ",", "Wo\u00b7Wan\u00b7deln\u00b7den", ",", "sei", "ich", "ver\u00b7wandt", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Uns mag der Mensch einst unerschrocken sehen!", "tokens": ["Uns", "mag", "der", "Mensch", "einst", "un\u00b7er\u00b7schro\u00b7cken", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr seid an mich, so nahen Gott, gebannt,", "tokens": ["Ihr", "seid", "an", "mich", ",", "so", "na\u00b7hen", "Gott", ",", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "ADV", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun f\u00fchlt es doch \u2013 ich kann euch nachts erwehen:", "tokens": ["Nun", "f\u00fchlt", "es", "doch", "\u2013", "ich", "kann", "euch", "nachts", "er\u00b7we\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Hirne ist, nicht Erde schafft die Wand.", "tokens": ["Im", "Hir\u00b7ne", "ist", ",", "nicht", "Er\u00b7de", "schafft", "die", "Wand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,", "PTKNEG", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Haupt im Hintergrund der hei\u00dfen Hauche", "tokens": ["Das", "Haupt", "im", "Hin\u00b7ter\u00b7grund", "der", "hei\u00b7\u00dfen", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "po