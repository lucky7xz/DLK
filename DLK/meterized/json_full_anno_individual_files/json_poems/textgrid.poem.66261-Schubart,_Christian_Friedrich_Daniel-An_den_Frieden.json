{"textgrid.poem.66261": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An den Frieden", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Friedensg\u00f6ttin, komm, ich flehe", "tokens": ["Frie\u00b7dens\u00b7g\u00f6t\u00b7tin", ",", "komm", ",", "ich", "fle\u00b7he"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir mit hochgehobner Hand,", "tokens": ["Dir", "mit", "hoch\u00b7ge\u00b7hob\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm herab von deiner Himmelsh\u00f6he,", "tokens": ["Komm", "her\u00b7ab", "von", "dei\u00b7ner", "Him\u00b7mels\u00b7h\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich bedarf mein armes Vaterland.", "tokens": ["Dich", "be\u00b7darf", "mein", "ar\u00b7mes", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh im Maienmonde wollen", "tokens": ["Sieh", "im", "Mai\u00b7en\u00b7mon\u00b7de", "wol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heere ziehen in das Feld.", "tokens": ["Hee\u00b7re", "zie\u00b7hen", "in", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie schon die Augen blutig rollen,", "tokens": ["Wie", "sie", "schon", "die", "Au\u00b7gen", "blu\u00b7tig", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zu verheeren eine ganze Welt.", "tokens": ["Zu", "ver\u00b7hee\u00b7ren", "ei\u00b7ne", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Freude flieht vor Mavors Rufe,", "tokens": ["Freu\u00b7de", "flieht", "vor", "Ma\u00b7vors", "Ru\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich schlachtendurstig naht;", "tokens": ["Der", "sich", "schlach\u00b7ten\u00b7durs\u00b7tig", "naht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner kriegerischen Rosse Hufe", "tokens": ["Sei\u00b7ner", "krie\u00b7ge\u00b7ri\u00b7schen", "Ros\u00b7se", "Hu\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stampfen, knicken unsre Fr\u00fchlingssaat.", "tokens": ["Stamp\u00b7fen", ",", "kni\u00b7cken", "uns\u00b7re", "Fr\u00fch\u00b7lings\u00b7saat", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Blumen sterben, wo die Sohle", "tokens": ["Blu\u00b7men", "ster\u00b7ben", ",", "wo", "die", "Soh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines ehrnen Kriegers geht;", "tokens": ["Ei\u00b7nes", "ehr\u00b7nen", "Krie\u00b7gers", "geht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig liegt das R\u00f6schen, die Viole,", "tokens": ["Trau\u00b7rig", "liegt", "das", "R\u00f6\u00b7schen", ",", "die", "Vi\u00b7o\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jedes Bl\u00fcmchen auf zertretnem Beet.", "tokens": ["Je\u00b7des", "Bl\u00fcm\u00b7chen", "auf", "zer\u00b7tret\u00b7nem", "Beet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "O so komm, du Friede, nieder,", "tokens": ["O", "so", "komm", ",", "du", "Frie\u00b7de", ",", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "PPER", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4nftige der Krieger Sinn.", "tokens": ["S\u00e4nf\u00b7ti\u00b7ge", "der", "Krie\u00b7ger", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend Deutsche, alle brav und bieder,", 