{"dta.poem.10999": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nutz der Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem, der mit recht ger\u00fchrter Seelen,", "tokens": ["Dem", ",", "der", "mit", "recht", "ge\u00b7r\u00fchr\u00b7ter", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch der Gesch\u00f6pfe Wunder-Pracht,", "tokens": ["Durch", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Wun\u00b7der\u00b7Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum deutlichen Begriff vom wahren Gott gebracht,", "tokens": ["Zum", "deut\u00b7li\u00b7chen", "Be\u00b7griff", "vom", "wah\u00b7ren", "Gott", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Darf man nicht erstlich anbefehlen,", "tokens": ["Darf", "man", "nicht", "erst\u00b7lich", "an\u00b7be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denselbigen zu f\u00fcrchten und zu lieben,", "tokens": ["Den\u00b7sel\u00b7bi\u00b7gen", "zu", "f\u00fcrch\u00b7ten", "und", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er wird gewi\u00df dazu von selbst schon angetrieben.", "tokens": ["Er", "wird", "ge\u00b7wi\u00df", "da\u00b7zu", "von", "selbst", "schon", "an\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}