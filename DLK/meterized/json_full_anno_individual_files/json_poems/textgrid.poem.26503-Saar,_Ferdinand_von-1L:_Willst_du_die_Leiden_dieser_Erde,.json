{"textgrid.poem.26503": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du die Leiden dieser Erde,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du die Leiden dieser Erde,", "tokens": ["Willst", "du", "die", "Lei\u00b7den", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschheit Jammer ganz versteh'n,", "tokens": ["Der", "Menschheit", "Jam\u00b7mer", "ganz", "ver\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00dft du mit scheuer Gramgeberde,", "tokens": ["Mu\u00dft", "du", "mit", "scheu\u00b7er", "Gram\u00b7ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kind im Stillen weinen seh'n;", "tokens": ["Ein", "Kind", "im", "Stil\u00b7len", "wei\u00b7nen", "seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Kind, das eben fortgewichen", "tokens": ["Ein", "Kind", ",", "das", "e\u00b7ben", "fort\u00b7ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus fr\u00f6hlicher Gespielen Kreis", "tokens": ["Aus", "fr\u00f6h\