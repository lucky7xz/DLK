{"textgrid.poem.63071": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ein Bild", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einer Galerie ein M\u00e4dchenbild \u2013 war es in Kassel,", "tokens": ["In", "ei\u00b7ner", "Ga\u00b7le\u00b7rie", "ein", "M\u00e4d\u00b7chen\u00b7bild", "\u2013", "war", "es", "in", "Kas\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$(", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In M\u00fcnchen, in Berlin? \u2013 ich wei\u00df nur, da\u00df es mir geh\u00f6rt.", "tokens": ["In", "M\u00fcn\u00b7chen", ",", "in", "Ber\u00b7lin", "?", "\u2013", "ich", "wei\u00df", "nur", ",", "da\u00df", "es", "mir", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$.", "$(", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ihr ganzer Leib ein Auge: eine Assel,", "tokens": ["Ihr", "gan\u00b7zer", "Leib", "ein", "Au\u00b7ge", ":", "ei\u00b7ne", "As\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die feuchte Gr\u00fcfte raschelnd st\u00f6rt.", "tokens": ["Die", "feuch\u00b7te", "Gr\u00fcf\u00b7te", "ra\u00b7schelnd", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch sah man n\u00e4her hin, so milderten die falben,", "tokens": ["Doch", "sah", "man", "n\u00e4\u00b7her", "hin", ",", "so", "mil\u00b7der\u00b7ten", "die", "fal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Verhetzten Blicke sich, die dem Beschauer fluchen,", "tokens": ["Ver\u00b7hetz\u00b7ten", "Bli\u00b7cke", "sich", ",", "die", "dem", "Be\u00b7schau\u00b7er", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sind wie junge, fr\u00fchgefangne Schwalben,", "tokens": ["Und", "sind", "wie", "jun\u00b7ge", ",", "fr\u00fch\u00b7ge\u00b7fang\u00b7ne", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die fl\u00fcgelschlagend ihren S\u00fcden suchen.", "tokens": ["Die", "fl\u00fc\u00b7gel\u00b7schla\u00b7gend", "ih\u00b7ren", "S\u00fc\u00b7den", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In einer Galerie ein M\u00e4dchenbild \u2013 war es in Kassel,", "tokens": ["In", "ei\u00b7ner", "Ga\u00b7le\u00b7rie", "ein", "M\u00e4d\u00b7chen\u00b7bild", "\u2013", "war", "es", "in", "Kas\u00b7sel", ","], "token_info": ["word", "word", "