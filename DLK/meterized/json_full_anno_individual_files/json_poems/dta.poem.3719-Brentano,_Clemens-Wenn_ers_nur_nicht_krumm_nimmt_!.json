{"dta.poem.3719": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wenn ers nur nicht krumm nimmt !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um um um mein Krummer,               ", "tokens": ["Um", "um", "um", "mein", "Krum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Krummer du bist mein,", "tokens": ["Krum\u00b7mer", "du", "bist", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ei du krummer Dingerler,", "tokens": ["Ei", "du", "krum\u00b7mer", "Din\u00b7ger\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie magst so lustig seyn?", "tokens": ["Wie", "magst", "so", "lus\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}