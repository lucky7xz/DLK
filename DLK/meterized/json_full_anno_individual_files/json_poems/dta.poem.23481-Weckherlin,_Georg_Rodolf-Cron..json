{"dta.poem.23481": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vnverwundlich vnder der feinde\u0303 h\u00f6hr", "tokens": ["Vn\u00b7ver\u00b7wund\u00b7lich", "vn\u00b7der", "der", "fein\u00b7d\u1ebd", "h\u00f6hr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Macht jhn die Dapferkeit verbleibe\u0303/", "tokens": ["Macht", "jhn", "die", "Dap\u00b7fer\u00b7keit", "ver\u00b7bleib\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Er kan mit Jhr/ als seinem schilt vnd", "tokens": ["Er", "kan", "mit", "Ihr", "/", "als", "sei\u00b7nem", "schilt", "vnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "$(", "KOUS", "PPOSAT", "VVFIN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "w\u00f6hr/", "tokens": ["w\u00f6hr", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Was sunst zuf\u00f6rchten ist vertreiben:", "tokens": ["Was", "sunst", "zu\u00b7f\u00f6rch\u00b7ten", "ist", "ver\u00b7trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Jhr/ gleichsam in einem vosten schlo\u00df", "tokens": ["In", "Ihr", "/", "gleich\u00b7sam", "in", "ei\u00b7nem", "vos\u00b7ten", "schlo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "$(", "ADJD", "APPR", "ART", "ADJA", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Kan sein gem\u00fcht/ m\u00e4chtig/ einig vn\u0303 gro\u00df/", "tokens": ["Kan", "sein", "ge\u00b7m\u00fcht", "/", "m\u00e4ch\u00b7tig", "/", "ei\u00b7nig", "v\u00f1", "gro\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VVPP", "$(", "ADJD", "$(", "ADJD", "ADV", "ADJD", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Des Lebens krieg sicher verwalten/", "tokens": ["Des", "Le\u00b7bens", "krieg", "si\u00b7cher", "ver\u00b7wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.9": {"text": "Vnd die bel\u00e4grung frey au\u00dfhalten:", "tokens": ["Vnd", "die", "be\u00b7l\u00e4\u00b7grung", "frey", "au\u00df\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit Jhr kan Er in gutem Gl\u00fcck", "tokens": ["Mit", "Ihr", "kan", "Er", "in", "gu\u00b7tem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "(welches oft falsch) sich gantz sanftm\u00fchtig", "tokens": ["(", "wel\u00b7ches", "oft", "falsch", ")", "sich", "gantz", "sanft\u00b7m\u00fch\u00b7tig"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PRELS", "ADV", "ADJD", "$(", "PRF", "ADV", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "naigen;", "tokens": ["nai\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Aber wider des Vngl\u00fccks d\u00fcck", "tokens": ["A\u00b7ber", "wi\u00b7der", "des", "Vn\u00b7gl\u00fccks", "d\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Sein starckes hertz allzeit sigreich erzaigen.", "tokens": ["Sein", "star\u00b7ckes", "hertz", "all\u00b7zeit", "sig\u00b7reich", "er\u00b7zai\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ja seiner streitbaren faust macht", "tokens": ["Ja", "sei\u00b7ner", "streit\u00b7ba\u00b7ren", "faust", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wirt seine feind (ob Got will) noch zust\u00f6ren/", "tokens": ["Wirt", "sei\u00b7ne", "feind", "(", "ob", "Got", "will", ")", "noch", "zu\u00b7st\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "KOUS", "NN", "VMFIN", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Das man Jhren verlust soll h\u00f6ren", "tokens": ["Das", "man", "Ih\u00b7ren", "ver\u00b7lust", "soll", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Durch meiner stim vnverg\u00e4ngliche\u0303 pracht.", "tokens": ["Durch", "mei\u00b7ner", "stim", "vn\u00b7ver\u00b7g\u00e4ng\u00b7lich\u1ebd", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "FM", "FM", "FM", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}