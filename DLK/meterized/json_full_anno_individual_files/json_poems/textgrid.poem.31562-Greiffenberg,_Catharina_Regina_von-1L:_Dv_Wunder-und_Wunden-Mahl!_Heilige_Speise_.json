{"textgrid.poem.31562": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dv Wunder-und Wunden-Mahl! Heilige Speise /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv Wunder-und Wunden-Mahl! Heilige Speise /", "tokens": ["Dv", "Wun\u00b7der\u00b7\u00b7und", "Wun\u00b7den\u00b7Mahl", "!", "Hei\u00b7li\u00b7ge", "Spei\u00b7se", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$.", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Vnsterblichkeit selber man jetzund verzehrt.", "tokens": ["Vns\u00b7terb\u00b7lich\u00b7keit", "sel\u00b7ber", "man", "je\u00b7tzund", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zum Osterlamm selber der Hirt sich verehrt /", "tokens": ["Zum", "Os\u00b7ter\u00b7lamm", "sel\u00b7ber", "der", "Hirt", "sich", "ver\u00b7ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "PRF", "VVPP", "$("], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "die Schafe er weidet und leitet uns leise /", "tokens": ["die", "Scha\u00b7fe", "er", "wei\u00b7det", "und", "lei\u00b7tet", "uns", "lei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "durch sichtbar-unsichtbar / hochherrlicher Wei\u00dfe.", "tokens": ["durch", "sicht\u00b7ba\u00b7run\u00b7sicht\u00b7bar", "/", "hoch\u00