{"textgrid.poem.35512": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "8. Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist die Stunde kommen,", "tokens": ["Nun", "ist", "die", "Stun\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ich von hinnen mu\u00df ...", "tokens": ["Da", "ich", "von", "hin\u00b7nen", "mu\u00df", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Mutter, liebe Mutter, gib", "tokens": ["O", "Mut\u00b7ter", ",", "lie\u00b7be", "Mut\u00b7ter", ",", "gib"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "VVIMP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir nun den Abschiedsku\u00df!", "tokens": ["Mir", "nun", "den", "Ab\u00b7schieds\u00b7ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df, du l\u00e4\u00dft mit Bangen", "tokens": ["Ich", "wei\u00df", ",", "du", "l\u00e4\u00dft", "mit", "Ban\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mich meine Stra\u00dfe ziehn \u2013", "tokens": ["Mich", "mei\u00b7ne", "Stra\u00b7\u00dfe", "ziehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und doch ein wild Verlangen", "tokens": ["Und", "doch", "ein", "wild", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nimmt mich so ganz gefangen,", "tokens": ["Nimmt", "mich", "so", "ganz", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Will mir die Brust vergl\u00fchn ...", "tokens": ["Will", "mir", "die", "Brust", "ver\u00b7gl\u00fchn", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Mutter, liebe Mutter,", "tokens": ["O", "Mut\u00b7ter", ",", "lie\u00b7be", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df nur das Weinen dein! ...", "tokens": ["La\u00df", "nur", "das", "Wei\u00b7nen", "dein", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du warst so treu, du warst so gut \u2013", "tokens": ["Du", "warst", "so", "treu", ",", "du", "warst", "so", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird's nie wieder sein ...", "tokens": ["So", "wird's", "nie", "wie\u00b7der", "sein", "..."], "tok