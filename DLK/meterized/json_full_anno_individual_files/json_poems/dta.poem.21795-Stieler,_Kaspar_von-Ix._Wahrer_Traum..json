{"dta.poem.21795": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n  Wahrer Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gieng' einmahl im Traum zu Schiffe", "tokens": ["Ich", "gieng'", "ein\u00b7mahl", "im", "Traum", "zu", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Mele war mit mir mein Kind/", "tokens": ["die", "Me\u00b7le", "war", "mit", "mir", "mein", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es blie\u00df der linde Westen Wind", "tokens": ["es", "blie\u00df", "der", "lin\u00b7de", "Wes\u00b7ten", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als unser Schiff zu Lande lieffe.", "tokens": ["als", "un\u00b7ser", "Schiff", "zu", "Lan\u00b7de", "lief\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Indehm entstund\u2019 ein Schiffgeschrey", "tokens": ["In\u00b7dehm", "ent\u00b7stund'", "ein", "Schiff\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df di\u00df das Innland Zypern sey.", "tokens": ["da\u00df", "di\u00df", "das", "Inn\u00b7land", "Zy\u00b7pern", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als wir das Ufer nu gegr\u00fcsset", "tokens": ["Als", "wir", "das", "U\u00b7fer", "nu", "ge\u00b7gr\u00fcs\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "umfieng mich Mel\u2019 und sprach zn mir:", "tokens": ["um\u00b7fi\u00b7eng", "mich", "Mel'", "und", "sprach", "zn", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "VVFIN", "NE", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schaz/ la\u00df uns schauen di\u00df Revier/", "tokens": ["Schaz", "/", "la\u00df", "uns", "schau\u00b7en", "di\u00df", "Re\u00b7vier", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "PPER", "VVFIN", "PDS", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "das Tahl/ so iener Flu\u00df begiesset/", "tokens": ["das", "Tahl", "/", "so", "ie\u00b7ner", "Flu\u00df", "be\u00b7gies\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und hier der Zinnen hohen Schein/", "tokens": ["und", "hier", "der", "Zin\u00b7nen", "ho\u00b7hen", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es war der Tempel der Dionen", "tokens": ["Es", "war", "der", "Tem\u00b7pel", "der", "Di\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "um welchen der Poeten Schaar", "tokens": ["um", "wel\u00b7chen", "der", "Po\u00b7et\u00b7en", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wo Lieb\u2019 und Liebes-Kinder wohnen.", "tokens": ["wo", "Lieb'", "und", "Lie\u00b7bes\u00b7Kin\u00b7der", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Altertuhm und G\u00f6ttligkeit", "tokens": ["Sein", "Al\u00b7ter\u00b7tuhm", "und", "G\u00f6tt\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "verk\u00fcrzt\u2019 uns leichtlich Weg und Zeit.", "tokens": ["ver\u00b7k\u00fcrzt'", "uns", "leicht\u00b7lich", "Weg", "und", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir kahmen zu den Marmort\u00fchren/", "tokens": ["Wir", "kah\u00b7men", "zu", "den", "Mar\u00b7mor\u00b7t\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kupido lie\u00df uns b\u00fckkend ein/", "tokens": ["Ku\u00b7pi\u00b7do", "lie\u00df", "uns", "b\u00fck\u00b7kend", "ein", "/"], 