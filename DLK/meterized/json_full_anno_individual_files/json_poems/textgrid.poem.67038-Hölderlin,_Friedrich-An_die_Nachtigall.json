{"textgrid.poem.67038": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir fl\u00fcsterts leise \u2013 Nachtigall! dir allein,", "tokens": ["Dir", "fl\u00fcs\u00b7terts", "lei\u00b7se", "\u2013", "Nach\u00b7ti\u00b7gall", "!", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$(", "NN", "$.", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir, s\u00fc\u00dfe Tr\u00e4nenweckerin! sagt es nur", "tokens": ["Dir", ",", "s\u00fc\u00b7\u00dfe", "Tr\u00e4\u00b7nen\u00b7we\u00b7cke\u00b7rin", "!", "sagt", "es", "nur"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Saite. \u2013 Stellas wehmutsvoller", "tokens": ["Die", "Sai\u00b7te", ".", "\u2013", "Stel\u00b7las", "weh\u00b7muts\u00b7vol\u00b7ler"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seufzer \u2013 er raubte mein Herz \u2013 dein Kehlchen \u2013", "tokens": ["Seuf\u00b7zer", "\u2013", "er", "raub\u00b7te", "mein", "Herz", "\u2013", "dein", "Kehl\u00b7chen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Es klagte \u2013 o! es klagte \u2013 wie Stella ists.", "token