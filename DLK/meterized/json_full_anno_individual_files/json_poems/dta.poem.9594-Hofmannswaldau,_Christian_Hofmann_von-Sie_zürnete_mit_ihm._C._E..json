{"dta.poem.9594": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie z\u00fcrnete mit ihm.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' Oranten j\u00fcngst nicht auffger\u00e4umt gefunden/", "tokens": ["Ich", "hab'", "O\u00b7ran\u00b7ten", "j\u00fcngst", "nicht", "auff\u00b7ge\u00b7r\u00e4umt", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "PTKNEG", "VVPP", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der donner ihres munds/ und ihrer augen blitz", "tokens": ["Der", "don\u00b7ner", "ih\u00b7res", "munds", "/", "und", "ih\u00b7rer", "au\u00b7gen", "blitz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Benahm mir allen muth und hemmte meinen witz/", "tokens": ["Be\u00b7nahm", "mir", "al\u00b7len", "muth", "und", "hemm\u00b7te", "mei\u00b7nen", "witz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df beyde augen mir auch voller thr\u00e4nen stunden.", "tokens": ["Da\u00df", "bey\u00b7de", "au\u00b7gen", "mir", "auch", "vol\u00b7ler", "thr\u00e4\u00b7nen", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar sch\u00e4mt ich anfangs mich; doch fiel mir endlich ein:", "tokens": ["Zwar", "sch\u00e4mt", "ich", "an\u00b7fangs", "mich", ";", "doch", "fiel", "mir", "end\u00b7lich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df blitz und donner nicht kan ohne regen seyn.", "tokens": ["Da\u00df", "blitz", "und", "don\u00b7ner", "nicht", "kan", "oh\u00b7ne", "re\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "PTKNEG", "VMFIN", "APPR", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}