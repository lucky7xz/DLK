{"textgrid.poem.38517": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgesang", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber Rebenh\u00fcgel,", "tokens": ["Ue\u00b7ber", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo sich Trauben f\u00e4rben,", "tokens": ["Wo", "sich", "Trau\u00b7ben", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ueber Obstgel\u00e4nder,", "tokens": ["Ue\u00b7ber", "Obst\u00b7ge\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wo sich Aepfel r\u00f6then,", "tokens": ["Wo", "sich", "A\u00b7e\u00b7pfel", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Leert der milde Fruchtmond", "tokens": ["Leert", "der", "mil\u00b7de", "Frucht\u00b7mond"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "L\u00e4chelnd das geudende Segensf\u00fcllhorn!", "tokens": ["L\u00e4\u00b7chelnd", "das", "geu\u00b7den\u00b7de", "Se\u00b7gens\u00b7f\u00fcll\u00b7horn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus den Haingestr\u00e4uchen,", "tokens": ["Aus", "den", "Hain\u00b7ge\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aus den Hekeng\u00e4ngen,", "tokens": ["Aus", "den", "He\u00b7ken\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aus den Gartenbeeten,", "tokens": ["Aus", "den", "Gar\u00b7ten\u00b7bee\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Zirpen tausend Grillen,", "tokens": ["Zir\u00b7pen", "tau\u00b7send", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Um die Abendd\u00e4mrung,", "tokens": ["Um", "die", "A\u00b7bend\u00b7d\u00e4m\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Feierges\u00e4nge dem Traubensch\u00f6pfer!", "tokens": ["Fei\u00b7er\u00b7ge\u00b7s\u00e4n\u00b7ge", "dem", "Trau\u00b7ben\u00b7sch\u00f6p\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wo sein Auge l\u00e4chelt,", "tokens": ["Wo", "sein", "Au\u00b7ge", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Reifen Honigfr\u00fcchte!", "tokens": ["Rei\u00b7fen", "Ho\u00b7nig\u00b7fr\u00fcch\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "An den vollen Zweigen,", "tokens": ["An", "den", "vol\u00b7len", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Giebt er jeder Pflaume", "tokens": ["Giebt", "er", "je\u00b7der", "Pflau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ihre Himmelbl\u00e4ue,", "tokens": ["Ih\u00b7re", "Him\u00b7mel\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Malt er dem Apfel die Purpurwange!", "tokens": ["Malt", "er", "dem", "Ap\u00b7fel", "die", "Pur\u00b7pur\u00b7wan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Auf den Rebenbergen,", "tokens": ["Auf", "den", "Re\u00b7ben\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo die Winzerm\u00e4dchen", "tokens": ["Wo", "die", "Win\u00b7zer\u00b7m\u00e4d\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hochges\u00e4nge t\u00f6nen,", "tokens": ["Hoch\u00b7ge\u00b7s\u00e4n\u00b7ge", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Knarren alle Keltern,", "tokens": ["Knar\u00b7ren", "al\u00b7le", "Kel\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und aus ihrem Schoosse", "tokens": ["Und", "aus", "ih\u00b7rem", "Schoos\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Tr\u00e4uft der begeisternde Trank der Freude.", "tokens": ["Tr\u00e4uft", "der", "be\u00b7geis\u00b7tern\u00b7de", "Trank", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Da\u00df das liebe Kelchglas,", "tokens": ["Da\u00df", "das", "lie\u00b7be", "Kelchg\u00b7las", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Oft, im Freundeskreise,", "tokens": ["Oft", ",", "im", "Freun\u00b7des\u00b7krei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unser Herz erfreuet,", "tokens": ["Un\u00b7ser", "Herz", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Unsren Geist befl\u00fcgelt:", "tokens": ["Un\u00b7sren", "Geist", "be\u00b7fl\u00fc\u00b7gelt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Danken alle Zungen", "tokens": ["Dan\u00b7ken", "al\u00b7le", "Zun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dir, o! allsegnender Rebengeber!", "tokens": ["Dir", ",", "o", "!", "all\u00b7seg\u00b7nen\u00b7der", "Re\u00b7ben\u00b7ge\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "$.", "PIAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n bist du, o Erde!", "tokens": ["Sch\u00f6n", "bist", "du", ",", "o", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kleidet deine H\u00fcgel,", "tokens": ["Klei\u00b7det", "dei\u00b7ne", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deine Saatgefilde,", "tokens": ["Dei\u00b7ne", "Saat\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deine Gartenfluren,", "tokens": ["Dei\u00b7ne", "Gar\u00b7ten\u00b7flu\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der allmilde Herbstmond", "tokens": ["Der", "all\u00b7mil\u00b7de", "Herbst\u00b7mond"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Lieblich mit farbigem Fruchtgewande.", "tokens": ["Lieb\u00b7lich", "mit", "far\u00b7bi\u00b7gem", "Frucht\u00b7ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6ner nur, o Mutter!", "tokens": ["Sch\u00f6\u00b7ner", "nur", ",", "o", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "FM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "L\u00e4chelst du im Lenze,", "tokens": ["L\u00e4\u00b7chelst", "du", "im", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn dir um die Loken", "tokens": ["Wenn", "dir", "um", "die", "Lo\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Weisse Bl\u00fcten s\u00e4useln,", "tokens": ["Weis\u00b7se", "Bl\u00fc\u00b7ten", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und dein G\u00f6tterantliz", "tokens": ["Und", "dein", "G\u00f6t\u00b7ter\u00b7ant\u00b7liz"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "W\u00f6lken die athmenden Mainachtd\u00fcfte!", "tokens": ["W\u00f6l\u00b7ken", "die", "ath\u00b7men\u00b7den", "Mai\u00b7nacht\u00b7d\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Hier am Quellenrande,", "tokens": ["Hier", "am", "Quel\u00b7len\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo mich Schilf umfl\u00fcstert,", "tokens": ["Wo", "mich", "Schilf", "um\u00b7fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo, von Laubgew\u00f6lben,", "tokens": ["Wo", ",", "von", "Laub\u00b7ge\u00b7w\u00f6l\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "D\u00fcrre Pappelbl\u00e4tter", "tokens": ["D\u00fcr\u00b7re", "Pap\u00b7pel\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf mich niederrieseln,", "tokens": ["Auf", "mich", "nie\u00b7der\u00b7rie\u00b7seln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Soll mich der Abend mit Maja finden!", "tokens": ["Soll", "mich", "der", "A\u00b7bend", "mit", "Ma\u00b7ja", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Fleuch, o s\u00fcsses M\u00e4dchen!", "tokens": ["Fleuch", ",", "o", "s\u00fcs\u00b7ses", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fleuch dein Teppichzimmer,", "tokens": ["Fleuch", "dein", "Tep\u00b7pich\u00b7zim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deiner Stadt Gepr\u00e4nge!", "tokens": ["Dei\u00b7ner", "Stadt", "Ge\u00b7pr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hier, im Abendschatten,", "tokens": ["Hier", ",", "im", "A\u00b7bend\u00b7schat\u00b7ten", ","], "token_