{"textgrid.poem.65164": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wenig wie der Quell, in dessen klaren", "tokens": ["So", "we\u00b7nig", "wie", "der", "Quell", ",", "in", "des\u00b7sen", "kla\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "$,", "APPR", "PDS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lichtwellen jetzt mein Angesicht zu schauen,", "tokens": ["Licht\u00b7wel\u00b7len", "jetzt", "mein", "An\u00b7ge\u00b7sicht", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Wenn er als breiter Strom durchwogt die Auen", "tokens": ["Wenn", "er", "als", "brei\u00b7ter", "Strom", "durch\u00b7wogt", "die", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das l\u00e4ngst zerfloss'ne Bild noch wird bewahren:", "tokens": ["Das", "l\u00e4ngst", "zer\u00b7floss'\u00b7ne", "Bild", "noch", "wird", "be\u00b7wah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So wenig wird in sp\u00e4t'rer Jahre Treiben,", "tokens": ["So", "we\u00b7nig", "wird", "in", "sp\u00e4t'\u00b7rer", "Jah\u00b7re", "Trei\u00b7ben", ","], "token_info": ["word", "word"