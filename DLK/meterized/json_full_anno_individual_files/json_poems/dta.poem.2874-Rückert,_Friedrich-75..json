{"dta.poem.2874": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lehre dich, mein Sohn! Nie \u00fcbe das, was \u00fcber", "tokens": ["Ich", "leh\u00b7re", "dich", ",", "mein", "Sohn", "!", "Nie", "\u00fc\u00b7be", "das", ",", "was", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PDS", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ma\u00df ist! Ueberall vom Uebel ist das Ueber.", "tokens": ["Das", "Ma\u00df", "ist", "!", "Ue\u00b7be\u00b7rall", "vom", "Ue\u00b7bel", "ist", "das", "Ue\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich \u00fcberliefr' es dir, wie's mir ist \u00fcbermacht:", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7lie\u00b7fr'", "es", "dir", ",", "wie's", "mir", "ist", "\u00fc\u00b7ber\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht gut ist Ueberflu\u00df, nicht gut ist Uebermacht.", "tokens": ["Nicht", "gut", "ist", "Ue\u00b7berf\u00b7lu\u00df", ",", "nicht", "gut", "ist", "Ue\u00b7ber\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "NN", "$,", "PTKNEG", "ADJD", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn hast du's \u00fcberdacht, wie oft die Uebermacht", "tokens": ["Denn", "hast", "du's", "\u00fc\u00b7berd\u00b7acht", ",", "wie", "oft", "die", "Ue\u00b7ber\u00b7macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIS", "VVPP", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Ueberpracht der Welt vergangen \u00fcber Nacht?", "tokens": ["Und", "Ue\u00b7ber\u00b7pracht", "der", "Welt", "ver\u00b7gan\u00b7gen", "\u00fc\u00b7ber", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wie den Ueberflu\u00df Uebergenu\u00df verschlingt,", "tokens": ["Und", "wie", "den", "Ue\u00b7berf\u00b7lu\u00df", "Ue\u00b7ber\u00b7ge\u00b7nu\u00df", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wie der Ueberdru\u00df aus Ueberflu\u00df entspringt?", "tokens": ["Und", "wie", "der", "Ue\u00b7berd\u00b7ru\u00df", "aus", "Ue\u00b7berf\u00b7lu\u00df", "ent\u00b7springt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie Drang zu Ueberdrang, Schwung wird zu Ueberschwang,", "tokens": ["Wie", "Drang", "zu", "Ue\u00b7ber\u00b7drang", ",", "Schwung", "wird", "zu", "Ue\u00b7ber\u00b7schwang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schnell zum B\u00f6sen ist des Besten Uebergang?", "tokens": ["Und", "schnell", "zum", "B\u00f6\u00b7sen", "ist", "des", "Bes\u00b7ten", "Ue\u00b7ber\u00b7gang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Leicht stumpf wird \u00fcberfein, leicht th\u00f6richt \u00fcberklug,", "tokens": ["Leicht", "stumpf", "wird", "\u00fc\u00b7berf\u00b7ein", ",", "leicht", "th\u00f6\u00b7richt", "\u00fc\u00b7berk\u00b7lug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "ADV", "$,", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil stets ein Gegentheil ins andre \u00fcberschlug.", "tokens": ["Weil", "stets", "ein", "Ge\u00b7gen\u00b7theil", "ins", "and\u00b7re", "\u00fc\u00b7bersc\u00b7hlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6n sei nicht \u00fcbersch\u00f6n, und hold nicht \u00fcberhold!", "tokens": ["Sch\u00f6n", "sei", "nicht", "\u00fc\u00b7bersc\u00b7h\u00f6n", ",", "und", "hold", "nicht", "\u00fc\u00b7ber\u00b7hold", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADJD", "$,", "KON", "ADJD", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Uebergoldung ist im Werth nicht \u00fcber Gold.", "tokens": ["Denn", "Ue\u00b7ber\u00b7gol\u00b7dung", "ist", "im", "Werth", "nicht", "\u00fc\u00b7ber", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPRART", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Um wirklich gut zu sein, sei selbst nicht \u00fcbergut;", "tokens": ["Um", "wirk\u00b7lich", "gut", "zu", "sein", ",", "sei", "selbst", "nicht", "\u00fc\u00b7ber\u00b7gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADJD", "PTKZU", "VAINF", "$,", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn der Muth ist dein, werd' er nicht Uebermuth.", "tokens": ["Und", "wenn", "der", "Muth", "ist", "dein", ",", "werd'", "er", "nicht", "Ue\u00b7ber\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "PPOSAT", "$,", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Denn jeder Trieb verdirbt, wann er wird \u00fcbertrieben:", "tokens": ["Denn", "je\u00b7der", "Trieb", "ver\u00b7dirbt", ",", "wann", "er", "wird", "\u00fc\u00b7bert\u00b7rie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch \u00fcbersch\u00e4tzen sollst du nichts noch \u00fcberlieben.", "tokens": ["Auch", "\u00fc\u00b7bersc\u00b7h\u00e4t\u00b7zen", "sollst", "du", "nichts", "noch", "\u00fc\u00b7berl\u00b7ie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Bei Ueberlegung nur darfst du was \u00fcber-legen;", "tokens": ["Bei", "Ue\u00b7ber\u00b7le\u00b7gung", "nur", "darfst", "du", "was", "\u00fc\u00b7ber\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Ueberlegenheit entspringt aus Ueberlegen.", "tokens": ["Denn", "Ue\u00b7ber\u00b7le\u00b7gen\u00b7heit", "ent\u00b7springt", "aus", "Ue\u00b7ber\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Die Ueberlegung doch ist unn\u00fctz auch, wor\u00fcber?", "tokens": ["Die", "Ue\u00b7ber\u00b7le\u00b7gung", "doch", "ist", "un\u00b7n\u00fctz", "auch", ",", "wo\u00b7r\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "ADV", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein S\u00f6hnchen, \u00fcber das, was einmal ist vor\u00fcber.", "tokens": ["Mein", "S\u00f6hn\u00b7chen", ",", "\u00fc\u00b7ber", "das", ",", "was", "ein\u00b7mal", "ist", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PDS", "$,", "PRELS", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}