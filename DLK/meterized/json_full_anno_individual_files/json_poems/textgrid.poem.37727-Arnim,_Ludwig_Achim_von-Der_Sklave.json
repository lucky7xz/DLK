{"textgrid.poem.37727": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Sklave", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr G\u00f6tter! wenn ich schwebend", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "wenn", "ich", "schwe\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nah' euerm heil'gen Kreis,", "tokens": ["Nah'", "eu\u00b7erm", "heil'\u00b7gen", "Kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In euern Wolken strebend", "tokens": ["In", "eu\u00b7ern", "Wol\u00b7ken", "stre\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch ahne auch schon leis:", "tokens": ["Euch", "ah\u00b7ne", "auch", "schon", "leis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer wagt die goldnen Fl\u00fcgel", "tokens": ["Wer", "wagt", "die", "gold\u00b7nen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu rei\u00dfen tief herab,", "tokens": ["Zu", "rei\u00b7\u00dfen", "tief", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis zu dem dunkeln H\u00fcgel", "tokens": ["Bis", "zu", "dem", "dun\u00b7keln", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo n\u00fcchtern g\u00e4hnt mein Grab!", "tokens": ["Wo", "n\u00fcch\u00b7tern", "g\u00e4hnt", "mein", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Furcht macht Luft so d\u00fcnne,", "tokens": ["Die", "Furcht", "macht", "Luft", "so", "d\u00fcn\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Federkron fiel aus,", "tokens": ["Die", "Fe\u00b7der\u00b7kron", "fiel", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh' ich mich recht besinne,", "tokens": ["Eh'", "ich", "mich", "recht", "be\u00b7sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo euer festes Haus!", "tokens": ["Wo", "eu\u00b7er", "fes\u00b7tes", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Sturmwind kommt geflogen,", "tokens": ["Ein", "Sturm\u00b7wind", "kommt", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nimmt mich auf seinen Arm,", "tokens": ["Nimmt", "mich", "auf", "sei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hat mich hinab gezogen", "tokens": ["Hat", "mich", "hin\u00b7ab", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Winter kalt und arm.", "tokens": ["Zum", "Win\u00b7ter", "kalt", "und", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich lieg' an d\u00fcrrem Boden,", "tokens": ["Ich", "lieg'", "an", "d\u00fcr\u00b7rem", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Aug' schlie\u00dft sich besch\u00e4mt,", "tokens": ["Das", "Aug'", "schlie\u00dft", "sich", "be\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gehemmt ist mir der Odem,", "tokens": ["Ge\u00b7hemmt", "ist", "mir", "der", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Fl\u00fcgel war gel\u00e4hmt;", "tokens": ["Mein", "Fl\u00fc\u00b7gel", "war", "ge\u00b7l\u00e4hmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Frost bestrickt mit Eise,", "tokens": ["Der", "Frost", "be\u00b7strickt", "mit", "Ei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Federn leicht und frei,", "tokens": ["Die", "Fe\u00b7dern", "leicht", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus ist es mit der Reise,", "tokens": ["Aus", "ist", "es", "mit", "der", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der J\u00e4ger kommt mir bei! \u2013", "tokens": ["Der", "J\u00e4\u00b7ger", "kommt", "mir", "bei", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Hund mich schlau umstehet,", "tokens": ["Der", "Hund", "mich", "schlau", "um\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bellt und lockt den Herrn,", "tokens": ["Und", "bellt", "und", "lockt", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der J\u00e4ger eilender gehet,", "tokens": ["Der", "J\u00e4\u00b7ger", "ei\u00b7len\u00b7der", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nimmt mir die Freiheit gern;", "tokens": ["Nimmt", "mir", "die", "Frei\u00b7heit", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Er legt mir um die Augen", "tokens": ["Er", "legt", "mir", "um", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Kappe eng und dicht,", "tokens": ["Die", "Kap\u00b7pe", "eng", "und", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kann nicht die Fl\u00fcgel brauchen,", "tokens": ["Kann", "nicht", "die", "Fl\u00fc\u00b7gel", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich sehe ja kein Licht.", "tokens": ["Ich", "se\u00b7he", "ja", "kein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zieht dann mir ab die Kappe,", "tokens": ["Zieht", "dann", "mir", "ab", "die", "Kap\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ich denk', nun bin ich frei", "tokens": ["Ich", "denk'", ",", "nun", "bin", "ich", "frei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fl\u00fccht von seinem Stabe,", "tokens": ["Und", "fl\u00fccht", "von", "sei\u00b7nem", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu euch ihr G\u00f6tter treu.", "tokens": ["Zu", "euch", "ihr", "G\u00f6t\u00b7ter", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von seinem Ruf bezwungen", "tokens": ["Von", "sei\u00b7nem", "Ruf", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus freier Luft ich raub'", "tokens": ["Aus", "frei\u00b7er", "Luft", "ich", "raub'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die fromme Lerch', gelungen", "tokens": ["Die", "from\u00b7me", "Ler\u00b7ch'", ",", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ist mir der freche Raub!", "tokens": ["Ist", "mir", "der", "fre\u00b7che", "Raub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das ist der Knechtschaft Schande,", "tokens": ["Das", "ist", "der", "Knecht\u00b7schaft", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich gezwungen bin,", "tokens": ["Da\u00df", "ich", "ge\u00b7zwun\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu legen in die Bande", "tokens": ["Zu", "le\u00b7gen", "in", "die", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch andrer Freien Sinn!", "tokens": ["Auch", "an\u00b7drer", "Frei\u00b7en", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So klagt der Falk' mit Trauern,", "tokens": ["So", "klagt", "der", "Fal\u00b7k'", "mit", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da rief sein Herr ihm bald:", "tokens": ["Da", "rief", "sein", "Herr", "ihm", "bald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du klagst! \u2013 nun sollst du lauern", "tokens": ["Du", "klagst", "!", "\u2013", "nun", "sollst", "du", "lau\u00b7ern"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf deine Jungen im Wald!", "tokens": ["Auf", "dei\u00b7ne", "Jun\u00b7gen", "im", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Der Falke wollt sich wehren;", "tokens": ["Der", "Fal\u00b7ke", "wollt", "sich", "weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er stand in Herrensold, \u2013", "tokens": ["Er", "stand", "in", "Her\u00b7ren\u00b7sold", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die nackte Brut mu\u00df er st\u00f6ren,", "tokens": ["Die", "nack\u00b7te", "Brut", "mu\u00df", "er", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem Herrn er sie zitternd holt.", "tokens": ["Dem", "Herrn", "er", "sie", "zit\u00b7ternd", "holt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie sind mir noch nicht fl\u00fcgge,", "tokens": ["Sie", "sind", "mir", "noch", "nicht", "fl\u00fcg\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch dien' dir dies zur Lehr':", "tokens": ["Doch", "dien'", "dir", "dies", "zur", "Lehr'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt nimm sie noch zur\u00fccke,", "tokens": ["Jetzt", "nimm", "sie", "noch", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und meine Gro\u00dfmuth ehr'!", "tokens": ["Und", "mei\u00b7ne", "Gro\u00df\u00b7muth", "ehr'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr G\u00f6tter! wenn ich schwebend", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "wenn", "ich", "schwe\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nah' euerm heil'gen Kreis,", "tokens": ["Nah'", "eu\u00b7erm", "heil'\u00b7gen", "Kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In euern Wolken strebend", "tokens": ["In", "eu\u00b7ern", "Wol\u00b7ken", "stre\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch ahne auch schon leis:", "tokens": ["Euch", "ah\u00b7ne", "auch", "schon", "leis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer wagt die goldnen Fl\u00fcgel", "tokens": ["Wer", "wagt", "die", "gold\u00b7nen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu rei\u00dfen tief herab,", "tokens": ["Zu", "rei\u00b7\u00dfen", "tief", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure":