{"dta.poem.18388": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "La Valiere an den k\u00f6nig Ludewich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was mein gro\u00dfm\u00e4chtgster wil/ in dem/ was er geschriben/", "tokens": ["Was", "mein", "gro\u00df\u00b7m\u00e4\u00b7chtgs\u00b7ter", "wil", "/", "in", "dem", "/", "was", "er", "ge\u00b7schri\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "VMFIN", "$(", "APPR", "ART", "$(", "PWS", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Ist etwas so ich nicht erst heut beginnen kan/", "tokens": ["Ist", "et\u00b7was", "so", "ich", "nicht", "erst", "heut", "be\u00b7gin\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein gantzer brieff begehrt/ ich soll den K\u00f6nig lieben/", "tokens": ["Sein", "gant\u00b7zer", "brieff", "be\u00b7gehrt", "/", "ich", "soll", "den", "K\u00f6\u00b7nig", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dessen k\u00fchne magd hats schon vorl\u00e4ugst gethan.", "tokens": ["Und", "des\u00b7sen", "k\u00fch\u00b7ne", "magd", "hats", "schon", "vor\u00b7l\u00e4ugst", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nunmehr verstreicht ein jahr/ das ich mich unterwunden/", "tokens": ["Nun\u00b7mehr", "ver\u00b7streicht", "ein", "jahr", "/", "das", "ich", "mich", "un\u00b7ter\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "PRELS", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sonne Galliens vermessen anzusehn/", "tokens": ["Die", "son\u00b7ne", "Gal\u00b7li\u00b7ens", "ver\u00b7mes\u00b7sen", "an\u00b7zu\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVIZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach aber da\u00df mein hertz hirmit zugleich empfunden/", "tokens": ["Ach", "a\u00b7ber", "da\u00df", "mein", "hertz", "hir\u00b7mit", "zu\u00b7gleich", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "KOUS", "PPOSAT", "NN", "PAV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie sich kein schlechtes wachs zur sonne soll erh\u00f6hn.", "tokens": ["Wie", "sich", "kein", "schlech\u00b7tes", "wachs", "zur", "son\u00b7ne", "soll", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "ADJA", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein auge dadurch ich von seinen bin entglommen/", "tokens": ["Mein", "au\u00b7ge", "da\u00b7durch", "ich", "von", "sei\u00b7nen", "bin", "ent\u00b7glom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "PPER", "APPR", "PPOSAT", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil es ein sonnen-licht und meins ein brenngla\u00df war/", "tokens": ["Weil", "es", "ein", "son\u00b7nen\u00b7licht", "und", "meins", "ein", "brenn\u00b7gla\u00df", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "PIS", "ART", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist offt mit thr\u00e4nen-fluth der glut zu h\u00fclffe kommen/", "tokens": ["Ist", "offt", "mit", "thr\u00e4\u00b7nen\u00b7fluth", "der", "glut", "zu", "h\u00fclf\u00b7fe", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ART", "NN", "PTKZU", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sonst w\u00e4r ich asche schon vermodert gantz und gar.", "tokens": ["Sonst", "w\u00e4r", "ich", "asc\u00b7he", "schon", "ver\u00b7mo\u00b7dert", "gantz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ADV", "VVPP", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dann ich den lichten brand dazu verh\u00f6len m\u00fcssen/", "tokens": ["Dann", "ich", "den", "lich\u00b7ten", "brand", "da\u00b7zu", "ver\u00b7h\u00f6\u00b7len", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "PAV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nachdem geweyhtes oel der Vesten ihn erweckt/", "tokens": ["Nach\u00b7dem", "ge\u00b7weyh\u00b7tes", "o\u00b7el", "der", "Ves\u00b7ten", "ihn", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Wer aber solchen gast im leibe wil verschliessen/", "tokens": ["Wer", "a\u00b7ber", "sol\u00b7chen", "gast", "im", "lei\u00b7be", "wil", "ver\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIAT", "NN", "APPRART", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist einer/ der da feur mit h\u00f6ltzern kappen deckt.", "tokens": ["Ist", "ei\u00b7ner", "/", "der", "da", "feur", "mit", "h\u00f6lt\u00b7zern", "kap\u00b7pen", "deckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "$(", "ART", "ADV", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich hielt mit brunst und kunst das heiligthum zusammen/", "tokens": ["Ich", "hielt", "mit", "brunst", "und", "kunst", "das", "hei\u00b7lig\u00b7thum", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und dessen funcken hatt ich vom altar entwandt/", "tokens": ["Und", "des\u00b7sen", "fun\u00b7cken", "hatt", "ich", "vom", "al\u00b7tar", "ent\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mein hertze brandte/ kurtz/ von K\u00f6niglichen flammen/", "tokens": ["Mein", "hert\u00b7ze", "brand\u00b7te", "/", "kurtz", "/", "von", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "ADJD", "$(", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wer h\u00e4tte deun der welt dergleichen raub bekandt?", "tokens": ["Wer", "h\u00e4t\u00b7te", "deun", "der", "welt", "derg\u00b7lei\u00b7chen", "raub", "be\u00b7kandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn aber sich mein herr beginnet zu vertrauen/", "tokens": ["Wenn", "a\u00b7ber", "sich", "mein", "herr", "be\u00b7gin\u00b7net", "zu", "ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und Valieren selbst der Vesten feur verg\u00f6nt;", "tokens": ["Und", "Va\u00b7lie\u00b7ren", "selbst", "der", "Ves\u00b7ten", "feur", "ver\u00b7g\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.23": {"text": "L\u00e4st sie sein grosses aug auch ihren tempel schauen/", "tokens": ["L\u00e4st", "sie", "sein", "gros\u00b7ses", "aug", "auch", "ih\u00b7ren", "tem\u00b7pel", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wo alles was er siht/ ihm hat gebrannt und brennt/", "tokens": ["Wo", "al\u00b7les", "was", "er", "siht", "/", "ihm", "hat", "ge\u00b7brannt", "und", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PWS", "PPER", "VVFIN", "$(", "PPER", "VAFIN", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die brust ist lamp und herd/ die flammt von liebes-oele/", "tokens": ["Die", "brust", "ist", "lamp", "und", "herd", "/", "die", "flammt", "von", "lie\u00b7bes\u00b7o\u00b7ele", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "NN", "$(", "ART", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.26": {"text": "Und deren pristerin des K\u00f6nigs k\u00fcne magd/", "tokens": ["Und", "de\u00b7ren", "pris\u00b7te\u00b7rin", "des", "K\u00f6\u00b7nigs", "k\u00fc\u00b7ne", "magd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der abgott/ dessen bild/ das opffer meine seele/", "tokens": ["Der", "ab\u00b7gott", "/", "des\u00b7sen", "bild", "/", "das", "opf\u00b7fer", "mei\u00b7ne", "see\u00b7le", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELAT", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nun Valiere hat zur gn\u00fcg/ mein f\u00fcrst/ gesagt.", "tokens": ["Nun", "Va\u00b7lie\u00b7re", "hat", "zur", "gn\u00fcg", "/", "mein", "f\u00fcrst", "/", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "APPRART", "NN", "$(", "PPOSAT", "VVFIN", "$(", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.29": {"text": "Allein was wil der Printz von purpur sich entkleiden/", "tokens": ["Al\u00b7lein", "was", "wil", "der", "Printz", "von", "pur\u00b7pur", "sich", "ent\u00b7klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "ART", "NN", "APPR", "ADJD", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sein reich und land verschm\u00e4hn/ um mich sich gleich zu sehn?", "tokens": ["Sein", "reich", "und", "land", "ver\u00b7schm\u00e4hn", "/", "um", "mich", "sich", "gleich", "zu", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "NN", "VVINF", "$(", "APPR", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Lilgen m\u00fcssen nicht aus ihren feldern scheiden/", "tokens": ["Die", "Lil\u00b7gen", "m\u00fcs\u00b7sen", "nicht", "aus", "ih\u00b7ren", "fel\u00b7dern", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und unsre liebe kan durchs scepter nur bestehn.", "tokens": ["Und", "uns\u00b7re", "lie\u00b7be", "kan", "durchs", "scep\u00b7ter", "nur", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VMFIN", "APPRART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die crone/ die er tr\u00e4gt ist sicherer zu k\u00fcssen/", "tokens": ["Die", "cro\u00b7ne", "/", "die", "er", "tr\u00e4gt", "ist", "si\u00b7che\u00b7rer", "zu", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als legt ich meinen mund gemeinen lippen bey/", "tokens": ["Als", "legt", "ich", "mei\u00b7nen", "mund", "ge\u00b7mei\u00b7nen", "lip\u00b7pen", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mich tr\u00e4te bald der hoff viel tausendmal mit f\u00fcssen/", "tokens": ["Mich", "tr\u00e4\u00b7te", "bald", "der", "hoff", "viel", "tau\u00b7send\u00b7mal", "mit", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "VVFIN", "ADV", "ADV", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "W\u00fcrd dieses nicht bekandt/ da\u00df ich des K\u00f6nigs sey.", "tokens": ["W\u00fcrd", "die\u00b7ses", "nicht", "be\u00b7kandt", "/", "da\u00df", "ich", "des", "K\u00f6\u00b7nigs", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "$(", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und hat mein leib als perl in muscheln nicht gelegen/", "tokens": ["Und", "hat", "mein", "leib", "als", "perl", "in", "mu\u00b7scheln", "nicht", "ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "KOKOM", "NE", "APPR", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Macht er als Diamant mich doch zum edelstein/", "tokens": ["Macht", "er", "als", "Di\u00b7a\u00b7mant", "mich", "doch", "zum", "e\u00b7del\u00b7stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOUS", "NN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.39": {"text": "Denn weil ein Ph\u00f6nix sich verbrennt/ um meinetwegen/", "tokens": ["Denn", "weil", "ein", "Ph\u00f6\u00b7nix", "sich", "ver\u00b7brennt", "/", "um", "mei\u00b7net\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "VVFIN", "$(", "KOUI", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mu\u00df ich nach dessen brandt ja auch ein Ph\u00f6nix seyn.", "tokens": ["Mu\u00df", "ich", "nach", "des\u00b7sen", "brandt", "ja", "auch", "ein", "Ph\u00f6\u00b7nix", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wird thon nicht letzt zu ertzt/ wenn mans in gold eintauchet/", "tokens": ["Wird", "thon", "nicht", "letzt", "zu", "ertzt", "/", "wenn", "mans", "in", "gold", "ein\u00b7tau\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "PTKZU", "VVFIN", "$(", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wenn mich mein Zevs verg\u00fcldt/ wie werd ich denn nicht gold?", "tokens": ["Wenn", "mich", "mein", "Zevs", "ver\u00b7g\u00fcldt", "/", "wie", "werd", "ich", "denn", "nicht", "gold", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Es gilt ein jedes ding nachdem es einer brauchet/", "tokens": ["Es", "gilt", "ein", "je\u00b7des", "ding", "nach\u00b7dem", "es", "ei\u00b7ner", "brau\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "KOUS", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So bleib ich ja nicht schlecht/ wenn mir der k\u00f6nig hold.", "tokens": ["So", "bleib", "ich", "ja", "nicht", "schlecht", "/", "wenn", "mir", "der", "k\u00f6\u00b7nig", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$(", "KOUS", "PPER", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Doch schwer ich/ da\u00df ich ihn nicht als den k\u00f6nig liebe/", "tokens": ["Doch", "schwer", "ich", "/", "da\u00df", "ich", "ihn", "nicht", "als", "den", "k\u00f6\u00b7nig", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "$(", "KOUS", "PPER", "PPER", "PTKNEG", "KOUS", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Mein auge siht auf ihn und seine crone nicht/", "tokens": ["Mein", "au\u00b7ge", "siht", "auf", "ihn", "und", "sei\u00b7ne", "cro\u00b7ne", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die anmuth/ die ihn regt/ ist hier des Adams ribe/", "tokens": ["Die", "an\u00b7muth", "/", "die", "ihn", "regt", "/", "ist", "hier", "des", "A\u00b7dams", "ri\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Die mein verliebter geist aus seiner seiten bricht.", "tokens": ["Die", "mein", "ver\u00b7lieb\u00b7ter", "geist", "aus", "sei\u00b7ner", "sei\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die welt r\u00fchmt f\u00fcrsten wol nach ihren land und leuten/", "tokens": ["Die", "welt", "r\u00fchmt", "f\u00fcrs\u00b7ten", "wol", "nach", "ih\u00b7ren", "land", "und", "leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Nach dem ihr gr\u00e4ntzestein der reiche sich erstreckt/", "tokens": ["Nach", "dem", "ihr", "gr\u00e4nt\u00b7zes\u00b7tein", "der", "rei\u00b7che", "sich", "er\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ART", "ADJA", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und unsre ehrfurcht schaut ihr anmuth nur von weiten/", "tokens": ["Und", "uns\u00b7re", "ehr\u00b7furcht", "schaut", "ihr", "an\u00b7muth", "nur", "von", "wei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Wenn auch der f\u00fcrstenhut der grossen m\u00e4ngel deckt.", "tokens": ["Wenn", "auch", "der", "f\u00fcrs\u00b7ten\u00b7hut", "der", "gros\u00b7sen", "m\u00e4n\u00b7gel", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ich aber lasse mich von keinen Purpur blendem/", "tokens": ["Ich", "a\u00b7ber", "las\u00b7se", "mich", "von", "kei\u00b7nen", "Pur\u00b7pur", "blen\u00b7dem", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "PIAT", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ich nehme die person/ nicht ihren thron/ in acht/", "tokens": ["Ich", "neh\u00b7me", "die", "per\u00b7son", "/", "nicht", "ih\u00b7ren", "thron", "/", "in", "acht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "$(", "PTKNEG", "PPOSAT", "NN", "$(", "APPR", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Sein kleinod und sein reich setz ich aus seinen h\u00e4nden/", "tokens": ["Sein", "klei\u00b7nod", "und", "sein", "reich", "setz", "ich", "aus", "sei\u00b7nen", "h\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.56": {"text": "So sind ich allererst was mich verliebt gemacht.", "tokens": ["So", "sind", "ich", "al\u00b7le\u00b7rerst", "was", "mich", "ver\u00b7liebt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PWS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Mein K\u00f6nig/ w\u00e4r es nicht die billigkeit erz\u00fcrnen/", "tokens": ["Mein", "K\u00f6\u00b7nig", "/", "w\u00e4r", "es", "nicht", "die", "bil\u00b7lig\u00b7keit", "er\u00b7z\u00fcr\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Dem reiche zu entziehn/ was dessen w\u00fcrdig ist/", "tokens": ["Dem", "rei\u00b7che", "zu", "ent\u00b7ziehn", "/", "was", "des\u00b7sen", "w\u00fcr\u00b7dig", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKZU", "VVINF", "$(", "PWS", "PDS", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "So spr\u00e4ch ich: w\u00e4r er doch gezeugt von einer dirnen/", "tokens": ["So", "spr\u00e4ch", "ich", ":", "w\u00e4r", "er", "doch", "ge\u00b7zeugt", "von", "ei\u00b7ner", "dir\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Denn mit bestande wird ein gleicher mund gek\u00fcst.", "tokens": ["Denn", "mit", "be\u00b7stan\u00b7de", "wird", "ein", "glei\u00b7cher", "mund", "ge\u00b7k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Urtheile nun/ mein printz/ wie weit ich mich verstigen/", "tokens": ["Ur\u00b7thei\u00b7le", "nun", "/", "mein", "printz", "/", "wie", "weit", "ich", "mich", "vers\u00b7ti\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.62": {"text": "Und was die liebe nicht f\u00fcr frembde wunder stifft?", "tokens": ["Und", "was", "die", "lie\u00b7be", "nicht", "f\u00fcr", "fremb\u00b7de", "wun\u00b7der", "stifft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Ein Zeifig macht sich auf dem Adler nachzusligen/", "tokens": ["Ein", "Zei\u00b7fig", "macht", "sich", "auf", "dem", "Ad\u00b7ler", "nach\u00b7zus\u00b7li\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und es gelingt ihm auch da\u00df er die sonne trifft.", "tokens": ["Und", "es", "ge\u00b7lingt", "ihm", "auch", "da\u00df", "er", "die", "son\u00b7ne", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Ach m\u00f6chte diese huld auf ewig mir verbleiben/", "tokens": ["Ach", "m\u00f6ch\u00b7te", "die\u00b7se", "huld", "auf", "e\u00b7wig", "mir", "ver\u00b7blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PDAT", "NN", "APPR", "ADJD", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Der himmel der sich regt/ bewegt uns auch mit sich/", "tokens": ["Der", "him\u00b7mel", "der", "sich", "regt", "/", "be\u00b7wegt", "uns", "auch", "mit", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PRF", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "APPR", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Wie leichte kan ein wind ein schiff zur\u00fccke treiben/", "tokens": ["Wie", "leich\u00b7te", "kan", "ein", "wind", "ein", "schiff", "zu\u00b7r\u00fc\u00b7cke", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "VMFIN", "ART", "NN", "ART", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Das erstlich durch die fluht mit vollem segel strich/", "tokens": ["Das", "erst\u00b7lich", "durch", "die", "fluht", "mit", "vol\u00b7lem", "se\u00b7gel", "strich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Die sch\u00f6nheit die er r\u00fchmt an meinen blassen wangen/", "tokens": ["Die", "sch\u00f6n\u00b7heit", "die", "er", "r\u00fchmt", "an", "mei\u00b7nen", "blas\u00b7sen", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Ist nichts als sein betrib/ und w\u00e4r sie gleich auch was/", "tokens": ["Ist", "nichts", "als", "sein", "be\u00b7trib", "/", "und", "w\u00e4r", "sie", "gleich", "auch", "was", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "PPOSAT", "NN", "$(", "KON", "VAFIN", "PPER", "ADV", "ADV", "PWS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Ist dieser fl\u00fcchtge Klee doch alsobald vergangen/", "tokens": ["Ist", "die\u00b7ser", "fl\u00fccht\u00b7ge", "Klee", "doch", "al\u00b7so\u