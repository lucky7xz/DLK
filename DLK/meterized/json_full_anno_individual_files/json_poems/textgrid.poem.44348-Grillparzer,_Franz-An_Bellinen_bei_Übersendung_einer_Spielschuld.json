{"textgrid.poem.44348": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An Bellinen bei \u00dcbersendung einer Spielschuld", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur\u00fcck hier send ich, was du mir geliehen,", "tokens": ["Zu\u00b7r\u00fcck", "hier", "send", "ich", ",", "was", "du", "mir", "ge\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "PPER", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ich dir lieh, ich nahm es schon zur\u00fcck,", "tokens": ["Was", "ich", "dir", "lieh", ",", "ich", "nahm", "es", "schon", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus eignem Reichtum nur kann Segen bl\u00fchen,", "tokens": ["Aus", "eig\u00b7nem", "Reich\u00b7tum", "nur", "kann", "Se\u00b7gen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VMFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erborgtes Gl\u00fcck ist nimmer wahres Gl\u00fcck.", "tokens": ["Er\u00b7borg\u00b7tes", "Gl\u00fcck", "ist", "nim\u00b7mer", "wah\u00b7res", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Reichen mag man noch sich schuldig wissen,", "tokens": ["Dem", "Rei\u00b7chen", "mag", "man", "noch", "sich", "schul\u00b7dig", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Armen sei sein Pfennig unber\u00fchrt.", "tokens": ["Dem", "Ar\u00b7men", "sei", "sein", "Pfen\u00b7nig", "un\u00b7be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hier ist die Schuld, der Schuldbrief liegt zerrissen;", "tokens": ["Hier", "ist", "die", "Schuld", ",", "der", "Schuld\u00b7brief", "liegt", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Frei geh ein jedes, wie der Weg ihn f\u00fchrt.", "tokens": ["Frei", "geh", "ein", "je\u00b7des", ",", "wie", "der", "Weg", "ihn", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PIAT", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zur\u00fcck hier send ich, was du mir geliehen,", "tokens": ["Zu\u00b7r\u00fcck", "hier", "send", "ich", ",", "was", "du", "mir", "ge\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "PPER", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ich dir lieh, ich nahm es schon zur\u00fcck,", "tokens": ["W