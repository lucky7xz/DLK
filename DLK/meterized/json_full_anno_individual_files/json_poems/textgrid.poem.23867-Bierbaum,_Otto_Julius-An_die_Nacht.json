{"textgrid.poem.23867": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "An die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00fcfteschw\u00fcle, feuchteschwere,", "tokens": ["D\u00fcf\u00b7tesc\u00b7hw\u00fc\u00b7le", ",", "feuch\u00b7te\u00b7schwe\u00b7re", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauschende, raunende, sterneleere,", "tokens": ["Rau\u00b7schen\u00b7de", ",", "rau\u00b7nen\u00b7de", ",", "ster\u00b7ne\u00b7lee\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schwarze, samtene Sommernacht!", "tokens": ["Schwar\u00b7ze", ",", "sam\u00b7te\u00b7ne", "Som\u00b7mer\u00b7nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mein Herz lauscht an deines bange,", "tokens": ["Mein", "Herz", "lauscht", "an", "dei\u00b7nes", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Nimm von mir, was mich so lange", "tokens": ["Nimm", "von", "mir", ",", "was", "mich", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "$,", "PWS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fcde hat gemacht.", "tokens": ["M\u00fc\u00b7de", "hat", "ge\u00b7macht", "."], "token_info":