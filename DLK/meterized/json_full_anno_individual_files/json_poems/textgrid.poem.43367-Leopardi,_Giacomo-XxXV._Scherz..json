{"textgrid.poem.43367": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XxXV. Scherz.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Knabe zu den Musen", "tokens": ["Als", "Kna\u00b7be", "zu", "den", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam ich und wollt' in ihre Lehre gehen.", "tokens": ["Kam", "ich", "und", "wollt'", "in", "ih\u00b7re", "Leh\u00b7re", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihrer Eine nahm mich bei der Hand", "tokens": ["Und", "ih\u00b7rer", "Ei\u00b7ne", "nahm", "mich", "bei", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und blieb mir zum Geleite", "tokens": ["Und", "blieb", "mir", "zum", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den ganzen Tag zur Seite,", "tokens": ["Den", "gan\u00b7zen", "Tag", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Werkstatt zu besehen", "tokens": ["Die", "Werk\u00b7statt", "zu", "be\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sammt allen Kunstger\u00e4then,", "tokens": ["Sammt", "al\u00b7len", "Kunst\u00b7ge\u00b7r\u00e4\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die n\u00f6thig den Poeten.", "tokens": ["Die", "n\u00f6\u00b7thig", "den", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.9": {"text": "Sie zeigte mir von allen", "tokens": ["Sie", "zeig\u00b7te", "mir", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Den Nutzen und Gebrauch,", "tokens": ["Den", "Nut\u00b7zen", "und", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Zu Vers und Prosa auch", "tokens": ["Zu", "Vers", "und", "Pro\u00b7sa", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Zu dienen nach Gefallen.", "tokens": ["Zu", "die\u00b7nen", "nach", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ich aber schaut' und fragte:", "tokens": ["Ich", "a\u00b7ber", "schaut'", "und", "frag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Muse, wo ist die Feile? \u2013 und sie sagte:"