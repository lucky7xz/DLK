{"dta.poem.12263": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Eile der Zeit in Gott .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Commandant zu Groswardeyn,               ", "tokens": ["Der", "Com\u00b7man\u00b7dant", "zu", "Gros\u00b7war\u00b7deyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00e4tt' ein einzig T\u00f6chterlein,", "tokens": ["Der", "h\u00e4tt'", "ein", "ein\u00b7zig", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Theresia ihr Nahmen war,", "tokens": ["The\u00b7re\u00b7sia", "ihr", "Nah\u00b7men", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott'sf\u00fcrchtig, z\u00fcchtig, keusch und klar.", "tokens": ["Gott's\u00b7f\u00fcrch\u00b7tig", ",", "z\u00fcch\u00b7tig", ",", "keusch", "und", "klar", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie war von ihrer Jugend an", "tokens": ["Sie", "war", "von", "ih\u00b7rer", "Ju\u00b7gend", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Andacht also zugethan,", "tokens": ["Der", "An\u00b7dacht", "al\u00b7so", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Beten, Singen allezeit", "tokens": ["Mit", "Be\u00b7ten", ",", "Sin\u00b7gen", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lobt sie die heilig' Dreifaltigkeit.", "tokens": ["Lobt", "sie", "die", "hei\u00b7lig'", "Drei\u00b7fal\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn sie nur Jesum nennen h\u00f6rt,", "tokens": ["Wenn", "sie", "nur", "Je\u00b7sum", "nen\u00b7nen", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So wurd ihr Lieb und Freud vermehrt,", "tokens": ["So", "wurd", "ihr", "Lieb", "und", "Freud", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Jesum war ihr Thun gericht,", "tokens": ["Auf", "Je\u00b7sum", "war", "ihr", "Thun", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seiner Braut sie sich verpflicht.", "tokens": ["Zu", "sei\u00b7ner", "Braut", "sie", "sich", "ver\u00b7pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein edler Herr th\u00e4t um sie freyn,", "tokens": ["Ein", "ed\u00b7ler", "Herr", "th\u00e4t", "um", "sie", "freyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vater gab den Willen drein", "tokens": ["Der", "Va\u00b7ter", "gab", "den", "Wil\u00b7len", "drein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mutter zu der Tochter spricht:", "tokens": ["Die", "Mut\u00b7ter", "zu", "der", "Toch\u00b7ter", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201emein Kind, nur diesen lasse nicht.\u201c", "tokens": ["\u201e", "mein", "Kind", ",", "nur", "die\u00b7sen", "las\u00b7se", "nicht", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ADV", "PDS", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Tochter sprach: \u201eAch Mutter mein!", "tokens": ["Die", "Toch\u00b7ter", "sprach", ":", "\u201e", "Ach", "Mut\u00b7ter", "mein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ITJ", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edas kann und mag ja nicht so seyn,", "tokens": ["\u201e", "das", "kann", "und", "mag", "ja", "nicht", "so", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "KON", "VMFIN", "ADV", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201emein Br\u00e4utigam ist schon bestellt,", "tokens": ["\u201e", "mein", "Br\u00e4u\u00b7ti\u00b7gam", "ist", "schon", "be\u00b7stellt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NE", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ederselb' ist nicht auf dieser Welt.\u201c", "tokens": ["\u201e", "der\u00b7selb'", "ist", "nicht", "auf", "die\u00b7ser", "Welt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "PTKNEG", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Mutter sprach: \u201eAch Tochter mein!", "tokens": ["Die", "Mut\u00b7ter", "sprach", ":", "\u201e", "Ach", "Toch\u00b7ter", "mein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ITJ", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201each thu uns nicht zuwider seyn!", "tokens": ["\u201e", "ach", "thu", "uns", "nicht", "zu\u00b7wi\u00b7der", "seyn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewir sind nunmehr zwey alte Leut,", "tokens": ["\u201e", "wir", "sind", "nun\u00b7mehr", "zwey", "al\u00b7te", "Leut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201emit Geld hat uns Gott auch erfreut.\u201c", "tokens": ["\u201e", "mit", "Geld", "hat", "uns", "Gott", "auch", "er\u00b7freut", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VAFIN", "PPER", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Die Tochter fing zu weinen an:", "tokens": ["Die", "Toch\u00b7ter", "fing", "zu", "wei\u00b7nen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich hab schon einen Br\u00e4utigam,", "tokens": ["\u201e", "ich", "hab", "schon", "ei\u00b7nen", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edem ich mich hab versprochen ganz,", "tokens": ["\u201e", "dem", "ich", "mich", "hab", "ver\u00b7spro\u00b7chen", "ganz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ezu tragen meinen Jungfernkranz.\u201c", "tokens": ["\u201e", "zu", "tra\u00b7gen", "mei\u00b7nen", "Jung\u00b7fern\u00b7kranz", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKZU", "VVINF", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Vater sprach: \u201eEs kann nicht seyn,", "tokens": ["Der", "Va\u00b7ter", "sprach", ":", "\u201e", "Es", "kann", "nicht", "seyn", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emein Kind, das bilde dir nicht ein,", "tokens": ["\u201e", "mein", "Kind", ",", "das", "bil\u00b7de", "dir", "nicht", "ein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewo willt du bleiben mit der Zeit,", "tokens": ["\u201e", "wo", "willt", "du", "blei\u00b7ben", "mit", "der", "Zeit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201esehr alt sind wir schon alle beyd.\u201c", "tokens": ["\u201e", "sehr", "alt", "sind", "wir", "schon", "al\u00b7le", "beyd", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der edle Herr bald wieder kam,", "tokens": ["Der", "ed\u00b7le", "Herr", "bald", "wie\u00b7der", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stellte man die Hochzeit an,", "tokens": ["Da", "stell\u00b7te", "man", "die", "Hoch\u00b7zeit", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn alles war voraus bereit,", "tokens": ["Denn", "al\u00b7les", "war", "vo\u00b7raus", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Braut war voller Traurigkeit.", "tokens": ["Die", "Braut", "war", "vol\u00b7ler", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sie ging in ihren Garten fr\u00fch,", "tokens": ["Sie", "ging", "in", "ih\u00b7ren", "Gar\u00b7ten", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da fiel sie nieder auf die Knie,", "tokens": ["Da", "fiel", "sie", "nie\u00b7der", "auf", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie rief von ganzem Herzen an", "tokens": ["Sie", "rief", "von", "gan\u00b7zem", "Her\u00b7zen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jesum, ihren liebsten Br\u00e4utigam.", "tokens": ["Je\u00b7sum", ",", "ih\u00b7ren", "liebs\u00b7ten", "Br\u00e4u\u00b7ti\u00b7gam", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Sie lag auf ihrem Angesicht,", 