{"textgrid.poem.65110": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon glaubt' ich meines Herzens Schl\u00e4ge", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon glaubt' ich meines Herzens Schl\u00e4ge", "tokens": ["Schon", "glaubt'", "ich", "mei\u00b7nes", "Her\u00b7zens", "Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschwichtigt und zur Ruh gebracht,", "tokens": ["Be\u00b7schwich\u00b7tigt", "und", "zur", "Ruh", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon glaubt' ich, \u00fcberwunden l\u00e4ge", "tokens": ["Schon", "glaubt'", "ich", ",", "\u00fc\u00b7berw\u00b7un\u00b7den", "l\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Staub vor mir des Lebens Macht,", "tokens": ["Im", "Staub", "vor", "mir", "des", "Le\u00b7bens", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verachtend blickt ich auf die Klage,", "tokens": ["Ver\u00b7ach\u00b7tend", "blickt", "ich", "auf", "die", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kalt l\u00e4chelnd auf versunk'nes Gl\u00fcck,", "tokens": ["Kalt", "l\u00e4\u00b7chelnd", "auf", "ver\u00b7sunk'\u00b7nes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und das Ged\u00e4chtni\u00df fr\u00fch'rer Tage", "tokens": ["Und", "das", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "fr\u00fch'\u00b7rer", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wich scheu vor meinem Stolz zur\u00fcck.", "tokens": ["Wich", "scheu", "vor", "mei\u00b7nem", "Stolz", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So w\u00e4hnt' ich mich geheilt, genesen,", "tokens": ["So", "w\u00e4hnt'", "ich", "mich", "ge\u00b7heilt", ",", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinwallend auf erlauchter Spur,", "tokens": ["Hin\u00b7wal\u00b7lend", "auf", "er\u00b7lauch\u00b7ter", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gel\u00e4utert und ger\u00e4umt mein Wesen", "tokens": ["Ge\u00b7l\u00e4u\u00b7tert", "und", "ge\u00b7r\u00e4umt", "mein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Schlacken irdischer Natur.", "tokens": ["Von", "Schla\u00b7cken", "ir\u00b7di\u00b7scher", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie stieg das Blut in meine Wangen,", "tokens": ["Wie", "stieg", "das", "Blut", "in", "mei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie strahlte im Triumph mein Blick!", "tokens": ["Wie", "strahl\u00b7te", "im", "Tri\u00b7umph", "mein", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Bew\u00e4ltigt hatt' ich Wunsch und Bangen,", "tokens": ["Be\u00b7w\u00e4l\u00b7tigt", "hatt'", "ich", "Wunsch", "und", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Wille nur, war mein Geschick.", "tokens": ["Mein", "Wil\u00b7le", "nur", ",", "war", "mein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So war's noch gestern. Wie nun heute?", "tokens": ["So", "wa\u00b7r's", "noch", "ge\u00b7stern", ".", "Wie", "nun", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVINF", "$.", "PWAV", "ADV", "ADV", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welch dunkler Bann hat mich ber\u00fchrt,", "tokens": ["Welch", "dunk\u00b7ler", "Bann", "hat", "mich", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die ihm abgerung'ne Beute", "tokens": ["Und", "die", "ihm", "ab\u00b7ge\u00b7rung'\u00b7ne", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Schmerze wieder zugef\u00fchrt?", "tokens": ["Dem", "Schmer\u00b7ze", "wie\u00b7der", "zu\u00b7ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Purpur, deckend Bettlerbl\u00f6\u00dfe!", "tokens": ["O", "Pur\u00b7pur", ",", "de\u00b7ckend", "Bett\u00b7ler\u00b7bl\u00f6\u00b7\u00dfe", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Kronenreif von Glitzerschaum!", "tokens": ["O", "Kro\u00b7nen\u00b7reif", "von", "Glit\u00b7zer\u00b7schaum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O wahngetr\u00e4umte Herrschergr\u00f6\u00dfe \u2013", "tokens": ["O", "wahn\u00b7ge\u00b7tr\u00e4um\u00b7te", "Herr\u00b7scher\u00b7gr\u00f6\u00b7\u00dfe", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Besiegt, zerst\u00f6rt hat euch ein Traum!", "tokens": ["Be\u00b7siegt", ",", "zer\u00b7st\u00f6rt", "hat", "euch", "ein", "Traum", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Traum, de\u00df finst'rer Zaubersegen", "tokens": ["Ein", "Traum", ",", "de\u00df", "finst'\u00b7rer", "Zau\u00b7ber\u00b7se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Fesseln meuchlings mich umreift.", "tokens": ["Mit", "Fes\u00b7seln", "meuch\u00b7lings", "mich", "um\u00b7reift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er brach herein, wie R\u00e4uber pflegen,", "tokens": ["Er", "brach", "her\u00b7ein", ",", "wie", "R\u00e4u\u00b7ber", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich die R\u00fcstung abgestreift.", "tokens": ["Als", "ich", "die", "R\u00fcs\u00b7tung", "ab\u00b7ge\u00b7streift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er brach herein bei n\u00e4cht'ger Stille,", "tokens": ["Er", "brach", "her\u00b7ein", "bei", "n\u00e4cht'\u00b7ger", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vampyrhaft saugte er mein Blut,", "tokens": ["Vam\u00b7pyr\u00b7haft", "saug\u00b7te", "er", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als schlimme W\u00e4chter, Geist und Wille", "tokens": ["Als", "schlim\u00b7me", "W\u00e4ch\u00b7ter", ",", "Geist", "und", "Wil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ersch\u00f6pft vom Tageswerk geruht.", "tokens": ["Er\u00b7sch\u00f6pft", "vom", "Ta\u00b7ges\u00b7werk", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die in mir so lange schliefen,", "tokens": ["Und", "die", "in", "mir", "so", "lan\u00b7ge", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die alte Lieb', das alte Leid,", "tokens": ["Die", "al\u00b7te", "Lieb'", ",", "das", "al\u00b7te", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie stiegen aus des Grabes Tiefen", "tokens": ["Sie", "stie\u00b7gen", "aus", "des", "Gra\u00b7bes", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihm erweckt, durch ihn befreit.", "tokens": ["Von", "ihm", "er\u00b7weckt", ",", "durch", "ihn", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sangen ihre Schmerzenlieder,", "tokens": ["Sie", "san\u00b7gen", "ih\u00b7re", "Schmer\u00b7zen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie winkten mit der Geisterhand,", "tokens": ["Sie", "wink\u00b7ten", "mit", "der", "Geis\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und aus der Asche schlugen wieder", "tokens": ["Und", "aus", "der", "A\u00b7sche", "schlu\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Flammen auf in wildem Brand.", "tokens": ["Die", "Flam\u00b7men", "auf", "in", "wil\u00b7dem", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da, pl\u00f6tzlich aus dem wirren Grund,", "tokens": ["Da", ",", "pl\u00f6tz\u00b7lich", "aus", "dem", "wir\u00b7ren", "Grund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rang sich ein Bild gewitterklar,", "tokens": ["Rang", "sich", "ein", "Bild", "ge\u00b7wit\u00b7ter\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Du tratst vor mich, wie in der Stunde,", "tokens": ["Du", "tratst", "vor", "mich", ",", "wie", "in", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die meines Friedens letzte war.", "tokens": ["Die", "mei\u00b7nes", "Frie\u00b7dens", "letz\u00b7te", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sind uns fremd im Leben, Sterben,", "tokens": ["Wir", "sind", "uns", "fremd", "im", "Le\u00b7ben", ",", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir haben f\u00fcrder nichts gemein,", "tokens": ["Wir", "ha\u00b7ben", "f\u00fcr\u00b7der", "nichts", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was dr\u00e4ngst du, ganz mich zu verderben.", "tokens": ["Was", "dr\u00e4ngst", "du", ",", "ganz", "mich", "zu", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADV", "PPER", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dich nun in meine Tr\u00e4ume ein?", "tokens": ["Dich", "nun", "in", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was nahst du mir mit fleh'ndem Munde,", "tokens": ["Was", "nahst", "du", "mir", "mit", "fleh'n\u00b7dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was blickt dein Aug' so schmerzenwild,", "tokens": ["Was", "blickt", "dein", "Aug'", "so", "schmer\u00b7zen\u00b7wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df aus der schon vernarbten Wunde", "tokens": ["Da\u00df", "aus", "der", "schon", "ver\u00b7narb\u00b7ten", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf's neu' der alte Blutstrom quillt?", "tokens": ["Auf's", "neu'", "der", "al\u00b7te", "Bluts\u00b7trom", "quillt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was mu\u00dft du mir die Kunde bringen", "tokens": ["Was", "mu\u00dft", "du", "mir", "die", "Kun\u00b7de", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit deinem tr\u00fcben Seelengru\u00df,", "tokens": ["Mit", "dei\u00b7nem", "tr\u00fc\u00b7ben", "See\u00b7len\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "