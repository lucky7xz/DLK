tra"}}, "stanza.10": {"line.1": {"text": "So w\u00e4hnt' ich mich geheilt, genesen,", "tokens": ["So", "w\u00e4hnt'", "ich", "mich", "ge\u00b7heilt", ",", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinwallend auf erlauchter Spur,", "tokens": ["Hin\u00b7wal\u00b7lend", "auf", "er\u00b7lauch\u00b7ter", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gel\u00e4utert und ger\u00e4umt mein Wesen", "tokens": ["Ge\u00b7l\u00e4u\u00b7tert", "und", "ge\u00b7r\u00e4umt", "mein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Schlacken irdischer Natur.", "tokens": ["Von", "Schla\u00b7cken", "ir\u00b7di\u00b7scher", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie stieg das Blut in meine Wangen,", "tokens": ["Wie", "stieg", "das", "Blut", "in", "mei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie strahlte im Triumph mein Blick!", "tokens": ["Wie", "strahl\u00b7te", "im", "Tri\u00b7umph", "mein", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Bew\u00e4ltigt hatt' ich Wunsch und Bang