{"dta.poem.6920": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Menschen zeiget GOTT der HERR,", "tokens": ["Jm", "Men\u00b7schen", "zei\u00b7get", "GoTT", "der", "HeRR", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Er", "tokens": ["Wie", "Er"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die wunderbarlichste Machin\u2019 hervorgebracht,", "tokens": ["Die", "wun\u00b7der\u00b7bar\u00b7lichs\u00b7te", "Ma\u00b7chin'", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Worinn von G\u00f6ttlichkeit ein Ausflu\u00df klar erscheint,", "tokens": ["Wo\u00b7rinn", "von", "G\u00f6tt\u00b7lich\u00b7keit", "ein", "Aus\u00b7flu\u00df", "klar", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sich, durch Mittel, die der Macht", "tokens": ["Der", "sich", ",", "durch", "Mit\u00b7tel", ",", "die", "der", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PRF", "$,", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der GOTTHEJT w\u00fcrdig sind, darinn vereint,", "tokens": ["Der", "GoTTHEJT", "w\u00fcr\u00b7dig", "sind", ",", "da\u00b7rinn", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$,", "PAV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die unser Geist, als der umschr\u00e4nckt,", "tokens": ["Die", "un\u00b7ser", "Geist", ",", "als", "der", "um\u00b7schr\u00e4nckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "KOUS", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht fassen kan. Da GOTT die Seelen,", "tokens": ["Nicht", "fas\u00b7sen", "kan", ".", "Da", "GoTT", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$.", "KOUS", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Um mit dem leiblichen was geistigs zu verm\u00e4hlen,", "tokens": ["Um", "mit", "dem", "leib\u00b7li\u00b7chen", "was", "geis\u00b7tigs", "zu", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "PWS", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In C\u00f6rper eingesenckt;", "tokens": ["In", "C\u00f6r\u00b7per", "ein\u00b7ge\u00b7senckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wie die Empfindlichkeit f\u00fcr sie allein", "tokens": ["Wie", "die", "Emp\u00b7find\u00b7lich\u00b7keit", "f\u00fcr", "sie", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nur sollte seyn;", "tokens": ["Nur", "soll\u00b7te", "seyn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "So wollt\u2019 er, da\u00df ein Druck, den C\u00f6rper zu bewegen,", "tokens": ["So", "wollt'", "er", ",", "da\u00df", "ein", "Druck", ",", "den", "C\u00f6r\u00b7per", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sollt\u2019 in der Seel ein schnell Gef\u00fchl erregen.", "tokens": ["Sollt'", "in", "der", "Seel", "ein", "schnell", "Ge\u00b7f\u00fchl", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Er wollte Leib und Seel auf diese Art verbinden,", "tokens": ["Er", "woll\u00b7te", "Leib", "und", "Seel", "auf", "die\u00b7se", "Art", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df, wenn man unsern Leib auf solche Art ber\u00fchrte,", "tokens": ["Da\u00df", ",", "wenn", "man", "un\u00b7sern", "Leib", "auf", "sol\u00b7che", "Art", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Man Farben, Hitz und Frost, auch Licht und Ton versp\u00fcrte.", "tokens": ["Man", "Far\u00b7ben", ",", "Hitz", "und", "Frost", ",", "auch", "Licht", "und", "Ton", "ver\u00b7sp\u00fcr\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da doch ein jeder Druck, der r\u00fcckwerts f\u00e4llt,", "tokens": ["Da", "doch", "ein", "je\u00b7der", "Druck", ",", "der", "r\u00fcck\u00b7werts", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Von Farben, T\u00f6nen, Licht, von Hitz und K\u00e4lt,", "tokens": ["Von", "Far\u00b7ben", ",", "T\u00f6\u00b7nen", ",", "Licht", ",", "von", "Hitz", "und", "K\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So fern als man dieselbigen erkennt,", "tokens": ["So", "fern", "als", "man", "die\u00b7sel\u00b7bi\u00b7gen", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PIS", "PDS", "VVFIN", "$,"], "meter": "-+--+----+", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Von der Materie sich trennt.", "tokens": ["Von", "der", "Ma\u00b7te\u00b7rie", "sich", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Ohn da\u00df sich \u00e4userlich nur das geringste weis\u2019t:", "tokens": ["Ohn", "da\u00df", "sich", "\u00e4u\u00b7ser\u00b7lich", "nur", "das", "ge\u00b7rings\u00b7te", "weis't", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "ADJD", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So zeigt blo\u00df die Jdee, so einfach, sich dem Geist.", "tokens": ["So", "zeigt", "blo\u00df", "die", "Jdee", ",", "so", "ein\u00b7fach", ",", "sich", "dem", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "$,", "PRF", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}