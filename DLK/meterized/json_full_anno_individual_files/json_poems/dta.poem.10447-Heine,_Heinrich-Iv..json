{"dta.poem.10447": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im Walde wandl' ich und weine,", "tokens": ["Im", "Wal\u00b7de", "wandl'", "ich", "und", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Drossel sitzt in der H\u00f6h';", "tokens": ["Die", "Dros\u00b7sel", "sitzt", "in", "der", "H\u00f6h'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie springt und singt gar feine:", "tokens": ["Sie", "springt", "und", "singt", "gar", "fei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warum ist dir so weh?", "tokens": ["Wa\u00b7rum", "ist", "dir", "so", "weh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201edie Schwalben, deine Schwestern,", "tokens": ["\u201e", "die", "Schwal\u00b7ben", ",", "dei\u00b7ne", "Schwes\u00b7tern", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die k\u00f6nnen's dir sagen, mein Kind;", "tokens": ["Die", "k\u00f6n\u00b7nen's", "dir", "sa\u00b7gen", ",", "mein", "Kind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie wohnten in klugen Nestern,", "tokens": ["Sie", "wohn\u00b7ten", "in", "klu\u00b7gen", "Nes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo Liebchens Fenster sind.\u201c", "tokens": ["Wo", "Lieb\u00b7chens", "Fens\u00b7ter", "sind", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NE", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}