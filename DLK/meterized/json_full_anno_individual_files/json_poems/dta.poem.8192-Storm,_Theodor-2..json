{"dta.poem.8192": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6ner Staatskalenderer;", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Staats\u00b7ka\u00b7len\u00b7de\u00b7rer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch sieht denn nicht ein Jeder,", "tokens": ["Doch", "sieht", "denn", "nicht", "ein", "Je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ART", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er gen\u00e4ht aus Leder?", "tokens": ["Da\u00df", "er", "ge\u00b7n\u00e4ht", "aus", "Le\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kommt nur der rechte Regentropf,", "tokens": ["Kommt", "nur", "der", "rech\u00b7te", "Re\u00b7gen\u00b7tropf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00e4scht die Nummer ihm vom Kopf,", "tokens": ["Und", "w\u00e4scht", "die", "Num\u00b7mer", "ihm", "vom", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ruft gewi\u00df ein Jeder:", "tokens": ["So", "ruft", "ge\u00b7wi\u00df", "ein", "Je\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr Gott, ein Kerl von Leder!", "tokens": ["Herr", "Gott", ",", "ein", "Kerl", "von", "Le\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}