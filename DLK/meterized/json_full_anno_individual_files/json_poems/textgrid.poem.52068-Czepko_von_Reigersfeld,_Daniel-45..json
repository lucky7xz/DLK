{"textgrid.poem.52068": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Fulvia, als ihr geleget ward zun F\u00fc\u00dfen", "tokens": ["Wie", "Ful\u00b7via", ",", "als", "ihr", "ge\u00b7le\u00b7get", "ward", "zun", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "APPRART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des R\u00f6mschen Redners Haubt, die Zunge raus geri\u00dfen,", "tokens": ["Des", "R\u00f6m\u00b7schen", "Red\u00b7ners", "Haubt", ",", "die", "Zun\u00b7ge", "raus", "ge\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sie an einen Pfahl gehefftet auff der Bahn,", "tokens": ["Und", "sie", "an", "ei\u00b7nen", "Pfahl", "ge\u00b7heff\u00b7tet", "auff", "der", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um, da\u00df sie den Anton zuvor gestochen an.", "tokens": ["Um", ",", "da\u00df", "sie", "den", "An\u00b7ton", "zu\u00b7vor", "ge\u00b7sto\u00b7chen", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "KOUS", "PPER", "ART", "NE", "ADV", "VVPP", "PTKVZ", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So hastu auch, in dem er dich an deinen Ehren,", "tokens": ["So", "has\u00b7tu", "auch", ",", "in", "dem", "er", "dich", "an", "dei\u00b7nen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "APPR", "PRELS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O keusche Lucia, so f\u00e4lschlich wil versehren,", "tokens": ["O", "keu\u00b7sche", "Lu\u00b7cia", ",", "so", "f\u00e4lschlich", "wil", "ver\u00b7seh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NE", "$,", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dergleichen Rach und Pein dem Vicebock gedra\u00fct,", "tokens": ["Derg\u00b7lei\u00b7chen", "Rach", "und", "Pein", "dem", "Vi\u00b7ce\u00b7bock", "ge\u00b7dr\u00b7a\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil seiner Zungen Gifft noch Gott noch Menschen scheut.", "tokens": ["Weil", "sei\u00b7ner", "Zun\u00b7gen", "Gifft", "noch", "Gott", "noch", "Men\u00b7schen", "scheut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Halt an, o Lucia, ob du in deinem Leiden", "tokens": ["Halt", "an", ",", "o", "Lu\u00b7cia", ",", "ob", "du", "in", "dei\u00b7nem", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NE", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ihm nicht die Zunge siehst aus seinen Rachen schneiden,", "tokens": ["Ihm", "nicht", "die", "Zun\u00b7ge", "siehst", "aus", "sei\u00b7nen", "Ra\u00b7chen", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch selbst durchbohren kanst; o Lucia halt an,", "tokens": ["Noch", "selbst", "durch\u00b7boh\u00b7ren", "kanst", ";", "o", "Lu\u00b7cia", "halt", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$.", "FM", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "O s\u00fc\u00dfe Rach! es hats die Gicht vor dich gethan.", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Rach", "!", "es", "hats", "die", "Gicht", "vor", "dich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie Fulvia, als ihr geleget ward zun F\u00fc\u00dfen", "tokens": ["Wie", "Ful\u00b7via", ",", "als", "ihr", "ge\u00b7le\u00b7get", "ward", "zun", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "APPRART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des R\u00f