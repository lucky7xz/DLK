{"dta.poem.20029": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf die aus den Kirchen weggebrachte altdeutsche  \n Gem\u00e4hlde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wollt bald alle wiederkehren", "tokens": ["Wollt", "bald", "al\u00b7le", "wie\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fromme Kinder deutscher Art!", "tokens": ["From\u00b7me", "Kin\u00b7der", "deut\u00b7scher", "Art", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den W\u00e4nden, ach! den leeren!", "tokens": ["An", "den", "W\u00e4n\u00b7den", ",", "ach", "!", "den", "lee\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ITJ", "$.", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Platz euch aufbewahrt.", "tokens": ["Ist", "ein", "Platz", "euch", "auf\u00b7be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weggeschleppt aus frommen Hallen", "tokens": ["Weg\u00b7ge\u00b7schleppt", "aus", "from\u00b7men", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's euch heimatlos und bang,", "tokens": ["Ist's", "euch", "hei\u00b7mat\u00b7los", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es kann euch nicht gefallen,", "tokens": ["Und", "es", "kann", "euch", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo nicht Duft und Orgelklang.", "tokens": ["Wo", "nicht", "Duft", "und", "Or\u00b7gel\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt ihr ferner Dome L\u00e4uten?", "tokens": ["H\u00f6rt", "ihr", "fer\u00b7ner", "Do\u00b7me", "L\u00e4u\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie traurend seht ihr aus!", "tokens": ["O", "wie", "trau\u00b7rend", "seht", "ihr", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja! euch ist's wie kranken Br\u00e4uten", "tokens": ["Ja", "!", "euch", "ist's", "wie", "kran\u00b7ken", "Br\u00e4u\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern vom lieben Mutterhaus.", "tokens": ["Fern", "vom", "lie\u00b7ben", "Mut\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr in prunkenden Gem\u00e4chern!", "tokens": ["Ihr", "in", "prun\u00b7ken\u00b7den", "Ge\u00b7m\u00e4\u00b7chern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euer Blick er macht nur Schmerz,", "tokens": ["Eu\u00b7er", "Blick", "er", "macht", "nur", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr unter morschen D\u00e4chern!", "tokens": ["Und", "ihr", "un\u00b7ter", "mor\u00b7schen", "D\u00e4\u00b7chern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr zerrei\u00dft des Pilgers Herz. \u2014", "tokens": ["Ihr", "zer\u00b7rei\u00dft", "des", "Pil\u00b7gers", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seht an manchen \u00fcpp'gen Stellen", "tokens": ["Seht", "an", "man\u00b7chen", "\u00fcpp'\u00b7gen", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoch auf Bergen, tief im Thal,", "tokens": ["Hoch", "auf", "Ber\u00b7gen", ",", "tief", "im", "Thal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Winken freundliche Kapellen,", "tokens": ["Win\u00b7ken", "freund\u00b7li\u00b7che", "Ka\u00b7pel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch im Innern sind sie kahl.", "tokens": ["Doch", "im", "In\u00b7nern", "sind", "sie", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kommt und f\u00fcllt verlass'ne Mauern,", "tokens": ["Kommt", "und", "f\u00fcllt", "ver\u00b7lass'\u00b7ne", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' der lezte Stein vergeht,", "tokens": ["Eh'", "der", "lez\u00b7te", "Stein", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Winde kaltes Schauern", "tokens": ["Und", "der", "Win\u00b7de", "kal\u00b7tes", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Heil'gen Asche weht!", "tokens": ["Durch", "der", "Heil'\u00b7gen", "A\u00b7sche", "weht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "F\u00fcllt die Nischen, die Alt\u00e4re,", "tokens": ["F\u00fcllt", "die", "Ni\u00b7schen", ",", "die", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deckt die wei\u00dfget\u00fcnchte Wand!", "tokens": ["Deckt", "die", "wei\u00df\u00b7ge\u00b7t\u00fcnch\u00b7te", "Wand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der K\u00fcnstler find' und ehre", "tokens": ["Und", "der", "K\u00fcnst\u00b7ler", "find'", "und", "eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch allw\u00e4rts im deutschen Land.", "tokens": ["Euch", "all\u00b7w\u00e4rts", "im", "deut\u00b7schen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}