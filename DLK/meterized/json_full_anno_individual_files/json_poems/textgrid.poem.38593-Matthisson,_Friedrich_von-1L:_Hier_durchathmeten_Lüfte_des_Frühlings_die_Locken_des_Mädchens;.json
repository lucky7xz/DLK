{"textgrid.poem.38593": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier durchathmeten L\u00fcfte des Fr\u00fchlings die Locken des M\u00e4dchens;", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier durchathmeten L\u00fcfte des Fr\u00fchlings die Locken des M\u00e4dchens;", "tokens": ["Hier", "durch\u00b7ath\u00b7me\u00b7ten", "L\u00fcf\u00b7te", "des", "Fr\u00fch\u00b7lings", "die", "Lo\u00b7cken", "des", "M\u00e4d\u00b7chens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Hier umwallte das Gras spielend ihr weisses Gewand;", "tokens": ["Hier", "um\u00b7wall\u00b7te", "das", "Gras", "spie\u00b7lend", "ihr", "weis\u00b7ses", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hier umschlang sie der selige J\u00fcngling, bekr\u00e4nzt mit den Bl\u00fcthen", "tokens": ["Hier", "um\u00b7schlang", "sie", "der", "se\u00b7li\u00b7ge", "J\u00fcng\u00b7ling", ",", "be\u00b7kr\u00e4nzt", "mit", "den", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Sp\u00e4ter Erh\u00f6rung, zuerst unter dem d\u00e4mmernden Baum!", "tokens": ["Sp\u00e4\u00b7ter", "Er\u00b7h\u00f6\u00b7rung", ",", "zu\u00b7erst", "un\u00b7ter", "dem