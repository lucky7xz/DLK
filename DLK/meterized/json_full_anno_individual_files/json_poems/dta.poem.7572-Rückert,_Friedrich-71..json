{"dta.poem.7572": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kam, ich wei\u00df nicht wie, zu dieser Siedelei,", "tokens": ["Ich", "kam", ",", "ich", "wei\u00df", "nicht", "wie", ",", "zu", "die\u00b7ser", "Sie\u00b7de\u00b7lei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertrieben und entflohn, gen\u00f6thiget und frei.", "tokens": ["Ver\u00b7trie\u00b7ben", "und", "ent\u00b7flohn", ",", "ge\u00b7n\u00f6t\u00b7hi\u00b7get", "und", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich nicht gerne kam, will ich doch gerne bleiben,", "tokens": ["Wenn", "ich", "nicht", "ger\u00b7ne", "kam", ",", "will", "ich", "doch", "ger\u00b7ne", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will, hergetrieben, mich von hier nicht lassen treiben.", "tokens": ["Will", ",", "her\u00b7ge\u00b7trie\u00b7ben", ",", "mich", "von", "hier", "nicht", "las\u00b7sen", "trei\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VVPP", "$,", "PRF", "APPR", "ADV", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bin angewurzelt, angewachsen; rei\u00dft nicht aus", "tokens": ["Bin", "an\u00b7ge\u00b7wur\u00b7zelt", ",", "an\u00b7ge\u00b7wach\u00b7sen", ";", "rei\u00dft", "nicht", "aus"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "VVPP", "$.", "VVFIN", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Pflanz' aus ihrem Grund, die Schneck' aus ihrem Haus!", "tokens": ["Die", "Pflanz'", "aus", "ih\u00b7rem", "Grund", ",", "die", "Schneck'", "aus", "ih\u00b7rem", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}