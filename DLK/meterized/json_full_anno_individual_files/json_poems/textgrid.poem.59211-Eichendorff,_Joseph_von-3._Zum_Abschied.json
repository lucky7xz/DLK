{"textgrid.poem.59211": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "3. Zum Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horcht! die Stunde hat geschlagen,", "tokens": ["Horcht", "!", "die", "Stun\u00b7de", "hat", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Schiffer steht am Bord,", "tokens": ["Und", "ein", "Schif\u00b7fer", "steht", "am", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fc\u00dft noch einmal, und es tragen", "tokens": ["Gr\u00fc\u00dft", "noch", "ein\u00b7mal", ",", "und", "es", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn die Wellen rauschend fort.", "tokens": ["Ihn", "die", "Wel\u00b7len", "rau\u00b7schend", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sturm w\u00fchlt, und die Zeiten b\u00e4umen", "tokens": ["Sturm", "w\u00fchlt", ",", "und", "die", "Zei\u00b7ten", "b\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehns\u00fcchtig sich himmelan,", "tokens": ["Sehn\u00