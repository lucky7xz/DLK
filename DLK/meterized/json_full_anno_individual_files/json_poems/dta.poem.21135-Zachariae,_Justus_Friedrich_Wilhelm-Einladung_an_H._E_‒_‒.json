{"dta.poem.21135": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Einladung an H. E \u2012 \u2012", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, Damon, wie von finstern Bergen", "tokens": ["Sieh", ",", "Da\u00b7mon", ",", "wie", "von", "fins\u00b7tern", "Ber\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Regen und der Unmuth braust,", "tokens": ["Der", "Re\u00b7gen", "und", "der", "Un\u00b7muth", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ein wolkengleicher Nebel", "tokens": ["Und", "wie", "ein", "wol\u00b7ken\u00b7glei\u00b7cher", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ausgestorbnen Wald umh\u00fcllt.", "tokens": ["Den", "aus\u00b7ge\u00b7storb\u00b7nen", "Wald", "um\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In ungehemmten dicken G\u00fcssen", "tokens": ["In", "un\u00b7ge\u00b7hemm\u00b7ten", "di\u00b7cken", "G\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verflie\u00dft der melancholsche Tag.", "tokens": ["Ver\u00b7flie\u00dft", "der", "me\u00b7lan\u00b7chol\u00b7sche", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Sonne steckt in schwarzen Wolken;", "tokens": ["Die", "Son\u00b7ne", "steckt", "in", "schwar\u00b7zen", "Wol\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wei\u00df, ob wir sie wieder sehn.", "tokens": ["Wer", "wei\u00df", ",", "ob", "wir", "sie", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch, Damon, \u00fcberla\u00df dem Schicksal,", "tokens": ["Doch", ",", "Da\u00b7mon", ",", "\u00fc\u00b7berl\u00b7a\u00df", "dem", "Schick\u00b7sal", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie es die liebe Sonne f\u00fchrt;", "tokens": ["Wie", "es", "die", "lie\u00b7be", "Son\u00b7ne", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und komm, und h\u00f6re, wie im Ofen", "tokens": ["Und", "komm", ",", "und", "h\u00f6\u00b7re", ",", "wie", "im", "O\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stamm der festen Eiche kracht.", "tokens": ["Der", "Stamm", "der", "fes\u00b7ten", "Ei\u00b7che", "kracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir wollen, vor die tr\u00fcben Fenster,", "tokens": ["Wir", "wol\u00b7len", ",", "vor", "die", "tr\u00fc\u00b7ben", "Fens\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sichernde Gardine ziehn;", "tokens": ["Die", "si\u00b7chern\u00b7de", "Gar\u00b7di\u00b7ne", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "So sehn wir nicht den wilden Regen,", "tokens": ["So", "sehn", "wir", "nicht", "den", "wil\u00b7den", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der \u00fcber hohe D\u00e4cher sch\u00e4umt.", "tokens": ["Der", "\u00fc\u00b7ber", "ho\u00b7he", "D\u00e4\u00b7cher", "sch\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was f\u00fcrchten wir des Nordwinds W\u00fcten", "tokens": ["Was", "f\u00fcrch\u00b7ten", "wir", "des", "Nord\u00b7winds", "W\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An einem bunten Caffeetisch!", "tokens": ["An", "ei\u00b7nem", "bun\u00b7ten", "Caf\u00b7fee\u00b7tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir k\u00f6nnen Fr\u00fchlingswetter schaffen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Freundschaft, durch Gespr\u00e4ch, und Wein.", "tokens": ["Durch", "Freund\u00b7schaft", ",", "durch", "Ge\u00b7spr\u00e4ch", ",", "und", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Komm, Damon, komm, du sollst es sehen,", "tokens": ["Komm", ",", "Da\u00b7mon", ",", "komm", ",", "du", "sollst", "es", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Lust und Freude bey uns herrscht;", "tokens": ["Wie", "Lust", "und", "Freu\u00b7de", "bey", "uns", "herrscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie die schimmernde Bouteille", "tokens": ["Und", "wie", "die", "schim\u00b7mern\u00b7de", "Bou\u00b7teil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das traurige Gem\u00fcth erhellt.", "tokens": ["Das", "trau\u00b7ri\u00b7ge", "Ge\u00b7m\u00fcth", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Jetzt, da uns noch kein kr\u00fcmmend Alter", "tokens": ["Jetzt", ",", "da", "uns", "noch", "kein", "kr\u00fcm\u00b7mend", "Al\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die eingeschrumpfte Stirne furcht;", "tokens": ["Die", "ein\u00b7ge\u00b7schrumpf\u00b7te", "Stir\u00b7ne", "furcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt, Damon, la\u00df uns uns genie\u00dfen,", "tokens": ["Jetzt", ",", "Da\u00b7mon", ",", "la\u00df", "uns", "uns", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VVIMP", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ungesorgt die Tage fliehn.", "tokens": ["Da\u00df", "un\u00b7ge\u00b7sorgt", "die", "Ta\u00b7ge", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}