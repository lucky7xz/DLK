{"textgrid.poem.65109": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn sich ein Herz im gl\u00fch'nden Schwung", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich ein Herz im gl\u00fch'nden Schwung", "tokens": ["Wenn", "sich", "ein", "Herz", "im", "gl\u00fch'n\u00b7den", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur k\u00fchnen Opferthat ermuthet,", "tokens": ["Zur", "k\u00fch\u00b7nen", "Op\u00b7fer\u00b7that", "er\u00b7mu\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Im Sturme der Begeisterung", "tokens": ["Im", "Stur\u00b7me", "der", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selbst vergessend, k\u00e4mpft und blutet", "tokens": ["Sich", "selbst", "ver\u00b7ges\u00b7send", ",", "k\u00e4mpft", "und", "blu\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn alle Wunden, die es tr\u00e4gt,", "tokens": ["Wenn", "al\u00b7le", "Wun\u00b7den", ",", "die", "es", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich lichten Siegesmalen prangen,", "tokens": ["Gleich", "lich\u00b7ten", "Sie\u00b7ges\u00b7ma\u00b7len", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da hat es, tief und s\u00fc\u00df bewegt,", "tokens": ["Da", "hat", "es", ",", "tief", "und", "s\u00fc\u00df", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den h\u00f6chsten Lohn bereits empfangen.", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Lohn", "be\u00b7reits", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wenn ein Herz von mild'rer Art,", "tokens": ["Und", "wenn", "ein", "Herz", "von", "mild'\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward ihm sein irdisch Gl\u00fcck vernichtet,", "tokens": ["Ward", "ihm", "sein", "ir\u00b7disch", "Gl\u00fcck", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus gramumh\u00fcllter Gegenwart", "tokens": ["Aus", "gra\u00b7mum\u00b7h\u00fcll\u00b7ter", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich in das Reich der Hoffnung fl\u00fcchtet.", "tokens": ["Sich", "in", "das", "Reich", "der", "Hoff\u00b7nung", "fl\u00fcch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn es da glaubt, Gott werde mild", "tokens": ["Wenn", "es", "da", "glaubt", ",", "Gott", "wer\u00b7de", "mild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm einst vergelten seine M\u00fchen,", "tokens": ["Ihm", "einst", "ver\u00b7gel\u00b7ten", "sei\u00b7ne", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da sieht auf \u00f6dem Herbstgefild'", "tokens": ["Da", "sieht", "auf", "\u00f6\u00b7dem", "Herbst\u00b7ge\u00b7fild'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es einen ew'gen Lenz erbl\u00fchen.", "tokens": ["Es", "ei\u00b7nen", "ew'\u00b7gen", "Lenz", "er\u00b7bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du aber, finst'rer Mann! dem nicht", "tokens": ["Du", "a\u00b7ber", ",", "finst'\u00b7rer", "Mann", "!", "dem", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$.", "ART", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begeisterung das Aug' befeuchtet,", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "das", "Aug'", "be\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du, dem des Glaubens tr\u00f6stend Licht", "tokens": ["Du", ",", "dem", "des", "Glau\u00b7bens", "tr\u00f6s\u00b7tend", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die n\u00e4cht'gen Bahnen nicht beleuchtet,", "tokens": ["Die", "n\u00e4cht'\u00b7gen", "Bah\u00b7nen", "nicht", "be\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der keine fromme Hoffnung hegt,", "tokens": ["Der", "kei\u00b7ne", "from\u00b7me", "Hoff\u00b7nung", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von dem die Liebe abgefallen,", "tokens": ["Von", "dem", "die", "Lie\u00b7be", "ab\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O sag mir, was dich st\u00fctzt und tr\u00e4gt,", "tokens": ["O", "sag", "mir", ",", "was", "dich", "st\u00fctzt", "und", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf deinem dunkeln Erdenwallen?", "tokens": ["Auf", "dei\u00b7nem", "dun\u00b7keln", "Er\u00b7den\u00b7wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Als du dein eigen Herz zerdr\u00fcckt,", "tokens": ["Als", "du", "dein", "ei\u00b7gen", "Herz", "zer\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um nicht ein andres zu verletzen,", "tokens": ["Um", "nicht", "ein", "and\u00b7res", "zu", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "ART", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als du ein fremdes Sein geschm\u00fcckt", "tokens": ["Als", "du", "ein", "frem\u00b7des", "Sein", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deines Lebens besten Sch\u00e4tzen,", "tokens": ["Mit", "dei\u00b7nes", "Le\u00b7bens", "bes\u00b7ten", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da ward die trunk'ne Seele nicht", "tokens": ["Da", "ward", "die", "trunk'\u00b7ne", "See\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von freud'gen Flammen dir durchlodert,", "tokens": ["Von", "freu\u00b7d'\u00b7gen", "Flam\u00b7men", "dir", "durch\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dein Thun schien dirnicht mehr als Pflicht,", "tokens": ["Dein", "Thun", "schien", "dir\u00b7nicht", "mehr", "als", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und du vollzogst, was sie gefordert.", "tokens": ["Und", "du", "voll\u00b7zogst", ",", "was", "sie", "ge\u00b7for\u00b7dert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dir ward kein Dank; mir aber ist", "tokens": ["Dir", "ward", "kein", "Dank", ";", "mir", "a\u00b7ber", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$.", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als m\u00fcsse vor dem Herrn der Welten,", "tokens": ["Als", "m\u00fcs\u00b7se", "vor", "dem", "Herrn", "der", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der unsre Thaten pr\u00fcft und mi\u00dft,", "tokens": ["Der", "uns\u00b7re", "Tha\u00b7ten", "pr\u00fcft", "und", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Opfer als das gr\u00f6\u00dfte gelten!", "tokens": ["Dein", "Op\u00b7fer", "als", "das", "gr\u00f6\u00df\u00b7te", "gel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn keine holde Zaubermacht", "tokens": ["Denn", "kei\u00b7ne", "hol\u00b7de", "Zau\u00b7ber\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bestrickte dich mit Engelsmienen \u2013", "tokens": ["Be\u00b7strick\u00b7te", "dich", "mit", "En\u00b7gels\u00b7mie\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast das Schwerste stark vollbracht,", "tokens": ["Du", "hast", "das", "Schwers\u00b7te", "stark", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Blo\u00df weil es dir als Recht erschienen.", "tokens": ["Blo\u00df", "weil", "es", "dir", "als", "Recht", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du tiefes Herz, das unter Eis", "tokens": ["Du", "tie\u00b7fes", "Herz", ",", "das", "un\u00b7ter", "Eis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verborgen wahrt die reinste Blume!", "tokens": ["Ver\u00b7bor\u00b7gen", "wahrt", "die", "reins\u00b7te", "Blu\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du Held, der da nichts ahnt und wei\u00df", "tokens": ["Du", "Held", ",", "der", "da", "nichts", "ahnt", "und", "wei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von seinem eig'nen Heldenthume!", "tokens": ["Von", "sei\u00b7nem", "eig'\u00b7nen", "Hel\u00b7den\u00b7thu\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn in der letzten Stunde Grau'n", "tokens": ["Wenn", "in", "der", "letz\u00b7ten", "Stun\u00b7de", "Grau'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Geist sich kl\u00e4rt sein wahrhaft Wesen,", "tokens": ["Dein", "Geist", "sich", "kl\u00e4rt", "sein", "wahr\u00b7haft", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "PPOSAT", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann wirst du, selig staunend, schau'n,", "tokens": ["Dann", "wirst", "du", ",", "se\u00b7lig", "stau\u00b7nend", ",", "schau'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie edel du, wie gro\u00df gewesen!", "tokens": ["Wie", "e\u00b7del", "du", ",", "wie", "gro\u00df", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "$,", "PWAV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn sich ein Herz im gl\u00fch'nden Schwung", "tokens": ["Wenn", "sich", "ein", "Herz", "im", "gl\u00fch'n\u00b7den", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur k\u00fchnen Opferthat ermuthet,", "tokens": ["Zur", "k\u00fch\u00b7nen", "Op\u00b7fer\u00b7that", "er\u00b7mu\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Im Sturme der Begeisterung", "tokens": ["Im", "Stur\u00b7me", "der", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selbst vergessend, k\u00e4mpft und blutet", "tokens": ["Sich", "selbst", "ver\u00b7ges\u00b7send", ",", "k\u00e4mpft", "und", "blu\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn alle Wunden, die es tr\u00e4gt,", "tokens": ["Wenn", "al\u00b7le", "Wun\u00b7den", ",", "die", "es", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich lichten Siegesmalen prangen,", "tokens": ["Gleich", "lich\u00b7ten", "Sie\u00b7ges\u00b7ma\u00b7len", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da hat es, tief und s\u00fc\u00df bewegt,", "tokens": ["Da", "hat", "es", ",", "tief", "und", "s\u00fc\u00df", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den h\u00f6chsten Lohn bereits empfangen.", "tokens": ["Den", "h\u00f6chs\u00b7ten", "Lohn", "be\u00b7reits", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wenn ein Herz von mild'rer Art,", "tokens": ["Und", "wenn", "ein", "Herz", "von", "mild'\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward ihm sein irdisch Gl\u00fcck vernichtet,", "tokens": ["Ward", "ihm", "sein", "ir\u00b7disch", "Gl\u00fcck", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus gramumh\u00fcllter Gegenwart", "tokens": ["Aus", "gra\u00b7mum\u00b7h\u00fcll\u00b7ter", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich in das Reich der Hoffnung fl\u00fcchtet.", "tokens": ["Sich", "in", "das", "Reich", "der", "Hoff\u00b7nung", "fl\u00fcch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn es da glaubt, Gott werde mild", "tokens": ["Wenn", "es", "da", "glaubt", ",", "Gott", "wer\u00b7de", "mild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm einst vergelten seine M\u00fchen,", "tokens": ["Ihm", "einst", "ver\u00b7gel\u00b7ten", "sei\u00b7ne", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da sieht auf \u00f6dem Herbstgefild'", "tokens": ["Da", "sieht", "auf", "\u00f6\u00b7dem", "Herbst\u00b7ge\u00b7fild'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es einen ew'gen Lenz erbl\u00fchen.", "tokens": ["Es", "ei\u00b7nen", "ew'\u00b7gen", "Lenz", "er\u00b7bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du aber, finst'rer Mann! dem nicht", "tokens": ["Du", "a\u00b7ber", ",", "finst'\u00b7rer", "Mann", "!", "dem", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$.", "ART", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begeisterung das Aug' befeuchtet,", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", "das", "Aug'", "be\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du, dem des Glauben