{"textgrid.poem.46803": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[freuet euch, ihr Lebenden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freuet euch, ihr Lebenden,", "tokens": ["Freu\u00b7et", "euch", ",", "ihr", "Le\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Liebenden Vereines,", "tokens": ["Lie\u00b7ben\u00b7den", "Ver\u00b7ei\u00b7nes", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Freut euch freudegebenden", "tokens": ["Freut", "euch", "freu\u00b7de\u00b7ge\u00b7ben\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Himmelssonnenscheines!", "tokens": ["Him\u00b7mels\u00b7son\u00b7nen\u00b7schei\u00b7nes", "!"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und die euch erworbenen,", "tokens": ["Und", "die", "euch", "er\u00b7wor\u00b7be\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Die man euch entrissen", "tokens": ["Die", "man", "euch", "ent\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Euere gestorbenen", "tokens": ["Eu\u00b7e\u00b7re", "ge\u00b7stor\u00b7be\u00b7nen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "m