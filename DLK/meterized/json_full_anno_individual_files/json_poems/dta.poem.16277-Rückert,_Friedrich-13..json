{"dta.poem.16277": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An Winterabenden (mir ward der Schwank erz\u00e4hlt", "tokens": ["An", "Win\u00b7ter\u00b7a\u00b7ben\u00b7den", "(", "mir", "ward", "der", "Schwank", "er\u00b7z\u00e4hlt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "PPER", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einem Freunde, den die Bibel viel gequ\u00e4lt)", "tokens": ["Von", "ei\u00b7nem", "Freun\u00b7de", ",", "den", "die", "Bi\u00b7bel", "viel", "ge\u00b7qu\u00e4lt", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Lie\u00df lesen, weil er horcht' in feierlicher Stille,", "tokens": ["Lie\u00df", "le\u00b7sen", ",", "weil", "er", "horcht'", "in", "fei\u00b7er\u00b7li\u00b7cher", "Stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein alter Herr die Schrift den Diener mit der Brille.", "tokens": ["Ein", "al\u00b7ter", "Herr", "die", "Schrift", "den", "Die\u00b7ner", "mit", "der", "Bril\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Brill' auf seiner Nas', in seiner Hand ein Stift,", "tokens": ["Die", "Brill'", "auf", "sei\u00b7ner", "Nas'", ",", "in", "sei\u00b7ner", "Hand", "ein", "Stift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So las er, bis er kam an einen Punkt der Schrift,", "tokens": ["So", "las", "er", ",", "bis", "er", "kam", "an", "ei\u00b7nen", "Punkt", "der", "Schrift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der f\u00fcr des Herrn Verstand zu hoch war und zu kraus", "tokens": ["Der", "f\u00fcr", "des", "Herrn", "Ver\u00b7stand", "zu", "hoch", "war", "und", "zu", "kraus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PTKA", "ADJD", "VAFIN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201everstehst du's, Hans?\u201c - Nein, Herr!-\u201eIch auch nicht, Hans, streich's aus!\u201c", "tokens": ["\u201e", "ver\u00b7stehst", "du's", ",", "Hans", "?", "\u201c", "Nein", ",", "Herr", "!", "\u201e", "Ich", "auch", "nicht", ",", "Hans", ",", "streich's", "aus", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PIS", "$,", "NE", "$.", "$(", "$(", "PTKANT", "$,", "NN", "$.", "$(", "$(", "PPER", "ADV", "PTKNEG", "$,", "NE", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "So ausgestrichen ward viel Unverstandenes.", "tokens": ["So", "aus\u00b7ge\u00b7stri\u00b7chen", "ward", "viel", "Un\u00b7ver\u00b7stan\u00b7de\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Doch blieb am Ende noch genug Vorhandenes.", "tokens": ["Doch", "blieb", "am", "En\u00b7de", "noch", "ge\u00b7nug", "Vor\u00b7han\u00b7de\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Wol denkt der alte Herr, da\u00df ohne viel Beschwerden", "tokens": ["Wol", "denkt", "der", "al\u00b7te", "Herr", ",", "da\u00df", "oh\u00b7ne", "viel", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Gemeinverst\u00e4ndlich so die heil'ge Schrift soll werden.", "tokens": ["Ge\u00b7mein\u00b7ver\u00b7st\u00e4nd\u00b7lich", "so", "die", "heil'\u00b7ge", "Schrift", "soll", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch als von vorn ins Buch es wieder gieng aufs Jahr,", "tokens": ["Doch", "als", "von", "vorn", "ins", "Buch", "es", "wie\u00b7der", "gieng", "aufs", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADV", "APPRART", "NN", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fand heuer dunkel sich, was ferden deutlich war.", "tokens": ["Fand", "heu\u00b7er", "dun\u00b7kel", "sich", ",", "was", "fer\u00b7den", "deut\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PRF", "$,", "PWS", "ADJA", "ADJD", "VAFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "\u201everstehst du's, Hans?\u201c - Nein, Herr!-\u201eIch auch nicht, Hans, streich's aus!\u201c", "tokens": ["\u201e", "ver\u00b7stehst", "du's", ",", "Hans", "?", "\u201c", "Nein", ",", "Herr", "!", "\u201e", "Ich", "auch", "nicht", ",", "Hans", ",", "streich's", "aus", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PIS", "$,", "NE", "$.", "$(", "$(", "PTKANT", "$,", "NN", "$.", "$(", "$(", "PPER", "ADV", "PTKNEG", "$,", "NE", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Da ward im dritten Jahr ein einz'ger Strich daraus.", "tokens": ["Da", "ward", "im", "drit\u00b7ten", "Jahr", "ein", "einz'\u00b7ger", "Strich", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Was lehret uns der Strich? da\u00df man in Schriften heilig", "tokens": ["Was", "leh\u00b7ret", "uns", "der", "Strich", "?", "da\u00df", "man", "in", "Schrif\u00b7ten", "hei\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$.", "KOUS", "PIS", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Unverst\u00e4ndliches ausstreichen soll voreilig.", "tokens": ["Nicht", "Un\u00b7ver\u00b7st\u00e4nd\u00b7li\u00b7ches", "aus\u00b7strei\u00b7chen", "soll", "vor\u00b7ei\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVINF", "VMFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Das Unverst\u00e4ndliche, la\u00df nur mit drein es gehn,", "tokens": ["Das", "Un\u00b7ver\u00b7st\u00e4nd\u00b7li\u00b7che", ",", "la\u00df", "nur", "mit", "drein", "es", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVIMP", "ADV", "APPR", "CARD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst wirst du selbst nicht das Verst\u00e4ndliche verstehn.", "tokens": ["Sonst", "wirst", "du", "selbst", "nicht", "das", "Ver\u00b7st\u00e4nd\u00b7li\u00b7che", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}