{"textgrid.poem.43306": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "Paphos", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, wo mein Herz, wenn es dich f\u00fchlet,", "tokens": ["Dich", ",", "wo", "mein", "Herz", ",", "wenn", "es", "dich", "f\u00fch\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Froh, wie in seiner Heimath gl\u00fcht,", "tokens": ["Froh", ",", "wie", "in", "sei\u00b7ner", "Hei\u00b7math", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich, Paphos, hab ich oft gespielet,", "tokens": ["Dich", ",", "Pa\u00b7phos", ",", "hab", "ich", "oft", "ge\u00b7spie\u00b7let", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sey du auch itzt mein reizend Lied.", "tokens": ["Sey", "du", "auch", "itzt", "mein", "rei\u00b7zend", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von deinen Blumen-D\u00fcften trunken,", "tokens": ["Von", "dei\u00b7nen", "Blu\u00b7men\u00b7D\u00fcf\u00b7ten", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ganz Gef\u00fchl f\u00fcr deinen May,", "tokens": ["Und", "ganz", "Ge\u00b7f\u00fchl", "f\u00fcr", "dei\u00b7nen", "May", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lernt ich, hin auf dein Moos gesunken,", "tokens": ["Lernt", "ich", ",", "hin", "auf", "dein", "Moos", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich begl\u00fcckt, wie Amor, sey.", "tokens": ["Da\u00df", "ich", "be\u00b7gl\u00fcckt", ",", "wie", "A\u00b7mor", ",", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "NE", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier bin ich, dem Ger\u00e4usch entwichen!", "tokens": ["Hier", "bin", "ich", ",", "dem", "Ge\u00b7r\u00e4usch", "ent\u00b7wi\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sey mir gegr\u00fc\u00dft, balsamscher Hayn!", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "bal\u00b7sam\u00b7scher", "Hayn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ein ganzer Fr\u00fchling von Ger\u00fcchen", "tokens": ["Ein", "gan\u00b7zer", "Fr\u00fch\u00b7ling", "von", "Ge\u00b7r\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ladt mich in deine Schatten ein.", "tokens": ["Ladt", "mich", "in", "dei\u00b7ne", "Schat\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er h\u00fcpft daher auf goldnen Aesten,", "tokens": ["Er", "h\u00fcpft", "da\u00b7her", "auf", "gold\u00b7nen", "A\u00b7es\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Der Lenz, in Bl\u00fcthen eingeh\u00fcllt,", "tokens": ["Der", "Lenz", ",", "in", "Bl\u00fc\u00b7then", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um den ein Schwarm von lichten Westen", "tokens": ["Um", "den", "ein", "Schwarm", "von", "lich\u00b7ten", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der B\u00e4ume Wipfel s\u00e4uselnd f\u00fcllt.", "tokens": ["Der", "B\u00e4u\u00b7me", "Wip\u00b7fel", "s\u00e4u\u00b7selnd", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die offnen Fluren abzuk\u00fchlen,", "tokens": ["Die", "off\u00b7nen", "Flu\u00b7ren", "ab\u00b7zu\u00b7k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Fliehn sie durch die erw\u00e4rmte Luft,", "tokens": ["Fliehn", "sie", "durch", "die", "er\u00b7w\u00e4rm\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ruhn itzt erm\u00fcdet aus, und spielen,", "tokens": ["Ruhn", "itzt", "er\u00b7m\u00fc\u00b7det", "aus", ",", "und", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und \u00fcbergie\u00dfen sich mit Duft.", "tokens": ["Und", "\u00fc\u00b7berg\u00b7ie\u00b7\u00dfen", "sich", "mit", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dann buhlen sie mit jungen Rosen,", "tokens": ["Dann", "buh\u00b7len", "sie", "mit", "jun\u00b7gen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die durch die Liebe fr\u00fcher bl\u00fchn;", "tokens": ["Die", "durch", "die", "Lie\u00b7be", "fr\u00fc\u00b7her", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und, ihnen sch\u00f6ner liebzukosen,", "tokens": ["Und", ",", "ih\u00b7nen", "sch\u00f6\u00b7ner", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Fliehn sie, und k\u00fcssen sie im Fliehn.", "tokens": ["Fliehn", "sie", ",", "und", "k\u00fcs\u00b7sen", "sie", "im", "Fliehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Fern liegt von mir auf j\u00e4hen Gipfeln,", "tokens": ["Fern", "liegt", "von", "mir", "auf", "j\u00e4\u00b7hen", "Gip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe heiligster Palast,", "tokens": ["Der", "Lie\u00b7be", "hei\u00b7ligs\u00b7ter", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den rings umher mit ew'gen Wipfeln", "tokens": ["Den", "rings", "um\u00b7her", "mit", "ew'\u00b7gen", "Wip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein junger Myrtenhayn umfa\u00dft.", "tokens": ["Ein", "jun\u00b7ger", "Myr\u00b7ten\u00b7hayn", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf den von seinem Feuersitze", "tokens": ["Auf", "den", "von", "sei\u00b7nem", "Feu\u00b7er\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tag verstohlen niedersieht,", "tokens": ["Der", "Tag", "ver\u00b7stoh\u00b7len", "nie\u00b7der\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn er in jugendlicher Hitze", "tokens": ["Wenn", "er", "in", "ju\u00b7gend\u00b7li\u00b7cher", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am Horizont vor\u00fcberflieht.", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "vor\u00b7\u00fc\u00b7berf\u00b7lieht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dem Arm des Mulciber entrissen,", "tokens": ["Dem", "Arm", "des", "Mul\u00b7ci\u00b7ber", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Hat Venus hier mit stillen K\u00fcssen", "tokens": ["Hat", "Ve\u00b7nus", "hier", "mit", "stil\u00b7len", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Einst den Adon zuerst erfreut.", "tokens": ["Einst", "den", "A\u00b7don", "zu\u00b7erst", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er starb, der Liebling der Cythere:", "tokens": ["Er", "starb", ",", "der", "Lieb\u00b7ling", "der", "Cy\u00b7the\u00b7re", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Doch ihn verewigen Alt\u00e4re,", "tokens": ["Doch", "ihn", "ve\u00b7re\u00b7wi\u00b7gen", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Die Paphos seinem Ruhm geweiht.", "tokens": ["Die", "Pa\u00b7phos", "sei\u00b7nem", "Ruhm", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Noch itzt beweinet ihn Cythere:", "tokens": ["Noch", "itzt", "be\u00b7wei\u00b7net", "ihn", "Cy\u00b7the\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Man glaubt, sie w\u00fcrd untr\u00f6stlich seyn,", "tokens": ["Man", "glaubt", ",", "sie", "w\u00fcrd", "un\u00b7tr\u00f6st\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wenn sonst kein Liebling \u00fcbrig w\u00e4re,", "tokens": ["Wenn", "sonst", "kein", "Lieb\u00b7ling", "\u00fcb\u00b7rig", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die G\u00f6ttinn wieder zu erfreun.", "tokens": ["Die", "G\u00f6t\u00b7tinn", "wie\u00b7der", "zu", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie k\u00f6mmt, die K\u00f6niginn der Herzen!", "tokens": ["Sie", "k\u00f6mmt", ",", "die", "K\u00f6\u00b7ni\u00b7ginn", "der", "Her\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie k\u00f6mmt, die Mutter der Natur!", "tokens": ["Sie", "k\u00f6mmt", ",", "die", "Mut\u00b7ter", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verfolgt von Amorn und von Scherzen", "tokens": ["Ver\u00b7folgt", "von", "A\u00b7morn", "und", "von", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Betritt ihr sch\u00f6ner Fu\u00df die Flur,", "tokens": ["Be\u00b7tritt", "ihr", "sch\u00f6\u00b7ner", "Fu\u00df", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hinterl\u00e4\u00dft auf seiner Spur,", "tokens": ["Und", "hin\u00b7ter\u00b7l\u00e4\u00dft", "auf", "sei\u00b7ner", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Aushauch einer Veilchen-Flur.", "tokens": ["Den", "Aus\u00b7hauch", "ei\u00b7ner", "Veil\u00b7chen\u00b7Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr h\u00fcpft ihr schlauer Sohn zur Seiten,", "tokens": ["Ihr", "h\u00fcpft", "ihr", "schlau\u00b7er", "Sohn", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der manchen Sklaven ihr gemacht;", "tokens": ["Der", "man\u00b7chen", "Skla\u00b7ven", "ihr", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auch Heben seh ich sie begleiten,", "tokens": ["Auch", "He\u00b7ben", "seh", "ich", "sie", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die mir durch sie so freundlich lacht.", "tokens": ["Die", "mir", "durch", "sie", "so", "freund\u00b7lich", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Schon tanzen in geschlungnen Reihen", "tokens": ["Schon", "tan\u00b7zen", "in", "ge\u00b7schlung\u00b7nen", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Grazien, die Schmeicheleyen,", "tokens": ["Die", "Gra\u00b7zi\u00b7en", ",", "die", "Schmei\u00b7che\u00b7le\u00b7yen", ","], "token_info": ["word", "word", "punct", "word", "word", "