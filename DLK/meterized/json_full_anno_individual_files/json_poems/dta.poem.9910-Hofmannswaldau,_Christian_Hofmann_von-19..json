{"dta.poem.9910": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jtzt pfleg ich auf erden furcht/ bald verlangen zu erwecken/", "tokens": ["Jtzt", "pfleg", "ich", "auf", "er\u00b7den", "furcht", "/", "bald", "ver\u00b7lan\u00b7gen", "zu", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$(", "ADV", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Jhrer viel die werden sich/ wenn sie mich ersehn/ verstecken:", "tokens": ["Ih\u00b7rer", "viel", "die", "wer\u00b7den", "sich", "/", "wenn", "sie", "mich", "er\u00b7sehn", "/", "ver\u00b7ste\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "PIAT", "PDS", "VAFIN", "PRF", "$(", "KOUS", "PPER", "PRF", "VVINF", "$(", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Jederman will vor mir lauffen/ meistens die so in der stadt;", "tokens": ["Je\u00b7der\u00b7man", "will", "vor", "mir", "lauf\u00b7fen", "/", "meis\u00b7tens", "die", "so", "in", "der", "stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "VVINF", "$(", "ADV", "ART", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch der landmann h\u00e4lt mich werther/ der gefallen an mir hat.", "tokens": ["Doch", "der", "land\u00b7mann", "h\u00e4lt", "mich", "wer