{"textgrid.poem.48455": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was sollen wir denn tun, was sollen wir ihm schenken,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sollen wir denn tun, was sollen wir ihm schenken,", "tokens": ["Was", "sol\u00b7len", "wir", "denn", "tun", ",", "was", "sol\u00b7len", "wir", "ihm", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PWS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dem Freunde, der uns liebt, um unser zu gedenken?", "tokens": ["dem", "Freun\u00b7de", ",", "der", "uns", "liebt", ",", "um", "un\u00b7ser", "zu", "ge\u00b7den\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUI", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mai, der ist zwar da mit seiner Blumenschaar,", "tokens": ["Der", "Mai", ",", "der", "ist", "zwar", "da", "mit", "sei\u00b7ner", "Blu\u00b7men\u00b7schaar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Mai, der mit sich malt das ausgezierte Jahr.", "tokens": ["der", "Mai", ",", "der", "mit", "sich", "malt", "das", "aus\u00b7ge\u00b7zier\u00b7te", "Jahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was aber sollen wir? Wie k\u00f6nnen wir ihm schicken", "tokens": ["Was", "a\u00b7ber", "sol\u00b7len", "wir", "?", "Wie", "k\u00f6n\u00b7nen", "wir", "ihm", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "$.", "PWAV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "von Blumen einen Kranz, sein Haupt damit zu schm\u00fccken?", "tokens": ["von", "Blu\u00b7men", "ei\u00b7nen", "Kranz", ",", "sein", "Haupt", "da\u00b7mit", "zu", "schm\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er ist zu weit von uns. Kein Kraut kan haben Statt,", "tokens": ["Er", "ist", "zu", "weit", "von", "uns", ".", "Kein", "Kraut", "kan", "ha\u00b7ben", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "APPR", "PPER", "$.", "PIAT", "NN", "VMFIN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die Nelken sterben hin, das Tausentsch\u00f6n wird matt,", "tokens": ["die", "Nel\u00b7ken", "ster\u00b7ben", "hin", ",", "das", "Tau\u00b7sent\u00b7sch\u00f6n", "wird", "matt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "der Majoran verwelkt, die lieblichen Violen", "tokens": ["der", "Ma\u00b7jo\u00b7ran", "ver\u00b7welkt", ",", "die", "lieb\u00b7li\u00b7chen", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "tun ihr Gesichte zu. Er kan nicht Atem holen,", "tokens": ["tun", "ihr", "Ge\u00b7sich\u00b7te", "zu", ".", "Er", "kan", "nicht", "A\u00b7tem", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "der Isop, er verreucht. Der schwachen Blumen Schein", "tokens": ["der", "I\u00b7sop", ",", "er", "ver\u00b7reucht", ".", "Der", "schwa\u00b7chen", "Blu\u00b7men", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PPER", "VVPP", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "kan eine solche Zeit nicht unterwegens sein.", "tokens": ["kan", "ei\u00b7ne", "sol\u00b7che", "Zeit", "nicht", "un\u00b7ter\u00b7we\u00b7gens", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PIAT", "NN", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "F\u00fcr Blumen wollen wir ein kleines B\u00e4ndlein winden", "tokens": ["F\u00fcr", "Blu\u00b7men", "wol\u00b7len", "wir", "ein", "klei\u00b7nes", "B\u00e4n\u00b7dlein", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN",