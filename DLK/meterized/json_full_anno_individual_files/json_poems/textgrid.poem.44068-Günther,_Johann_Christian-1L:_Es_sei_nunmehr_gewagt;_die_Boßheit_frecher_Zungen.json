], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich w\u00fcste warlich nicht, wo es geschrieben st\u00fcnde,", "tokens": ["Ich", "w\u00fcs\u00b7te", "war\u00b7lich", "nicht", ",", "wo", "es", "ge\u00b7schrie\u00b7ben", "st\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der stolzen H\u00f6hnerey auf ewig nachzusehn;", "tokens": ["Der", "stol\u00b7zen", "H\u00f6h\u00b7ne\u00b7rey", "auf", "e\u00b7wig", "nach\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Eifer n\u00e4hrt die Zeit, das Feuer lebt vom Winde,", "tokens": ["Den", "Ei\u00b7fer", "n\u00e4hrt", "die", "Zeit", ",", "das", "Feu\u00b7er", "lebt", "vom", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mir kan durch anders nichts als Rache gnung geschehn.", "tokens": ["Mir", "kan", "durch", "an\u00b7ders", "nichts", "als", "Ra\u00b7che", "gnung", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "PIS", "KOKOM", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Geh ich zur Compagnie, so schimpft man, was ich liebe;", "tokens": ["Geh", "ich", "zur", "Com\u00b7pag\u00b7nie", ",", "so", "schimpft", "man", ",