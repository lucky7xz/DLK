{"dta.poem.747": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII.  Vber die gebaine der au\u00dfgegrabenen  \n Philosetten.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "O H\u00e4slich' Anblick! ach! wo sindt die g\u00fcldnen Har! ", "tokens": ["O", "H\u00e4s\u00b7lich'", "An\u00b7blick", "!", "ach", "!", "wo", "sindt", "die", "g\u00fcld\u00b7nen", "Har", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$.", "XY", "$.", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ist der stirnen Schnee? wo ist der glantz der Wan- ", "tokens": ["Wo", "ist", "der", "stir\u00b7nen", "Schnee", "?", "wo", "ist", "der", "glantz", "der", "Wan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "gen:", "tokens": ["gen", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Der Wangen die mit Blut vnd Lilien vmbfangen?", "tokens": ["Der", "Wan\u00b7gen", "die", "mit", "Blut", "vnd", "Li\u00b7li\u00b7en", "vmb\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der rosen rote Mund! wo ist der Z\u00e4hue schar? ", "tokens": ["Der", "ro\u00b7sen", "ro\u00b7te", "Mund", "!", "wo", "ist", "der", "Z\u00e4\u00b7hue", "schar", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo sindt die Sternen hin? Wo ist der Augen par ", "tokens": ["Wo", "sindt", "die", "Ster\u00b7nen", "hin", "?", "Wo", "ist", "der", "Au\u00b7gen", "par"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "PWAV", "VAFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit den die Liebe spielt/ jtzt flechten schwartze Schlangen ", "tokens": ["Mit", "den", "die", "Lie\u00b7be", "spielt", "/", "jtzt", "flech\u00b7ten", "schwart\u00b7ze", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$(", "ADV", "VVINF", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich vmb das weite Maul/ die Nasen ist vergangen", "tokens": ["Sich", "vmb", "das", "wei\u00b7te", "Maul", "/", "die", "Na\u00b7sen", "ist", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die keinem helfenbein vorhin zu gleichen war.", "tokens": ["Die", "kei\u00b7nem", "hel\u00b7fen\u00b7bein", "vor\u00b7hin", "zu", "glei\u00b7chen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist jemand der noch kan behertzt vnd sonder grawen", "tokens": ["Ist", "je\u00b7mand", "der", "noch", "kan", "be\u00b7hertzt", "vnd", "son\u00b7der", "gra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "ADV", "VMFIN", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Der Ohren kahlen ort/ der Augen lucken schawen?", "tokens": ["Der", "Oh\u00b7ren", "kah\u00b7len", "ort", "/", "der", "Au\u00b7gen", "lu\u00b7cken", "scha\u00b7wen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Ist jemandt/ der sich nicht f\u00fcr dieser stirn entsetzt?", "tokens": ["Ist", "je\u00b7mandt", "/", "der", "sich", "nicht", "f\u00fcr", "die\u00b7ser", "stirn", "ent\u00b7setzt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$(", "PRELS", "PRF", "PTKNEG", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der dencke wie sich doch sein Geist denn wird befinden", "tokens": ["Der", "den\u00b7cke", "wie", "sich", "doch", "sein", "Geist", "denn", "wird", "be\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOKOM", "PRF", "ADV", "PPOSAT", "NN", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er in kurtzem wird auff gleichen schlag verschwinden/", "tokens": ["Wenn", "er", "in", "kurt\u00b7zem", "wird", "auff", "glei\u00b7chen", "schlag", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil schon der Todt auff jhn die schnellen pfeile wetzt.", "tokens": ["Weil", "schon", "der", "Todt", "auff", "jhn", "die", "schnel\u00b7len", "pfei\u00b7le", "wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}