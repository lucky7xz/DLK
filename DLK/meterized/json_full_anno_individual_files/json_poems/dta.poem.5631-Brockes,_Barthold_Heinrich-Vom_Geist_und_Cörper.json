{"dta.poem.5631": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies ist, mit unleugbarer Klarheit,", "tokens": ["Dies", "ist", ",", "mit", "un\u00b7leug\u00b7ba\u00b7rer", "Klar\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Menschen allererste Wahrheit;", "tokens": ["Der", "Men\u00b7schen", "al\u00b7le\u00b7rers\u00b7te", "Wahr\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir dencken; und dadurch erhellet, da\u00df wir seyn.", "tokens": ["Wir", "den\u00b7cken", ";", "und", "da\u00b7durch", "er\u00b7hel\u00b7let", ",", "da\u00df", "wir", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PAV", "VVFIN", "$,", "KOUS", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Zweifel nun befreyt von allem falschen Schein,", "tokens": ["Durch", "Zwei\u00b7fel", "nun", "be\u00b7freyt", "von", "al\u00b7lem", "fal\u00b7schen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "APPR", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mu\u00df ich mich weiter noch, dies zu gestehen, lencken:", "tokens": ["Mu\u00df", "ich", "mich", "wei\u00b7ter", "noch", ",", "dies", "zu", "ge\u00b7ste\u00b7hen", ",", "len\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "$,", "PDS", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ich ein Wesen sey, das f\u00e4hig, zu gedencken.", "tokens": ["Da\u00df", "ich", "ein", "We\u00b7sen", "sey", ",", "das", "f\u00e4\u00b7hig", ",", "zu", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "PRELS", "ADJD", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}