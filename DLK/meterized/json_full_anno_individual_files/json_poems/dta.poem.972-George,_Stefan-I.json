{"dta.poem.972": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "I", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Deine stirne verborgen halb durch die beiden                 ", "tokens": ["Dei\u00b7ne", "stir\u00b7ne", "ver\u00b7bor\u00b7gen", "halb", "durch", "die", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVPP", "ADJD", "APPR", "ART", "PIAT"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "W\u00f6lkchen von haaren (sie sind blond und seiden)                 ", "tokens": ["W\u00f6lk\u00b7chen", "von", "haa\u00b7ren", "(", "sie", "sind", "blond", "und", "sei\u00b7den", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "PPER", "VAFIN", "ADJD", "KON", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Deine stirne spricht mir von jugendlichem leide               ", "tokens": ["Dei\u00b7ne", "stir\u00b7ne", "spricht", "mir", "von", "ju\u00b7gend\u00b7li\u00b7chem", "lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Deine lippen (sie sind stumm) erz&#228;hlen die geschichte                 ", "tokens": ["Dei\u00b7ne", "lip\u00b7pen", "(", "sie", "sind", "stumm", ")", "erz", "&#228;", "hlen", "die", "ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "XML_entity", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VAFIN", "ADJD", "$(", "ADJD", "$(", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Der seelen verurteilt in gottes gerichte &#183;                 ", "tokens": ["Der", "see\u00b7len", "ver\u00b7ur\u00b7teilt", "in", "got\u00b7tes", "ge\u00b7rich\u00b7te", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Erregender spiegel dein auge spiel damit nicht!               ", "tokens": ["Er\u00b7re\u00b7gen\u00b7der", "spie\u00b7gel", "dein", "au\u00b7ge", "spiel", "da\u00b7mit", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "PAV", "PTKNEG", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wenn du l&#228;chelst (endlich flog &#252;ber dir der schlummer her)                 ", "tokens": ["Wenn", "du", "l", "&#228;", "chelst", "(", "end\u00b7lich", "flog", "&#252;", "ber", "dir", "der", "schlum\u00b7mer", "her", ")"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "FM", "FM", "FM", "$(", "ADV", "VVFIN", "$(", "APPR", "PPER", "ART", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Dein l&#228;cheln gleicht dem weinen sehr                 ", "tokens": ["Dein", "l", "&#228;", "cheln", "gleicht", "dem", "wei\u00b7nen", "sehr"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VVINF", "VVFIN", "ART", "ADJA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du neigst ein wenig dein haupt von kummer schwer.               ", "tokens": ["Und", "du", "neigst", "ein", "we\u00b7nig", "dein", "haupt", "von", "kum\u00b7mer", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "PIS", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}