{"dta.poem.429": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie im gr\u00fcnen schlaffen fand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr L\u00fcffte wehet nicht/ halt euren Oden auf/", "tokens": ["Ihr", "L\u00fcff\u00b7te", "we\u00b7het", "nicht", "/", "halt", "eu\u00b7ren", "O\u00b7den", "auf", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "$(", "VVFIN", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steh ", "tokens": ["Steh"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "La\u00df/ la\u00df/ mein Engels-Kind in s\u00fcsser Ruhe liegen/", "tokens": ["La\u00df", "/", "la\u00df", "/", "mein", "En\u00b7gels\u00b7Kind", "in", "s\u00fcs\u00b7ser", "Ru\u00b7he", "lie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "VVIMP", "$(", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df Ros-und Nelcken sich zu ihren F\u00fcssen schmiegen.", "tokens": ["La\u00df", "Ros\u00b7\u00b7und", "Nel\u00b7cken", "sich", "zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hauch an mein Paradies mit ", "tokens": ["Hauch", "an", "mein", "Pa\u00b7ra\u00b7dies", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Still! da\u00df kein ", "tokens": ["Still", "!", "da\u00df", "kein"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$.", "KOUS", "PIAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Macht kein Ger\u00e4usche nicht ", "tokens": ["Macht", "kein", "Ge\u00b7r\u00e4u\u00b7sche", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "PTKNEG"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Still/ da\u00df sie schlaffend bleibt/ und ich den Ku\u00df erwische.", "tokens": ["Still", "/", "da\u00df", "sie", "schlaf\u00b7fend", "bleibt", "/", "und", "ich", "den", "Ku\u00df", "er\u00b7wi\u00b7sche", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "KOUS", "PPER", "ADJD", "VVFIN", "$(", "KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}