{"textgrid.poem.57564": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "November", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um meine Stiefel rauscht das Laub", "tokens": ["Um", "mei\u00b7ne", "Stie\u00b7fel", "rauscht", "das", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nackten Waldesriesen,", "tokens": ["Der", "nack\u00b7ten", "Wal\u00b7des\u00b7rie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein graues, tr\u00fcbes Schummerlicht", "tokens": ["Ein", "grau\u00b7es", ",", "tr\u00fc\u00b7bes", "Schum\u00b7mer\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umd\u00fcstert Wald und Wiesen.", "tokens": ["Um\u00b7d\u00fcs\u00b7tert", "Wald", "und", "Wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Luft ist rauh und nebelna\u00df,", "tokens": ["Die", "Luft", "ist", "rauh", "und", "ne\u00b7bel\u00b7na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nordwest beginnt zu wehen,", "tokens": ["Nord\u00b7west", "be\u00b7ginnt", "zu", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein greller, schriller Amselpfiff", "tokens": ["Ein", "grel\u00b7ler", ",", "schril\u00b7ler", "Am\u00b7sel\u00b7pfiff"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Klingt jammernd aus den Schlehen.", "tokens": ["Klingt", "jam\u00b7mernd", "aus", "den", "Schle\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im schwanken Zickzackfluge tanzt", "tokens": ["Im", "schwan\u00b7ken", "Zick\u00b7zack\u00b7flu\u00b7ge", "tanzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gespenstig um die Eichen", "tokens": ["Ge\u00b7spens\u00b7tig", "um", "die", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Wintermotten fahle Schar,", "tokens": ["Der", "Win\u00b7ter\u00b7mot\u00b7ten", "fah\u00b7le", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die letzten Lebenszeichen.", "tokens": ["Die", "letz\u00b7ten", "Le\u00b7bens\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie treibt der Liebe Peitschenschlag,", "tokens": ["Sie", "treibt", "der", "Lie\u00b7be", "Peit\u00b7schen\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu suchen ihre Weibchen,", "tokens": ["Zu", "su\u00b7chen", "ih\u00b7re", "Weib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die h\u00e4ngen fl\u00fcgellos am Stamm", "tokens": ["Die", "h\u00e4n\u00b7gen", "fl\u00fc\u00b7gel\u00b7los", "am", "Stamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit aufgedunsnem Leibchen ...", "tokens": ["Mit", "auf\u00b7ge\u00b7duns\u00b7nem", "Leib\u00b7chen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zur rechten Hand ein Waidmannssteg", "tokens": ["Zur", "rech\u00b7ten", "Hand", "ein", "Waid\u00b7mannss\u00b7teg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch schwarze Tannendichtug,", "tokens": ["Durch", "schwar\u00b7ze", "Tan\u00b7nen\u00b7dich\u00b7tug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und mitten drin, breit\u00e4stig, schirmt", "tokens": ["Und", "mit\u00b7ten", "drin", ",", "brei\u00b7t\u00e4s\u00b7tig", ",", "schirmt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Buchenbaum die Lichtung \u2013", "tokens": ["Ein", "Bu\u00b7chen\u00b7baum", "die", "Lich\u00b7tung", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es war im Mai und jubelnd hat", "tokens": ["Es", "war", "im", "Mai", "und", "ju\u00b7belnd", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Buchfinks Sang geklungen,", "tokens": ["Des", "Buch\u00b7finks", "Sang", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was hier geschah, das habe ich", "tokens": ["Was", "hier", "ge\u00b7schah", ",", "das", "ha\u00b7be", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im kecken Lied gesungen.", "tokens": ["Im", "ke\u00b7cken", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O gr\u00fcner, sonnenhei\u00dfer Tag,", "tokens": ["O", "gr\u00fc\u00b7ner", ",", "son\u00b7nen\u00b7hei\u00b7\u00dfer", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Herbsttag, kalt und tr\u00fcbe \u2013", "tokens": ["O", "Herbst\u00b7tag", ",", "kalt", "und", "tr\u00fc\u00b7be", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Herzen \u00e4chzt der letzte Schrei", "tokens": ["Im", "Her\u00b7zen", "\u00e4chzt", "der", "letz\u00b7te", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der totgetretnen Liebe.", "tokens": ["Der", "tot\u00b7ge\u00b7tret\u00b7nen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Frostschmetterling und Menschenweib,", "tokens": ["Frost\u00b7schmet\u00b7ter\u00b7ling", "und", "Men\u00b7schen\u00b7weib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Untrennbar mir zu denken!", "tokens": ["Un\u00b7trenn\u00b7bar", "mir", "zu", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wann wird euch Weibern die Kultur", "tokens": ["Wann", "wird", "euch", "Wei\u00b7bern", "die", "Kul\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Geistesschwingen schenken?", "tokens": ["Die", "Geis\u00b7tes\u00b7schwin\u00b7gen", "schen\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein neuer Mai, ein neues Gr\u00fcn", "tokens": ["Ein", "neu\u00b7er", "Mai", ",", "ein", "neu\u00b7es", "Gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und frische Liebessuche,", "tokens": ["Und", "fri\u00b7sche", "Lie\u00b7bes\u00b7su\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch verlorne Liebesm\u00fch,", "tokens": ["Und", "doch", "ver\u00b7lor\u00b7ne", "Lie\u00b7bes\u00b7m\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wei\u00dft es, alte Buche.", "tokens": ["Du", "wei\u00dft", "es", ",", "al\u00b7te", "Bu\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ein starker Ast von deinem Stamm,", "tokens": ["Ein", "star\u00b7ker", "Ast", "von", "dei\u00b7nem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Strick um meinen Nacken \u2013", "tokens": ["Ein", "Strick", "um", "mei\u00b7nen", "Na\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das w\u00e4r' ein herbstlich Stimmungsbild,", "tokens": ["Das", "w\u00e4r'", "ein", "herbst\u00b7lich", "Stim\u00b7mungs\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wirkung w\u00fcrde packen.", "tokens": ["Die", "Wir\u00b7kung", "w\u00fcr\u00b7de", "pa\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "