{"dta.poem.118": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Thorheiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.57", "id:0.42"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Galante Lydia du angenehmes Kind", "tokens": ["Ga\u00b7lan\u00b7te", "Ly\u00b7dia", "du", "an\u00b7ge\u00b7neh\u00b7mes", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor deiner Sch\u00f6nheits-Pracht wie nichts zu achten sind/", "tokens": ["Vor", "dei\u00b7ner", "Sch\u00f6n\u00b7heits\u00b7Pracht", "wie", "nichts", "zu", "ach\u00b7ten", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "PIS", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der weissen Liljen Zier wie auch der Rosen Prangen", "tokens": ["Der", "weis\u00b7sen", "Lil\u00b7jen", "Zier", "wie", "auch", "der", "Ro\u00b7sen", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KOKOM", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Helffen bein erschw\u00e4rtzt vor deinen glatten Wangen/", "tokens": ["Das", "Helf\u00b7fen", "bein", "er\u00b7schw\u00e4rtzt", "vor", "dei\u00b7nen", "glat\u00b7ten", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! zeige sch\u00f6nstes Kind dein gold-entflammtes Haar", "tokens": ["Ach", "!", "zei\u00b7ge", "sch\u00f6ns\u00b7tes", "Kind", "dein", "gold\u00b7ent\u00b7flamm\u00b7tes", "Haar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Entbl\u00f6sse deine Brust das Schwaanen gleiche Paar", "tokens": ["Ent\u00b7bl\u00f6s\u00b7se", "dei\u00b7ne", "Brust", "das", "Schwaa\u00b7nen", "glei\u00b7che", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So aus der Marmor Klufft der weissen Schultern prahlet/", "tokens": ["So", "aus", "der", "Mar\u00b7mor", "Klufft", "der", "weis\u00b7sen", "Schul\u00b7tern", "prah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erhebe doch den Glantz damit dein Auge strahlet", "tokens": ["Er\u00b7he\u00b7be", "doch", "den", "Glantz", "da\u00b7mit", "dein", "Au\u00b7ge", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit schwartzer Demmerung der Augen braun verh\u00fclt/", "tokens": ["Mit", "schwart\u00b7zer", "Dem\u00b7me\u00b7rung", "der", "Au\u00b7gen", "braun", "ver\u00b7h\u00fclt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein M\u00e4dgen schencke mir die Rose/ so erf\u00fclt", "tokens": ["Mein", "M\u00e4d\u00b7gen", "schen\u00b7cke", "mir", "die", "Ro\u00b7se", "/", "so", "er\u00b7f\u00fclt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein sch\u00f6nes Wangen-Feld mit Purpur angenetzet/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Wan\u00b7gen\u00b7Feld", "mit", "Pur\u00b7pur", "an\u00b7ge\u00b7net\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach! reiche mir den Mund der mit Rubin besetzet/", "tokens": ["Ach", "!", "rei\u00b7che", "mir", "den", "Mund", "der", "mit", "Ru\u00b7bin", "be\u00b7set\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ART", "NN", "ART", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Lippen wo Corall sich mit der Anmuth paart/", "tokens": ["Die", "Lip\u00b7pen", "wo", "Co\u00b7rall", "sich", "mit", "der", "An\u00b7muth", "paart", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "NE", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und labe deinen Knecht nach Turtel-Tauben Art", "tokens": ["Und", "la\u00b7be", "dei\u00b7nen", "Knecht", "nach", "Tur\u00b7tel\u00b7Tau\u00b7ben", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit s\u00fcssem ", "tokens": ["Mit", "s\u00fcs\u00b7sem"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Du nimmst mir Th\u00f6rigten die besten Nahrungs-Fl\u00fcsse/", "tokens": ["Du", "nimmst", "mir", "Th\u00f6\u00b7rig\u00b7ten", "die", "bes\u00b7ten", "Nah\u00b7rungs\u00b7F\u00b7l\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein Ku\u00df entseelt mein Hertz/ und st\u00f6hret meine Lust/", "tokens": ["Dein", "Ku\u00df", "ent\u00b7seelt", "mein", "Hertz", "/", "und", "st\u00f6h\u00b7ret", "mei\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Warum entziehest du das Leben meiner Brust?", "tokens": ["Wa\u00b7rum", "ent\u00b7zie\u00b7hest", "du", "das", "Le\u00b7ben", "mei\u00b7ner", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Verh\u00fclle deine Brust/ bedeck die ", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "dei\u00b7ne", "Brust", "/", "be\u00b7deck", "die"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "ADJD", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die als ein Marmor-Meer mit sanfften Oden wallen.", "tokens": ["Die", "als", "ein", "Mar\u00b7mor\u00b7Meer", "mit", "sanff\u00b7ten", "O\u00b7den", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die angenehme Schoo\u00df ich weis nicht was anbeut/", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Schoo\u00df", "ich", "weis", "nicht", "was", "an\u00b7beut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "PTKNEG", "PWS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein Liebes Wesen giebt stets neue Lieblichkeit.", "tokens": ["Dein", "Lie\u00b7bes", "We\u00b7sen", "giebt", "stets", "neu\u00b7e", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Verstecke deine Brust/ die mich mit ihren Pfeilen/", "tokens": ["Ver\u00b7ste\u00b7cke", "dei\u00b7ne", "Brust", "/", "die", "mich", "mit", "ih\u00b7ren", "Pfei\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So weisser als der Schnee/ verr\u00e4thrisch wil ereilen/", "tokens": ["So", "weis\u00b7ser", "als", "der", "Schnee", "/", "ver\u00b7r\u00e4t\u00b7hrisch", "wil", "er\u00b7ei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$(", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die stets ihr blanckes Schwerd zu meinem Schaden wetzt/", "tokens": ["Die", "stets", "ihr", "blan\u00b7ckes", "Schwerd", "zu", "mei\u00b7nem", "Scha\u00b7den", "wetzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und mein schon schwaches Hertz noch mehr in Ohnmacht setzt.", "tokens": ["Und", "mein", "schon", "schwa\u00b7ches", "Hertz", "noch", "mehr", "in", "Ohn\u00b7macht", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Allein/ du siehst aus Wuth nicht meiner Seelen-Plagen/", "tokens": ["Al\u00b7lein", "/", "du", "siehst", "aus", "Wuth", "nicht", "mei\u00b7ner", "See\u00b7len\u00b7Pla\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VVFIN", "APPR", "NN", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und l\u00e4ssest mich halb todt in meiner Noht verzagen.", "tokens": ["Und", "l\u00e4s\u00b7sest", "mich", "halb", "todt", "in", "mei\u00b7ner", "Noht", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}