{"dta.poem.18978": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "5.  \n  Sie ist gantz lieblich vnd l\u00f6blich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Das gold des Morenla\u0303ds/ wie pur es auch ka\u0303 sein/", "tokens": ["Das", "gold", "des", "Mo\u00b7ren\u00b7l\u00e3ds", "/", "wie", "pur", "es", "auch", "k\u00e3", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "PWAV", "ADJD", "PPER", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df jhres krausen haars k\u00f6stlichem schimmern", "tokens": ["Mu\u00df", "jhres", "krau\u00b7sen", "haars", "k\u00f6st\u00b7li\u00b7chem", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "weichen:", "tokens": ["wei\u00b7chen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Der rohteste Coral/ des sch\u00f6nsten Rubins schein", "tokens": ["Der", "roh\u00b7tes\u00b7te", "Co\u00b7ral", "/", "des", "sch\u00f6ns\u00b7ten", "Ru\u00b7bins", "schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist jhres Rosenmunds reichtumb nicht zuver-", "tokens": ["Ist", "jhres", "Ro\u00b7sen\u00b7munds", "reich\u00b7tumb", "nicht", "zu\u00b7ver"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "gleichen:", "tokens": ["glei\u00b7chen", ":"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Vnd keine perlein seind so weissz/ so gleich/ so rein/", "tokens": ["Vnd", "kei\u00b7ne", "per\u00b7lein", "seind", "so", "weissz", "/", "so", "gleich", "/", "so", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "ADV", "ADV", "$(", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als die/ die jhres munds red vnnd geschm\u00f6ll be-", "tokens": ["Als", "die", "/", "die", "jhres", "munds", "red", "vnnd", "ge\u00b7schm\u00f6ll", "be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "$(", "ART", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "reichen:", "tokens": ["rei\u00b7chen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "So kan auch die Natur vn\u0303 Kunst kein helfenbein/", "tokens": ["So", "kan", "auch", "die", "Na\u00b7tur", "v\u00f1", "Kunst", "kein", "hel\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "ADJA", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das so zart/ glat vnd weissz/ wie jhr leib/ herau\u00df", "tokens": ["Das", "so", "zart", "/", "glat", "vnd", "weissz", 