asure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Der augen s\u00fcsse wayd/ die todte zu besehlen/", "tokens": ["Der", "au\u00b7gen", "s\u00fcs\u00b7se", "wayd", "/", "die", "tod\u00b7te", "zu", "be\u00b7seh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00f6nheit gantze sum/ der Tugenten Richt-", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "gant\u00b7ze", "sum", "/", "der", "Tu\u00b7gen\u00b7ten", "Richt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NE", "$(", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "schnur;", "tokens": ["schnur", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wie kan ich jm\u0303er dan/ Sie liebend/ lobend/ fehlen?", "tokens": ["Wie", "kan", "ich", "jm\u0303er", "dan", "/", "Sie", "lie\u00b7bend", "/", "lo\u00b7bend", "/", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "$(", "PPER", "ADJD", "$(", "VVPP", "$(", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}