{"dta.poem.448": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Ha\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sp\u00fchre Lieb' und Ha\u00df/ vielleicht gelangt an mir", "tokens": ["Ich", "sp\u00fch\u00b7re", "Lieb'", "und", "Ha\u00df", "/", "viel\u00b7leicht", "ge\u00b7langt", "an", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$(", "ADV", "VVPP", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum ich solches thu?", "tokens": ["Wa\u00b7rum", "ich", "sol\u00b7ches", "thu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Ursach wei\u00df ich nicht/ nur da\u00df ichs bey mir sp\u00fchr/", "tokens": ["Die", "Ur\u00b7sach", "wei\u00df", "ich", "nicht", "/", "nur", "da\u00df", "ichs", "bey", "mir", "sp\u00fchr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "ADV", "KOUS", "PIS", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich qu\u00e4hl mich noch dazu.", "tokens": ["Ich", "qu\u00e4hl", "mich", "noch", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}