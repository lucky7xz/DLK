{"dta.poem.5991": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Du Soleil, des Planetes,  \n du Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn, um die Umst\u00e4nd' all noch besser zu entdecken,", "tokens": ["Wenn", ",", "um", "die", "Um\u00b7st\u00e4nd'", "all", "noch", "bes\u00b7ser", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUI", "ART", "NN", "PIAT", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von den Bewegungen, die scheinbar theils, theils wahr,", "tokens": ["Von", "den", "Be\u00b7we\u00b7gun\u00b7gen", ",", "die", "schein\u00b7bar", "theils", ",", "theils", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die Blicke sich ins Firmament erstrecken;", "tokens": ["Die", "Bli\u00b7cke", "sich", "ins", "Fir\u00b7ma\u00b7ment", "er\u00b7stre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So treffen wir daselbst recht wunderbar", "tokens": ["So", "tref\u00b7fen", "wir", "da\u00b7selbst", "recht", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So viele Vorw\u00fcrff\u2019 an, die eines tieffen Denckens", "tokens": ["So", "vie\u00b7le", "Vor\u00b7w\u00fcrff'", "an", ",", "die", "ei\u00b7nes", "tief\u00b7fen", "Den\u00b7ckens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor andern w\u00fcrdig sind. Man mu\u00df bewundrend sehen", "tokens": ["Vor", "an\u00b7dern", "w\u00fcr\u00b7dig", "sind", ".", "Man", "mu\u00df", "be\u00b7wund\u00b7rend", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADJD", "VAFIN", "$.", "PIS", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Das helle Tages-Licht best\u00e4ndig vorwerts gehen.", "tokens": ["Das", "hel\u00b7le", "Ta\u00b7ges\u00b7Licht", "be\u00b7st\u00e4n\u00b7dig", "vor\u00b7werts", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es l\u00e4ufft von Osten ab, und l\u00e4ufft nach Westen zu,", "tokens": ["Es", "l\u00e4ufft", "von", "Os\u00b7ten", "ab", ",", "und", "l\u00e4ufft", "nach", "Wes\u00b7ten", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erhellet Himmel, Erd und Fluht, stets ohne Ruh.", "tokens": ["Er\u00b7hel\u00b7let", "Him\u00b7mel", ",", "Erd", "und", "Fluht", ",", "stets", "oh\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da man auch glauben kan, es sey dies Licht der Welt,", "tokens": ["Da", "man", "auch", "glau\u00b7ben", "kan", ",", "es", "sey", "dies", "Licht", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$,", "PPER", "VAFIN", "PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In jener ungemessnen Ferne,", "tokens": ["In", "je\u00b7ner", "un\u00b7ge\u00b7mess\u00b7nen", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gantz unbeweglich hingestellt,", "tokens": ["Gantz", "un\u00b7be\u00b7weg\u00b7lich", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Um welches die nie stillen Sterne", "tokens": ["Um", "wel\u00b7ches", "die", "nie", "stil\u00b7len", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "In einer steten R\u00fcnde wandern,", "tokens": ["In", "ei\u00b7ner", "ste\u00b7ten", "R\u00fcn\u00b7de", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und da\u00df von seinem Blick sie, einer nach dem andern,", "tokens": ["Und", "da\u00df", "von", "sei\u00b7nem", "Blick", "sie", ",", "ei\u00b7ner", "nach", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "$,", "ART", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jhr Licht erborgen und erlangen,", "tokens": ["Ihr", "Licht", "er\u00b7bor\u00b7gen", "und", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Einfolglich ihren Tag empfangen.", "tokens": ["Ein\u00b7folg\u00b7lich", "ih\u00b7ren", "Tag", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}