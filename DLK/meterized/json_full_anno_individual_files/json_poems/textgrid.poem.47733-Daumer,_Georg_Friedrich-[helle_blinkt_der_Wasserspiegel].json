{"textgrid.poem.47733": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[helle blinkt der Wasserspiegel]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Helle blinkt der Wasserspiegel,", "tokens": ["Hel\u00b7le", "blinkt", "der", "Was\u00b7ser\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitten in der Spiegelhelle", "tokens": ["Mit\u00b7ten", "in", "der", "Spie\u00b7gel\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt sich aus der Fluth ein H\u00fcgel,", "tokens": ["Hebt", "sich", "aus", "der", "Fluth", "ein", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitten auf der H\u00fcgelwelle", "tokens": ["Mit\u00b7ten", "auf", "der", "H\u00fc\u00b7gel\u00b7wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer Eiche gr\u00fcne Pracht", "tokens": ["Ei\u00b7ner", "Ei\u00b7che", "gr\u00fc\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich arme, kleine Waise", "tokens": ["Und", "ich", "ar\u00b7me", ",", "klei\u00b7ne", "Wai\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "mea