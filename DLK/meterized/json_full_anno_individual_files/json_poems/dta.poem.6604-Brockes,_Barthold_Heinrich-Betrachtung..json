{"dta.poem.6604": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, wenn wir auch gleich so weit", "tokens": ["Al\u00b7lein", ",", "wenn", "wir", "auch", "gleich", "so", "weit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sinnen br\u00e4chten, da\u00df mit Freuden,", "tokens": ["Die", "Sin\u00b7nen", "br\u00e4ch\u00b7ten", ",", "da\u00df", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An aller Vorw\u00fcrff Herrlichkeit;", "tokens": ["An", "al\u00b7ler", "Vor\u00b7w\u00fcrff", "Herr\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir, Unschulds voll, die Augen k\u00f6nnten weiden:", "tokens": ["Wir", ",", "Un\u00b7schulds", "voll", ",", "die", "Au\u00b7gen", "k\u00f6nn\u00b7ten", "wei\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ADJD", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn eine gl\u00fcckliche Vermuthung uns gleich lehret,", "tokens": ["Wenn", "ei\u00b7ne", "gl\u00fcck\u00b7li\u00b7che", "Ver\u00b7mu\u00b7thung", "uns", "gleich", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Worauf man viele Zeit gewandt;", "tokens": ["Wo\u00b7rauf", "man", "vie\u00b7le", "Zeit", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie etwan die Natur verf\u00e4hret,", "tokens": ["Wie", "et\u00b7wan", "die", "Na\u00b7tur", "ver\u00b7f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Mit Mitteln, die uns unbekannt", "tokens": ["Mit", "Mit\u00b7teln", ",", "die", "uns", "un\u00b7be\u00b7kannt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Um eine solche Meng von unterschiednen Dingen", "tokens": ["Um", "ei\u00b7ne", "sol\u00b7che", "Meng", "von", "un\u00b7ter\u00b7schied\u00b7nen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu bilden und hervor zu bringen;", "tokens": ["Zu", "bil\u00b7den", "und", "her\u00b7vor", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wie unvollkommen ist jedennoch unser dencken,", "tokens": ["Wie", "un\u00b7voll\u00b7kom\u00b7men", "ist", "je\u00b7den\u00b7noch", "un\u00b7ser", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Das wir mit so viel M\u00fch\u2019 auf alle Vorw\u00fcrff lencken,", "tokens": ["Das", "wir", "mit", "so", "viel", "M\u00fch'", "auf", "al\u00b7le", "Vor\u00b7w\u00fcrff", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADV", "PIAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn wir nicht wissen zu erkl\u00e4ren,", "tokens": ["Wenn", "wir", "nicht", "wis\u00b7sen", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie alles das zusammen h\u00e4ngt,", "tokens": ["Wie", "al\u00b7les", "das", "zu\u00b7sam\u00b7men", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wodurch der Geist das, was er f\u00fchlt empf\u00e4ngt.", "tokens": ["Wo\u00b7durch", "der", "Geist", "das", ",", "was", "er", "f\u00fchlt", "emp\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PDS", "$,", "PWS", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "O sch\u00f6ner Gegenwurf von unsern Lehren!", "tokens": ["O", "sch\u00f6\u00b7ner", "Ge\u00b7gen\u00b7wurf", "von", "un\u00b7sern", "Leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}