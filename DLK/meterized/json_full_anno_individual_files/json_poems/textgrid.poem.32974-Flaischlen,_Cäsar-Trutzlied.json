{"textgrid.poem.32974": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "Trutzlied", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Geld im Beutel Sorgen macht,", "tokens": ["Wenn", "Geld", "im", "Beu\u00b7tel", "Sor\u00b7gen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie reiche Leute sagen,", "tokens": ["wie", "rei\u00b7che", "Leu\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "von uns dann h\u00e4tte wahrlich keins", "tokens": ["von", "uns", "dann", "h\u00e4t\u00b7te", "wahr\u00b7lich", "keins"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "ADV", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "viel Grund, sich zu beklagen:", "tokens": ["viel", "Grund", ",", "sich", "zu", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "was unsereins zu sehen kriegt,", "tokens": ["was", "un\u00b7ser\u00b7eins", "zu", "se\u00b7hen", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ist selten lang zu heben,", "tokens": ["ist", "sel\u00b7ten", "lang", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "von darum also k\u00f6nnten wir", "tokens": ["von", "da\u00b7rum", "al\u00b7so", "k\u00f6nn\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PAV", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "wie Gott in Frankreich leben.", "tokens": ["wie", "Gott", "in", "Fran\u00b7kreich", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch ob auch arme Teufel nur,", "tokens": ["Doch", "ob", "auch", "ar\u00b7me", "Teu\u00b7fel", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das macht uns wenig N\u00f6te,", "tokens": ["das", "macht", "uns", "we\u00b7nig", "N\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wir haben, drum so mancher gern", "tokens": ["wir", "ha\u00b7ben", ",", "drum", "so", "man\u00b7cher", "gern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PAV", "ADV", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sein ganzes Gold uns b\u00f6te,", "tokens": ["sein", "gan\u00b7zes", "Gold", "uns", "b\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "wir haben: jedes \u00c4rgernis", "tokens": ["wir", "ha\u00b7ben", ":", "je\u00b7des", "\u00c4r\u00b7ger\u00b7nis"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ins Gegenteil zu wenden,", "tokens": ["ins", "Ge\u00b7gen\u00b7teil", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ein frohes Herz stets und Humor,", "tokens": ["ein", "fro\u00b7hes", "Herz", "stets", "und", "Hu\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "so kein Gericht kann pf\u00e4nden.", "tokens": ["so", "kein", "Ge\u00b7richt", "kann", "pf\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und klappt auch nirgends was und ist", "tokens": ["Und", "klappt", "auch", "nir\u00b7gends", "was", "und", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PWS", "KON", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "jedwede M\u00fch vergebens,", "tokens": ["jed\u00b7we\u00b7de", "M\u00fch", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wir singen uns ein lustig Lied", "tokens": ["wir", "sin\u00b7gen", "uns", "ein", "lus\u00b7tig", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und freun uns doch des Lebens!", "tokens": ["und", "freun", "uns", "doch", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und das gerade ist die Kunst ...", "tokens": ["Und", "das", "ge\u00b7ra\u00b7de", "ist", "die", "Kunst", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit Geld kann's jeder haben ...", "tokens": ["mit", "Geld", "kann's", "je\u00b7der", "ha\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "auch ohne da\u00df man zahlen mu\u00df,", "tokens": ["auch", "oh\u00b7ne", "da\u00df", "man", "zah\u00b7len", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "am Leben sich zu laben.", "tokens": ["am", "Le\u00b7ben", "sich", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und hier, Herr, sag ich, liegt der Punkt,", "tokens": ["Und", "hier", ",", "Herr", ",", "sag", "ich", ",", "liegt", "der", "Punkt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Punkt, an dem sich's bandelt:", "tokens": ["der", "Punkt", ",", "an", "dem", "sich's", "ban\u00b7delt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und wenn wie Kuckuckskinder nur", "tokens": ["und", "wenn", "wie", "Ku\u00b7ckucks\u00b7kin\u00b7der", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "KOKOM", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Schicksal uns behandelt,", "tokens": ["das", "Schick\u00b7sal", "uns", "be\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "wir kriechen dennoch nicht zu Kreuz", "tokens": ["wir", "krie\u00b7chen", "den\u00b7noch", "nicht", "zu", "Kreuz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und werden keine Mucker:", "tokens": ["und", "wer\u00b7den", "kei\u00b7ne", "Mu\u00b7cker", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "wenn wir dem Gl\u00fcck so kuckuck sind,", "tokens": ["wenn", "wir", "dem", "Gl\u00fcck", "so", "ku\u00b7ckuck", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "ist's uns noch viel kuckucker!", "tokens": ["ist's", "uns", "noch", "viel", "ku\u00b7cku\u00b7cker", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir wollen, was da werden soll,", "tokens": ["Wir", "wol\u00b7len", ",", "was", "da", "wer\u00b7den", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "ADV", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "getrost uns selber schmieden,", "tokens": ["ge\u00b7trost", "uns", "sel\u00b7ber", "schmie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "denn was das Gl\u00fcck im Scho\u00dfe h\u00e4lt,", "tokens": ["denn", "was", "das", "Gl\u00fcck", "im", "Scho\u00b7\u00dfe", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sind doch nur lauter Nieten.", "tokens": ["sind", "doch", "nur", "lau\u00b7ter", "Nie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir knieen nicht, wir betteln nicht,", "tokens": ["Wir", "kni\u00b7e\u00b7en", "nicht", ",", "wir", "bet\u00b7teln", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "es m\u00f6g uns Rosen streuen,", "tokens": ["es", "m\u00f6g", "uns", "Ro\u00b7sen", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "wir haben das Geheimnis, auch", "tokens": ["wir", "ha\u00b7ben", "das", "Ge\u00b7heim\u00b7nis", ",", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "an Dornen uns zu freuen.", "tokens": ["an", "Dor\u00b7nen", "uns", "zu", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So stehn wir stolz und trotzgewillt,", "tokens": ["So", "stehn", "wir", "stolz", "und", "trotz\u00b7ge\u00b7willt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wenn andre furchtsam zagen,", "tokens": ["wenn", "and\u00b7re", "furcht\u00b7sam", "za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "wir wissen, was wir wollen, und", "tokens": ["wir", "wis\u00b7sen", ",", "was", "wir", "wol\u00b7len", ",", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVINF", "$,", "PRELS", "PPER", "VMFIN", "$,", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wir wissen, was wir wagen!", "tokens": ["wir", "wis\u00b7sen", ",", "was", "wir", "wa\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und l\u00f6st Freund Hein die Frage dann", "tokens": ["Und", "l\u00f6st", "Freund", "Hein", "die", "Fra\u00b7ge", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "NE", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zum Schlu\u00df unwiderleglich,", "tokens": ["zum", "Schlu\u00df", "un\u00b7wi\u00b7der\u00b7leg\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "so haben wir's uns wenigstens", "tokens": ["so", "ha\u00b7ben", "wir's", "uns", "we\u00b7nigs\u00b7tens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "so froh gemacht als m\u00f6glich!", "tokens": ["so", "froh", "ge\u00b7macht", "als", "m\u00f6g\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn Geld im Beutel Sorgen macht,", "tokens": ["Wenn", "Geld", "im", "Beu\u00b7tel", "Sor\u00b7gen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie reiche Leute sagen,", "tokens": ["wie", "rei\u00b7che", "Leu\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "von uns dann h\u00e4tte wahrlich keins", "tokens": ["von", "uns", "dann", "h\u00e4t\u00b7te", "wahr\u00b7lich", "keins"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "ADV", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "viel Grund, sich zu beklagen:", "tokens": ["viel", "Grund", ",", "sich", "zu", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word"