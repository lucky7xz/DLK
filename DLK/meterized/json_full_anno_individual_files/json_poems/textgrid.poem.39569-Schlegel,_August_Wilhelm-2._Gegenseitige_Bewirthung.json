{"textgrid.poem.39569": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "2. Gegenseitige Bewirthung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst brachte seinem Schiller Goethe", "tokens": ["Erst", "brach\u00b7te", "sei\u00b7nem", "Schil\u00b7ler", "Goe\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das derb materiell Concrete:", "tokens": ["Das", "derb", "ma\u00b7te\u00b7ri\u00b7ell", "Con\u00b7cre\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das sollt' ihm st\u00e4rken Leib und Seele;", "tokens": ["Das", "sollt'", "ihm", "st\u00e4r\u00b7ken", "Leib", "und", "See\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch w\u00fcrgt' es hart ihn in der Kehle,", "tokens": ["Doch", "w\u00fcrgt'", "es", "hart", "ihn", "in", "der", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was Niemand leichtlich wohl vermeidet,", "tokens": ["Was", "Nie\u00b7mand", "leicht\u00b7lich", "wohl", "ver\u00b7mei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn er die Krebs' in Viertel schneidet.", "tokens": ["Wenn", "er", "die", "Krebs'", "in", "Vier\u00b7tel", "schnei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann brachte Schiller das Abstracte,", "tokens": ["Dann", "brach\u00b7te", "Schil\u00b7ler", "das", "A\u00b7bstrac\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch das Verzwickte, das Vertrakte.", "tokens": ["Auch", "das", "Ver\u00b7zwick\u00b7te", ",", "das", "Ver\u00b7trak\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da schnitt nun Goethe viel Grimassen:", "tokens": ["Da", "schnitt", "nun", "Goe\u00b7the", "viel", "Gri\u00b7mas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wu\u00dft' er sich ein Herz zu fa\u00dfen.", "tokens": ["Doch", "wu\u00dft'", "er", "sich", "ein", "Herz", "zu", "fa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Konnt' es dem Gaumen nicht behagen,", "tokens": ["Konnt'", "es", "dem", "Gau\u00b7men", "nicht", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verdaut' er's doch mit tapferm Magen.", "tokens": ["Ver\u00b7daut'", "er's", "doch", "mit", "tap\u00b7ferm", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So lebten sie, in solchem Handel,", "tokens": ["So", "leb\u00b7ten", "sie", ",", "in", "sol\u00b7chem", "Han\u00b7del", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Friedlich beisammen ohne Wandel:", "tokens": ["Fried\u00b7lich", "bei\u00b7sam\u00b7men", "oh\u00b7ne", "Wan\u00b7del", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nie sah man zu der Welt Gedeihen", "tokens": ["Nie", "sah", "man", "zu", "der", "Welt", "Ge\u00b7dei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich edle Geister so casteien.", "tokens": ["Sich", "ed\u00b7le", "Geis\u00b7ter", "so", "cas\u00b7tei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df, Publicum, dich's nicht verdrie\u00dfen!", "tokens": ["La\u00df", ",", "Pub\u00b7li\u00b7cum", ",", "dich's", "nicht", "ver\u00b7drie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM.la", "$,", "PRELS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.t