{"textgrid.poem.43324": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdu Amor bessre deinen Bogen!", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu Amor bessre deinen Bogen!", "tokens": ["\u00bb", "du", "A\u00b7mor", "bess\u00b7re", "dei\u00b7nen", "Bo\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sehn ist wohl zu schlaff.", "tokens": ["Die", "Sehn", "ist", "wohl", "zu", "schlaff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drey Pfeile sind nun schon nach meiner Brust geflogen,", "tokens": ["Drey", "Pfei\u00b7le", "sind", "nun", "schon", "nach", "mei\u00b7ner", "Brust", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und keiner, keiner traf. \u2013\u00ab", "tokens": ["Und", "kei\u00b7ner", ",", "kei\u00b7ner", "traf", ".", "\u2013", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da hat mein Spott ihn weggetrieben!", "tokens": ["Da", "hat", "mein", "Spott", "ihn", "weg\u00b7ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFI