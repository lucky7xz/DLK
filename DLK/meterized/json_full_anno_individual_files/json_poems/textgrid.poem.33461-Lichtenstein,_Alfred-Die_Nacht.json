{"textgrid.poem.33461": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vertr\u00e4umte Polizisten watscheln bei Laternen.", "tokens": ["Ver\u00b7tr\u00e4um\u00b7te", "Po\u00b7li\u00b7zis\u00b7ten", "wat\u00b7scheln", "bei", "La\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zerbrochne Bettler meckern, wenn sie Leute ahnen.", "tokens": ["Zer\u00b7broch\u00b7ne", "Bett\u00b7ler", "me\u00b7ckern", ",", "wenn", "sie", "Leu\u00b7te", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,", "KOUS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An manchen Ecken stottern starke Stra\u00dfenbahnen,", "tokens": ["An", "man\u00b7chen", "E\u00b7cken", "stot\u00b7tern", "star\u00b7ke", "Stra\u00b7\u00dfen\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sanfte Autodroschken fallen zu den Sternen.", "tokens": ["Und", "sanf\u00b7te", "Au\u00b7to\u00b7droschken", "fal\u00b7len", "zu", "den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Um harte H\u00e4user humpeln Huren hin und wieder,", "tokens": ["Um"