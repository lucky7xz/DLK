{"textgrid.poem.67198": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sagen, die der Erde sich entfernen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sagen, die der Erde sich entfernen,", "tokens": ["Die", "Sa\u00b7gen", ",", "die", "der", "Er\u00b7de", "sich", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Geiste, der gewesen ist und wiederkehret,", "tokens": ["Vom", "Geis\u00b7te", ",", "der", "ge\u00b7we\u00b7sen", "ist", "und", "wie\u00b7der\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VAPP", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie kehren zu der Menschheit sich, und vieles lernen", "tokens": ["Sie", "keh\u00b7ren", "zu", "der", "Menschheit", "sich", ",", "und", "vie\u00b7les", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "$,", "KON", "PIS", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wir aus der Zeit, die eilends sich verzehret.", "tokens": ["Wir", "aus", "der", "Zeit", ",", "die", "ei\u00b7lends", "sich", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Bilder der Vergangenheit sind nicht verlassen", "tokens": ["Die", "Bil\u00b7der", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "sind", "nicht", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von der Natur, als wie die Tag' verblassen", "tokens": ["Von", "der", "Na\u00b7tur", ",", "als", "wie", "die", "Tag'", "ver\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im hohen Sommer, kehrt der Herbst zur Erde nieder,", "tokens": ["Im", "ho\u00b7hen", "Som\u00b7mer", ",", "kehrt", "der", "Herbst", "zur", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Geist der Schauer findet sich am Himmel wieder.", "tokens": ["Der", "Geist", "der", "Schau\u00b7er", "fin\u00b7det", "sich", "am", "Him\u00b7mel", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In kurzer Zeit hat vieles sich geendet,", "tokens": ["In", "kur\u00b7zer", "Zeit", "hat", "vie\u00b7les", "sich", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Landmann, der am Pfluge sich gezeiget,", "tokens": ["Der", "Land\u00b7mann", ",", "der", "am", "Pflu\u00b7ge", "sich", "ge\u00b7zei\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er siehet, wie das Jahr sich frohem Ende neiget,", "tokens": ["Er", "sie\u00b7het", ",", "wie", "das", "Jahr", "sich", "fro\u00b7hem", "En\u00b7de", "nei\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In solchen Bildern ist des Menschen Tag vollendet.", "tokens": ["In", "sol\u00b7chen", "Bil\u00b7dern", "ist", "des", "Men\u00b7schen", "Tag", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Erde Rund mit Felsen ausgezieret", "tokens": ["Der", "Er\u00b7de", "Rund", "mit", "Fel\u00b7sen", "aus\u00b7ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist wie die Wolke nicht, die abends sich verlieret,", "tokens": ["Ist", "wie", "die", "Wol\u00b7ke", "nicht", ",", "die", "a\u00b7bends", "sich", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "PTKNEG", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es zeiget sich mit einem goldnen Tage,", "tokens": ["Es", "zei\u00b7get", "sich", "mit", "ei\u00b7nem", "gold\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die Vollkommenheit ist ohne Klage.", "tokens": ["Und", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "ist", "oh\u00b7ne", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Sagen, die der Erde sich entfernen,", "tokens": ["Die", "Sa\u00b7gen", ",", "die", "der", "Er\u00b7de", "sich", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Geiste, der gewesen ist und wiederkehret,", "tokens": ["Vom", "Geis\u00b7te", ",", "der", "ge\u00b7we\u00b7sen", "ist", "und", "wie\u00b7der\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VAPP", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie kehren zu der Menschheit sich, und vieles lernen", "tokens": ["Sie", "keh\u00b7ren", "zu", "der", "Menschheit", "sich", ",", "und", "vie\u00b7les", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "$,", "KON", "PIS", "VVINF"], "me