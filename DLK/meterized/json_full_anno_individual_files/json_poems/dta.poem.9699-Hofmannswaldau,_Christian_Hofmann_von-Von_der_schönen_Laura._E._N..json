{"dta.poem.9699": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der sch\u00f6nen Laura.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Laura denckt/ sie sey vortreflich sch\u00f6ne.", "tokens": ["Die", "Lau\u00b7ra", "denckt", "/", "sie", "sey", "vor\u00b7tre\u00b7flich", "sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VAFIN", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn ich sie nur h\u00f6hne/", "tokens": ["Und", "wenn", "ich", "sie", "nur", "h\u00f6h\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So bildet sie sich doch die wahrheit ein.", "tokens": ["So", "bil\u00b7det", "sie", "sich", "doch", "die", "wahr\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sag ich zum spa\u00df/ die wangen w\u00e4ren rosen/", "tokens": ["Sag", "ich", "zum", "spa\u00df", "/", "die", "wan\u00b7gen", "w\u00e4\u00b7ren", "ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$(", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die dennoch quitten seyn/", "tokens": ["Die", "den\u00b7noch", "quit\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So wei\u00df sie sich abscheulich lieb-zu kosen.", "tokens": ["So", "wei\u00df", "sie", "sich", "ab\u00b7scheu\u00b7lich", "lie\u00b7bzu", "ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nenn ich die augen sonnen/", "tokens": ["Nenn", "ich", "die", "au\u00b7gen", "son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die um und um mit butter voll geronnen/", "tokens": ["Die", "um", "und", "um", "mit", "but\u00b7ter", "voll", "ge\u00b7ron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "KON", "APPR", "APPR", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So giebt sie mir ein solch entz\u00fcckt gesichte/", "tokens": ["So", "giebt", "sie", "mir", "ein", "solch", "ent\u00b7z\u00fcckt", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "PIAT", "VVPP", "VVFIN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Als wie ein kater niest.", "tokens": ["Als", "wie", "ein", "ka\u00b7ter", "niest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Nenn ich den hal\u00df und busen alabastern/", "tokens": ["Nenn", "ich", "den", "hal\u00df", "und", "bu\u00b7sen", "a\u00b7la\u00b7bas\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der gelber marmel ist/", "tokens": ["Der", "gel\u00b7ber", "mar\u00b7mel", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "So schicket sie sich gar zu einem ku\u00dfe/", "tokens": ["So", "schi\u00b7cket", "sie", "sich", "gar", "zu", "ei\u00b7nem", "ku\u00b7\u00dfe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da denck ich mit verdru\u00dfe:", "tokens": ["Da", "denck", "ich", "mit", "ver\u00b7dru\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Du blinde welt/ wie lange schenckstu rastern?", "tokens": ["Du", "blin\u00b7de", "welt", "/", "wie", "lan\u00b7ge", "schencks\u00b7tu", "ras\u00b7tern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PWAV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}