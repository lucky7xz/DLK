{"textgrid.poem.35005": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Hohelied", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Weibes Leib ist ein Gedicht,", "tokens": ["Des", "Wei\u00b7bes", "Leib", "ist", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gott der Herr geschrieben", "tokens": ["Das", "Gott", "der", "Herr", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ins gro\u00dfe Stammbuch der Natur,", "tokens": ["Ins", "gro\u00b7\u00dfe", "Stamm\u00b7buch", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ihn der Geist getrieben.", "tokens": ["Als", "ihn", "der", "Geist", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, g\u00fcnstig war die Stunde ihm,", "tokens": ["Ja", ",", "g\u00fcns\u00b7tig", "war", "die", "Stun\u00b7de", "ihm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gott war hochbegeistert;", "tokens": ["Der", "Gott", "war", "hoch\u00b7be\u00b7geis\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat den spr\u00f6den, rebellischen Stoff", "tokens": ["Er", "hat", "den", "spr\u00f6\u00b7den", ",", "re\u00b7bel\u00b7li\u00b7schen", "Stoff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "Ganz k\u00fcnstlerisch bemeistert.", "tokens": ["Ganz", "k\u00fcnst\u00b7le\u00b7risch", "be\u00b7meis\u00b7tert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "F\u00fcrwahr, der Leib des Weibes ist", "tokens": ["F\u00fcr\u00b7wahr", ",", "der", "Leib", "des", "Wei\u00b7bes", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Hohelied der Lieder;", "tokens": ["Das", "Ho\u00b7he\u00b7lied", "der", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gar wunderbare Strophen sind", "tokens": ["Gar", "wun\u00b7der\u00b7ba\u00b7re", "Stro\u00b7phen", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die schlanken, wei\u00dfen Glieder.", "tokens": ["Die", "schlan\u00b7ken", ",", "wei\u00b7\u00dfen", "Glie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O welche g\u00f6ttliche Idee", "tokens": ["O", "wel\u00b7che", "g\u00f6tt\u00b7li\u00b7che", "I\u00b7dee"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PWAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist dieser Hals, der blanke,", "tokens": ["Ist", "die\u00b7ser", "Hals", ",", "der", "blan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Worauf sich wiegt der kleine Kopf,", "tokens": ["Wo\u00b7rauf", "sich", "wiegt", "der", "klei\u00b7ne", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der lockige Hauptgedanke!", "tokens": ["Der", "lo\u00b7cki\u00b7ge", "Haupt\u00b7ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Br\u00fcstchen Rosenknospen sind", "tokens": ["Der", "Br\u00fcst\u00b7chen", "Ro\u00b7sen\u00b7knos\u00b7pen", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Epigrammatisch gefeilet;", "tokens": ["E\u00b7pig\u00b7ram\u00b7ma\u00b7tisch", "ge\u00b7fei\u00b7let", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Uns\u00e4glich entz\u00fcckend ist die Z\u00e4sur,", "tokens": ["Un\u00b7s\u00e4g\u00b7lich", "ent\u00b7z\u00fc\u00b7ckend", "ist", "die", "Z\u00e4\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die streng den Busen teilet.", "tokens": ["Die", "streng", "den", "Bu\u00b7sen", "tei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den plastischen Sch\u00f6pfer offenbart", "tokens": ["Den", "plas\u00b7ti\u00b7schen", "Sch\u00f6p\u00b7fer", "of\u00b7fen\u00b7bart"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der H\u00fcften Parallele;", "tokens": ["Der", "H\u00fcf\u00b7ten", "Par\u00b7al\u00b7le\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Zwischensatz mit dem Feigenblatt", "tokens": ["Der", "Zwi\u00b7schen\u00b7satz", "mit", "dem", "Fei\u00b7gen\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist auch eine sch\u00f6ne Stelle.", "tokens": ["Ist", "auch", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Das ist kein abstraktes Begriffspoem!", "tokens": ["Das", "ist", "kein", "abs\u00b7trak\u00b7tes", "Be\u00b7griff\u00b7spoem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Das Lied hat Fleisch und Rippen,", "tokens": ["Das", "Lied", "hat", "Fleisch", "und", "Rip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat Hand und Fu\u00df; es lacht und k\u00fc\u00dft", "tokens": ["Hat", "Hand", "und", "Fu\u00df", ";", "es", "lacht", "und", "k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sch\u00f6ngereimten Lippen.", "tokens": ["Mit", "sch\u00f6n\u00b7ge\u00b7reim\u00b7ten", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hier atmet wahre Poesie!", "tokens": ["Hier", "at\u00b7met", "wah\u00b7re", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Anmut in jeder Wendung!", "tokens": ["An\u00b7mut", "in", "je\u00b7der", "Wen\u00b7dung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und auf der Stirne tr\u00e4gt das Lied", "tokens": ["Und", "auf", "der", "Stir\u00b7ne", "tr\u00e4gt", "das", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Stempel der Vollendung.", "tokens": ["Den", "Stem\u00b7pel", "der", "Vol\u00b7len\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Lobsingen will ich dir, o Herr,", "tokens": ["Lob\u00b7sin\u00b7gen", "will", "ich", "dir", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dich im Staub anbeten!", "tokens": ["Und", "dich", "im", "Staub", "an\u00b7be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind nur St\u00fcmper gegen dich,", "tokens": ["Wir", "sind", "nur", "St\u00fcm\u00b7per", "ge\u00b7gen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den himmlischen Poeten.", "tokens": ["Den", "himm\u00b7li\u00b7schen", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Versenken will ich mich, o Herr,", "tokens": ["Ver\u00b7sen\u00b7ken", "will", "ich", "mich", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deines Liedes Pr\u00e4chten;", "tokens": ["In", "dei\u00b7nes", "Lie\u00b7des", "Pr\u00e4ch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich widme seinem Studium", "tokens": ["Ich", "wid\u00b7me", "sei\u00b7nem", "Stu\u00b7di\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Tag mitsamt den N\u00e4chten.", "tokens": ["Den", "Tag", "mit\u00b7samt", "den", "N\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ja, Tag und Nacht studier ich dran,", "tokens": ["Ja", ",", "Tag", "und", "Nacht", "stu\u00b7dier", "ich", "dran", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will keine Zeit verlieren;", "tokens": ["Will", "kei\u00b7ne", "Zeit", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Beine werden mir so d\u00fcnn \u2013", "tokens": ["Die", "Bei\u00b7ne", "wer\u00b7den", "mir", "so", "d\u00fcnn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kommt vom vielen Studieren.", "tokens": ["Das", "kommt", "vom", "vie\u00b7len", "Stu\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "PIAT", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.12": {"line.1": {"text": "Des Weibes Leib ist ein Gedicht,", "tokens": ["Des", "Wei\u00b7bes", "Leib", "ist", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Gott der Herr geschrieben", "tokens": ["Das", "Gott", "der", "Herr", "ge\u00b7schrie\u00b7ben"], "