: "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Hier atmet wahre Poesie!", "tokens": ["Hier", "at\u00b7met", "wah\u00b7re", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Anmut in jeder Wendung!", "tokens": ["An\u00b7mut", "in", "je\u00b7der", "Wen\u00b7dung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und auf der Stirne tr\u00e4gt das Lied", "tokens": ["Und", "auf", "der", "Stir\u00b7ne", "tr\u00e4gt", "das", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Stempel der Vollendung.", "tokens": ["Den", "Stem\u00b7pel", "der", "Vol\u00b7len\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.20": {"line.1": {"text": "Lobsingen will ich dir, o Herr,", "tokens": ["Lob\u00b7sin\u00b7gen", "will", "ich", "dir", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dich im Staub anbeten!", "tokens": ["Und", "dich", "im", "Staub", "an\u00b7be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind nur St\u00fcmper gegen dich,", "tokens": ["Wir", "sind", "nur", "St\u00fcm\u00b7per", "ge\u00b7gen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", 