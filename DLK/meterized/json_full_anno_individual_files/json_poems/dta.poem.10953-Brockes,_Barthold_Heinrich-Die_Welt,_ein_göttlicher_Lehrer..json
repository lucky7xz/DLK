{"dta.poem.10953": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Welt,  \n ein g\u00f6ttlicher Lehrer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stral der unsichtbaren Sonne! Weltkreis, der noch mehr,\nals sich", "tokens": ["Stral", "der", "un\u00b7sicht\u00b7ba\u00b7ren", "Son\u00b7ne", "!", "Welt\u00b7kreis", ",", "der", "noch", "mehr", ",", "als", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "NN", "$,", "PRELS", "ADV", "ADV", "$,", "KOUS", "PRF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Unsern Augen, unserm Geist,", "tokens": ["Un\u00b7sern", "Au\u00b7gen", ",", "un\u00b7serm", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Sch\u00f6nheit, Pracht und Ordnung, die verborgne Gott-", "tokens": ["In", "der", "Sch\u00f6n\u00b7heit", ",", "Pracht", "und", "Ord\u00b7nung", ",", "die", "ver\u00b7borg\u00b7ne", "Got\u00b7t"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Man kann durch dich blo\u00df allein, und wahrhaftig, ohne dich,", "tokens": ["Man", "kann", "durch", "dich", "blo\u00df", "al\u00b7lein", ",", "und", "wahr\u00b7haf\u00b7tig", ",", "oh\u00b7ne", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "ADV", "ADV", "$,", "KON", "ADJD", "$,", "KOUI", "PPER", "$,"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Keinen Gott verstehn und finden!", "tokens": ["Kei\u00b7nen", "Gott", "ver\u00b7stehn", "und", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wesen! worin unsre Seelen, durch die Sinnen, ihn empfinden,", "tokens": ["We\u00b7sen", "!", "wo\u00b7rin", "uns\u00b7re", "See\u00b7len", ",", "durch", "die", "Sin\u00b7nen", ",", "ihn", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Worin wir, durch h\u00f6ren, riechen, f\u00fchlen, auch durch sehn und", "tokens": ["Wo\u00b7rin", "wir", ",", "durch", "h\u00f6\u00b7ren", ",", "rie\u00b7chen", ",", "f\u00fch\u00b7len", ",", "auch", "durch", "sehn", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPR", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "ADV", "APPR", "VVINF", "KON"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Eine Weisheit, Liebe, Macht, so die Gottheit selbst, entdecken,", "tokens": ["Ei\u00b7ne", "Weis\u00b7heit", ",", "Lie\u00b7be", ",", "Macht", ",", "so", "die", "Got\u00b7theit", "selbst", ",", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "ADV", "ART", "NN", "ADV", "$,", "VVINF", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Und worin, wenn wir mit Freuden nnd Bewundrung dich", "tokens": ["Und", "wo\u00b7rin", ",", "wenn", "wir", "mit", "Freu\u00b7den", "nnd", "Be\u00b7wund\u00b7rung", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "PPER"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Als ein w\u00fcrdig Werk des Sch\u00f6pfers, wir den Sch\u00f6pfer selbst", "tokens": ["Als", "ein", "w\u00fcr\u00b7dig", "Werk", "des", "Sch\u00f6p\u00b7fers", ",", "wir", "den", "Sch\u00f6p\u00b7fer", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "NN", "ART", "NN", "$,", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Aller Himmel Himmel Heere, alle Welt hat Gott gemacht,", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel", "Hee\u00b7re", ",", "al\u00b7le", "Welt", "hat", "Gott", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "NN", "$,", "PIAT", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Wie er wollt, da\u00df sie entst\u00fcnden. Es entdecket ihre Pracht", "tokens": ["Wie", "er", "wollt", ",", "da\u00df", "sie", "ent\u00b7st\u00fcn\u00b7den", ".", "Es", "ent\u00b7de\u00b7cket", "ih\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "$,", "KOUS", "PPER", "VVPP", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Seinen \u00fcberall verborgnen, \u00fcberall vorhandnen Schein.", "tokens": ["Sei\u00b7nen", "\u00fc\u00b7be\u00b7rall", "ver\u00b7borg\u00b7nen", ",", "\u00fc\u00b7be\u00b7rall", "vor\u00b7hand\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VVINF", "$,", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "La\u00df doch deine Seele dann, liebster Mensch, nicht Sinn-los seyn!", "tokens": ["La\u00df", "doch", "dei\u00b7ne", "See\u00b7le", "dann", ",", "liebs\u00b7ter", "Mensch", ",", "nicht", "Sinn\u00b7los", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "ADV", "$,", "ADJA", "NN", "$,", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Weil, wenn du die Creatur, und die sch\u00f6ne Welt nicht sp\u00fcrest,", "tokens": ["Weil", ",", "wenn", "du", "die", "Crea\u00b7tur", ",", "und", "die", "sch\u00f6\u00b7ne", "Welt", "nicht", "sp\u00fc\u00b7rest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Du, nicht deine Lust allein, dich, ja selber Gott, verlierest.", "tokens": ["Du", ",", "nicht", "dei\u00b7ne", "Lust", "al\u00b7lein", ",", "dich", ",", "ja", "sel\u00b7ber", "Gott", ",", "ver\u00b7lie\u00b7rest", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PTKNEG", "PPOSAT", "NN", "ADV", "$,", "PPER", "$,", "ADV", "ADV", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}