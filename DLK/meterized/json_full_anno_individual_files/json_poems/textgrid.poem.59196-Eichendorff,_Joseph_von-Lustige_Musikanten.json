{"textgrid.poem.59196": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Lustige Musikanten", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wald, der Wald! da\u00df Gott ihn gr\u00fcn erhalt,", "tokens": ["Der", "Wald", ",", "der", "Wald", "!", "da\u00df", "Gott", "ihn", "gr\u00fcn", "er\u00b7halt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "KOUS", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gibt gut Quartier und nimmt doch nichts daf\u00fcr.", "tokens": ["Gibt", "gut", "Quar\u00b7tier", "und", "nimmt", "doch", "nichts", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "VVFIN", "ADV", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zum gr\u00fcnen Wald wir Herberg halten,", "tokens": ["Zum", "gr\u00fc\u00b7nen", "Wald", "wir", "Her\u00b7berg", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn Hoffart ist nicht unser Ziel,", "tokens": ["Denn", "Hof\u00b7fart", "ist", "nicht", "un\u00b7ser", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Wirtshaus, wo wir nicht bezahlten,", "tokens": ["Im", "Wirts\u00b7haus", ",", "wo", "wir", "nicht", "be\u00b7zahl\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es war der Ehre gar zuviel.", "tokens": ["Es", "war", "der", "Eh\u00b7re", "gar", "zu\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wirt, er wollt uns gar nicht lassen,", "tokens": ["Der", "Wirt", ",", "er", "wollt", "uns", "gar", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie lie\u00dfen Kann und Kartenspiel,", "tokens": ["Sie", "lie\u00b7\u00dfen", "Kann", "und", "Kar\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ganze Stadt war in den Gassen,", "tokens": ["Die", "gan\u00b7ze", "Stadt", "war", "in", "den", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und von den B\u00e4nken mit Gebraus", "tokens": ["Und", "von", "den", "B\u00e4n\u00b7ken", "mit", "Ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "St\u00fcrzt' die Schule heraus,", "tokens": ["St\u00fcrzt'", "die", "Schu\u00b7le", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Wuchs der Haufe von Haus zu Haus,", "tokens": ["Wuchs", "der", "Hau\u00b7fe", "von", "Haus", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Schwenkt' die M\u00fctzen und jubelt' und wogt',", "tokens": ["Schwenkt'", "die", "M\u00fct\u00b7zen", "und", "ju\u00b7belt'", "und", "wogt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Der Hatschier, die Stadtwacht, der Bettelvogt,", "tokens": ["Der", "Hat\u00b7schier", ",", "die", "Stadt\u00b7wacht", ",", "der", "Bet\u00b7tel\u00b7vogt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "--+-++-+-+", "measure": "anapaest.init"}, "line.13": {"text": "Wie wenn ein Prinz zieht auf die Freit,", "tokens": ["Wie", "wenn", "ein", "Prinz", "zieht", "auf", "die", "Freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Gab alles, alles uns f\u00fcrstlich Geleit.", "tokens": ["Gab", "al\u00b7les", ",", "al\u00b7les", "uns", "f\u00fcrst\u00b7lich", "Ge\u00b7leit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PIS", "PPER", "ADJD", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Wir aber schlugen den Markt hinab", "tokens": ["Wir", "a\u00b7ber", "schlu\u00b7gen", "den", "Markt", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Uns durch die Leut mit dem Wanderstab,", "tokens": ["Uns", "durch", "die", "Leut", "mit", "dem", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und hoch mit dem Tamburin, da\u00df es schallt' \u2013", "tokens": ["Und", "hoch", "mit", "dem", "Tam\u00b7bu\u00b7rin", ",", "da\u00df", "es", "schallt'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Zum Wald, zum Wald, zum sch\u00f6nen, gr\u00fcnen Wald!", "tokens": ["Zum", "Wald", ",", "zum", "Wald", ",", "zum", "sch\u00f6\u00b7nen", ",", "gr\u00fc\u00b7nen", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und da nun alle schlafen gingen,", "tokens": ["Und", "da", "nun", "al\u00b7le", "schla\u00b7fen", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wald steckt' seine Irrlicht' an,", "tokens": ["Der", "Wald", "steckt'", "sei\u00b7ne", "Irr\u00b7licht'", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fr\u00f6sche tapfer St\u00e4ndchen bringen,", "tokens": ["Die", "Fr\u00f6\u00b7sche", "tap\u00b7fer", "St\u00e4nd\u00b7chen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fledermaus schwirrt leis voran,", "tokens": ["Die", "Fle\u00b7der\u00b7maus", "schwirrt", "leis", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und in dem Flu\u00df auf feuchtem Steine", "tokens": ["Und", "in", "dem", "Flu\u00df", "auf", "feuch\u00b7tem", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "G\u00e4hnt laut der alte Wassermann,", "tokens": ["G\u00e4hnt", "laut", "der", "al\u00b7te", "Was\u00b7ser\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Str\u00e4hlt sich den Bart im Mondenscheine,", "tokens": ["Str\u00e4hlt", "sich", "den", "Bart", "im", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und fragt ein Irrlicht, wer wir sind?", "tokens": ["Und", "fragt", "ein", "Irr\u00b7licht", ",", "wer", "wir", "sind", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das aber duckt sich geschwind;", "tokens": ["Das", "a\u00b7ber", "duckt", "sich", "ge\u00b7schwind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Denn \u00fcber ihn weg im Wind", "tokens": ["Denn", "\u00fc\u00b7ber", "ihn", "weg", "im", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Durch die Wipfel der wilde J\u00e4ger geht,", "tokens": ["Durch", "die", "Wip\u00b7fel", "der", "wil\u00b7de", "J\u00e4\u00b7ger", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und auf dem alten Turm sich dreht", "tokens": ["Und", "auf", "dem", "al\u00b7ten", "Turm", "sich", "dreht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und kr\u00e4ht der Wetterhahn uns nach:", "tokens": ["Und", "kr\u00e4ht", "der", "Wet\u00b7ter\u00b7hahn", "uns", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ob wir nicht einkehrn unter sein Dach?", "tokens": ["Ob", "wir", "nicht", "ein\u00b7kehrn", "un\u00b7ter", "sein", "Dach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "O Gockel, verfallen ist ja dein Haus,", "tokens": ["O", "Go\u00b7ckel", ",", "ver\u00b7fal\u00b7len", "ist", "ja", "dein", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVPP", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Es sieht die Eule zum Fenster heraus,", "tokens": ["Es", "sieht", "die", "Eu\u00b7le", "zum", "Fens\u00b7ter", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und aus allen Toren rauschet der Wald.", "tokens": ["Und", "aus", "al\u00b7len", "To\u00b7ren", "rau\u00b7schet", "der", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Der Wald, der Wald, der sch\u00f6ne, gr\u00fcne Wald!", "tokens": ["Der", "Wald", ",", "der", "Wald", ",", "der", "sch\u00f6\u00b7ne", ",", "gr\u00fc\u00b7ne", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wenn wir m\u00fcd einst, sehn wir blinken", "tokens": ["Und", "wenn", "wir", "m\u00fcd", "einst", ",", "sehn", "wir", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "$,", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eine goldne Stadt still \u00fcberm Land,", "tokens": ["Ei\u00b7ne", "gold\u00b7ne", "Stadt", "still", "\u00fc\u00b7berm", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Am Tor Sankt Peter schon tut winken:", "tokens": ["Am", "Tor", "Sankt", "Pe\u00b7ter", "schon", "tut", "win\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbnur hier herein, Herr Musikant!\u00ab", "tokens": ["\u00bb", "nur", "hier", "her\u00b7ein", ",", "Herr", "Mu\u00b7si\u00b7kant", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "PTKVZ", "$,", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Engel von den Zinnen fragen,", "tokens": ["Die", "En\u00b7gel", "von", "den", "Zin\u00b7nen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie sie uns erst recht erkannt,", "tokens": ["Und", "wie", "sie", "uns", "erst", "recht", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie gleich die silbernen Pauken schlagen,", "tokens": ["Sie", "gleich", "die", "sil\u00b7ber\u00b7nen", "Pau\u00b7ken", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sankt Peter selbst die Becken schwenkt,", "tokens": ["Sankt", "Pe\u00b7ter", "selbst", "die", "Be\u00b7cken", "schwenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und voll Geigen h\u00e4ngt", "tokens": ["Und", "voll", "Gei\u00b7gen", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Der Himmel, C\u00e4cilia an zu streichen f\u00e4ngt,", "tokens": ["Der", "Him\u00b7mel", ",", "C\u00e4\u00b7ci\u00b7lia", "an", "zu", "strei\u00b7chen", "f\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "APZR", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Dazwischen Hoch vivat! da\u00df es prasselt und pufft,", "tokens": ["Da\u00b7zwi\u00b7schen", "Hoch", "vi\u00b7vat", "!", "da\u00df", "es", "pras\u00b7selt", "und", "pufft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Werfen die andern vom Wall in die Luft", "tokens": ["Wer\u00b7fen", "die", "an\u00b7dern", "vom", "Wall", "in", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Sternschnuppen, Kom