{"dta.poem.22516": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "66. Von der Allerseeligsten Jungfrauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Der Jungfr\u00e4uliche Leib/ der unser Himmelbrodt/", "tokens": ["Der", "Jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Leib", "/", "der", "un\u00b7ser", "Him\u00b7mel\u00b7brodt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "$("], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In sich beschlossen hilt/ ist warlich nicht mehr Todt.", "tokens": ["In", "sich", "be\u00b7schlos\u00b7sen", "hilt", "/", "ist", "war\u00b7lich", "nicht", "mehr", "Todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "VVFIN", "$(", "VAFIN", "ADV", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es fault kein Cederbaum: so w\u00e4r\u2019 es auch nicht fein/", "tokens": ["Es", "fault", "kein", "Ce\u00b7der\u00b7baum", ":", "so", "w\u00e4r'", "es", "auch", "nicht", "fein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann ausserm Tempel GOtts sein\u2019 Arche solte seyn.", "tokens": ["Wann", "aus\u00b7serm", "Tem\u00b7pel", "Gotts", "sein'", "Ar\u00b7che", "sol\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word",