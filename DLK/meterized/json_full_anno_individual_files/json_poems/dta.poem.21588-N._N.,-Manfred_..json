{"dta.poem.21588": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Manfred .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O schn\u00f6der Tod, was meidest Du die Brust,", "tokens": ["O", "schn\u00f6\u00b7der", "Tod", ",", "was", "mei\u00b7dest", "Du", "die", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die l\u00e4ngst bereit, als Freund Dich zu empfangen?", "tokens": ["Die", "l\u00e4ngst", "be\u00b7reit", ",", "als", "Freund", "Dich", "zu", "emp\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "KOUS", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was machst Du Herzen still, die noch mit Lust", "tokens": ["Was", "machst", "Du", "Her\u00b7zen", "still", ",", "die", "noch", "mit", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An dieser Welt und ihren Freuden hangen!", "tokens": ["An", "die\u00b7ser", "Welt", "und", "ih\u00b7ren", "Freu\u00b7den", "han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O sieh mich an! Wie ist mein Aug\u2019 so m\u00fcd\u2019!", "tokens": ["O", "sieh", "mich", "an", "!", "Wie", "ist", "mein", "Aug'", "so", "m\u00fcd'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie matt der Fu\u00df von all\u2019 den falschen Wegen!", "tokens": ["Wie", "matt", "der", "Fu\u00df", "von", "all'", "den", "fal\u00b7schen", "We\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Seele stolze Flammen sind vergl\u00fcht,", "tokens": ["Der", "See\u00b7le", "stol\u00b7ze", "Flam\u00b7men", "sind", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Nichts mehr will sich in der Tiefe regen.", "tokens": ["Und", "Nichts", "mehr", "will", "sich", "in", "der", "Tie\u00b7fe", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VMFIN", "PRF", "APPR", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O setz\u2019 ihr vor den s\u00fc\u00dfen Labetrunk,", "tokens": ["O", "setz'", "ihr", "vor", "den", "s\u00fc\u00b7\u00dfen", "La\u00b7be\u00b7trunk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach dem sie ach so sehnsuchtsvoll begehrt;", "tokens": ["Nach", "dem", "sie", "ach", "so", "sehn\u00b7suchts\u00b7voll", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zur Qual geworden ist Erinnerung,", "tokens": ["Zur", "Qual", "ge\u00b7wor\u00b7den", "ist", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAPP", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und Alles nur noch des Vergessens werth.", "tokens": ["Und", "Al\u00b7les", "nur", "noch", "des", "Ver\u00b7ges\u00b7sens", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}