{"dta.poem.7656": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu werden das was du nicht bist, das was du werden", "tokens": ["Zu", "wer\u00b7den", "das", "was", "du", "nicht", "bist", ",", "das", "was", "du", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "VAFIN", "PDS", "PRELS", "PPER", "PTKNEG", "VAFIN", "$,", "PRELS", "PWS", "PPER", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sollst, was du werden kannst, ist eng der Raum auf Erden.", "tokens": ["Sollst", ",", "was", "du", "wer\u00b7den", "kannst", ",", "ist", "eng", "der", "Raum", "auf", "Er\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWS", "PPER", "VAINF", "VMFIN", "$,", "VAFIN", "ADJD", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist Unendliches, darum aus dieser Zeit", "tokens": ["Es", "ist", "Un\u00b7end\u00b7li\u00b7ches", ",", "da\u00b7rum", "aus", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "$,", "PAV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dehnt es hin\u00fcber sich in die Unendlichkeit.", "tokens": ["Dehnt", "es", "hin\u00b7\u00fc\u00b7ber", "sich", "in", "die", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Getrost! was du hier thust, das nimmst du mit von hinnen;", "tokens": ["Ge\u00b7trost", "!", "was", "du", "hier", "thust", ",", "das", "nimmst", "du", "mit", "von", "hin\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "PPER", "ADV", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "APPR", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was vollendet dort will seyn, mu\u00df hier beginnen.", "tokens": ["Und", "was", "voll\u00b7en\u00b7det", "dort", "will", "seyn", ",", "mu\u00df", "hier", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "VMFIN", "VAINF", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}