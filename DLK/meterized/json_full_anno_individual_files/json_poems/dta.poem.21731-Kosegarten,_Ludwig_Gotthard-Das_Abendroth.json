{"dta.poem.21731": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Abendroth", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Abend bl&#252;ht!", "tokens": ["Der", "A\u00b7bend", "bl", "&#252;", "ht", "!"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Westen gl\u00fcht!", "tokens": ["Der", "Wes\u00b7ten", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wo bist du, holdes Licht, entglommen?", "tokens": ["Wo", "bist", "du", ",", "hol\u00b7des", "Licht", ",", "ent\u00b7glom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus welchem Stern herabgeschwommen?", "tokens": ["Aus", "wel\u00b7chem", "Stern", "her\u00b7ab\u00b7ge\u00b7schwom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein lichter Brand", "tokens": ["Ein", "lich\u00b7ter", "Brand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Flammt See und Land.", "tokens": ["Flammt", "See", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es lodern in dem rothen Scheine", "tokens": ["Es", "lo\u00b7dern", "in", "dem", "ro\u00b7then", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fluren rings, und rings die Hayne.", "tokens": ["Die", "Flu\u00b7ren", "rings", ",", "und", "rings", "die", "Hay\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie sieht so hehr", "tokens": ["Wie", "sieht", "so", "hehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das d\u00fcstre Meer!", "tokens": ["Das", "d\u00fcst\u00b7re", "Meer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Welle tanzt des Glanzes trunken,", "tokens": ["Die", "Wel\u00b7le", "tanzt", "des", "Glan\u00b7zes", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und spr\u00fcht lusttaumelnd Feuerfunken.", "tokens": ["Und", "spr\u00fcht", "lust\u00b7tau\u00b7melnd", "Feu\u00b7er\u00b7fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es mahlt der Strahl", "tokens": ["Es", "mahlt", "der", "Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das liebe Thal,", "tokens": ["Das", "lie\u00b7be", "Thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das sie bewohnt, der Holden Holde", "tokens": ["Das", "sie", "be\u00b7wohnt", ",", "der", "Hol\u00b7den", "Hol\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Rosengluth und mattem Golde.", "tokens": ["Mit", "Ro\u00b7sen\u00b7gluth", "und", "mat\u00b7tem", "Gol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Geuss Hesperus", "tokens": ["Geuss", "Hes\u00b7pe\u00b7rus"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Mit leisem Gruss", "tokens": ["Mit", "lei\u00b7sem", "Gruss"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf sie den Inhalt meiner Lieder,", "tokens": ["Auf", "sie", "den", "In\u00b7halt", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sch\u00f6nsten deiner Rosen nieder.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "dei\u00b7ner", "Ro\u00b7sen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Viel sch\u00f6ner bl\u00fcht,", "tokens": ["Viel", "sch\u00f6\u00b7ner", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Viel w\u00e4rmer gl\u00fcht", "tokens": ["Viel", "w\u00e4r\u00b7mer", "gl\u00fcht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die blasse Rose ihrer Wangen,", "tokens": ["Die", "blas\u00b7se", "Ro\u00b7se", "ih\u00b7rer", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und weckt inbr\u00fcnstiges Verlangen.", "tokens": ["Und", "weckt", "in\u00b7br\u00fcns\u00b7ti\u00b7ges", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Von ihr Ein Blick,", "tokens": ["Von", "ihr", "Ein", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein trauter Nick", "tokens": ["Ein", "trau\u00b7ter", "Nick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Durchzuckt elektrisch Mark und Leben,", "tokens": ["Durch\u00b7zuckt", "e\u00b7lekt\u00b7risch", "Mark", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht den feinsten Nerv erbeben.", "tokens": ["Und", "macht", "den", "feins\u00b7ten", "Nerv", "er\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Drum, Hesperus,", "tokens": ["Drum", ",", "Hes\u00b7pe\u00b7rus", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["PAV", "$,", "NE", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Beut Gruss und Kuss", "tokens": ["Beut", "Gruss", "und", "Kuss"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Herrlichen, der Tadellosen,", "tokens": ["Der", "Herr\u00b7li\u00b7chen", ",", "der", "Ta\u00b7del\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und opfr' ihr deine sch\u00f6nsten Rosen.", "tokens": ["Und", "opf\u00b7r'", "ihr", "dei\u00b7ne", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Bewunderung", "tokens": ["Be\u00b7wun\u00b7de\u00b7rung"], "token_info": ["word"], "pos": ["NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Und Huldigung", "tokens": ["Und", "Hul\u00b7di\u00b7gung"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Heischt nur ", "tokens": ["Heischt", "nur"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Weil Huld und Heiligkeit es hebet.", "tokens": ["Weil", "Huld", "und", "Hei\u00b7lig\u00b7keit", "es", "he\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}