{"dta.poem.19295": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der alte Meister sprach: La\u00df k\u00fcrzlich dir entfalten,", "tokens": ["Der", "al\u00b7te", "Meis\u00b7ter", "sprach", ":", "La\u00df", "k\u00fcrz\u00b7lich", "dir", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VVIMP", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woran im Forschen du, im Wirken dich sollst halten.", "tokens": ["Wo\u00b7ran", "im", "For\u00b7schen", "du", ",", "im", "Wir\u00b7ken", "dich", "sollst", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPER", "$,", "APPRART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Unzug\u00e4ngliches gibt es in der Natur,", "tokens": ["Ein", "Un\u00b7zu\u00b7g\u00e4ng\u00b7li\u00b7ches", "gibt", "es", "in", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ein Zug\u00e4ngliches; die unterscheide nur!", "tokens": ["Und", "ein", "Zu\u00b7g\u00e4ng\u00b7li\u00b7ches", ";", "die", "un\u00b7ter\u00b7schei\u00b7de", "nur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "ART", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer nicht zu scheiden wei\u00df, der qu\u00e4lt sich lebensl\u00e4nglich", "tokens": ["Wer", "nicht", "zu", "schei\u00b7den", "wei\u00df", ",", "der", "qu\u00e4lt", "sich", "le\u00b7bens\u00b7l\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Unzug\u00e4nglichen, und macht es nie zug\u00e4nglich.", "tokens": ["Am", "Un\u00b7zu\u00b7g\u00e4ng\u00b7li\u00b7chen", ",", "und", "macht", "es", "nie", "zu\u00b7g\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wer es wei\u00df, wird ans Zug\u00e4ngliche sich halten,", "tokens": ["Doch", "wer", "es", "wei\u00df", ",", "wird", "ans", "Zu\u00b7g\u00e4ng\u00b7li\u00b7che", "sich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und frei auf festem Grund nach allen Seiten walten.", "tokens": ["Und", "frei", "auf", "fes\u00b7tem", "Grund", "nach", "al\u00b7len", "Sei\u00b7ten", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ja selbst auf diesem Weg, dem unverf\u00e4nglichen,", "tokens": ["Ja", "selbst", "auf", "die\u00b7sem", "Weg", ",", "dem", "un\u00b7ver\u00b7f\u00e4ng\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird er von ferne nahn dem Unzug\u00e4nglichen;", "tokens": ["Wird", "er", "von", "fer\u00b7ne", "nahn", "dem", "Un\u00b7zu\u00b7g\u00e4ng\u00b7li\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}