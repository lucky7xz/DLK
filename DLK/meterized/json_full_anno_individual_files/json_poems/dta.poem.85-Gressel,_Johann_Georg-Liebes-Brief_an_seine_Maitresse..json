{"dta.poem.85": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an seine  Maitresse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schreibe/ sch\u00f6nstes Kind/ von Fleisch und Blut getrieben/", "tokens": ["Ich", "schrei\u00b7be", "/", "sch\u00f6ns\u00b7tes", "Kind", "/", "von", "Fleisch", "und", "Blut", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergib/ wo dieser Brief zu frey gerahten ist!", "tokens": ["Ver\u00b7gib", "/", "wo", "die\u00b7ser", "Brief", "zu", "frey", "ge\u00b7rah\u00b7ten", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "PWAV", "PDAT", "NN", "PTKA", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es heisset die Natur uns alle beyde lieben/", "tokens": ["Es", "heis\u00b7set", "die", "Na\u00b7tur", "uns", "al\u00b7le", "bey\u00b7de", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PIAT", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich weis/ da\u00df du mit mir von gleicher Regung bist.", "tokens": ["Ich", "weis", "/", "da\u00df", "du", "mit", "mir", "von", "glei\u00b7cher", "Re\u00b7gung", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$(", "KOUS", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du darfft dar\u00fcber dir gar kein Gewissen nehmen/", "tokens": ["Du", "darfft", "da\u00b7r\u00fc\u00b7ber", "dir", "gar", "kein", "Ge\u00b7wis\u00b7sen", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PPER", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was bildest du dir mehr als ander Menschen ein?", "tokens": ["Was", "bil\u00b7dest", "du", "dir", "mehr", "als", "an\u00b7der", "Men\u00b7schen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PIAT", "KOKOM", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weswegen wilt du dich vor deinem Schatten sch\u00e4men?", "tokens": ["Wes\u00b7we\u00b7gen", "wilt", "du", "dich", "vor", "dei\u00b7nem", "Schat\u00b7ten", "sch\u00e4\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie lange wilt du selbst auf dich tyrannisch seyn?", "tokens": ["Wie", "lan\u00b7ge", "wilt", "du", "selbst", "auf", "dich", "ty\u00b7ran\u00b7nisch", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du weist es Grausahmste/ da\u00df ich als ", "tokens": ["Du", "weist", "es", "Grau\u00b7sahms\u00b7te", "/", "da\u00df", "ich", "als"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "KOUS", "PPER", "KOUS"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und gleichwol legst du mir erst schwere Ketten an/", "tokens": ["Und", "gleich\u00b7wol", "legst", "du", "mir", "erst", "schwe\u00b7re", "Ket\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was soll die Jungfrauschafft/ das leichte Spinn-Gewebe/", "tokens": ["Was", "soll", "die", "Jung\u00b7frausc\u00b7hafft", "/", "das", "leich\u00b7te", "Spinn\u00b7Ge\u00b7we\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Ding/ das jeder sucht/ und niemand finden kan?", "tokens": ["Das", "Ding", "/", "das", "je\u00b7der", "sucht", "/", "und", "nie\u00b7mand", "fin\u00b7den", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "VVFIN", "$(", "KON", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "La\u00df deine Rosen bald im ersten Fr\u00fchling pfl\u00fccken/", "tokens": ["La\u00df", "dei\u00b7ne", "Ro\u00b7sen", "bald", "im", "ers\u00b7ten", "Fr\u00fch\u00b7ling", "pfl\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gedencke/ da\u00df sie nicht auf kaltem Eise bl\u00fchn/", "tokens": ["Ge\u00b7den\u00b7cke", "/", "da\u00df", "sie", "nicht", "auf", "kal\u00b7tem", "Ei\u00b7se", "bl\u00fchn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Liebe wil sich nicht zum sp\u00e4hten Alter schicken/", "tokens": ["Die", "Lie\u00b7be", "wil", "sich", "nicht", "zum", "sp\u00e4h\u00b7ten", "Al\u00b7ter", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PTKNEG", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es pflegt ihr nackend Kind im Winter weg zu ziehn.", "tokens": ["Es", "pflegt", "ihr", "na\u00b7ckend", "Kind", "im", "Win\u00b7ter", "weg", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Closter glaub es mir hat allzustrenge Lehren/", "tokens": ["Das", "Clos\u00b7ter", "glaub", "es", "mir", "hat", "all\u00b7zu\u00b7stren\u00b7ge", "Leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dis ist kein Leben nicht/ das mich und dich vergn\u00fcgt/", "tokens": ["Dis", "ist", "kein", "Le\u00b7ben", "nicht", "/", "das", "mich", "und", "dich", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "PTKNEG", "$(", "PRELS", "PPER", "KON", "PPER", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Die Sch\u00f6nheit wird veracht/ die keiner darf verehren/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "wird", "ver\u00b7acht", "/", "die", "kei\u00b7ner", "darf", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "PIS", "VMFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie ist ein G\u00f6tzen Bild/ das in den Winckeln liegt.", "tokens": ["Sie", "ist", "ein", "G\u00f6t\u00b7zen", "Bild", "/", "das", "in", "den", "Win\u00b7ckeln", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Weswegen zeigst du mir die rundgew\u00f6lbten Br\u00fcste?", "tokens": ["Wes\u00b7we\u00b7gen", "zeigst", "du", "mir", "die", "rund\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Br\u00fcs\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Sie laden meinen Mund/ und meine Finger ein/", "tokens": ["Sie", "la\u00b7den", "mei\u00b7nen", "Mund", "/", "und", "mei\u00b7ne", "Fin\u00b7ger", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Warum erhitzt du mich/ und reitzest meine L\u00fcste?", "tokens": ["Wa\u00b7rum", "er\u00b7hitzt", "du", "mich", "/", "und", "reit\u00b7zest", "mei\u00b7ne", "L\u00fcs\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wer kan ein ", "tokens": ["Wer", "kan", "ein"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.25": {"text": "Wie offt betracht ich nicht die wunder sch\u00f6nen Gaben/", "tokens": ["Wie", "offt", "be\u00b7tracht", "ich", "nicht", "die", "wun\u00b7der", "sch\u00f6\u00b7nen", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und dencke bey mir selbst/ dis siehet alle Welt/", "tokens": ["Und", "den\u00b7cke", "bey", "mir", "selbst", "/", "dis", "sie\u00b7het", "al\u00b7le", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "$(", "PDS", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was mu\u00df nicht dieses Kind vor andre Sachen haben/", "tokens": ["Was", "mu\u00df", "nicht", "die\u00b7ses", "Kind", "vor", "and\u00b7re", "Sa\u00b7chen", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "PDAT", "NN", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die sie nicht zeigen will/ und mir verborgen h\u00e4lt?", "tokens": ["Die", "sie", "nicht", "zei\u00b7gen", "will", "/", "und", "mir", "ver\u00b7bor\u00b7gen", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMFIN", "$(", "KON", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du wirst dis Heiligthum doch ewig nicht verstecken/", "tokens": ["Du", "wirst", "dis", "Hei\u00b7lig\u00b7thum", "doch", "e\u00b7wig", "nicht", "ver\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "NN", "ADV", "ADJD", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sonst geht die S\u00fc\u00dfigkeit mit deiner Jugend hin/", "tokens": ["Sonst", "geht", "die", "S\u00fc\u00b7\u00dfig\u00b7keit", "mit", "dei\u00b7ner", "Ju\u00b7gend", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und bist du es gesinnt vor einem auffzudecken/", "tokens": ["Und", "bist", "du", "es", "ge\u00b7sinnt", "vor", "ei\u00b7nem", "auff\u00b7zu\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADJD", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So glaub ich/ da\u00df ich hier der allern\u00e4chste bin.", "tokens": ["So", "glaub", "ich", "/", "da\u00df", "ich", "hier", "der", "al\u00b7ler\u00b7n\u00e4chs\u00b7te", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Du darffst die Jungferschafft nicht mit zu Grabe tragen/", "tokens": ["Du", "darffst", "die", "Jung\u00b7fer\u00b7schafft", "nicht", "mit", "zu", "Gra\u00b7be", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Jhr seyd von unserm Fleisch/ und unserm Bein gemacht/", "tokens": ["Ihr", "seyd", "von", "un\u00b7serm", "Fleisch", "/", "und", "un\u00b7serm", "Bein", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Doch solt es deine Schaam bey Tage mir versagen/", "tokens": ["Doch", "solt", "es", "dei\u00b7ne", "Schaam", "bey", "Ta\u00b7ge", "mir", "ver\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So g\u00f6nne mir die Lust bey Schatten reicher Nacht.", "tokens": ["So", "g\u00f6n\u00b7ne", "mir", "die", "Lust", "bey", "Schat\u00b7ten", "rei\u00b7cher", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich will mein Paradie\u00df auch nicht im fiustern fehlen/", "tokens": ["Ich", "will", "mein", "Pa\u00b7ra\u00b7die\u00df", "auch", "nicht", "im", "fius\u00b7tern", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der angenehme Weg ist mir nicht unbekannt/", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7me", "Weg", "ist", "mir", "nicht", "un\u00b7be\u00b7kannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Indessen solt ich nicht die rechte Strasse w\u00e4hlen/", "tokens": ["In\u00b7des\u00b7sen", "solt", "ich", "nicht", "die", "rech\u00b7te", "Stras\u00b7se", "w\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So sey du F\u00fchrerin/ ich folge deiner Hand.", "tokens": ["So", "sey", "du", "F\u00fch\u00b7re\u00b7rin", "/", "ich", "fol\u00b7ge", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}