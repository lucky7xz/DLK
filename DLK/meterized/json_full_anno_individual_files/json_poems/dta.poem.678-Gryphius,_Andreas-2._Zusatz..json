{"dta.poem.678": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00fcndtsche frey zu seyn/ nur da\u00df ich meine zeit ", "tokens": ["Ich", "w\u00fcndt\u00b7sche", "frey", "zu", "seyn", "/", "nur", "da\u00df", "ich", "mei\u00b7ne", "zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "VAINF", "$(", "ADV", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deinem dienste m\u00f6ge schlissen", "tokens": ["In", "dei\u00b7nem", "diens\u00b7te", "m\u00f6\u00b7ge", "schlis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "HeRR wenn du dis mein netz zurissen! ", "tokens": ["HeRR", "wenn", "du", "dis", "mein", "netz", "zu\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PDS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wil ich dein hohes lob vermehren weit vnd breit.", "tokens": ["Wil", "ich", "dein", "ho\u00b7hes", "lob", "ver\u00b7meh\u00b7ren", "weit", "vnd", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Nahme soll in meinem munde/", "tokens": ["Dein", "Nah\u00b7me", "soll", "in", "mei\u00b7nem", "mun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein ruhm in meinen Lippen bl\u00fchn/ ", "tokens": ["Dein", "ruhm", "in", "mei\u00b7nen", "Lip\u00b7pen", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd wenn ich mus von hinnen zihn;", "tokens": ["Vnd", "wenn", "ich", "mus", "von", "hin\u00b7nen", "zihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sey dis das werck der letzten stunde.", "tokens": ["Sey", "dis", "das", "werck", "der", "letz\u00b7ten", "stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd wenn ich werd\u2019 auffs newe leben:", "tokens": ["Vnd", "wenn", "ich", "werd'", "auffs", "ne\u00b7we", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Wil ich von deinem lob anheben.", "tokens": ["Wil", "ich", "von", "dei\u00b7nem", "lob", "an\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}