{"dta.poem.8077": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Rache .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der Knecht hat erstochen den edeln Herrn,", "tokens": ["Der", "Knecht", "hat", "er\u00b7sto\u00b7chen", "den", "e\u00b7deln", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Knecht w\u00e4r\u2019 selber ein Ritter gern.", "tokens": ["Der", "Knecht", "w\u00e4r'", "sel\u00b7ber", "ein", "Rit\u00b7ter", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er hat ihn erstochen im dunkeln Hain", "tokens": ["Er", "hat", "ihn", "er\u00b7sto\u00b7chen", "im", "dun\u00b7keln", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und den Leib versenket im tiefen Rhein.", "tokens": ["Und", "den", "Leib", "ver\u00b7sen\u00b7ket", "im", "tie\u00b7fen", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NE", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hat angeleget die R\u00fcstung blank,", "tokens": ["Hat", "an\u00b7ge\u00b7le\u00b7get", "die", "R\u00fcs\u00b7tung", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf des Herren Ro\u00df sich geschwungen frank.", "tokens": ["Auf", "des", "Her\u00b7ren", "Ro\u00df", "sich", "ge\u00b7schwun\u00b7gen", "frank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PRF", "VVPP", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und als er sprengen will \u00fcber die Br\u00fcck\u2019,", "tokens": ["Und", "als", "er", "spren\u00b7gen", "will", "\u00fc\u00b7ber", "die", "Br\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da stutzet das Ro\u00df und b\u00e4umt sich zur\u00fcck.", "tokens": ["Da", "stut\u00b7zet", "das", "Ro\u00df", "und", "b\u00e4umt", "sich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als er die g\u00fcldnen Sporen ihm gab,", "tokens": ["Und", "als", "er", "die", "g\u00fcld\u00b7nen", "Spo\u00b7ren", "ihm", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schleudert\u2019s ihn wild in den Strom hinab.", "tokens": ["Da", "schleu\u00b7dert's", "ihn", "wild", "in", "den", "Strom", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Mit Arm, mit Fu\u00df er rudert und ringt,", "tokens": ["Mit", "Arm", ",", "mit", "Fu\u00df", "er", "ru\u00b7dert", "und", "ringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der schwere Panzer ihn niederzwingt.", "tokens": ["Der", "schwe\u00b7re", "Pan\u00b7zer", "ihn", "nie\u00b7der\u00b7zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}