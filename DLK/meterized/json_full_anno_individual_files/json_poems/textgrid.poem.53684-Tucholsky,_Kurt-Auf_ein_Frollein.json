word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PWS", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich h\u00f6r dich Butterbrot zum Aal bestellen \u2013", "tokens": ["Ich", "h\u00f6r", "dich", "But\u00b7ter\u00b7brot", "zum", "Aal", "be\u00b7stel\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott segne deinen lieben Appetit!", "tokens": ["Gott", "seg\u00b7ne", "dei\u00b7nen", "lie\u00b7ben", "Ap\u00b7pe\u00b7tit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchr dich durch Theater und Lokale,", "tokens": ["Ich", "f\u00fchr", "dich", "durch", "The\u00b7a\u00b7ter", "und", "Lo\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "durch Paradiese in der Liebe Land;", "tokens": ["durch", "Pa\u00b7ra\u00b7die\u00b7se", "in", "der", "Lie\u00b7be", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "du gibst im Auto mir mit einem Male", "tokens": ["du", "gibst", "im", "Au\u00b7to", "mir", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die manik\u00fcrte, kleine, dicke Hand.", "tokens