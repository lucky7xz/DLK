{"dta.poem.3120": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "165.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Einer Wurzel sprie\u00dft, aus Einer Quelle flie\u00dft,", "tokens": ["Aus", "Ei\u00b7ner", "Wur\u00b7zel", "sprie\u00dft", ",", "aus", "Ei\u00b7ner", "Quel\u00b7le", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was weit ins Leben sich erschlie\u00dft und sich ergie\u00dft.", "tokens": ["Was", "weit", "ins", "Le\u00b7ben", "sich", "er\u00b7schlie\u00dft", "und", "sich", "er\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPRART", "NN", "PRF", "VVFIN", "KON", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Zweige wissen nicht, was unten sie verflicht,", "tokens": ["Die", "Zwei\u00b7ge", "wis\u00b7sen", "nicht", ",", "was", "un\u00b7ten", "sie", "ver\u00b7flicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie schwanken wohlgemuth und tauchen auf ins Licht.", "tokens": ["Sie", "schwan\u00b7ken", "wohl\u00b7ge\u00b7muth", "und", "tau\u00b7chen", "auf", "ins", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Wellen merken kaum, was still sie h\u00e4lt im Zaum,", "tokens": ["Die", "Wel\u00b7len", "mer\u00b7ken", "kaum", ",", "was", "still", "sie", "h\u00e4lt", "im", "Zaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWS", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie schwanken auf und ab, und kr\u00f6nen sich mit Schaum.", "tokens": ["Sie", "schwan\u00b7ken", "auf", "und", "ab", ",", "und", "kr\u00f6\u00b7nen", "sich", "mit", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Am Zaume h\u00e4lt sie doch und unterm goldnen Joch", "tokens": ["Am", "Zau\u00b7me", "h\u00e4lt", "sie", "doch", "und", "un\u00b7term", "gold\u00b7nen", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe, der nichts ist zu nieder noch zu hoch.", "tokens": ["Die", "Lie\u00b7be", ",", "der", "nichts", "ist", "zu", "nie\u00b7der", "noch", "zu", "hoch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "PTKZU", "PTKVZ", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr seid nicht klein, noch gro\u00df, Kinder aus meinem Scho\u00df,", "tokens": ["Ihr", "seid", "nicht", "klein", ",", "noch", "gro\u00df", ",", "Kin\u00b7der", "aus", "mei\u00b7nem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "ADV", "ADJD", "$,", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Seid nichts in euch, in mir seid ihr ein Etwas blo\u00df.", "tokens": ["Seid", "nichts", "in", "euch", ",", "in", "mir", "seid", "ihr", "ein", "Et\u00b7was", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PIS", "APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "PPER", "ART", "ADV", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}