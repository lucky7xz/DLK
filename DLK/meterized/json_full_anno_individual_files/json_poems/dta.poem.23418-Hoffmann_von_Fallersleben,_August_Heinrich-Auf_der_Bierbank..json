{"dta.poem.23418": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Bierbank.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Leben! welch ein Streiten", "tokens": ["Welch", "ein", "Le\u00b7ben", "!", "welch", "ein", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$.", "PWAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr die Wahrheit und das Recht!", "tokens": ["F\u00fcr", "die", "Wahr\u00b7heit", "und", "das", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Bierbank \u2014", "tokens": ["Auf", "der", "Bier\u00b7bank"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Unsre Sitten, unsre Zeiten,", "tokens": ["Uns\u00b7re", "Sit\u00b7ten", ",", "uns\u00b7re", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, sie sind f\u00fcrwahr nicht schlecht!", "tokens": ["Nein", ",", "sie", "sind", "f\u00fcr\u00b7wahr", "nicht", "schlecht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Bierbank.", "tokens": ["Auf", "der", "Bier\u00b7bank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Weg mit Gilde, Zunft und Innung,", "tokens": ["Weg", "mit", "Gil\u00b7de", ",", "Zunft", "und", "In\u00b7nung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weg mit allem Rang und Stand!", "tokens": ["Weg", "mit", "al\u00b7lem", "Rang", "und", "Stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Bierbank \u2014", "tokens": ["Auf", "der", "Bier\u00b7bank"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hier gilt nur allein Gesinnung,", "tokens": ["Hier", "gilt", "nur", "al\u00b7lein", "Ge\u00b7sin\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hier gilt nur das Vaterland!", "tokens": ["Hier", "gilt", "nur", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auf der Bierbank.", "tokens": ["Auf", "der", "Bier\u00b7bank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Alle Lauheit geht zu Nichte,", "tokens": ["Al\u00b7le", "Lau\u00b7heit", "geht", "zu", "Nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Freisinn wird gest\u00e4hlt", "tokens": ["Und", "der", "Frei\u00b7sinn", "wird", "ge\u00b7st\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Bierbank \u2014", "tokens": ["Auf", "der", "Bier\u00b7bank"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und dem Gang der Weltgeschichte", "tokens": ["Und", "dem", "Gang", "der", "Welt\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchlen wir uns mitverm\u00e4hlt", "tokens": ["F\u00fch\u00b7len", "wir", "uns", "mit\u00b7ver\u00b7m\u00e4hlt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Bierbank.", "tokens": ["Auf", "der", "Bier\u00b7bank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "O wie sind wir treu verbunden,", "tokens": ["O", "wie", "sind", "wir", "treu", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gutes Muths und gleichgesinnt!", "tokens": ["Gu\u00b7tes", "Muths", "und", "gleich\u00b7ge\u00b7sinnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Bierbank \u2014", "tokens": ["Auf", "der", "Bier\u00b7bank"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "O die s\u00fc\u00dfen lieben Stunden,", "tokens": ["O", "die", "s\u00fc\u00b7\u00dfen", "lie\u00b7ben", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Warum fliehn sie so geschwind!", "tokens": ["Wa\u00b7rum", "fliehn", "sie", "so", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Bierbank.", "tokens": ["Auf", "der", "Bier\u00b7bank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Deutschland ist noch nicht verloren!", "tokens": ["Deutschland", "ist", "noch", "nicht", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Deutschland strotzt von Kraft und Geist", "tokens": ["Deutschland", "strotzt", "von", "Kraft", "und", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf der Bierbank \u2014", "tokens": ["Auf", "der", "Bier\u00b7bank"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Allem sei der Tod geschworen,", "tokens": ["Al\u00b7lem", "sei", "der", "Tod", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was nur welsch und undeutsch hei\u00dft,", "tokens": ["Was", "nur", "welsch", "und", "un\u00b7deutsch", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Bierbank.", "tokens": ["Auf", "der", "Bier\u00b7bank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}