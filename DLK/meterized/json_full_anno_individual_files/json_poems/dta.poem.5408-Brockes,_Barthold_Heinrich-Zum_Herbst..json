{"dta.poem.5408": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich im Herbst, in der Allee,", "tokens": ["Da", "ich", "im", "Herbst", ",", "in", "der", "Al\u00b7lee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In abgefallnen Bl\u00e4ttern gehe,", "tokens": ["In", "ab\u00b7ge\u00b7fall\u00b7nen", "Bl\u00e4t\u00b7tern", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die, in ges\u00e4rbter Zierlichkeit,", "tokens": ["Die", ",", "in", "ge\u00b7s\u00e4rb\u00b7ter", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4ren sie mit Flei\u00df gestreut,", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "mit", "Flei\u00df", "ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die dunckel-braunen Steige zieren,", "tokens": ["Die", "dun\u00b7ckel\u00b7brau\u00b7nen", "Stei\u00b7ge", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So da\u00df sie durch die bunte Pracht", "tokens": ["So", "da\u00df", "sie", "durch", "die", "bun\u00b7te", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu dessen Ruhm, der alles macht,", "tokens": ["Zu", "des\u00b7sen", "Ruhm", ",", "der", "al\u00b7les", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich, inniglich ger\u00fchret, f\u00fchren;", "tokens": ["Mich", ",", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fch\u00b7ret", ",", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Deucht mich da\u00df auch, da sie vergehn,", "tokens": ["Deucht", "mich", "da\u00df", "auch", ",", "da", "sie", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOUS", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch ihrer Farben buntes Gl\u00e4ntzen,", "tokens": ["Durch", "ih\u00b7rer", "Far\u00b7ben", "bun\u00b7tes", "Gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wodurch sie Steig\u2019- und Beeten kr\u00e4ntzen,", "tokens": ["Wo\u00b7durch", "sie", "Steig'", "und", "Bee\u00b7ten", "kr\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "TRUNC", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Bl\u00e4tter ihren HErrn erh\u00f6hn.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "ih\u00b7ren", "Herrn", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mich deucht, ob h\u00f6rt\u2019 ich sie, vom dunckel-braunen Grunde,", "tokens": ["Mich", "deucht", ",", "ob", "h\u00f6rt'", "ich", "sie", ",", "vom", "dun\u00b7ckel\u00b7brau\u00b7nen", "Grun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auf welchem sie in grosser Menge lagen,", "tokens": ["Auf", "wel\u00b7chem", "sie", "in", "gros\u00b7ser", "Men\u00b7ge", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Mit theils bereits erbla\u00dft-theils annoch rothem Munde,", "tokens": ["Mit", "theils", "be\u00b7reits", "e\u00b7rbla\u00dft\u00b7theils", "an\u00b7noch", "ro\u00b7them", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mir dieses noch, zum Abschied sagen:", "tokens": ["Mir", "die\u00b7ses", "noch", ",", "zum", "Ab\u00b7schied", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "ADV", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wir scheiden zwar nachdem wir schon gegr\u00fcnet,", "tokens": ["Wir", "schei\u00b7den", "zwar", "nach\u00b7dem", "wir", "schon", "ge\u00b7gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und fast ein gantzes Jahr", "tokens": ["Und", "fast", "ein", "gant\u00b7zes", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Mit Farb-und Schatten dir gedienet;", "tokens": ["Mit", "Fa\u00b7rb\u00b7\u00b7und", "Schat\u00b7ten", "dir", "ge\u00b7die\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Allein wir scheiden nicht vor immer;", "tokens": ["Al\u00b7lein", "wir", "schei\u00b7den", "nicht", "vor", "im\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "APPR", "ADV", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Schau nun die Knospen auf den Zweigen,", "tokens": ["Schau", "nun", "die", "Knos\u00b7pen", "auf", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Die werden dir, in neuem Schimmer,", "tokens": ["Die", "wer\u00b7den", "dir", ",", "in", "neu\u00b7em", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Uns wiederum in andern zeigen.", "tokens": ["Uns", "wie\u00b7de\u00b7rum", "in", "an\u00b7dern", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du, durch unsre Sch\u00f6nheit, nun,", "tokens": ["Hast", "du", ",", "durch", "uns\u00b7re", "Sch\u00f6n\u00b7heit", ",", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wie du schuldig warst zu thun,", "tokens": ["So", "wie", "du", "schul\u00b7dig", "warst", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Sch\u00f6pfer, den wir dir gewiesen,", "tokens": ["Den", "Sch\u00f6p\u00b7fer", ",", "den", "wir", "dir", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch \u00f6ftern frohen Danck gepriesen;", "tokens": ["Durch", "\u00f6f\u00b7tern", "fro\u00b7hen", "Danck", "ge\u00b7prie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So haben wir, da wir gegr\u00fcnt,", "tokens": ["So", "ha\u00b7ben", "wir", ",", "da", "wir", "ge\u00b7gr\u00fcnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Sch\u00f6pfer, auch durch dich, gedient.", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", ",", "auch", "durch", "dich", ",", "ge\u00b7dient", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hast du es aber nicht gethan;", "tokens": ["Hast", "du", "es", "a\u00b7ber", "nicht", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sieh uns jetzt zuletzt noch an!", "tokens": ["So", "sieh", "uns", "jetzt", "zu\u00b7letzt", "noch", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dancke GOtt, da\u00df unsre Pracht", "tokens": ["Und", "dan\u00b7cke", "Gott", ",", "da\u00df", "uns\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich oft ger\u00fchrt und froh gemacht.", "tokens": ["Dich", "oft", "ge\u00b7r\u00fchrt", "und", "froh", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ihm allein ist zuzuschreiben,", "tokens": ["Denn", "ihm", "al\u00b7lein", "ist", "zu\u00b7zu\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df wir, mit solchem holden Gr\u00fcnen", "tokens": ["Da\u00df", "wir", ",", "mit", "sol\u00b7chem", "hol\u00b7den", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bekleidet, dir zur Lust erschienen,", "tokens": ["Be\u00b7klei\u00b7det", ",", "dir", "zur", "Lust", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df wir vergehen und doch bleiben!", "tokens": ["Da\u00df", "wir", "ver\u00b7ge\u00b7hen", "und", "doch", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}