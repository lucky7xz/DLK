{"dta.poem.19757": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Edelk\u00f6nigs - Kinder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es waren zwei Edelk\u00f6nigs-Kinder,               ", "tokens": ["Es", "wa\u00b7ren", "zwei", "E\u00b7del\u00b7k\u00f6\u00b7nigs\u00b7Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die beiden die hatten sich lieb,", "tokens": ["Die", "bei\u00b7den", "die", "hat\u00b7ten", "sich", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PDS", "VAFIN", "PRF", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Beisammen konten sie dir nit kommen,", "tokens": ["Bei\u00b7sam\u00b7men", "kon\u00b7ten", "sie", "dir", "nit", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Wasser war viel zu tief.", "tokens": ["Das", "Was\u00b7ser", "war", "viel", "zu", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach Liebchen k\u00f6ntest du schwimmen,", "tokens": ["Ach", "Lieb\u00b7chen", "k\u00f6n\u00b7test", "du", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So schwimme doch her zu mir,", "tokens": ["So", "schwim\u00b7me", "doch", "her", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drey Kerzlein wollt ich dir anstecken,", "tokens": ["Drey", "Kerz\u00b7lein", "wollt", "ich", "dir", "an\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die selten auch leuchten dir.", "tokens": ["Die", "sel\u00b7ten", "auch", "leuch\u00b7ten", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sa\u00df ein loses N\u00f6nnechen,", "tokens": ["Da", "sa\u00df", "ein", "lo\u00b7ses", "N\u00f6n\u00b7ne\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Das that, als wenn es schlief,", "tokens": ["Das", "that", ",", "als", "wenn", "es", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es that die Kerzlein ausblasen,", "tokens": ["Es", "that", "die", "Kerz\u00b7lein", "aus\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der J\u00fcngling vertrank so tief.", "tokens": ["Der", "J\u00fcng\u00b7ling", "ver\u00b7trank", "so", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach Mutter herzliebste Mutter,", "tokens": ["Ach", "Mut\u00b7ter", "herz\u00b7liebs\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie thut mir mein H\u00e4uptchen so weh,", "tokens": ["Wie", "thut", "mir", "mein", "H\u00e4upt\u00b7chen", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "K\u00f6nt ich ein kleine Weile", "tokens": ["K\u00f6nt", "ich", "ein", "klei\u00b7ne", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spazieren gehn l\u00e4ngst der See.", "tokens": ["Spa\u00b7zie\u00b7ren", "gehn", "l\u00e4ngst", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Ach Tochter herzliebste Tochter,", "tokens": ["Ach", "Toch\u00b7ter", "herz\u00b7liebs\u00b7te", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Allein solst du da nit gehn,", "tokens": ["Al\u00b7lein", "solst", "du", "da", "nit", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weck auf deine j\u00fcngste Schwester,", "tokens": ["Weck", "auf", "dei\u00b7ne", "j\u00fcngs\u00b7te", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und la\u00df sie mit dir gehn.", "tokens": ["Und", "la\u00df", "sie", "mit", "dir", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach Mutter herzliebste Mutter,", "tokens": ["Ach", "Mut\u00b7ter", "herz\u00b7liebs\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Schwester ist noch ein Kind,", "tokens": ["Mein", "Schwes\u00b7ter", "ist", "noch", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sie pfl\u00fcckt ja all die Blumen,", "tokens": ["Sie", "pfl\u00fcckt", "ja", "all", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die in dem gr\u00fcnen Wald sind.", "tokens": ["Die", "in", "dem", "gr\u00fc\u00b7nen", "Wald", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach Mutter herzliebste Mutter,", "tokens": ["Ach", "Mut\u00b7ter", "herz\u00b7liebs\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie thut mir mein H\u00e4uptchen so weh,", "tokens": ["Wie", "thut", "mir", "mein", "H\u00e4upt\u00b7chen", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "K\u00f6nt ich eine kleine Weile", "tokens": ["K\u00f6nt", "ich", "ei\u00b7ne", "klei\u00b7ne", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spaziren gehn l\u00e4ngst der See.", "tokens": ["Spa\u00b7zi\u00b7ren", "gehn", "l\u00e4ngst", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Ach Tochter, herzliebste Tochter,", "tokens": ["Ach", "Toch\u00b7ter", ",", "herz\u00b7liebs\u00b7te", "Toch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alleine sollst du da nit gehn,", "tokens": ["Al\u00b7lei\u00b7ne", "sollst", "du", "da", "nit", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weck auf deinen j\u00fcngsten Bruder,", "tokens": ["Weck", "auf", "dei\u00b7nen", "j\u00fcng\u00b7sten", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und la\u00df ihn mit dir gehn.", "tokens": ["Und", "la\u00df", "ihn", "mit", "dir", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ach Mutter, herzliebste Mutter,", "tokens": ["Ach", "Mut\u00b7ter", ",", "herz\u00b7liebs\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Bruder ist noch ein Kind,", "tokens": ["Mein", "Bru\u00b7der", "ist", "noch", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er f\u00e4ngt ja alle die Haasen,", "tokens": ["Er", "f\u00e4ngt", "ja", "al\u00b7le", "die", "Haa\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die in dem gr\u00fcnen Wald sind.", "tokens": ["Die", "in", "dem", "gr\u00fc\u00b7nen", "Wald", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Mutter und die ging schlafen,", "tokens": ["Die", "Mut\u00b7ter", "und", "die", "ging", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "VVFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Tochter ging ihren Gang,", "tokens": ["Die", "Toch\u00b7ter", "ging", "ih\u00b7ren", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie ging so lange spazieren,", "tokens": ["Sie", "ging", "so", "lan\u00b7ge", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis sie ein Fischer fand.", "tokens": ["Bis", "sie", "ein", "Fi\u00b7scher", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Den Fischer sah sie fischen,", "tokens": ["Den", "Fi\u00b7scher", "sah", "sie", "fi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fisch mir ein verdientes roth Gold,", "tokens": ["Fisch", "mir", "ein", "ver\u00b7dien\u00b7tes", "roth", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "ADJD", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Fisch mir doch einen Todten,", "tokens": ["Fisch", "mir", "doch", "ei\u00b7nen", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Er ist ein Edelk\u00f6nigs-Kind.", "tokens": ["Er", "ist", "ein", "E\u00b7del\u00b7k\u00f6\u00b7nigs\u00b7Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Fischer fischte so lange,", "tokens": ["Der", "Fi\u00b7scher", "fischte", "so", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis er den Todten fand,", "tokens": ["Bis", "er", "den", "Tod\u00b7ten", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er grif ihn bei den Haaren,", "tokens": ["Er", "grif", "ihn", "bei", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schleift ihn an das Land.", "tokens": ["Und", "schleift", "ihn", "an", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sie nahm ihn in ihre Arme,", "tokens": ["Sie", "nahm", "ihn", "in", "ih\u00b7re", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und k\u00fc\u00dft ihm seinen Mund:", "tokens": ["Und", "k\u00fc\u00dft", "ihm", "sei\u00b7nen", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Adie mein Vater und Mutter,", "tokens": ["A\u00b7die", "mein", "Va\u00b7ter", "und", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wir sehn uns nimmermehr.", "tokens": ["Wir", "sehn", "uns", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}