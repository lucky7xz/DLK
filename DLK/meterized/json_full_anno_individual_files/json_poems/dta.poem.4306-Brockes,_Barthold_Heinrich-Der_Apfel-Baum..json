{"dta.poem.4306": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Apfel-Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist, entschlage dich der Sorgen! gieb allhier", "tokens": ["Mein", "Geist", ",", "ent\u00b7schla\u00b7ge", "dich", "der", "Sor\u00b7gen", "!", "gieb", "all\u00b7hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$.", "VVIMP", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur fr\u00f6hlicher Betrachtung Raum!", "tokens": ["Nur", "fr\u00f6h\u00b7li\u00b7cher", "Be\u00b7trach\u00b7tung", "Raum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beschaue diesen sch\u00f6nen Baum,", "tokens": ["Be\u00b7schau\u00b7e", "die\u00b7sen", "sch\u00f6\u00b7nen", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwege seinen Bau, den Stamm, des Wipfels Zier!", "tokens": ["Er\u00b7we\u00b7ge", "sei\u00b7nen", "Bau", ",", "den", "Stamm", ",", "des", "Wip\u00b7fels", "Zier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Komm, z\u00e4hl\u2019, in seiner Zweige L\u00e4nge,", "tokens": ["Komm", ",", "z\u00e4hl'", ",", "in", "sei\u00b7ner", "Zwei\u00b7ge", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Ehren Dem, Der sie und alles macht,", "tokens": ["Zu", "Eh\u00b7ren", "Dem", ",", "Der", "sie", "und", "al\u00b7les", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "$,", "ART", "PPER", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der hohlen Ehren-B\u00f6gen Menge,", "tokens": ["Der", "hoh\u00b7len", "Eh\u00b7ren\u00b7B\u00f6\u00b7gen", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Woran, mit lebenden Festonen ausgeschm\u00fcckt,", "tokens": ["Wo\u00b7ran", ",", "mit", "le\u00b7ben\u00b7den", "Fes\u00b7to\u00b7nen", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Man ein Rubienen-Roht, und fast smaragdne Pracht,", "tokens": ["Man", "ein", "Ru\u00b7bie\u00b7nen\u00b7Roht", ",", "und", "fast", "sma\u00b7ragd\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "An Frucht- und Bl\u00e4tter-Werk erblickt!", "tokens": ["An", "Frucht", "und", "Bl\u00e4t\u00b7ter\u00b7Werk", "er\u00b7blickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Schau, wie sie rings herum, durch eigne Frucht gebogen,", "tokens": ["Schau", ",", "wie", "sie", "rings", "he\u00b7rum", ",", "durch", "eig\u00b7ne", "Frucht", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "PTKVZ", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "(als w\u00e4ren sie,", "tokens": ["(", "als", "w\u00e4\u00b7ren", "sie", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "In Regel-rechter Symmetrie,", "tokens": ["In", "Re\u00b7gel\u00b7rech\u00b7ter", "Sym\u00b7me\u00b7trie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Recht in die R\u00fcnd\u2019 herabgezogen,)", "tokens": ["Recht", "in", "die", "R\u00fcnd'", "her\u00b7ab\u00b7ge\u00b7zo\u00b7gen", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ein gr\u00fcn Gew\u00f6lb im Cirkel-Schlag formieren!", "tokens": ["Ein", "gr\u00fcn", "Ge\u00b7w\u00f6lb", "im", "Cir\u00b7kel\u00b7Schlag", "for\u00b7mie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Schau, wie an ihm, mit Aepfeln ohne Zahl,", "tokens": ["Schau", ",", "wie", "an", "ihm", ",", "mit", "A\u00b7e\u00b7pfeln", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "PPER", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Die wie Rubienen gl\u00fch\u2019n, im Sonnen-Licht zumahl,", "tokens": ["Die", "wie", "Ru\u00b7bie\u00b7nen", "gl\u00fch'n", ",", "im", "Son\u00b7nen\u00b7Licht", "zu\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "$,", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Zweige mehr, als wie mit Laub, sich zieren!", "tokens": ["Die", "Zwei\u00b7ge", "mehr", ",", "als", "wie", "mit", "Laub", ",", "sich", "zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "KOKOM", "APPR", "NN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Schau bey der R\u00f6ht\u2019 annoch der Schale Gl\u00e4tt\u2019 und Glanz,", "tokens": ["Schau", "bey", "der", "R\u00f6ht'", "an\u00b7noch", "der", "Scha\u00b7le", "Gl\u00e4tt'", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Die halb sich r\u00f6htet, halb verg\u00fcldet,", "tokens": ["Die", "halb", "sich", "r\u00f6h\u00b7tet", ",", "halb", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Worauf die Sonne selbst, im kleinen Blick, sich ganz,", "tokens": ["Wo\u00b7rauf", "die", "Son\u00b7ne", "selbst", ",", "im", "klei\u00b7nen", "Blick", ",", "sich", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "$,", "APPRART", "ADJA", "NN", "$,", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als wie in einem Spiegel, bildet!", "tokens": ["Als", "wie", "in", "ei\u00b7nem", "Spie\u00b7gel", ",", "bil\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wodurch der ganze Baum voll Kugeln von Rubien,", "tokens": ["Wo\u00b7durch", "der", "gan\u00b7ze", "Baum", "voll", "Ku\u00b7geln", "von", "Ru\u00b7bi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "An R\u00f6hte, Gl\u00e4tt\u2019 und Schimmer, schien,", "tokens": ["An", "R\u00f6h\u00b7te", ",", "Gl\u00e4tt'", "und", "Schim\u00b7mer", ",", "schien", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wobey man oberwerts, auch unten bey den Stielen,", "tokens": ["Wo\u00b7bey", "man", "o\u00b7ber\u00b7werts", ",", "auch", "un\u00b7ten", "bey", "den", "Stie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Woselbst die Frucht stets etwas eingedr\u00fcckt,", "tokens": ["Wo\u00b7selbst", "die", "Frucht", "stets", "et\u00b7was", "ein\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Wenn sie die Sonne trifft, bey vielen", "tokens": ["Wenn", "sie", "die", "Son\u00b7ne", "trifft", ",", "bey", "vie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Ein gl\u00e4nzend Cirkelchen, recht Silber-wei\u00df, erblickt.", "tokens": ["Ein", "gl\u00e4n\u00b7zend", "Cir\u00b7kel\u00b7chen", ",", "recht", "Sil\u00b7ber\u00b7wei\u00df", ",", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Unglaublich ist die mannigfache Weise,", "tokens": ["Un\u00b7glaub\u00b7lich", "ist", "die", "man\u00b7nig\u00b7fa\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie, auf jedweden Apfels Kreise,", "tokens": ["Wie", ",", "auf", "jed\u00b7we\u00b7den", "Ap\u00b7fels", "Krei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das bald sich r\u00fcndende, bald r\u00fcckwerts fall\u2019nde Licht,", "tokens": ["Das", "bald", "sich", "r\u00fcn\u00b7den\u00b7de", ",", "bald", "r\u00fcck\u00b7werts", "fall'n\u00b7de", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PRF", "VVFIN", "$,", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald abwerts gleitend, sich bald senkt, bald bricht.", "tokens": ["Bald", "ab\u00b7werts", "glei\u00b7tend", ",", "sich", "bald", "senkt", ",", "bald", "bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "PRF", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie mannigfach die schnell dadurch erzeugten Schatten", "tokens": ["Wie", "man\u00b7nig\u00b7fach", "die", "schnell", "da\u00b7durch", "er\u00b7zeug\u00b7ten", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJD", "PAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sich, unsrer Augen Lust noch zu vermehren, gatten.", "tokens": ["Sich", ",", "uns\u00b7rer", "Au\u00b7gen", "Lust", "noch", "zu", "ver\u00b7meh\u00b7ren", ",", "gat\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "$,", "PPOSAT", "NN", "NN", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie oft ein nettes Schatten-Blatt", "tokens": ["Wie", "oft", "ein", "net\u00b7tes", "Schat\u00b7ten\u00b7Blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf einem Apfel sich formieret hat.", "tokens": ["Auf", "ei\u00b7nem", "Ap\u00b7fel", "sich", "for\u00b7mie\u00b7ret", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie oft sich dunkle, gr\u00fcne Stellen,", "tokens": ["Wie", "oft", "sich", "dunk\u00b7le", ",", "gr\u00fc\u00b7ne", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PRF", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durchs schnelle Schlag-Licht, schnell erhellen.", "tokens": ["Durchs", "schnel\u00b7le", "Schlag\u00b7Licht", ",", "schnell", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Durch welches bunte Licht- und bunte Schatten-Spiel", "tokens": ["Durch", "wel\u00b7ches", "bun\u00b7te", "Licht", "und", "bun\u00b7te", "Schat\u00b7ten\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein gleichsam bebendes annehmliches Gew\u00fchl", "tokens": ["Ein", "gleich\u00b7sam", "be\u00b7ben\u00b7des", "an\u00b7nehm\u00b7li\u00b7ches", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "ADJA", "NN"], "meter": "+--+--++-+-+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Jm ganzen Baum entsteht, das lieblich, bunt und sch\u00f6n.", "tokens": ["Jm", "gan\u00b7zen", "Baum", "ent\u00b7steht", ",", "das", "lieb\u00b7lich", ",", "bunt", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein menschlich Auge kann sich matt, nicht satt, dran seh\u2019n.", "tokens": ["Ein", "menschlich", "Au\u00b7ge", "kann", "sich", "matt", ",", "nicht", "satt", ",", "dran", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["ART", "ADJD", "NN", "VMFIN", "PRF", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "PAV", "VVFIN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie ich mich nun gar oft an seinem bunten Schein", "tokens": ["Wie", "ich", "mich", "nun", "gar", "oft", "an", "sei\u00b7nem", "bun\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergn\u00fcgt\u2019, auch oft dabey gedacht:", "tokens": ["Ver\u00b7gn\u00fcgt'", ",", "auch", "oft", "da\u00b7bey", "ge\u00b7dacht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott Lob! Der diesen Baum gemacht!", "tokens": ["Gott", "Lob", "!", "Der", "die\u00b7sen", "Baum", "ge\u00b7macht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott Lob auch! da\u00df er mein!", "tokens": ["Gott", "Lob", "auch", "!", "da\u00df", "er", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$.", "KOUS", "PPER", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, 