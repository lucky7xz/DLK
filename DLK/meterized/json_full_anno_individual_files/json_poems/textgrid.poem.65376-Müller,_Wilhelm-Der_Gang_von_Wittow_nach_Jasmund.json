{"textgrid.poem.65376": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Gang von Wittow nach Jasmund", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verdammte lange schmale Heide!", "tokens": ["Ver\u00b7damm\u00b7te", "lan\u00b7ge", "schma\u00b7le", "Hei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu beiden Seiten brummt das Meer,", "tokens": ["Zu", "bei\u00b7den", "Sei\u00b7ten", "brummt", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versteckt in einem Aschenkleide,", "tokens": ["Ver\u00b7steckt", "in", "ei\u00b7nem", "A\u00b7schen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Senkt sich der Himmel tief und schwer.", "tokens": ["Senkt", "sich", "der", "Him\u00b7mel", "tief", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Wege liegen scharfe Steine", "tokens": ["Im", "We\u00b7ge", "lie\u00b7gen", "schar\u00b7fe", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schneiden in die Sohlen mir \u2013", "tokens": ["Und", "schnei\u00b7den", "in", "die", "Soh\u00b7len", "mir", "\