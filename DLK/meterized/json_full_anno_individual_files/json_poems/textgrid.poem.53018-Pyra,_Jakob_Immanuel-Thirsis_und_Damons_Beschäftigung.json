{"textgrid.poem.53018": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "Thirsis und Damons Besch\u00e4ftigung", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hatte die gefrorne Nacht", "tokens": ["Es", "hat\u00b7te", "die", "ge\u00b7fror\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich mit dem schnellen Heer der Sternen,", "tokens": ["Sich", "mit", "dem", "schnel\u00b7len", "Heer", "der", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit dem Monde in der Fernen,", "tokens": ["Und", "mit", "dem", "Mon\u00b7de", "in", "der", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "APPR", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schon lange auf die Flucht gemacht.", "tokens": ["Schon", "lan\u00b7ge", "auf", "die", "Flucht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indem der Sonnen neuer Strahl", "tokens": ["In\u00b7dem", "der", "Son\u00b7nen", "neu\u00b7er", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sch\u00e4fer muntres Volck erweckte,", "tokens": ["Der", "Sch\u00e4\u00b7fer", "mun\u00b7tres", "Volck", "er\u00b7weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Berge, Wiesen, Wald und Thal,", "tokens": ["Und", "Ber\u00b7ge", ",", "Wie\u00b7sen", ",", "Wald", "und", "Thal", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Augen wiederum entdeckte.", "tokens": ["Den", "Au\u00b7gen", "wie\u00b7de\u00b7rum", "ent\u00b7deck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Reiff, der Feld und Thal erf\u00fcllt,", "tokens": ["Der", "Reiff", ",", "der", "Feld", "und", "Thal", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward durch die Sonn in Thau verkehret;", "tokens": ["Ward", "durch", "die", "Sonn", "in", "Thau", "ver\u00b7keh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da sie alles aufgekl\u00e4ret,", "tokens": ["Und", "da", "sie", "al\u00b7les", "auf\u00b7ge\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So spiegelt sie ihr goldnes Bild", "tokens": ["So", "spie\u00b7gelt", "sie", "ihr", "gold\u00b7nes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sowohl in dieser Tropfen Na\u00df,", "tokens": ["So\u00b7wohl", "in", "die\u00b7ser", "Trop\u00b7fen", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die auf dem falben Grase stehen,", "tokens": ["Die", "auf", "dem", "fal\u00b7ben", "Gra\u00b7se", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als in den Spiegeln grosser Seen.", "tokens": ["Als", "in", "den", "Spie\u00b7geln", "gros\u00b7ser", "Seen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr Strahl erquickte Feld und Gras.", "tokens": ["Ihr", "Strahl", "er\u00b7quick\u00b7te", "Feld", "und", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Hirten \u00f6ffneten nun schon", "tokens": ["Die", "Hir\u00b7ten", "\u00f6ff\u00b7ne\u00b7ten", "nun", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Thore knarrend an den St\u00e4llen.", "tokens": ["Die", "Tho\u00b7re", "knar\u00b7rend", "an", "den", "St\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drauf h\u00f6rte man den Klang der Schellen,", "tokens": ["Drauf", "h\u00f6r\u00b7te", "man", "den", "Klang", "der", "Schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des muntern Viehs vermischten Ton.", "tokens": ["Des", "mun\u00b7tern", "Viehs", "ver\u00b7mischten", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Man sahe mit Vergn\u00fcgen an,", "tokens": ["Man", "sa\u00b7he", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie froh die jungen L\u00e4mmer springen,", "tokens": ["Wie", "froh", "die", "jun\u00b7gen", "L\u00e4m\u00b7mer", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie die Schafe bl\u00f6kend dringen;", "tokens": ["Und", "wie", "die", "Scha\u00b7fe", "bl\u00f6\u00b7kend", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So geht der Bock gantz stoltz voran.", "tokens": ["So", "geht", "der", "Bock", "gantz", "stoltz", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Inzwischen hatte Thirsis sich", "tokens": ["I\u00b7nzwi\u00b7schen", "hat\u00b7te", "Thir\u00b7sis", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "PRF"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In jenen tiefen Hain begeben,", "tokens": ["In", "je\u00b7nen", "tie\u00b7fen", "Hain", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Worinn er, bey dem stillen Leben,", "tokens": ["Wo\u00b7rinn", "er", ",", "bey", "dem", "stil\u00b7len", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehr oft vor sich allein entwich.", "tokens": ["Sehr", "oft", "vor", "sich", "al\u00b7lein", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier pflegt er in der Einsamkeit,", "tokens": ["Hier", "pflegt", "er", "in", "der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In d\u00fcrrer B\u00e4ume d\u00fcnnen Schatten,", "tokens": ["In", "d\u00fcr\u00b7rer", "B\u00e4u\u00b7me", "d\u00fcn\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit seinem Damon sich zu gatten,", "tokens": ["Mit", "sei\u00b7nem", "Da\u00b7mon", "sich", "zu", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dieser war auch itzt nicht weit.", "tokens": ["Und", "die\u00b7ser", "war", "auch", "itzt", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie sungen den, der in der Nacht", "tokens": ["Sie", "sun\u00b7gen", "den", ",", "der", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So m\u00e4chtig sie auf dieser Erden,", "tokens": ["So", "m\u00e4ch\u00b7tig", "sie", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihrer H\u00fctte und den Heerden,", "tokens": ["Mit", "ih\u00b7rer", "H\u00fct\u00b7te", "und", "den", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch seiner Geister Schutz, bewacht.", "tokens": ["Durch", "sei\u00b7ner", "Geis\u00b7ter", "Schutz", ",", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald r\u00fchrten sie der Saiten Chor,", "tokens": ["Bald", "r\u00fchr\u00b7ten", "sie", "der", "Sai\u00b7ten", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald stimmten sie ihr Sch\u00e4fer-Rohr;", "tokens": ["Bald", "stimm\u00b7ten", "sie", "ihr", "Sch\u00e4\u00b7fer\u00b7Rohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jetzt t\u00f6neten des Maro Lieder", "tokens": ["Jetzt", "t\u00f6\u00b7ne\u00b7ten", "des", "Ma\u00b7ro", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Auf ihren deutschen Fl\u00f6ten wieder.", "tokens": ["Auf", "ih\u00b7ren", "deut\u00b7schen", "Fl\u00f6\u00b7ten", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drauf h\u00f6reten sie auf den H\u00f6hn", "tokens": ["Drauf", "h\u00f6\u00b7re\u00b7ten", "sie", "auf", "den", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Die Doris ihrem Damon rufen", "tokens": ["Die", "Do\u00b7ris", "ih\u00b7rem", "Da\u00b7mon", "ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sahen von des H\u00fcgels Stufen", "tokens": ["Und", "sa\u00b7hen", "von", "des", "H\u00fc\u00b7gels", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie eilig aus dem W\u00e4ldchen gehn.", "tokens": ["Sie", "ei\u00b7lig", "aus", "dem", "W\u00e4ld\u00b7chen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kam und schlung die weisse Hand", "tokens": ["Sie", "kam", "und", "schlung", "die", "weis\u00b7se", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um ihres Liebsten Hals mit k\u00fcssen,", "tokens": ["Um", "ih\u00b7res", "Liebs\u00b7ten", "Hals", "mit", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wollte liebreich strafend wissen,", "tokens": ["Und", "woll\u00b7te", "lieb\u00b7reich", "stra\u00b7fend", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Warum er sich von ihr gewandt.", "tokens": ["Wa\u00b7rum", "er", "sich", "von", "ihr", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann fragte sie, was wir denn hier", "tokens": ["Dann", "frag\u00b7te", "sie", ",", "was", "wir", "denn", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entfernet und alleine singen,", "tokens": ["Ent\u00b7fer\u00b7net", "und", "al\u00b7lei\u00b7ne", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie wir hie die Zeit verbringen,", "tokens": ["Und", "wie", "wir", "hie", "die", "Zeit", "ver\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach zu uns: Gehorchet mir,", "tokens": ["Und", "sprach", "zu", "uns", ":", "Ge\u00b7hor\u00b7chet", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn heute ist das Namensfest", "tokens": ["Denn", "heu\u00b7te", "ist", "das", "Na\u00b7mens\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der edlen Dorothee erschienen.", "tokens": ["Der", "ed\u00b7len", "Do\u00b7ro\u00b7thee", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wollt ihr die Freundin nicht bedienen?", "tokens": ["Wollt", "ihr", "die", "Freun\u00b7din", "nicht", "be\u00b7die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "