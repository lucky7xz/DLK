{"textgrid.poem.60485": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Bettlers Klage", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin einsam, schwach und alt,", "tokens": ["Bin", "ein\u00b7sam", ",", "schwach", "und", "alt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mich h\u00fcllen Lumpen ein,", "tokens": ["Mich", "h\u00fcl\u00b7len", "Lum\u00b7pen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bl\u00e4st der Wind so kalt,", "tokens": ["Wie", "bl\u00e4st", "der", "Wind", "so", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Geht mir durch Mark und Bein.", "tokens": ["Geht", "mir", "durch", "Mark", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bettle vor der T\u00fcr,", "tokens": ["Ich", "bett\u00b7le", "vor", "der", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und hab ich lang gefleht,", "tokens": ["Und", "hab", "ich", "lang", "ge\u00b7fleht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "me