{"dta.poem.7190": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrende Ruinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo etwas auf der Welt geschickt, zu GOTT zu leiten;", "tokens": ["Wo", "et\u00b7was", "auf", "der", "Welt", "ge\u00b7schickt", ",", "zu", "GoTT", "zu", "lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVPP", "$,", "APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist es dies verworrne ", "tokens": ["So", "ist", "es", "dies", "ver\u00b7worr\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In welchem ich, in deutlich-heller Klarheit,", "tokens": ["In", "wel\u00b7chem", "ich", ",", "in", "deut\u00b7lich\u00b7hel\u00b7ler", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch in gebrochnen Lettern, seh,", "tokens": ["Auch", "in", "ge\u00b7broch\u00b7nen", "Let\u00b7tern", ",", "seh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lehre voller Licht und Wahrheit", "tokens": ["Die", "Leh\u00b7re", "vol\u00b7ler", "Licht", "und", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von irdischen Verg\u00e4nglichkeiten.", "tokens": ["Von", "ir\u00b7di\u00b7schen", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}