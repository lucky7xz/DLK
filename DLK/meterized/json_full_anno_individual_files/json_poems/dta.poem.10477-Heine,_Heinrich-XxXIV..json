{"dta.poem.10477": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Und als ich Euch meine Schmerzen geklagt,", "tokens": ["Und", "als", "ich", "Euch", "mei\u00b7ne", "Schmer\u00b7zen", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da habt Ihr geg\u00e4hnt und nichts gesagt;", "tokens": ["Da", "habt", "Ihr", "ge\u00b7g\u00e4hnt", "und", "nichts", "ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "KON", "PIS", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch als ich sie zierlich in Verse gebracht,", "tokens": ["Doch", "als", "ich", "sie", "zier\u00b7lich", "in", "Ver\u00b7se", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Da habt Ihr mir gro\u00dfe Elogen gemacht.", "tokens": ["Da", "habt", "Ihr", "mir", "gro\u00b7\u00dfe", "E\u00b7lo\u00b7gen", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}