{"dta.poem.23716": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Br&#252;der trinkt, es trinkt die Sonne,", "tokens": ["Br", "&#252;", "der", "trinkt", ",", "es", "trinkt", "die", "Son\u00b7ne", ","], "token_info": ["word", "XML_entity", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie hat schon tausend Str\u00f6me", "tokens": ["Und", "sie", "hat", "schon", "tau\u00b7send", "Str\u00f6\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Bruder ausgetrunken!", "tokens": ["Oh\u00b7ne", "Bru\u00b7der", "aus\u00b7ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Br\u00fcder trinkt, es trinkt die Erde,", "tokens": ["Br\u00fc\u00b7der", "trinkt", ",", "es", "trinkt", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht, sie durstet, seht, wie durstig,", "tokens": ["Seht", ",", "sie", "durs\u00b7tet", ",", "seht", ",", "wie", "durs\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVPP", "$,", "VVFIN", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Trinkt sie diese Regentropfen!", "tokens": ["Trinkt", "sie", "die\u00b7se", "Re\u00b7gen\u00b7trop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seht, dort um den Vater Bacchus", "tokens": ["Seht", ",", "dort", "um", "den", "Va\u00b7ter", "Bac\u00b7chus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "APPR", "ART", "NN", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Stehn die Reben frisch am Berge;", "tokens": ["Stehn", "die", "Re\u00b7ben", "frisch", "am", "Ber\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn es hat das Na\u00df der Wolken", "tokens": ["Denn", "es", "hat", "das", "Na\u00df", "der", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihren heissen Durst gel\u00f6schet.", "tokens": ["Ih\u00b7ren", "heis\u00b7sen", "Durst", "ge\u00b7l\u00f6\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Br\u00fcder seht, das Na\u00df der Reben", "tokens": ["Br\u00fc\u00b7der", "seht", ",", "das", "Na\u00df", "der", "Re\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wartet in den vollen Gl\u00e4sern,", "tokens": ["War\u00b7tet", "in", "den", "vol\u00b7len", "Gl\u00e4\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wollt ihr euren Durst nicht l\u00f6schen?", "tokens": ["Wollt", "ihr", "eu\u00b7ren", "Durst", "nicht", "l\u00f6\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}