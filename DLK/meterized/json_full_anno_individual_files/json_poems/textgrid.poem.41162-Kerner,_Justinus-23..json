{"textgrid.poem.41162": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschenhand hat nicht dies Bild gemacht,", "tokens": ["Men\u00b7schen\u00b7hand", "hat", "nicht", "dies", "Bild", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PDS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gleich den andern kam's durch eigne Macht", "tokens": ["Gleich", "den", "an\u00b7dern", "kam's", "durch", "eig\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ungeahnet aus der Tinte Nacht.", "tokens": ["Un\u00b7ge\u00b7ah\u00b7net", "aus", "der", "Tin\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Es erblickend hab' ich still gedacht:", "tokens": ["Es", "er\u00b7bli\u00b7ckend", "hab'", "ich", "still", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Als der Herr sein Werk hier hat vollbracht,", "tokens": ["Als", "der", "Herr", "sein", "Werk", "hier", "hat", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Fuhr er nieder in der Schatten Reich,", "tokens": ["Fuhr", "er", "nie\u00b7der", "in", "der", "Schat\u00b7ten", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Hat auch diesen noch sein Wort gebracht.", "tokens": ["Hat", "auch", "die\u00b7sen", "noch", "sein", "Wort", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ihr unsel'ge Geister, geht in euch!", "tokens": ["Ihr", "un\u00b7sel'\u00b7ge", "Geis\u00b7ter", ",", "geht", "in", "euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Licht nun auf den schwarzen, leeren Grund,", "tokens": ["Licht", "nun", "auf", "den", "schwar\u00b7zen", ",", "lee\u00b7ren", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Dann f\u00fchlt Reue: denn o welchen Fund", "tokens": ["Dann", "f\u00fchlt", "Reu\u00b7e", ":", "denn", "o", "wel\u00b7chen", "Fund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$.", "KON", "FM", "PWAT", "NN"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.12": {"text": "Werdet schauen ihr voll Schmerz und Schauer,", "tokens": ["Wer\u00b7det", "schau\u00b7en", "ihr", "voll", "Schmerz", "und", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PPER", "ADJD", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Um Erl\u00f6sung flehn in tiefer Trauer.", "tokens": ["Um", "Er\u00b7l\u00f6\u00b7sung", "flehn", "in", "tie\u00b7fer", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Der am Kreuz dem Sch\u00e4cher einst vergeben,", "tokens": ["Der", "am", "Kreuz", "dem", "Sch\u00e4\u00b7cher", "einst", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Als er gl\u00e4ubig sich zu ihm gewandt,", "tokens": ["Als", "er", "gl\u00e4u\u00b7big", "sich", "zu", "ihm", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Der wird dann mit liebevoller Hand", "tokens": ["Der", "wird", "dann", "mit", "lie\u00b7be\u00b7vol\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Aus der Nacht auch euch zum Lichte heben.", "tokens": ["Aus", "der", "Nacht", "auch", "euch", "zum", "Lich\u00b7te", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter"