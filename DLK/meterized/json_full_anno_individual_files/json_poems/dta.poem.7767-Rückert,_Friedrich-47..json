{"dta.poem.7767": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein rechter Lehrer ist, wer pilgernd alle St\u00e4ten", "tokens": ["Ein", "rech\u00b7ter", "Leh\u00b7rer", "ist", ",", "wer", "pil\u00b7gernd", "al\u00b7le", "St\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PWS", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Gangas Quellenmund hat bis ans Meer betreten;", "tokens": ["Von", "Gan\u00b7gas", "Quel\u00b7len\u00b7mund", "hat", "bis", "ans", "Meer", "be\u00b7tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An jedem heil'gen Strom, der in die Ganga m\u00fcndet,", "tokens": ["An", "je\u00b7dem", "heil'\u00b7gen", "Strom", ",", "der", "in", "die", "Gan\u00b7ga", "m\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat im Gebet gekniet, und sich im Bad ents\u00fcndet;", "tokens": ["Hat", "im", "Ge\u00b7bet", "ge\u00b7kniet", ",", "und", "sich", "im", "Bad", "ent\u00b7s\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,", "KON", "PRF", "APPRART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und dann zur Einsamkeit den Duft zur\u00fcckgebracht", "tokens": ["Und", "dann", "zur", "Ein\u00b7sam\u00b7keit", "den", "Duft", "zu\u00b7r\u00fcck\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Gottes Gnadenf\u00fcll' und seiner Sch\u00f6pfung Pracht.", "tokens": ["Von", "Got\u00b7tes", "Gna\u00b7den\u00b7f\u00fcll'", "und", "sei\u00b7ner", "Sch\u00f6p\u00b7fung", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und in der Einsamkeit das helle Bild entfaltet", "tokens": ["Und", "in", "der", "Ein\u00b7sam\u00b7keit", "das", "hel\u00b7le", "Bild", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Gottes Herrlichkeit, die durch die Sch\u00f6pfung waltet.", "tokens": ["Von", "Got\u00b7tes", "Herr\u00b7lich\u00b7keit", ",", "die", "durch", "die", "Sch\u00f6p\u00b7fung", "wal\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Auf seines Mundes Wort mag wohl ein Sch\u00fcler lauschen,", "tokens": ["Auf", "sei\u00b7nes", "Mun\u00b7des", "Wort", "mag", "wohl", "ein", "Sch\u00fc\u00b7ler", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vereinigt h\u00f6rt' er dort die heil'gen Str\u00f6me rauschen.", "tokens": ["Ver\u00b7ei\u00b7nigt", "h\u00f6rt'", "er", "dort", "die", "heil'\u00b7gen", "Str\u00f6\u00b7me", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}