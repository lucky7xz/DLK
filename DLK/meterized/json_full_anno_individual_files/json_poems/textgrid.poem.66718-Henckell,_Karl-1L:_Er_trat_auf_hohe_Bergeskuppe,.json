{"textgrid.poem.66718": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er trat auf hohe Bergeskuppe,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er trat auf hohe Bergeskuppe,", "tokens": ["Er", "trat", "auf", "ho\u00b7he", "Ber\u00b7ges\u00b7kup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fr\u00fchrot beschien sein Angesicht,", "tokens": ["Fr\u00fch\u00b7rot", "be\u00b7schien", "sein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Nebeldrache zog die Schuppe", "tokens": ["Der", "Ne\u00b7bel\u00b7dra\u00b7che", "zog", "die", "Schup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck in tiefer Kl\u00fcfte Schicht.", "tokens": ["Zu\u00b7r\u00fcck", "in", "tie\u00b7fer", "Kl\u00fcf\u00b7te", "Schicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein frisches Brausen war zu h\u00f6ren,", "tokens": ["Ein", "fri\u00b7sches", "Brau\u00b7sen", "war", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Frei atmete die Brust der Welt,", "tokens": ["Frei", "at\u00b7me\u00b7te", "die", "Brust", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.7": {"text": "Die Gipfel grauer Wetterf\u00f6hren", "tokens": ["Die", "Gip\u00b7fel", "grau\u00b7er", "Wet\u00b7ter\u00b7f\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durchschauert's morgenwindgeschwellt:", "tokens": ["Durch\u00b7schau\u00b7ert's", "mor\u00b7gen\u00b7wind\u00b7ge\u00b7schwellt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbder Gipfel Hauch hat mich geboren,", "tokens": ["\u00bb", "der", "Gip\u00b7fel", "Hauch", "hat", "mich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der H\u00f6hen Licht hat mich gezeugt,", "tokens": ["Der", "H\u00f6\u00b7hen", "Licht", "hat", "mich", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "R\u00fctteln will ich an Gottes Toren,", "tokens": ["R\u00fct\u00b7teln", "will", "ich", "an", "Got\u00b7tes", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Bis Gott sich meinem Geiste beugt.", "tokens": ["Bis", "Gott", "sich", "mei\u00b7nem", "Geis\u00b7te", "beugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Erde Ball will ich betreten", "tokens": ["Der", "Er\u00b7de", "Ball", "will", "ich", "be\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit unerh\u00f6rtem Siegerschritt,", "tokens": ["Mit", "un\u00b7er\u00b7h\u00f6r\u00b7tem", "Sie\u00b7ger\u00b7schritt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich will den Teig der Menschheit kneten,", "tokens": ["Ich", "will", "den", "Teig", "der", "Menschheit", "kne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Bis h\u00f6chste Form den Kranz erstritt.", "tokens": ["Bis", "h\u00f6chs\u00b7te", "Form", "den", "Kranz", "er\u00b7stritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von bl\u00f6den Augen will ich rei\u00dfen", "tokens": ["Von", "bl\u00f6\u00b7den", "Au\u00b7gen", "will", "ich", "rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die tr\u00fcben Schleier Not und Wahn,", "tokens": ["Die", "tr\u00fc\u00b7ben", "Schlei\u00b7er", "Not", "und", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und ein Erobrer will ich hei\u00dfen,", "tokens": ["Und", "ein", "E\u00b7rob\u00b7rer", "will", "ich", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Doch Gl\u00fcck umleuchte meine Bahn!", "tokens": ["Doch", "Gl\u00fcck", "um\u00b7leuch\u00b7te", "mei\u00b7ne", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich wei\u00df, was in den Tiefen schmachtet,", "tokens": ["Ich", "wei\u00df", ",", "was", "in", "den", "Tie\u00b7fen", "schmach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ich wittre, was zum Lichte dr\u00e4ngt,", "tokens": ["Ich", "witt\u00b7re", ",", "was", "zum", "Lich\u00b7te", "dr\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und schon als Kind hab ich verachtet,", "tokens": ["Und", "schon", "als", "Kind", "hab", "ich", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Was uns in Rost und Ketten zw\u00e4ngt.", "tokens": ["Was", "uns", "in", "Rost", "und", "Ket\u00b7ten", "zw\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Du Glaube, der den Sinn zerr\u00fcttet,", "tokens": ["Du", "Glau\u00b7be", ",", "der", "den", "Sinn", "zer\u00b7r\u00fct\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Du Satzung, die das Leben narrt,", "tokens": ["Du", "Sat\u00b7zung", ",", "die", "das", "Le\u00b7ben", "narrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Weh euch, die ihr erstickt, versch\u00fcttet", "tokens": ["Weh", "euch", ",", "die", "ihr", "er\u00b7stickt", ",", "ver\u00b7sch\u00fct\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Das Glutgef\u00fchl der Gegenwart!", "tokens": ["Das", "Glut\u00b7ge\u00b7f\u00fchl", "der", "Ge\u00b7gen\u00b7wart", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ein Feuer ist in mich geschlagen", "tokens": ["Ein", "Feu\u00b7er", "ist", "in", "mich", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Von unsichtbarer Riesenhand,", "tokens": ["Von", "un\u00b7sicht\u00b7ba\u00b7rer", "Rie\u00b7sen\u00b7hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Das mu\u00df von Herd zu Herd ich tragen", "tokens": ["Das", "mu\u00df", "von", "Herd", "zu", "Herd", "ich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "NN", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Durch das erschreckte Menschenland.", "tokens": ["Durch", "das", "er\u00b7schreck\u00b7te", "Men\u00b7schen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ich sehe H\u00e4nde mir erhoben", "tokens": ["Ich", "se\u00b7he", "H\u00e4n\u00b7de", "mir", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und Augen mir entgegengl\u00fchn,", "tokens": ["Und", "Au\u00b7gen", "mir", "ent\u00b7ge\u00b7gen\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Viel arme Toren h\u00f6r' ich toben,", "tokens": ["Viel", "ar\u00b7me", "To\u00b7ren", "h\u00f6r'", "ich", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die lichtscheu sich im Finstern m\u00fchn.", "tokens": ["Die", "licht\u00b7scheu", "sich", "im", "Fins\u00b7tern", "m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die wollen nichts von meinen Gaben,", "tokens": ["Die", "wol\u00b7len", "nichts", "von", "mei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und schm\u00e4hend kehren sie sich fort,", "tokens": ["Und", "schm\u00e4\u00b7hend", "keh\u00b7ren", "sie", "sich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Um ihre Toten zu begraben", "tokens": ["Um", "ih\u00b7re", "To\u00b7ten", "zu", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mit Lippenwerk und L\u00fcgenwort.", "tokens": ["Mit", "Lip\u00b7pen\u00b7werk", "und", "L\u00fc\u00b7gen\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Denn was die Helden je empfunden,", "tokens": ["Denn", "was", "die", "Hel\u00b7den", "je", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die sie verhimmeln blickverkl\u00e4rt,", "tokens": ["Die", "sie", "ver\u00b7him\u00b7meln", "blick\u00b7ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Verleugnen sie zu allen Stunden,", "tokens": ["Ver\u00b7leug\u00b7nen", "sie", "zu", "al\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Die ihr gew\u00f6hnlich Dasein w\u00e4hrt.", "tokens": ["Die", "ihr", "ge\u00b7w\u00f6hn\u00b7lich", "Da\u00b7sein", "w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und was im Leben sie erdr\u00fccken,", "tokens": ["Und", "was", "im", "Le\u00b7ben", "sie", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Was ihr erdr\u00fcckter Sinn verdammt,", "tokens": ["Was", "ihr", "er\u00b7dr\u00fcck\u00b7ter", "Sinn", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Das preisen nun mit Mundentz\u00fccken", "tokens": ["Das", "prei\u00b7sen", "nun", "mit", "Mun\u00b7dent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Sie heilig, hehr und gottentstammt.", "tokens": ["Sie", "hei\u00b7lig", ",", "hehr", "und", "got\u00b7tent\u00b7stammt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Verstehen will ich und umfassen,", "tokens": ["Ver\u00b7ste\u00b7hen", "will", "ich", "und", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "O Einfalt, dich, die nicht versteht,", "tokens": ["O", "Ein\u00b7falt", ",", "dich", ",", "die", "nicht", "ver\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Doch diese Heuchler mu\u00df ich hassen,", "tokens": ["Doch", "die\u00b7se", "Heuch\u00b7ler", "mu\u00df", "ich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Die L\u00fcgen strafen ihr Gebet.", "tokens": ["Die", "L\u00fc\u00b7gen", "stra\u00b7fen", "ihr", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Vielleicht, da\u00df einst ein Strahl des Lichtes", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "einst", "ein", "Strahl", "des", "Lich\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Der Einfalt D\u00e4mmer noch erhellt,", "tokens": ["Der", "Ein\u00b7falt", "D\u00e4m\u00b7mer", "noch", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Vom Zug des niedertr\u00e4chtigen Wichtes", "tokens": ["Vom", "Zug", "des", "nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7gen", "Wich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Bleibt h\u00e4\u00dflich die Natur entstellt. \u2013", "tokens": ["Bleibt", "h\u00e4\u00df\u00b7lich", "die", "Na\u00b7tur", "ent\u00b7stellt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": " ... Gleichviel! Ich h\u00f6re Adler schreien,", "tokens": ["...", "Gleich\u00b7viel", "!", "Ich", "h\u00f6\u00b7re", "Ad\u00b7ler", "schrei\u00b7en", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Um meine Stirne rauscht ihr Flug,", "tokens": ["Um", "mei\u00b7ne", "Stir\u00b7ne", "rauscht", "ihr", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Ich bin gekommen zu befreien", "tokens": ["Ich", "bin", "ge\u00b7kom\u00b7men", "zu", "be\u00b7frei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Und folge meines Wesens Zug.", "tokens": ["Und", "fol\u00b7ge", "mei\u00b7nes", "We\u00b7sens", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Der Maulwurf mag im Dunkel w\u00fchlen,", "tokens": ["Der", "Maul\u00b7wurf", "mag", "im", "Dun\u00b7kel", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Der Uhu flattern durch die Nacht,", "tokens": ["Der", "U\u00b7hu", "flat\u00b7tern", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Ich will des \u00c4thers Wonnen f\u00fchlen", "tokens": ["Ich", "will", "des", "\u00c4\u00b7thers", "Won\u00b7nen", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Und suchen, was allselig macht.\u00ab", "tokens": ["Und", "su\u00b7chen", ",", "was", "all\u00b7se\u00b7lig", "macht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er stand auf hoher Felsenwarte,", "tokens": ["Er", "stand", "auf", "ho\u00b7her", "Fel\u00b7sen\u00b7war\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In F\u00f6hrenharfen griff der Wind,", "tokens": ["In", "F\u00f6h\u00b7ren\u00b7har\u00b7fen", "griff", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Urlied durch die Kronen knarrte:", "tokens": ["Sein", "Ur\u00b7lied", "durch", "die", "Kro\u00b7nen", "knarr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measur