{"textgrid.poem.41522": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am f\u00fcnften Sonntage nach hl. drei K\u00f6nige", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In die Dornen ist dein Wort gefallen,", "tokens": ["In", "die", "Dor\u00b7nen", "ist", "dein", "Wort", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die Dornen, die mein Herz zerrissen;", "tokens": ["In", "die", "Dor\u00b7nen", ",", "die", "mein", "Herz", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du, mein Gott, nur du allein kannst wissen,", "tokens": ["Du", ",", "mein", "Gott", ",", "nur", "du", "al\u00b7lein", "kannst", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie sie schmerzlich sind vor andern allen;", "tokens": ["Wie", "sie", "schmerz\u00b7lich", "sind", "vor", "an\u00b7dern", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "APPR", "PIS", "PIAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In die Dornen meiner bittern Reue,", "tokens": ["In", "die", "Dor\u00b7nen", "mei\u00b7ner", "bit\u00b7tern", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die noch keine Tr\u00f6stung will empfangen.", "tokens": ["Die", "noch", "kei\u00b7ne", "Tr\u00f6s\u00b7tung", "will", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "So verbarg ich es in finstrer Scheue,", "tokens": ["So", "ver\u00b7barg", "ich", "es", "in", "finst\u00b7rer", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und so ist es tr\u00fcbe aufgegangen,", "tokens": ["Und", "so", "ist", "es", "tr\u00fc\u00b7be", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und so w\u00e4chst es auf in bittrer Wonne,", "tokens": ["Und", "so", "w\u00e4chst", "es", "auf", "in", "bit\u00b7trer", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Dornen lassen es gedeihen;", "tokens": ["Und", "die", "Dor\u00b7nen", "las\u00b7sen", "es", "ge\u00b7dei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, mein Boden ist zu hart, im Freien", "tokens": ["Ach", ",", "mein", "Bo\u00b7den", "ist", "zu", "hart", ",", "im", "Frei\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Leckt den Tau vom Felsen ihm die Sonne.", "tokens": ["Leckt", "den", "Tau", "vom", "Fel\u00b7sen", "ihm", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kann es gleich nur langsam sich entfalten,", "tokens": ["Kann", "es", "gleich", "nur", "lang\u00b7sam", "sich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schirmen sie es treulich doch vor St\u00fcrmen", "tokens": ["Schir\u00b7men", "sie", "es", "treu\u00b7lich", "doch", "vor", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und dem Hauch der Lust, dem todeskalten,", "tokens": ["Und", "dem", "Hauch", "der", "Lust", ",", "dem", "to\u00b7des\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und wenn sich des Zweifels Wolken t\u00fcrmen.", "tokens": ["Und", "wenn", "sich", "des", "Zwei\u00b7fels", "Wol\u00b7ken", "t\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "In die Dornen ist dein Wort gefallen,", "tokens": ["In", "die", "Dor\u00b7nen", "ist", "dein", "Wort", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und sie werden blut'ge Rosen tragen;", "tokens": ["Und", "sie", "wer\u00b7den", "blut'\u00b7ge", "Ro\u00b7sen", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Soll ich einst dir zu vertrauen wagen,", "tokens": ["Soll", "ich", "einst", "dir", "zu", "ver\u00b7trau\u00b7en", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Darf ich nur in ihrem Kranze wallen.", "tokens": ["Darf", "ich", "nur", "in", "ih\u00b7rem", "Kran\u00b7ze", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn er recht erstrahlt im Feuerglanze", "tokens": ["Wenn", "er", "recht", "er\u00b7strahlt", "im", "Feu\u00b7er\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und das Haupt mir sengt mit tiefen Wunden,", "tokens": ["Und", "das", "Haupt", "mir", "sengt", "mit", "tie\u00b7fen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dann gedeiht die zarte Gottespflanze,", "tokens": ["Dann", "ge\u00b7deiht", "die", "zar\u00b7te", "Got\u00b7tes\u00b7pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mu\u00df an seinem Schmerzenstrahl gesunden.", "tokens": ["Mu\u00df", "an", "sei\u00b7nem", "Schmer\u00b7zen\u00b7strahl", "ge\u00b7sun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "In Entsagung schwinden mu\u00df mein Leben,", "tokens": ["In", "Ent\u00b7sa\u00b7gung", "schwin\u00b7den", "mu\u00df", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In Betrachtung meine Zeit ersterben;", "tokens": ["In", "Be\u00b7trach\u00b7tung", "mei\u00b7ne", "Zeit", "ers\u00b7ter\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-++--", "measure": "unknown.measure.penta"}, "line.3": {"text": "So nur kann ich um das H\u00f6chste werben,", "tokens": ["So", "nur", "kann", "ich", "um", "das", "H\u00f6chs\u00b7te", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Augen darf ich nicht erheben.", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "darf", "ich", "nicht", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ach, ich habe sie mi\u00dfbraucht zu S\u00fcnden", "tokens": ["Ach", ",", "ich", "ha\u00b7be", "sie", "mi\u00df\u00b7braucht", "zu", "S\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und verscherzt des Aufblicks reine Freude,", "tokens": ["Und", "ver\u00b7scherzt", "des", "Auf\u00b7blicks", "rei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dann nur kann ich noch den Himmel finden,", "tokens": ["Dann", "nur", "kann", "ich", "noch", "den", "Him\u00b7mel", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "So ich ihn in Scham zu schauen meide.", "tokens": ["So", "ich", "ihn", "in", "Scham", "zu", "schau\u00b7en", "mei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn ich blicke in die milden Mienen,", "tokens": ["Wenn", "ich", "bli\u00b7cke", "in", "die", "mil\u00b7den", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O, wie schmerzlich mu\u00df es mich betr\u00fcben,", "tokens": ["O", ",", "wie", "schmerz\u00b7lich", "mu\u00df", "es", "mich", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denen noch das teure Recht geblieben,", "tokens": ["De\u00b7nen", "noch", "das", "teu\u00b7re", "Recht", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihrem Gott in Freudigkeit zu dienen!", "tokens": ["Ih\u00b7rem", "Gott", "in", "Freu\u00b7dig\u00b7keit", "zu", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mu\u00df auch hier die tr\u00fcben Augen lenken,", "tokens": ["Mu\u00df", "auch", "hier", "die", "tr\u00fc\u00b7ben", "Au\u00b7gen", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mu\u00df ergl\u00fchend sie zur Erde schlagen,", "tokens": ["Mu\u00df", "er\u00b7gl\u00fc\u00b7hend", "sie", "zur", "Er\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "In ein reines Auge sie zu senken,", "tokens": ["In", "ein", "rei\u00b7nes", "Au\u00b7ge", "sie", "zu", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Kann ich nimmer sonder Frevel wagen.", "tokens": ["Kann", "ich", "nim\u00b7mer", "son\u00b7der", "Fre\u00b7vel", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und wie tief neig' ich die Stirn, die tr\u00fcbe,", "tokens": ["Und", "wie", "tief", "neig'", "ich", "die", "Stirn", ",", "die", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die S\u00fcnde rauscht an mir vor\u00fcber,", "tokens": ["Wenn", "die", "S\u00fcn\u00b7de", "rauscht", "an", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meinen manche, da\u00df mich Abscheu triebe,", "tokens": ["Mei\u00b7nen", "man\u00b7che", ",", "da\u00df", "mich", "Ab\u00b7scheu", "trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und gewinnen lieber mich und lieber,", "tokens": ["Und", "ge\u00b7win\u00b7nen", "lie\u00b7ber", "mich", "und", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "PPER", "KON", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ist es oft nur mein vergangnes Leben,", "tokens": ["Ist", "es", "oft", "nur", "mein", "ver\u00b7gang\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Grauenhaft zum zweiten Mal geboren.", "tokens": ["Grau\u00b7en\u00b7haft", "zum", "zwei\u00b7ten", "Mal", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, und oft empfind' ich gar mit Beben,", "tokens": ["Ach", ",", "und", "oft", "emp\u00b7find'", "ich", "gar", "mit", "Be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie der Finstre noch kein Spiel verloren!", "tokens": ["Wie", "der", "Finst\u00b7re", "noch", "kein", "Spiel", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aber, was er auch f\u00fcr T\u00fccke hege,", "tokens": ["A\u00b7ber", ",", "was", "er", "auch", "f\u00fcr", "T\u00fc\u00b7cke", "he\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "K\u00e4mpfen will ich um des Himmels Grenzen,", "tokens": ["K\u00e4mp\u00b7fen", "will", "ich", "um", "des", "Him\u00b7mels", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Augen sollen freudig gl\u00e4nzen,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "sol\u00b7len", "freu\u00b7dig", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn ich mich in meine Dornen lege,", "tokens": ["Wenn", "ich", "mich", "in", "mei\u00b7ne", "Dor\u00b7nen", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Da\u00df die Welt nicht meinen Kampf darf r\u00fcgen,", "tokens": ["Da\u00df", "die", "Welt", "nicht", "mei\u00b7nen", "Kampf", "darf", "r\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Oder gar mit eitelm Lob geleiten,", "tokens": ["O\u00b7der", "gar", "mit", "ei\u00b7telm", "Lob", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wohl, ich kann durch Gottes Wunder siegen,", "tokens": ["Wohl", ",", "ich", "kann", "durch", "Got\u00b7tes", "Wun\u00b7der", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aber nimmer mit zwei Feinden streiten.", "tokens": ["A\u00b7ber", "nim\u00b7mer", "mit", "zwei", "Fein\u00b7den", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ob ein Tag mir steigen wird auf Erden,", "tokens": ["Ob", "ein", "Tag", "mir", "stei\u00b7gen", "wird", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo ich frei mich zu den Deinen z\u00e4hle?", "tokens": ["Wo", "ich", "frei", "mich", "zu", "den", "Dei\u00b7nen", "z\u00e4h\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo kein Schwert mehr f\u00e4hrt durch meine Seele,", "tokens": ["Wo", "kein", "Schwert", "mehr", "f\u00e4hrt", "durch", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn mir deine H\u00e4nde sichtbar werden!", "tokens": ["Wenn", "mir", "dei\u00b7ne", "H\u00e4n\u00b7de", "sicht\u00b7bar", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Herr, und soll der Tag mir nimmer scheinen,", "tokens": ["Herr", ",", "und", "soll", "der", "Tag", "mir", "nim\u00b7mer", "schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VMFIN", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "D\u00fcrft' ich ihn in Ewigkeit nicht hoffen,", "tokens": ["D\u00fcrft'", "ich", "ihn", "in", "E\u00b7wig\u00b7keit", "nicht", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dennoch mu\u00df ich meine Schulden weinen,", "tokens": ["Den\u00b7noch", "mu\u00df", "ich", "mei\u00b7ne", "Schul\u00b7den", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "O, der S\u00fcnder hat sich selbst getroffen!", "tokens": ["O", ",", "der", "S\u00fcn\u00b7der", "hat", "sich", "selbst", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "In die Dornen ist dein Wort gefallen,", "tokens": ["In", "die", "Dor\u00b7nen", "ist", "dein", "Wort", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die Dornen, die mein Herz zerrissen;", "tokens": ["In", "die", "Dor\u00b7nen", ",", "die", "mein", "Herz", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du, mein Gott, nur du allein kannst wissen,", "tokens": ["Du", ",", "mein", "Gott", ",", "nur", "du", "al\u00b7lein", "kannst", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie sie schmerzlich sind vor andern allen;", "tokens": ["Wie", "sie", "schmerz\u00b7lich", "sind", "vor", "an\u00b7dern", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "APPR", "PIS", "PIAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In die Dornen meiner bittern Reue,", "tokens": ["In", "die", "Dor\u00b7nen", "mei\u00b7ner", "bit\u00b7tern", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die noch keine Tr\u00f6stung will empfangen.", "tokens": ["Die", "noch", "kei\u00b7ne", "Tr\u00f6s\u00b7tung", "will", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "So verbarg ich es in finstrer Scheue,", "tokens": ["So", "ver\u00b7barg", "ich", "es", "in", "finst\u00b7rer", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und so ist es tr\u00fcbe aufgegangen,", "tokens": ["Und", "so", "ist", "es", "tr\u00fc\u00b7be", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Und so w\u00e4chst es auf in bittrer Wonne,", "tokens": ["Und", "so", "w\u00e4chst", "es", "auf", "in", "bit\u00b7trer", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Dornen lassen es gedeihen;", "tokens": ["Und", "die", "Dor\u00b7nen", "las\u00b7sen", "es", "ge\u00b7dei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, mein Boden ist zu hart, im Freien", "tokens": ["Ach", ",", "mein", "Bo\u00b7den", "ist", "zu", "hart", ",", "im", "Frei\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Leckt den Tau vom Felsen ihm die Sonne.", "tokens": ["Leckt", "den", "Tau", "vom", "Fel\u00b7sen", "ihm", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kann es gleich nur langsam sich entfalten,", "tokens": ["Kann", "es", "gleich", "nur", "lang\u00b7sam", "sich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schirmen sie es treulich doch vor St\u00fcrmen", "tokens": ["Schir\u00b7men", "sie", "es", "treu\u00b7lich", "doch", "vor", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und dem Hauch der Lust, dem todeskalten,", "tokens": ["Und", "dem", "Hauch", "der", "Lust", ",", "dem", "to\u00b7des\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und wenn sich des Zweifels Wolken t\u00fcrmen.", "tokens": ["Und", "wenn", "sich", "des", "Zwei\u00b7fels", "Wol\u00b7ken", "t\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "In die Dornen ist dein Wort gefallen,", "tokens": ["In", "die", "Dor\u00b7nen", "ist", "dein", "Wort", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und sie werden blut'ge Rosen tragen;", "tokens": ["Und", "sie", "wer\u00b7den", "blut'\u00b7ge", "Ro\u00b7sen", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Soll ich einst dir zu vertrauen wagen,", "tokens": ["Soll", "ich", "einst", "dir", "zu", "ver\u00b7trau\u00b7en", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Darf ich nur in ihrem Kranze wallen.", "tokens": ["Darf", "ich", "nur", "in", "ih\u00b7rem", "Kran\u00b7ze", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn er recht erstrahlt im Feuerglanze", "tokens": ["Wenn", "er", "recht", "er\u00b7strahlt", "im", "Feu\u00b7er\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und das Haupt mir sengt mit tiefen Wunden,", "tokens": ["Und", "das", "Haupt", "mir", "sengt", "mit", "tie\u00b7fen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dann gedeiht die zarte Gottespflanze,", "tokens": ["Dann", "ge\u00b7deiht", "die", "zar\u00b7te", "Got\u00b7tes\u00b7pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mu\u00df an seinem Schmerzenstrahl gesunden.", "tokens": ["Mu\u00df", "an", "sei\u00b7nem", "Schmer\u00b7zen\u00b7strahl", "ge\u00b7sun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "In Entsagung schwinden mu\u00df mein Leben,", "tokens": ["In", "Ent\u00b7sa\u00b7gung", "schwin\u00b7den", "mu\u00df", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In Betrachtung meine Zeit ersterben;", "tokens": ["In", "Be\u00b7trach\u00b7tung", "mei\u00b7ne", "Zeit", "ers\u00b7ter\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-++--", "measure": "unknown.measure.penta"}, "line.3": {"text": "So nur kann ich um das H\u00f6chste werben,", "tokens": ["So", "nur", "kann", "ich", "um", "das", "H\u00f6chs\u00b7te", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Augen darf ich nicht erheben.", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "darf", "ich", "nicht", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ach, ich habe sie mi\u00dfbraucht zu S\u00fcnden", "tokens": ["Ach", ",", "ich", "ha\u00b7be", "sie", "mi\u00df\u00b7braucht", "zu", "S\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und verscherzt des Aufblicks reine Freude,", "tokens": ["Und", "ver\u00b7scherzt", "des", "Auf\u00b7blicks", "rei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dann nur kann ich noch den Himmel finden,", "tokens": ["Dann", "nur", "kann", "ich", "noch", "den", "Him\u00b7mel", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "So ich ihn in Scham zu schauen meide.", "tokens": ["So", "ich", "ihn", "in", "Scham", "zu", "schau\u00b7en", "mei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Wenn ich blicke in die milden Mienen,", "tokens": ["Wenn", "ich", "bli\u00b7cke", "in", "die", "mil\u00b7den", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O, wie schmerzlich mu\u00df es mich betr\u00fcben,", "tokens": ["O", ",", "wie", "schmerz\u00b7lich", "mu\u00df", "es", "mich", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denen noch das teure Recht geblieben,", "tokens": ["De\u00b7nen", "noch", "das", "teu\u00b7re", "Recht", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihrem Gott in Freudigkeit zu dienen!", "tokens": ["Ih\u00b7rem", "Gott", "in", "Freu\u00b7dig\u00b7keit", "zu", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mu\u00df auch hier die tr\u00fcben Augen lenken,", "tokens": ["Mu\u00df", "auch", "hier", "die", "tr\u00fc\u00b7ben", "Au\u00b7gen", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mu\u00df ergl\u00fchend sie zur Erde schlagen,", "tokens": ["Mu\u00df", "er\u00b7gl\u00fc\u00b7hend", "sie", "zur", "Er\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "In ein reines Auge sie zu senken,", "tokens": ["In", "ein", "rei\u00b7nes", "Au\u00b7ge", "sie", "zu", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Kann ich nimmer sonder Frevel wagen.", "tokens": ["Kann", "ich", "nim\u00b7mer", "son\u00b7der", "Fre\u00b7vel", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Und wie tief neig' ich die Stirn, die tr\u00fcbe,", "tokens": ["Und", "wie", "tief", "neig'", "ich", "die", "Stirn", ",", "die", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die S\u00fcnde rauscht an mir vor\u00fcber,", "tokens": ["Wenn", "die", "S\u00fcn\u00b7de", "rauscht", "an", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meinen manche, da\u00df mich Abscheu triebe,", "tokens": ["Mei\u00b7nen", "man\u00b7che", ",", "da\u00df", "mich", "Ab\u00b7scheu", "trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und gewinnen lieber mich und lieber,", "tokens": ["Und", "ge\u00b7win\u00b7nen", "lie\u00b7ber", "mich", "und", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "PPER", "KON", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ist es oft nur mein vergangnes Leben,", "tokens": ["Ist", "es", "oft", "nur", "mein", "ver\u00b7gang\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Grauenhaft zum zweiten Mal geboren.", "tokens": ["Grau\u00b7en\u00b7haft", "zum", "zwei\u00b7ten", "Mal", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ach, und oft empfind' ich gar mit Beben,", "tokens": ["Ach", ",", "und", "oft", "emp\u00b7find'", "ich", "gar", "mit", "Be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie der Finstre noch kein Spiel verloren!", "tokens": ["Wie", "der", "Finst\u00b7re", "noch", "kein", "Spiel", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Aber, was er auch f\u00fcr T\u00fccke hege,", "tokens": ["A\u00b7ber", ",", "was", "er", "auch", "f\u00fcr", "T\u00fc\u00b7cke", "he\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "K\u00e4mpfen will ich um des Himmels Grenzen,", "tokens": ["K\u00e4mp\u00b7fen", "will", "ich", "um", "des", "Him\u00b7mels", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Augen sollen freudig gl\u00e4nzen,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "sol\u00b7len", "freu\u00b7dig", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn ich mich in meine Dornen lege,", "tokens": ["Wenn", "ich", "mich", "in", "mei\u00b7ne", "Dor\u00b7nen", "le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Da\u00df die Welt nicht meinen Kampf darf r\u00fcgen,", "tokens": ["Da\u00df", "die", "Welt", "nicht", "mei\u00b7nen", "Kampf", "darf", "r\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Oder gar mit eitelm Lob geleiten,", "tokens": ["O\u00b7der", "gar", "mit", "ei\u00b7telm", "Lob", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wohl, ich kann durch Gottes Wunder siegen,", "tokens": ["Wohl", ",", "ich", "kann", "durch", "Got\u00b7tes", "Wun\u00b7der", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aber nimmer mit zwei Feinden streiten.", "tokens": ["A\u00b7ber", "nim\u00b7mer", "mit", "zwei", "Fein\u00b7den", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Ob ein Tag mir steigen wird auf Erden,", "tokens": ["Ob", "ein", "Tag", "mir", "stei\u00b7gen", "wird", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo ich frei mich zu den Deinen z\u00e4hle?", "tokens": ["Wo", "ich", "frei", "mich", "zu", "den", "Dei\u00b7nen", "z\u00e4h\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo kein Schwert mehr f\u00e4hrt durch meine Seele,", "tokens": ["Wo", "kein", "Schwert", "mehr", "f\u00e4hrt", "durch", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn mir deine H\u00e4nde sichtbar werden!", "tokens": ["Wenn", "mir", "dei\u00b7ne", "H\u00e4n\u00b7de", "sicht\u00b7bar", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Herr, und soll der Tag mir nimmer scheinen,", "tokens": ["Herr", ",", "und", "soll", "der", "Tag", "mir", "nim\u00b7mer", "schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VMFIN", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "D\u00fcrft' ich ihn in Ewigkeit nicht hoffen,", "tokens": ["D\u00fcrft'", "ich", "ihn", "in", "E\u00b7wig\u00b7keit", "nicht", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dennoch mu\u00df ich meine Schulden weinen,", "tokens": ["Den\u00b7noch", "mu\u00df", "ich", "mei\u00b7ne", "Schul\u00b7den", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "O, der S\u00fcnder hat sich selbst getroffen!", "tokens": ["O", ",", "der", "S\u00fcn\u00b7der", "hat", "sich", "selbst", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}