{"dta.poem.21603": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, heilige Strahlampel des Himmels,                 ", "tokens": ["Will\u00b7kom\u00b7men", ",", "hei\u00b7li\u00b7ge", "Strah\u00b7lam\u00b7pel", "des", "Him\u00b7mels", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die du emporziehst \u00fcber der Berge erglitzernden Saum,", "tokens": ["Die", "du", "em\u00b7por\u00b7ziehst", "\u00fc\u00b7ber", "der", "Ber\u00b7ge", "er\u00b7glit\u00b7zern\u00b7den", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sei mir gegr\u00fc\u00dft! Nun gie\u00dfest du nieder", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "!", "Nun", "gie\u00b7\u00dfest", "du", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In die Wiege der dir zujauchzenden Th\u00e4ler", "tokens": ["In", "die", "Wie\u00b7ge", "der", "dir", "zu\u00b7jauch\u00b7zen\u00b7den", "Th\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "ADJA", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Deines Lichtes allm\u00e4chtige Vollfluth.", "tokens": ["Dei\u00b7nes", "Lich\u00b7tes", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Voll\u00b7fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Nun uml\u00e4chelst du, trauteste Mutter", "tokens": ["Nun", "um\u00b7l\u00e4\u00b7chelst", "du", ",", "trau\u00b7tes\u00b7te", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "All deine Kinder auf der Erde tiefgr\u00fcndiger Breitung,", "tokens": ["All", "dei\u00b7ne", "Kin\u00b7der", "auf", "der", "Er\u00b7de", "tief\u00b7gr\u00fcn\u00b7di\u00b7ger", "Brei\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-++--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Alljedes Buschwerk, jeglichen Baum,", "tokens": ["All\u00b7je\u00b7des", "Busc\u00b7hwerk", ",", "jeg\u00b7li\u00b7chen", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Der sehnsuchtshei\u00df dir seine Gezweige emporstreckt;", "tokens": ["Der", "sehn\u00b7suchts\u00b7hei\u00df", "dir", "sei\u00b7ne", "Ge\u00b7zwei\u00b7ge", "em\u00b7por\u00b7streckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und aus des Kelches kunstreichem Pokale", "tokens": ["Und", "aus", "des", "Kel\u00b7ches", "kuns\u00b7trei\u00b7chem", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Lockst du die prangende Bl\u00fcthe hervor", "tokens": ["Lockst", "du", "die", "pran\u00b7gen\u00b7de", "Bl\u00fc\u00b7the", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und mit der Fr\u00fcchte gesegneter Schenkung", "tokens": ["Und", "mit", "der", "Fr\u00fcch\u00b7te", "ge\u00b7se\u00b7gne\u00b7ter", "Schen\u00b7kung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Kr\u00f6nst du der Bl\u00fcthe farbige Pracht. \u2014", "tokens": ["Kr\u00f6nst", "du", "der", "Bl\u00fc\u00b7the", "far\u00b7bi\u00b7ge", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Aber doch lieber und vielmalig sch\u00f6ner", "tokens": ["A\u00b7ber", "doch", "lie\u00b7ber", "und", "viel\u00b7ma\u00b7lig", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "KON", "ADJD", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Scheint mir die Sonne, die jetzo mir in der Seele erwacht!", "tokens": ["Scheint", "mir", "die", "Son\u00b7ne", ",", "die", "jet\u00b7zo", "mir", "in", "der", "See\u00b7le", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.16": {"text": "Endlich, endlich steiget sie hoch", "tokens": ["End\u00b7lich", ",", "end\u00b7lich", "stei\u00b7get", "sie", "hoch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Und zersetzt mit siegessichrer Gewalt", "tokens": ["Und", "zer\u00b7setzt", "mit", "sie\u00b7ges\u00b7sich\u00b7rer", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Das hartn\u00e4ckig sich str\u00e4ubende, finster sich b\u00e4umende,", "tokens": ["Das", "hart\u00b7n\u00e4\u00b7ckig", "sich", "str\u00e4u\u00b7ben\u00b7de", ",", "fins\u00b7ter", "sich", "b\u00e4u\u00b7men\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "VVFIN", "$,", "VVFIN", "PRF", "ADJA", "$,"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Dunkelheitsn\u00e4chtige Wettergew\u00f6lk,", "tokens": ["Dun\u00b7kel\u00b7heits\u00b7n\u00e4ch\u00b7ti\u00b7ge", "Wet\u00b7ter\u00b7ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.20": {"text": "Das auch in mir so lange gehauset,", "tokens": ["Das", "auch", "in", "mir", "so", "lan\u00b7ge", "ge\u00b7hau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Auch meine Seele mit taglichtscheuem Gespinnst umsponnen.", "tokens": ["Auch", "mei\u00b7ne", "See\u00b7le", "mit", "tag\u00b7licht\u00b7scheu\u00b7em", "Ge\u00b7spinnst", "um\u00b7spon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Denn g\u00e4nzlich nun hab\u2019 ich den R\u00fccken gekehret", "tokens": ["Denn", "g\u00e4nz\u00b7lich", "nun", "hab'", "ich", "den", "R\u00fc\u00b7cken", "ge\u00b7keh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.23": {"text": "Dem nachtumschatteten Gie\u00dfbach des Lebens", "tokens": ["Dem", "nach\u00b7tum\u00b7schat\u00b7te\u00b7ten", "Gie\u00df\u00b7bach", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Und fernab den niedrig gischtenden Wogen,", "tokens": ["Und", "fern\u00b7ab", "den", "nied\u00b7rig", "gischten\u00b7den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.25": {"text": "Mit denen auch ich ehedem bin getrieben,", "tokens": ["Mit", "de\u00b7nen", "auch", "ich", "e\u00b7he\u00b7dem", "bin", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.26": {"text": "Haus\u2019 ich hier oben auf goldigem Bergdach,", "tokens": ["Haus'", "ich", "hier", "o\u00b7ben", "auf", "gol\u00b7di\u00b7gem", "Berg\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Ein einsamer Siedler, Zwiesprach nur haltend mit mir allen", "tokens": ["Ein", "ein\u00b7sa\u00b7mer", "Sied\u00b7ler", ",", "Zwie\u00b7sprach", "nur", "hal\u00b7tend", "mit", "mir", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "ADV", "ADJD", "APPR", "PPER", "PIAT"], "meter": "-+--+-++-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "Und dem pfeilergetragenen, moospelzumflossenen Felsdom.", "tokens": ["Und", "dem", "pfei\u00b7ler\u00b7ge\u00b7tra\u00b7ge\u00b7nen", ",", "moos\u00b7pel\u00b7zum\u00b7flos\u00b7se\u00b7nen", "Fels\u00b7dom", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.29": {"text": "In das h\u00e4rne Gewand Erkenntni\u00df und Wahrheit suchenden Sehens", "tokens": ["In", "das", "h\u00e4r\u00b7ne", "Ge\u00b7wand", "Er\u00b7kennt\u00b7ni\u00df", "und", "Wahr\u00b7heit", "su\u00b7chen\u00b7den", "Se\u00b7hens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "KON", "NN", "ADJA", "NN"], "meter": "--+--+-+--+-+--+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Ist nun endlich geh\u00fcllt meine irdischem Flitter abgewendete Sele.", "tokens": ["Ist", "nun", "end\u00b7lich", "ge\u00b7h\u00fcllt", "mei\u00b7ne", "ir\u00b7di\u00b7schem", "Flit\u00b7ter", "ab\u00b7ge\u00b7wen\u00b7de\u00b7te", "Se\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+--+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.31": {"text": "Ja wahrlich! G\u00e4nzlich habe ich nun entsaget dem sinnebeth\u00f6renen,", "tokens": ["Ja", "wahr\u00b7lich", "!", "G\u00e4nz\u00b7lich", "ha\u00b7be", "ich", "nun", "ent\u00b7sa\u00b7get", "dem", "sin\u00b7ne\u00b7bet\u00b7h\u00f6\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--+--+---+-", "measure": "iambic.septa.relaxed"}, "line.32": {"text": "Nimmer doch wahres Gen\u00fcge erschaffenden Hasten und Gehen;", "tokens": ["Nim\u00b7mer", "doch", "wah\u00b7res", "Ge\u00b7n\u00fc\u00b7ge", "er\u00b7schaf\u00b7fen\u00b7den", "Has\u00b7ten", "und", "Ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.33": {"text": "Und also zerthauen die eisharten Krusten,", "tokens": ["Und", "al\u00b7so", "zer\u00b7thau\u00b7en", "die", "eis\u00b7har\u00b7ten", "Krus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.34": {"text": "Die mich umstarrt mit ert\u00f6dtender K\u00e4lte,", "tokens": ["Die", "mich", "um\u00b7starrt", "mit", "er\u00b7t\u00f6d\u00b7ten\u00b7der", "K\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Namenlosen Jubels schwell\u2019 ich empor in die str\u00f6menden Lfte,", "tokens": ["Na\u00b7men\u00b7lo\u00b7sen", "Ju\u00b7bels", "schwell'", "ich", "em\u00b7por", "in", "die", "str\u00f6\u00b7men\u00b7den", "Lf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Wachse hinauf in des Aethers allweite Zonen.", "tokens": ["Wach\u00b7se", "hin\u00b7auf", "in", "des", "A\u00b7e\u00b7thers", "all\u00b7wei\u00b7te", "Zo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.37": {"text": "Losgestreift aus den stumpfumzirkenden Engen ichs\u00fcchtiger Selbsteit,", "tokens": ["Los\u00b7ge\u00b7streift", "aus", "den", "stumpf\u00b7um\u00b7zir\u00b7ken\u00b7den", "En\u00b7gen", "ich\u00b7s\u00fcch\u00b7ti\u00b7ger", "Selbs\u00b7teit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.38": {"text": "F\u00fchle ich mich, in seligster Wonne erschauernd,", "tokens": ["F\u00fch\u00b7le", "ich", "mich", ",", "in", "se\u00b7ligs\u00b7ter", "Won\u00b7ne", "er\u00b7schau\u00b7ernd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.39": {"text": "Zusammengegossen mit dem Alles im Schoo\u00dfe des Weltgl\u00fcs", "tokens": ["Zu\u00b7sam\u00b7men\u00b7ge\u00b7gos\u00b7sen", "mit", "dem", "Al\u00b7les", "im", "Schoo\u00b7\u00dfe", "des", "Welt\u00b7gl\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "PIS", "APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Umfassenden Wesen der Allheit!", "tokens": ["Um\u00b7fas\u00b7sen\u00b7den", "We\u00b7sen", "der", "All\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}