{"textgrid.poem.55422": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling gr\u00fcnte zeitig, bl\u00fchte froh", "tokens": ["Der", "Fr\u00fch\u00b7ling", "gr\u00fcn\u00b7te", "zei\u00b7tig", ",", "bl\u00fch\u00b7te", "froh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Narziss' und Tulpe, dann die Rose so;", "tokens": ["Na\u00b7rziss'", "und", "Tul\u00b7pe", ",", "dann", "die", "Ro\u00b7se", "so", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch Fr\u00fcchte reiften mit gedr\u00e4ngtem Segen", "tokens": ["Auch", "Fr\u00fcch\u00b7te", "reif\u00b7ten", "mit", "ge\u00b7dr\u00e4ng\u00b7tem", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nah und n\u00e4hern Sonnenglut entgegen;", "tokens": ["Der", "nah", "und", "n\u00e4\u00b7hern", "Son\u00b7nen\u00b7glut", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie zierten wechselnd l\u00e4ngst ersehnte Zeit", "tokens": ["Sie", "zier\u00b7ten", "wech\u00b7selnd", "l\u00e4ngst", "er\u00b7sehn\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und schmeichelten der tiefsten Einsamkeit.", "tokens": ["Und", "schmei\u00b7chel\u00b7ten", "der", "tiefs\u00b7ten", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da stellte sich dem Hocherstaunten dar", "tokens": ["Da", "stell\u00b7te", "sich", "dem", "Ho\u00b7cher\u00b7staun\u00b7ten", "dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein hehrer F\u00fcrst und Jugend Paar um Paar,", "tokens": ["Ein", "heh\u00b7rer", "F\u00fcrst", "und", "Ju\u00b7gend", "Paar", "um", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So gut als lieb, ehrw\u00fcrdig und erfreulich;", "tokens": ["So", "gut", "als", "lieb", ",", "ehr\u00b7w\u00fcr\u00b7dig", "und", "er\u00b7freu\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der innre Sinn bewahret sie getreulich,", "tokens": ["Der", "inn\u00b7re", "Sinn", "be\u00b7wah\u00b7ret", "sie", "ge\u00b7treu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In Fr\u00fchlings-, Sommer-, Herbst- und Wintertagen", "tokens": ["In", "Fr\u00fch\u00b7lings", ",", "Som\u00b7mer", ",", "Herbs\u00b7t", "und", "Win\u00b7ter\u00b7ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "TRUNC", "$,", "TRUNC", "$,", "TRUNC", "KON", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die holden Bilder auf- und abzutragen;", "tokens": ["Die", "hol\u00b7den", "Bil\u00b7der", "auf", "und", "ab\u00b7zu\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "TRUNC", "KON", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So kann er dann, bei solcher Sterne Schein,", "tokens": ["So", "kann", "er", "dann", ",", "bei", "sol\u00b7cher", "Ster\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Auch wenn er wollte, niemals einsam sein.", "tokens": ["Auch", "wenn", "er", "woll\u00b7te", ",", "nie\u00b7mals", "ein\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VMFIN", "$,", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Fr\u00fchling gr\u00fcnte zeitig, bl\u00fchte froh", "tokens": ["Der", "Fr\u00fch\u00b7ling", "gr\u00fcn\u00b7te", "zei\u00b7tig", ",", "bl\u00fch\u00b7te", "froh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Narziss' und Tulpe, dann die Rose so;", "tokens": ["Na\u00b7rziss'", "und", "Tul\u00b7pe", ",", "dann", "die", "Ro\u00b7se", "so", ";"], "token_info": ["word", "word", "word