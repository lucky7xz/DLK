{"dta.poem.11977": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf alle M\u00fcnzen, die in seinem Lande schl\u00e4gt", "tokens": ["Auf", "al\u00b7le", "M\u00fcn\u00b7zen", ",", "die", "in", "sei\u00b7nem", "Lan\u00b7de", "schl\u00e4gt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein F\u00fcrst, ist immer nur das gleiche Bild gepr\u00e4gt.", "tokens": ["Ein", "F\u00fcrst", ",", "ist", "im\u00b7mer", "nur", "das", "glei\u00b7che", "Bild", "ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dagegen Gottes Kunst ist viel erfindungsreicher,", "tokens": ["Da\u00b7ge\u00b7gen", "Got\u00b7tes", "Kunst", "ist", "viel", "er\u00b7fin\u00b7dungs\u00b7rei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschieden alle pr\u00e4gt sein Stempel aus sein gleicher.", "tokens": ["Ver\u00b7schie\u00b7den", "al\u00b7le", "pr\u00e4gt", "sein", "Stem\u00b7pel", "aus", "sein", "glei\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du nimmst die M\u00fcnze, wie der F\u00fcrst sie hat gepr\u00e4gt;", "tokens": ["Du", "nimmst", "die", "M\u00fcn\u00b7ze", ",", "wie", "der", "F\u00fcrst", "sie", "hat", "ge\u00b7pr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimm auch den Menschen an, der Gottes Bildnis tr\u00e4gt!", "tokens": ["Nimm", "auch", "den", "Men\u00b7schen", "an", ",", "der", "Got\u00b7tes", "Bild\u00b7nis", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du nimmst die M\u00fcnze noch, wenn ihr Gepr\u00e4g erlischt;", "tokens": ["Du", "nimmst", "die", "M\u00fcn\u00b7ze", "noch", ",", "wenn", "ihr", "Ge\u00b7pr\u00e4g", "er\u00b7lischt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimm auch den Menschen, wenn das Bild ist halb verwischt!", "tokens": ["Nimm", "auch", "den", "Men\u00b7schen", ",", "wenn", "das", "Bild", "ist", "halb", "ver\u00b7wischt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}