{"dta.poem.3931": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Der dritte  Titul  soll von baar verleiten Sachen,  \n was da zu halten sey, ein kurtz Gesetze machen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Da aber nun der Schnee zu schmeltzen angefangen,", "tokens": ["Da", "a\u00b7ber", "nun", "der", "Schnee", "zu", "schmelt\u00b7zen", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist die Schlitten-Lust zugleich mit untergangen.", "tokens": ["So", "ist", "die", "Schlit\u00b7ten\u00b7Lust", "zu\u00b7gleich", "mit", "un\u00b7ter\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Inde\u00df hat mancher sich in grosse Schuld gesteckt,", "tokens": ["In\u00b7de\u00df", "hat", "man\u00b7cher", "sich", "in", "gros\u00b7se", "Schuld", "ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "PRF", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie beygelegter Brief weitl\u00e4ufftiger entdeckt:", "tokens": ["Wie", "bey\u00b7ge\u00b7leg\u00b7ter", "Brief", "weit\u00b7l\u00e4uff\u00b7ti\u00b7ger", "ent\u00b7deckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Herr Vetter,", "tokens": ["Herr", "Vet\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "weil ich wei\u00df, da\u00df sie mich z\u00e4rtlich lieben,", "tokens": ["weil", "ich", "wei\u00df", ",", "da\u00df", "sie", "mich", "z\u00e4rt\u00b7lich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "So hab ich auch an sie hier diesen Brief geschrieben:", "tokens": ["So", "hab", "ich", "auch", "an", "sie", "hier", "die\u00b7sen", "Brief", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Zustand, wo ich bin, ist \u00fcberaus betr\u00fcbt,", "tokens": ["Der", "Zu\u00b7stand", ",", "wo", "ich", "bin", ",", "ist", "\u00fc\u00b7be\u00b7raus", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn nicht ihr guter Rath mir einen Beytrag giebt.", "tokens": ["Wenn", "nicht", "ihr", "gu\u00b7ter", "Rath", "mir", "ei\u00b7nen", "Bey\u00b7trag", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als letzt ein derber Schnee in unsrer Stadt gefallen,", "tokens": ["Als", "letzt", "ein", "der\u00b7ber", "Schnee", "in", "uns\u00b7rer", "Stadt", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ich die Schlittenfahrt mit Schellen h\u00f6rte schalle\u0303,", "tokens": ["Und", "ich", "die", "Schlit\u00b7ten\u00b7fahrt", "mit", "Schel\u00b7len", "h\u00f6r\u00b7te", "schall\u1ebd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So dacht ich bey mir selbst, du wilst kein Pinsel seyn,", "tokens": ["So", "dacht", "ich", "bey", "mir", "selbst", ",", "du", "wilst", "kein", "Pin\u00b7sel", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,", "PPER", "VMFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und satzte mich sogleich in einen Schlitten ein.", "tokens": ["Und", "satz\u00b7te", "mich", "sog\u00b7leich", "in", "ei\u00b7nen", "Schlit\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das Ding gefiel mir wohl, ich that es alle Tage,", "tokens": ["Das", "Ding", "ge\u00b7fiel", "mir", "wohl", ",", "ich", "that", "es", "al\u00b7le", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es hieng mir gleichsam an, als eine Fieber-Plage,", "tokens": ["Es", "hieng", "mir", "gleich\u00b7sam", "an", ",", "als", "ei\u00b7ne", "Fie\u00b7ber\u00b7Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nun aber, da es aus, so ist der Beutel leer;", "tokens": ["Nun", "a\u00b7ber", ",", "da", "es", "aus", ",", "so", "ist", "der", "Beu\u00b7tel", "leer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach! wer nur nicht so viel dabey noch schuldig w\u00e4r.", "tokens": ["Ach", "!", "wer", "nur", "nicht", "so", "viel", "da\u00b7bey", "noch", "schul\u00b7dig", "w\u00e4r", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "ADV", "PTKNEG", "ADV", "ADV", "PAV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hier \u00fcberschick ich ihm die Sum\u0303a meiner Schulden:", "tokens": ["Hier", "\u00fc\u00b7bersc\u00b7hick", "ich", "ihm", "die", "Sum\u0303a", "mei\u00b7ner", "Schul\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Auf meine neue Uhr stehn ein und zwantzig Gulden,", "tokens": ["Auf", "mei\u00b7ne", "neu\u00b7e", "Uhr", "stehn", "ein", "und", "zwant\u00b7zig", "Gul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sechs Hemden schickt ich auch zu meiner W\u00e4scherin", "tokens": ["Sechs", "Hem\u00b7den", "schickt", "ich", "auch", "zu", "mei\u00b7ner", "W\u00e4\u00b7sche\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der ich vor dieses mahl vier Thaler schuldig bin.", "tokens": ["Der", "ich", "vor", "die\u00b7ses", "mahl", "vier", "Tha\u00b7ler", "schul\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "ADV", "CARD", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das rothe Son\u0303tags-Kleid mit silbernen Balletten,", "tokens": ["Das", "ro\u00b7the", "So\u00f1\u00b7tags\u00b7Kleid", "mit", "sil\u00b7ber\u00b7nen", "Bal\u00b7let\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mein goldner Pathen-Ring, die Spitzen-Hand-", "tokens": ["Mein", "gold\u00b7ner", "Pa\u00b7then\u00b7Ring", ",", "die", "Spit\u00b7zen\u00b7Han\u00b7d"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Manchetten,", "tokens": ["Man\u00b7chet\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.25": {"text": "Der Struv, der grosse Hopp, Bergers ", "tokens": ["Der", "Struv", ",", "der", "gros\u00b7se", "Hopp", ",", "Ber\u00b7gers"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Schilteri Synopsis Juris Canonici,", "tokens": ["Schil\u00b7te\u00b7ri", "Syn\u00b7op\u00b7sis", "Ju\u00b7ris", "Ca\u00b7no\u00b7ni\u00b7ci", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Der seidne Cammer-Rock, (und nehmen sies nicht", "tokens": ["Der", "seid\u00b7ne", "Cam\u00b7mer\u00b7Rock", ",", "(", "und", "neh\u00b7men", "sies", "nicht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "KON", "VVINF", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "\u00fcbel,)", "tokens": ["\u00fc\u00b7bel", ",", ")"], "token_info": ["word", "punct", "punct"], "pos": ["ADJD", "$,", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Carpzovi Opera, und meine grosse Bibel,", "tokens": ["Car\u00b7pzo\u00b7vi", "O\u00b7pe\u00b7ra", ",", "und", "mei\u00b7ne", "gros\u00b7se", "Bi\u00b7bel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und alles, was bey mir nicht allzu mangelhafft,", "tokens": ["Und", "al\u00b7les", ",", "was", "bey", "mir", "nicht", "all\u00b7zu", "man\u00b7gel\u00b7hafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "APPR", "PPER", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Steht, wie ein Pursche sagt, ietzt zur Gevatterschafft.", "tokens": ["Steht", ",", "wie", "ein", "Pur\u00b7sche", "sagt", ",", "ietzt", "zur", "Ge\u00b7vat\u00b7ter\u00b7schafft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Leute drohen mir, sie wollen mich verklagen,", "tokens": ["Die", "Leu\u00b7te", "dro\u00b7hen", "mir", ",", "sie", "wol\u00b7len", "mich", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dem Vater darf ich nichts von meinem Ungl\u00fck sage\u0303,", "tokens": ["Dem", "Va\u00b7ter", "darf", "ich", "nichts", "von", "mei\u00b7nem", "Un\u00b7gl\u00fck", "sag\u1ebd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Herr Vetter, thun sie doch die G\u00fctigkeit an mir,", "tokens": ["Herr", "Vet\u00b7ter", ",", "thun", "sie", "doch", "die", "G\u00fc\u00b7tig\u00b7keit", "an", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und stellen der ", "tokens": ["Und", "stel\u00b7len", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Zwey hundert Thaler Geld kan alles wieder heben.", "tokens": ["Zwey", "hun\u00b7dert", "Tha\u00b7ler", "Geld", "kan", "al\u00b7les", "wie\u00b7der", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "NN", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie k\u00f6nten dem ", "tokens": ["Sie", "k\u00f6n\u00b7ten", "dem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.38": {"text": "Ich h\u00f6rte dieses Jahr gantz ", "tokens": ["Ich", "h\u00f6r\u00b7te", "die\u00b7ses", "Jahr", "gantz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Ich wei\u00df, so th\u00e4t ihm auch der Wechsel nicht so weh.", "tokens": ["Ich", "wei\u00df", ",", "so", "th\u00e4t", "ihm", "auch", "der", "Wech\u00b7sel", "nicht", "so", "weh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich bitte noch einmahl, sich meiner anzunehmen,", "tokens": ["Ich", "bit\u00b7te", "noch", "ein\u00b7mahl", ",", "sich", "mei\u00b7ner", "an\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "$,", "PRF", "PPOSAT", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Schuldner werden mich sonst ehistens besch\u00e4me\u0303.", "tokens": ["Die", "Schuld\u00b7ner", "wer\u00b7den", "mich", "sonst", "e\u00b7his\u00b7tens", "be\u00b7sch\u00e4m\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Doch ihre G\u00fctigkeit verspricht mir wieder Ruh.", "tokens": ["Doch", "ih\u00b7re", "G\u00fc\u00b7tig\u00b7keit", "ver\u00b7spricht", "mir", "wie\u00b7der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Davor verharr ich auch ", "tokens": ["Da\u00b7vor", "ver\u00b7harr", "ich", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Fahre zu.", "tokens": ["Fah\u00b7re", "zu", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}