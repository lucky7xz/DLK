{"dta.poem.7133": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheinet uns so gar ein Traum zu zeigen,", "tokens": ["Es", "schei\u00b7net", "uns", "so", "gar", "ein", "Traum", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie unsre Seel ein Dencken, so ihr eigen,", "tokens": ["Wie", "uns\u00b7re", "Seel", "ein", "Den\u00b7cken", ",", "so", "ihr", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "$,", "ADV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von den Sinnen gantz verschiedlich.", "tokens": ["Und", "von", "den", "Sin\u00b7nen", "gantz", "ver\u00b7schied\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich deucht, da\u00df ich im Traum fast eine Probe sehe,", "tokens": ["Mich", "deucht", ",", "da\u00df", "ich", "im", "Traum", "fast", "ei\u00b7ne", "Pro\u00b7be", "se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf welche Weis\u2019 ein Geist und eine Seele,", "tokens": ["Auf", "wel\u00b7che", "Weis'", "ein", "Geist", "und", "ei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr sich allein gedenck\u2019 und auch bestehe,", "tokens": ["F\u00fcr", "sich", "al\u00b7lein", "ge\u00b7denck", "und", "auch", "be\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ADJD", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ohn da\u00df sie mit dem C\u00f6rper sich verm\u00e4hle,", "tokens": ["Ohn", "da\u00df", "sie", "mit", "dem", "C\u00f6r\u00b7per", "sich", "ver\u00b7m\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Indem sie ohne Licht, und sonder Auge,", "tokens": ["In\u00b7dem", "sie", "oh\u00b7ne", "Licht", ",", "und", "son\u00b7der", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Jm Traum zu sehn und zu empfinden tauge.", "tokens": ["Jm", "Traum", "zu", "sehn", "und", "zu", "emp\u00b7fin\u00b7den", "tau\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}