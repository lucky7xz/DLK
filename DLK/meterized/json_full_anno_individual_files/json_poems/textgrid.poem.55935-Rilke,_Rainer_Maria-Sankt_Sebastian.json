{"textgrid.poem.55935": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Sankt Sebastian", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Liegender so steht er; ganz", "tokens": ["Wie", "ein", "Lie\u00b7gen\u00b7der", "so", "steht", "er", ";", "ganz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "PPER", "$.", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "hingehalten von dem gro\u00dfen Willen.", "tokens": ["hin\u00b7ge\u00b7hal\u00b7ten", "von", "dem", "gro\u00b7\u00dfen", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weitentr\u00fcckt wie M\u00fctter, wenn sie stillen,", "tokens": ["Wei\u00b7ten\u00b7tr\u00fcckt", "wie", "M\u00fct\u00b7ter", ",", "wenn", "sie", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und in sich gebunden wie ein Kranz.", "tokens": ["und", "in", "sich", "ge\u00b7bun\u00b7den", "wie", "ein", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und die Pfeile kommen: jetzt und jetzt", "tokens": ["Und", "die", "Pfei\u00b7le", "kom\u00b7men", ":", "jetzt", "und", "jetzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "$.", "ADV", "KON", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und als spr\u00e4ngen sie aus seinen Lenden,", "tokens": ["und", "als", "spr\u00e4n\u00b7gen", "sie", "aus", "sei\u00b7nen", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOKOM", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "eisern bebend mit den freien Enden.", "tokens": ["ei\u00b7sern", "be\u00b7bend", "mit", "den", "frei\u00b7en", "En\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch er l\u00e4chelt dunkel, unverletzt.", "tokens": ["Doch", "er", "l\u00e4\u00b7chelt", "dun\u00b7kel", ",", "un\u00b7ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Einmal nur wird seine Trauer gro\u00df,", "tokens": ["Ein\u00b7mal", "nur", "wird", "sei\u00b7ne", "Trau\u00b7er", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und die Augen liegen schmerzlich blo\u00df,", "tokens": ["und", "die", "Au\u00b7gen", "lie\u00b7gen", "schmerz\u00b7lich", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "bis sie etwas leugnen, wie Geringes,", "tokens": ["bis", "sie", "et\u00b7was", "leug\u00b7nen", ",", "wie", "Ge\u00b7rin\u00b7ges", ","], "token_in