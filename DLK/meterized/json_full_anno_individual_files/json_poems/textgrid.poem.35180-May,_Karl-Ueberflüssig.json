{"textgrid.poem.35180": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ueberfl\u00fcssig", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nehmt mir den Stein von meinem Grabe;", "tokens": ["Nehmt", "mir", "den", "Stein", "von", "mei\u00b7nem", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "F\u00fcr mich giebts keinen Leichenstein!", "tokens": ["F\u00fcr", "mich", "giebts", "kei\u00b7nen", "Lei\u00b7chen\u00b7stein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich, der ich nun verkl\u00e4rt mich habe,", "tokens": ["Ich", ",", "der", "ich", "nun", "ver\u00b7kl\u00e4rt", "mich", "ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will doch f\u00fcr euch kein Todter sein!", "tokens": ["Will", "doch", "f\u00fcr", "euch", "kein", "Tod\u00b7ter", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum das Weinen und das Klagen,", "tokens": ["Wa\u00b7rum", "das", "Wei\u00b7nen", "und", "das", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu der Gram, das Herzeleid?", "tokens": ["Wo\u00b7zu", "der", "Gram", ",", "das", "Her\u00b7ze\u00b7leid", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ihr von mir hinausgetragen,", "tokens": ["Was", "ihr", "von", "mir", "hin\u00b7aus\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War nur das abgelegte Kleid.", "tokens": ["War", "nur", "das", "ab\u00b7ge\u00b7leg\u00b7te", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bin im Geist bei euch geblieben,", "tokens": ["Ich", "bin", "im", "Geist", "bei", "euch", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr den es keine Trennung giebt,", "tokens": ["F\u00fcr", "den", "es", "kei\u00b7ne", "Tren\u00b7nung", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und werde euch auch ferner lieben,", "tokens": ["Und", "wer\u00b7de", "euch", "auch", "fer\u00b7ner", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So, wie ich euch bisher geliebt.", "tokens": ["So", ",", "wie", "ich", "euch", "bis\u00b7her", "ge\u00b7liebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar k\u00f6nnt ihr mich jetzt nicht mehr sehen,", "tokens": ["Zwar", "k\u00f6nnt", "ihr", "mich", "jetzt", "nicht", "mehr", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Obgleich ihr mir noch sichtbar seid,", "tokens": ["Ob\u00b7gleich", "ihr", "mir", "noch", "sicht\u00b7bar", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist ja weiter nichts geschehen,", "tokens": ["Doch", "ist", "ja", "wei\u00b7ter", "nichts", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als: ich bekam ein andres Kleid.", "tokens": ["Als", ":", "ich", "be\u00b7kam", "ein", "and\u00b7res", "Kleid", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dieses Kleid, ich soll es tragen", "tokens": ["Und", "die\u00b7ses", "Kleid", ",", "ich", "soll", "es", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meinem Heil, zu meinem Gl\u00fcck.", "tokens": ["Zu", "mei\u00b7nem", "Heil", ",", "zu", "mei\u00b7nem", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte \u2013 tr\u00f6stend will ichs sagen \u2013", "tokens": ["Das", "al\u00b7te", "\u2013", "tr\u00f6s\u00b7tend", "will", "ichs", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADJD", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00fcnsche es mir nicht zur\u00fcck.", "tokens": ["Ich", "w\u00fcn\u00b7sche", "es", "mir", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch, wenn ihr weint, d\u00fcrft ihr nicht w\u00e4hnen,", "tokens": ["Doch", ",", "wenn", "ihr", "weint", ",", "d\u00fcrft", "ihr", "nicht", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich k\u00f6nne mich euch selig nahn;", "tokens": ["Ich", "k\u00f6n\u00b7ne", "mich", "euch", "se\u00b7lig", "nahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es thut mir jede eurer Thr\u00e4nen", "tokens": ["Es", "thut", "mir", "je\u00b7de", "eu\u00b7rer", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch weher, als sie euch gethan.", "tokens": ["Noch", "we\u00b7her", ",", "als", "sie", "euch", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00dft sie fortan nicht weiter flie\u00dfen,", "tokens": ["La\u00dft", "sie", "for\u00b7tan", "nicht", "wei\u00b7ter", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So lieb ihr es auch mit mir meint;", "tokens": ["So", "lieb", "ihr", "es", "auch", "mit", "mir", "meint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie auf den H\u00fcgel auszugie\u00dfen,", "tokens": ["Sie", "auf", "den", "H\u00fc\u00b7gel", "aus\u00b7zu\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dazu sind sie doch nicht geweint.", "tokens": ["Da\u00b7zu", "sind", "sie", "doch", "nicht", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Drum, nehmt den Stein von meinem Grabe,",