.tetra"}, "line.5": {"text": "Der Magistrat denkt nichts dabei,", "tokens": ["Der", "Ma\u00b7gist\u00b7rat", "denkt", "nichts", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kann am Proppen lecken!", "tokens": ["Ich", "kann", "am", "Prop\u00b7pen", "le\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich tret' dem Bund der Landwirt' bei", "tokens": ["Ich", "tret'", "dem", "Bund", "der", "Land\u00b7wirt'", "bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mache da ein Mordsgeschrei", "tokens": ["Und", "ma\u00b7che", "da", "ein", "Mords\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Kompensationen;", "tokens": ["Um", "Kom\u00b7pen\u00b7sa\u00b7ti\u00b7o\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Denn kommt erst der Kanal hier durch,", "tokens": ["Denn", "kommt", "erst", "der", "Ka\u00b7nal", "hier", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bin ich erst recht in Merseburg", "tokens": ["Bin", "ich", "erst", "recht", "in", "Mer\u00b7se\u00b7burg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER