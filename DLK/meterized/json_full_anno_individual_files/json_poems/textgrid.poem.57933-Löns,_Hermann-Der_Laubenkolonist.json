{"textgrid.poem.57933": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Laubenkolonist", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich armer Laubenkolonist,", "tokens": ["Ich", "ar\u00b7mer", "Lau\u00b7ben\u00b7ko\u00b7lo\u00b7nist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sitz' ich nun mit meinem Mist", "tokens": ["Da", "sitz'", "ich", "nun", "mit", "mei\u00b7nem", "Mist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kann ihn nicht gebrauchen;", "tokens": ["Und", "kann", "ihn", "nicht", "ge\u00b7brau\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es steigert mich der Magistrat,", "tokens": ["Es", "stei\u00b7gert", "mich", "der", "Ma\u00b7gist\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kuckuck baue nun Salat,", "tokens": ["Der", "Ku\u00b7ckuck", "bau\u00b7e", "nun", "Sa\u00b7lat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es lohnt nicht mehr das Jauchen!", "tokens": ["Es", "lohnt", "nicht", "mehr", "das", "Jau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das G\u00e4rtchen war so sch\u00f6n bestellt,", "tokens": ["Das", "G\u00e4rt\u00b7chen", "war", "so", "sch\u00f6n", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Teufel seinen Schwanz drauf h\u00e4lt,", "tokens": ["Der", "Teu\u00b7fel", "sei\u00b7nen", "Schwanz", "drauf", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun wird sich's nicht mehr lohnen;", "tokens": ["Nun", "wird", "sich's", "nicht", "mehr", "loh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir sind doch nicht in Kanaan,", "tokens": ["Wir", "sind", "doch", "nicht", "in", "Ka\u00b7naan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Es h\u00e4ngen an den Stangen dran", "tokens": ["Es", "h\u00e4n\u00b7gen", "an", "den", "Stan\u00b7gen", "dran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch man blo\u00df Vietsebohnen!", "tokens": ["Doch", "man", "blo\u00df", "Vie\u00b7tse\u00b7boh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Hamaus fri\u00dft den Sellerie,", "tokens": ["Die", "Ha\u00b7maus", "fri\u00dft", "den", "Sel\u00b7le\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kartoffeln, die geraten nie,", "tokens": ["Kar\u00b7tof\u00b7feln", ",", "die", "ge\u00b7ra\u00b7ten", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Appelbaum tr\u00e4gt Pr\u00fcmmel;", "tokens": ["Der", "Ap\u00b7pel\u00b7baum", "tr\u00e4gt", "Pr\u00fcm\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Porro wird so lang wie'n Mann,", "tokens": ["Der", "Por\u00b7ro", "wird", "so", "lang", "wie'n", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch setzt er keine Bollen an,", "tokens": ["Doch", "setzt", "er", "kei\u00b7ne", "Bol\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das wei\u00df der liebe Himmel!", "tokens": ["Das", "wei\u00df", "der", "lie\u00b7be", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Erdbeern holt sich Spatz und Star,", "tokens": ["Die", "Erd\u00b7beern", "holt", "sich", "Spatz", "und", "Star", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Radieschen werden m\u00e4chtig zwar,", "tokens": ["Ra\u00b7die\u00b7schen", "wer\u00b7den", "m\u00e4ch\u00b7tig", "zwar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch daf\u00fcr sind sie stockig;", "tokens": ["Doch", "da\u00b7f\u00fcr", "sind", "sie", "sto\u00b7ckig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das bi\u00dfchen, was noch \u00fcbrigbleibt,", "tokens": ["Das", "bi\u00df\u00b7chen", ",", "was", "noch", "\u00fcb\u00b7rig\u00b7bleibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann unser Magistrat eintreibt,", "tokens": ["Dann", "un\u00b7ser", "Ma\u00b7gist\u00b7rat", "ein\u00b7treibt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da wird ein B\u00e4hlamm bockig!", "tokens": ["Da", "wird", "ein", "B\u00e4\u00b7hlamm", "bo\u00b7ckig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Was soll ich mit der Laube nun", "tokens": ["Was", "soll", "ich", "mit", "der", "Lau\u00b7be", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit der gr\u00fcnen Banke tun", "tokens": ["Und", "mit", "der", "gr\u00fc\u00b7nen", "Ban\u00b7ke", "tun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den zwei Rosenst\u00f6cken?", "tokens": ["Und", "den", "zwei", "Ro\u00b7sen\u00b7st\u00f6\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Geld, das ist doch nicht von Blei,", "tokens": ["Mein", "Geld", ",", "das", "ist", "doch", "nicht", "von", "Blei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Magistrat denkt nichts dabei,", "tokens": ["Der", "Ma\u00b7gist\u00b7rat", "denkt", "nichts", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kann am Proppen lecken!", "tokens": ["Ich", "kann", "am", "Prop\u00b7pen", "le\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich tret' dem Bund der Landwirt' bei", "tokens": ["Ich", "tret'", "dem", "Bund", "der", "Land\u00b7wirt'", "bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mache da ein Mordsgeschrei", "tokens": ["Und", "ma\u00b7che", "da", "ein", "Mords\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Kompensationen;", "tokens": ["Um", "Kom\u00b7pen\u00b7sa\u00b7ti\u00b7o\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Denn kommt erst der Kanal hier durch,", "tokens": ["Denn", "kommt", "erst", "der", "Ka\u00b7nal", "hier", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bin ich erst recht in Merseburg", "tokens": ["Bin", "ich", "erst", "recht", "in", "Mer\u00b7se\u00b7burg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit meinen Vietsebohnen!", "tokens": ["Mit", "mei\u00b7nen", "Vie\u00b7tse\u00b7boh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich armer Laubenkolonist,", "tokens": ["Ich", "ar\u00b7mer", "Lau\u00b7ben\u00b7ko\u00b7lo\u00b7nist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sitz' ich nun mit meinem Mist", "tokens": ["Da", "sitz'", "ich", "nun", "mit", "mei\u00b7nem", "Mist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kann ihn nicht gebrauchen;", "tokens": ["Und", "kann", "ihn", "nicht", "ge\u00