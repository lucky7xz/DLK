{"dta.poem.15628": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entweder Oder ist der Waffen, der zweischneidigen,", "tokens": ["Ent\u00b7we\u00b7der", "O\u00b7der", "ist", "der", "Waf\u00b7fen", ",", "der", "zwei\u00b7schnei\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--++--", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Geschickteste, womit Streitredner sich vertheidigen.", "tokens": ["Ge\u00b7schick\u00b7tes\u00b7te", ",", "wo\u00b7mit", "Strei\u00b7tred\u00b7ner", "sich", "ver\u00b7thei\u00b7di\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "PRF", "VVINF", "$."], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Entweder, oder; eins von beiden mu\u00dft du doch;", "tokens": ["Ent\u00b7we\u00b7der", ",", "o\u00b7der", ";", "eins", "von", "bei\u00b7den", "mu\u00dft", "du", "doch", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "$.", "PIS", "APPR", "PIS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun welches willst du? sag! Ich sage: weder, noch.", "tokens": ["Nun", "wel\u00b7ches", "willst", "du", "?", "sag", "!", "Ich", "sa\u00b7ge", ":", "we\u00b7der", ",", "noch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "PPER", "$.", "VVFIN", "$.", "PPER", "VVFIN", "$.", "KON", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn keins von beiden mir gef\u00e4llt, ist das mein Brauch;", "tokens": ["Wenn", "keins", "von", "bei\u00b7den", "mir", "ge\u00b7f\u00e4llt", ",", "ist", "das", "mein", "Brauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "PPER", "VVPP", "$,", "VAFIN", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist mir beides recht, sag' ich: sowohl alsauch.", "tokens": ["Und", "ist", "mir", "bei\u00b7des", "recht", ",", "sag'", "ich", ":", "so\u00b7wohl", "al\u00b7sauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "ADJD", "$,", "VVFIN", "PPER", "$.", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}