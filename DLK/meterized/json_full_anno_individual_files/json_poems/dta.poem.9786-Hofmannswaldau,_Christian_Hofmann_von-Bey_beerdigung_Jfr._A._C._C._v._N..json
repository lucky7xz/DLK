{"dta.poem.9786": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey beerdigung Jfr. A. C. C. v. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein schwanen-reiner geist/ der aus der wohnung schritt/", "tokens": ["Dein", "schwa\u00b7nen\u00b7rei\u00b7ner", "geist", "/", "der", "aus", "der", "woh\u00b7nung", "schritt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh\u2019 man durch mittel ihn zu bleiben konte zwingen/", "tokens": ["Eh'", "man", "durch", "mit\u00b7tel", "ihn", "zu", "blei\u00b7ben", "kon\u00b7te", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPER", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Befiehlet uns itzund die leiche zu besingen.", "tokens": ["Be\u00b7fieh\u00b7let", "uns", "it\u00b7zund", "die", "lei\u00b7che", "zu", "be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein schatten-leises ach! ist itzt dein hochzeit-lied/", "tokens": ["Ein", "schat\u00b7ten\u00b7lei\u00b7ses", "ach", "!", "ist", "itzt", "dein", "hoch\u00b7zeit\u00b7lied", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ITJ", "$.", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nachdem du durch den todt bist aus der welt geschwommen/", "tokens": ["Nach\u00b7dem", "du", "durch", "den", "todt", "bist", "aus", "der", "welt", "ge\u00b7schwom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJD", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ans gelobte land in sichern hafen kommen.", "tokens": ["Und", "ans", "ge\u00b7lob\u00b7te", "land", "in", "si\u00b7chern", "ha\u00b7fen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da siehestu die welt/ wie wir den schneeball/ an/", "tokens": ["Da", "sie\u00b7hes\u00b7tu", "die", "welt", "/", "wie", "wir", "den", "schnee\u00b7ball", "/", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "PWAV", "PPER", "ART", "NN", "$(", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der unumzirckte glantz dient dir zu einem kleide/", "tokens": ["Der", "un\u00b7um\u00b7zirck\u00b7te", "glantz", "dient", "dir", "zu", "ei\u00b7nem", "klei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die unschuld schm\u00fcckt dein haupt an statt der weissen seide/", "tokens": ["Die", "un\u00b7schuld", "schm\u00fcckt", "dein", "haupt", "an", "statt", "der", "weis\u00b7sen", "sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die auch der liljen-pracht entf\u00e4rbet machen kan/", "tokens": ["Die", "auch", "der", "lil\u00b7jen\u00b7pracht", "ent\u00b7f\u00e4r\u00b7bet", "ma\u00b7chen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein zimmer ist gebaut in die gestirnten zinnen/", "tokens": ["Dein", "zim\u00b7mer", "ist", "ge\u00b7baut", "in", "die", "ge\u00b7stirn\u00b7ten", "zin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Worunter schnee und blitz auff menschen sich entspinnen.", "tokens": ["Wo\u00b7run\u00b7ter", "schnee", "und", "blitz", "auff", "men\u00b7schen", "sich", "ent\u00b7spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVIMP", "APPR", "ADJA", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier liegt das blasse nichts von iederman beweint/", "tokens": ["Hier", "liegt", "das", "blas\u00b7se", "nichts", "von", "ie\u00b7der\u00b7man", "be\u00b7weint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "PIS", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es ruhet zucht und ehr vor deiner schlaffst\u00e4tt schwelle/", "tokens": ["Es", "ru\u00b7het", "zucht", "und", "ehr", "vor", "dei\u00b7ner", "schlaff\u00b7st\u00e4tt", "schwel\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die keuschheit lehnt sich drauff/ ein unzertrennt geselle", "tokens": ["Die", "keuschheit", "lehnt", "sich", "drauff", "/", "ein", "un\u00b7zer\u00b7trennt", "ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PAV", "$(", "ART", "ADJD", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und was ersinnlich ist/ und was noch \u00fcbrig scheint/", "tokens": ["Und", "was", "er\u00b7sinn\u00b7lich", "ist", "/", "und", "was", "noch", "\u00fcb\u00b7rig", "scheint", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$(", "KON", "PWS", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das schlachtet sie dir noch. Das wohnhau\u00df deiner seele", "tokens": ["Das", "schlach\u00b7tet", "sie", "dir", "noch", ".", "Das", "wohn\u00b7hau\u00df", "dei\u00b7ner", "see\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bewacht der H\u00f6chste dir in deines grabes h\u00f6hle.", "tokens": ["Be\u00b7wacht", "der", "H\u00f6chs\u00b7te", "dir", "in", "dei\u00b7nes", "gra\u00b7bes", "h\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du schl\u00e4ffst im seegen ein/ des vaters treue hand", "tokens": ["Du", "schl\u00e4ffst", "im", "see\u00b7gen", "ein", "/", "des", "va\u00b7ters", "treu\u00b7e", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "VVFIN", "ART", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dr\u00fcckt dir die augen zu/ du ruhst in seinem hertzen/", "tokens": ["Dr\u00fcckt", "dir", "die", "au\u00b7gen", "zu", "/", "du", "ruhst", "in", "sei\u00b7nem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKZU", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die seele brennet dir an statt der hochzeit kertzen/", "tokens": ["Die", "see\u00b7le", "bren\u00b7net", "dir", "an", "statt", "der", "hoch\u00b7zeit", "kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein herber thr\u00e4n verbleibt sein allerbestes pfand/", "tokens": ["Ein", "her\u00b7ber", "thr\u00e4n", "ver\u00b7bleibt", "sein", "al\u00b7ler\u00b7bes\u00b7tes", "pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das er zuletzte dir in tieffstem kummer schencket", "tokens": ["Das", "er", "zu\u00b7letz\u00b7te", "dir", "in", "tieffs\u00b7tem", "kum\u00b7mer", "schen\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Aus einem vater-hertz durch diesen ri\u00df gekr\u00e4ncket.", "tokens": ["Aus", "ei\u00b7nem", "va\u00b7ter\u00b7hertz", "durch", "die\u00b7sen", "ri\u00df", "ge\u00b7kr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PDAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Grab! das die tugend selbst mit ruhme \u00fcberdeckt/", "tokens": ["Grab", "!", "das", "die", "tu\u00b7gend", "selbst", "mit", "ruh\u00b7me", "\u00fc\u00b7berd\u00b7eckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Grab! das mit recht verlacht smaragden und rubinen/", "tokens": ["Grab", "!", "das", "mit", "recht", "ver\u00b7lacht", "sma\u00b7rag\u00b7den", "und", "ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "APPR", "ADJD", "VVPP", "VAINF", "KON", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Weil alle todtenbein auch wieder sollen gr\u00fcnen/", "tokens": ["Weil", "al\u00b7le", "tod\u00b7ten\u00b7bein", "auch", "wie\u00b7der", "sol\u00b7len", "gr\u00fc\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Grab! das uns ingesamt ein grosses leid erweckt/", "tokens": ["Grab", "!", "das", "uns", "in\u00b7ge\u00b7samt", "ein", "gros\u00b7ses", "leid", "er\u00b7weckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PPER", "ADJD", "ART", "ADJA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du solst die zeile hier auff deinem leichstein haben:", "tokens": ["Du", "solst", "die", "zei\u00b7le", "hier", "auff", "dei\u00b7nem", "leichs\u00b7tein", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der kern der ist bey GOtt/ die schalen sind vergraben.", "tokens": ["Der", "kern", "der", "ist", "bey", "Gott", "/", "die", "scha\u00b7len", "sind", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "APPR", "NN", "$(", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}