{"textgrid.poem.49898": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Neue Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann auch ein Mensch des andern auf der Erde", "tokens": ["Kann", "auch", "ein", "Mensch", "des", "an\u00b7dern", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz, wie er m\u00f6chte, sein?", "tokens": ["Ganz", ",", "wie", "er", "m\u00f6ch\u00b7te", ",", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VMFIN", "$,", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u2013 In langer Nacht bedacht ich mir's, und mu\u00dfte sagen, nein!", "tokens": ["\u2013", "In", "lan\u00b7ger", "Nacht", "be\u00b7dacht", "ich", "mir's", ",", "und", "mu\u00df\u00b7te", "sa\u00b7gen", ",", "nein", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVFIN", "PPER", "NE", "$,", "KON", "VMFIN", "VVINF", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "So kann ich niemands hei\u00dfen auf der Erde,", "tokens": ["So", "kann", "ich", "nie\u00b7mands", "hei\u00b7\u00dfen", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und niemand w\u00e4re mein?", "tokens": ["Und", "nie\u00b7mand", "w\u00e4\u00b7re", "mein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u2013 Aus Finsternissen hell in mir aufz\u00fcckt ein Freudenschein:", "tokens": ["\u2013", "Aus", "Fins\u00b7ter\u00b7nis\u00b7sen", "hell", "in", "mir", "auf\u00b7z\u00fcckt", "ein", "Freu\u00b7den\u00b7schein", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ADJD", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Sollt ich mit Gott nicht k\u00f6nnen sein,", "tokens": ["Sollt", "ich", "mit", "Gott", "nicht", "k\u00f6n\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PTKNEG", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wie ich m\u00f6chte, mein und dein?", "tokens": ["So", "wie", "ich", "m\u00f6ch\u00b7te", ",", "mein", "und", "dein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VMFIN", "$,", "PPOSAT", "KON", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was hielte mich, da\u00df ich's nicht heute werde?", "tokens": ["Was", "hiel\u00b7te", "mich", ",", "da\u00df", "ich's", "nicht", "heu\u00b7te", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein s\u00fc\u00dfes Schrecken geht durch mein Gebein!", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", "Schre\u00b7cken", "geht", "durch", "mein", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich wundert, da\u00df es mir ein Wunder wollte sein,", "tokens": ["Mich", "wun\u00b7dert", ",", "da\u00df", "es", "mir", "ein", "Wun\u00b7der", "woll\u00b7te", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott selbst zu eigen haben auf der Erde!", "tokens": ["Gott", "selbst", "zu", "ei\u00b7gen", "ha\u00b7ben", "auf", "der", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKA", "ADJD", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Kann auch ein Mensch des andern auf der Erde", "tokens": ["Kann", "auch", "ein", "Mensch", "des", "an\u00b7dern", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz, wie er m\u00f6chte, sein?", "tokens": ["Ganz", ",", "wie", "er", "m\u00f6ch\u00b7te", ",", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VMFIN", "$,", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u2013 In langer Nacht bedacht ich mir's, und mu\u00dfte sagen, nein!", "tokens": ["\u2013", "In", "lan\u00b7ger", "Nacht", "be\u00b7dacht", "ich", "mir's", ",", "und", "mu\u00df\u00b7