{"textgrid.poem.66620": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heut haben sie den Winter verbrannt;", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut haben sie den Winter verbrannt;", "tokens": ["Heut", "ha\u00b7ben", "sie", "den", "Win\u00b7ter", "ver\u00b7brannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In h\u00f6llischen Flammen stand", "tokens": ["In", "h\u00f6l\u00b7li\u00b7schen", "Flam\u00b7men", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Tannzweigh\u00fcgel. Funken flogen", "tokens": ["Der", "Tann\u00b7zweig\u00b7h\u00fc\u00b7gel", ".", "Fun\u00b7ken", "flo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rosaspr\u00fchend. Rauchwolken zogen,", "tokens": ["Ro\u00b7sa\u00b7spr\u00fc\u00b7hend", ".", "Rauch\u00b7wol\u00b7ken", "zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$.", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Schmutzgrau aufwirbelnd. Hoch auf der Stangen", "tokens": ["Schmutz\u00b7grau", "auf\u00b7wir\u00b7belnd", ".", "Hoch", "auf", "der", "Stan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Puppen wollten nicht Feuer fangen.", "tokens": ["Die", "Pup\u00b7pen", "woll\u00b7ten", "nicht", "Feu\u00b7er", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Aber jetzt ein Knall. Leuchtkugeln stiegen,", "tokens": ["A\u00b7ber", "jetzt", "ein", "Knall", ".", "Leucht\u00b7ku\u00b7geln", "stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$.", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die Puppen huben an, sich zu wiegen,", "tokens": ["Die", "Pup\u00b7pen", "hu\u00b7ben", "an", ",", "sich", "zu", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Von Glut gekr\u00fcmmt und gefoltert,", "tokens": ["Von", "Glut", "ge\u00b7kr\u00fcmmt", "und", "ge\u00b7fol\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Nickten sie stumm sich zu \u2013", "tokens": ["Nick\u00b7ten", "sie", "stumm", "sich", "zu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "PTKZU", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Lichterloh sind sie heruntergepoltert", "tokens": ["Lich\u00b7ter\u00b7loh", "sind", "sie", "her\u00b7un\u00b7ter\u00b7ge\u00b7pol\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Und verkohlt im Nu.", "tokens": ["Und", "ver\u00b7kohlt", "im", "Nu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Um das Freudenfeuer im Kreise", "tokens": ["Um", "das", "Freu\u00b7den\u00b7feu\u00b7er", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Zogen die \u00bbZ\u00fcnfte\u00ab nach Ahnenweise,", "tokens": ["Zo\u00b7gen", "die", "\u00bb", "Z\u00fcnf\u00b7te", "\u00ab", "nach", "Ah\u00b7nen\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$(", "NN", "$(", "APPR", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "Zwar heut alles nur Spiel und Schein,", "tokens": ["Zwar", "heut", "al\u00b7les", "nur", "Spiel", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "NN", "KON", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Mu\u00df doch j\u00e4hrlich gezunftet sein.", "tokens": ["Mu\u00df", "doch", "j\u00e4hr\u00b7lich", "ge\u00b7zunf\u00b7tet", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Meistens \u00bbbessere\u00ab Z\u00fcricher Herrn,", "tokens": ["Meis\u00b7tens", "\u00bb", "bes\u00b7se\u00b7re", "\u00ab", "Z\u00fc\u00b7ri\u00b7cher", "Herrn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "$(", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Die da mit Zange und Knieriem marschierten,", "tokens": ["Die", "da", "mit", "Zan\u00b7ge", "und", "Knie\u00b7ri\u00b7em", "mar\u00b7schier\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Unter dem blauen Vergn\u00fcgungsstern", "tokens": ["Un\u00b7ter", "dem", "blau\u00b7en", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7stern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Sich gewerkschaftlich am\u00fcsierten.", "tokens": ["Sich", "ge\u00b7werk\u00b7schaft\u00b7lich", "a\u00b7m\u00fc\u00b7sier\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.21": {"text": "K\u00e4sebla\u00df Schneiderlein neben mir,", "tokens": ["K\u00e4\u00b7se\u00b7bla\u00df", "Schnei\u00b7derl\u00b7ein", "ne\u00b7ben", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.22": {"text": "Eben mit Frau und Kind noch gekommen,", "tokens": ["E\u00b7ben", "mit", "Frau", "und", "Kind", "noch", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.23": {"text": "L\u00e4chelte tr\u00fcb ob der sauberen Zier", "tokens": ["L\u00e4\u00b7chel\u00b7te", "tr\u00fcb", "ob", "der", "sau\u00b7be\u00b7ren", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOUS", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.24": {"text": "Dieser fr\u00f6hlichen Innungsfrommen.", "tokens": ["Die\u00b7ser", "fr\u00f6h\u00b7li\u00b7chen", "In\u00b7nungs\u00b7from\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Und die \u00bbSchneider\u00ab schwangen die Scheren,", "tokens": ["Und", "die", "\u00bb", "Schnei\u00b7der", "\u00ab", "schwan\u00b7gen", "die", "Sche\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "NE", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Tanzten wie Ziegenb\u00f6cke vorbei,", "tokens": ["Tanz\u00b7ten", "wie", "Zie\u00b7gen\u00b7b\u00f6\u00b7cke", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.27": {"text": "Und den Ambo\u00df, den zentnerschweren,", "tokens": ["Und", "den", "Am\u00b7bo\u00df", ",", "den", "zent\u00b7ner\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.28": {"text": "Schleppten der stattlichen \u00bbSchmiede\u00ab drei.", "tokens": ["Schlepp\u00b7ten", "der", "statt\u00b7li\u00b7chen", "\u00bb", "Schmie\u00b7de", "\u00ab", "drei", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$(", "NN", "$(", "CARD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.29": {"text": "\u00bbb\u00e4cker\u00ab in mehlwei\u00dfen Sch\u00fcrzen,", "tokens": ["\u00bb", "b\u00e4\u00b7cker", "\u00ab", "in", "mehl\u00b7wei\u00b7\u00dfen", "Sch\u00fcr\u00b7zen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Schulternd mit Brezeln und Brot,", "tokens": ["Schul\u00b7ternd", "mit", "Bre\u00b7zeln", "und", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.31": {"text": "\u00bbkaufherrn\u00ab mit Safran und W\u00fcrzen", "tokens": ["\u00bb", "kauf\u00b7herrn", "\u00ab", "mit", "Saf\u00b7ran", "und", "W\u00fcr\u00b7zen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVINF", "$(", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schlugen den Winter feierlich tot.", "tokens": ["Schlu\u00b7gen", "den", "Win\u00b7ter", "fei\u00b7er\u00b7lich", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.33": {"text": "\u00bbwenn ich an meinem Ambo\u00df steh", "tokens": ["\u00bb", "wenn", "ich", "an", "mei\u00b7nem", "Am\u00b7bo\u00df", "steh"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Und h\u00e4mmre lustig drein\u00ab \u2013", "tokens": ["Und", "h\u00e4mm\u00b7re", "lus\u00b7tig", "drein", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Rot wehte der Flammberg in die H\u00f6h,", "tokens": ["Rot", "weh\u00b7te", "der", "Flamm\u00b7berg", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "++--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Becken und Pauken schmetterten ein.", "tokens": ["Be\u00b7cken", "und", "Pau\u00b7ken", "schmet\u00b7ter\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.37": {"text": "Und das neugierige Publikum", "tokens": ["Und", "das", "neu\u00b7gie\u00b7ri\u00b7ge", "Pub\u00b7li\u00b7kum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Bummelt' in hellen Haufen herum.", "tokens": ["Bum\u00b7melt'", "in", "hel\u00b7len", "Hau\u00b7fen", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "M\u00e4dchenaugen noch einmal so keck,", "tokens": ["M\u00e4d\u00b7chen\u00b7au\u00b7gen", "noch", "ein\u00b7mal", "so", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Verliebten M\u00e4usen der wahre Speck.", "tokens": ["Ver\u00b7lieb\u00b7ten", "M\u00e4u\u00b7sen", "der", "wah\u00b7re", "Speck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Schau! am B\u00e4umchen zu meiner Seiten", "tokens": ["Schau", "!", "am", "B\u00e4um\u00b7chen", "zu", "mei\u00b7ner", "Sei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.42": {"text": "Lehnten zwei Schwestern \u2013 Halleluja!", "tokens": ["Lehn\u00b7ten", "zwei", "Schwes\u00b7tern", "\u2013", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "CARD", "NN", "$(", "ITJ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.43": {"text": "Da soll einen der Teufel nicht reiten \u2013", "tokens": ["Da", "soll", "ei\u00b7nen", "der", "Teu\u00b7fel", "nicht", "rei\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.44": {"text": "Nektar und Ambrosia!", "tokens": ["Nek\u00b7tar", "und", "A\u00b7mbro\u00b7sia", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "\u00c4ste prasselten laut zusammen,", "tokens": ["\u00c4s\u00b7te", "pras\u00b7sel\u00b7ten", "laut", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.46": {"text": "Gr\u00fcnlich ringelten sich die Flammen.", "tokens": ["Gr\u00fcn\u00b7lich", "rin\u00b7gel\u00b7ten", "sich", "die", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.47": {"text": "Zu der fr\u00f6hlichen Fr\u00fchlingsmette", "tokens": ["Zu", "der", "fr\u00f6h\u00b7li\u00b7chen", "Fr\u00fch\u00b7lings\u00b7met\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.48": {"text": "Schimmerte hell die Alpenkette ...", "tokens": ["Schim\u00b7mer\u00b7te", "hell", "die", "Al\u00b7pen\u00b7ket\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.49": {"text": "Lenzfrohes Lachen ... Teterete!", "tokens": ["Lenz\u00b7fro\u00b7hes", "La\u00b7chen", "...", "Te\u00b7te\u00b7re\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NE", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.50": {"text": "Bekr\u00e4nzte Nachen tanzten im See ...", "tokens": ["Be\u00b7kr\u00e4nz\u00b7te", "Na\u00b7chen", "tanz\u00b7ten", "im", "See", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.51": {"text": "Und die Z\u00fcricher kreuzfidel", "tokens": ["Und", "die", "Z\u00fc\u00b7ri\u00b7cher", "kreuz\u00b7fi\u00b7del"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.52": {"text": "Spritzten ins Feuer der Freuden \u00d6l.", "tokens": ["Spritz\u00b7ten", "ins", "Feu\u00b7er", "der", "Freu\u00b7den", "\u00d6l", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.53": {"text": "W\u00e4hrend der Mond aus blauen Fernen", "tokens": ["W\u00e4h\u00b7rend", "der", "Mond", "aus", "blau\u00b7en", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.54": {"text": "Silberglanz auf die T\u00fcrme go\u00df,", "tokens": ["Sil\u00b7ber\u00b7glanz", "auf", "die", "T\u00fcr\u00b7me", "go\u00df", ","], "token_info": ["word", "word", "word", "