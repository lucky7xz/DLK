{"textgrid.poem.63853": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich einem Schiffer, der zur\u00fcckgekehrt", "tokens": ["Gleich", "ei\u00b7nem", "Schif\u00b7fer", ",", "der", "zu\u00b7r\u00fcck\u00b7ge\u00b7kehrt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von mancher Fahrt, in seiner engen H\u00fctte", "tokens": ["Von", "man\u00b7cher", "Fahrt", ",", "in", "sei\u00b7ner", "en\u00b7gen", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Treu aufbewahrt nach guter Schiffersitte,", "tokens": ["Treu", "auf\u00b7be\u00b7wahrt", "nach", "gu\u00b7ter", "Schif\u00b7fer\u00b7sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ihm die Fremde K\u00f6stliches beschert,", "tokens": ["Was", "ihm", "die", "Frem\u00b7de", "K\u00f6st\u00b7li\u00b7ches", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So hielt auch er des Aufbewahrens wert,", "tokens": ["So", "hielt", "auch", "er", "des", "Auf\u00b7be\u00b7wah\u00b7rens", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was auf des Fl\u00fcgelpferds weltweitem Ritte", "tokens": ["Was", "auf", "des", "Fl\u00fc\u00b7gel\u00b7pferds", "welt\u00b7wei\u00b7tem", "Rit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Welschlands und des Morgenlandes Mitte", "tokens": ["In", "Wel\u00b7schlands", "und", "des", "Mor\u00b7gen\u00b7lan\u00b7des", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielsprachig ihn der Muse Gunst gelehrt.", "tokens": ["Viel\u00b7spra\u00b7chig", "ihn", "der", "Mu\u00b7se", "Gunst", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wie nachdichtend alles er umfa\u00dfte,", "tokens": ["Doch", "wie", "nach\u00b7dich\u00b7tend", "al\u00b7les", "er", "um\u00b7fa\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVPP", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Herz geh\u00f6rte dem nur, was entsprungen", "tokens": ["Sein", "Herz", "ge\u00b7h\u00f6r\u00b7te", "dem", "nur", ",", "was", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADV", "$,", "PWS", "VVINF"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Dem tiefen Grund der heimatlichen Scholle.", "tokens": ["Dem", "tie\u00b7fen", "Grund", "der", "hei\u00b7mat\u00b7li\u00b7chen", "Schol\u0