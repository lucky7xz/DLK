{"dta.poem.10356": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XvII.  \n  Das Lied von den Dukaten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Meine g\u00fcldenen Dukaten,", "tokens": ["Mei\u00b7ne", "g\u00fcl\u00b7de\u00b7nen", "Du\u00b7ka\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt wo seyd Ihr hingerathen?", "tokens": ["Sagt", "wo", "seyd", "Ihr", "hin\u00b7ge\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seyd Ihr bei den g\u00fcldnen Fischlein,", "tokens": ["Seyd", "Ihr", "bei", "den", "g\u00fcld\u00b7nen", "Fisc\u00b7hlein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die im Bache froh und munter", "tokens": ["Die", "im", "Ba\u00b7che", "froh", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tauchen auf und tauchen unter?", "tokens": ["Tau\u00b7chen", "auf", "und", "tau\u00b7chen", "un\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "APPR", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seyd Ihr bei den g\u00fcldnen Bl\u00fcmlein,", "tokens": ["Seyd", "Ihr", "bei", "den", "g\u00fcld\u00b7nen", "Bl\u00fcm\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die auf lieblich gr\u00fcner Aue", "tokens": ["Die", "auf", "lieb\u00b7lich", "gr\u00fc\u00b7ner", "Au\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Funkeln hell vom Morgenthaue?", "tokens": ["Fun\u00b7keln", "hell", "vom", "Mor\u00b7gent\u00b7hau\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seyd Ihr bei den g\u00fcldnen V\u00f6glein,", "tokens": ["Seyd", "Ihr", "bei", "den", "g\u00fcld\u00b7nen", "V\u00f6\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die da schweifen glanzumwoben", "tokens": ["Die", "da", "schwei\u00b7fen", "glan\u00b7zum\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den blauen L\u00fcften oben?", "tokens": ["In", "den", "blau\u00b7en", "L\u00fcf\u00b7ten", "o\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seyd Ihr bei den g\u00fcldnen Sternlein,", "tokens": ["Seyd", "Ihr", "bei", "den", "g\u00fcld\u00b7nen", "Stern\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die im leuchtenden Gewimmel", "tokens": ["Die", "im", "leuch\u00b7ten\u00b7den", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4cheln jede Nacht am Himmel?", "tokens": ["L\u00e4\u00b7cheln", "je\u00b7de", "Nacht", "am", "Him\u00b7mel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! Ihr g\u00fcldenen Dukaten", "tokens": ["Ach", "!", "Ihr", "g\u00fcl\u00b7de\u00b7nen", "Du\u00b7ka\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwimmt nicht in des Baches Well',", "tokens": ["Schwimmt", "nicht", "in", "des", "Ba\u00b7ches", "Well'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Funkelt nicht auf gr\u00fcner Au',", "tokens": ["Fun\u00b7kelt", "nicht", "auf", "gr\u00fc\u00b7ner", "Au'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebet nicht in L\u00fcften blau,", "tokens": ["Schwe\u00b7bet", "nicht", "in", "L\u00fcf\u00b7ten", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4chelt nicht am Himmel hell, \u2014", "tokens": ["L\u00e4\u00b7chelt", "nicht", "am", "Him\u00b7mel", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "ADJD", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Manich\u00e4er, traun!", "tokens": ["Mei\u00b7ne", "Ma\u00b7nic\u00b7h\u00e4\u00b7er", ",", "traun", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Halten Euch in ihren Klau'n.", "tokens": ["Hal\u00b7ten", "Euch", "in", "ih\u00b7ren", "Klau'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}