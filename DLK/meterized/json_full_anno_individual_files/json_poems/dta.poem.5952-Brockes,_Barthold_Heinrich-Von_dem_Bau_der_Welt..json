{"dta.poem.5952": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch diese Wirbel, die so sehr verschiedlich seyn,", "tokens": ["Durch", "die\u00b7se", "Wir\u00b7bel", ",", "die", "so", "sehr", "ver\u00b7schied\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Theilt die Materie sich ein.", "tokens": ["Theilt", "die", "Ma\u00b7te\u00b7rie", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es ist von einem jeden", "tokens": ["Es", "ist", "von", "ei\u00b7nem", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Umkrei\u00df sonderlich und unterschieden.", "tokens": ["Der", "Um\u00b7krei\u00df", "son\u00b7der\u00b7lich", "und", "un\u00b7ter\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da denn dieselben, welche fest,", "tokens": ["Da", "denn", "die\u00b7sel\u00b7ben", ",", "wel\u00b7che", "fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDAT", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Einfolglich weniger gebogen und gepresst;", "tokens": ["Ein\u00b7folg\u00b7lich", "we\u00b7ni\u00b7ger", "ge\u00b7bo\u00b7gen", "und", "ge\u00b7presst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die breiten ihren Lauf", "tokens": ["Die", "brei\u00b7ten", "ih\u00b7ren", "Lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Viel weiter aus, gehn h\u00f6her noch hinauf.", "tokens": ["Viel", "wei\u00b7ter", "aus", ",", "gehn", "h\u00f6\u00b7her", "noch", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "VVFIN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die aber, deren Krafft geringer; sind verschr\u00e4nckt", "tokens": ["Die", "a\u00b7ber", ",", "de\u00b7ren", "Krafft", "ge\u00b7rin\u00b7ger", ";", "sind", "ver\u00b7schr\u00e4nckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "$,", "PRELAT", "NN", "ADJD", "$.", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In einem kleinen Creyse:", "tokens": ["In", "ei\u00b7nem", "klei\u00b7nen", "Crey\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Daher in ihrer steten Reise", "tokens": ["Da\u00b7her", "in", "ih\u00b7rer", "ste\u00b7ten", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Jhr Gang sich lange nicht so hefftig ausw\u00e4rts lenckt.", "tokens": ["Ihr", "Gang", "sich", "lan\u00b7ge", "nicht", "so", "heff\u00b7tig", "aus\u00b7w\u00e4rts", "lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "PTKNEG", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}