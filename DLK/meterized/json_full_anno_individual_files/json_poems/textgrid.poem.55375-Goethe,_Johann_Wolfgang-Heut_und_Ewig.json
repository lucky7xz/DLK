{"textgrid.poem.55375": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Heut und Ewig", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unm\u00f6glich ist's, den Tag dem Tag zu zeigen,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "ist's", ",", "den", "Tag", "dem", "Tag", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nur Verworrnes im Verworrnen spiegelt,", "tokens": ["Der", "nur", "Ver\u00b7worr\u00b7nes", "im", "Ver\u00b7worr\u00b7nen", "spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jeder selbst sich f\u00fchlt als recht und eigen,", "tokens": ["Und", "je\u00b7der", "selbst", "sich", "f\u00fchlt", "als", "recht", "und", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "PRF", "VVFIN", "KOKOM", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Statt sich zu z\u00fcgeln, nur am andern z\u00fcgelt;", "tokens": ["Statt", "sich", "zu", "z\u00fc\u00b7geln", ",", "nur", "am", "an\u00b7dern", "z\u00fc\u00b7gelt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKZU", "VVINF", "$,", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da ist's den Lippen besser, da\u00df sie schweigen,", "tokens": 