{"textgrid.poem.42115": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Dann, wenn du gehst...", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann, wenn Du gehst, scheinst Du mir nie gewesen.", "tokens": ["Dann", ",", "wenn", "Du", "gehst", ",", "scheinst", "Du", "mir", "nie", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich finde mich, wie der vom Traum erwacht,", "tokens": ["Ich", "fin\u00b7de", "mich", ",", "wie", "der", "vom", "Traum", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Versehnt nach einer n\u00e4chsten tiefern Nacht,", "tokens": ["Ver\u00b7sehnt", "nach", "ei\u00b7ner", "n\u00e4chs\u00b7ten", "tie\u00b7fern", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur alten L\u00fcge l\u00e4chelnd zu genesen.", "tokens": ["Zur", "al\u00b7ten", "L\u00fc\u00b7ge", "l\u00e4\u00b7chelnd", "zu", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Dann, wenn Du kommst, wei\u00df ich mich nicht erhalten", "tokens": ["Dann", ",", "wenn", "Du", "kommst", ",", "wei\u00df", "ich", "mich", "nicht", "er\u00b7hal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Je ohne Dich, Du Herz der toten Welt:", "tokens": ["Je", "oh\u00b7ne", "Dich", ",", "Du", "Herz", "der", "to\u00b7ten", "Welt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du Brand, vor dessen Glut mich das Erkalten,", "tokens": ["Du", "Brand", ",", "vor", "des\u00b7sen", "Glut", "mich", "das", "Er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPR", "PRELAT", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem ich entrann, erinnernd \u00fcberf\u00e4llt \u2013", "tokens": ["Dem", "ich", "ent\u00b7rann", ",", "e\u00b7rin\u00b7nernd", "\u00fc\u00b7berf\u00b7\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So schwank ich, willig immer zu verlachen", "tokens": ["So", "schwank", "ich", ",", "wil\u00b7lig", "im\u00b7mer", "zu", "ver\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der fr\u00fchern Stunde Armut; find ich mich", "tokens": ["Der"