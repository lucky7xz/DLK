{"textgrid.poem.32534": {"metadata": {"author": {"name": "Immermann, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Widerspruch, du Herr der Welt!", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Widerspruch, du Herr der Welt!", "tokens": ["Wi\u00b7der\u00b7spruch", ",", "du", "Herr", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als die G\u00f6tter aus dem Chaos", "tokens": ["Als", "die", "G\u00f6t\u00b7ter", "aus", "dem", "Chaos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Buken diese Welt, die nicht'ge,", "tokens": ["Bu\u00b7ken", "die\u00b7se", "Welt", ",", "die", "nicht'\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "PRELS", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah sie aus, wie ein Geb\u00e4ck,", "tokens": ["Sah", "sie", "aus", ",", "wie", "ein", "Ge\u00b7b\u00e4ck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das sich durfte sehen lassen,", "tokens": ["Das", "sich", "durf\u00b7te", "se\u00b7hen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rund und gl\u00e4nzend, braun und schier,", "tokens": ["Rund", "und", "gl\u00e4n\u00b7zend", ",", "braun", "und", "schier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eingefa\u00dft von schmucker Rinde.", "tokens": ["Ein\u00b7ge\u00b7fa\u00dft", "von", "schmu\u00b7cker", "Rin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch im Innern blieb sie Chaos,", "tokens": ["Doch", "im", "In\u00b7nern", "blieb", "sie", "Chaos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis ins tiefste Eingeweide.", "tokens": ["Bis", "ins", "tiefs\u00b7te", "Ein\u00b7ge\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und sobald die Rinde birst,", "tokens": ["Und", "so\u00b7bald", "die", "Rin\u00b7de", "birst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Streckt des Chaos Sohn, der D\u00e4mon,", "tokens": ["Streckt", "des", "Chaos", "Sohn", ",", "der", "D\u00e4\u00b7mon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "Neckisch vor das irre Haupt,", "tokens": ["Ne\u00b7ckisch", "vor", "das", "ir\u00b7re", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Streckst du vor das Haupt, das hinten", "tokens": ["Streckst", "du", "vor", "das", "Haupt", ",", "das", "hin\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Tr\u00e4gt die Augen, vorn das Haar,", "tokens": ["Tr\u00e4gt", "die", "Au\u00b7gen", ",", "vorn", "das", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Oberw\u00e4rts die Nas' und unten", "tokens": ["O\u00b7berw\u00b7\u00e4rts", "die", "Nas'", "und", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Einen quergef\u00fcgten Mund,", "tokens": ["Ei\u00b7nen", "quer\u00b7ge\u00b7f\u00fcg\u00b7ten", "Mund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Streckst du vor die Wunderglieder,", "tokens": ["Streckst", "du", "vor", "die", "Wun\u00b7der\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Widerspruch, o Herr der Welt!", "tokens": ["Wi\u00b7der\u00b7spruch", ",", "o", "Herr", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tr\u00e4nen, so die Freude weint,", "tokens": ["Tr\u00e4\u00b7nen", ",", "so", "die", "Freu\u00b7de", "weint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die Zeichen deiner Herrschaft,", "tokens": ["Sind", "die", "Zei\u00b7chen", "dei\u00b7ner", "Herr\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn die Verzweiflung lacht,", "tokens": ["Und", "wenn", "die", "Ver\u00b7zwei\u00b7flung", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Klinget deines Ruhms Trompete.", "tokens": ["Klin\u00b7get", "dei\u00b7nes", "Ruhms", "Trom\u00b7pe\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Braut, im Herzen Glut,", "tokens": ["Wenn", "die", "Braut", ",", "im", "Her\u00b7zen", "Glut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ficht im Zeichen spr\u00f6den Sch\u00e4mens,", "tokens": ["Ficht", "im", "Zei\u00b7chen", "spr\u00f6\u00b7den", "Sch\u00e4\u00b7mens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ein langersehntes Gl\u00fcck,", "tokens": ["Wenn", "ein", "lan\u00b7ger\u00b7sehn\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kaum erlangt, uns angewidert,", "tokens": ["Kaum", "er\u00b7langt", ",", "uns", "an\u00b7ge\u00b7wi\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dann, wie oft noch sonst im Jahr,", "tokens": ["Dann", ",", "wie", "oft", "noch", "sonst", "im", "Jahr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Feierst du die hohen Feste,", "tokens": ["Fei\u00b7erst", "du", "die", "ho\u00b7hen", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Widerspruch, o Herr der Welt!", "tokens": ["Wi\u00b7der\u00b7spruch", ",", "o", "Herr", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und im Liede nur ersch\u00f6lle", "tokens": ["Und", "im", "Lie\u00b7de", "nur", "er\u00b7sch\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht dein m\u00e4cht'ges Herrscherwort?", "tokens": ["Nicht", "dein", "m\u00e4cht'\u00b7ges", "Herr\u00b7scher\u00b7wort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind doch unsre armen Reime", "tokens": ["Sind", "doch", "uns\u00b7re", "ar\u00b7men", "Rei\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch ein St\u00fcckchen Welt, erkennen", "tokens": ["Auch", "ein", "St\u00fcck\u00b7chen", "Welt", ",", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcssen sie ja wohl den Meister.", "tokens": ["M\u00fcs\u00b7sen", "sie", "ja", "wohl", "den", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rebellion und Hochverrat", "tokens": ["Re\u00b7bel\u00b7lion", "und", "Hoch\u00b7ver\u00b7rat"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bleibe meiner Seele ferne!", "tokens": ["Blei\u00b7be", "mei\u00b7ner", "See\u00b7le", "fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nein, ich beuge dir mein Knie!", "tokens": ["Nein", ",", "ich", "beu\u00b7ge", "dir", "mein", "Knie", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Unter deinem milden Zepter", "tokens": ["Un\u00b7ter", "dei\u00b7nem", "mil\u00b7den", "Zep\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lebt man herrlich und in Freuden!", "tokens": ["Lebt", "man", "herr\u00b7lich", "und", "in", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ordnung und Zusammenhang,", "tokens": ["Ord\u00b7nung", "und", "Zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Diese Polizeiverwalter,", "tokens": ["Die\u00b7se", "Po\u00b7li\u00b7zei\u00b7ver\u00b7wal\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Hast du gn\u00e4digst abgesetzet,", "tokens": ["Hast", "du", "gn\u00e4\u00b7digst", "ab\u00b7ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wir vergessen, was wir sangen", "tokens": ["Wir", "ver\u00b7ges\u00b7sen", ",", "was", "wir", "san\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "In den fr\u00fcheren Romanzen,", "tokens": ["In", "den", "fr\u00fc\u00b7he\u00b7ren", "Ro\u00b7man\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und wir fall'n aus dem Charakter,", "tokens": ["Und", "wir", "fall'n", "aus", "dem", "Cha\u00b7rak\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ohn' uns just den Hals zu brechen.", "tokens": ["Ohn'", "uns", "just", "den", "Hals", "zu", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lebe hoch, du milder F\u00fcrst,", "tokens": ["Le\u00b7be", "hoch", ",", "du", "mil\u00b7der", "F\u00fcrst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lebe hoch, du g\u00fct'ger K\u00f6nig,", "tokens": ["Le\u00b7be", "hoch", ",", "du", "g\u00fct'\u00b7ger", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sohn des Chaos, m\u00e4cht'ger D\u00e4mon,", "tokens": ["Sohn", "des", "Chaos", ",", "m\u00e4cht'\u00b7ger", "D\u00e4\u00b7mon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Widersprach, du Herr des Liedes!", "tokens": ["Wi\u00b7der\u00b7sprach", ",", "du", "Herr", "des", "Lie\u00b7des", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Widerspruch, du Herr der Welt!", "tokens": ["Wi\u00b7der\u00b7spruch", ",", "du", "Herr", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Widerspruch, du Herr der Welt!", "tokens": ["Wi\u00b7der\u00b7spruch", ",", "du", "Herr", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Als die G\u00f6tter aus dem Chaos", "tokens": ["Als", "die", "G\u00f6t\u00b7ter", "aus", "dem", "Chaos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Buken diese Welt, die nicht'ge,", "tokens": ["Bu\u00b7ken", "die\u00b7se", "Welt", ",", "die", "nicht'\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "PRELS", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah sie aus, wie ein Geb\u00e4ck,", "tokens": ["Sah", "sie", "aus", ",", "wie", "ein", "Ge\u00b7b\u00e4ck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das sich durfte sehen lassen,", "tokens": ["Das", "sich", "durf\u00b7te", "se\u00b7hen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rund und gl\u00e4nzend, braun und schier,", "tokens": ["Rund", "und", "gl\u00e4n\u00b7zend", ",", "braun", "und", "schier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eingefa\u00dft von schmucker Rinde.", "tokens": ["Ein\u00b7ge\u00b7fa\u0