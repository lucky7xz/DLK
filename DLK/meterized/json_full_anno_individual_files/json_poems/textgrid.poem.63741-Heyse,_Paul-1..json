{"textgrid.poem.63741": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei B\u00fcbchen sah ich heut, in Lumpen beide,", "tokens": ["Zwei", "B\u00fcb\u00b7chen", "sah", "ich", "heut", ",", "in", "Lum\u00b7pen", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eins barfu\u00df, eins mit Stiefeln ausger\u00fcstet,", "tokens": ["Eins", "bar\u00b7fu\u00df", ",", "eins", "mit", "Stie\u00b7feln", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Danach wohl keine Seele sonst gel\u00fcstet \u2013", "tokens": ["Da\u00b7nach", "wohl", "kei\u00b7ne", "See\u00b7le", "sonst", "ge\u00b7l\u00fcs\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fast wie das Messer ohne Griff und Schneide.", "tokens": ["Fast", "wie", "das", "Mes\u00b7ser", "oh\u00b7ne", "Griff", "und", "Schnei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sein Spie\u00dfgesell indessen sah's voll Neide,", "tokens": ["Sein", "Spie\u00df\u00b7ge\u00b7sell", "in\u00b7des\u00b7sen", "sah's", "voll", "Nei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sich der Freund mit seinem Schuhwerk br\u00fcstet;", "tokens": ["Wie", "sich", "der", "Freund", "mit", "sei\u00b7nem", "Schuh\u00b7werk", "br\u00fcs\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ob es auch der Zahn der Zeit verw\u00fcstet,", "tokens": ["Denn", "ob", "es", "auch", "der", "Zahn", "der", "Zeit", "ver\u00b7w\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Strahlt der Besitzer doch in stolzer Freude.", "tokens": ["Strahlt", "der", "Be\u00b7sit\u00b7zer", "doch", "in", "stol\u00b7zer", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Den Soldo, den er erst erbetteln m\u00fcssen,", "tokens": ["Den", "Sol\u00b7do", ",", "den", "er", "erst", "er\u00b7bet\u00b7teln", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gab er dem Stiefelputzer, mit Grimassen \u2013!", "tokens": ["Gab", "er", "dem", "Stie\u00b7fel\u00b7put\u00b7zer", ",", "mit", "Gri\u00b7mas\u00b7sen", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Grinsend von einem bis zum andern Ohre.", "tokens": ["Grin\u00b7send", "von", "ei\u00b7nem", "bis", "zum", "an\u00b7dern", "Oh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Und sein Triumphblick tat der Welt zu wissen:", "tokens": ["Und", "sein", "Tri\u00b7umph\u00b7blick", "tat", "der", "Welt", "zu", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer Stiefel hat, kann sie auch putzen lassen,", "tokens": ["Wer", "Stie\u00b7fel", "hat", ",", "kann", "sie", "auch", "put\u00b7zen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wer sie putzen l\u00e4\u00dft, ist ein Signore.", "tokens": ["Und", "wer", "sie", "put\u00b7zen", "l\u00e4\u00dft", ",", "ist", "ein", "Sig\u00b7no\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VVFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zwei B\u00fcbchen sah ich heut, in Lumpen beide,", "tokens": ["Zwei", "B\u00fcb\u00b7chen", "sah", "ich", "heut", ",", "in", "Lum\u