{"dta.poem.14524": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Umkehrende .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie bald in allen B\u00e4umen", "tokens": ["Wie", "bald", "in", "al\u00b7len", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht nun die Morgenluft,", "tokens": ["Geht", "nun", "die", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sch\u00fctteln sich in Tr\u00e4umen,", "tokens": ["Sie", "sch\u00fct\u00b7teln", "sich", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und durch den rothen Duft", "tokens": ["Und", "durch", "den", "ro\u00b7then", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eine fromme Lerche steiget,", "tokens": ["Ei\u00b7ne", "from\u00b7me", "Ler\u00b7che", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn Alles still noch ist,", "tokens": ["Wenn", "Al\u00b7les", "still", "noch", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den rechten Weg Dir zeiget \u2014", "tokens": ["Den", "rech\u00b7ten", "Weg", "Dir", "zei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gelobt sei Jesus Christ!", "tokens": ["Ge\u00b7lobt", "sei", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}