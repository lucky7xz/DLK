{"dta.poem.18506": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die verk\u00fcrtzten winter-n\u00e4chte bey  \n der G\u00fcttner- und Kn\u00e4belischen  \n Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Numehro tritt bereits der winter auff die bahn/", "tokens": ["Nu\u00b7meh\u00b7ro", "tritt", "be\u00b7reits", "der", "win\u00b7ter", "auff", "die", "bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das laub der b\u00e4ume f\u00e4llt/ der erden schoo\u00df gefrieret/", "tokens": ["Das", "laub", "der", "b\u00e4u\u00b7me", "f\u00e4llt", "/", "der", "er\u00b7den", "schoo\u00df", "ge\u00b7frie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das wasser legt das ey\u00df als einen harnisch an/", "tokens": ["Das", "was\u00b7ser", "legt", "das", "ey\u00df", "als", "ei\u00b7nen", "har\u00b7nisch", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "KOKOM", "ART", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in den l\u00fcften wird ein scharffer frost gesp\u00fcret.", "tokens": ["Und", "in", "den", "l\u00fcf\u00b7ten", "wird", "ein", "scharf\u00b7fer", "frost", "ge\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch was die Jungfern-schaar bey dieser kalten zeit", "tokens": ["Doch", "was", "die", "Jung\u00b7fern\u00b7schaar", "bey", "die\u00b7ser", "kal\u00b7ten", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Am allermeisten kr\u00e4nckt/ das sind die langen n\u00e4chte/", "tokens": ["Am", "al\u00b7ler\u00b7meis\u00b7ten", "kr\u00e4nckt", "/", "das", "sind", "die", "lan\u00b7gen", "n\u00e4ch\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "$(", "PDS", "VAFIN", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn diese sind gewi\u00df bey steter einsamkeit", "tokens": ["Denn", "die\u00b7se", "sind", "ge\u00b7wi\u00df", "bey", "ste\u00b7ter", "ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein alzuhartes joch vor ihr beliebt geschlechte.", "tokens": ["Ein", "al\u00b7zu\u00b7har\u00b7tes", "joch", "vor", "ihr", "be\u00b7liebt", "ge\u00b7schlech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem \u00fcbel/ welches sonst der winter mit sich bringt/", "tokens": ["Dem", "\u00fc\u00b7bel", "/", "wel\u00b7ches", "sonst", "der", "win\u00b7ter", "mit", "sich", "bringt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "PWS", "ADV", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wei\u00df ihr ge\u00fcbter geist gar leicht zu widerstehen/", "tokens": ["Wei\u00df", "ihr", "ge\u00b7\u00fcb\u00b7ter", "geist", "gar", "leicht", "zu", "wi\u00b7der\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn wenn der kleinste frost auff ihre glieder dringt/", "tokens": ["Denn", "wenn", "der", "kleins\u00b7te", "frost", "auff", "ih\u00b7re", "glie\u00b7der", "dringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verschweren sie es gleich vom ofen weg zu gehen.", "tokens": ["Ver\u00b7schwe\u00b7ren", "sie", "es", "gleich", "vom", "o\u00b7fen", "weg", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der kurtze tag l\u00e4st sich durch eine schlittenfahrt/", "tokens": ["Der", "kurt\u00b7ze", "tag", "l\u00e4st", "sich", "durch", "ei\u00b7ne", "schlit\u00b7ten\u00b7fahrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "VVFIN", "$("], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Durch spielen und geschw\u00e4tz mit leichter m\u00fch vertreiben.", "tokens": ["Durch", "spie\u00b7len", "und", "ge\u00b7schw\u00e4tz", "mit", "leich\u00b7ter", "m\u00fch", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "KON", "ADJD", "APPR", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allein die n\u00e4chte sind von einer andern art/", "tokens": ["Al\u00b7lein", "die", "n\u00e4ch\u00b7te", "sind", "von", "ei\u00b7ner", "an\u00b7dern", "art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wy will das gute volck vor ihrer l\u00e4nge bleiben?", "tokens": ["Wy", "will", "das", "gu\u00b7te", "volck", "vor", "ih\u00b7rer", "l\u00e4n\u00b7ge", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der frost benimmt alsdenn den augen ihre ruh/", "tokens": ["Der", "frost", "be\u00b7nimmt", "als\u00b7denn", "den", "au\u00b7gen", "ih\u00b7re", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die kammer ist zu kalt/ der w\u00e4rmstein w\u00e4hrt nicht lange/", "tokens": ["Die", "kam\u00b7mer", "ist", "zu", "kalt", "/", "der", "w\u00e4rms\u00b7tein", "w\u00e4hrt", "nicht", "lan\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$(", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und decket ohne dem nur wenig glieder zu:", "tokens": ["Und", "de\u00b7cket", "oh\u00b7ne", "dem", "nur", "we\u00b7nig", "glie\u00b7der", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die schwartze finsterni\u00df macht ihnen gleichfals bange.", "tokens": ["Die", "schwart\u00b7ze", "fins\u00b7ter\u00b7ni\u00df", "macht", "ih\u00b7nen", "gleich\u00b7fals", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie halten iede mau\u00df vor einen polter-geist/", "tokens": ["Sie", "hal\u00b7ten", "ie\u00b7de", "mau\u00df", "vor", "ei\u00b7nen", "pol\u00b7ter\u00b7geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr hertz wird allzuleicht von schrecken eingenommen/", "tokens": ["Ihr", "hertz", "wird", "all\u00b7zu\u00b7leicht", "von", "schre\u00b7cken", "ein\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und weil kein guter freund hiebey gesellschafft leist/", "tokens": ["Und", "weil", "kein", "gu\u00b7ter", "freund", "hie\u00b7bey", "ge\u00b7sell\u00b7schafft", "leist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "NN", "ADV", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So seuftzen sie/ da\u00df doch der morgen m\u00f6ge kommen.", "tokens": ["So", "seuft\u00b7zen", "sie", "/", "da\u00df", "doch", "der", "mor\u00b7gen", "m\u00f6\u00b7ge", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "ADV", "ART", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Indessen st\u00e4rckt zwar offt sie ein verliebter traum/", "tokens": ["In\u00b7des\u00b7sen", "st\u00e4rckt", "zwar", "offt", "sie", "ein", "ver\u00b7lieb\u00b7ter", "traum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie f\u00fchlen tausend lust in einem s\u00fcssen schlummer/", "tokens": ["Sie", "f\u00fch\u00b7len", "tau\u00b7send", "lust", "in", "ei\u00b7nem", "s\u00fcs\u00b7sen", "schlum\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Jedoch erwachen sie; so bringt der leere raum/", "tokens": ["Je\u00b7doch", "er\u00b7wa\u00b7chen", "sie", ";", "so", "bringt", "der", "lee\u00b7re", "raum", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der sich im bette zeigt/ nur neue noth und kummer.", "tokens": ["Der", "sich", "im", "bet\u00b7te", "zeigt", "/", "nur", "neu\u00b7e", "noth", "und", "kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "ADJA", "VVFIN", "$(", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wiewohl hat sie demnach/ verlobte Braut/ gethan", "tokens": ["Wie\u00b7wohl", "hat", "sie", "dem\u00b7nach", "/", "ver\u00b7lob\u00b7te", "Braut", "/", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "$(", "ADJA", "NN", "$(", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df ihr durch kluge wahl ein solcher stand beliebet/", "tokens": ["Da\u00df", "ihr", "durch", "klu\u00b7ge", "wahl", "ein", "sol\u00b7cher", "stand", "be\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "In welchem sie die macht des winters trotzen kan/", "tokens": ["In", "wel\u00b7chem", "sie", "die", "macht", "des", "win\u00b7ters", "trot\u00b7zen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "VVFIN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und wo es niemahls nicht zu lange n\u00e4chte giebet.", "tokens": ["Und", "wo", "es", "nie\u00b7mahls", "nicht", "zu", "lan\u00b7ge", "n\u00e4ch\u00b7te", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PTKNEG", "PTKA", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der nacht-frost machet hier nicht die geringste pein.", "tokens": ["Der", "nacht\u00b7frost", "ma\u00b7chet", "hier", "nicht", "die", "ge\u00b7rings\u00b7te", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der w\u00e4rmstein/ welchen sie das gl\u00fccke heist umbfassen/", "tokens": ["Der", "w\u00e4rms\u00b7tein", "/", "wel\u00b7chen", "sie", "das", "gl\u00fc\u00b7cke", "heist", "umb\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "PWAT", "PPER", "PDS", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wird durch die gantze nacht voll glut und hitze seyn/", "tokens": ["Wird", "durch", "die", "gant\u00b7ze", "nacht", "voll", "glut", "und", "hit\u00b7ze", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und auch kein glied an ihr nicht unerw\u00e4rmet lassen.", "tokens": ["Und", "auch", "kein", "glied", "an", "ihr", "nicht", "un\u00b7er\u00b7w\u00e4r\u00b7met", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die furcht/ die sonst so leicht das Jungfern-volck besiegt/", "tokens": ["Die", "furcht", "/", "die", "sonst", "so", "leicht", "das", "Jung\u00b7fern\u00b7volck", "be\u00b7siegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADV", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wird nicht verm\u00f6gend seyn im dunckeln sie zu schrecken.", "tokens": ["Wird", "nicht", "ver\u00b7m\u00f6\u00b7gend", "seyn", "im", "dun\u00b7ckeln", "sie", "zu", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "VAINF", "APPRART", "ADJA", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie wird denselbigen/ dem sie in armen liegt/", "tokens": ["Sie", "wird", "den\u00b7sel\u00b7bi\u00b7gen", "/", "dem", "sie", "in", "ar\u00b7men", "liegt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "$(", "PRELS", "PPER", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Durch einen sanften ku\u00df gleich wissen aufzuwecken.", "tokens": ["Durch", "ei\u00b7nen", "sanf\u00b7ten", "ku\u00df", "gleich", "wis\u00b7sen", "auf\u00b7zu\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wohlan/ begl\u00fccktes paar