{"textgrid.poem.43312": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Amor fliegt mit Schmetterlingen,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor fliegt mit Schmetterlingen,", "tokens": ["A\u00b7mor", "fliegt", "mit", "Schmet\u00b7ter\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um in frohem Wechselstreit", "tokens": ["Um", "in", "fro\u00b7hem", "Wech\u00b7sel\u00b7streit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich den Preis der Schnelligkeit", "tokens": ["Sich", "den", "Preis", "der", "Schnel\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor den Thierchen zu erringen:", "tokens": ["Vor", "den", "Thier\u00b7chen", "zu", "er\u00b7rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch er f\u00e4llt aus M\u00fcdigkeit", "tokens": ["Doch", "er", "f\u00e4llt", "aus", "M\u00fc\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schnell in einen Bach und schreyt.", "tokens": ["Schnell", "in", "ei\u00b7nen", "Bach", "und", "schreyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An jedem Zipfel keichten zehn,", "tokens": ["An", "je\u00b7dem", "Zip\u00b7fel", "keich\u00b7ten", "zehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PIAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und an der Mitte keichten zehn,", "tokens": ["Und", "an", "der", "Mit\u00b7te", "keich\u00b7ten", "zehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle, \u2013 lustig wars zu sehn \u2013", "tokens": ["Und", "al\u00b7le", ",", "\u2013", "lus\u00b7tig", "wars", "zu", "sehn", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "$(", "ADJD", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verirrten sich in dieses Dinges Falten,", "tokens": ["Ver\u00b7irr\u00b7ten", "sich", "in", "die\u00b7ses", "Din\u00b7ges", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und schrien, sie w\u00fcrden es nicht halten.", "tokens": ["Und", "schri\u00b7en", ",", "sie", "w\u00fcr\u00b7den", "es", "nicht", "hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Kein M\u00e4dchen soll ihr auf der Erden", "tokens": ["Kein", "M\u00e4d\u00b7chen", "soll", "ihr", "auf", "der", "Er\u00b7den"], "token_info": ["wor