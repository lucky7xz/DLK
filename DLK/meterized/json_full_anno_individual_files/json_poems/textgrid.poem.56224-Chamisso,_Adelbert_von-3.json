{"textgrid.poem.56224": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vater kam, der Vater frug nach seinem Jungen,", "tokens": ["Der", "Va\u00b7ter", "kam", ",", "der", "Va\u00b7ter", "frug", "nach", "sei\u00b7nem", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weil der Knabe so geweint,", "tokens": ["Und", "weil", "der", "Kna\u00b7be", "so", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hat ihm auch der Alte gleich ein Lied gesungen,", "tokens": ["So", "hat", "ihm", "auch", "der", "Al\u00b7te", "gleich", "ein", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie er's im Herzen treu gemeint.", "tokens": ["Wie", "er's", "im", "Her\u00b7zen", "treu", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als so ich schrie, wie du nun schreist, die Zeiten waren", "tokens": ["Als", "so", "ich", "schrie", ",", "wie", "du", "nun", "schreist", ",", "die", "Zei\u00b7ten", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht so, wie sie geworden sind,", "tokens": ["Nicht", "so", ",", "wie", "sie", "ge\u00b7wor\u00b7den", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "PPER", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geduld, Geduld! und kommst du erst zu meinen Jahren,", "tokens": ["Ge\u00b7duld", ",", "Ge\u00b7duld", "!", "und", "kommst", "du", "erst", "zu", "mei\u00b7nen", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird es wieder anders, Kind!", "tokens": ["So", "wird", "es", "wie\u00b7der", "an\u00b7ders", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da legten sie, mit gl\u00e4ub'gem Sinn, zu mir dem Knaben", "tokens": ["Da", "leg\u00b7ten", "sie", ",", "mit", "gl\u00e4ub'\u00b7gem", "Sinn", ",", "zu", "mir", "dem", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Vaters Wappenschild und Schwert;", "tokens": ["Des", "Va\u00b7ters", "Wap\u00b7pen\u00b7schild", "und", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Erbe war's, und hatte noch, und sollte haben", "tokens": ["Mein", "Er\u00b7be", "wa\u00b7r's", ",", "und", "hat\u00b7te", "noch", ",", "und", "soll\u00b7te", "ha\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KON", "VAFIN", "ADV", "$,", "KON", "VMFIN", "VAFIN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auf alle Zeiten guten Wert.", "tokens": ["Auf", "al\u00b7le", "Zei\u00b7ten", "gu\u00b7ten", "Wert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin ergraut, die alte Zeit ist abgelaufen,", "tokens": ["Ich", "bin", "er\u00b7graut", ",", "die", "al\u00b7te", "Zeit", "ist", "ab\u00b7ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Erb ist worden eitel Rauch.", "tokens": ["Mein", "Erb", "ist", "wor\u00b7den", "ei\u00b7tel", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VAPP", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00dfte, was ich hab und bin, mir selbst erkaufen,", "tokens": ["Ich", "mu\u00df\u00b7te", ",", "was", "ich", "hab", "und", "bin", ",", "mir", "selbst", "er\u00b7kau\u00b7fen", ","]