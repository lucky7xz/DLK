{"dta.poem.11615": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die Elemente.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Am schwarzen Berg da steht der Riese,", "tokens": ["Am", "schwar\u00b7zen", "Berg", "da", "steht", "der", "Rie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht hoch der Mond dar\u00fcber her;", "tokens": ["Steht", "hoch", "der", "Mond", "da\u00b7r\u00fc\u00b7ber", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wei\u00dfen Nebel auf der Wiese", "tokens": ["Die", "wei\u00b7\u00dfen", "Ne\u00b7bel", "auf", "der", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Wassergeister aus dem Meer:", "tokens": ["Sind", "Was\u00b7ser\u00b7geis\u00b7ter", "aus", "dem", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihrem Gebieter nachgezogen,", "tokens": ["Ih\u00b7rem", "Ge\u00b7bie\u00b7ter", "nach\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Vergiften sie die reine Nacht,", "tokens": ["Ver\u00b7gif\u00b7ten", "sie", "die", "rei\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus deren hochgeschwungnem Bogen", "tokens": ["Aus", "de\u00b7ren", "hoch\u00b7ge\u00b7schwung\u00b7nem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das volle Heer der Sterne lacht.", "tokens": ["Das", "vol\u00b7le", "Heer", "der", "Ster\u00b7ne", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Still schaut der Herr auf seine Geister,", "tokens": ["Still", "schaut", "der", "Herr", "auf", "sei\u00b7ne", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Faust am Herzen fest geballt;", "tokens": ["Die", "Faust", "am", "Her\u00b7zen", "fest", "ge\u00b7ballt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hei\u00dft der Elemente Meister,", "tokens": ["Er", "hei\u00dft", "der", "E\u00b7le\u00b7men\u00b7te", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Hei\u00dft Herr der t\u00f6dtlichen Gewalt;", "tokens": ["Hei\u00dft", "Herr", "der", "t\u00f6dt\u00b7li\u00b7chen", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Gott hat sie ihm \u00fcbergeben,", "tokens": ["Ein", "Gott", "hat", "sie", "ihm", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, ihm die schmerzenreichste Lust!", "tokens": ["Ach", ",", "ihm", "die", "schmer\u00b7zen\u00b7reichs\u00b7te", "Lust", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und namenlose Seufzer heben", "tokens": ["Und", "na\u00b7men\u00b7lo\u00b7se", "Seuf\u00b7zer", "he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die ehrne, g\u00f6ttergleiche Brust.", "tokens": ["Die", "ehr\u00b7ne", ",", "g\u00f6t\u00b7ter\u00b7glei\u00b7che", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Keule schwingt er jezt, die alte,", "tokens": ["Die", "Keu\u00b7le", "schwingt", "er", "jezt", ",", "die", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Schlage dr\u00f6hnt der Erde Rund,", "tokens": ["Vom", "Schla\u00b7ge", "dr\u00f6hnt", "der", "Er\u00b7de", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann springt durch die gewalt'ge Spalte", "tokens": ["Dann", "springt", "durch", "die", "ge\u00b7walt'\u00b7ge", "Spal\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Riesenk\u00f6rper in den Grund.", "tokens": ["Der", "Rie\u00b7sen\u00b7k\u00f6r\u00b7per", "in", "den", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die fest verschlossnen Feuer tauchen", "tokens": ["Die", "fest", "ver\u00b7schloss\u00b7nen", "Feu\u00b7er", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hoch aus uraltem Schlund herauf,", "tokens": ["Hoch", "aus", "ur\u00b7al\u00b7tem", "Schlund", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da fangen W\u00e4lder an zu rauchen", "tokens": ["Da", "fan\u00b7gen", "W\u00e4l\u00b7der", "an", "zu", "rau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und prasseln wild im Sturme auf.", "tokens": ["Und", "pras\u00b7seln", "wild", "im", "Stur\u00b7me", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er aber darf nicht still sich f\u00fchlen,", "tokens": ["Er", "a\u00b7ber", "darf", "nicht", "still", "sich", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PTKNEG", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschaulich im verborgnen Schacht,", "tokens": ["Be\u00b7schau\u00b7lich", "im", "ver\u00b7borg\u00b7nen", "Schacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Gold und Edelsteine k\u00fchlen", "tokens": ["Wo", "Gold", "und", "E\u00b7del\u00b7stei\u00b7ne", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hellen Augs der Elfe wacht:", "tokens": ["Und", "hel\u00b7len", "Augs", "der", "El\u00b7fe", "wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach einem unverr\u00fcckten Willen,", "tokens": ["Nach", "ei\u00b7nem", "un\u00b7ver\u00b7r\u00fcck\u00b7ten", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der bl\u00fcht in der Gestirne Flur,", "tokens": ["Der", "bl\u00fcht", "in", "der", "Ge\u00b7stir\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00df er die ew'gen Kr\u00e4fte stillen", "tokens": ["Mu\u00df", "er", "die", "ew'\u00b7gen", "Kr\u00e4f\u00b7te", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Lust und Schrecken der Natur.", "tokens": ["Mit", "Lust", "und", "Schre\u00b7cken", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Soll er den Flug von hundert Wettern", "tokens": ["Soll", "er", "den", "Flug", "von", "hun\u00b7dert", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Laut donnernd durcheinander ziehn,", "tokens": ["Laut", "don\u00b7nernd", "durch\u00b7ein\u00b7an\u00b7der", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Menschen H\u00fctte niederschmettern,", "tokens": ["Des", "Men\u00b7schen", "H\u00fct\u00b7te", "nie\u00b7der\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verderben auf das Meerschiff spr\u00fchn,", "tokens": ["Ver\u00b7der\u00b7ben", "auf", "das", "Meer\u00b7schiff", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da will das edle Herz zerrei\u00dfen,", "tokens": ["Da", "will", "das", "ed\u00b7le", "Herz", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da sieht er schrecklich sich allein:", "tokens": ["Da", "sieht", "er", "schreck\u00b7lich", "sich", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und doch kann er nicht w\u00fcrdig hei\u00dfen,", "tokens": ["Und", "doch", "kann", "er", "nicht", "w\u00fcr\u00b7dig", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Mit G\u00f6ttern ganz ein Gott zu seyn.", "tokens": ["Mit", "G\u00f6t\u00b7tern", "ganz", "ein", "Gott", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch aber blieb ihm ", "tokens": ["Noch", "a\u00b7ber", "blieb", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nachdem er Land und Meer bewegt,", "tokens": ["Nach\u00b7dem", "er", "Land", "und", "Meer", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er bei Nacht auf \u00f6der Haide", "tokens": ["Wenn", "er", "bei", "Nacht", "auf", "\u00f6\u00b7der", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sehnsucht seiner Seele pflegt.", "tokens": ["Die", "Sehn\u00b7sucht", "sei\u00b7ner", "See\u00b7le", "pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da h\u00e4ngen ungeheure Ketten", "tokens": ["Da", "h\u00e4n\u00b7gen", "un\u00b7ge\u00b7heu\u00b7re", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus tiefstem Wolkenraum herab,", "tokens": ["Aus", "tiefs\u00b7tem", "Wol\u00b7ken\u00b7raum", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dran er, als m\u00fc\u00dften sie ihn retten,", "tokens": ["Dran", "er", ",", "als", "m\u00fc\u00df\u00b7ten", "sie", "ihn", "ret\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "KOUS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich schwingt zum Himmel auf und ab.", "tokens": ["Sich", "schwingt", "zum", "Him\u00b7mel", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort weilen rosige Gestalten", "tokens": ["Dort", "wei\u00b7len", "ro\u00b7si\u00b7ge", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In heitern H\u00f6hen, himmlisch klar,", "tokens": ["In", "hei\u00b7tern", "H\u00f6\u00b7hen", ",", "himm\u00b7lisch", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fest an goldnen Seilen halten", "tokens": ["Und", "fest", "an", "gold\u00b7nen", "Sei\u00b7len", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schwesterlich das Kettenpaar;", "tokens": ["Sie", "schwes\u00b7ter\u00b7lich", "das", "Ket\u00b7ten\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie liegen \u00e4ngstlich auf den Knieen", "tokens": ["Sie", "lie\u00b7gen", "\u00e4ngst\u00b7lich", "auf", "den", "Kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Und sehen sanft zum wilden Spiel,", "tokens": ["Und", "se\u00b7hen", "sanft", "zum", "wil\u00b7den", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie sie im Gebete gl\u00fchen,", "tokens": ["Und", "wie", "sie", "im", "Ge\u00b7be\u00b7te", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "L\u00f6st, wie ein Traum, sich sein Gef\u00fchl.", "tokens": ["L\u00f6st", ",", "wie", "ein", "Traum", ",", "sich", "sein", "Ge\u00b7f\u00fchl", "."