{"textgrid.poem.48264": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Swend Gabelbart", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Swend ", "tokens": ["Swend"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Er siegreich das Zepter von D\u00e4nemark h\u00e4lt,", "tokens": ["Er", "sieg\u00b7reich", "das", "Zep\u00b7ter", "von", "D\u00e4\u00b7ne\u00b7mark", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Seine Schiffe von Insel zu Insel ziehn,", "tokens": ["Sei\u00b7ne", "Schif\u00b7fe", "von", "In\u00b7sel", "zu", "In\u00b7sel", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Unterworfen ist Wendland und Julin,", "tokens": ["Un\u00b7ter\u00b7wor\u00b7fen", "ist", "Wend\u00b7land", "und", "Ju\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NE", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und nun gen Westen, \u00fcber das Meer", "tokens": ["Und", "nun", "gen", "Wes\u00b7ten", ",", "\u00fc\u00b7ber", "das", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Jagt er, der Schrecken vor ihm her,", "tokens": ["Jagt", "er", ",", "der", "Schre\u00b7cken", "vor", "ihm", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "In die Themsem\u00fcndung f\u00e4hrt er ein,", "tokens": ["In", "die", "Them\u00b7se\u00b7m\u00fcn\u00b7dung", "f\u00e4hrt", "er", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ganz London ist ein Feuerschein.", "tokens": ["Ganz", "Lon\u00b7don", "ist", "ein", "Feu\u00b7er\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und nun zu Ro\u00df und nun zu Hauf", "tokens": ["Und", "nun", "zu", "Ro\u00df", "und", "nun", "zu", "Hauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Essex und Norfolk zieht er hinauf,", "tokens": ["Es\u00b7sex", "und", "Nor\u00b7folk", "zieht", "er", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Und mit Zechgenossen und Kumpanei", "tokens": ["Und", "mit", "Zech\u00b7ge\u00b7nos\u00b7sen", "und", "Kum\u00b7pa\u00b7nei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Reitet er ein in Sankt Edmunds-Abtei.", "tokens": ["Rei\u00b7tet", "er", "ein", "in", "Sankt", "Ed\u00b7munds\u00b7Ab\u00b7tei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPR", "VVFIN", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Da sitzen sie nun die Hall' entlang,", "tokens": ["Da", "sit\u00b7zen", "sie", "nun", "die", "Hall'", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus der Kirche klingt frommer M\u00f6nche Gesang.", "tokens": ["Aus", "der", "Kir\u00b7che", "klingt", "from\u00b7mer", "M\u00f6n\u00b7che", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwas soll das Gepl\u00e4rr uns?\u00ab Und in die Kapell'n", "tokens": ["\u00bb", "was", "soll", "das", "Ge\u00b7pl\u00e4rr", "uns", "?", "\u00ab", "Und", "in", "die", "Ka\u00b7pell'n"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "PPER", "$.", "$(", "KON", "APPR", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Swend Gabelbart l\u00e4\u00dft seinen Marstall er stell'n,", "tokens": ["Swend", "Ga\u00b7bel\u00b7bart", "l\u00e4\u00dft", "sei\u00b7nen", "Mar\u00b7stall", "er", "stell'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Er mag sie nicht h\u00f6ren, die Litanein,", "tokens": ["Er", "mag", "sie", "nicht", "h\u00f6\u00b7ren", ",", "die", "Li\u00b7tan\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "(l\u00e4rm und Gewieher, so soll es sein),", "tokens": ["(", "l\u00e4rm", "und", "Ge\u00b7wie\u00b7her", ",", "so", "soll", "es", "sein", ")", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "KON", "NN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$(", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "In der Rosse Gestampf erlischt der Chor,", "tokens": ["In", "der", "Ros\u00b7se", "Ge\u00b7stampf", "er\u00b7lischt", "der", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Swend aber lacht: \u00bb", "tokens": ["Swend", "a\u00b7ber", "lacht", ":", "\u00bb"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Sch\u00fcttet Hafer auf Sankt Edmunds Truh,", "tokens": ["Sch\u00fct\u00b7tet", "Ha\u00b7fer", "auf", "Sankt", "Ed\u00b7munds", "Truh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Er selber nickt euch den Segen dazu.\u00ab", "tokens": ["Er", "sel\u00b7ber", "nickt", "euch", "den", "Se\u00b7gen", "da\u00b7zu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sankt Edmund, an schwarz-goldener Wand,", "tokens": ["Sankt", "Ed\u00b7mund", ",", "an", "schwa\u00b7rz\u00b7gol\u00b7de\u00b7ner", "Wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hall' aufw\u00e4rts in seiner Nische stand.", "tokens": ["Hall'", "auf\u00b7w\u00e4rts", "in", "sei\u00b7ner", "Ni\u00b7sche", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Einst war er K\u00f6nig. Ein mattes Licht", "tokens": ["Einst", "war", "er", "K\u00f6\u00b7nig", ".", "Ein", "mat\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umspielt ihn flackernd; Swend aber spricht:", "tokens": ["Um\u00b7spielt", "ihn", "fla\u00b7ckernd", ";", "Swend", "a\u00b7ber", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$.", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u00bbsankt Edmund, du schufst hier Kirch' und Abtei,", "tokens": ["\u00bb", "sankt", "Ed\u00b7mund", ",", "du", "schufst", "hier", "Kirch'", "und", "Ab\u00b7tei", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dein Land, es ging verloren dabei,", "tokens": ["Dein", "Land", ",", "es", "ging", "ver\u00b7lo\u00b7ren", "da\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "VVPP", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Nun stehst du da, tr\u00e4gst m\u00f6nchisch Gewand,", "tokens": ["Nun", "stehst", "du", "da", ",", "tr\u00e4gst", "m\u00f6n\u00b7chisch", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "H\u00e4ltst wie zum Spott ein Schwert in der Hand,", "tokens": ["H\u00e4ltst", "wie", "zum", "Spott", "ein", "Schwert", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Und doch, o K\u00f6nig, warst K\u00f6nig du je,", "tokens": ["Und", "doch", ",", "o", "K\u00f6\u00b7nig", ",", "warst", "K\u00f6\u00b7nig", "du", "je", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NN", "$,", "VAFIN", "NN", "PPER", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Du t\u00e4test jetzt ab deine Todesruh", "tokens": ["Du", "t\u00e4\u00b7test", "jetzt", "ab", "dei\u00b7ne", "To\u00b7des\u00b7ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und k\u00e4mst als ein R\u00e4cher auf mich zu,", "tokens": ["Und", "k\u00e4mst", "als", "ein", "R\u00e4\u00b7cher", "auf", "mich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und ob zerbrochen auch dein Schwert,", "tokens": ["Und", "ob", "zer\u00b7bro\u00b7chen", "auch", "dein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVPP", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Es w\u00e4re dir doch des Kampfes wert,", "tokens": ["Es", "w\u00e4\u00b7re", "dir", "doch", "des", "Kamp\u00b7fes", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Aus dieser Hall' hier, aus diesem Haus,", "tokens": ["Aus", "die\u00b7ser", "Hall'", "hier", ",", "aus", "die\u00b7sem", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Auch mit ", "tokens": ["Auch", "mit"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Ein M\u00f6nchsbild warst du bei Lebzeit schon.\u00ab", "tokens": ["Ein", "M\u00f6nchs\u00b7bild", "warst", "du", "bei", "Leb\u00b7zeit", "schon", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "ADV", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Swend Gabelbart schwieg. Im Kreise rundum", "tokens": ["Swend", "Ga\u00b7bel\u00b7bart", "schwieg", ".", "Im", "Krei\u00b7se", "run\u00b7dum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "$.", "APPRART", "NN", "NE"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ward es so still und ward es so stumm,", "tokens": ["Ward", "es", "so", "still", "und", "ward", "es", "so", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In der Nische das Licht immer d\u00fcsterer brennt.", "tokens": ["In", "der", "Ni\u00b7sche", "das", "Licht", "im\u00b7mer", "d\u00fcs\u00b7te\u00b7rer", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Da steigt es herab vom Postament,", "tokens": ["Da", "steigt", "es", "her\u00b7ab", "vom", "Pos\u00b7ta\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und tapp und tapp, in steinerem Schuh", "tokens": ["Und", "tapp", "und", "tapp", ",", "in", "stei\u00b7ne\u00b7rem", "Schuh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Auf Swend Gabelbart schreitet Sankt Edmund zu,", "tokens": ["Auf", "Swend", "Ga\u00b7bel\u00b7bart", "schrei\u00b7tet", "Sankt", "Ed\u00b7mund", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "+-+-++--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Vor streckt er sein zerbrochen Schwert:", "tokens": ["Vor", "streckt", "er", "sein", "zer\u00b7bro\u00b7chen", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbnun, Swend, la\u00df sehn, wer besser bewehrt.\u00ab", "tokens": ["\u00bb", "nun", ",", "Swend", ",", "la\u00df", "sehn", ",", "wer", "bes\u00b7ser", "be\u00b7wehrt", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "VVPP", "$,", "VVIMP", "VVINF", "$,", "PWS", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Aus des K\u00f6nigs Aug' ein Entsetzen spricht,", "tokens": ["Aus", "des", "K\u00f6\u00b7nigs", "Aug'", "ein", "Ent\u00b7set\u00b7zen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Er schl\u00e4gt nach dem Schwert, ", "tokens": ["Er", "schl\u00e4gt", "nach", "dem", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Das stumpfe Schwert, es traf ihn gut,", "tokens": ["Das", "stump\u00b7fe", "Schwert", ",", "es", "traf", "ihn", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iam