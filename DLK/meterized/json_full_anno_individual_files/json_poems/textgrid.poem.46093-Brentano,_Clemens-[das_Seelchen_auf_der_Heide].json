", "word", "word"], "pos": ["APPR", "NN", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "In Mond und Sternenschein.", "tokens": ["In", "Mond", "und", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Todwund lag ich zum Sterben,", "tokens": ["Tod\u00b7wund", "lag", "ich", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der Seele Kleid zu f\u00e4rben", "tokens": ["Der", "See\u00b7le", "Kleid", "zu", "f\u00e4r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Mit roter Farbe Schein.", "tokens": ["Mit", "ro\u00b7ter", "Far\u00b7be", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ich trug es ohn Verweilen", "tokens": ["Ich", "trug", "es", "ohn", "Ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Hin viele, viele Meilen,", "tokens": ["Hin", "vie\u00b7le", ",", "vie\u00b7le", "Mei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Da war mein Tuch zu klein,", "tokens": ["Da", "war", "mein", "Tuch", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+