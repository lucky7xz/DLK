"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Schifflein ist geschwebet", "tokens": ["Mein", "Schif\u00b7flein", "ist", "ge\u00b7schwe\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "In st\u00e4ter Not und Pein.", "tokens": ["In", "st\u00e4\u00b7ter", "Not", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mit Tr\u00e4nen ich's erweichte,", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "ich's", "er\u00b7weich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mit Tr\u00e4nen ich es bleichte", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "ich", "es", "bleich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "In Mond und Sternenschein.", "tokens": ["In", "Mond", "und", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Todwund lag ich zum Sterben,", "tokens": ["Tod\u00b7wund", "lag", "ich", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der Seele Kleid zu f\u00e4rben", "tokens": ["Der", "See\u00b7le", "Kleid", "zu", "f\u00e4r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Mit roter Farbe Schein.", "tokens": ["Mit", "ro\u00b7ter", "Far\u00b7be", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ich trug es ohn Verweilen", "tokens": ["Ich", "trug", "es", "ohn", "Ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Hin viele, viele Meilen,", "tokens": ["Hin", "vie\u00b7le", ",", "vie\u00b7le", "Mei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Da war mein Tuch zu klein,", "tokens": ["Da", "war", "mein", "Tuch", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Das Seelchen zu bedecken,", "tokens": ["Das", "Seel\u00b7chen", "zu", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Da zuckt an allen Ecken", "tokens": ["Da", "zuckt", "an", "al\u00b7len", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Heraus das Fl\u00e4mmelein,", "tokens": ["He\u00b7raus", "das", "Fl\u00e4m\u00b7mel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Und irret auf