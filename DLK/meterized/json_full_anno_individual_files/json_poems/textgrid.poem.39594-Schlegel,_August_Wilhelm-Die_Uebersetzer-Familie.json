{"textgrid.poem.39594": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Uebersetzer-Familie", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei S\u00f6hne zeugte Vo\u00df, Heinrich Johann, der gro\u00dfe;", "tokens": ["Drei", "S\u00f6h\u00b7ne", "zeug\u00b7te", "Vo\u00df", ",", "Hein\u00b7rich", "Jo\u00b7hann", ",", "der", "gro\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "NE", "$,", "NE", "NE", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drei Uebersetzer auch, bereits im Mutterschoo\u00dfe.", "tokens": ["Drei", "Ue\u00b7ber\u00b7set\u00b7zer", "auch", ",", "be\u00b7reits", "im", "Mut\u00b7ter\u00b7schoo\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erst Heinrich, Abraham, dann Adam noch zuletzt:", "tokens": ["Erst", "Hein\u00b7rich", ",", "Ab\u00b7ra\u00b7ham", ",", "dann", "A\u00b7dam", "noch", "zu\u00b7letzt", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "NE", "$,", "ADV", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Selbvierte haben die den Shakspeare \u00fcbersetzt.", "tokens": ["Selb\u00b7vier\u00b7te", "ha\u00b7ben", "die", "den", "Shak\u00b7spea\u00b7re", "\u00fc\u00b7bers\u00b7etzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-