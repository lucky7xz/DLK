{"dta.poem.3827": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Schreiben,  \n Bey Ableben eines Berg-Steigers verfertiget.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Schicht! Schicht!", "tokens": ["Schicht", "!", "Schicht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ey wie? das schickt sich nicht,", "tokens": ["Ey", "wie", "?", "das", "schickt", "sich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "$.", "PDS", "VVFIN", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Bergmann von so jungen Jahren", "tokens": ["Ein", "Berg\u00b7mann", "von", "so", "jun\u00b7gen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will weiter keinen Gang befahren.", "tokens": ["Will", "wei\u00b7ter", "kei\u00b7nen", "Gang", "be\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wilst du nicht mehr den Schachthut drehn?", "tokens": ["Wilst", "du", "nicht", "mehr", "den", "Schacht\u00b7hut", "drehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll man dich nicht mehr fahren sehn?", "tokens": ["Soll", "man", "dich", "nicht", "mehr", "fah\u00b7ren", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "PTKNEG", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist denn dein Hoffen und Verlangen,", "tokens": ["Ist", "denn", "dein", "Hof\u00b7fen", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist denn dein Gl\u00fcck auf! gantz vergangen?", "tokens": ["Ist", "denn", "dein", "Gl\u00fcck", "auf", "!", "gantz", "ver\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$.", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was ruffst du: ", "tokens": ["Was", "ruffst", "du", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Das schickt sich nicht.", "tokens": ["Das", "schickt", "sich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schicht! Schicht!", "tokens": ["Schicht", "!", "Schicht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wie? da\u00df dein Gruben-Licht", "tokens": ["Wie", "?", "da\u00df", "dein", "Gru\u00b7ben\u00b7Licht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bereits so gar verdunckelt scheinet,", "tokens": ["Be\u00b7reits", "so", "gar", "ver\u00b7dun\u00b7ckelt", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das h\u00e4tt ich warrlich nicht vermeinet,", "tokens": ["Das", "h\u00e4tt", "ich", "warr\u00b7lich", "nicht", "ver\u00b7mei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da es so balde dunckel brennt,", "tokens": ["Da", "es", "so", "bal\u00b7de", "dun\u00b7ckel", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So, da\u00df man kein Gestein erkennt.", "tokens": ["So", ",", "da\u00df", "man", "kein", "Ge\u00b7stein", "er\u00b7kennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df es doch nicht den Schein verliehren,", "tokens": ["La\u00df", "es", "doch", "nicht", "den", "Schein", "ver\u00b7lieh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O! suche solches anzusch\u00fcren.", "tokens": ["O", "!", "su\u00b7che", "sol\u00b7ches", "an\u00b7zu\u00b7sch\u00fc\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was ruffst du ", "tokens": ["Was", "ruffst", "du"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "O sch\u00fcre doch dein Gruben-Licht.", "tokens": ["O", "sch\u00fc\u00b7re", "doch", "dein", "Gru\u00b7ben\u00b7Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schicht! Schicht!", "tokens": ["Schicht", "!", "Schicht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Da es an Krafft gebricht,", "tokens": ["Da", "es", "an", "Krafft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So leg ich Stahl und Eisen nieder,", "tokens": ["So", "leg", "ich", "Stahl", "und", "Ei\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja ich ergreiff sie niemahls wieder,", "tokens": ["Ja", "ich", "er\u00b7greiff", "sie", "nie\u00b7mahls", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe den Beschlu\u00df gemacht:", "tokens": ["Ich", "ha\u00b7be", "den", "Be\u00b7schlu\u00df", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich meide k\u00fcnfftig Gang und Schacht,", "tokens": ["Ich", "mei\u00b7de", "k\u00fcnff\u00b7tig", "Gang", "und", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich suche niemahls mehr zu bauen,", "tokens": ["Ich", "su\u00b7che", "nie\u00b7mahls", "mehr", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will in kein Gestein mehr hauen,", "tokens": ["Ich", "will", "in", "kein", "Ge\u00b7stein", "mehr", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Drum ruff ich ", "tokens": ["Drum", "ruff", "ich"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Da es an Krafft gebricht.", "tokens": ["Da", "es", "an", "Krafft", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schicht! Schicht!", "tokens": ["Schicht", "!", "Schicht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mein Berg-F\u00fcrst, JEsus, spricht:", "tokens": ["Mein", "Ber\u00b7gF\u00fcrst", ",", "Je\u00b7sus", ",", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm nun zum himmlischen Berg-Reyhen,", "tokens": ["Komm", "nun", "zum", "himm\u00b7li\u00b7schen", "Ber\u00b7gRey\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da soll dir Ruhe angedeyhen,", "tokens": ["Da", "soll", "dir", "Ru\u00b7he", "an\u00b7ge\u00b7dey\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wohlan, verla\u00df dein irrdisch Hau\u00df,", "tokens": ["Wo\u00b7hlan", ",", "ver\u00b7la\u00df", "dein", "irr\u00b7disch", "Hau\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nunmehro pocht dein Seiger aus.", "tokens": ["Nun\u00b7meh\u00b7ro", "pocht", "dein", "Sei\u00b7ger", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Leib f\u00e4hrt in die Teuff der Erden,", "tokens": ["Dein", "Leib", "f\u00e4hrt", "in", "die", "Teuff", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du solst ein Himmels-B\u00fcrger werden.", "tokens": ["Du", "solst", "ein", "Him\u00b7mels\u00b7B\u00fcr\u00b7ger", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Drum ruff ich ", "tokens": ["Drum", "ruff", "ich"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Weil es mein JEsus spricht.", "tokens": ["Weil", "es", "mein", "Je\u00b7sus", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}