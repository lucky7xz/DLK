{"dta.poem.18974": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1.  \n  Vorrede vnd bitt an Seine  \n Liebste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ich dicht/ Ich sag/ Ich sing: Ach nein/ Ich seuftz/", "tokens": ["Ich", "dicht", "/", "Ich", "sag", "/", "Ich", "sing", ":", "Ach", "nein", "/", "Ich", "seuftz", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$.", "NN", "PTKANT", "$(", "PPER", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "schrey/ klag/", "tokens": ["schrey", "/", "klag", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$(", "VVFIN", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.3": {"text": "Die lieb/ dz layd/ damit mein junges hertz gestritte\u0304/", "tokens": ["Die", "lieb", "/", "dz", "layd", "/", "da\u00b7mit", "mein", "jun\u00b7ges", "hertz", "ge\u00b7strit\u00b7t\u0113", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "KOUS", "NN", "$(", "KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verlierend allen trost vnd hofnung mit dem tag/", "tokens": ["Ver\u00b7lie\u00b7rend", "al\u00b7len", "trost", "vnd", "hof\u00b7nung", "mit", "dem", "tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "KON", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verwundet durch vnd durch endlich den tod er-", "tokens": ["Ver\u00b7wun\u00b7det", "durch", "vnd", "durch", "end\u00b7lich", "den", "tod", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "KON", "APPR", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "litten.", "tokens": ["lit\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Kein soldat in der schlacht vnd gr\u00f6sten niderlag", "tokens": ["Kein", "sol\u00b7dat", "in", "der", "schlacht", "vnd", "gr\u00f6s\u00b7ten", "ni\u00b7der\u00b7lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War jemahls/ als mein hertz/ zerhacket vnd zer-", "tokens": ["War", "je\u00b7mahls", "/", "als", "mein", "hertz", "/", "zer\u00b7ha\u00b7cket", "vnd", "zer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "$(", "KOKOM", "PPOSAT", "NN", "$(", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schnitten;", "tokens": ["schnit\u00b7ten", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Vnd bittend vmb quartier kont ich weder vertrag/", "tokens": ["Vnd", "bit\u00b7tend", "vmb", "quar\u00b7tier", "kont", "ich", "we\u00b7der", "ver\u00b7trag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "NE", "VMFIN", "PPER", "KON", "VVIMP", "$("], "meter": "-+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Noch meiner feindin gnad erbeutten noch erbitten.", "tokens": ["Noch", "mei\u00b7ner", "fein\u00b7din", "gnad", "er\u00b7beut\u00b7ten", "noch", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O grewliche Sch\u00f6nheit/ die mit ernst oder schertz/", "tokens": ["O", "grew\u00b7li\u00b7che", "Sch\u00f6n\u00b7heit", "/", "die", "mit", "ernst", "o\u00b7der", "schertz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ART", "APPR", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach ewerm aignen lust/ den sehlen widerstrebet/", "tokens": ["Nach", "e\u00b7werm", "aig\u00b7nen", "lust", "/", "den", "seh\u00b7len", "wi\u00b7der\u00b7stre\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "ART", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erken\u0303et doch wie gro\u00df ewer stoltz vn\u0303 mein schmertz!", "tokens": ["Er\u00b7ke\u00f1et", "doch", "wie", "gro\u00df", "e\u00b7wer", "stoltz", "v\u00f1", "mein", "schmertz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "O die Jhr/ wan jhr wolt/ den tod/ das leben/ gebet/", "tokens": ["O", "die", "Ihr", "/", "wan", "jhr", "wolt", "/", "den", "tod", "/", "das", "le\u00b7ben", "/", "ge\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "PPOSAT", "$(", "PWAV", "PPER", "VMFIN", "$(", "ART", "NN", "$(", "PDS", "VVINF", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verleyhet das durch Euch/ weil ja durch Euch", "tokens": ["Ver\u00b7ley\u00b7het", "das", "durch", "Euch", "/", "weil", "ja", "durch", "Euch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "PPER", "$(", "KOUS", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mein hertz", "tokens": ["mein", "hertz"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Get\u00f6dtet/ mein Gesang hingegen werd belebet!", "tokens": ["Ge\u00b7t\u00f6d\u00b7tet", "/", "mein", "Ge\u00b7sang", "hin\u00b7ge\u00b7gen", "werd", "be\u00b7le\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "PPOSAT", "NN", "ADV", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}