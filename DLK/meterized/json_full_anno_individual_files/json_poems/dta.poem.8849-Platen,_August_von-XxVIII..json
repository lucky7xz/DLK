{"dta.poem.8849": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Denn liebend schlingt mein Arm um deine Lende sich!", "tokens": ["Denn", "lie\u00b7bend", "schlingt", "mein", "Arm", "um", "dei\u00b7ne", "Len\u00b7de", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo f\u00e4nde Muth das Herz, sich karg zur\u00fcckzuziehn,", "tokens": ["Wo", "f\u00e4n\u00b7de", "Muth", "das", "Herz", ",", "sich", "karg", "zu\u00b7r\u00fcck\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "$,", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es gebe ganz sich hin, und es verschwende sich!", "tokens": ["Es", "ge\u00b7be", "ganz", "sich", "hin", ",", "und", "es", "ver\u00b7schwen\u00b7de", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Lenz der Liebe tritt hervor, und das Gesetz", "tokens": ["Der", "Lenz", "der", "Lie\u00b7be", "tritt", "her\u00b7vor", ",", "und", "das", "Ge\u00b7setz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es neigt, dem Winter gleich, zu seinem Ende sich:", "tokens": ["Es", "neigt", ",", "dem", "Win\u00b7ter", "gleich", ",", "zu", "sei\u00b7nem", "En\u00b7de", "sich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Eine bete dich, wie seine Heil'gen, an,", "tokens": ["Der", "Ei\u00b7ne", "be\u00b7te", "dich", ",", "wie", "sei\u00b7ne", "Heil'\u00b7gen", ",", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Andre kniee fromm vor eine Blende sich!", "tokens": ["Der", "And\u00b7re", "kni\u00b7ee", "fromm", "vor", "ei\u00b7ne", "Blen\u00b7de", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "APPR", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem Strengen g\u00f6nnen wir, zu werden was er soll,", "tokens": ["Dem", "Stren\u00b7gen", "g\u00f6n\u00b7nen", "wir", ",", "zu", "wer\u00b7den", "was", "er", "soll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PTKZU", "VAINF", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch auch des Freyen Geist, o Freund, vollende sich!", "tokens": ["Doch", "auch", "des", "Frey\u00b7en", "Geist", ",", "o", "Freund", ",", "voll\u00b7en\u00b7de", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$,", "FM", "NN", "$,", "ADJA", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}