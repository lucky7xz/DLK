{"textgrid.poem.39191": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir ist zuweilen so schwer und tr\u00fcb,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist zuweilen so schwer und tr\u00fcb,", "tokens": ["Mir", "ist", "zu\u00b7wei\u00b7len", "so", "schwer", "und", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So tr\u00fcbe und so schwer,", "tokens": ["So", "tr\u00fc\u00b7be", "und", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist, als h\u00e4tte mich niemand lieb,", "tokens": ["Mir", "ist", ",", "als", "h\u00e4t\u00b7te", "mich", "nie\u00b7mand", "lieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ja, selbst die Liebe nicht mehr.", "tokens": ["Ja", ",", "selbst", "die", "Lie\u00b7be", "nicht", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Mein Wein ist ohne Glut und Duft,", "tokens": ["Mein", "Wein", "ist", "oh\u00b7ne", "Glut", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+