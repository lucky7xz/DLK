{"dta.poem.6070": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir seeglen, als im Schiff, auf unsrer Erd' herum", "tokens": ["Wir", "seeg\u00b7len", ",", "als", "im", "Schiff", ",", "auf", "uns\u00b7rer", "Erd'", "he\u00b7rum"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andern Jrr-Geftirn, als Schiffe, gleicher Weise,", "tokens": ["Die", "an\u00b7dern", "Jrr\u00b7Geftirn", ",", "als", "Schif\u00b7fe", ",", "glei\u00b7cher", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So durch den Wind gef\u00fchrt, vollf\u00fchren ihre Reise", "tokens": ["So", "durch", "den", "Wind", "ge\u00b7f\u00fchrt", ",", "voll\u00b7f\u00fch\u00b7ren", "ih\u00b7re", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jm d\u00fcnnen Reich der Lufft. Ein jeder eilt", "tokens": ["Jm", "d\u00fcn\u00b7nen", "Reich", "der", "Lufft", ".", "Ein", "je\u00b7der", "eilt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$.", "ART", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In einem gantz besonderm Creise.", "tokens": ["In", "ei\u00b7nem", "gantz", "be\u00b7son\u00b7derm", "Crei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sind nach einerley Gesetz zu gehn bem\u00fcht.", "tokens": ["Sie", "sind", "nach", "ei\u00b7ner\u00b7ley", "Ge\u00b7setz", "zu", "gehn", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch wie ein Reisender, wenn er die Fluhten theilt,", "tokens": ["Doch", "wie", "ein", "Rei\u00b7sen\u00b7der", ",", "wenn", "er", "die", "Fluh\u00b7ten", "theilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In Ruhe glaubt zu seyn, und alles lauffen siehet;", "tokens": ["In", "Ru\u00b7he", "glaubt", "zu", "seyn", ",", "und", "al\u00b7les", "lauf\u00b7fen", "sie\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKZU", "VAINF", "$,", "KON", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So sehn wir auch das Lauffen dieser Sterne.", "tokens": ["So", "sehn", "wir", "auch", "das", "Lauf\u00b7fen", "die\u00b7ser", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}