{"textgrid.poem.40055": {"metadata": {"author": {"name": "Hoyers, Anna Ovena", "birth": "N.A.", "death": "N.A."}, "title": "1L: Leve Peer Niel\u00dfon latet my weten/", "genre": "verse", "period": "N.A.", "pub_year": 1619, "urn": "N.A.", "language": ["de:0.71", "nl:0.14", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leve Peer Niel\u00dfon latet my weten/", "tokens": ["Le\u00b7ve", "Peer", "Niel\u00b7\u00dfon", "la\u00b7tet", "my", "we\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "NE", "VVFIN", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "M\u00f6g' gy so gern Blickhems Botter eten?", "tokens": ["M\u00f6g'", "gy", "so", "gern", "Blick\u00b7hems", "Bot\u00b7ter", "e\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "ADV", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wille gy hebben noch ein halff punt mehr/", "tokens": ["Wil\u00b7le", "gy", "heb\u00b7ben", "noch", "ein", "halff", "punt", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "VVFIN", "ADV", "ART", "NN", "ADJD", "ADV", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "So sendet den Linnen B\u00fcdel wedder her/", "tokens": ["So", "sen\u00b7det", "den", "Lin\u00b7nen", "B\u00fc\u00b7del", "wed\u00b7der", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "ADV", "$("], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Edder tho Juwes Organisten Fruw/", "tokens": ["Ed\u00b7der", "tho", "Ju\u00b7wes", "Or\u00b7ga\u00b7nis\u00b7ten", "Fruw", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Ick gaff er de Botter darin/ nich Juw:", "tokens": ["Ick", "gaff", "er", "de", "Bot\u00b7ter", "da\u00b7rin", "/", "nich", "Juw", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "NN", "PAV", "$(", "PTKNEG", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Doch wiel jy so verleckert darup wehrn", "tokens": ["Doch", "wiel", "jy", "so", "ver\u00b7le\u00b7ckert", "da\u00b7rup", "wehrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "PAV", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "G\u00fcnne ich se Juw ock van herten gern.", "tokens": ["G\u00fcn\u00b7ne", "ich", "se", "Juw", "ock", "van", "her\u00b7ten", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "NE", "NE", "NE", "VVFIN", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Leet ist my dat dar nich ein gantz punt was.", "tokens": ["Leet", "ist", "my", "dat", "dar", "nich", "ein", "gantz", "punt", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "NE", "PTKVZ", "PTKNEG", "ART", "ADV", "ADJD", "PWS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wo recht idel wol komt et Juw tho pa\u00df/", "tokens": ["Wo", "recht", "i\u00b7del", "wol", "komt", "et", "Juw", "tho", "pa\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "VVFIN", "NE", "NE", "NE", "VVFIN", "$("], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "Dat gy so fry van uns m\u00f6get halen/", "tokens": ["Dat", "gy", "so", "fry", "van", "uns", "m\u00f6\u00b7get", "ha\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "NE", "NE", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Andern m\u00f6tt gy solck wahr betalen?", "tokens": ["An\u00b7dern", "m\u00f6tt", "gy", "solck", "wahr", "be\u00b7ta\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "FM", "FM", "FM", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "De Wedwen/ Weysen un Frembden dwingen/", "tokens": ["De", "Wed\u00b7wen", "/", "Wey\u00b7sen", "un", "Fremb\u00b7den", "dwin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "FM", "FM", "NN", "VVINF", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Ehnen affnehmen/ nist wedder bringen/", "tokens": ["Eh\u00b7nen", "aff\u00b7neh\u00b7men", "/", "nist", "wed\u00b7der", "brin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "VVFIN", "KON", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "I\u00df sulckes der Westerwycker wise?", "tokens": ["I\u00df", "sul\u00b7ckes", "der", "Wes\u00b7ter\u00b7wy\u00b7cker", "wi\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "So i\u00df et ein arth de ick nich prise.", "tokens": ["So", "i\u00df", "et", "ein", "ar\u00b7th", "de", "ick", "nich", "pri\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Gy hebben den Breef mit underschreven", "tokens": ["Gy", "heb\u00b7ben", "den", "Breef", "mit", "un\u00b7der\u00b7schre\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Den my Hinrich Han\u00dfen hefft gegeven/", "tokens": ["Den", "my", "Hin\u00b7rich", "Han\u00b7\u00dfen", "hefft", "ge\u00b7ge\u00b7ven", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "NN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Van wegen des wolgebarnen Herren/", "tokens": ["Van", "we\u00b7gen", "des", "wol\u00b7ge\u00b7bar\u00b7nen", "Her\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Dat my nemand schall molesteren.", "tokens": ["Dat", "my", "ne\u00b7mand", "schall", "mo\u00b7les\u00b7te\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ick kan bewisen mit Juw egen hant;", "tokens": ["Ick", "kan", "be\u00b7wi\u00b7sen", "mit", "Juw", "e\u00b7gen", "hant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Wo gy wort holden/ dat i\u00df Juw bekant.", "tokens": ["Wo", "gy", "wort", "hol\u00b7den", "/", "dat", "i\u00df", "Juw", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVINF", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Doch wat gy dhon/ dat moth syn wolgedahn;", "tokens": ["Doch", "wat", "gy", "dhon", "/", "dat", "moth", "syn", "wol\u00b7ge\u00b7dahn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ick wilt ock nu so darby laten stahn.", "tokens": ["Ick", "wilt", "ock", "nu", "so", "dar\u00b7by", "la\u00b7ten", "stahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "ADV", "ADV", "PAV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Gott lath Juw de Botter wol bekamen/", "tokens": ["Gott", "lath", "Juw", "de", "Bot\u00b7ter", "wol", "be\u00b7ka\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "NE", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Etet Brot tho/ denn dat h\u00f6rt thosamen.", "tokens": ["E\u00b7tet", "Brot", "tho", "/", "denn", "dat", "h\u00f6rt", "tho\u00b7sa\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "$(", "KON", "PAV", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Botter schlicken ohn Brot/ i\u00df nicht gesunt/", "tokens": ["Bot\u00b7ter", "schli\u00b7cken", "ohn", "Brot", "/", "i\u00df", "nicht", "ge\u00b7sunt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$(", "VVFIN", "PTKNEG", "VVPP", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Dat sach ick lest an minen olden Hunt;", "tokens": ["Dat", "sach", "ick", "lest", "an", "mi\u00b7nen", "ol\u00b7den", "Hunt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "De nam ock ein st\u00fcck Botter van der banck/", "tokens": ["De", "nam", "ock", "ein", "st\u00fcck", "Bot\u00b7ter", "van", "der", "banck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "ART", "ADJA", "NN", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Vnd fratt ohn Brot/ de arm schelm ward so kranck.", "tokens": ["Vnd", "fratt", "ohn", "Brot", "/", "de", "arm", "schelm", "ward", "so", "kranck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$(", "NE", "ADJD", "ADJD", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wiel nu ein Hunt dit nicht kan verdragen/", "tokens": ["Wiel", "nu", "ein", "Hunt", "dit", "nicht", "kan", "ver\u00b7dra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NE", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "De doch sonst hefft einen starcken magen/", "tokens": ["De", "doch", "sonst", "hefft", "ei\u00b7nen", "star\u00b7cken", "ma\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Wat scholde denn ein B\u00f6rger edder Buhr/", "tokens": ["Wat", "schol\u00b7de", "denn", "ein", "B\u00f6r\u00b7ger", "ed\u00b7der", "Buhr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Al\u00df gy/ de nich so starck i\u00df van natur?", "tokens": ["Al\u00df", "gy", "/", "de", "nich", "so", "starck", "i\u00df", "van", "na\u00b7tur", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "NE", "PTKNEG", "ADV", "ADJD", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Dir hebb' ick willen thor warnung schriven/", "tokens": ["Dir", "hebb'", "ick", "wil\u00b7len", "thor", "war\u00b7nung", "schri\u00b7ven", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "NN", "NN", "VVINF", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.36": {"text": "Dat gy m\u00f6gen in gesuntheit bliven.", "tokens": ["Dat", "gy", "m\u00f6\u00b7gen", "in", "ge\u00b7sun\u00b7theit", "bli\u00b7ven", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "St\u00f6rve gy henn/ so trurd de gantze Stadt/", "tokens": ["St\u00f6r\u00b7ve", "gy", "henn", "/", "so", "trurd", "de", "gant\u00b7ze", "Stadt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$(", "ADV", "ADJD", "NE", "ADJA", "NN", "$("], "met