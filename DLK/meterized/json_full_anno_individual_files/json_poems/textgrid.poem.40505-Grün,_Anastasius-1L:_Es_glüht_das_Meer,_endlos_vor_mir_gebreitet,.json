{"textgrid.poem.40505": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es gl\u00fcht das Meer, endlos vor mir gebreitet,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00fcht das Meer, endlos vor mir gebreitet,", "tokens": ["Es", "gl\u00fcht", "das", "Meer", ",", "end\u00b7los", "vor", "mir", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie die Erinnerung an ros'gen Mai,", "tokens": ["Wie", "die", "E\u00b7rin\u00b7ne\u00b7rung", "an", "ros'\u00b7gen", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und jenes Segel, das dar\u00fcber gleitet,", "tokens": ["Und", "je\u00b7nes", "Se\u00b7gel", ",", "das", "da\u00b7r\u00fc\u00b7ber", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich d\u00fcnkt's, als ob mein eignes Herz es sei.", "tokens": ["Mich", "d\u00fcnkt's", ",", "als", "ob", "mein", "eig\u00b7nes", "Herz", "es", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du unst\u00e4t Fahrzeug dort, das schwank und irre", "tokens": ["Du", "un\u00b7st\u00e4t", "Fahr\u00b7zeug", "dort", ",", "das", "schwank", "und", "ir\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "ADV", "$,", "PRELS", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fern durch die Wogen steuert hin und her,", "tokens": ["Fern", "durch", "die", "Wo\u00b7gen", "steu\u00b7ert", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer sagt mir wohl, wohin dein Segel schwirre", "tokens": ["Wer", "sagt", "mir", "wohl", ",", "wo\u00b7hin", "dein", "Se\u00b7gel", "schwir\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In diesem weiten, inselreichen Meer?", "tokens": ["In", "die\u00b7sem", "wei\u00b7ten", ",", "in\u00b7sel\u00b7rei\u00b7chen", "Meer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Welch Eiland einst dein Port aus all den blauen,", "tokens": ["Welch", "Ei\u00b7land", "einst", "dein", "Port", "aus", "all", "den", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerstreut im Spiegel abendrother Gluth,", "tokens": ["Zer\u00b7streut", "im", "Spie\u00b7gel", "a\u00b7ben\u00b7dro\u00b7ther", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie H\u00e4upter holder Jungfrau'n anzuschauen", "tokens": ["Wie", "H\u00e4up\u00b7ter", "hol\u00b7der", "Jung\u00b7frau'n", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJA", "NN", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auftauchend aus dem Bade lauer Fluth?", "tokens": ["Auf\u00b7tau\u00b7chend", "aus", "dem", "Ba\u00b7de", "lau\u00b7er", "Fluth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob dieses hier, auf dessen Flur von Rosen", "tokens": ["Ob", "die\u00b7ses", "hier", ",", "auf", "des\u00b7sen", "Flur", "von", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ADV", "$,", "APPR", "PRELAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Abend jetzt auch seine Rosen streut,", "tokens": ["Der", "A\u00b7bend", "jetzt", "auch", "sei\u00b7ne", "Ro\u00b7sen", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Himmelsbl\u00fcthen mit den ird'schen kosen,", "tokens": ["Da\u00df", "Him\u00b7mels\u00b7bl\u00fc\u00b7then", "mit", "den", "ird'\u00b7schen", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Erd' und Himmel gl\u00fchn im Blumenstreit?", "tokens": ["Und", "Erd'", "und", "Him\u00b7mel", "gl\u00fchn", "im", "Blu\u00b7men\u00b7streit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ob jenes dort, so stolz die Stirne tragend,", "tokens": ["Ob", "je\u00b7nes", "dort", ",", "so", "stolz", "die", "Stir\u00b7ne", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "$,", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Morgenroth drauf seinen Ku\u00df gepre\u00dft,", "tokens": ["Wenn", "Mor\u00b7gen\u00b7roth", "drauf", "sei\u00b7nen", "Ku\u00df", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch dessen goldner Felsenwall, hochragend,", "tokens": ["Doch", "des\u00b7sen", "gold\u00b7ner", "Fel\u00b7sen\u00b7wall", ",", "hoch\u00b7ra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Kahn der Sehnsucht nimmer landen l\u00e4\u00dft?", "tokens": ["Den", "Kahn", "der", "Sehn\u00b7sucht", "nim\u00b7mer", "lan\u00b7den", "l\u00e4\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ob jene Insel, die, da\u00df sanft es lande,", "tokens": ["Ob", "je\u00b7ne", "In\u00b7sel", ",", "die", ",", "da\u00df", "sanft", "es", "lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "$,", "KOUS", "ADJD", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch Schifflein lockt, und lieblich anzusehn,", "tokens": ["Manch", "Schif\u00b7flein", "lockt", ",", "und", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Mondenglanz sich gie\u00dft auf ihre Strande", "tokens": ["Wenn", "Mon\u00b7den\u00b7glanz", "sich", "gie\u00dft", "auf", "ih\u00b7re", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PRF", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und goldne Stern' in Meer und Aether stehn?", "tokens": ["Und", "gold\u00b7ne", "Stern'", "in", "Meer", "und", "A\u00b7e\u00b7ther", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Ob es die blondgelockte, deren Felder", "tokens": ["Ob", "es", "die", "blond\u00b7ge\u00b7lock\u00b7te", ",", "de\u00b7ren", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In \u00fcpp'ger Saat hinfluthen helles Gold?", "tokens": ["In", "\u00fcpp'\u00b7ger", "Saat", "hin\u00b7flu\u00b7then", "hel\u00b7les", "Gold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die schwarzgelockte, der ein Kranz der W\u00e4lder", "tokens": ["Die", "schwarz\u00b7ge\u00b7lock\u00b7te", ",", "der", "ein", "Kranz", "der", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie lindes Haar reich um die Schultern rollt?", "tokens": ["Wie", "lin\u00b7des", "Haar", "reich", "um", "die", "Schul\u00b7tern", "rollt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wer sagt es mir, wohin die\u00df Segel schwirre,", "tokens": ["Wer", "sagt", "es", "mir", ",", "wo\u00b7hin", "die\u00df", "Se\u00b7gel", "schwir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ob's ein Schiff auch, was dort treibt umher?", "tokens": ["Und", "ob's", "ein", "Schiff", "auch", ",", "was", "dort", "treibt", "um\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ob's nicht vielleicht mein Herz, das schwanke, irre,", "tokens": ["Ob's", "nicht", "viel\u00b7leicht", "mein", "Herz", ",", "das", "schwan\u00b7ke", ",", "ir\u00b7re", ","], "token_info": ["word",