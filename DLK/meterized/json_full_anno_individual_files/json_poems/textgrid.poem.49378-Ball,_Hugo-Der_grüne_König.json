{"textgrid.poem.49378": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Der gr\u00fcne K\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1922, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir, Johann, Amadeus Adelgreif,", "tokens": ["Wir", ",", "Jo\u00b7hann", ",", "A\u00b7ma\u00b7deus", "A\u00b7del\u00b7greif", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcrst von Saprunt und beiderlei Smeraldis,", "tokens": ["F\u00fcrst", "von", "Sap\u00b7runt", "und", "bei\u00b7der\u00b7lei", "Sme\u00b7ral\u00b7dis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "KON", "PIAT", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Erzkaiser \u00fcber allen Unterschleif", "tokens": ["Erz\u00b7kai\u00b7ser", "\u00fc\u00b7ber", "al\u00b7len", "Un\u00b7ter\u00b7schleif"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Obers\u00e4ckelmeister vom Schmalkaldis", "tokens": ["Und", "O\u00b7bers\u00b7\u00e4\u00b7ckel\u00b7meis\u00b7ter", "vom", "Schmal\u00b7kal\u00b7dis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Erheben unsern grimmen L\u00f6wenschweif", "tokens": ["Er\u00b7he\u00b7ben", "un\u00b7sern", "grim\u00b7men", "L\u00f6\u00b7wen\u00b7schweif"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dekretieren vor den leeren Saldis:", "t