{"dta.poem.18235": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Du gabst mir scheidend das Geleit,", "tokens": ["Du", "gabst", "mir", "schei\u00b7dend", "das", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im nahen Wald blies eine Hirtenfl\u00f6te", "tokens": ["Im", "na\u00b7hen", "Wald", "blies", "ei\u00b7ne", "Hir\u00b7ten\u00b7fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein altes Lied aus alter Zeit.", "tokens": ["Ein", "al\u00b7tes", "Lied", "aus", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht K\u00fcsse waren's, die wir heimlich tauschten,", "tokens": ["Nicht", "K\u00fcs\u00b7se", "wa\u00b7ren's", ",", "die", "wir", "heim\u00b7lich", "tauschten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u2014 Es war die Zeit des Bl\u00e4tterfalls \u2014", "tokens": ["Es", "war", "die", "Zeit", "des", "Bl\u00e4t\u00b7ter\u00b7falls"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch als am Kreuzweg die drei Linden rauschten,", "tokens": ["Doch", "als", "am", "Kreuz\u00b7weg", "die", "drei", "Lin\u00b7den", "rauschten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "CARD", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fielst du mir weinend um den Hals!", "tokens": ["Fielst", "du", "mir", "wei\u00b7nend", "um", "den", "Hals", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und deiner Liebe langverhaltnes Leiden,", "tokens": ["Und", "dei\u00b7ner", "Lie\u00b7be", "lang\u00b7ver\u00b7halt\u00b7nes", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus deinem Herzen brach's hervor,", "tokens": ["Aus", "dei\u00b7nem", "Her\u00b7zen", "brach's", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ahntest du's, da\u00df Jedes von uns Beiden", "tokens": ["Als", "ahn\u00b7test", "du's", ",", "da\u00df", "Je\u00b7des", "von", "uns", "Bei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PIS", "$,", "KOUS", "PIS", "APPR", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Andern auch sich selbst verlor!", "tokens": ["Im", "An\u00b7dern", "auch", "sich", "selbst", "ver\u00b7lor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Worte sprachst du, die ich nie vergessen,", "tokens": ["Und", "Wor\u00b7te", "sprachst", "du", ",", "die", "ich", "nie", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ach, uns g\u00f6nnte das Geschick", "tokens": ["Doch", "ach", ",", "uns", "g\u00f6nn\u00b7te", "das", "Ge\u00b7schick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur noch ein letztes Aneinanderpressen ...", "tokens": ["Nur", "noch", "ein", "letz\u00b7tes", "An\u00b7ein\u00b7an\u00b7der\u00b7pres\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es war ein dunkler Augenblick!", "tokens": ["Es", "war", "ein", "dunk\u00b7ler", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch nicht entweihen will ich jene Stunde,", "tokens": ["Doch", "nicht", "ent\u00b7wei\u00b7hen", "will", "ich", "je\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "VMFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum still, o still, Erinnerung!", "tokens": ["Drum", "still", ",", "o", "still", ",", "E\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "FM", "FM", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn nie schlie\u00dft sich ein Herz um seine Wunde,", "tokens": ["Denn", "nie", "schlie\u00dft", "sich", "ein", "Herz", "um", "sei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein echtes Leid bleibt ewig jung.", "tokens": ["Ein", "ech\u00b7tes", "Leid", "bleibt", "e\u00b7wig", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch immer, wenn des Abends letzte R\u00f6the", "tokens": ["Noch", "im\u00b7mer", ",", "wenn", "des", "A\u00b7bends", "letz\u00b7te", "R\u00f6\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Meer taucht, wird das Herz mir weit,", "tokens": ["Ins", "Meer", "taucht", ",", "wird", "das", "Herz", "mir", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mich umklingt wie eine Hirtenfl\u00f6te", "tokens": ["Und", "mich", "um\u00b7klingt", "wie", "ei\u00b7ne", "Hir\u00b7ten\u00b7fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein altes Lied aus alter Zeit.", "tokens": ["Ein", "al\u00b7tes", "Lied", "aus", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}