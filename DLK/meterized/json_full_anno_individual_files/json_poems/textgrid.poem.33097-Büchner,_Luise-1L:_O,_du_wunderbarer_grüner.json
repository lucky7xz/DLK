": "Noch so laut, als w\u00e4r es heut',", "tokens": ["Noch", "so", "laut", ",", "als", "w\u00e4r", "es", "heut'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der besten Geister einem,", "tokens": ["Da\u00df", "der", "bes\u00b7ten", "Geis\u00b7ter", "ei\u00b7nem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ganz erf\u00fcllt vom h\u00f6chsten Drang,", "tokens": ["Ganz", "er\u00b7f\u00fcllt", "vom", "h\u00f6chs\u00b7ten", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df dem treusten, w\u00e4rmsten Herzen", "tokens": ["Da\u00df", "dem", "treus\u00b7ten", ",", "w\u00e4rms\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie get\u00f6nt den Grabgesang!", "tokens": ["Sie", "ge\u00b7t\u00f6nt", "den", "Grab\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}