{"textgrid.poem.47830": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00e4ber in Schleswig", "genre": "verse", "period": "N.A.", "pub_year": 1863, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Kranz noch Kreuz; das Unkraut wuchert tief;", "tokens": ["Nicht", "Kranz", "noch", "Kreuz", ";", "das", "Un\u00b7kraut", "wu\u00b7chert", "tief", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn die der Tod bei Idstedt einst entboten,", "tokens": ["Denn", "die", "der", "Tod", "bei", "Ids\u00b7tedt", "einst", "ent\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "APPR", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier schlafen sie, und deutsche Ehre schlief", "tokens": ["Hier", "schla\u00b7fen", "sie", ",", "und", "deut\u00b7sche", "Eh\u00b7re", "schlief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hier dreizehn Jahre lang bei diesen Toten.", "tokens": ["Hier", "drei\u00b7zehn", "Jah\u00b7re", "lang", "bei", "die\u00b7sen", "To\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und dreizehn Jahre litten jung und alt,", "tokens": ["Und", "drei\u00b7zehn", "Jah\u00b7re", "lit\u00b7ten", "jung", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was leben blieb, des kleinen Feindes T\u00fccken,", "tokens": ["Was", "le\u00b7ben", "blieb", ",", "des", "klei\u00b7nen", "Fein\u00b7des", "T\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VVFIN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und konnten nichts als, stumm die Faust geballt,", "tokens": ["Und", "konn\u00b7ten", "nichts", "als", ",", "stumm", "die", "Faust", "ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "KOKOM", "$,", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Schrei des Zorns in ihrer Brust ersticken.", "tokens": ["Den", "Schrei", "des", "Zorns", "in", "ih\u00b7rer", "Brust", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Schmach ist aus; der ehrne W\u00fcrfel f\u00e4llt!", "tokens": ["Die", "Schmach", "ist", "aus", ";", "der", "ehr\u00b7ne", "W\u00fcr\u00b7fel", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt oder nie! Erf\u00fcllet sind die Zeiten,", "tokens": ["Jetzt", "o\u00b7der", "nie", "!", "Er\u00b7f\u00fcl\u00b7let", "sind", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des D\u00e4nenk\u00f6nigs Totenglocke gellt;", "tokens": ["Des", "D\u00e4\u00b7nen\u00b7k\u00f6\u00b7nigs", "To\u00b7ten\u00b7glo\u00b7cke", "gellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir klinget es wie Osterglockenl\u00e4uten!", "tokens": ["Mir", "klin\u00b7get", "es", "wie", "Os\u00b7ter\u00b7glo\u00b7cken\u00b7l\u00e4u\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Erde dr\u00f6hnt; von Deutschland weht es her,", "tokens": ["Die", "Er\u00b7de", "dr\u00f6hnt", ";", "von", "Deutschland", "weht", "es", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "NE", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mir ist, ich h\u00f6r ein Lied im Winde klingen,", "tokens": ["Mir", "ist", ",", "ich", "h\u00f6r", "ein", "Lied", "im", "Win\u00b7de", "klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es kommt heran schon wie ein brausend Meer,", "tokens": ["Es", "kommt", "he\u00b7ran", "schon", "wie", "ein", "brau\u00b7send", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ADV", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um endlich alle Schande zu verschlingen! \u2013 \u2013", "tokens": ["Um", "end\u00b7lich", "al\u00b7le", "Schan\u00b7de", "zu", "ver\u00b7schlin\u00b7gen", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUI", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "T\u00f6richter Traum! \u2013 Es klingt kein deutsches Lied,", "tokens": ["T\u00f6\u00b7rich\u00b7ter", "Traum", "!", "\u2013", "Es", "klingt", "kein", "deut\u00b7sches", "Lied", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "$(", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Vorw\u00e4rts schallt von deutschen Bataillonen;", "tokens": ["Kein", "Vor\u00b7w\u00e4rts", "schallt", "von", "deut\u00b7schen", "Ba\u00b7tail\u00b7lo\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl dr\u00f6hnt der Grund, wohl naht es Glied an Glied;", "tokens": ["Wohl", "dr\u00f6hnt", "der", "Grund", ",", "wohl", "naht", "es", "Glied", "an", "Glied", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch sind's die Reiter d\u00e4nischer Schwadronen.", "tokens": ["Doch", "sin\u00b7d's", "die", "Rei\u00b7ter", "d\u00e4\u00b7ni\u00b7scher", "Schwad\u00b7ro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie kommen nicht. Das Londoner Papier,", "tokens": ["Sie", "kom\u00b7men", "nicht", ".", "Das", "Lon\u00b7do\u00b7ner", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es wiegt zu schwer, sie wagen's nicht zu heben.", "tokens": ["Es", "wiegt", "zu", "schwer", ",", "sie", "wa\u00b7gen's", "nicht", "zu", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Stunde dr\u00e4ngt. So helft, ihr Toten hier!", "tokens": ["Die", "Stun\u00b7de", "dr\u00e4ngt", ".", "So", "helft", ",", "ihr", "To\u00b7ten", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich rufe euch und hoffe nichts vom Leben.", "tokens": ["Ich", "ru\u00b7fe", "euch", "und", "hof\u00b7fe", "nichts", "vom", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wacht auf, ihr Reiter! Sch\u00fcttelt ab den Sand,", "tokens": ["Wacht", "auf", ",", "ihr", "Rei\u00b7ter", "!", "Sch\u00fct\u00b7telt", "ab", "den", "Sand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Besteigt noch einmal die gest\u00fcrzten Renner!", "tokens": ["Be\u00b7steigt", "noch", "ein\u00b7mal", "die", "ge\u00b7st\u00fcrz\u00b7ten", "Ren\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blast, blast, ihr J\u00e4ger! F\u00fcr das Vaterland", "tokens": ["Blast", ",", "blast", ",", "ihr", "J\u00e4\u00b7ger", "!", "F\u00fcr", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch einen Strau\u00df! Wir brauchen M\u00e4nner, M\u00e4nner!", "tokens": ["Noch", "ei\u00b7nen", "Strau\u00df", "!", "Wir", "brau\u00b7chen", "M\u00e4n\u00b7ner", ",", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NE", "$.", "PPER", "VVFIN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Tambour, hervor aus deinem schwarzen Schrein!", "tokens": ["Tam\u00b7bour", ",", "her\u00b7vor", "aus", "dei\u00b7nem", "schwar\u00b7zen", "Schrein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch einmal gilt's, das Trommelfell zu schlagen;", "tokens": ["Noch", "ein\u00b7mal", "gilt's", ",", "das", "Trom\u00b7mel\u00b7fell", "zu", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll euer Grab in deutscher Erde sein,", "tokens": ["Soll", "eu\u00b7er", "Grab", "in", "deut\u00b7scher", "Er\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So m\u00fc\u00dft ihr noch ein zweites Leben wagen! \u2013", "tokens": ["So", "m\u00fc\u00dft", "ihr", "noch", "ein", "zwei\u00b7tes", "Le\u00b7ben", "wa\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich ruf umsonst! ihr ruht auf ewig aus;", "tokens": ["Ich", "ruf", "um\u00b7sonst", "!", "ihr", "ruht", "auf", "e\u00b7wig", "aus", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "APPR", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr wurdet eine duldsame Gemeinde.", "tokens": ["Ihr", "wur\u00b7det", "ei\u00b7ne", "duld\u00b7sa\u00b7me", "Ge\u00b7mein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber schrei es in die Welt hinaus:", "tokens": ["Ich", "a\u00b7ber", "schrei", "es", "in", "die", "Welt", "hin\u00b7aus", ":"], "token_info": ["