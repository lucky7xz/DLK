{"textgrid.poem.44109": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[wie gl\u00fccklich lebt doch eine Stadt]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00fccklich lebt doch eine Stadt,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "lebt", "doch", "ei\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit Athen den Preis der freyen K\u00fcnste hat,", "tokens": ["Die", "mit", "A\u00b7then", "den", "Preis", "der", "frey\u00b7en", "K\u00fcns\u00b7te", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wo sich das Chor der Musen reget", "tokens": ["Wo", "sich", "das", "Chor", "der", "Mu\u00b7sen", "re\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wo der Wei\u00dfheit Musterplaz", "tokens": ["Und", "wo", "der", "Wei\u00df\u00b7heit", "Mus\u00b7ter\u00b7plaz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Kleinod und den edlen Schaz", "tokens": ["Das", "Klei\u00b7nod", "und", "den", "ed\u00b7len", "Schaz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der guten Wi\u00dfenschaft uns in die Herzen pr\u00e4get.", "tokens": ["Der", "gu\u00b7ten", "Wi\u00b7\u00dfen\u00b7schaft", "uns", "in", "die", "Her\u00b7zen", "pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Nacht der bangen Finstern\u00fc\u00df,", "tokens": ["Die", "Nacht", "der", "ban\u00b7gen", "Fins\u00b7ter\u00b7n\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So dir, Elysien, der Wei\u00dfheit Glanz entri\u00df,", "tokens": ["So", "dir", ",", "E\u00b7ly\u00b7si\u00b7en", ",", "der", "Wei\u00df\u00b7heit", "Glanz", "ent\u00b7ri\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NE", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird durch des Phoebus Licht vertrieben.", "tokens": ["Wird", "durch", "des", "Phoe\u00b7bus", "Licht", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Klugheit steigt, die Thorheit f\u00e4llt,", "tokens": ["Die", "Klug\u00b7heit", "steigt", ",", "die", "Thor\u00b7heit", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Seitdem nun die gelehrte Welt", "tokens": ["Seit\u00b7dem", "nun", "die", "ge\u00b7lehr\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Irrthum und den Wahn des P\u00f6bels aufgerieben.", "tokens": ["Den", "Irr\u00b7thum", "und", "den", "Wahn", "des", "P\u00f6\u00b7bels", "auf\u00b7ge\u00b7rie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo vor Gradivens Tempel war,", "tokens": ["Wo", "vor", "Gra\u00b7di\u00b7vens", "Tem\u00b7pel", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NE", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da baut die Tugend jezt der Pallas ein Altar,", "tokens": ["Da", "baut", "die", "Tu\u00b7gend", "jezt", "der", "Pal\u00b7las", "ein", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wo der Helden Blut geschwommen,", "tokens": ["Und", "wo", "der", "Hel\u00b7den", "Blut", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geust man vielleicht die Dinte hin,", "tokens": ["Geust", "man", "viel\u00b7leicht", "die", "Din\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Da, wo Minervens Sch\u00fcler knien,", "tokens": ["Da", ",", "wo", "Mi\u00b7ner\u00b7vens", "Sch\u00fc\u00b7ler", "kni\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Hat etwan Lechus einst den Fu\u00dffall angenommen.", "tokens": ["Hat", "et\u00b7wan", "Le\u00b7chus", "einst", "den", "Fu\u00df\u00b7fall", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Fleuch hin, verdammte Barbarey!", "tokens": ["Fleuch", "hin", ",", "ver\u00b7damm\u00b7te", "Bar\u00b7ba\u00b7rey", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Scepter ist zerst\u00fcckt, dein Purpurrock entzwey,", "tokens": ["Dein", "Scep\u00b7ter", "ist", "zer\u00b7st\u00fcckt", ",", "dein", "Pur\u00b7pur\u00b7rock", "ent\u00b7zwey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir aber deiner Last entbunden.", "tokens": ["Wir", "a\u00b7ber", "dei\u00b7ner", "Last", "ent\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00fcnste bl\u00fchn und nehmen zu,", "tokens": ["Die", "K\u00fcns\u00b7te", "bl\u00fchn", "und", "neh\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem des Friedens s\u00fc\u00dfe Ruh", "tokens": ["Nach\u00b7dem", "des", "Frie\u00b7dens", "s\u00fc\u00b7\u00dfe", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Oelzweig um den Hut des F\u00fcrstenthums gewunden.", "tokens": ["Den", "O\u00b7el\u00b7zweig", "um", "den", "Hut", "des", "F\u00fcrs\u00b7ten\u00b7thums", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VAPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Begl\u00fccktes Schweidniz, nimm in Acht,", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Schweid\u00b7niz", ",", "nimm", "in", "Acht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie das Verh\u00e4ngn\u00fc\u00df dich vor andern herrlich macht,", "tokens": ["Wie", "das", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "dich", "vor", "an\u00b7dern", "herr\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "APPR", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schau deiner S\u00f6hne Wohlergehen;", "tokens": ["Schau", "dei\u00b7ner", "S\u00f6h\u00b7ne", "Woh\u00b7ler\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ihrer Arbeit