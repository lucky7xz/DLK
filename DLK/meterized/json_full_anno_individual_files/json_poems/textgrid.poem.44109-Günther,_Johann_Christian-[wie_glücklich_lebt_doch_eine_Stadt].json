word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was deiner Lehrer Flei\u00df gethan.", "tokens": ["Was", "dei\u00b7ner", "Leh\u00b7rer", "Flei\u00df", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich deucht, sie hebt verwundernd an:", "tokens": ["Mich", "deucht", ",", "sie", "hebt", "ver\u00b7wun\u00b7dernd", "an", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt dann in Schlesien das Alter vor den Jahren!", "tokens": ["Kommt", "dann", "in", "Schle\u00b7si\u00b7en", "das", "Al\u00b7ter", "vor", "den", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie gl\u00fccklich lebt doch eine Stadt,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "lebt", "doch", "ei\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit Athen den Preis der freyen K\u00fcnste hat,", "tokens": ["Die", "mit", "A\u00b7then", "den", "Preis", "der", "frey\u00b7en", "K\u00fcns\u00b7te", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.h