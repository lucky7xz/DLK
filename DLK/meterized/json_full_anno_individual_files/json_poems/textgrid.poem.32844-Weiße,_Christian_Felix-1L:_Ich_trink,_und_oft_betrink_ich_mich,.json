{"textgrid.poem.32844": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich trink, und oft betrink ich mich,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trink, und oft betrink ich mich,", "tokens": ["Ich", "trink", ",", "und", "oft", "be\u00b7trink", "ich", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein M\u00e4dchen, auf dein Wohlergehen!", "tokens": ["Mein", "M\u00e4d\u00b7chen", ",", "auf", "dein", "Woh\u00b7ler\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da solltest du den Vater Bacchus sehen:", "tokens": ["Da", "soll\u00b7test", "du", "den", "Va\u00b7ter", "Bac\u00b7chus", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er denkt, und br\u00fcstet sich,", "tokens": ["Er", "denkt", ",", "und", "br\u00fcs\u00b7tet", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sey blos ihm zum Trost geschehen.", "tokens": ["Es", "sey", "blos", "ihm", "zum", "Trost", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der gute Narr! er br\u00fcste sich,", "tokens": ["Der", "gu\u00b7te", "Narr", "!", "er", "br\u00fcs\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frohlocke gegen seine Br\u00fcder!", "tokens": ["Froh\u00b7lo\u00b7cke", "ge\u00b7gen", "sei\u00b7ne", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sag es dir, doch sag es ihm nicht wieder!", "tokens": ["Ich", "sag", "es", "dir", ",", "doch", "sag", "es", "ihm", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Liebe nur f\u00fcr dich,", "tokens": ["Aus", "Lie\u00b7be", "nur", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trink ich, trink ich mich \u00f6fters nieder.", "tokens": ["Trink", "ich", ",", "trink", "ich", "mich", "\u00f6f\u00b7ters", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ich trink, und oft betrink ich mich,", "tokens": ["Ich", "trink", ",", "und", "oft", "be\u00b7trink", "ich", "mich", ","], "token_info": ["word",