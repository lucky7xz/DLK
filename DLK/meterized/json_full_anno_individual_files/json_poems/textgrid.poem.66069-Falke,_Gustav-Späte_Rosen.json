{"textgrid.poem.66069": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Sp\u00e4te Rosen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahrelang sehnten wir uns,", "tokens": ["Jah\u00b7re\u00b7lang", "sehn\u00b7ten", "wir", "uns", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Einen Garten unser zu nennen,", "tokens": ["Ei\u00b7nen", "Gar\u00b7ten", "un\u00b7ser", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Darin eine k\u00fchle Laube steht", "tokens": ["Da\u00b7rin", "ei\u00b7ne", "k\u00fch\u00b7le", "Lau\u00b7be", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und rote Rosen brennen.", "tokens": ["Und", "ro\u00b7te", "Ro\u00b7sen", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun steht das G\u00e4rtchen im ersten Gr\u00fcn,", "tokens": ["Nun", "steht", "das", "G\u00e4rt\u00b7chen", "im", "ers\u00b7ten", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Laube in dichten Reben,", "tokens": ["Die", "Lau\u00b7be", "in", "dich\u00b7ten", "Re\u00b7ben", ","], "token_info": ["word", "word", "word",