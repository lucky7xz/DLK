 "punct"], "pos": ["CARD", "NN", "ADV", "$,", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die achtet sonst kein L\u00e4ufer sehr,", "tokens": ["Die", "ach\u00b7tet", "sonst", "kein", "L\u00e4u\u00b7fer", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo niemand kann den Preis erjagen.", "tokens": ["Wo", "nie\u00b7mand", "kann", "den", "Preis", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Erbla\u00dfter Freund, dein Lebenslicht", "tokens": ["Er\u00b7bla\u00df\u00b7ter", "Freund", ",", "dein", "Le\u00b7bens\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlischt in deinen besten Tagen.", "tokens": ["Ver\u00b7lischt", "in", "dei\u00b7nen", "bes\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sehn mit nassem Angesicht,", "tokens": ["Wir", "sehn", "mit", "nas\u00b7sem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich zeitig in die Grube tragen.", "tokens": ["Dich", "zei\u00b7tig", "in", "die", "Gru\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tod und dein so fr\u00fches Grab", "tokens": ["Der", "Tod", "und", "dein", "so", "fr\u00fc\u00b7hes", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rei\u00dft dir zu schnell den Faden ab,", "tokens": ["Rei\u00dft", "dir", "zu", "schnell", "den", "Fa\u00b7den", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Daran dein kluger Flei\u00df gesponnen:", "tokens": ["Da\u00b7ran", "dein", "klu\u00b7ger", "Flei\u00df", "ge\u00b7spon\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch hat auch so dein edler Geist,", "tokens": ["Doch", "hat", "auch", "so", "dein", "ed\u00b7ler", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Indem er sich der Welt entrei\u00dft,", "tokens": ["In\u00b7dem", "er", "sich", "der", "Welt", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch den Verlust weit mehr gewonnen.", "tokens": ["Durch", "den", "Ver\u00b7lust", "weit", "mehr", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "++-+-+-+-", "measure": "iambic