{"textgrid.poem.57460": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "Bey dem Hintritte eines jungen Studirenden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df unsre ganze Wissenschaft", "tokens": ["Da\u00df", "uns\u00b7re", "gan\u00b7ze", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In gar zu engen Gr\u00e4nzen bleibe;", "tokens": ["In", "gar", "zu", "en\u00b7gen", "Gr\u00e4n\u00b7zen", "blei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und stets mit gar zu matter Kraft", "tokens": ["Und", "stets", "mit", "gar", "zu", "mat\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die schwachen Sprossen aufw\u00e4rts treibe;", "tokens": ["Die", "schwa\u00b7chen", "Spros\u00b7sen", "auf\u00b7w\u00e4rts", "trei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Witz, Ged\u00e4chtni\u00df und Verstand,", "tokens": ["Da\u00df", "Witz", ",", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So viel sie gr\u00fcndliches erkannt,", "tokens": ["So", "viel", "sie", "gr\u00fcnd\u00b7li\u00b7ches", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gleichwohl noch nichts Vollkommnes wissen:", "tokens": ["Gleich\u00b7wohl", "noch", "nichts", "Voll\u00b7komm\u00b7nes", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das hat ohn allen Unterscheid,", "tokens": ["Das", "hat", "ohn", "al\u00b7len", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Alterthum und unsre Zeit", "tokens": ["Das", "Al\u00b7ter\u00b7thum", "und", "uns\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erkennen und gestehen m\u00fcssen.", "tokens": ["Er\u00b7ken\u00b7nen", "und", "ge\u00b7ste\u00b7hen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Beklaget nicht der Weisen Zahl", "tokens": ["Be\u00b7kla\u00b7get", "nicht", "der", "Wei\u00b7sen", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Menschen gar zu fr\u00fches Sterben?", "tokens": ["Des", "Men\u00b7schen", "gar", "zu", "fr\u00fc\u00b7hes", "Ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcnschen sie nicht manchesmal,", "tokens": ["Und", "w\u00fcn\u00b7schen", "sie", "nicht", "man\u00b7ches\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sp\u00e4t, als Nestor zu verderben?", "tokens": ["So", "sp\u00e4t", ",", "als", "Nes\u00b7tor", "zu", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kunst ist gro\u00df, das Leben klein!", "tokens": ["Die", "Kunst", "ist", "gro\u00df", ",", "das", "Le\u00b7ben", "klein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man wollte gern ein Meister seyn,", "tokens": ["Man", "woll\u00b7te", "gern", "ein", "Meis\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mu\u00df als Sch\u00fcler schon erblassen:", "tokens": ["Und", "mu\u00df", "als", "Sch\u00fc\u00b7ler", "schon", "er\u00b7blas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KOUS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Wissenschaft ist noch nicht da,", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", "ist", "noch", "nicht", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch ist man schon dem Grabe nah;", "tokens": ["Doch", "ist", "man", "schon", "dem", "Gra\u00b7be", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Doch mu\u00df man schon die Welt verlassen.", "tokens": ["Doch", "mu\u00df", "man", "schon", "die", "Welt", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist diese Wahrheit allgemein,", "tokens": ["Ist", "die\u00b7se", "Wahr\u00b7heit", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trifft sie auch die grauen Alten;", "tokens": ["Und", "trifft", "sie", "auch", "die", "grau\u00b7en", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die auch den sp\u00e4tsten Leichenstein", "tokens": ["Die", "auch", "den", "sp\u00e4ts\u00b7ten", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ihres Wissens Gr\u00e4nzstein halten:", "tokens": ["F\u00fcr", "ih\u00b7res", "Wis\u00b7sens", "Gr\u00e4nz\u00b7stein", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was darf sich denn die Jugend viel", "tokens": ["Was", "darf", "sich", "denn", "die", "Ju\u00b7gend", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um das zu nah gesteckte Ziel", "tokens": ["Um", "das", "zu", "nah", "ge\u00b7steck\u00b7te", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des kurzen Lebenslaufs beklagen?", "tokens": ["Des", "kur\u00b7zen", "Le\u00b7bens\u00b7laufs", "be\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zehn Schritte minder, oder mehr;", "tokens": ["Zehn", "Schrit\u00b7te", "min\u00b7der", ",", "o\u00b7der", "mehr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die achtet sonst kein L\u00e4ufer sehr,", "tokens": ["Die", "ach\u00b7tet", "sonst", "kein", "L\u00e4u\u00b7fer", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo niemand kann den Preis erjagen.", "tokens": ["Wo", "nie\u00b7mand", "kann", "den", "Preis", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Erbla\u00dfter Freund, dein Lebenslicht", "tokens": ["Er\u00b7bla\u00df\u00b7ter", "Freund", ",", "dein", "Le\u00b7bens\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlischt in deinen besten Tagen.", "tokens": ["Ver\u00b7lischt", "in", "dei\u00b7nen", "bes\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sehn mit nassem Angesicht,", "tokens": ["Wir", "sehn", "mit", "nas\u00b7sem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich zeitig in die Grube tragen.", "tokens": ["Dich", "zei\u00b7tig", "in", "die", "Gru\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tod und dein so fr\u00fches Grab", "tokens": ["Der", "Tod", "und", "dein", "so", "fr\u00fc\u00b7hes", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rei\u00dft dir zu schnell den Faden ab,", "tokens": ["Rei\u00dft", "dir", "zu", "schnell", "den", "Fa\u00b7den", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Daran dein kluger Flei\u00df gesponnen:", "tokens": ["Da\u00b7ran", "dein", "klu\u00b7ger", "Flei\u00df", "ge\u00b7spon\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch hat auch so dein edler Geist,", "tokens": ["Doch", "hat", "auch", "so", "dein", "ed\u00b7ler", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Indem er sich der Welt entrei\u00dft,", "tokens": ["In\u00b7dem", "er", "sich", "der", "Welt", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Durch den Verlust weit mehr gewonnen.", "tokens": ["Durch", "den", "Ver\u00b7lust", "weit", "mehr", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du r\u00fcckst aus dieser Lindenstadt", "tokens": ["Du", "r\u00fcckst", "aus", "die\u00b7ser", "Lin\u00b7den\u00b7stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur hohen Schule weiser Frommen;", "tokens": ["Zur", "ho\u00b7hen", "Schu\u00b7le", "wei\u00b7ser", "From\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die den zu ihrem Lehrer hat,", "tokens": ["Die", "den", "zu", "ih\u00b7rem", "Leh\u00b7rer", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von dem Verstand und Wahrheit kommen.", "tokens": ["Von", "dem", "Ver\u00b7stand", "und", "Wahr\u00b7heit", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du siehst nun aus der Ewigkeit,", "tokens": ["Du", "siehst", "nun", "aus", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie fruchtlos wir die meiste Zeit", "tokens": ["Wie", "frucht\u00b7los", "wir", "die", "meis\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf Wissenschaft und K\u00fcnste wenden;", "tokens": ["Auf", "Wis\u00b7sen\u00b7schaft", "und", "K\u00fcns\u00b7te", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beklagst auch unsern Unverstand,", "tokens": ["Be\u00b7klagst", "auch", "un\u00b7sern", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn wir uns oft mit eigner Hand", "tokens": ["Wenn", "wir", "uns", "oft", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die bl\u00f6den Augen vollends blenden.", "tokens": ["Die", "bl\u00f6\u00b7den", "Au\u00b7gen", "vol\u00b7lends", "blen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O selig l wer so weit schon ist,", "tokens": ["O", "se\u00b7lig", "l", "wer", "so", "weit", "schon", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "PWS", "ADV", "ADJD", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wo selbst die Lehrer lernen k\u00f6nnen.", "tokens": ["Wo", "selbst", "die", "Leh\u00b7rer", "ler\u00b7nen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sehen, da\u00df du gl\u00fccklich bist;", "tokens": ["Wir", "se\u00b7hen", ",", "da\u00df", "du", "gl\u00fcck\u00b7lich", "bist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kann man dir den Stand nicht g\u00f6nnen?", "tokens": ["Wie", "kann", "man", "dir", "den", "Stand", "nicht", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man sehnet sich vielmehr zugleich,", "tokens": ["Man", "seh\u00b7net", "sich", "viel\u00b7mehr", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN",