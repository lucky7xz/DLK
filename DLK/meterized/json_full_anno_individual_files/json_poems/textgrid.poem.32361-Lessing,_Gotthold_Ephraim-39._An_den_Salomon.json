{"textgrid.poem.32361": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "39. An den Salomon", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochweiser Salomon! dein Spruch,", "tokens": ["Hoch\u00b7wei\u00b7ser", "Sa\u00b7lo\u00b7mon", "!", "dein", "Spruch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbda\u00df unter Tausenden kein gutes Weib zu finden\u00ab,", "tokens": ["\u00bb", "da\u00df", "un\u00b7ter", "Tau\u00b7sen\u00b7den", "kein", "gu\u00b7tes", "Weib", "zu", "fin\u00b7den", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "APPR", "NN", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geh\u00f6rt \u2013 gerad' heraus \u2013 zu deinen Zungens\u00fcnden;", "tokens": ["Ge\u00b7h\u00f6rt", "\u2013", "ge\u00b7rad'", "he\u00b7raus", "\u2013", "zu", "dei\u00b7nen", "Zun\u00b7gen\u00b7s\u00fcn\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "ADV", "PTKVZ", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jeder Fluch ist minder Fluch,", "tokens": ["Und", "je\u00b7der", "Fluch", "ist", "min\u00b7der", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als dieser sch\u00f6ne Sittenspruch.", "tokens": ["Als", "die\u00b7ser", "sch\u00f6\u00b7n