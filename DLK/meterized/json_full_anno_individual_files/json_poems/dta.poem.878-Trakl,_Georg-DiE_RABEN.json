{"dta.poem.878": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "DiE RABEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber den schwarzen Winkel hasten", "tokens": ["\u00dc\u00b7ber", "den", "schwar\u00b7zen", "Win\u00b7kel", "has\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Am Mittag die Raben mit hartem Schrei.", "tokens": ["Am", "Mit\u00b7tag", "die", "Ra\u00b7ben", "mit", "har\u00b7tem", "Schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ihr Schatten streift an der Hirschkuh vorbei", "tokens": ["Ihr", "Schat\u00b7ten", "streift", "an", "der", "Hirschkuh", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und manchmal sieht man sie m\u00fcrrisch rasten.", "tokens": ["Und", "manch\u00b7mal", "sieht", "man", "sie", "m\u00fcr\u00b7risch", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "O wie sie die braune Stille st\u00f6ren,", "tokens": ["O", "wie", "sie", "die", "brau\u00b7ne", "Stil\u00b7le", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der ein Acker sich verz\u00fcckt,", "tokens": ["In", "der", "ein", "A\u00b7cker", "sich", "ver\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ein Weib, das schwere Ahnung ber\u00fcckt,", "tokens": ["Wie", "ein", "Weib", ",", "das", "schwe\u00b7re", "Ah\u00b7nung", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und manchmal kann man sie keifen h\u00f6ren", "tokens": ["Und", "manch\u00b7mal", "kann", "man", "sie", "kei\u00b7fen", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIS", "PPER", "VVINF", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Um ein Aas, das sie irgendwo wittern,", "tokens": ["Um", "ein", "Aas", ",", "das", "sie", "ir\u00b7gend\u00b7wo", "wit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und pl\u00f6tzlich richten nach Nord sie den Flug", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "rich\u00b7ten", "nach", "Nord", "sie", "den", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NE", "PPER", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schwinden wie ein Leichenzug", "tokens": ["Und", "schwin\u00b7den", "wie", "ein", "Lei\u00b7chen\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In L\u00fcften, die von Wollust zittern.", "tokens": ["In", "L\u00fcf\u00b7ten", ",", "die", "von", "Wol\u00b7lust", "zit\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}