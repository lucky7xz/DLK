{"dta.poem.10787": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Vff ein paar H\u00e4ndschuch", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckseelig weret jhr/ H\u00e4ndschuch/ wann jhr solt k\u00f6nnen", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "we\u00b7ret", "jhr", "/", "H\u00e4nd\u00b7schuch", "/", "wann", "jhr", "solt", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$(", "NN", "$(", "PWAV", "PPER", "VMFIN", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ewr gro\u00df Gl\u00fcckseligkeit vern\u00fcnfftiglich erkennen/", "tokens": ["Ewr", "gro\u00df", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "ver\u00b7n\u00fcnff\u00b7tig\u00b7lich", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "VVINF", "$("], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Es sollen herbergen in euch zwey H\u00e4ndelein/", "tokens": ["Es", "sol\u00b7len", "her\u00b7ber\u00b7gen", "in", "euch", "zwey", "H\u00e4n\u00b7del\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "PPER", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die weisser als der Schnee/ purer dann Helffenbein/", "tokens": ["Die", "weis\u00b7ser", "als", "der", "Schnee", "/", "pu\u00b7rer", "dann", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "ART", "NN", "$(", "ADJD", "ADV", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Deren subtiligkeit die sch\u00f6nste Perlen weichen.", "tokens": ["De\u00b7ren", "sub\u00b7ti\u00b7lig\u00b7keit", "die", "sch\u00f6ns\u00b7te", "Per\u00b7len", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Was soll ich aber sie vil mit dem Schnee vergleichen/", "tokens": ["Was", "soll", "ich", "a\u00b7ber", "sie", "vil", "mit", "dem", "Schnee", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mit Perlen oder ein einigem Helffenbein?", "tokens": ["Mit", "Per\u00b7len", "o\u00b7der", "ein", "ei\u00b7ni\u00b7gem", "Helf\u00b7fen\u00b7bein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In jhnen alles ist/ was irgendt sch\u00f6n magsein.", "tokens": ["In", "jh\u00b7nen", "al\u00b7les", "ist", "/", "was", "ir\u00b7gendt", "sch\u00f6n", "mag\u00b7sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "VAFIN", "$(", "PWS", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Seht hier die N\u00e4gelein/ so zierlich rund beschaffen/", "tokens": ["Seht", "hier", "die", "N\u00e4\u00b7ge\u00b7lein", "/", "so", "zier\u00b7lich", "rund", "be\u00b7schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "ADV", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der zarten Jungfrawschafft holdselig wehr vnd Waffen/", "tokens": ["Der", "zar\u00b7ten", "Jung\u00b7fraw\u00b7schafft", "hold\u00b7se\u00b7lig", "wehr", "vnd", "Waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Seht hier die hurtige gebogne Gleichelein/", "tokens": ["Seht", "hier", "die", "hur\u00b7ti\u00b7ge", "ge\u00b7bog\u00b7ne", "Glei\u00b7che\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Seht hier die Fingerlein/ die er vor Pfeil thut preisen", "tokens": ["Seht", "hier", "die", "Fin\u00b7ger\u00b7lein", "/", "die", "er", "vor", "Pfeil", "thut", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Seiner Artillerey/ ob sie sich schon erweisen", "tokens": ["Sei\u00b7ner", "Ar\u00b7til\u00b7le\u00b7rey", "/", "ob", "sie", "sich", "schon", "er\u00b7wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PPER", "PRF", "ADV", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Vngleich in jhrer leng/ seind sie doch gleich in dem/", "tokens": ["Vn\u00b7gleich", "in", "jhrer", "leng", "/", "seind", "sie", "doch", "gleich", "in", "dem", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "$("], "meter": "+--+-+-++-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Da\u00df sie vor anderen allein sein angenehm.", "tokens": ["Da\u00df", "sie", "vor", "an\u00b7de\u00b7ren", "al\u00b7lein", "sein", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ADV", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Beseht die flache Handt/ innwendig der jhr finden", "tokens": ["Be\u00b7seht", "die", "fla\u00b7che", "Handt", "/", "inn\u00b7wen\u00b7dig", "der", "jhr", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "ADJD", "ART", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Werd manch verborgne Kunst nit jedem zuergr\u00fcnden/", "tokens": ["Werd", "manch", "ver\u00b7borg\u00b7ne", "Kunst", "nit", "je\u00b7dem", "zu\u00b7er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "PTKNEG", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ihr werdet finden viel geheimnu\u00df der Natur", "tokens": ["Ihr", "wer\u00b7det", "fin\u00b7den", "viel", "ge\u00b7heim\u00b7nu\u00df", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.20": {"text": "Artig gezirckelt au\u00df/ gleich als mit einer Schnur.", "tokens": ["Ar\u00b7tig", "ge\u00b7zir\u00b7ckelt", "au\u00df", "/", "gleich", "als", "mit", "ei\u00b7ner", "Schnur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PTKVZ", "$(", "ADV", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Da stehet all mein Gl\u00fcck vnd vngl\u00fcck vffgeschrieben/", "tokens": ["Da", "ste\u00b7het", "all", "mein", "Gl\u00fcck", "vnd", "vn\u00b7gl\u00fcck", "vff\u00b7ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "PPOSAT", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da steht wie hoch ich sie/ wie hoch sie mich mu\u00df lieben/", "tokens": ["Da", "steht", "wie", "hoch", "ich", "sie", "/", "wie", "hoch", "sie", "mich", "mu\u00df", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJD", "PPER", "PPER", "$(", "PWAV", "ADJD", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So manche Lini sich allda entwerffen thut/", "tokens": ["So", "man\u00b7che", "Li\u00b7ni", "sich", "all\u00b7da", "ent\u00b7werf\u00b7fen", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PRF", "PAV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So manche Tugent helt in sich jhr Keuscher muth.", "tokens": ["So", "man\u00b7che", "Tu\u00b7gent", "helt", "in", "sich", "jhr", "Keu\u00b7scher", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "APPR", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gl\u00fcckseelig seit jhr zwar/ H\u00e4ndschuch/ denen verg\u00f6nnet", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "seit", "jhr", "zwar", "/", "H\u00e4nd\u00b7schuch", "/", "de\u00b7nen", "ver\u00b7g\u00f6n\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "ADV", "$(", "NN", "$(", "PDS", "VVFIN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Da\u00df jhr der Liebsten H\u00e4ndt so offte k\u00fcssen k\u00f6nnet/", "tokens": ["Da\u00df", "jhr", "der", "Liebs\u00b7ten", "H\u00e4ndt", "so", "off\u00b7te", "k\u00fcs\u00b7sen", "k\u00f6n\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Stoltzirt drumb aber nicht/ die Ehr so jhr empfangt", "tokens": ["Stolt\u00b7zirt", "drumb", "a\u00b7ber", "nicht", "/", "die", "Ehr", "so", "jhr", "emp\u00b7fangt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "ADV", "PTKNEG", "$(", "ART", "NN", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Von jhr allein vnd nicht von euch an euch gelangt.", "tokens": ["Von", "jhr", "al\u00b7lein", "vnd", "nicht", "von", "euch", "an", "euch", "ge\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "KON", "PTKNEG", "APPR", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Wie stattlich jhr auch seit/ wan\u0303 sie euch an jhr treget/", "tokens": ["Wie", "statt\u00b7lich", "jhr", "auch", "seit", "/", "wa\u00f1", "sie", "euch", "an", "jhr", "tre\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "APPR", "$(", "PWAV", "PPER", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Also gering jhr seit/ so bald sie euch ableget/", "tokens": ["Al\u00b7so", "ge\u00b7ring", "jhr", "seit", "/", "so", "bald", "sie", "euch", "ab\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$(", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$("], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Doch/ dieweil jhr durch mich seit gebracht zu diesem Gl\u00fcck/", "tokens": ["Doch", "/", "die\u00b7weil", "jhr", "durch", "mich", "seit", "ge\u00b7bracht", "zu", "die\u00b7sem", "Gl\u00fcck", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPER", "APPR", "PPER", "ADV", "VVPP", "APPR", "PDAT", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Last mich genie\u00dfen auch bi\u00dfweil ein guten blick.", "tokens": ["Last", "mich", "ge\u00b7nie\u00b7\u00dfen", "auch", "bi\u00df\u00b7weil", "ein", "gu\u00b7ten", "blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ADV", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}