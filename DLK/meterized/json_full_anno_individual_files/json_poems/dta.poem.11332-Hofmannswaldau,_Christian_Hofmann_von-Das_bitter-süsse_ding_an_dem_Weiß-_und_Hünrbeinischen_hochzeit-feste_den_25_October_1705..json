{"dta.poem.11332": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das bitter-s\u00fcsse ding an dem Wei\u00df- und  \n H\u00fcnrbeinischen hochzeit-feste  \n den 25 October 1705.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein grosser streit von bitter-s\u00fcssen dingen,", "tokens": ["Es", "ist", "ein", "gros\u00b7ser", "streit", "von", "bit\u00b7ter\u00b7s\u00fcs\u00b7sen", "din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was unter beyder art mit recht den vorzug hat?", "tokens": ["Was", "un\u00b7ter", "bey\u00b7der", "art", "mit", "recht", "den", "vor\u00b7zug", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PIAT", "NN", "APPR", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dort will die wermuth sich zur ersten stelle dringen,", "tokens": ["Dort", "will", "die", "wer\u00b7muth", "sich", "zur", "ers\u00b7ten", "stel\u00b7le", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hier giebt alle welt dem honig rang und statt.", "tokens": ["Und", "hier", "giebt", "al\u00b7le", "welt", "dem", "ho\u00b7nig", "rang", "und", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch hat der himmel selbst ein wunder-ding gezeuget,", "tokens": ["Doch", "hat", "der", "him\u00b7mel", "selbst", "ein", "wun\u00b7der\u00b7ding", "ge\u00b7zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das zehnmal s\u00fcsser ist als aller zucker-cand,", "tokens": ["Das", "zehn\u00b7mal", "s\u00fcs\u00b7ser", "ist", "als", "al\u00b7ler", "zu\u00b7cke\u00b7rcand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VAFIN", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dessen bitterkeit doch wermuth \u00fcbersteiget,", "tokens": ["Und", "des\u00b7sen", "bit\u00b7ter\u00b7keit", "doch", "wer\u00b7muth", "\u00fc\u00b7bers\u00b7tei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und coloquinten schimpfft: Das ist der ehestand.", "tokens": ["Und", "co\u00b7loq\u00b7uin\u00b7ten", "schimpfft", ":", "Das", "ist", "der", "e\u00b7hes\u00b7tand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man mu\u00df das bittre ding noch vor der hochzeit schmecken:", "tokens": ["Man", "mu\u00df", "das", "bitt\u00b7re", "ding", "noch", "vor", "der", "hoch\u00b7zeit", "schme\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Spendiren, putz und staat macht hier und da verdru\u00df:", "tokens": ["Spen\u00b7di\u00b7ren", ",", "putz", "und", "staat", "macht", "hier", "und", "da", "ver\u00b7dru\u00df", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "ADV", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So kan auch dieses nicht den appetit erwecken,", "tokens": ["So", "kan", "auch", "die\u00b7ses", "nicht", "den", "ap\u00b7pe\u00b7tit", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PDS", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn mancher courtisan mit k\u00f6rben handeln mu\u00df.", "tokens": ["Wenn", "man\u00b7cher", "cour\u00b7ti\u00b7san", "mit", "k\u00f6r\u00b7ben", "han\u00b7deln", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da mu\u00df ein k\u00fcnfftger mann vor seine hochzeit sorgen;", "tokens": ["Da", "mu\u00df", "ein", "k\u00fcnfft\u00b7ger", "mann", "vor", "sei\u00b7ne", "hoch\u00b7zeit", "sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was kost das ehren-kleid vor sich und vor die braut?", "tokens": ["Was", "kost", "das", "eh\u00b7ren\u00b7kleid", "vor", "sich", "und", "vor", "die", "braut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "PRF", "KON", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die k\u00fcche fordert geld, und solt\u2019 er alles borgen,", "tokens": ["Die", "k\u00fc\u00b7che", "for\u00b7dert", "geld", ",", "und", "solt'", "er", "al\u00b7les", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NN", "$,", "KON", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was kost die kammer-pracht? und wer die wohnung baut?", "tokens": ["Was", "kost", "die", "kam\u00b7mer\u00b7pracht", "?", "und", "wer", "die", "woh\u00b7nung", "baut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "KON", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie klingt der hochzeit-brief? Was krieget man vor g\u00e4ste?", "tokens": ["Wie", "klingt", "der", "hoch\u00b7zeit\u00b7brief", "?", "Was", "krie\u00b7get", "man", "vor", "g\u00e4s\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "PWS", "VVFIN", "PIS", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie wird der gantze schwarm recht zu bewirthen seyn?", "tokens": ["Wie", "wird", "der", "gant\u00b7ze", "schwarm", "recht", "zu", "be\u00b7wirt\u00b7hen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "ADJD", "ADJD", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kommt denn die herrlichkeit, so stellt sich bey dem feste", "tokens": ["Kommt", "denn", "die", "herr\u00b7lich\u00b7keit", ",", "so", "stellt", "sich", "bey", "dem", "fes\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch noch was neues stets von bittern sorgen ein:", "tokens": ["Doch", "noch", "was", "neu\u00b7es", "stets", "von", "bit\u00b7tern", "sor\u00b7gen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "ADJA", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bald tritt der marschall auf, und bald der hochzeit-bitter,", "tokens": ["Bald", "tritt", "der", "mar\u00b7schall", "auf", ",", "und", "bald", "der", "hoch\u00b7zeit\u00b7bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bald f\u00e4hrt der gast zu fr\u00fch, und bald zu spat nach hau\u00df;", "tokens": ["Bald", "f\u00e4hrt", "der", "gast", "zu", "fr\u00fch", ",", "und", "bald", "zu", "spat", "nach", "hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "PTKA", "ADJD", "$,", "KON", "ADV", "PTKZU", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jtzt schm\u00e4let jung und magd, itzt sehn die t\u00fctsche-m\u00fctter,", "tokens": ["Jtzt", "schm\u00e4\u00b7let", "jung", "und", "magd", ",", "itzt", "sehn", "die", "t\u00fct\u00b7sche\u00b7m\u00fct\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und bald der musicant mehr als catonisch aus;", "tokens": ["Und", "bald", "der", "mu\u00b7si\u00b7cant", "mehr", "als", "ca\u00b7to\u00b7nisch", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PIAT", "KOKOM", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man sorget vor den tantz. Wills dann ins bette gehen,", "tokens": ["Man", "sor\u00b7get", "vor", "den", "tantz", ".", "Wills", "dann", "ins", "bet\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$.", "VMFIN", "ADV", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "So findt sich tausend noth: Noth um die erste krafft;", "tokens": ["So", "findt", "sich", "tau\u00b7send", "noth", ":", "Noth", "um", "die", "ers\u00b7te", "krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "CARD", "NN", "$.", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Noth, wie das nacht-gezeug und andre sachen stehen;", "tokens": ["Noth", ",", "wie", "das", "nacht\u00b7ge\u00b7zeug", "und", "and\u00b7re", "sa\u00b7chen", "ste\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "KON", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Noth endlich um die scham: Noth um die jungferschafft.", "tokens": ["Noth", "end\u00b7lich", "um", "die", "scham", ":", "Noth", "um", "die", "jung\u00b7fer\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJD", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Nach diesem heist es erst: Schafft l\u00f6ffel, sch\u00fcssel, tiegel!", "tokens": ["Nach", "die\u00b7sem", "heist", "es", "erst", ":", "Schafft", "l\u00f6f\u00b7fel", ",", "sch\u00fcs\u00b7sel", ",", "tie\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADV", "$.", "NN", "NE", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sorgt vor den kleider-schmuck! schafft bette, holtz und licht!", "tokens": ["Sorgt", "vor", "den", "klei\u00b7der\u00b7schmuck", "!", "schafft", "bet\u00b7te", ",", "holtz", "und", "licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "VVFIN", "VVFIN", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der frau die junge magd, und auch die sch\u00f6nsten spiegel!", "tokens": ["Der", "frau", "die", "jun\u00b7ge", "magd", ",", "und", "auch", "die", "sch\u00f6ns\u00b7ten", "spie\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Es fehle, was da will, nur an dem staate nicht.", "tokens": ["Es", "feh\u00b7le", ",", "was", "da", "will", ",", "nur", "an", "dem", "staa\u00b7te", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "VMFIN", "$,", "ADV", "APPR", "ART", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dann f\u00e4ngt das neue weib allm\u00e4hlich an zu siechen;", "tokens": ["Dann", "f\u00e4ngt", "das", "neu\u00b7e", "weib", "all\u00b7m\u00e4h\u00b7lich", "an", "zu", "sie\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie merckt, da\u00df sich der schmertz in haupt und seite regt;", "tokens": ["Sie", "merckt", ",", "da\u00df", "sich", "der", "schmertz", "in", "haupt", "und", "sei\u00b7te", "regt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie kan durchaus kein fleisch vor grossem eckel riechen;", "tokens": ["Sie", "kan", "durc\u00b7haus", "kein", "fleisch", "vor", "gros\u00b7sem", "ec\u00b7kel", "rie\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sie f\u00fchlet, da\u00df das hertz in ihr gedoppelt schl\u00e4gt.", "tokens": ["Sie", "f\u00fch\u00b7let", ",", "da\u00df", "das", "hertz", "in", "ihr", "ge\u00b7dop\u00b7pelt", "schl\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die stunde r\u00fcckt heran, die wiege zu bestellen,", "tokens": ["Die", "stun\u00b7de", "r\u00fcckt", "he\u00b7ran", ",", "die", "wie\u00b7ge", "zu", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das bett und wochen-zeug will eingekauffet seyn,", "tokens": ["Das", "bett", "und", "wo\u00b7chen\u00b7zeug", "will", "ein\u00b7ge\u00b7kauf\u00b7fet", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Man kaufft die windel-schnur von mehr als vierzehn ellen,", "tokens": ["Man", "kaufft", "die", "win\u00b7del\u00b7schnur", "von", "mehr", "als", "vier\u00b7zehn", "el\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "PIAT", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und mutter Liese b\u00fc\u00dft nicht einen zweyer ein.", "tokens": ["Und", "mut\u00b7ter", "Lie\u00b7se", "b\u00fc\u00dft", "nicht", "ei\u00b7nen", "zwey\u00b7er", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Kommt nun der klapper-storch, so kommen neue grillen", "tokens": ["Kommt", "nun", "der", "klap\u00b7per\u00b7storch", ",", "so", "kom\u00b7men", "neu\u00b7e", "gril\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Vor kind und w\u00f6chnerin, wer soll gevatter stehn?", "tokens": ["Vor", "kind", "und", "w\u00f6ch\u00b7ne\u00b7rin", ",", "wer", "soll", "ge\u00b7vat\u00b7ter", "stehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWS", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die amme wird gedingt, das h\u00e4nselgen zu stillen,", "tokens": ["Die", "am\u00b7me", "wird", "ge\u00b7dingt", ",", "das", "h\u00e4n\u00b7sel\u00b7gen", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PDS", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ein kinder-m\u00e4gdgen mu\u00df der frau zu handen gehn;", "tokens": ["Ein", "kin\u00b7der\u0