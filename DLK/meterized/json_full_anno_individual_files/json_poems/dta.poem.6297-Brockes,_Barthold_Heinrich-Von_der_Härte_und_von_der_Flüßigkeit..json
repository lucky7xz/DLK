{"dta.poem.6297": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zerbrechliche Gef\u00e4\u00df\u2019, indem sie innerlich", "tokens": ["Zer\u00b7brech\u00b7li\u00b7che", "Ge\u00b7f\u00e4\u00b7\u00df'", ",", "in\u00b7dem", "sie", "in\u00b7ner\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu sehr gepresst, zu sehr gedr\u00fccket,", "tokens": ["Zu", "sehr", "ge\u00b7presst", ",", "zu", "sehr", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVPP", "$,", "PTKA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die werden durch das Na\u00df, indem es sich", "tokens": ["Die", "wer\u00b7den", "durch", "das", "Na\u00df", ",", "in\u00b7dem", "es", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jm frieren ausgedehnt, zerknicket und zerst\u00fccket.", "tokens": ["Jm", "frie\u00b7ren", "aus\u00b7ge\u00b7dehnt", ",", "zer\u00b7kni\u00b7cket", "und", "zer\u00b7st\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}