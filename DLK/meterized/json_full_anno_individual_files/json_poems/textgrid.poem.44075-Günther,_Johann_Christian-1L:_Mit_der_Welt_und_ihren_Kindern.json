{"textgrid.poem.44075": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit der Welt und ihren Kindern", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der Welt und ihren Kindern", "tokens": ["Mit", "der", "Welt", "und", "ih\u00b7ren", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mach ich mich nicht gern gemein,", "tokens": ["Mach", "ich", "mich", "nicht", "gern", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sie mir die Ruh verhindern", "tokens": ["Weil", "sie", "mir", "die", "Ruh", "ver\u00b7hin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und oft Schmach vor Danck verleihn.", "tokens": ["Und", "oft", "Schmach", "vor", "Danck", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will man mich darum verdencken,", "tokens": ["Will", "man", "mich", "da\u00b7rum", "ver\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird es mich so sehr nicht kr\u00e4ncken.", "tokens": ["Wird", "es", "mich", "so", "sehr", "nicht", "kr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In der stillen Einsamkeit", "tokens": ["In", "der", "stil\u00b7len", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6r ich weder Hohn noch Neid.", "tokens": ["H\u00f6r", "ich", "we\u00b7der", "Hohn", "noch", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit mir selbst und meinem Gotte", "tokens": ["Mit", "mir", "selbst", "und", "mei\u00b7nem", "Got\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bring ich s\u00fc\u00dfe Stunden zu,", "tokens": ["Bring", "ich", "s\u00fc\u00b7\u00dfe", "Stun\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich mit verg\u00e4lltem Spotte", "tokens": ["Wo", "ich", "mit", "ver\u00b7g\u00e4ll\u00b7tem", "Spot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinem Menschen Unrecht thu.", "tokens": ["Kei\u00b7nem", "Men\u00b7schen", "Un\u00b7recht", "thu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In Gesellschaft lernt man S\u00fcnden", "tokens": ["In", "Ge\u00b7sell\u00b7schaft", "lernt", "man", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Weg zur H\u00f6lle finden,", "tokens": ["Und", "den", "Weg", "zur", "H\u00f6l\u00b7le", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bey Exempeln b\u00f6ser Art", "tokens": ["Bey", "Ex\u00b7em\u00b7peln", "b\u00f6\u00b7ser", "Art"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+----+", "measure": "dactylic.init"}, "line.8": {"text": "Wird die Unschuld schwer bewahrt.", "tokens": ["Wird", "die", "Un\u00b7schuld", "schwer", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fchren mich die besten Schwestern", "tokens": ["F\u00fch\u00b7ren", "mich", "die", "bes\u00b7ten", "Schwes\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Je zuweilen in ihr Haus,", "tokens": ["Je", "zu\u00b7wei\u00b7len", "in", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r ich Lachen oder L\u00e4stern,", "tokens": ["H\u00f6r", "ich", "La\u00b7chen", "o\u00b7der", "L\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beides geht auf Thorheit aus.", "tokens": ["Bei\u00b7des", "geht", "auf", "Thor\u00b7heit", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwo verschneiden stets die Dritte,", "tokens": ["Zwo", "ver\u00b7schnei\u00b7den", "stets", "die", "Drit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mienen, Worte, Kleid und Schritte,", "tokens": ["Mie\u00b7nen", ",", "Wor\u00b7te", ",", "Kleid", "und", "Schrit\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Klein und Gro\u00df und Alt und Jung", "tokens": ["Klein", "und", "Gro\u00df", "und", "Alt", "und", "Jung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "KON", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00fc\u00dfen durch die Musterung.", "tokens": ["M\u00fc\u00b7\u00dfen", "durch", "die", "Mus\u00b7te\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Glaubt man nicht geschminckten L\u00fcgen,", "tokens": ["Glaubt", "man", "nicht", "ge\u00b7schminck\u00b7ten", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heist es Einfalt oder Stolz;", "tokens": ["Heist", "es", "Ein\u00b7falt", "o\u00b7der", "Stolz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan man nicht galant betriegen,", "tokens": ["Kan", "man", "nicht", "ga\u00b7lant", "be\u00b7trie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nennt man uns ein grobes Holz.", "tokens": ["Nennt", "man", "uns", "ein", "gro\u00b7bes", "Holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sucht man ein vertraulich Herze,", "tokens": ["Sucht", "man", "ein", "ver\u00b7trau\u00b7lich", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So erf\u00e4hrt man sich zum Schmerze,", "tokens": ["So", "er\u00b7f\u00e4hrt", "man", "sich", "zum", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df der Ku\u00df Verr\u00e4therey", "tokens": ["Da\u00df", "der", "Ku\u00df", "Ver\u00b7r\u00e4\u00b7the\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Freundschaft Arglist sey.", "tokens": ["Und", "die", "Freund\u00b7schaft", "Arg\u00b7list", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Reizt mich nicht aus meinem Zimmer", "tokens": ["Reizt", "mich", "nicht", "aus", "mei\u00b7nem", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Lust der Eitelkeit;", "tokens": ["Durch", "die", "Lust", "der", "Ei\u00b7tel\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn hier bleib ich jezt und immer", "tokens": ["Denn", "hier", "bleib", "ich", "jezt", "und", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Selbstzufriedenheit.", "tokens": ["Mit", "der", "Selbst\u00b7zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier bespricht sich mein Gem\u00fcthe", "tokens": ["Hier", "be\u00b7spricht", "sich", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit des Himmels Vaterg\u00fcte,", "tokens": ["Mit", "des", "Him\u00b7mels", "Va\u00b7ter\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die mich leicht das Fastnachtsfest", "tokens": ["Die", "mich", "leicht", "das", "Fast\u00b7nachts\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieser Welt verlachen l\u00e4st.", "tokens": ["Die\u00b7ser", "Welt", "ver\u00b7la\u00b7chen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit der Welt und ihren Kindern", "tokens": ["Mit", "der", "Welt", "und", "ih\u00b7ren", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mach ich mich nicht gern gemein,", "tokens": ["Mach", "ich", "mich", "nicht", "gern", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sie mir die Ruh verhindern", "tokens": ["Weil", "sie", "mir", "die", "Ruh", "ver\u00b7hin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und oft Schmach vor Danck verleihn.", "tokens": ["Und", "oft", "Schmach", "vor", "Danck", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will man mich darum verdencken,", "tokens": ["Will", "man", "mich", "da\u00b7rum", "ver\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird es mich so sehr nicht kr\u00e4ncken.", "tokens": ["