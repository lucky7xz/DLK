{"textgrid.poem.57691": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rchen", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Heideh\u00fcgel geht ein Singen,", "tokens": ["Am", "Hei\u00b7de\u00b7h\u00fc\u00b7gel", "geht", "ein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leises Singen her und hin,", "tokens": ["Ein", "lei\u00b7ses", "Sin\u00b7gen", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wiegt in einer goldenen Wiege", "tokens": ["Da", "wiegt", "in", "ei\u00b7ner", "gol\u00b7de\u00b7nen", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Kind die Zwergenk\u00f6nigin.", "tokens": ["Ihr", "Kind", "die", "Zwer\u00b7gen\u00b7k\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich denke an das alte M\u00e4rchen,", "tokens": ["Ich", "den\u00b7ke", "an", "das", "al\u00b7te", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es liegt mein Kopf in deinem Scho\u00df,", "tokens": ["Es", "liegt", "mein", "Kopf", "in", "dei\u00b7nem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Mund singt mir ein Wiegenliedchen,", "tokens": ["Dein", "Mund", "singt", "mir", "ein", "Wie\u00b7gen\u00b7lied\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Augen werden gro\u00df.", "tokens": ["Und", "mei\u00b7ne", "Au\u00b7gen", "wer\u00b7den", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Herz, das ist so still und selig,", "tokens": ["Mein", "Herz", ",", "das", "ist", "so", "still", "und", "se\u00b7lig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein goldener Traum dar\u00fcber fliegt,", "tokens": ["Ein", "gol\u00b7de\u00b7ner", "Traum", "da\u00b7r\u00fc\u00b7ber", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es liegt in einer goldnen Wiege,", "tokens": ["Es", "liegt", "in", "ei\u00b7ner", "gold\u00b7nen", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die langsam hin und her sich wiegt.", "tokens": ["Die", "