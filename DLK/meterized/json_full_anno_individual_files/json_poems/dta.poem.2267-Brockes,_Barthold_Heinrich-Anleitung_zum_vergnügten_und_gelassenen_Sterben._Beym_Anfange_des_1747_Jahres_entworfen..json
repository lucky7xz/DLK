{"dta.poem.2267": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anleitung  \n zum vergn\u00fcgten und gelassenen  \n Sterben.  \n Beym Anfange des 1747 Jahres entworfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Merkt, wie sich des Sch\u00f6pfers Weisheit, in dem", "tokens": ["Merkt", ",", "wie", "sich", "des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "in", "dem"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "NN", "$,", "APPR", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tode selbst, entdeckt!", "tokens": ["To\u00b7de", "selbst", ",", "ent\u00b7deckt", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zieht ihm ab die schwarze Larve, welche nur die Thoren", "tokens": ["Zieht", "ihm", "ab", "die", "schwar\u00b7ze", "Lar\u00b7ve", ",", "wel\u00b7che", "nur", "die", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "schreckt.", "tokens": ["schreckt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Aus der Furcht des Todes blo\u00df stammt das allgemeine", "tokens": ["Aus", "der", "Furcht", "des", "To\u00b7des", "blo\u00df", "stammt", "das", "all\u00b7ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Klagen.", "tokens": ["Kla\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Was sind in dem Tode selbst doch f\u00fcr wesentliche Pla-", "tokens": ["Was", "sind", "in", "dem", "To\u00b7de", "selbst", "doch", "f\u00fcr", "we\u00b7sent\u00b7li\u00b7che", "Pla"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+---+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "gen?", "tokens": ["gen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Wir selbst scheuchen uns einander, und es reibet jeder-", "tokens": ["Wir", "selbst", "scheu\u00b7chen", "uns", "ein\u00b7an\u00b7der", ",", "und", "es", "rei\u00b7bet", "je\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "PPER", "VVFIN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "mann", "tokens": ["mann"], "token_info": ["word"], "pos": ["NN"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Sein gef\u00fchltes \u00e4ngstlichs Grauen insgemein dem andern an,", "tokens": ["Sein", "ge\u00b7f\u00fchl\u00b7tes", "\u00e4ngst\u00b7lichs", "Grau\u00b7en", "ins\u00b7ge\u00b7mein", "dem", "an\u00b7dern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Ja man \u00fcbertr\u00e4gt sich gleichsam diese Furcht von Hand", "tokens": ["Ja", "man", "\u00fc\u00b7ber\u00b7tr\u00e4gt", "sich", "gleich\u00b7sam", "die\u00b7se", "Furcht", "von", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIS", "VVFIN", "PRF", "ADJD", "PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "zu Hand", "tokens": ["zu", "Hand"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Allemal annoch vergr\u00f6\u00dfert, um dieselbe, wie bekannt,", "tokens": ["Al\u00b7le\u00b7mal", "an\u00b7noch", "ver\u00b7gr\u00f6\u00b7\u00dfert", ",", "um", "die\u00b7sel\u00b7be", ",", "wie", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "KOUI", "PDAT", "$,", "PWAV", "ADJD", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Jmmer mehr noch zu vergr\u00f6\u00dfern. Wie viel Dichter,", "tokens": ["Jm\u00b7mer", "mehr", "noch", "zu", "ver\u00b7gr\u00f6\u00b7\u00dfern", ".", "Wie", "viel", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKZU", "VVINF", "$.", "PWAV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "wie wir lesen,", "tokens": ["wie", "wir", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Wie viel Mahler, wie wir sehn, sind so sinnreich stets", "tokens": ["Wie", "viel", "Mah\u00b7ler", ",", "wie", "wir", "sehn", ",", "sind", "so", "sinn\u00b7reich", "stets"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PPER", "VVINF", "$,", "VAFIN", "ADV", "ADJD", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "gewesen,", "tokens": ["ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VAPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Jhn stets sch\u00e4ndlicher zu bilden, scheu\u00dflicher stets vorzu-", "tokens": ["Jhn", "stets", "sch\u00e4nd\u00b7li\u00b7cher", "zu", "bil\u00b7den", ",", "scheu\u00df\u00b7li\u00b7cher", "stets", "vor\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "ADJD", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "stellen;", "tokens": ["stel\u00b7len", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Mehrentheils sind ihre Werke unsers bangen Jrrthums", "tokens": ["Meh\u00b7ren\u00b7theils", "sind", "ih\u00b7re", "Wer\u00b7ke", "un\u00b7sers", "ban\u00b7gen", "Jrr\u00b7thums"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.22": {"text": "Quellen.", "tokens": ["Quel\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "La\u00dft uns doch einmal erw\u00e4gen, ob der Tod denn in der", "tokens": ["La\u00dft", "uns", "doch", "ein\u00b7mal", "er\u00b7w\u00e4\u00b7gen", ",", "ob", "der", "Tod", "denn", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF", "$,", "KOUS", "ART", "NN", "KON", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.24": {"text": "That", "tokens": ["That"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Etwas, das so f\u00fcrchterlich und so scheu\u00dflich, an sich hat.", "tokens": ["Et\u00b7was", ",", "das", "so", "f\u00fcrch\u00b7ter\u00b7lich", "und", "so", "scheu\u00df\u00b7lich", ",", "an", "sich", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "APPR", "PRF", "VAFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}