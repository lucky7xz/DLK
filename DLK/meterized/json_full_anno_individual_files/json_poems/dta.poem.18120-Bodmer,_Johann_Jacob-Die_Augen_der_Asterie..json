{"dta.poem.18120": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Augen der Asterie.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Asteris bey Nacht den Himmel angesehen,", "tokens": ["Als", "As\u00b7te\u00b7ris", "bey", "Nacht", "den", "Him\u00b7mel", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat sie der Sternen Zahl vermehrt durch ihren Schein,", "tokens": ["Hat", "sie", "der", "Ster\u00b7nen", "Zahl", "ver\u00b7mehrt", "durch", "ih\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vermagst du das, mein Lieb, wie mag es dann geschehen,", "tokens": ["Ver\u00b7magst", "du", "das", ",", "mein", "Lieb", ",", "wie", "mag", "es", "dann", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PPOSAT", "NN", "$,", "PWAV", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df mein Gesicht vergeht von deinen Aeugelein?", "tokens": ["Da\u00df", "mein", "Ge\u00b7sicht", "ver\u00b7geht", "von", "dei\u00b7nen", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}