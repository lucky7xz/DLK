"token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "NE", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Kumm her, wi spelen \u00fcm twei Gr\u00f6schen!\u00ab", "tokens": ["Kumm", "her", ",", "wi", "spe\u00b7len", "\u00fcm", "twei", "Gr\u00f6\u00b7schen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Na, dat is gaud, sei setten sick,", "tokens": ["Na", ",", "dat", "is", "gaud", ",", "sei", "set\u00b7ten", "sick", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM.nl", "FM.nl", "FM.nl", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Un Kopmann Hahn verl\u00fcst dat Spill.", "tokens": ["Un", "Kop\u00b7mann", "Hahn", "ver\u00b7l\u00fcst", "dat", "Spill", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un wil hei'n Spa\u00df sick maken will,", "tokens": ["Un", "wil", "hei'n", "Spa\u00df", "sick", "ma\u00b7ken", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADJA", "NN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Seggt hei tau'n Gastwirt: \u00bbH\u00fcr mal, Knop,", "tokens": ["Seggt", "hei", "tau'n", "Gast\u00b7wirt", ":", "\u00bb", "H\u00fcr", "mal", ",", "Knop", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "ADV", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dit rekent sick ganz licht tauhop:", "tokens": ["Dit", "re\u00b7kent", "sick", "ganz", "licht", "tau\u00b7hop", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Twei Gr\u00f6schen heww'ck an di verluren,", "tokens": ["Twei", "Gr\u00f6\u00b7schen", "hew\u00b7w'ck", "an", "di", "ver\u00b7lu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Twei Gr\u00f6schen kriggst du v\u00f6r den Win,", "tokens": ["Twei", "Gr\u00f6\u00b7schen", "kriggst", "du", "v\u00f6r", "den", "Win", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dat geiht grad up, dat hewt sick just.\u00ab", "tokens": ["Dat", "geiht", "grad", "up", ",", "dat", "hewt", "sick", "just", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "Oll Knop, de kratzt sick acht're Uhren:", "tokens": ["Oll", "Knop", ",", "de", "kratzt", "sick", "acht'\u00b7re", "Uh\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbja\u00ab, seggt hei, \u00bbdat ward richtig sin:", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "hei", ",", "\u00bb", "dat", "ward", "rich\u00b7tig", "sin", ":"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "PTKVZ", "$,", "$(", "PAV", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.22": {"text": "Min Virtel Win un din Verlust,", "tokens": ["Min", "Vir\u00b7tel", "Win", "un", "din", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ja, du hest recht, dat hewt sick just.\u00ab", "tokens": ["Ja", ",", "du", "hest", "recht", ",", "dat", "hewt", "sick", "just", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADJD", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Indessen ward hei doch noch mal", "tokens": ["In\u00b7des\u00b7sen", "ward", "hei", "doch", "noch", "mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "ADV", "ADV", "ADV"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "De Reknung heimlich \u00e4werslagen,", "tokens": ["De", "Re\u00b7knung", "heim\u00b7lich", "\u00e4\u00b7wers\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Un as de Kopmann gahen will,", "tokens": ["Un", "as", "de", "Kop\u00b7mann", "ga\u00b7hen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE"