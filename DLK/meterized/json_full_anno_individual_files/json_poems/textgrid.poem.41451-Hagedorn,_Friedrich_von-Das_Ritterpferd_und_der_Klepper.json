{"textgrid.poem.41451": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Ritterpferd und der Klepper", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kriegerisches Pferd, die Lust der Ritterschaft,", "tokens": ["Ein", "krie\u00b7ge\u00b7ri\u00b7sches", "Pferd", ",", "die", "Lust", "der", "Rit\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War w\u00fcrdig seiner Zucht, und freudig, voller Kraft,", "tokens": ["War", "w\u00fcr\u00b7dig", "sei\u00b7ner", "Zucht", ",", "und", "freu\u00b7dig", ",", "vol\u00b7ler", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$,", "KON", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War gleich an Muth und Kunst, an Dauer und Verm\u00f6gen,", "tokens": ["War", "gleich", "an", "Muth", "und", "Kunst", ",", "an", "Dau\u00b7er", "und", "Ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zog aus, und wieherte Geharnischten entgegen,", "tokens": ["Zog", "aus", ",", "und", "wie\u00b7her\u00b7te", "Ge\u00b7har\u00b7nischten", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "ADJA", "NN",