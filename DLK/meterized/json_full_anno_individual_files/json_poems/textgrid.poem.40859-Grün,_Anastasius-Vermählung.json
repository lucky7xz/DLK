{"textgrid.poem.40859": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Verm\u00e4hlung", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wogt im Dom zu Br\u00fcgge um S\u00e4ulen und Altar", "tokens": ["Wie", "wogt", "im", "Dom", "zu", "Br\u00fcg\u00b7ge", "um", "S\u00e4u\u00b7len", "und", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von vielen tausend Kerzen ein Lichtstrom wunderbar!", "tokens": ["Von", "vie\u00b7len", "tau\u00b7send", "Ker\u00b7zen", "ein", "Licht\u00b7strom", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie sind der Priester Schaaren in hoher Pracht geschm\u00fcckt,", "tokens": ["Wie", "sind", "der", "Pries\u00b7ter", "Schaa\u00b7ren", "in", "ho\u00b7her", "Pracht", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie gl\u00e4nzt der Zug so herrlich, der gegen die Kirche r\u00fcckt!", "tokens": ["Wie", "gl\u00e4nzt", "der", "Zug", "so", "herr\u00b7lich", ",", "der", "ge\u00b7gen", "die", "Kir\u00b7che", "r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Vorn flattert hochgewaltig des Doppelpanieres Glanz,", "tokens": ["Vorn", "flat\u00b7tert", "hoch\u00b7ge\u00b7wal\u00b7tig", "des", "Dop\u00b7pel\u00b7pa\u00b7nie\u00b7res", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da prangt bei Oestreichs Purpur Burgunds Goldlilienkranz;", "tokens": ["Da", "prangt", "bei", "O\u00b7e\u00b7streichs", "Pur\u00b7pur", "Bur\u00b7gunds", "Gold\u00b7li\u00b7li\u00b7en\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "NE", "NE", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Wohl stark ist solcher V\u00f6lker und solcher L\u00e4nder Band,", "tokens": ["Wohl", "stark", "ist", "sol\u00b7cher", "V\u00f6l\u00b7ker", "und", "sol\u00b7cher", "L\u00e4n\u00b7der", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch fester noch und st\u00e4rker der Kranz, den Liebe wand!", "tokens": ["Doch", "fes\u00b7ter", "noch", "und", "st\u00e4r\u00b7ker", "der", "Kranz", ",", "den", "Lie\u00b7be", "wand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KON", "ADJD", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Herold' aus siebzig L\u00e4ndern mit Bannern ihres Lands,", "tokens": ["He\u00b7rold'", "aus", "sieb\u00b7zig", "L\u00e4n\u00b7dern", "mit", "Ban\u00b7nern", "ih\u00b7res", "Lands", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von Rittern, blank ger\u00fcstet, ein herrlich bl\u00fch'nder Kranz,", "tokens": ["Von", "Rit\u00b7tern", ",", "blank", "ge\u00b7r\u00fcs\u00b7tet", ",", "ein", "herr\u00b7lich", "bl\u00fch'n\u00b7der", "Kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVPP", "$,", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die ritten ernst und schweigend, von Gottes Hauch umlauscht;", "tokens": ["Die", "rit\u00b7ten", "ernst", "und", "schwei\u00b7gend", ",", "von", "Got\u00b7tes", "Hauch", "um\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Rosse nur scharr'n und schnauben, nur Waff' und R\u00fcstung rauscht", "tokens": ["Die", "Ros\u00b7se", "nur", "scharr'n", "und", "schnau\u00b7ben", ",", "nur", "Waff'", "und", "R\u00fcs\u00b7tung", "rauscht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF", "KON", "VVFIN", "$,", "ADV", "NN", "KON", "NN", "VVFIN"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf Helmen und auf Bannern wankt lustig gr\u00fcnes Reis,", "tokens": ["Auf", "Hel\u00b7men", "und", "auf", "Ban\u00b7nern", "wankt", "lus\u00b7tig", "gr\u00fc\u00b7nes", "Reis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Viel hundert Rosse steigen, wie Quellenschaum so wei\u00df,", "tokens": ["Viel", "hun\u00b7dert", "Ros\u00b7se", "stei\u00b7gen", ",", "wie", "Quel\u00b7len\u00b7schaum", "so", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVINF", "$,", "PWAV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Viel hundert Panzer gl\u00e4nzen, wie Schnee im Vollmondschein,", "tokens": ["Viel", "hun\u00b7dert", "Pan\u00b7zer", "gl\u00e4n\u00b7zen", ",", "wie", "Schnee", "im", "Voll\u00b7mond\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVINF", "$,", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Harfenpsalme s\u00e4useln wie Wellengeriesel darein:", "tokens": ["Und", "Har\u00b7fen\u00b7psal\u00b7me", "s\u00e4u\u00b7seln", "wie", "Wel\u00b7len\u00b7ge\u00b7rie\u00b7sel", "da\u00b7rein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KOKOM", "NN", "PAV", "$."], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "K\u00e4m' eine M\u00f6ve gezogen dar\u00fcber im luft'gen Dome,", "tokens": ["K\u00e4m'", "ei\u00b7ne", "M\u00f6\u00b7ve", "ge\u00b7zo\u00b7gen", "da\u00b7r\u00fc\u00b7ber", "im", "luft'\u00b7gen", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "PAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Schnell w\u00e4r sie herabgeflogen zu baden im Silberstrome;", "tokens": ["Schnell", "w\u00e4r", "sie", "her\u00b7ab\u00b7ge\u00b7flo\u00b7gen", "zu", "ba\u00b7den", "im", "Sil\u00b7ber\u00b7stro\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVINF", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Sprosser, dessen Klaglied von jenem Balkone schallt,", "tokens": ["Der", "Spros\u00b7ser", ",", "des\u00b7sen", "Klag\u00b7lied", "von", "je\u00b7nem", "Bal\u00b7ko\u00b7ne", "schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der meint, da unten bl\u00fche ein junger Lorbeerwald.", "tokens": ["Der", "meint", ",", "da", "un\u00b7ten", "bl\u00fc\u00b7he", "ein", "jun\u00b7ger", "Lor\u00b7beer\u00b7wald", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Seht dort in dreien Wagen Schalksnarrn vor\u00fcberlenken,", "tokens": ["Seht", "dort", "in", "drei\u00b7en", "Wa\u00b7gen", "Schalks\u00b7narrn", "vor\u00b7\u00fc\u00b7ber\u00b7len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die sitzen still und sittsam, wie M\u00f6nch' in Zellenschr\u00e4nken,", "tokens": ["Die", "sit\u00b7zen", "still", "und", "sitt\u00b7sam", ",", "wie", "M\u00f6n\u00b7ch'", "in", "Zel\u00b7len\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "$,", "PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Selbst Kunzen, ihrem Meister, will nun kein Schwank gedeihn;", "tokens": ["Selbst", "Kun\u00b7zen", ",", "ih\u00b7rem", "Meis\u00b7ter", ",", "will", "nun", "kein", "Schwank", "ge\u00b7deihn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPOSAT", "NN", "$,", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dem Narren ist's ein Festtag, braucht er nicht Narr zu sein.", "tokens": ["Dem", "Nar\u00b7ren", "ist's", "ein", "Fest\u00b7tag", ",", "braucht", "er", "nicht", "Narr", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Drauf Meister edlen Waidwerks und lust'ger Mummerei'n, \u2013", "tokens": ["Drauf", "Meis\u00b7ter", "ed\u00b7len", "Waid\u00b7werks", "und", "lust'\u00b7ger", "Mum\u00b7me\u00b7rei'n", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du ludest selbst zum Feste Bewohner des Kerkers ein;", "tokens": ["Du", "lu\u00b7dest", "selbst", "zum", "Fes\u00b7te", "Be\u00b7woh\u00b7ner", "des", "Ker\u00b7kers", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den Lichthauch Gottes zu schauen, zu saugen, edler F\u00fcrst,", "tokens": ["Den", "Licht\u00b7hauch", "Got\u00b7tes", "zu", "schau\u00b7en", ",", "zu", "sau\u00b7gen", ",", "ed\u00b7ler", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er\u00f6ffnest du ihr Grabthor, und ihre Fessel birst!", "tokens": ["Er\u00b7\u00f6ff\u00b7nest", "du", "ihr", "Grab\u00b7thor", ",", "und", "ih\u00b7re", "Fes\u00b7sel", "birst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.8": {"line.1": {"text": "Wie gl\u00fchn des Brautpaars Kronen von funkelndem Edelgestein,", "tokens": ["Wie", "gl\u00fchn", "des", "Braut\u00b7paars", "Kro\u00b7nen", "von", "fun\u00b7keln\u00b7dem", "E\u00b7del\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie leuchtet noch viel heller der Augen lichter Schein!", "tokens": ["Wie", "leuch\u00b7tet", "noch", "viel", "hel\u00b7ler", "der", "Au\u00b7gen", "lich\u00b7ter", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADJD", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie sind so still die Lippen, doch sprechend so sch\u00f6n und laut!", "tokens": ["Wie", "sind", "so", "still", "die", "Lip\u00b7pen", ",", "doch", "spre\u00b7chend", "so", "sch\u00f6n", "und", "laut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nichts wei\u00df von ihrem Schmucke, wer in ihr Antlitz schaut. \u2013", "tokens": ["Nichts", "wei\u00df", "von", "ih\u00b7rem", "Schmu\u00b7cke", ",", "wer", "in", "ihr", "Ant\u00b7litz", "schaut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da sprach ein greiser Bischof den Segen im Gotteshaus,", "tokens": ["Da", "sprach", "ein", "grei\u00b7ser", "Bi\u00b7schof", "den", "Se\u00b7gen", "im", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Drauf tauschten Braut und Br\u00e4ut'gam die goldnen Ringlein aus;", "tokens": ["Drauf", "tauschten", "Braut", "und", "Br\u00e4ut'\u00b7gam", "die", "gold\u00b7nen", "Rin\u00b7glein", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "KON", "NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da barst der Ringe einer \u2013 das deutet Gutes nicht! \u2013", "tokens": ["Da", "barst", "der", "Rin\u00b7ge", "ei\u00b7ner", "\u2013", "das", "deu\u00b7tet", "Gu\u00b7tes", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "$(", "PDS", "VVFIN", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und einem der Ministranten erlosch der Kerze Licht.", "tokens": ["Und", "ei\u00b7nem", "der", "Mi\u00b7nis\u00b7tran\u00b7ten", "er\u00b7losch", "der", "Ker\u00b7ze", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Des Nachts, als Gott den Himmel mit vielen Lichtern erhellt,", "tokens": ["Des", "Nachts", ",", "als", "Gott", "den", "Him\u00b7mel", "mit", "vie\u00b7len", "Lich\u00b7tern", "er\u00b7hellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "NN", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da wurden fast zu Br\u00fcgge mehr Lichter aufgestellt,", "tokens": ["Da", "wur\u00b7den", "fast", "zu", "Br\u00fcg\u00b7ge", "mehr", "Lich\u00b7ter", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"te