{"dta.poem.8778": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "W\u00e4in\u00e4mdinens Harfe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Rudert eines Tags auf S\u00fcmpfen,", "tokens": ["Ru\u00b7dert", "ei\u00b7nes", "Tags", "auf", "S\u00fcmp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf Seen des andern Tages,", "tokens": ["Und", "auf", "Seen", "des", "an\u00b7dern", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am dritten Tag im Meere,", "tokens": ["Und", "am", "drit\u00b7ten", "Tag", "im", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Stehend auf des Hechtes Schultern,", "tokens": ["Ste\u00b7hend", "auf", "des", "Hech\u00b7tes", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf des rothen Lachses Finnen.", "tokens": ["Auf", "des", "ro\u00b7then", "Lach\u00b7ses", "Fin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er beginnt, den Sohn zu fragen:", "tokens": ["Er", "be\u00b7ginnt", ",", "den", "Sohn", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Stehn auf Reisig oder Stein wir,", "tokens": ["Stehn", "auf", "Rei\u00b7sig", "o\u00b7der", "Stein", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder auf des Hechtes Schultern,", "tokens": ["O\u00b7der", "auf", "des", "Hech\u00b7tes", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Oder auf des Lachses Finnen?", "tokens": ["O\u00b7der", "auf", "des", "Lach\u00b7ses", "Fin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und der Sohn erwiedert eilig:", "tokens": ["Und", "der", "Sohn", "er\u00b7wie\u00b7dert", "ei\u00b7lig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht auf Stein und nicht auf Reisig,", "tokens": ["Nicht", "auf", "Stein", "und", "nicht", "auf", "Rei\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "PTKNEG", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auf des Hechtes festen Schultern,", "tokens": ["Auf", "des", "Hech\u00b7tes", "fes\u00b7ten", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Auf des rothen Lachses Finnen.", "tokens": ["Auf", "des", "ro\u00b7then", "Lach\u00b7ses", "Fin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "W\u00e4in\u00e4m\u00f6inen selbst, der alte,", "tokens": ["W\u00e4i\u00b7n\u00e4\u00b7m\u00f6i\u00b7nen", "selbst", ",", "der", "al\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Stie\u00df das Schwert in's Meer danieder,", "tokens": ["Stie\u00df", "das", "Schwert", "in's", "Meer", "da\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und zertheilte so den Fisch,", "tokens": ["Und", "zer\u00b7theil\u00b7te", "so", "den", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Zog das Haupt in seinen Nachen,", "tokens": ["Zog", "das", "Haupt", "in", "sei\u00b7nen", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Lie\u00df den Schwanz im Meere liegen.", "tokens": ["Lie\u00df", "den", "Schwanz", "im", "Mee\u00b7re", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Jenes blickt er an und wendet's:", "tokens": ["Je\u00b7nes", "blickt", "er", "an", "und", "wen\u00b7det's", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "KON", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Was kann d'raus der Schmied verfert'gen?", "tokens": ["Was", "kann", "d'raus", "der", "Schmied", "ver\u00b7fert'\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Was kann d'raus der Schmieder schmieden?", "tokens": ["Was", "kann", "d'raus", "der", "Schmie\u00b7der", "schmie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "W\u00e4in\u00e4m\u00f6inen selbst, der alte,", "tokens": ["W\u00e4i\u00b7n\u00e4\u00b7m\u00f6i\u00b7nen", "selbst", ",", "der", "al\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Nimmt auf sich des Schmiedes Arbeit,", "tokens": ["Nimmt", "auf", "sich", "des", "Schmie\u00b7des", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Macht vom Bein des Hechts die Harfe,", "tokens": ["Macht", "vom", "Bein", "des", "Hechts", "die", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Macht das Kantele von Gr\u00e4ten,", "tokens": ["Macht", "das", "Kan\u00b7te\u00b7le", "von", "Gr\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Und von Fischgeripp die Leier.", "tokens": ["Und", "von", "Fischge\u00b7ripp", "die", "Lei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Und woraus der Harfe Schrauben?", "tokens": ["Und", "wo\u00b7raus", "der", "Har\u00b7fe", "Schrau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Aus des gro\u00dfen Hechtes Z\u00e4hnen.", "tokens": ["Aus", "des", "gro\u00b7\u00dfen", "Hech\u00b7tes", "Z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Und woraus der Harfe Saiten?", "tokens": ["Und", "wo\u00b7raus", "der", "Har\u00b7fe", "Sai\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Aus dem Haupthaar Kalevas.", "tokens": ["Aus", "dem", "Haupt\u00b7haar", "Ka\u00b7le\u00b7vas", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Zu dem Sohne sprach der Alte:", "tokens": ["Zu", "dem", "Soh\u00b7ne", "sprach", "der", "Al\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Hole mir mein Kantele", "tokens": ["Ho\u00b7le", "mir", "mein", "Kan\u00b7te\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Unter die gewohnten Finger,", "tokens": ["Un\u00b7ter", "die", "ge\u00b7wohn\u00b7ten", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Unter die gew\u00f6hnten H\u00e4nde!", "tokens": ["Un\u00b7ter", "die", "ge\u00b7w\u00f6hn\u00b7ten", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Freude str\u00f6mt nun \u00fcber Freude,", "tokens": ["Freu\u00b7de", "str\u00f6mt", "nun", "\u00fc\u00b7ber", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Auf Gel\u00e4chter folgt Gel\u00e4chter,", "tokens": ["Auf", "Ge\u00b7l\u00e4ch\u00b7ter", "folgt", "Ge\u00b7l\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "W\u00e4hrend spielet W\u00e4in\u00e4m\u00f6inen", "tokens": ["W\u00e4h\u00b7rend", "spie\u00b7let", "W\u00e4i\u00b7n\u00e4\u00b7m\u00f6i\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Auf dem Kantele von Gr\u00e4ten,", "tokens": ["Auf", "dem", "Kan\u00b7te\u00b7le", "von", "Gr\u00e4\u00