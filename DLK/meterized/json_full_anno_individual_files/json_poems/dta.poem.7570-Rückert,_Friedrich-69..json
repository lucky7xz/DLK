{"dta.poem.7570": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seligkeit ist nicht, nur selig selbst zu seyn,", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "ist", "nicht", ",", "nur", "se\u00b7lig", "selbst", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "$,", "ADV", "ADJD", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Seligkeit ist nicht allein und nicht zu zweyn;", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "ist", "nicht", "al\u00b7lein", "und", "nicht", "zu", "zweyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "KON", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Seligkeit ist nicht zu vielen, nur zu allen;", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "ist", "nicht", "zu", "vie\u00b7len", ",", "nur", "zu", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir kann nur Seligkeit der ganzen Welt gefallen.", "tokens": ["Mir", "kann", "nur", "Se\u00b7lig\u00b7keit", "der", "gan\u00b7zen", "Welt", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer selig w\u00e4r' und m\u00fc\u00dft' unselig andre wissen,", "tokens": ["Wer", "se\u00b7lig", "w\u00e4r'", "und", "m\u00fc\u00dft'", "un\u00b7se\u00b7lig", "and\u00b7re", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "VMFIN", "ADJD", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eigne Seligkeit w\u00e4r' ihm dadurch entrissen.", "tokens": ["Die", "eig\u00b7ne", "Se\u00b7lig\u00b7keit", "w\u00e4r'", "ihm", "da\u00b7durch", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und die Vergessenheit kann Seligkeit nicht seyn,", "tokens": ["Und", "die", "Ver\u00b7ges\u00b7sen\u00b7heit", "kann", "Se\u00b7lig\u00b7keit", "nicht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielmehr das Wissen ist die Seligkeit allein.", "tokens": ["Viel\u00b7mehr", "das", "Wis\u00b7sen", "ist", "die", "Se\u00b7lig\u00b7keit", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum kann die Seligkeit auf Erden nicht bestehn,", "tokens": ["Drum", "kann", "die", "Se\u00b7lig\u00b7keit", "auf", "Er\u00b7den", "nicht", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil hier die Seligen soviel Unsel'ge sehn.", "tokens": ["Weil", "hier", "die", "Se\u00b7li\u00b7gen", "so\u00b7viel", "Un\u00b7sel'\u00b7ge", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und der Gedanke nur gibt Seligkeit auf Erden,", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7ke", "nur", "gibt", "Se\u00b7lig\u00b7keit", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df die Unseligen auch selig sollen werden.", "tokens": ["Da\u00df", "die", "Un\u00b7se\u00b7li\u00b7gen", "auch", "se\u00b7lig", "sol\u00b7len", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Wer dieses wei\u00df, der tr\u00e4gt mit Eifer bei sein Theil", "tokens": ["Wer", "die\u00b7ses", "wei\u00df", ",", "der", "tr\u00e4gt", "mit", "Ei\u00b7fer", "bei", "sein", "Theil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum allgemeinen, wie zum eignen Seelenheil.", "tokens": ["Zum", "all\u00b7ge\u00b7mei\u00b7nen", ",", "wie", "zum", "eig\u00b7nen", "See\u00b7len\u00b7heil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PWAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Gott aber wei\u00df den Weg zu Aller Heil allein;", "tokens": ["Gott", "a\u00b7ber", "wei\u00df", "den", "Weg", "zu", "Al\u00b7ler", "Heil", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum ist nur selig Gott, in ihm nur kannst du's seyn.", "tokens": ["Drum", "ist", "nur", "se\u00b7lig", "Gott", ",", "in", "ihm", "nur", "kannst", "du's", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ADJD", "NN", "$,", "APPR", "PPER", "ADV", "VMFIN", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}