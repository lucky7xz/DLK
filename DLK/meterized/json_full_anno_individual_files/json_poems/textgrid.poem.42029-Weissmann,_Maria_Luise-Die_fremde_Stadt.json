{"textgrid.poem.42029": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die fremde Stadt", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel ist aus viel Zement gemauert,", "tokens": ["Der", "Him\u00b7mel", "ist", "aus", "viel", "Ze\u00b7ment", "ge\u00b7mau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sehr nah. Und grell mit T\u00fcnche \u00fcbermalt", "tokens": ["Sehr", "nah", ".", "Und", "grell", "mit", "T\u00fcn\u00b7che", "\u00fc\u00b7ber\u00b7malt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "KON", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von jenem Blau, das Litfa\u00dfs\u00e4ule strahlt;", "tokens": ["Von", "je\u00b7nem", "Blau", ",", "das", "Lit\u00b7fa\u00df\u00b7s\u00e4u\u00b7le", "strahlt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus Winkeln, dumpf und schwer, Verh\u00e4ngnis lauert,", "tokens": ["Aus", "Win\u00b7keln", ",", "dumpf", "und", "schwer", ",", "Ver\u00b7h\u00e4ng\u00b7nis", "lau\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und Ecken starren, oh so todumschauert, \u2013", "tokens": ["Und", "E\u00b7cken", "star\u00b7ren", ",", "oh", 