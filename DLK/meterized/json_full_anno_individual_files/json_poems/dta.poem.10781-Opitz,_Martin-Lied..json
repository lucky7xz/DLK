{"dta.poem.10781": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Last fahren ewr verlangen", "tokens": ["Last", "fah\u00b7ren", "ewr", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die jhr au\u00dfforschen wolt/", "tokens": ["Die", "jhr", "au\u00df\u00b7for\u00b7schen", "wolt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob ich von jhr gefangen/", "tokens": ["Ob", "ich", "von", "jhr", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd sie mir seye holdt.", "tokens": ["Vnd", "sie", "mir", "se\u00b7ye", "holdt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Je mehr mein Flamme brennet/", "tokens": ["Je", "mehr", "mein", "Flam\u00b7me", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Je minder ist der schein/", "tokens": ["Je", "min\u00b7der", "ist", "der", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Je minder sie bekennet/", "tokens": ["Je", "min\u00b7der", "sie", "be\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Je gr\u00f6sser ist die Pein.", "tokens": ["Je", "gr\u00f6s\u00b7ser", "ist", "die", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sie gleicht dem Donnerstrahle/", "tokens": ["Sie", "gleicht", "dem", "Don\u00b7ner\u00b7strah\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der innerhalb verzehrt/", "tokens": ["Der", "in\u00b7ner\u00b7halb", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Au\u00dfwendig vberale", "tokens": ["Au\u00df\u00b7wen\u00b7dig", "vbe\u00b7ra\u00b7le"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Nicht das geringst versehrt.", "tokens": ["Nicht", "das", "ge\u00b7ringst", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Sie hat mich nur im Hertzen", "tokens": ["Sie", "hat", "mich", "nur", "im", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "So inniglich entz\u00fcndt/", "tokens": ["So", "in\u00b7nig\u00b7lich", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Also/ da\u00df auch kein schmertzen", "tokens": ["Al\u00b7so", "/", "da\u00df", "auch", "kein", "schmert\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "ADV", "PIAT", "VVINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.16": {"text": "Der vbrig Leib empfindt.", "tokens": ["Der", "vb\u00b7rig", "Leib", "emp\u00b7findt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ja wen\u0303 mein Hertz ged\u00e4chte/", "tokens": ["Ja", "we\u00f1", "mein", "Hertz", "ge\u00b7d\u00e4ch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Da\u00df irgendt die ser Lieb", "tokens": ["Da\u00df", "ir\u00b7gendt", "die", "ser", "Lieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Geheimnu\u00df man au\u00dfbrechte/", "tokens": ["Ge\u00b7heim\u00b7nu\u00df", "man", "au\u00df\u00b7brech\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Vns beiden zu betrub/", "tokens": ["Vns", "bei\u00b7den", "zu", "be\u00b7trub", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Es w\u00fcrden sich verhelen", "tokens": ["Es", "w\u00fcr\u00b7den", "sich", "ver\u00b7he\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Selbst die gedancken mein/", "tokens": ["Selbst", "die", "ge\u00b7dan\u00b7cken", "mein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.23": {"text": "Auch meinem Mund befehlen", "tokens": ["Auch", "mei\u00b7nem", "Mund", "be\u00b7feh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Hinfort gar stumm zu sein.", "tokens": ["Hin\u00b7fort", "gar", "stumm", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Wie k\u00f6nt mir dan\u0303 gedeyen", "tokens": ["Wie", "k\u00f6nt", "mir", "da\u00f1", "ge\u00b7de\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PAV", "VVINF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.26": {"text": "Gl\u00fcckseliger zu sein?", "tokens": ["Gl\u00fcck\u00b7se\u00b7li\u00b7ger", "zu", "sein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.27": {"text": "Was k\u00f6nt mir mehr verleihen", "tokens": ["Was", "k\u00f6nt", "mir", "mehr", "ver\u00b7lei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Zu mindern meine Pein?", "tokens": ["Zu", "min\u00b7dern", "mei\u00b7ne", "Pein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Als in der still zu mehren", "tokens": ["Als", "in", "der", "still", "zu", "meh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Die s\u00fcsse Liebes brunst/", "tokens": ["Die", "s\u00fcs\u00b7se", "Lie\u00b7bes", "brunst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Vnd so mich zuverehren", "tokens": ["Vnd", "so", "mich", "zu\u00b7ve\u00b7reh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Der aller liebsten Gunst/", "tokens": ["Der", "al\u00b7ler", "liebs\u00b7ten", "Gunst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Mich in geheim beschlie\u00dfen", "tokens": ["Mich", "in", "ge\u00b7heim", "be\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Bey dieser argen Zeit/", "tokens": ["Bey", "die\u00b7ser", "ar\u00b7gen", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Vnd in mir selbst geniessen", "tokens": ["Vnd", "in", "mir", "selbst", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Meiner Gl\u00fcckseeligkeit/", "tokens": ["Mei\u00b7ner", "Gl\u00fcck\u00b7see\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.37": {"text": "So/ Schatz/ so werd ich sehen/", "tokens": ["So", "/", "Schatz", "/", "so", "werd", "ich", "se\u00b7hen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$(", "ADV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Bey dem verstande dein/", "tokens": ["Bey", "dem", "ver\u00b7stan\u00b7de", "dein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PPOSAT", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.39": {"text": "Dich mich vilmehr verstehen/", "tokens": ["Dich", "mich", "vil\u00b7mehr", "ver\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.40": {"text": "Als bey den worten mein/", "tokens": ["Als", "bey", "den", "wor\u00b7ten", "mein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPOSAT", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "So/ Schatz/ so red mein Hertze", "tokens": ["So", "/", "Schatz", "/", "so", "red", "mein", "Hert\u00b7ze"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "NN", "$(", "ADV", "VVFIN", "PPOSAT", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Durch eine newe sprach/", "tokens": ["Durch", "ei\u00b7ne", "ne\u00b7we", "sprach", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+---+", "measure": "dactylic.init"}, "line.43": {"text": "Erzehlendt seinen schmertze", "tokens": ["Er\u00b7zeh\u00b7lendt", "sei\u00b7nen", "schmert\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Durch ein stillschweigendt klag.", "tokens": ["Durch", "ein", "still\u00b7schwei\u00b7gendt", "klag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVFIN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.45": {"text": "Es deut was es begeret/", "tokens": ["Es", "deut", "was", "es", "be\u00b7ge\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.46": {"text": "Vnd zeiget was es sey/", "tokens": ["Vnd", "zei\u00b7get", "was", "es", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Vnd will von euch gewehret", "tokens": ["Vnd", "will", "von", "euch", "ge\u00b7weh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Sein einer gleichen trew.", "tokens": ["Sein", "ei\u00b7ner", "glei\u00b7chen", "trew", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Nun sagt/ thut der nit flehen/", "tokens": ["Nun", "sagt", "/", "thut", "der", "nit", "fle\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "ART", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Thut der nit bitter sehr/", "tokens": ["Thut", "der", "nit", "bit\u00b7ter", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "PTKNEG", "ADJD", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Der seine Lieb lest sehen/", "tokens": ["Der", "sei\u00b7ne", "Lieb", "lest", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Vnd sonst nichts saget mehr?", "tokens": ["Vnd", "sonst", "nichts", "sa\u00b7get", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}