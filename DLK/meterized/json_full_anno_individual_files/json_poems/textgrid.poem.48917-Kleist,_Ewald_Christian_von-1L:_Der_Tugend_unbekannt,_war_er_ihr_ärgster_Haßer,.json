{"textgrid.poem.48917": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Tugend unbekannt, war er ihr \u00e4rgster Ha\u00dfer,", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tugend unbekannt, war er ihr \u00e4rgster Ha\u00dfer,", "tokens": ["Der", "Tu\u00b7gend", "un\u00b7be\u00b7kannt", ",", "war", "er", "ihr", "\u00e4rgs\u00b7ter", "Ha\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihn sein Stolz befiel, flo\u00df Menschenblut wie Wasser,", "tokens": ["Wenn", "ihn", "sein", "Stolz", "be\u00b7fiel", ",", "flo\u00df", "Men\u00b7schen\u00b7blut", "wie", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er war voll Eigennutz und liebte Schmeicheley,", "tokens": ["Er", "war", "voll", "Ei\u00b7gen\u00b7nutz", "und", "lieb\u00b7te", "Schmei\u00b7che\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Raubt ungestraft und blieb nie seinen Worten treu,", "tokens": ["Raubt", "un\u00b7ge\u00b7straft", "und", "blieb", "nie", "sei\u00b7nen", "Wor\u00b7ten", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "War vielfach und gelehrt, sich in die Zeit zu schicken,", "tokens": ["War", "viel\u00b7fach", "und", "ge\u00b7lehrt", ",", "sich", "in", "die", "Zeit", "zu", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "VVPP", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verband mit zwanzig sich, um einen zu erdr\u00fccken,", "tokens": ["Ver\u00b7band", "mit", "zwan\u00b7zig", "sich", ",", "um", "ei\u00b7nen", "zu", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "PRF", "$,", "KOUI", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Religion und Eyd war ihm ein Puppenspiel,", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", "und", "Eyd", "war", "ihm", "ein", "Pup\u00b7pen\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Durch Labyrinthe ging er stets zum nahen Ziel,", "tokens": ["Durch", "La\u00b7by\u00b7rin\u00b7the", "ging", "er", "stets", "zum", "na\u00b7hen", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hurt' und verfolgte Wild \u2013 O Mahler halt ein wenig,", "tokens": ["Hurt'", "und", "ver\u00b7folg\u00b7te", "Wild", "\u2013", "O", "Mah\u00b7ler", "halt", "ein", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "NE", "$(", "NE", "NE", "VVFIN", "ART", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Halt! ich versteh dich schon, das hei\u00dft: er war ein K\u00f6nig.", "tokens": ["Halt", "!", "ich", "ver\u00b7steh", "dich", "schon", ",", "das", "hei\u00dft", ":", "er", "war", "ein", "K\u00f6\u00b7nig", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,", "PDS", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Tugend unbekannt, war er ihr \u00e4rgster Ha\u00dfer,", "tokens": ["Der", "Tu\u00b7gend", "un\u00b7be\u00b7kannt", ",", "war", "er", "ihr", "\u00e4rgs\u00b7ter", "Ha\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihn sein Stolz befiel, flo\u00df Menschenblut wie Wasser,", "tokens": ["Wenn", "ihn", "sein", "Stolz", "be\u00b7fiel", ",", "flo\u00df", "Men\u00b7schen\u00b7blut", "wie", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er war voll Eigennutz und liebte Schmeicheley,", "tokens": ["Er", "war", "voll", "Ei\u00b7gen\u00b7nutz", "und", "lieb\u00b7te", "Schmei\u00b7che\u00b7ley", ","], "token_info": ["wo