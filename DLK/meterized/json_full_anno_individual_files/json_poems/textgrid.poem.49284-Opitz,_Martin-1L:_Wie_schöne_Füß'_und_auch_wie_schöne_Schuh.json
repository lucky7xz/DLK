{"textgrid.poem.49284": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sch\u00f6ne F\u00fc\u00df' und auch wie sch\u00f6ne Schuh", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6ne F\u00fc\u00df' und auch wie sch\u00f6ne Schuh", "tokens": ["Wie", "sch\u00f6\u00b7ne", "F\u00fc\u00df'", "und", "auch", "wie", "sch\u00f6\u00b7ne", "Schuh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "KON", "ADV", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sind deine doch, du F\u00fcrstentochter du!", "tokens": ["Sind", "dei\u00b7ne", "doch", ",", "du", "F\u00fcrs\u00b7ten\u00b7toch\u00b7ter", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADV", "$,", "PPER", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Spangen stehn beysammen deine Lenden,", "tokens": ["Wie", "Span\u00b7gen", "stehn", "bey\u00b7sam\u00b7men", "dei\u00b7ne", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sehr wol gemacht von guten Meisterh\u00e4nden.", "tokens": ["Sehr", "wol", "ge\u00b7macht", "von", "gu\u00b7ten", "Meis\u00b7ter\u00b7h\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Nabel wie ein runder Becher steht,", "tokens": ["Dein", "Na\u00b7bel", "wie", "ein", "run\u00b7der", "Be\u00b7cher", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem niemals Tranck und s\u00fcsser Wein abgeht;", "tokens": ["Dem", "nie\u00b7mals", "Tranck", "und", "s\u00fcs\u00b7ser", "Wein", "ab\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Bauch gleicht sich dem Weitzenhauffen eben,", "tokens": ["Der", "Bauch", "gleicht", "sich", "dem", "Weit\u00b7zen\u00b7hauf\u00b7fen", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der rings umbher mit Rosen ist umbgeben.", "tokens": ["Der", "rings", "um\u00b7bher", "mit", "Ro\u00b7sen", "ist", "umb\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gleich wie man sicht zwey junge Rehe sich", "tokens": ["Gleich", "wie", "man", "sicht", "zwey", "jun\u00b7ge", "Re\u00b7he", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PIS", "VVFIN", "CARD", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit geilem Spiel' ergetzen lustiglich", "tokens": ["Mit", "gei\u00b7lem", "Spiel'", "er\u00b7get\u00b7zen", "lus\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und fr\u00f6lich sein an einer gr\u00fcnen W\u00fcste,", "tokens": ["Und", "fr\u00f6\u00b7lich", "sein", "an", "ei\u00b7ner", "gr\u00fc\u00b7nen", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So stehn dir auch die rund erhabnen Br\u00fcste.", "tokens": ["So", "stehn", "dir", "auch", "die", "rund", "er\u00b7hab\u00b7nen", "Br\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dein weisser Hals giebt von sich solchen Schein,", "tokens": ["Dein", "weis\u00b7ser", "Hals", "giebt", "von", "sich", "sol\u00b7chen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als wie ein Thurn gemacht au\u00df Helffenbein.", "tokens": ["Als", "wie", "ein", "Thurn", "ge\u00b7macht", "au\u00df", "Helf\u00b7fen\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVPP", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wangen sind wie Hesbons sch\u00f6ne Teiche", "tokens": ["Die", "Wan\u00b7gen", "sind", "wie", "Hes\u00b7bons", "sch\u00f6\u00b7ne", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Bathrabs Thor' in Armons seinem Reiche.", "tokens": ["Am", "Ba\u00b7thr\u00b7abs", "Thor'", "in", "Ar\u00b7mons", "sei\u00b7nem", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "NE", "APPR", "NE", "PPOSAT", "NE", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Nas' ist dir wie Libans Thurn erh\u00f6ht", "tokens": ["Die", "Nas'", "ist", "dir", "wie", "Li\u00b7bans", "Thurn", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "KOKOM", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier wo der Weg hin nach Damaseus geht:", "tokens": ["Hier", "wo", "der", "Weg", "hin", "nach", "Da\u00b7ma\u00b7seus", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Haupt sieht au\u00df wie Karmel an dem Strande", "tokens": ["Das", "Haupt", "sieht", "au\u00df", "wie", "Kar\u00b7mel", "an", "dem", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mittelsee im palestiner Lande.", "tokens": ["Der", "Mit\u00b7tel\u00b7see", "im", "pa\u00b7les\u00b7ti\u00b7ner", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Das edle Haar mit dem du, Liebste, bl\u00fchst,", "tokens": ["Das", "ed\u00b7le", "Haar", "mit", "dem", "du", ",", "Liebs\u00b7te", ",", "bl\u00fchst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRELS", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat einen Glantz wie K\u00f6nigs Purpur ist.", "tokens": ["Hat", "ei\u00b7nen", "Glantz", "wie", "K\u00f6\u00b7nigs", "Pur\u00b7pur", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du hast doch nichts, als lauter solche Gaben", "tokens": ["Du", "hast", "doch", "nichts", ",", "als", "lau\u00b7ter", "sol\u00b7che", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "$,", "KOUS", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die manch' ihr w\u00fcndscht und du kanst einig haben.", "tokens": ["Die", "man\u00b7ch'", "ihr", "w\u00fcnd\u00b7scht", "und", "du", "kanst", "ei\u00b7nig", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPER", "VVFIN", "KON", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Was ist es nun, das dir an Lenge gleicht?", "tokens": ["Was", "ist", "es", "nun", ",", "das", "dir", "an", "Len\u00b7ge", "gleicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Palmenbaum, der keiner Last nicht weicht.", "tokens": ["Ein", "Pal\u00b7men\u00b7baum", ",", "der", "kei\u00b7ner", "Last", "nicht", "weicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Br\u00fcste stehn wie Trauben, die noch reiffen", "tokens": ["Die", "Br\u00fcs\u00b7te", "stehn", "wie", "Trau\u00b7ben", ",", "die", "noch", "reif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und harte sind zum ersten anzugreiffen.", "tokens": ["Und", "har\u00b7te", "sind", "zum", "ers\u00b7ten", "an\u00b7zu\u00b7greif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VAFIN", "APPRART", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Was geb' ich doch dem S\u00e4umen weiter Raum,", "tokens": ["Was", "geb'", "ich", "doch", "dem", "S\u00e4u\u00b7men", "wei\u00b7ter", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und steige nicht auff meinen Palmenbaum?", "tokens": ["Und", "stei\u00b7ge", "nicht", "auff", "mei\u00b7nen", "Pal\u00b7men\u00b7baum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df deine Br\u00fcst' als junge Trauben stehen,", "tokens": ["La\u00df", "dei\u00b7ne", "Br\u00fcst'", "als", "jun\u00b7ge", "Trau\u00b7ben", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Nasen Ruch f\u00fcr schmeckend' Oepffel gehen.", "tokens": ["Der", "Na\u00b7sen", "Ruch", "f\u00fcr", "schme\u00b7ckend'", "O\u00b7e\u00b7pf\u00b7fel", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dein zarter Schlund sey wie ein s\u00fcsser Wein,", "tokens": ["Dein", "zar\u00b7ter", "Schlund", "sey", "wie", "ein", "s\u00fcs\u00b7ser", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der uns erquickt und schl\u00e4fft die Sinnen ein", "tokens": ["Der", "uns", "er\u00b7quickt", "und", "schl\u00e4fft", "die", "Sin\u00b7nen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "KON", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und machet, da\u00df dein Buhle Sachen saget,", "tokens": ["Und", "ma\u00b7chet", ",", "da\u00df", "dein", "Buh\u00b7le", "Sa\u00b7chen", "sa\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie einer, der im Traume nach was fraget.", "tokens": ["Wie", "ei\u00b7ner", ",", "der", "im", "Trau\u00b7me", "nach", "was", "fra\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "APPRART", "NN", "APPR", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich bleib' und bin de\u00df Liebsten f\u00fcr und f\u00fcr,", "tokens": ["Ich", "bleib'", "und", "bin", "de\u00df", "Liebs\u00b7ten", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ART", "NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann seine Lust beruhet gantz auff mir.", "tokens": ["Dann", "sei\u00b7ne", "Lust", "be\u00b7ru\u00b7het", "gantz", "auff", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Komm, Hertze, komm, la\u00df uns zu Felde bleiben", "tokens": ["Komm", ",", "Hert\u00b7ze", ",", "komm", ",", "la\u00df", "uns", "zu", "Fel\u00b7de", "blei\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVIMP", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In feister Ruh' und da die Zeit vertreiben.", "tokens": ["In", "feis\u00b7ter", "Ruh'", "und", "da", "die", "Zeit", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wir lassen nur der Statt nicht rechten Schein,", "tokens": ["Wir", "las\u00b7sen", "nur", "der", "Statt", "nicht", "rech\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr eitels Thun und falsche Freude sein,", "tokens": ["Ihr", "ei\u00b7tels", "Thun", "und", "fal\u00b7sche", "Freu\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir wolln mit dir, o Morgenr\u00f6th', auffstehen", "tokens": ["Wir", "wolln", "mit", "dir", ",", "o", "Mor\u00b7gen\u00b7r\u00f6\u00b7th'", ",", "auffs\u00b7te\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "$,", "FM", "FM", "$,", "VVIZU"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und fr\u00f6lich hin in unsern Weinberg gehen.", "tokens": ["Und", "fr\u00f6\u00b7lich", "hin", "in", "un\u00b7sern", "Wein\u00b7berg", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Wir wollen sehn, ob nicht der Stock schier bl\u00fcht,", "tokens": ["Wir", "wol\u00b7len", "sehn", ",", "ob", "nicht", "der", "Stock", "schier", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PTKNEG", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ob er nicht mit neuen Augen sieht;", "tokens": ["Und", "ob", "er", "nicht", "mit", "neu\u00b7en", "Au\u00b7gen", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG",