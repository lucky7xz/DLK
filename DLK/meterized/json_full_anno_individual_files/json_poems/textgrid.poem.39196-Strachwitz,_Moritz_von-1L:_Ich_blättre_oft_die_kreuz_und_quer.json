ct"], "pos": ["APPR", "NE", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich war vom Helm bis an den Sporn", "tokens": ["Ich", "war", "vom", "Helm", "bis", "an", "den", "Sporn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Stahl geschn\u00fcrt, in blanken,", "tokens": ["In", "Stahl", "ge\u00b7schn\u00fcrt", ",", "in", "blan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ritt in raschem Kampfeszorn", "tokens": ["Und", "ritt", "in", "ra\u00b7schem", "Kamp\u00b7fes\u00b7zorn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durchs Schlachtfeld der Gedanken.", "tokens": ["Durchs", "Schlacht\u00b7feld", "der", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gedanke hier, Gedanke dort,", "tokens": ["Ge\u00b7dan\u00b7ke", "hier", ",", "Ge\u00b7dan\u00b7ke", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das war ein hei\u00df Gedr\u00e4nge,", "tokens": ["Das", "war", "ein", "hei\u00df", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wild wirbelte von Ort zu Ort", "tokens": ["Wild", "wir\u00b7bel\u00b7te", "von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Im Kn\u00e4ul das Handgemenge;", "tokens": ["Im", "Kn\u00e4ul", "das", "Hand\u00b7ge\u00b7men\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich aber trug mein Fl\u00fcgelro\u00df,", "tokens": ["Mich", "a\u00b7ber", "trug", "mein", "Fl\u00fc\u00b7gel\u00b7ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Lied voll Sturm und Flammen,", "tokens": ["Ein", "Lied", "voll", "Sturm", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch Lanzenwald und Wagentro\u00df", "tokens": ["Durch", "Lan\u00b7zen\u00b7wald", "und", "Wa\u00b7gen\u00b7tro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u2013 brach mit mir zusammen.", "tokens": ["Und", "\u2013", "brach", "mit", "mir", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da lag der tote Pegasus", "tokens": ["Da", "lag", "der", "to\u00b7te", "Pe\u00b7ga\u00b7sus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf mir in voller Schwere,", "tokens": ["Auf", "mir", "in", "vol\u00b7ler", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber mich wie Wogenschu\u00df", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "wie", "Wo\u00b7gen\u00b7schu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinrollten beide Heere.", "tokens": ["Hin\u00b7roll\u00b7ten", "bei\u00b7de", "Hee\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ha rechts die Schlacht und links die Schlacht!", "tokens": ["Ha", "rechts", "die", "Schlacht", "und", "links", "die", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da lag ich wund und m\u00fcd'", "tokens": ["Da", "lag", "ich", "wund", "und", "m\u00fcd'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und rief empor mit aller Macht:", "tokens": ["Und", "rief", "em\u00b7por", "mit", "al\u00b7ler", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Leben f\u00fcr ein Lied!", "tokens": ["Mein", "Le\u00b7ben", "f\u00fcr", "ein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn innen tobt der Liederstreit,", "tokens": ["Wenn", "in\u00b7nen", "tobt", "der", "Lie\u00b7der\u00b7streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Worte aber versagen,", "tokens": ["Die", "Wor\u00b7te", "a\u00b7ber", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kein Wort, kein Reim, kein Lied bereit,", "tokens": ["Kein", "Wort", ",", "kein", "Reim", ",", "kein", "Lied", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um drauf dahinzujagen.", "tokens": ["Um", "drauf", "da\u00b7hin\u00b7zu\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn uns das Herz das Lied nicht gibt", "tokens": ["Wenn", "uns", "das", "Herz", "das", "Lied", "nicht", "gibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und doch zum Liede zieht,", "tokens": ["Und", "doch", "zum", "Lie\u00b7de", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da ruft man wohl zum Tod betr\u00fcbt:", "tokens": ["Da", "ruft", "man", "wohl", "zum", "Tod