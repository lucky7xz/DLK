{"textgrid.poem.57868": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Vertraulich", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die saueren Gurken sind nun reif,", "tokens": ["Die", "sau\u00b7e\u00b7ren", "Gur\u00b7ken", "sind", "nun", "reif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Enten sind fl\u00fcgge geworden,", "tokens": ["Die", "En\u00b7ten", "sind", "fl\u00fcg\u00b7ge", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch merkt man leider wenig davon", "tokens": ["Doch", "merkt", "man", "lei\u00b7der", "we\u00b7nig", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ADV", "PIS", "PAV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Im S\u00fcden und im Norden.", "tokens": ["Im", "S\u00fc\u00b7den", "und", "im", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es bl\u00fcht die hohe Politik", "tokens": ["Es", "bl\u00fcht", "die", "ho\u00b7he", "Po\u00b7li\u00b7tik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz winterm\u00e4\u00dfig weiter,", "tokens": ["Ganz", "win\u00b7ter\u00b7m\u00e4\u00b7\u00dfig", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier schwingt man Reden, dort den Dolch,", "tokens": ["Hier", "schwingt", "man", "Re\u00b7den", ",", "dort", "den", "Dolch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich finde beides nicht heiter.", "tokens": ["Ich", "fin\u00b7de", "bei\u00b7des", "nicht", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch in Hannover redet man,", "tokens": ["Auch", "in", "Han\u00b7no\u00b7ver", "re\u00b7det", "man", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "PIS", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Besonders bei Sch\u00fctzenfestessen,", "tokens": ["Be\u00b7son\u00b7ders", "bei", "Sch\u00fct\u00b7zen\u00b7fes\u00b7tes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da wird der st\u00f6rrigen B\u00fcrgerschaft", "tokens": ["Da", "wird", "der", "st\u00f6r\u00b7ri\u00b7gen", "B\u00fcr\u00b7ger\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Rute zugemessen.", "tokens": ["Die", "Ru\u00b7te", "zu\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "