{"textgrid.poem.67406": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Vorsehung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Mutter, wenn sie ihre Kinder", "tokens": ["Wie", "die", "Mut\u00b7ter", ",", "wenn", "sie", "ih\u00b7re", "Kin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um sich siehet, liebevoll sie anblickt,", "tokens": ["Um", "sich", "sie\u00b7het", ",", "lie\u00b7be\u00b7voll", "sie", "an\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Diesem einen Ku\u00df auf Stirn und Wange,", "tokens": ["Die\u00b7sem", "ei\u00b7nen", "Ku\u00df", "auf", "Stirn", "und", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jenes sich ans Herz dr\u00fcckt und ein andres", "tokens": ["Je\u00b7nes", "sich", "ans", "Herz", "dr\u00fcckt", "und", "ein", "and\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "PRF", "APPRART", "NN", "VVFIN", "KON", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Auf den Schoo\u00df hebt, auf den Knieen wieget,", "tokens": ["Auf", "den", "Schoo\u00df", "hebt", ",", "auf", "den", "Kni\u00b7e\u00b7en", "wie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und indem sie ihrer aller W\u00fcnsche", "tokens": ["Und", "in\u00b7dem", "sie", "ih\u00b7rer", "al\u00b7ler", "W\u00fcn\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "In den Blicken, in Geberden lieset,", "tokens": ["In", "den", "Bli\u00b7cken", ",", "in", "Ge\u00b7ber\u00b7den", "lie\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Giebt sie jedem etwas, dem ein L\u00e4cheln,", "tokens": ["Giebt", "sie", "je\u00b7dem", "et\u00b7was", ",", "dem", "ein", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PIS", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Dem ein s\u00fc\u00dfes Wort, dem dritten z\u00fcrnt sie,", "tokens": ["Dem", "ein", "s\u00fc\u00b7\u00dfes", "Wort", ",", "dem", "drit\u00b7ten", "z\u00fcrnt", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Scheint zu z\u00fcrnen und hat ihn am Liebsten:", "tokens": ["Scheint", "zu", "z\u00fcr\u00b7nen", "und", "hat", "ihn", "am", "Liebs\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "KON", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "So ist uns die m\u00fctterliche Vorsicht;", "tokens": ["So", "ist", "uns", "die", "m\u00fct\u00b7ter\u00b7li\u00b7che", "Vor\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Immer wachsam, horchend auf den leisen", "tokens": ["Im\u00b7mer", "wach\u00b7sam", ",", "hor\u00b7chend", "auf", "den", "lei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Seufzer, schafft sie Jedem Trost und Labsal,", "tokens": ["Seuf\u00b7zer", ",", "schafft", "sie", "Je\u00b7dem", "Trost", "und", "Lab\u00b7sal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Sorgt f\u00fcr Alle, leistet Allen Hilfe;", "tokens": ["Sorgt", "f\u00fcr", "Al\u00b7le", ",", "leis\u00b7tet", "Al\u00b7len", "Hil\u00b7fe", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIS", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Und wenn sie zuweilen auch versaget,", "tokens": ["Und", "wenn", "sie", "zu\u00b7wei\u00b7len", "auch", "ver\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Lockt sie uns entweder, mehr zu bitten,", "tokens": ["Lockt", "sie", "uns", "ent\u00b7we\u00b7der", ",", "mehr", "zu", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Oder sinnt auf eine sch\u00f6nre Gabe.", "tokens": ["O\u00b7der", "sinnt", "auf", "ei\u00b7ne", "sch\u00f6n\u00b7re", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie die Mutter, wenn sie ihre Kinder", "tokens": ["Wie", "die", "Mut\u00b7ter", ",", "wenn", "sie", "ih\u00b7re", "Kin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um sich siehet, liebevoll sie anblickt,", "tokens": ["Um", "sich", "sie\u00b7het", ",", "lie\u00b7be\u00b7voll", "sie", "an\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Diesem einen Ku\u00df auf Stirn und Wange,", "tokens": ["Die\u00b7sem", "ei\u00b7nen", "Ku\u00df", "auf", "Stirn", "und", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jenes sich ans Herz dr\u00fcckt und ein andres", "tokens": ["Je\u00b7nes", "sich", "ans", "Herz", "dr\u00fcckt", "und", "ein", "and\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "PRF", "APPRART", "NN