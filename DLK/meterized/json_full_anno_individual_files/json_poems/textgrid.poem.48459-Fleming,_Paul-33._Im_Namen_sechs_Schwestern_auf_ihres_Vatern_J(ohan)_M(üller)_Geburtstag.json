{"textgrid.poem.48459": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "33. Im Namen sechs Schwestern auf ihres Vatern J(ohan) M(\u00fcller) Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag auch ein sch\u00f6ner Tag gefunden werden k\u00f6nnen,", "tokens": ["Mag", "auch", "ein", "sch\u00f6\u00b7ner", "Tag", "ge\u00b7fun\u00b7den", "wer\u00b7den", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wol durch das ganze Jahr, der unsre Mut und Sinnen", "tokens": ["wol", "durch", "das", "gan\u00b7ze", "Jahr", ",", "der", "uns\u00b7re", "Mut", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "durch sich selbst mehr vergn\u00fcgt und uns hei\u00dft fr\u00f6lich sein,", "tokens": ["durch", "sich", "selbst", "mehr", "ver\u00b7gn\u00fcgt", "und", "uns", "hei\u00dft", "fr\u00f6\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ADV", "VVPP", "KON", "PPER", "VVFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als eben heute der, der seinen lieben Schein", "tokens": ["als", "e\u00b7ben", "heu\u00b7te", "der", ",", "der", "sei\u00b7nen", "lie\u00b7ben", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "auf uns, auf unser Haus, auf unsern Vater senket,", "tokens": ["auf", "uns", ",", "auf", "un\u00b7ser", "Haus", ",", "auf", "un\u00b7sern", "Va\u00b7ter", "sen\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der ihm den seinen Tag, uns seine Freude schenket?", "tokens": ["der", "ihm", "den", "sei\u00b7nen", "Tag", ",", "uns", "sei\u00b7ne", "Freu\u00b7de", "schen\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nein! Es ist keiner nicht, der uns mehr Freude macht,", "tokens": ["Nein", "!", "Es", "ist", "kei\u00b7ner", "nicht", ",", "der", "uns", "mehr", "Freu\u00b7de", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PIS", "PTKNEG", "$,", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "als dieser, der uns itzt so s\u00fc\u00df' hat angelacht.", "tokens": ["als", "die\u00b7ser", ",", "der", "uns", "itzt", "so", "s\u00fc\u00df'", "hat", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum ist es billig auch, da\u00df wir ihn recht begehen", "tokens": ["Drum", "ist", "es", "bil\u00b7lig", "auch", ",", "da\u00df", "wir", "ihn", "recht", "be\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "ADV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und mit Gl\u00fcckw\u00fcndschungen vor unserm Vater stehen.", "tokens": ["und", "mit", "Gl\u00fcck\u00b7w\u00fcnd\u00b7schun\u00b7gen", "vor", "un\u00b7serm", "Va\u00b7ter", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Komt, komt, ihr Schwestern, komt, la\u00dft uns die werte Zeit", "tokens": ["Komt", ",", "komt", ",", "ihr", "Schwes\u00b7tern", ",", "komt", ",", "la\u00dft", "uns", "die", "wer\u00b7te", "Zeit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "begehn mit s\u00fc\u00dfer Lust und sch\u00f6ner Fr\u00f6lichkeit!", "tokens": ["be\u00b7gehn", "mit", "s\u00fc\u00b7\u00dfer", "Lust", "und", "sch\u00f6\u00b7ner", "Fr\u00f6\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df Band, das wir selbselbst so haben aufgewunden", "tokens": ["Di\u00df", "Band", ",", "das", "wir", "selb\u00b7selbst", "so", "ha\u00b7ben", "auf\u00b7ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und auf den sch\u00f6nen Tag zu Ehren euch gebunden,", "tokens": ["und", "auf", "den", "sch\u00f6\u00b7nen", "Tag", "zu", "Eh\u00b7ren", "euch", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "nehmt, Vater, wie es ist, von euren T\u00f6chtern an!", "tokens": ["nehmt", ",", "Va\u00b7ter", ",", "wie", "es", "ist", ",", "von", "eu\u00b7ren", "T\u00f6ch\u00b7tern", "an", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den Wundsch hat iede schon vor euch zu Gott getan:", "tokens": ["Den", "Wund\u00b7sch", "hat", "ie\u00b7de", "schon", "vor", "euch", "zu", "Gott", "ge\u00b7tan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADV", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.17": {"text": "Lebt lange, lebt gesund, lebt selig, wie ihr lebet,", "tokens": ["Lebt", "lan\u00b7ge", ",", "lebt", "ge\u00b7sund", ",", "lebt", "se\u00b7lig", ",", "wie", "ihr", "le\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "p