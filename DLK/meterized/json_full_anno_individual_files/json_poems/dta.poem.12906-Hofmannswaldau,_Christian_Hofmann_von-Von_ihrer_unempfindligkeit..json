{"dta.poem.12906": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von ihrer unempfindligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Belebter schnee der sch\u00f6nen brust!", "tokens": ["Be\u00b7leb\u00b7ter", "schnee", "der", "sch\u00f6\u00b7nen", "brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verliebter augen zweck und lust!", "tokens": ["Ver\u00b7lieb\u00b7ter", "au\u00b7gen", "zweck", "und", "lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es scheint, da\u00df die natur dich so wol ha\u00dft als liebet,", "tokens": ["Es", "scheint", ",", "da\u00df", "die", "na\u00b7tur", "dich", "so", "wol", "ha\u00dft", "als", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "ADV", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie, so gro\u00df sie auch die pracht", "tokens": ["Weil", "sie", ",", "so", "gro\u00df", "sie", "auch", "die", "pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von deinen sch\u00f6nen bergen macht,", "tokens": ["Von", "dei\u00b7nen", "sch\u00f6\u00b7nen", "ber\u00b7gen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir dennoch einen stein an statt des hertzens giebet.", "tokens": ["Dir", "den\u00b7noch", "ei\u00b7nen", "stein", "an", "statt", "des", "hert\u00b7zens", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}