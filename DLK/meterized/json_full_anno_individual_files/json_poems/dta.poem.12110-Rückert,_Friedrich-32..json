{"dta.poem.12110": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah auf einer Trift zusammen Ro\u00df und Rind,", "tokens": ["Ich", "sah", "auf", "ei\u00b7ner", "Trift", "zu\u00b7sam\u00b7men", "Ro\u00df", "und", "Rind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gemischt, wie Ritterschaft und b\u00e4urisches Gesind.", "tokens": ["Ge\u00b7mischt", ",", "wie", "Rit\u00b7ter\u00b7schaft", "und", "b\u00e4u\u00b7ri\u00b7sches", "Ge\u00b7sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Rinder hatten nicht Ro\u00dfadel angenommen,", "tokens": ["Die", "Rin\u00b7der", "hat\u00b7ten", "nicht", "Ro\u00df\u00b7a\u00b7del", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Zu Rindes Ansehn war das edle Ro\u00df gekommen.", "tokens": ["Zu", "Rin\u00b7des", "An\u00b7sehn", "war", "das", "ed\u00b7le", "Ro\u00df", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo irgend hohes sich und niedres will anneigen,", "tokens": ["Wo", "ir\u00b7gend", "ho\u00b7hes", "sich", "und", "nie\u00b7dres", "will", "an\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "PRF", "KON", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird Hohes ehr herab als Niedres aufwerts steigen.", "tokens": ["Wird", "Ho\u00b7hes", "ehr", "her\u00b7ab", "als", "Nie\u00b7dres", "auf\u00b7werts", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "KOUS", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}