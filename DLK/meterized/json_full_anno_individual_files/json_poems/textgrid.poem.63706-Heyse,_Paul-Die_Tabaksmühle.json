{"textgrid.poem.63706": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Die Tabaksm\u00fchle", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort unter den Weiden das windschiefe Dach,", "tokens": ["Dort", "un\u00b7ter", "den", "Wei\u00b7den", "das", "wind\u00b7schie\u00b7fe", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da treibet ein M\u00fchlrad der rauschende Bach", "tokens": ["Da", "trei\u00b7bet", "ein", "M\u00fchl\u00b7rad", "der", "rau\u00b7schen\u00b7de", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mit Rasseln und Raunen und lautem Taktak;", "tokens": ["Mit", "Ras\u00b7seln", "und", "Rau\u00b7nen", "und", "lau\u00b7tem", "Tak\u00b7tak", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der M\u00fcller mahlt braunen Bresiltabak.", "tokens": ["Der", "M\u00fcl\u00b7ler", "mahlt", "brau\u00b7nen", "Bre\u00b7silt\u00b7a\u00b7bak", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der bei\u00dfet wie Pfeffer, durchbeizet die Luft;", "tokens": ["Der", "bei\u00b7\u00dfet", "wie", "Pfef\u00b7fer", ",", "durch\u00b7bei\u00b7zet", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "punct