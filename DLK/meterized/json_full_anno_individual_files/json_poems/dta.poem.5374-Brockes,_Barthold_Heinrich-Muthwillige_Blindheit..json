{"dta.poem.5374": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Muthwillige Blindheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, la\u00dft uns, da wir ja in andern Sachen", "tokens": ["Ach", ",", "la\u00dft", "uns", ",", "da", "wir", "ja", "in", "an\u00b7dern", "Sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht schl\u00e4frig sind, doch auch zu GOttes Ehren", "tokens": ["Nicht", "schl\u00e4f\u00b7rig", "sind", ",", "doch", "auch", "zu", "Got\u00b7tes", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "ADV", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil f\u00fcr sein Werck, das er so wunderbar geschaffen,", "tokens": ["Weil", "f\u00fcr", "sein", "Werck", ",", "das", "er", "so", "wun\u00b7der\u00b7bar", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Seele leider scheint best\u00e4ndig fort zu schlaffen.", "tokens": ["Die", "See\u00b7le", "lei\u00b7der", "scheint", "be\u00b7st\u00e4n\u00b7dig", "fort", "zu", "schlaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn weniger, als wir von GOttes Wundern sehn", "tokens": ["Denn", "we\u00b7ni\u00b7ger", ",", "als", "wir", "von", "Got\u00b7tes", "Wun\u00b7dern", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jm Wachen, kann es fast im Schlaffen nicht geschehn.", "tokens": ["Jm", "Wa\u00b7chen", ",", "kann", "es", "fast", "im", "Schlaf\u00b7fen", "nicht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VMFIN", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bey den Gedancken f\u00e4llt mir ein,", "tokens": ["Bey", "den", "Ge\u00b7dan\u00b7cken", "f\u00e4llt", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Was unl\u00e4ngst soll von Philopotamus", "tokens": ["Was", "un\u00b7l\u00e4ngst", "soll", "von", "Phi\u00b7lo\u00b7po\u00b7ta\u00b7mus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geschehen und gesprochen seyn:", "tokens": ["Ge\u00b7sche\u00b7hen", "und", "ge\u00b7spro\u00b7chen", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem derselbe sich fast g\u00e4ntzlich blind gesoffen,", "tokens": ["Nach\u00b7dem", "der\u00b7sel\u00b7be", "sich", "fast", "g\u00e4ntz\u00b7lich", "blind", "ge\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PRF", "ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sagt ihm sein Artzt: wofern er nicht", "tokens": ["Sagt", "ihm", "sein", "Artzt", ":", "wo\u00b7fern", "er", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein meist bereits verlohrenes Gesicht", "tokens": ["Sein", "meist", "be\u00b7reits", "ver\u00b7loh\u00b7re\u00b7nes", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wollt\u2019 \u00fcberall verliehren; m\u00fcste Wein", "tokens": ["Wollt'", "\u00fc\u00b7be\u00b7rall", "ver\u00b7lieh\u00b7ren", ";", "m\u00fcs\u00b7te", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durchaus nicht mehr von ihm getruncken seyn.", "tokens": ["Durc\u00b7haus", "nicht", "mehr", "von", "ihm", "ge\u00b7trun\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nun was geschicht?", "tokens": ["Nun", "was", "ge\u00b7schicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Er sieht ein grosses Gla\u00df voll Wein von ungefehr,", "tokens": ["Er", "sieht", "ein", "gros\u00b7ses", "Gla\u00df", "voll", "Wein", "von", "un\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "NN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ergreift es alsobald, schl\u00e4gt hin und her", "tokens": ["Er\u00b7greift", "es", "al\u00b7so\u00b7bald", ",", "schl\u00e4gt", "hin", "und", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mit seiner d\u00fcrren Zung\u2019, und spricht:", "tokens": ["Mit", "sei\u00b7ner", "d\u00fcr\u00b7ren", "Zung'", ",", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zu guter Nacht, geliebtes Augen-Licht!", "tokens": ["Zu", "gu\u00b7ter", "Nacht", ",", "ge\u00b7lieb\u00b7tes", "Au\u00b7gen\u00b7Licht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mit diesem setzt ers an und macht das Wein-Glas", "tokens": ["Mit", "die\u00b7sem", "setzt", "ers", "an", "und", "macht", "das", "Wein\u00b7Glas"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "PTKVZ", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Fast jeder wird ob dieser That erschrecken;", "tokens": ["Fast", "je\u00b7der", "wird", "ob", "die\u00b7ser", "That", "er\u00b7schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "KOUS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Doch mu\u00df ich vielen dies, zur h\u00f6chsten Scham, ent-", "tokens": ["Doch", "mu\u00df", "ich", "vie\u00b7len", "dies", ",", "zur", "h\u00f6chs\u00b7ten", "Scham", ",", "ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "PDS", "$,", "APPRART", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Es machte Philopotamus", "tokens": ["Es", "mach\u00b7te", "Phi\u00b7lo\u00b7po\u00b7ta\u00b7mus"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Durch die Begier sich leiblich blind;", "tokens": ["Durch", "die", "Be\u00b7gier", "sich", "leib\u00b7lich", "blind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Allein wie manches Menschen-Kind", "tokens": ["Al\u00b7lein", "wie", "man\u00b7ches", "Men\u00b7schen\u00b7Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Spricht ebenfals: Wenn ich nur reich zu werden tauge;", "tokens": ["Spricht", "e\u00b7ben\u00b7fals", ":", "Wenn", "ich", "nur", "reich", "zu", "wer\u00b7den", "tau\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "KOUS", "PPER", "ADV", "ADJD", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergn\u00fcgt sich einmahl nur mein kitzelndes Gef\u00fchl;", "tokens": ["Ver\u00b7gn\u00fcgt", "sich", "ein\u00b7mahl", "nur", "mein", "kit\u00b7zeln\u00b7des", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erhalt\u2019 ich in der Ehr\u2019 mein vorgestrecktes Ziel;", "tokens": ["Er\u00b7halt'", "ich", "in", "der", "Ehr'", "mein", "vor\u00b7ge\u00b7streck\u00b7tes", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kann ich nur reicht, geliebt seyn, mich erh\u00f6hn;", "tokens": ["Kann", "ich", "nur", "reicht", ",", "ge\u00b7liebt", "seyn", ",", "mich", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "$,", "VVPP", "VAINF", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verlang ich nimmermehr, und w\u00e4r es noch so sch\u00f6n,", "tokens": ["Ver\u00b7lang", "ich", "nim\u00b7mer\u00b7mehr", ",", "und", "w\u00e4r", "es", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Sch\u00f6pfers Werck zu sehn.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Werck", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach m\u00f6chte dieses dir nicht mindern Schrecken,", "tokens": ["Ach", "m\u00f6ch\u00b7te", "die\u00b7ses", "dir", "nicht", "min\u00b7dern", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PDAT", "PPER", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als Philopotamus brutale That, erwecken!", "tokens": ["Als", "Phi\u00b7lo\u00b7po\u00b7ta\u00b7mus", "bru\u00b7ta\u00b7le", "That", ",", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach, m\u00f6gtest du des Sch\u00f6pfers Weisheit, Macht,", "tokens": ["Ach", ",", "m\u00f6g\u00b7test", "du", "des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ART", "NN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und Lieb\u2019, in seiner Wercke Pracht,", "tokens": ["Und", "Lieb'", ",", "in", "sei\u00b7ner", "Wer\u00b7cke", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zu sehen, und mit Lust zu fassen,", "tokens": ["Zu", "se\u00b7hen", ",", "und", "mit", "Lust", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dich durch Begierden blind, nicht ferner hindern lassen!", "tokens": ["Dich", "durch", "Be\u00b7gier\u00b7den", "blind", ",", "nicht", "fer\u00b7ner", "hin\u00b7dern", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJD", "$,", "PTKNEG", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}