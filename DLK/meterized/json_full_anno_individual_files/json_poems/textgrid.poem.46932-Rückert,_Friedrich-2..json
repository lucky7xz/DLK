{"textgrid.poem.46932": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich st\u00fcnd' auf einem hohen Turme,", "tokens": ["Da\u00df", "ich", "st\u00fcnd'", "auf", "ei\u00b7nem", "ho\u00b7hen", "Tur\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weit sichtbar rings in allen deutschen Reichen,", "tokens": ["Weit", "sicht\u00b7bar", "rings", "in", "al\u00b7len", "deut\u00b7schen", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit einer Stimme, Donnern zu vergleichen,", "tokens": ["Mit", "ei\u00b7ner", "Stim\u00b7me", ",", "Don\u00b7nern", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu rufen in den Sturm mit mehr als Sturme:", "tokens": ["Zu", "ru\u00b7fen", "in", "den", "Sturm", "mit", "mehr", "als", "Stur\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "APPR", "PIAT", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie lang willst du dich winden gleich dem Wurme.", "tokens": ["Wie", "lang", "willst", "du", "dich", "win\u00b7den", "gleich", "dem", "Wur\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PRF", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Krumm unter deines Feinds Triumphrads Speichen?", "tokens": ["Krumm", "un\u00b7ter", "dei\u00b7nes", "Feinds", "Tri\u00b7um\u00b7phrads", "Spei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hat er die harte Haut noch nicht mit Streichen", "tokens": ["Hat", "er", "die", "har\u00b7te", "Haut", "noch", "nicht", "mit", "Strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dir g'nug gerieben, da\u00df dich's endlich wurme?", "tokens": ["Dir", "g'\u00b7nug", "ge\u00b7rie\u00b7ben", ",", "da\u00df", "dich's", "end\u00b7lich", "wur\u00b7me", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Die Berge, wenn sie k\u00f6nnten, w\u00fcrden rufen:", "tokens": ["Die", "Ber\u00b7ge", ",", "wenn", "sie", "k\u00f6nn\u00b7ten", ",", "w\u00fcr\u00b7den", "ru\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "$,", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wir selber f\u00fchlten mit f\u00fchllosem R\u00fccken", "tokens": ["Wir", "sel\u00b7ber", "f\u00fchl\u00b7ten", "mit", "f\u00fchl\u00b7lo\u00b7sem", "R\u00fc\u00b