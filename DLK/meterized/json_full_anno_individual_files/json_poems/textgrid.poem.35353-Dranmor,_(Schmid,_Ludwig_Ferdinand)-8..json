{"textgrid.poem.35353": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit ist die Welt und breitet ihre Sch\u00e4tze", "tokens": ["Weit", "ist", "die", "Welt", "und", "brei\u00b7tet", "ih\u00b7re", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Vor unsern Augen aus; wen es gel\u00fcstet", "tokens": ["Vor", "un\u00b7sern", "Au\u00b7gen", "aus", ";", "wen", "es", "ge\u00b7l\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach goldnen Fr\u00fcchten, wage kampfger\u00fcstet", "tokens": ["Nach", "gold\u00b7nen", "Fr\u00fcch\u00b7ten", ",", "wa\u00b7ge", "kampf\u00b7ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinaus sich auf die gro\u00dfen Tummelpl\u00e4tze", "tokens": ["Hin\u00b7aus", "sich", "auf", "die", "gro\u00b7\u00dfen", "Tum\u00b7mel\u00b7pl\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Kr\u00e4ften, immer neuen, lebensfrischen;", "tokens": ["Mit", "Kr\u00e4f\u00b7ten", ",", "im\u00b7mer", "neu\u00b7en", ",", "le\u00b7bens\u00b7fri\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn \u00bbHammer oder Ambos\u00ab ist die Frage,", "tokens": ["Denn", "\u00bb", "Ham\u00b7mer", "o\u00b7der", "Am\u00b7bos", "\u00ab", "ist", "die", "Fra\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "KON", "NE", "$(", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wer sich selbst besch\u00fctzen mu\u00df, erschlage", "tokens": ["Und", "wer", "sich", "selbst", "be\u00b7sch\u00fct\u00b7zen", "mu\u00df", ",", "er\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "PRF", "ADV", "VVINF", "VMFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Schlangen, die zu seinen F\u00fc\u00dfen zischen.", "tokens": ["Die", "Schlan\u00b7gen", ",", "die", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "zi\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Durch Tr\u00fcmmer geht der Angriff wie die Flucht;", "tokens": ["Durch", "Tr\u00fcm\u00b7mer", "geht", "der", "An\u00b7griff", "wie", "die", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Feinde Tro\u00df erm\u00fcdend und verscheuchend,", "tokens": ["Der", "Fein\u00b7de", "Tro\u00df", "er\u00b7m\u00fc\u00b7dend", "und", "ver\u00b7scheu\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Auf steiler H\u00f6he, schwei\u00dfbedeckt und keuchend,", "tokens": ["Auf", "stei\u00b7ler", "H\u00f6\u00b7he", ",", "schwei\u00df\u00b7be\u00b7deckt", "und", "keu\u00b7chend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So pfl\u00fcckt der Sieger die ersehnte Frucht.", "tokens": ["So", "pfl\u00fcckt", "der", "Sie\u00b7ger", "die", "er\u00b7sehn\u00b7te", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mein Zuruf gilt allein den k\u00fchnen Thaten,", "tokens": ["Mein", "Zu\u00b7ruf", "gilt", "al\u00b7lein", "den", "k\u00fch\u00b7nen", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der offnen Fehde, den beherzten W\u00fcrfen,", "tokens": ["Der", "off\u00b7nen", "Feh\u00b7de", ",", "den", "be\u00b7herz\u00b7ten", "W\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nicht jener Brut erb\u00e4rmlicher Piraten,", "tokens": ["Nicht", "je\u00b7ner", "Brut", "er\u00b7b\u00e4rm\u00b7li\u00b7cher", "Pi\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dir nur gestohlnen Wein hinunterschl\u00fcrfen.", "tokens": ["Dir", "nur", "ge\u00b7stohl\u00b7nen", "Wein", "hin\u00b7un\u00b7ter\u00b7schl\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Kaum einer siegt von hundert wackern Streitern.", "tokens": ["Kaum", "ei\u00b7ner", "siegt", "von", "hun\u00b7dert", "wa\u00b7ckern", "Strei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Hei\u00df ist die M\u00fche, ungewi\u00df der Preis,", "tokens": ["Hei\u00df", "ist", "die", "M\u00fc\u00b7he", ",", "un\u00b7ge\u00b7wi\u00df", "der", "Preis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Statt eines gr\u00fcnen oft ein d\u00fcrres Reis,", "tokens": ["Statt", "ei\u00b7nes", "gr\u00fc\u00b7nen", "oft", "ein", "d\u00fcr\u00b7res", "Reis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ein Splitter aus zerbrochnen Himmelsleitern;", "tokens": ["Ein", "Split\u00b7ter", "aus", "zer\u00b7broch\u00b7nen", "Him\u00b7mels\u00b7lei\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Statt Engelsch\u00f6re \u2013 nichts als k\u00fchle Phrasen", "tokens": ["Statt", "En\u00b7gel\u00b7sch\u00f6\u00b7re", "\u2013", "nichts", "als", "k\u00fch\u00b7le", "Phra\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$(", "PIS", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "F\u00fcr den Entt\u00e4uschten, M\u00fcden, Lebenssatten;", "tokens": ["F\u00fcr", "den", "Ent\u00b7t\u00e4uschten", ",", "M\u00fc\u00b7den", ",", "Le\u00b7bens\u00b7sat\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Vielleicht ein Halt auf schattigen Oasen", "tokens": ["Viel\u00b7leicht", "ein", "Halt", "auf", "schat\u00b7ti\u00b7gen", "O\u00b7a\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und dann die Ewigkeit in Todesschatten.", "tokens": ["Und", "dann", "die", "E\u00b7wig\u00b7keit", "in", "To\u00b7des\u00b7schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Wo sind die andern? Links und rechts versch\u00fcttet,", "tokens": ["Wo", "sind", "die", "an\u00b7dern", "?", "Links", "und", "rechts", "ver\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "$.", "NE", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Wenn nicht ein \u00e4rmlich Brot mit Thr\u00e4nen essend,", "tokens": ["Wenn", "nicht", "ein", "\u00e4rm\u00b7lich", "Brot", "mit", "Thr\u00e4\u00b7nen", "es\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wenn nicht in Dunkelheit sich selbst vergessend,", "tokens": ["Wenn", "nicht", "in", "Dun\u00b7kel\u00b7heit", "sich", "selbst", "ver\u00b7ges\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Der Leib gebrochen und der Geist zerr\u00fcttet.", "tokens": ["Der", "Leib", "ge\u00b7bro\u00b7chen", "und", "der", "Geist", "zer\u00b7r\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Doch besser noch, der Jugend nicht entsagen,", "tokens": ["Doch", "bes\u00b7ser", "noch", ",", "der", "Ju\u00b7gend", "nicht", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Dir tief ins Auge schauen, Sphinx von Theben,", "tokens": ["Dir", "tief", "ins", "Au\u00b7ge", "schau\u00b7en", ",", "Sphinx", "von", "The\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVINF", "$,", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Als, eine Null im schalen Alltagsleben,", "tokens": ["Als", ",", "ei\u00b7ne", "Null", "im", "scha\u00b7len", "All\u00b7tags\u00b7le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Sich nicht an deines R\u00e4tsels L\u00f6sung wagen.", "tokens": ["Sich", "nicht", "an", "dei\u00b7nes", "R\u00e4t\u00b7sels", "L\u00f6\u00b7sung", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Es h\u00e4uft der Tod Skelette auf Skelette;", "tokens": ["Es", "h\u00e4uft", "der", "Tod", "Ske\u00b7let\u00b7te", "auf", "Ske\u00b7let\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Doch ist es edler, mit ersch\u00f6pfter Kraft", "tokens": ["Doch", "ist", "es", "ed\u00b7ler", ",", "mit", "er\u00b7sch\u00f6pf\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Verscheiden, als in engbegrenzter Haft", "tokens": ["Ver\u00b7schei\u00b7den", ",", "als", "in", "eng\u00b7be\u00b7grenz\u00b7ter", "Haft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Zu Grunde gehen an der Sklavenkette.", "tokens": ["Zu", "Grun\u00b7de", "ge\u00b7hen", "an", "der", "Skla\u00b7ven\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Die Welt ist gro\u00