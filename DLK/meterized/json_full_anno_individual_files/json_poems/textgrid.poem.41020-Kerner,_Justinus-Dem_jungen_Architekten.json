"}}, "stanza.6": {"line.1": {"text": "Drum keine Tr\u00e4nen dieser Bahre,", "tokens": ["Drum", "kei\u00b7ne", "Tr\u00e4\u00b7nen", "die\u00b7ser", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist den Engeln Gottes Lust!", "tokens": ["Sie", "ist", "den", "En\u00b7geln", "Got\u00b7tes", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schlingt Rosen ihm in seine Haare,", "tokens": ["Schlingt", "Ro\u00b7sen", "ihm", "in", "sei\u00b7ne", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Legt Lilien auf seine Brust!", "tokens": ["Legt", "Li\u00b7li\u00b7en", "auf", "sei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Ma\u00dfstab ruht, es ruht das Eisen", "tokens": ["Der", "Ma\u00df\u00b7stab", "ruht", ",", "es", "ruht", "das", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In deiner Hand, der Pinsel ruht,", "tokens": ["In", "dei\u00b7ner", "Hand", ",", "der", "Pin\u00b7sel", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die du i