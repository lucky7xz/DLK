{"dta.poem.8666": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Unm\u00f6gliches .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Bevor mein Blick den Zauber noch getrunken,", "tokens": ["Be\u00b7vor", "mein", "Blick", "den", "Zau\u00b7ber", "noch", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der, wie die Farbenpracht am Demant gl\u00fcht,", "tokens": ["Der", ",", "wie", "die", "Far\u00b7ben\u00b7pracht", "am", "De\u00b7mant", "gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich tausendfach, doch immer neu, umbl\u00fcht;", "tokens": ["Dich", "tau\u00b7send\u00b7fach", ",", "doch", "im\u00b7mer", "neu", ",", "um\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Horcht' ich dem Freund, in Ahnungen versunken.", "tokens": ["Horcht'", "ich", "dem", "Freund", ",", "in", "Ah\u00b7nun\u00b7gen", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir seh'n des Berges Haupt in Purpur prangen,", "tokens": ["Wir", "seh'n", "des", "Ber\u00b7ges", "Haupt", "in", "Pur\u00b7pur", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn schon die Sonne sank und D\u00e4mmerung", "tokens": ["Wenn", "schon", "die", "Son\u00b7ne", "sank", "und", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Den Hain umflort: so strahlt' Erinnerung", "tokens": ["Den", "Hain", "um\u00b7flort", ":", "so", "strahlt'", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An dich, Geliebte, von des Freundes Wangen;", "tokens": ["An", "dich", ",", "Ge\u00b7lieb\u00b7te", ",", "von", "des", "Freun\u00b7des", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Begeistert taucht' er in des Busens Tiefen", "tokens": ["Be\u00b7geis\u00b7tert", "taucht'", "er", "in", "des", "Bu\u00b7sens", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den Pinsel, und er malte warm und mild", "tokens": ["Den", "Pin\u00b7sel", ",", "und", "er", "mal\u00b7te", "warm", "und", "mild"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dem sel'gen Horcher dein entz\u00fcckend Bild,", "tokens": ["Dem", "sel'\u00b7gen", "Hor\u00b7cher", "dein", "ent\u00b7z\u00fc\u00b7ckend", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Gef\u00fchle weckend, die seit lange schliefen.", "tokens": ["Ge\u00b7f\u00fch\u00b7le", "we\u00b7ckend", ",", "die", "seit", "lan\u00b7ge", "schlie\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PRELS", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch wie's dem Dichter nimmer will gelingen,", "tokens": ["Doch", "wie's", "dem", "Dich\u00b7ter", "nim\u00b7mer", "will", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Des Busens Drang ins enge Wort zu zwingen,", "tokens": ["Des", "Bu\u00b7sens", "Drang", "ins", "en\u00b7ge", "Wort", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Hin\u00fcber uns in seine Welt zu singen:", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "uns", "in", "sei\u00b7ne", "Welt", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "So hat der Freund vergebens dich gemalt,", "tokens": ["So", "hat", "der", "Freund", "ver\u00b7ge\u00b7bens", "dich", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sie nicht erreicht, die g\u00f6ttliche Gestalt,", "tokens": ["Sie", "nicht", "er\u00b7reicht", ",", "die", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und deiner Seele stille Allgewalt.", "tokens": ["Und", "dei\u00b7ner", "See\u00b7le", "stil\u00b7le", "All\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}