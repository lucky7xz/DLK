{"dta.poem.304": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet sie nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Galante Lesbia, ach z\u00fcrne nicht auf mich!", "tokens": ["Ga\u00b7lan\u00b7te", "Les\u00b7bia", ",", "ach", "z\u00fcr\u00b7ne", "nicht", "auf", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ITJ", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn dich mein falscher Schein zur Liebe hat bewogen/", "tokens": ["Wenn", "dich", "mein", "fal\u00b7scher", "Schein", "zur", "Lie\u00b7be", "hat", "be\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Stellen hat dein Hertz/ das ehrlich ist/ betrogen/", "tokens": ["Mein", "Stel\u00b7len", "hat", "dein", "Hertz", "/", "das", "ehr\u00b7lich", "ist", "/", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$(", "PDS", "ADJD", "VAFIN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Galante Lesbia ach z\u00fcrne nicht auf mich!", "tokens": ["Ga\u00b7lan\u00b7te", "Les\u00b7bia", "ach", "z\u00fcr\u00b7ne", "nicht", "auf", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Verbanne nur die Gunst/ so dein Gem\u00fchte hegt/", "tokens": ["Ver\u00b7ban\u00b7ne", "nur", "die", "Gunst", "/", "so", "dein", "Ge\u00b7m\u00fch\u00b7te", "hegt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Schicksahl wehret mir dein Artig-seyn zu lieben/", "tokens": ["Mein", "Schick\u00b7sahl", "weh\u00b7ret", "mir", "dein", "Ar\u00b7tig\u00b7seyn", "zu", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Stellen soll nicht mehr dein treues Hertz betr\u00fcben/", "tokens": ["Mein", "Stel\u00b7len", "soll", "nicht", "mehr", "dein", "treu\u00b7es", "Hertz", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verbanne nur die Gunst/ so dein Gem\u00fchte hegt.", "tokens": ["Ver\u00b7ban\u00b7ne", "nur", "die", "Gunst", "/", "so", "dein", "Ge\u00b7m\u00fch\u00b7te", "hegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Befreye deine Brust von ", "tokens": ["Be\u00b7fre\u00b7ye", "dei\u00b7ne", "Brust", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es l\u00e4st mein Wanckelmuth mich nicht best\u00e4ndig bleiben/", "tokens": ["Es", "l\u00e4st", "mein", "Wan\u00b7ckel\u00b7muth", "mich", "nicht", "be\u00b7st\u00e4n\u00b7dig", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Schiff verlanget nicht im Haven einzutreiben/", "tokens": ["Mein", "Schiff", "ver\u00b7lan\u00b7get", "nicht", "im", "Ha\u00b7ven", "ein\u00b7zu\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPRART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Befreye deine Brust von ", "tokens": ["Be\u00b7fre\u00b7ye", "dei\u00b7ne", "Brust", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Feuer l\u00f6sche aus/ das deine Liebe nehrt/", "tokens": ["Das", "Feu\u00b7er", "l\u00f6\u00b7sche", "aus", "/", "das", "dei\u00b7ne", "Lie\u00b7be", "nehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich sp\u00fchre keinen Brand/ es frieren meine Sinnen/", "tokens": ["Ich", "sp\u00fch\u00b7re", "kei\u00b7nen", "Brand", "/", "es", "frie\u00b7ren", "mei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Hertze l\u00e4st sich nicht durch frem des Feur gewinnen/", "tokens": ["Mein", "Hert\u00b7ze", "l\u00e4st", "sich", "nicht", "durch", "frem", "des", "Feur", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "PRF", "PTKNEG", "APPR", "ADJA", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Feuer l\u00f6sche aus/ das deine Liebe nehrt.", "tokens": ["Das", "Feu\u00b7er", "l\u00f6\u00b7sche", "aus", "/", "das", "dei\u00b7ne", "Lie\u00b7be", "nehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Verbunden will ich dir zu steten Diensten seyn/", "tokens": ["Ver\u00b7bun\u00b7den", "will", "ich", "dir", "zu", "ste\u00b7ten", "Diens\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein! mein Hertze kan kein Liebes Feur ernehren/", "tokens": ["Al\u00b7lein", "!", "mein", "Hert\u00b7ze", "kan", "kein", "Lie\u00b7bes", "Feur", "er\u00b7neh\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPOSAT", "VVFIN", "VMFIN", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Sternen Einflu\u00df wills mit aller Macht verwehren/", "tokens": ["Der", "Ster\u00b7nen", "Ein\u00b7flu\u00df", "wills", "mit", "al\u00b7ler", "Macht", "ver\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verbunden aber will ich dir zu Dienste seyn.", "tokens": ["Ver\u00b7bun\u00b7den", "a\u00b7ber", "will", "ich", "dir", "zu", "Diens\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "PPER", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich z\u00fcrne selbst mit mir ob dieser b\u00f6sen Art/", "tokens": ["Ich", "z\u00fcr\u00b7ne", "selbst", "mit", "mir", "ob", "die\u00b7ser", "b\u00f6\u00b7sen", "Art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "KOUS", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch will mein Hertz nicht zur Liebe sich bequehmen/", "tokens": ["Doch", "will", "mein", "Hertz", "nicht", "zur", "Lie\u00b7be", "sich", "be\u00b7queh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PTKNEG", "APPRART", "NN", "PRF", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich mag der ", "tokens": ["Ich", "mag", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Drum z\u00fcrn\u2019 ich mit mir selbst ob dieser b\u00f6sen Art.", "tokens": ["Drum", "z\u00fcrn'", "ich", "mit", "mir", "selbst", "ob", "die\u00b7ser", "b\u00f6\u00b7sen", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "KOUS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dis aber ", "tokens": ["Dis", "a\u00b7ber"], "token_info": ["word", "word"], "pos": ["PDS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wenn mich dein sch\u00f6ner Mund will seinen Bruder nennen", "tokens": ["Wenn", "mich", "dein", "sch\u00f6\u00b7ner", "Mund", "will", "sei\u00b7nen", "Bru\u00b7der", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vielleichte macht mich noch dein Liebes-Wesen brennen/", "tokens": ["Viel\u00b7leich\u00b7te", "macht", "mich", "noch", "dein", "Lie\u00b7bes\u00b7We\u00b7sen", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schafft/ da\u00df mit der Zeit ich geh dein W\u00fcnschen ein.", "tokens": ["Und", "schafft", "/", "da\u00df", "mit", "der", "Zeit", "ich", "geh", "dein", "W\u00fcn\u00b7schen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}