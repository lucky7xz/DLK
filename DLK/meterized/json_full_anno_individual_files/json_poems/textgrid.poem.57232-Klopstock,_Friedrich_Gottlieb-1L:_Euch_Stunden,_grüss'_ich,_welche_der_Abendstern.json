{"textgrid.poem.57232": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Euch Stunden, gr\u00fcss' ich, welche der Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1748, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch Stunden, gr\u00fcss' ich, welche der Abendstern", "tokens": ["Euch", "Stun\u00b7den", ",", "gr\u00fcss'", "ich", ",", "wel\u00b7che", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still in der D\u00e4mrung mir zur Erfindung bringt,", "tokens": ["Still", "in", "der", "D\u00e4m\u00b7rung", "mir", "zur", "Er\u00b7fin\u00b7dung", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "O geht nicht, ohne mich zu segnen,", "tokens": ["O", "geht", "nicht", ",", "oh\u00b7ne", "mich", "zu", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht ohne grosse Gedanken weiter!", "tokens": ["Nicht", "oh\u00b7ne", "gros\u00b7se", "Ge\u00b7dan\u00b7ken", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Thor des Himmels sprach ein Unsterblicher:", "tokens": ["Im", "Thor", "des", "Him\u00b7mels", "sprach", "ein", "U\u00b7nsterb\u00b7li\u00b7cher", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbeilt, heilge Stunden, die ihr die Unterwelt", "tokens": ["\u00bb", "eilt", ",", "heil\u00b7ge", "Stun\u00b7den", ",", "die", "ihr", "die", "Un\u00b7ter\u00b7welt"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aus diesen hohen Pforten Gottes", "tokens": ["Aus", "die\u00b7sen", "ho\u00b7hen", "Pfor\u00b7ten", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selten besuchet, zu jenem J\u00fcngling,", "tokens": ["Sel\u00b7ten", "be\u00b7su\u00b7chet", ",", "zu", "je\u00b7nem", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Der Gott, den Mittler, Adams Geschlechte singt!", "tokens": ["Der", "Gott", ",", "den", "Mitt\u00b7ler", ",", "A\u00b7dams", "Ge\u00b7schlech\u00b7te", "singt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Deckt ihn mit dieser schattigen k\u00fchlen Nacht", "tokens": ["Deckt", "ihn", "mit", "die\u00b7ser", "schat\u00b7ti\u00b7gen", "k\u00fch\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "ADJA", "ADJA", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der goldnen Fl\u00fcgel, dass er einsam", "tokens": ["Der", "gold\u00b7nen", "Fl\u00fc\u00b7gel", ",", "dass", "er", "ein\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter dem himlischen Schatten dichte", "tokens": ["Un\u00b7ter", "dem", "him\u00b7li\u00b7schen", "Schat\u00b7ten", "dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Was ihr gebahret, Stunden, das werden einst,", "tokens": ["Was", "ihr", "ge\u00b7bah\u00b7ret", ",", "Stun\u00b7den", ",", "das", "wer\u00b7den", "einst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "NN", "$,", "PDS", "VAFIN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weissaget Salem, ferne Jahrhunderte", "tokens": ["Weis\u00b7sa\u00b7get", "Sa\u00b7lem", ",", "fer\u00b7ne", "Jahr\u00b7hun\u00b7der\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NE", "$,", "ADJA", "NN"], "meter": "+--+-+-+---", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Vernehmen, werden Gott, den Mittler", "tokens": ["Ver\u00b7neh\u00b7men", ",", "wer\u00b7den", "Gott", ",", "den", "Mitt\u00b7ler"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VAFIN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ernster betrachten, und heilig leben.\u00ab", "tokens": ["Erns\u00b7ter", "be\u00b7trach\u00b7ten", ",", "und", "hei\u00b7lig", "le\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "ADJD", "VVINF", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Er sprachs. Ein Nachklang von dem Unsterblichen", "tokens": ["Er", "sprachs", ".", "Ein", "Nach\u00b7klang", "von", "dem", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fuhr mir gewaltig durch mein Gebein dahin;", "tokens": ["Fuhr", "mir", "ge\u00b7wal\u00b7tig", "durch", "mein", "Ge\u00b7bein", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PAV", "$."], "meter": "+--+-++-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ich stand, als ging' in Donnerwettern", "tokens": ["Ich", "stand", ",", "als", "ging'", "in", "Don\u00b7ner\u00b7wet\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber mir Gott, und erstaunte freudig.", "tokens": ["\u00dc\u00b7ber", "mir", "Gott", ",", "und", "er\u00b7staun\u00b7te", "freu\u00b7dig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Dass diesem Ort kein schwatzender Prediger,", "tokens": ["Dass", "die\u00b7sem", "Ort", "kein", "schwat\u00b7zen\u00b7der", "Pre\u00b7di\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Kein wandelloser Christ, der Propheten selbst", "tokens": ["Kein", "wan\u00b7del\u00b7lo\u00b7ser", "Christ", ",", "der", "Pro\u00b7phe\u00b7ten", "selbst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht f\u00fchlt, sich nahe! Jeder Laut, der", "tokens": ["Nicht", "f\u00fchlt", ",", "sich", "na\u00b7he", "!", "Je\u00b7der", "Laut", ",", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKNEG", "VVFIN", "$,", "PRF", "PTKVZ", "$.", "PIS", "APPR", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00f6ttliche Dinge nicht t\u00f6nt, verstumme!", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Din\u00b7ge", "nicht", "t\u00f6nt", ",", "ver\u00b7stum\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVFIN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Deckt, heilge Stunden, decket mit eurer Nacht", "tokens": ["Deckt", ",", "heil\u00b7ge", "Stun\u00b7den", ",", "de\u00b7cket", "mit", "eu\u00b7rer", "Nacht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den stillen Eingang, dass ihn kein Sterblicher", "tokens": ["Den", "stil\u00b7len", "Ein\u00b7gang", ",", "dass", "ihn", "kein", "Sterb\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Betrete, winkt selbst meiner Freunde", "tokens": ["Be\u00b7tre\u00b7te", ",", "winkt", "selbst", "mei\u00b7ner", "Freun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gerne gehorchten, geliebten Fuss weg!", "tokens": ["Ger\u00b7ne", "ge\u00b7horch\u00b7ten", ",", "ge\u00b7lieb\u00b7ten", "Fuss", "weg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Nur nicht, wenn Schmidt will aus den Versamlungen", "tokens": ["Nur", "nicht", ",", "wenn", "Schmidt", "will", "aus", "den", "Ver\u00b7sam\u00b7lun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "$,", "KOUS", "NE", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Musen Sions zu mir her\u00fcbergehn;", "tokens": ["Der", "Mu\u00b7sen", "Si\u00b7ons", "zu", "mir", "her\u00b7\u00fc\u00b7ber\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch, dass du nur vom Weltgerichte,", "tokens": ["Doch", ",", "dass", "du", "nur", "vom", "Welt\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder von deiner erhabnen Schwester,", "tokens": ["O\u00b7der", "von", "dei\u00b7ner", "er\u00b7hab\u00b7nen", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Dich unterredest! Auch wenn sie richtet, ist", "tokens": ["Dich", "un\u00b7ter\u00b7re\u00b7dest", "!", "Auch", "wenn", "sie", "rich\u00b7tet", ",", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "KOUS", "PPER", "VVFIN", "$,", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie liebensw\u00fcrdig. Was ihr empfindend Herz", "tokens": ["Sie", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", ".", "Was", "ihr", "emp\u00b7fin\u00b7dend", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "$.", "PWS", "PPER", "VVPP", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In unsern Liedern nicht empfunden,", "tokens": ["In", "un\u00b7sern", "Lie\u00b7dern", "nicht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sey nicht mehr! was sie empfand, sey ewig!", "tokens": ["Sey", "nicht", "mehr", "!", "was", "sie", "emp\u00b7fand", ",", "sey", "e\u00b7wig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "$.", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Euch Stunden, gr\u00fcss' ich, welche der Abendstern", "tokens": ["Euch", "Stun\u00b7den", ",", "gr\u00fcss'", "ich", ",", "wel\u00b7che", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "punct