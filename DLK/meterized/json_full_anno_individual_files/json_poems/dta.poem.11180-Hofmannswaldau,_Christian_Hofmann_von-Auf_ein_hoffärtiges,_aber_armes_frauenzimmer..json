{"dta.poem.11180": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein hoff\u00e4rtiges, aber armes  \n frauenzimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du zierst den kopff mit band, die stirn mit krausen haaren,", "tokens": ["Du", "zierst", "den", "kopff", "mit", "band", ",", "die", "stirn", "mit", "krau\u00b7sen", "haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVFIN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den hals mit perl und gold, die brust mit nackten waaren;", "tokens": ["Den", "hals", "mit", "perl", "und", "gold", ",", "die", "brust", "mit", "nack\u00b7ten", "waa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du denckst, wer da nicht will? Ach nein! du irrest sehr:", "tokens": ["Du", "denckst", ",", "wer", "da", "nicht", "will", "?", "Ach", "nein", "!", "du", "ir\u00b7rest", "sehr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADV", "PTKNEG", "VMFIN", "$.", "NN", "PTKANT", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was nutzt ein offner kram, wenn das gew\u00f6lbe leer?", "tokens": ["Was", "nutzt", "ein", "off\u00b7ner", "kram", ",", "wenn", "das", "ge\u00b7w\u00f6l\u00b7be", "leer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}