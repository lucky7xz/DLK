{"textgrid.poem.39068": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, sie taugt nicht, unsere Jugend!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, sie taugt nicht, unsere Jugend!", "tokens": ["Nein", ",", "sie", "taugt", "nicht", ",", "un\u00b7se\u00b7re", "Ju\u00b7gend", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nein, ihr fehlt die beste Tugend,", "tokens": ["Nein", ",", "ihr", "fehlt", "die", "bes\u00b7te", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ihr gebricht die Billigkeit.", "tokens": ["ihr", "ge\u00b7bricht", "die", "Bil\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herzlich lieben, herzlich hassen,", "tokens": ["Herz\u00b7lich", "lie\u00b7ben", ",", "herz\u00b7lich", "has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ja, in B\u00fccher mag es passen;", "tokens": ["ja", ",", "in", "B\u00fc\u00b7cher", "mag", "es", "pas\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "doch als Mensch mu\u00df man sich fassen,", "tokens": ["doch", "als", "Mensch", "mu\u00df", "man", "sich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VMFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "und die Wahrheit kommt nicht weit:", "tokens": ["und", "die", "Wahr\u00b7heit", "kommt", "nicht", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seid doch billig!", "tokens": ["Seid", "doch", "bil\u00b7lig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Seid doch willig!", "tokens": ["Seid", "doch", "wil\u00b7lig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Jedes Ding hat seine Zeit.", "tokens": ["Je\u00b7des", "Ding", "hat", "sei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Allzu scharf, wi\u00dft ihr, macht schartig.", "tokens": ["All\u00b7zu", "scharf", ",", "wi\u00dft", "ihr", ",", "macht", "schar\u00b7tig", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tadeln d\u00fcrft ihr aber artig!", "tokens": ["Ta\u00b7deln", "d\u00fcrft", "ihr", "a\u00b7ber", "ar\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00e4sonieren, aber sacht!", "tokens": ["R\u00e4\u00b7so\u00b7nie\u00b7ren", ",", "a\u00b7ber", "sacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcstern m\u00fc\u00dft ihr, niemals sprechen,", "tokens": ["Fl\u00fcs\u00b7tern", "m\u00fc\u00dft", "ihr", ",", "nie\u00b7mals", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "immer biegen, niemals brechen;", "tokens": ["im\u00b7mer", "bie\u00b7gen", ",", "nie\u00b7mals", "bre\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "jeder Mensch hat seine Schw\u00e4chen,", "tokens": ["je\u00b7der", "Mensch", "hat", "sei\u00b7ne", "Schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "jeder Tag hat seine Nacht:", "tokens": ["je\u00b7der", "Tag", "hat", "sei\u00b7ne", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seid doch billig!", "tokens": ["Seid", "doch", "bil\u00b7lig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Seid doch willig!", "tokens": ["Seid", "doch", "wil\u00b7lig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Nehmt die Billigkeit in acht!", "tokens": ["Nehmt", "die", "Bil\u00b7lig\u00b7keit", "in", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schwarz und Wei\u00df, das sind Extreme:", "tokens": ["Schwarz", "und", "Wei\u00df", ",", "das", "sind", "Ext\u00b7re\u00b7me", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "PDS", "VAFIN", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Grau, das ist das Angenehme,", "tokens": ["Grau", ",", "das", "ist", "das", "An\u00b7ge\u00b7neh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "das so Schwarz wie Wei\u00df enth\u00e4lt.", "tokens": ["das", "so", "Schwarz", "wie", "Wei\u00df", "ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "KOKOM", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede Sache hat zwei Seiten,", "tokens": ["Je\u00b7de", "Sa\u00b7che", "hat", "zwei", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00fcber jede l\u00e4\u00dft sich streiten;", "tokens": ["\u00fc\u00b7ber", "je\u00b7de", "l\u00e4\u00dft", "sich", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "anders denken andre Zeiten,", "tokens": ["an\u00b7ders", "den\u00b7ken", "and\u00b7re", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "und das Neueste gef\u00e4llt:", "tokens": ["und", "das", "Neu\u00b7es\u00b7te", "ge\u00b7f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seid doch billig!", "tokens": ["Seid", "doch", "bil\u00b7lig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Seid doch willig!", "tokens": ["Seid", "doch", "wil\u00b7lig", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Billigkeit regiert die Welt.", "tokens": ["Bil\u00b7lig\u00b7keit", "re\u00b7giert", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seht, ihr selber werdet \u00e4lter,", "tokens": ["Seht", ",", "ihr", "sel\u00b7ber", "wer\u00b7det", "\u00e4l\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "eure Herzen werden k\u00e4lter,", "tokens": ["eu\u00b7re", "Her\u00b7zen", "wer\u00b7den", "k\u00e4l\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und das Lebens\u