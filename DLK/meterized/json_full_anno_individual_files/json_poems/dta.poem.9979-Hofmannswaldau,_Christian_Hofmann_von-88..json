{"dta.poem.9979": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ungl\u00fcck unser vater ist/ unsern leib beseelen schmertzen;", "tokens": ["Un\u00b7gl\u00fcck", "un\u00b7ser", "va\u00b7ter", "ist", "/", "un\u00b7sern", "leib", "be\u00b7see\u00b7len", "schmert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAFIN", "$(", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dennoch wir die artzney sind f\u00fcr die wunden in dem hertzen.", "tokens": ["Den\u00b7noch", "wir", "die", "artz\u00b7ney", "sind", "f\u00fcr", "die", "wun\u00b7den", "in", "dem", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Offters wird von uns entdeckt/ was kein mensch hat solle\u0303 wissen;", "tokens": ["Off\u00b7ters", "wird", "von", "uns", "ent\u00b7deckt", "/", "was", "kein", "mensch", "hat", "soll\u1ebd", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$(", "PWS", "PIAT", "NN", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Darumb wir zur straffe auch kaum gebohren sterben m\u00fcssen.", "tokens": ["Da\u00b7rumb", "wir", "zur", "straf\u00b7fe", "auch", "kaum", "ge\u00b7boh\u00b7ren", "ster\u00b7ben", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPRART", "VVFIN", "ADV", "ADV", "VVPP", "VVINF", "VMINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}}}}}