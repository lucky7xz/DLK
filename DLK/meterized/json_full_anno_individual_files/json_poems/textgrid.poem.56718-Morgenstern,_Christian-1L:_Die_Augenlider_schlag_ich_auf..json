{"textgrid.poem.56718": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Augenlider schlag ich auf.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Augenlider schlag ich auf.", "tokens": ["Die", "Au\u00b7gen\u00b7li\u00b7der", "schlag", "ich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab so gro\u00df und sch\u00f6n getr\u00e4umt,", "tokens": ["Ich", "hab", "so", "gro\u00df", "und", "sch\u00f6n", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df noch mein Blick in seinem Lauf", "tokens": ["da\u00df", "noch", "mein", "Blick", "in", "sei\u00b7nem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als wie ein m\u00fcder Wandrer s\u00e4umt.", "tokens": ["als", "wie", "ein", "m\u00fc\u00b7der", "Wand\u00b7rer", "s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon werden fern im gelben Ost", "tokens": ["Schon", "wer\u00b7den", "fern", "im", "gel\u00b7ben", "Ost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Sonnenrosse aufgez\u00e4umt.", "tokens": ["die", "Son\u00b7nen\u00b7ros\u00b7se", "auf\u00b7ge\u00b7z\u00e4umt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von ihren M\u00e4hnen flie\u00dfen Feuer,", "tokens": ["Von", "ih\u00b7ren", "M\u00e4h\u00b7nen", "flie\u00b7\u00dfen", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und Feuer stiebt von ihrem Huf.", "tokens": ["und", "Feu\u00b7er", "stiebt", "von", "ih\u00b7rem", "Huf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hinab zur Ebne kriecht der Frost.", "tokens": ["Hin\u00b7ab", "zur", "Eb\u00b7ne", "kriecht", "der", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und von der Berge Hochgem\u00e4uer", "tokens": ["Und", "von", "der", "Ber\u00b7ge", "Hoch\u00b7ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "ert\u00f6nt der Aare Morgenruf.", "tokens": ["er\u00b7t\u00f6nt", "der", "Aa\u00b7re", "Mor\u00b7gen\u00b7ruf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun wach ich ganz. Vor meiner Schau", "tokens": ["Nun", "wach", "ich", "ganz", ".", "Vor", "mei\u00b7ner", "Schau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "erw\u00f6lbt azurn sich ein Palast.", "tokens": ["er\u00b7w\u00f6lbt", "a\u00b7zurn", "sich", "ein", "Pa\u00b7last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bleicht der Felsenfliesen Grau", "tokens": ["Es", "bleicht", "der", "Fel\u00b7sen\u00b7flie\u00b7sen", "Grau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und l\u00e4dt den Purpur sich zu Gast.", "tokens": ["und", "l\u00e4dt", "den", "Pur\u00b7pur", "sich", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Quellge\u00e4ders dumpfes Blau", "tokens": ["Des", "Quell\u00b7ge\u00e4\u00b7ders", "dum\u00b7pfes", "Blau"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "verblitzt in heitren Silberglast.", "tokens": ["ver\u00b7blitzt", "in", "hei\u00b7tren", "Sil\u00b7ber\u00b7glast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und langsam taucht aus fahler Nacht", "tokens": ["Und", "lang\u00b7sam", "taucht", "aus", "fah\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der Ebnen bunte Teppichpracht.", "tokens": ["der", "Eb\u00b7nen", "bun\u00b7te", "Tep\u00b7pich\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "All dies mein Lehn aus Phanta's Hand!", "tokens": ["All", "dies", "mein", "Lehn", "aus", "Phan\u00b7ta's", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "PPOSAT", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein K\u00f6nig ich ob Meer und Land,", "tokens": ["Ein", "K\u00f6\u00b7nig", "ich", "ob", "Meer", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ob Wolkenraum, ob Firmament!", "tokens": ["ob", "Wol\u00b7ken\u00b7raum", ",", "ob", "Fir\u00b7ma\u00b7ment", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Gott, des Reich nicht Grenze kennt.", "tokens": ["Ein", "Gott", ",", "des", "Reich", "nicht", "Gren\u00b7ze", "kennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies alles mein! Wohin ich schreite,", "tokens": ["Dies", "al\u00b7les", "mein", "!", "Wo\u00b7hin", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPOSAT", "$.", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "begr\u00fc\u00dft mich dienend die Natur:", "tokens": ["be\u00b7gr\u00fc\u00dft", "mich", "die\u00b7nend", "die", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ein Nymphenheer gebiert die Flur", "tokens": ["ein", "Nym\u00b7phen\u00b7heer", "ge\u00b7biert", "die", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "aus ihrem Scho\u00df mir zum Geleite;", "tokens": ["aus", "ih\u00b7rem", "Scho\u00df", "mir", "zum", "Ge\u00b7lei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "und G\u00f6tter steigen aus der Weite", "tokens": ["und", "G\u00f6t\u00b7ter", "stei\u00b7gen", "aus", "der", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "des Alls herab auf meine Spur.", "tokens": ["des", "Alls", "her\u00b7ab", "auf", "mei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das m\u00e4chtigste, das feinste Klingen", "tokens": ["Das", "m\u00e4ch\u00b7tigs\u00b7te", ",", "das", "feins\u00b7te", "Klin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "entlauscht dem Erdenrund mein Ohr.", "tokens": ["ent\u00b7lauscht", "dem", "Er\u00b7den\u00b7rund", "mein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es h\u00f6rt die Meere donnernd springen", "tokens": ["Es", "h\u00f6rt", "die", "Mee\u00b7re", "don\u00b7nernd", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den felsgekr\u00e4nzten Strand empor,", "tokens": ["den", "fels\u00b7ge\u00b7kr\u00e4nz\u00b7ten", "Strand", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "es h\u00f6rt der Menschenstimmen Chor", "tokens": ["es", "h\u00f6rt", "der", "Men\u00b7schen\u00b7stim\u00b7men", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und h\u00f6rt der V\u00f6gel helles Singen,", "tokens": ["und", "h\u00f6rt", "der", "V\u00f6\u00b7gel", "hel\u00b7les", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "der Quellen sch\u00fcchternen Tenor,", "tokens": ["der", "Quel\u00b7len", "sch\u00fcch\u00b7ter\u00b7nen", "Te\u00b7nor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der W\u00e4lder Ba\u00df, der Glocken Schwingen.", "tokens": ["der", "W\u00e4l\u00b7der", "Ba\u00df", ",", "der", "Glo\u00b7cken", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das ist das gro\u00dfe Tafellied", "tokens": ["Das", "ist", "das", "gro\u00b7\u00dfe", "Ta\u00b7fel\u00b7lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in Phanta's Schlo\u00df, die Mittagsweise.", "tokens": ["in", "Phan\u00b7ta's", "Schlo\u00df", ",", "die", "Mit\u00b7tags\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Fugenwerk der Sph\u00e4ren-Kreise", "tokens": ["Vom", "Fu\u00b7gen\u00b7werk", "der", "Sph\u00e4\u00b7ren\u00b7Krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zwar freilich nur ein kleinstes Glied.", "tokens": ["zwar", "frei\u00b7lich", "nur", "ein", "kleins\u00b7tes", "Glied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Erst wenn mit breiten Nebelstreifen", "tokens": ["Erst", "wenn", "mit", "brei\u00b7ten", "Ne\u00b7bel\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "des Abends Hand die Welt verh\u00e4ngt", "tokens": ["des", "A\u00b7bends", "Hand", "die", "Welt", "ver\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und meiner Sinne ma\u00dflos Schweifen", "tokens": ["und", "mei\u00b7ner", "Sin\u00b7ne", "ma\u00df\u00b7los", "Schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in engere Bezirke zw\u00e4ngt \u2013", "tokens": ["in", "en\u00b7ge\u00b7re", "Be\u00b7zir\u00b7ke", "zw\u00e4ngt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wenn sich die D\u00e4mmerungen sch\u00fcrzen", "tokens": ["wenn", "sich", "die", "D\u00e4m\u00b7me\u00b7run\u00b7gen", "sch\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "zum wallenden Gewand der Nacht", "tokens": ["zum", "wal\u00b7len\u00b7den", "Ge\u00b7wand", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und aus der Himmel Kraterschacht", "tokens": ["und", "aus", "der", "Him\u00b7mel", "Kra\u00b7ter\u00b7schacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Legionen Strahlenstr\u00f6me st\u00fcrzen \u2013", "tokens": ["Le\u00b7gi\u00b7o\u00b7nen", "Strah\u00b7len\u00b7str\u00f6\u00b7me", "st\u00fcr\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "wenn die Gefilde heilig stumm,", "tokens": ["wenn", "die", "Ge\u00b7fil\u00b7de", "hei\u00b7lig", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "und alles Sein ein tiefer Friede \u2013", "tokens": ["und", "al\u00b7les", "Sein", "ein", "tie\u00b7fer", "Frie\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "dann erst erbebt vom Weltenliede,", "tokens": ["dann", "erst", "er\u00b7bebt", "vom", "Wel\u00b7ten\u00b7lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "vom Sph\u00e4renklang mein Heiligtum.", "tokens": ["vom", "Sph\u00e4\u00b7ren\u00b7klang", "mein", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf Silberwellen kommt gegangen", "tokens": ["Auf", "Sil\u00b7ber\u00b7wel\u00b7len", "kommt", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "unsagbar s\u00fc\u00dfe Harmonie,", "tokens": ["un\u00b7sag\u00b7bar", "s\u00fc\u00b7\u00dfe", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "in eine Weise eingefangen,", "tokens": ["in", "ei\u00b7ne", "Wei\u00b7se", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "unendlichfache Melodie.", "tokens": ["un\u00b7end\u00b7lich\u00b7fa\u00b7che", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "punc