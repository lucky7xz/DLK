{"dta.poem.2108": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der laue Luftkreis, angef\u00fcllt mit einer fruchtbarn", "tokens": ["Der", "lau\u00b7e", "Luft\u00b7kreis", ",", "an\u00b7ge\u00b7f\u00fcllt", "mit", "ei\u00b7ner", "frucht\u00b7barn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebenskraft,", "tokens": ["Le\u00b7bens\u00b7kraft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Senkt sein verliebtes m\u00e4nnlichs Feuer, in eines dichten", "tokens": ["Senkt", "sein", "ver\u00b7lieb\u00b7tes", "m\u00e4nn\u00b7lichs", "Feu\u00b7er", ",", "in", "ei\u00b7nes", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Negens Saft", "tokens": ["Ne\u00b7gens", "Saft"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "In die geliebte Schoo\u00df der Erde, die lange nichts von", "tokens": ["In", "die", "ge\u00b7lieb\u00b7te", "Schoo\u00df", "der", "Er\u00b7de", ",", "die", "lan\u00b7ge", "nichts", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADV", "PIS", "APPR"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "ihm genossen:", "tokens": ["ihm", "ge\u00b7nos\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ob nun dieselbe noch so gro\u00df,", "tokens": ["Ob", "nun", "die\u00b7sel\u00b7be", "noch", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDAT", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird, da ihr Br\u00e4utigam noch gr\u00f6\u00dfer, ihr ganzer K\u00f6rper", "tokens": ["Wird", ",", "da", "ihr", "Br\u00e4u\u00b7ti\u00b7gam", "noch", "gr\u00f6\u00b7\u00dfer", ",", "ihr", "gan\u00b7zer", "K\u00f6r\u00b7per"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "NE", "ADV", "ADJD", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "\u00fcbergossen", "tokens": ["\u00fc\u00b7ber\u00b7gos\u00b7sen"], "token_info": ["word"], "pos": ["VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Mit Anmuth und mit Fruchtbarkeit. Woraus, in unge-", "tokens": ["Mit", "An\u00b7muth", "und", "mit", "Frucht\u00b7bar\u00b7keit", ".", "Wo\u00b7raus", ",", "in", "un\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$.", "PWAV", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "z\u00e4hlter Menge", "tokens": ["z\u00e4hl\u00b7ter", "Men\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Und fast mit sichtbarem Gedrenge", "tokens": ["Und", "fast", "mit", "sicht\u00b7ba\u00b7rem", "Ge\u00b7dren\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die angenehme Fr\u00fchlingskinder, Laub, Aehren, Gras", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Fr\u00fch\u00b7lings\u00b7kin\u00b7der", ",", "Laub", ",", "A\u00b7eh\u00b7ren", ",", "Gras"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "und Blumen sprossen.", "tokens": ["und", "Blu\u00b7men", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Vern\u00fcnft\u2019ge Menschenseel\u2019 erw\u00e4ge, in dieser Handlung,", "tokens": ["Ver\u00b7n\u00fcnft'\u00b7ge", "Men\u00b7schen\u00b7seel'", "er\u00b7w\u00e4\u00b7ge", ",", "in", "die\u00b7ser", "Hand\u00b7lung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "doch die Spur", "tokens": ["doch", "die", "Spur"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Von einer, dir allein zum Nutzen, so \u00e4msig wirkenden", "tokens": ["Von", "ei\u00b7ner", ",", "dir", "al\u00b7lein", "zum", "Nut\u00b7zen", ",", "so", "\u00e4m\u00b7sig", "wir\u00b7ken\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PPER", "ADV", "APPRART", "NN", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Natur,", "tokens": ["Na\u00b7tur", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Und in derselben dessen Weisheit, der aller Himmel Him-", "tokens": ["Und", "in", "der\u00b7sel\u00b7ben", "des\u00b7sen", "Weis\u00b7heit", ",", "der", "al\u00b7ler", "Him\u00b7mel", "Him"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "PRELAT", "NN", "$,", "PRELS", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "mel Kreise,", "tokens": ["mel", "Krei\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Der alle Sonn- und Welten schuf und lenkt, auf solche", "tokens": ["Der", "al\u00b7le", "Sonn", "und", "Wel\u00b7ten", "schuf", "und", "lenkt", ",", "auf", "sol\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "TRUNC", "KON", "NN", "VVFIN", "KON", "VVFIN", "$,", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "weise Weise.", "tokens": ["wei\u00b7se", "Wei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}