{"textgrid.poem.66123": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Hans Sporck", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trabt ein Regiment durch Westfalen,", "tokens": ["Trabt", "ein", "Re\u00b7gi\u00b7ment", "durch", "West\u00b7fa\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Bayrische Dragoner. Die prahlen.", "tokens": ["Bay\u00b7ri\u00b7sche", "Dra\u00b7go\u00b7ner", ".", "Die", "prah\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wie Hans Sporck der Glanz in die Augen sticht.", "tokens": ["Wie", "Hans", "Sporck", "der", "Glanz", "in", "die", "Au\u00b7gen", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hans Sporck war Kuhjung. Das pa\u00dfte ihm nicht.", "tokens": ["Hans", "Sporck", "war", "Kuh\u00b7jung", ".", "Das", "pa\u00df\u00b7te", "ihm", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "NN", "$.", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Handschlag, und Hans Sporck vertauscht", "tokens": ["Hand\u00b7schlag", ",", "und", "Hans", "Sporck", "ver\u00b7tauscht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "NE", "NE", "VVFIN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Die Kuh mit dem Pferd.", "tokens": ["Die", "Kuh", "mit", "dem", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "\u2013 Die Fahne rauscht.", "tokens": ["\u2013", "Die", "Fah\u00b7ne", "rauscht", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hin und her wogt die Kriegsflut. Hans Sporck", "tokens": ["Hin", "und", "her", "wogt", "die", "Kriegs\u00b7flut", ".", "Hans", "Sporck"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "ADV", "VVFIN", "ART", "NN", "$.", "NE", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Schwimmt lustig oben wie ein Kork.", "tokens": ["Schwimmt", "lus\u00b7tig", "o\u00b7ben", "wie", "ein", "Kork", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Tilly siegt, ist er auch dabei", "tokens": ["Wo", "Til\u00b7ly", "siegt", ",", "ist", "er", "auch", "da\u00b7bei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "PAV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und avanciert in der Reiterei.", "tokens": ["Und", "a\u00b7van\u00b7ciert", "in", "der", "Rei\u00b7te\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Zehn Jahre ziehn durch Bellonas Tor,", "tokens": ["Zehn", "Jah\u00b7re", "ziehn", "durch", "Bel\u00b7lo\u00b7nas", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Hans Sporck ist Generalmajor.", "tokens": ["Hans", "Sporck", "ist", "Ge\u00b7ne\u00b7ral\u00b7ma\u00b7jor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kaiser Leopoldus hat", "tokens": ["Kai\u00b7ser", "Leo\u00b7pol\u00b7dus", "hat"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "VAFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die T\u00fcrken vorm Tor. Wackelt der Staat?", "tokens": ["Die", "T\u00fcr\u00b7ken", "vorm", "Tor", ".", "Wa\u00b7ckelt", "der", "Staat", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "An dreimalhunderttausend Heiden", "tokens": ["An", "drei\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "Hei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gedenken bis Wien ihre Rosse zu weiden.", "tokens": ["Ge\u00b7den\u00b7ken", "bis", "Wi\u00b7en", "ih\u00b7re", "Ros\u00b7se", "zu", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Montecuccoli l\u00e4\u00dft die Fahnen wehn,", "tokens": ["Mon\u00b7te\u00b7cuc\u00b7co\u00b7li", "l\u00e4\u00dft", "die", "Fah\u00b7nen", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und Hans Sporck l\u00e4\u00dft Becher und W\u00fcrfel stehn.", "tokens": ["Und", "Hans", "Sporck", "l\u00e4\u00dft", "Be\u00b7cher", "und", "W\u00fcr\u00b7fel", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Seit Ulm, wo Waffenruh dekretiert,", "tokens": ["Seit", "Ulm", ",", "wo", "Waf\u00b7fen\u00b7ruh", "de\u00b7kre\u00b7tiert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wurd nicht geritten und scharm\u00fctziert.", "tokens": ["Wurd", "nicht", "ge\u00b7rit\u00b7ten", "und", "schar\u00b7m\u00fct\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "KON", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Da freut sich ein ehrlicher Reitersmann,", "tokens": ["Da", "freut", "sich", "ein", "ehr\u00b7li\u00b7cher", "Rei\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Wenn er wieder mal in den Sattel kann.", "tokens": ["Wenn", "er", "wie\u00b7der", "mal", "in", "den", "Sat\u00b7tel", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VMFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Heidenhund lauf,", "tokens": ["Hei\u00b7den\u00b7hund", "lauf", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Hans Sporck sitzt auf!", "tokens": ["Hans", "Sporck", "sitzt", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Bei Sankt Gotthardt an der Raab", "tokens": ["Bei", "Sankt", "Got\u00b7thardt", "an", "der", "Ra\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "NE", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Setzt Hans Sporck sich in Trab.", "tokens": ["Setzt", "Hans", "Sporck", "sich", "in", "Trab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "PRF", "APPR", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Alle seine Regimenter", "tokens": ["Al\u00b7le", "sei\u00b7ne", "Re\u00b7gi\u00b7men\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirft er auf die t\u00fcrkischen Sakramenter.", "tokens": ["Wirft", "er", "auf", "die", "t\u00fcr\u00b7ki\u00b7schen", "Sak\u00b7ra\u00b7men\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "Wie ein Besen aus blanken Blitzen", "tokens": ["Wie", "ein", "Be\u00b7sen", "aus", "blan\u00b7ken", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wei\u00df er dem Feind im Nacken zu sitzen.", "tokens": ["Wei\u00df", "er", "dem", "Feind", "im", "Na\u00b7cken", "zu", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Montecuccoli reibt sich die H\u00e4nde:", "tokens": ["Mon\u00b7te\u00b7cuc\u00b7co\u00b7li", "reibt", "sich", "die", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Der Sporck bringts zu Ende!", "tokens": ["Der", "Sporck", "bringts", "zu", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Hans Sporck reitet vor des Kaisers Haus", "tokens": ["Hans", "Sporck", "rei\u00b7tet", "vor", "des", "Kai\u00b7sers", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und sch\u00fcttet seine Lorbeern aus:", "tokens": ["Und", "sch\u00fct\u00b7tet", "sei\u00b7ne", "Lor\u00b7beern", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zweimalhunderttausend und mehr", "tokens": ["Zwei\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "und", "mehr"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Stoben wie Kehricht vor uns her,", "tokens": ["Sto\u00b7ben", "wie", "Keh\u00b7richt", "vor", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und diesen schmierigen Turban hier", "tokens": ["Und", "die\u00b7sen", "schmie\u00b7ri\u00b7gen", "Tur\u00b7ban", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Verlor auf der Flucht der Gro\u00dfvezier.", "tokens": ["Ver\u00b7lor", "auf", "der", "Flucht", "der", "Gro\u00df\u00b7ve\u00b7zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Leopoldus mit frommem Bedacht", "tokens": ["Leo\u00b7pol\u00b7dus", "mit", "from\u00b7mem", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Dankt Gott: \u00bbJa, Sporck, h\u00e4tte ders nicht gemacht!\u00ab", "tokens": ["Dankt", "Gott", ":", "\u00bb", "Ja", ",", "Sporck", ",", "h\u00e4t\u00b7te", "ders", "nicht", "ge\u00b7macht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "$.", "$(", "PTKANT", "$,", "NN", "$,", "VAFIN", "ADV", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Blitz,", "tokens": ["Blitz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Springt Hans Sporck da vom Sitz", "tokens": ["Springt", "Hans", "Sporck", "da", "vom", "Sitz"], "token_info": ["word", "word", "word", "word", "word", "word"],