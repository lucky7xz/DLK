{"dta.poem.10672": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet au\u00df dem Italienischen der Veronica  \n Gambara/ warumb sie nicht mehr von Bule-  \n rey schreib.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "In vppiger Begier/ in vnbedachtem Sinn/", "tokens": ["In", "vp\u00b7pi\u00b7ger", "Be\u00b7gier", "/", "in", "vn\u00b7be\u00b7dach\u00b7tem", "Sinn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd zwischen Forcht vnd Trost hab ich bi\u00dfher gestrebet/", "tokens": ["Vnd", "zwi\u00b7schen", "Forcht", "vnd", "Trost", "hab", "ich", "bi\u00df\u00b7her", "ge\u00b7stre\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Jetzt trawrig/ jetzt in Lust vnd Fr\u00f6ligkeit gelebet/", "tokens": ["Jetzt", "traw\u00b7rig", "/", "jetzt", "in", "Lust", "vnd", "Fr\u00f6\u00b7lig\u00b7keit", "ge\u00b7le\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil in vnst\u00e4tem Gl\u00fcck ich gantz gewesen bin/", "tokens": ["Weil", "in", "vns\u00b7t\u00e4\u00b7tem", "Gl\u00fcck", "ich", "gantz", "ge\u00b7we\u00b7sen", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPER", "ADV", "VAPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bald hab ich nur in Leyd gesucht Frewd vnd Gewinn/", "tokens": ["Bald", "hab", "ich", "nur", "in", "Leyd", "ge\u00b7sucht", "Frewd", "vnd", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "NN", "KON", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vnd in der Thr\u00e4nen Bach ohn vnderla\u00df geschwebet/", "tokens": ["Vnd", "in", "der", "Thr\u00e4\u00b7nen", "Bach", "ohn", "vn\u00b7der\u00b7la\u00df", "ge\u00b7schwe\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald bin ich widerumb an vppigkeit geklebet/", "tokens": ["Bald", "bin", "ich", "wi\u00b7de\u00b7rumb", "an", "vp\u00b7pig\u00b7keit", "ge\u00b7kle\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ist die junge Zeit von mir geflossen hin.", "tokens": ["So", "ist", "die", "jun\u00b7ge", "Zeit", "von", "mir", "ge\u00b7flos\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun aber ich jetzundt auff anders bin bedacht/", "tokens": ["Nun", "a\u00b7ber", "ich", "je\u00b7tzundt", "auff", "an\u00b7ders", "bin", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sag\u2019 ich: Ihr liebe Ver\u00df/ ich geb\u2019 euch gute Nacht/", "tokens": ["Sag'", "ich", ":", "Ihr", "lie\u00b7be", "Ver\u00df", "/", "ich", "geb'", "euch", "gu\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich will mich k\u00fcnfftig gantz zuschweigen vnderfangen.", "tokens": ["Ich", "will", "mich", "k\u00fcnff\u00b7tig", "gantz", "zu\u00b7schwei\u00b7gen", "vn\u00b7der\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch wann die alte Lust zuschreiben wieder k\u00fcmpt/", "tokens": ["Doch", "wann", "die", "al\u00b7te", "Lust", "zu\u00b7schrei\u00b7ben", "wie\u00b7der", "k\u00fcmpt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Alsbald mein eygne schuld mir diesen Sinn benimpt/", "tokens": ["Als\u00b7bald", "mein", "eyg\u00b7ne", "schuld", "mir", "die\u00b7sen", "Sinn", "be\u00b7nimpt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PPER", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weil ich die That bedenck/ die ich zuvor begangen.", "tokens": ["Weil", "ich", "die", "That", "be\u00b7denck", "/", "die", "ich", "zu\u00b7vor", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$(", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}