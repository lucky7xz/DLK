{"dta.poem.21272": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Triumph.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Sch&#228;me dich, Kamill,", "tokens": ["Sch", "&#228;", "me", "dich", ",", "Ka\u00b7mill", ","], "token_info": ["word", "XML_entity", "word", "word", "punct", "word", "punct"], "pos": ["XY", "$(", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Dass du mit vier Sonnenpferden", "tokens": ["Dass", "du", "mit", "vier", "Son\u00b7nen\u00b7pfer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dein errettetes Rom zogst!", "tokens": ["In", "dein", "er\u00b7ret\u00b7te\u00b7tes", "Rom", "zogst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und du, Romulischer Feinde", "tokens": ["Und", "du", ",", "Ro\u00b7mu\u00b7li\u00b7scher", "Fein\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gl\u00fccklicher Sieger, o Julius,", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "Sie\u00b7ger", ",", "o", "Ju\u00b7lius", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Dass dich, mit goldenen St\u00e4dten und", "tokens": ["Dass", "dich", ",", "mit", "gol\u00b7de\u00b7nen", "St\u00e4d\u00b7ten", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "KON"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Und mit Adlern und Spolien", "tokens": ["Und", "mit", "Ad\u00b7lern", "und", "Spo\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Deiner Br\u00fcder umgeben,", "tokens": ["Dei\u00b7ner", "Br\u00fc\u00b7der", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Zum hohen Kapitol dein stolzer Wagen", "tokens": ["Zum", "ho\u00b7hen", "Ka\u00b7pi\u00b7tol", "dein", "stol\u00b7zer", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Friederich, ein Prinz der Brennen,", "tokens": ["Frie\u00b7de\u00b7rich", ",", "ein", "Prinz", "der", "Bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ward angefallen von V\u00f6lkern Hungariens,", "tokens": ["Ward", "an\u00b7ge\u00b7fal\u00b7len", "von", "V\u00f6l\u00b7kern", "Hun\u00b7ga\u00b7ri\u00b7ens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+--+-++--", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Von Illyriens Reitern und Daciens:", "tokens": ["Von", "Il\u00b7ly\u00b7ri\u00b7ens", "Rei\u00b7tern", "und", "Da\u00b7ci\u00b7ens", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Alle dem Zepter der K\u00f6niginn zinsbar,", "tokens": ["Al\u00b7le", "dem", "Zep\u00b7ter", "der", "K\u00f6\u00b7ni\u00b7ginn", "zins\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.14": {"text": "Die Vindobonens saatenreiche Fluren,", "tokens": ["Die", "Vin\u00b7do\u00b7bo\u00b7nens", "saa\u00b7ten\u00b7rei\u00b7che", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und Austrasiens Auen beherrscht,", "tokens": ["Und", "Aus\u00b7tra\u00b7si\u00b7ens", "Au\u00b7en", "be\u00b7herrscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Und der Bajonen Gebirge,", "tokens": ["Und", "der", "Ba\u00b7jo\u00b7nen", "Ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Und Hesperiens goldene G\u00e4rten;", "tokens": ["Und", "Hes\u00b7pe\u00b7ri\u00b7ens", "gol\u00b7de\u00b7ne", "G\u00e4r\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Dieser erhabenen F\u00fcrstinn,", "tokens": ["Die\u00b7ser", "er\u00b7ha\u00b7be\u00b7nen", "F\u00fcrs\u00b7tinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.19": {"text": "Deren Wohlfahrt vom Himmel in", "tokens": ["De\u00b7ren", "Wohl\u00b7fahrt", "vom", "Him\u00b7mel", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "APPR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Sieben Sprachen erflehet wird;", "tokens": ["Sie\u00b7ben", "Spra\u00b7chen", "er\u00b7fle\u00b7het", "wird", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Deren Heere, gef\u00fchrt vom Stab\u2019 Eugens,", "tokens": ["De\u00b7ren", "Hee\u00b7re", ",", "ge\u00b7f\u00fchrt", "vom", "Stab'", "Eu\u00b7gens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "VVPP", "APPRART", "NN", "NE", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Ehmals unbezwinglich, \u2014 und itzt", "tokens": ["Eh\u00b7mals", "un\u00b7be\u00b7zwing\u00b7lich", ",", "und", "itzt"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "$(", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Verbunden waren mit allen, die", "tokens": ["Ver\u00b7bun\u00b7den", "wa\u00b7ren", "mit", "al\u00b7len", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PIAT", "$,", "PRELS"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Am M\u00e4otischen, Kaspischen, Finnischen", "tokens": ["Am", "M\u00e4o\u00b7ti\u00b7schen", ",", "Kas\u00b7pi\u00b7schen", ",", "Fin\u00b7ni\u00b7schen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Sunde wohnen, den rauhen", "tokens": ["Sun\u00b7de", "woh\u00b7nen", ",", "den", "rau\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.26": {"text": "Samojeden, den Ostiaken,", "tokens": ["Sa\u00b7mo\u00b7je\u00b7den", ",", "den", "Os\u00b7ti\u00b7a\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "Und dem Tartar am Sangarfluss:", "tokens": ["Und", "dem", "Tar\u00b7tar", "am", "San\u00b7gar\u00b7fluss", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Einer Monarchinn dienstbar, einer,", "tokens": ["Ei\u00b7ner", "Mon\u00b7ar\u00b7chinn", "dienst\u00b7bar", ",", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PIS", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.29": {"text": "Die den weiten Umkreis", "tokens": ["Die", "den", "wei\u00b7ten", "Um\u00b7kreis"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "Ihrer Welten nicht kennt.", "tokens": ["Ih\u00b7rer", "Wel\u00b7ten", "nicht", "kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Auch trat zu ihnen der S\u00f6hne Sarmatiens", "tokens": ["Auch", "trat", "zu", "ih\u00b7nen", "der", "S\u00f6h\u00b7ne", "Sar\u00b7ma\u00b7ti\u00b7ens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "NE"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Selbsterw\u00e4hleter K\u00f6nig,", "tokens": ["Selbs\u00b7ter\u00b7w\u00e4h\u00b7le\u00b7ter", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.33": {"text": "Und stellte seine Sachsen, ein treues Volk,", "tokens": ["Und", "stell\u00b7te", "sei\u00b7ne", "Sach\u00b7sen", ",", "ein", "treu\u00b7es", "Volk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Mitten auf den Pfad des Siegers,", "tokens": ["Mit\u00b7ten", "auf", "den", "Pfad", "des", "Sie\u00b7gers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Unter eine Felsenburg.", "tokens": ["Un\u00b7ter", "ei\u00b7ne", "Fel\u00b7sen\u00b7burg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Und die hohen Satrapen Germaniens", "tokens": ["Und", "die", "ho\u00b7hen", "Sa\u00b7tra\u00b7pen", "Ger\u00b7ma\u00b7ni\u00b7ens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.37": {"text": "Fielen zahlreich dem Bunde bey.", "tokens": ["Fie\u00b7len", "zahl\u00b7reich", "dem", "Bun\u00b7de", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.38": {"text": "Und die theur erkauften Suenonen", "tokens": ["Und", "die", "theur", "er\u00b7kauf\u00b7ten", "Sue\u00b7no\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Drangen aus dem beeisten Norden hervor:", "tokens": ["Dran\u00b7gen", "aus", "dem", "be\u00b7eis\u00b7ten", "Nor\u00b7den", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.40": {"text": "Enkel der Helden, mit denen ein J\u00fcngling", "tokens": ["En\u00b7kel", "der", "Hel\u00b7den", ",", "mit", "de\u00b7nen", "ein", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Europen und Asien schreckte.", "tokens": ["Eu\u00b7ro\u00b7pen", "und", "A\u00b7sien", "schreck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.42": {"text": "Und Gallien, das an zwey Meeren thront,", "tokens": ["Und", "Gal\u00b7li\u00b7en", ",", "das", "an", "zwey", "Mee\u00b7ren", "thront", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Dessen Fahnen und Wimpel", "tokens": ["Des\u00b7sen", "Fah\u00b7nen", "und", "Wim\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.44": {"text": "Unter allen Himmeln wehn,", "tokens": ["Un\u00b7ter", "al\u00b7len", "Him\u00b7meln", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Liess seinen Schwarm aus,", "tokens": ["Liess", "sei\u00b7nen", "Schwarm", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "Gleich dem Heere schwirrender Grillen,", "tokens": ["Gleich", "dem", "Hee\u00b7re", "schwir\u00b7ren\u00b7der", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.47": {"text": "Die vor sich her ein bl\u00fchend Land,", "tokens": ["Die", "vor", "sich", "her", "ein", "bl\u00fc\u00b7hend", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "APZR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und hinter sich W\u00fcsten sehn. \u2014", "tokens": ["Und", "hin\u00b7ter", "sich", "W\u00fcs\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PRF", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.49": {"text": "Aber, Thalia, lass ab", "tokens": ["A\u00b7ber", ",", "Tha\u00b7lia", ",", "lass", "ab"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NE", "$,", "VVFIN", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.50": {"text": "Die Flotten und Fussknecht\u2019 und Reiter", "tokens": ["Die", "Flot\u00b7ten", "und", "Fuss\u00b7knecht'", "und", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.51": {"text": "Friederich, so sage, bekriegt", "tokens": ["Frie\u00b7de\u00b7rich", ",", "so", "sa\u00b7ge", ",", "be\u00b7kriegt"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.52": {"text": "Von scheels\u00fcchtigen, oder get\u00e4uschten,", "tokens": ["Von", "scheel\u00b7s\u00fcch\u00b7ti\u00b7gen", ",", "o\u00b7der", "ge\u00b7t\u00e4uschten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$,", "KON", "VVPP", "$,"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.53": {"text": "Oder gezwungenen F\u00fcrsten,", "tokens": ["O\u00b7der", "ge\u00b7zwun\u00b7ge\u00b7nen", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.54": {"text": "Kehrte, nach sieben blutigen Jahren,", "tokens": ["Kehr\u00b7te", ",", "nach", "sie\u00b7ben", "blu\u00b7ti\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.55": {"text": "So m\u00e4chtig zur\u00fcck, als er auszog,", "tokens": ["So", "m\u00e4ch\u00b7tig", "zu\u00b7r\u00fcck", ",", "als", "er", "aus\u00b7zog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.56": {"text": "Nur an Ehre gr\u00f6sser,", "tokens": ["Nur", "an", "Eh\u00b7re", "gr\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.57": {"text": "Und triumphirte nicht. \u2014", "tokens": ["Und", "tri\u00b7um\u00b7phir\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.58": {"text": "Siehe! er lenkt unsern Ehrenbogen aus,", "tokens": ["Sie\u00b7he", "!", "er", "lenkt", "un\u00b7sern", "Eh\u00b7ren\u00b7bo\u00b7gen", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.59": {"text": "Und unsern goldbeh\u00e4ngten Rossen,", "tokens": ["Und", "un\u00b7sern", "gold\u00b7be\u00b7h\u00e4ng\u00b7ten", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Und besteigt den pralenden Wagen nicht!", "tokens": ["Und", "be\u00b7steigt", "den", "pra\u00b7len\u00b7den", "Wa\u00b7gen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.61": {"text": "Denn sich selbst mit eines Gottes Zufriedenheit", "tokens": ["Denn", "sich", "selbst", "mit", "ei\u00b7nes", "Got\u00b7tes", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.62": {"text": "Ansehn, ist der Triumphe", "tokens": ["An\u00b7sehn", ",", "ist", "der", "Tri\u00b7um\u00b7phe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVINF", "$,", "VAFIN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.63": {"text": "Allerh\u00f6chster. \u2014 Und des Dichters", "tokens": ["Al\u00b7ler\u00b7h\u00f6chs\u00b7ter", ".", "Und", "des", "Dich\u00b7ters"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$.", "$(", "KON", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.64": {"text": "Allerh\u00f6chster Triumph ist,", "tokens": ["Al\u00b7ler\u00b7h\u00f6chs\u00b7ter", "Tri\u00b7umph", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Diesen K\u00f6nig besingen.", "tokens": ["Die\u00b7sen", "K\u00f6\u00b7nig", "be\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.66": {"text": "Drum schweige du nie von ihm, mein Lied,", "tokens": ["Drum", "schwei\u00b7ge", "du", "nie", "von", "ihm", ",", "mein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.67": {"text": "Stolzer, als der Ge\u00efsche", "tokens": ["Stol\u00b7zer", ",", "als", "der", "Ge\u00ef\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.68": {"text": "Und der Thebanische P\u00e4an,", "tokens": ["Und", "der", "The\u00b7ba\u00b7ni\u00b7sche", "P\u00e4\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.69": {"text": "Keinem Golde feil,", "tokens": ["Kei\u00b7nem", "Gol\u00b7de", "feil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.70": {"text": "Auch selbst dem seinigen nicht.", "tokens": ["Auch", "selbst", "dem", "sei\u00b7ni\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PPOSS", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.71": {"text": "Und ob er auch diesen Triumph verlenkt,", "tokens": ["Und", "ob", "er", "auch", "die\u00b7sen", "Tri\u00b7umph", "ver\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.72": {"text": "Und, deiner T\u00f6ne nicht gewohnt,", "tokens": ["Und", ",", "dei\u00b7ner", "T\u00f6\u00b7ne", "nicht", "ge\u00b7wohnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.73": {"text": "Sein Ohr zu Galliens Schw\u00e4nen neigt:", "tokens": ["Sein", "Ohr", "zu", "Gal\u00b7li\u00b7ens", "Schw\u00e4\u00b7nen", "neigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.74": {"text": "So singe du doch den Brennuss\u00f6hnen", "tokens": ["So", "sin\u00b7ge", "du", "doch", "den", "Bren\u00b7nus\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.75": {"text": "Ihren Erretter unnachgesungen.", "tokens": ["Ih\u00b7ren", "Er\u00b7ret\u00b7ter", "un\u00b7nach\u00b7ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}