{"textgrid.poem.57731": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Longobarde", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es flogen drei Schw\u00e4ne \u00fcber die Heide,", "tokens": ["Es", "flo\u00b7gen", "drei", "Schw\u00e4\u00b7ne", "\u00fc\u00b7ber", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drei silberne Schw\u00e4ne und keiner mehr;", "tokens": ["Drei", "sil\u00b7ber\u00b7ne", "Schw\u00e4\u00b7ne", "und", "kei\u00b7ner", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "KON", "PIS", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie kamen von Morgen und Abend und Mitternacht,", "tokens": ["Sie", "ka\u00b7men", "von", "Mor\u00b7gen", "und", "A\u00b7bend", "und", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Aber von Mittag kam keiner her.", "tokens": ["A\u00b7ber", "von", "Mit\u00b7tag", "kam", "kei\u00b7ner", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ihr Silbergefieder trug rote Rosen", "tokens": ["Ihr", "Sil\u00b7ber\u00b7ge\u00b7fie\u00b7der", "trug", "ro\u00b7te", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vom Abendrot in die Nacht hinein;", "tokens": ["Vom", "A\u00b7ben\u00b7drot", "in", "die", "Nacht", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Sterne schimmerten ihre Schn\u00e4bel,", "tokens": ["Wie", "Ster\u00b7ne", "schim\u00b7mer\u00b7ten", "ih\u00b7re", "Schn\u00e4\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie goldene Sterne mit hellem Schein.", "tokens": ["Wie", "gol\u00b7de\u00b7ne", "Ster\u00b7ne", "mit", "hel\u00b7lem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Sie sangen drei Lieder \u00fcber die Heide,", "tokens": ["Sie", "san\u00b7gen", "drei", "Lie\u00b7der", "\u00fc\u00b7ber", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drei alte Lieder, s\u00fc\u00df und schwer;", "tokens": ["Drei", "al\u00b7te", "Lie\u00b7der", ",", "s\u00fc\u00df", "und", "schwer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drei Lieder von Liebe und M\u00fche und Frieden,", "tokens": ["Drei", "Lie\u00b7der", "von", "Lie\u00b7be", "und", "M\u00fc\u00b7he", "und", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Drei liebe Lieder und keines mehr.", "tokens": ["Drei", "lie\u00b7be", "Lie\u00b7der", "und", "kei\u00b7nes", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "KON", "PIS", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Er stand am Tore und sah in die Heide", "tokens": ["Er", "stand", "am", "To\u00b7re", "und", "sah", "in", "die", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und hatte wieder das fremde Gesicht;", "tokens": ["Und", "hat\u00b7te", "wie\u00b7der", "das", "frem\u00b7de", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Seine Augen flogen hinunter zum S\u00fcden,", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "flo\u00b7gen", "hin\u00b7un\u00b7ter", "zum", "S\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und was sie sahen, er kannte es nicht.", "tokens": ["Und", "was", "sie", "sa\u00b7hen", ",", "er", "kann\u00b7te", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Er sah das Meer und die wei\u00dfen St\u00e4dte", "tokens": ["Er", "sah", "das", "Meer", "und", "die", "wei\u00b7\u00dfen", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schwarzes Volk, gering und gemein;", "tokens": ["Und", "schwar\u00b7zes", "Volk", ",", "ge\u00b7ring", "und", "ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er sah es unter dem Schwerte sich ducken,", "tokens": ["Er", "sah", "es", "un\u00b7ter", "dem", "Schwer\u00b7te", "sich", "du\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und das Schwert war rot und das Schwert war sein.", "tokens": ["Und", "das", "Schwert", "war", "rot", "und", "das", "Schwert", "war", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VAFIN", "VAINF", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Er sah seine Faust das Szepter halten,", "tokens": ["Er", "sah", "sei\u00b7ne", "Faust", "das", "Szep\u00b7ter", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Faust, gewohnt des Pfluges Sterz;", "tokens": ["Die", "Faust", ",", "ge\u00b7wohnt", "des", "Pflu\u00b7ges", "Sterz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hei\u00df quoll es ihm in die blauen Augen,", "tokens": ["Hei\u00df", "quoll", "es", "ihm", "in", "die", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unruhig schlug sein junges Herz.", "tokens": ["Un\u00b7ru\u00b7hig", "schlug", "sein", "jun\u00b7ges", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Es kam ein Adler von Mittag geflogen,", "tokens": ["Es", "kam", "ein", "Ad\u00b7ler", "von", "Mit\u00b7tag", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein goldener Adler und keiner mehr;", "tokens": ["Ein", "gol\u00b7de\u00b7ner", "Ad\u00b7ler", "und", "kei\u00b7ner", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PIS", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Seine Schwingen zerschnitten die Abendwolken,", "tokens": ["Sei\u00b7ne", "Schwin\u00b7gen", "zer\u00b7schnit\u00b7ten", "die", "Ab\u00b7end\u00b7wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sein Schlachtgesang fuhr vor ihm her.", "tokens": ["Sein", "Schlacht\u00b7ge\u00b7sang", "fuhr", "vor", "ihm", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es schlugen Flammen aus seinen Augen,", "tokens": ["Es", "schlu\u00b7gen", "Flam\u00b7men", "aus", "sei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Flammen, wie Rubine so rot;", "tokens": ["Flam\u00b7men", ",", "wie", "Ru\u00b7bi\u00b7ne", "so", "rot", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "ADV", "ADJD", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Seine Krallen schleuderten helle Blitze,", "tokens": ["Sei\u00b7ne", "Kral\u00b7len", "schleu\u00b7der\u00b7ten", "hel\u00b7le", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Jeder von ihnen war der Tod.", "tokens": ["Je\u00b7der", "von", "ih\u00b7nen", "war", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Es klang sein Mordgeschrei \u00fcber die Heide,", "tokens": ["Es", "klang", "sein", "Mord\u00b7ge\u00b7schrei", "\u00fc\u00b7ber", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der hei\u00dfe Schrei, der Schrei voll Mut;", "tokens": ["Der", "hei\u00b7\u00dfe", "Schrei", ",", "der", "Schrei", "voll", "Mut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drei s\u00fc\u00dfe Schwanenlieder verstummten,", "tokens": ["Drei", "s\u00fc\u00b7\u00dfe", "Schwa\u00b7nen\u00b7lie\u00b7der", "ver\u00b7stumm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum gelben Sand flo\u00df rotes Blut.", "tokens": ["Zum", "gel\u00b7ben", "Sand", "flo\u00df", "ro\u00b7tes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er lag ohne Schlaf auf seinem Lager,", "tokens": ["Er", "lag", "oh\u00b7ne", "Schlaf", "auf", "sei\u00b7nem", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Herz war matt, seine Seele krank;", "tokens": ["Sein", "Herz", "war", "matt", ",", "sei\u00b7ne", "See\u00b7le", "krank", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-