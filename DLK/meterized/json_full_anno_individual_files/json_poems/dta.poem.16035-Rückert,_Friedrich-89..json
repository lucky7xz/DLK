{"dta.poem.16035": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich der Unmuth plagt in deiner Einsamkeit,", "tokens": ["Wenn", "dich", "der", "Un\u00b7muth", "plagt", "in", "dei\u00b7ner", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trag unter Menschen ihn, und sei davon befreit.", "tokens": ["Trag", "un\u00b7ter", "Men\u00b7schen", "ihn", ",", "und", "sei", "da\u00b7von", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "$,", "KON", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du siehst, sie sind vergn\u00fcgt; warum willst du dich gr\u00e4men?", "tokens": ["Du", "siehst", ",", "sie", "sind", "ver\u00b7gn\u00fcgt", ";", "wa\u00b7rum", "willst", "du", "dich", "gr\u00e4\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$.", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Schande, wenn sie dich an Lebensmuth besch\u00e4men.", "tokens": ["O", "Schan\u00b7de", ",", "wenn", "sie", "dich", "an", "Le\u00b7bens\u00b7muth", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie leiden und sind still, la\u00df dirs zur Lehre dienen;", "tokens": ["Sie", "lei\u00b7den", "und", "sind", "still", ",", "la\u00df", "dirs", "zur", "Leh\u00b7re", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VAFIN", "ADJD", "$,", "VVFIN", "PIS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und klagen sie wie du, so tr\u00f6ste dich mit ihnen.", "tokens": ["Und", "kla\u00b7gen", "sie", "wie", "du", ",", "so", "tr\u00f6s\u00b7te", "dich", "mit", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "PPER", "$,", "ADV", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht nur von Starken f\u00fchlt der Schwache sich gest\u00e4rkt,", "tokens": ["Nicht", "nur", "von", "Star\u00b7ken", "f\u00fchlt", "der", "Schwa\u00b7che", "sich", "ge\u00b7st\u00e4rkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVFIN", "ART", "ADJA", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er selber f\u00fchlt sich stark, wo er noch schw\u00e4chre merkt.", "tokens": ["Er", "sel\u00b7ber", "f\u00fchlt", "sich", "stark", ",", "wo", "er", "noch", "schw\u00e4ch\u00b7re", "merkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "ADJD", "$,", "PWAV", "PPER", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}