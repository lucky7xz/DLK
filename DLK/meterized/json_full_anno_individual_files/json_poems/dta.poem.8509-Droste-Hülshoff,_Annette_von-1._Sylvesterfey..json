{"dta.poem.8509": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "1.  \n Sylvesterfey.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der morsche Tag ist eingesunken,", "tokens": ["Der", "mor\u00b7sche", "Tag", "ist", "ein\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Auge gl\u00e4sern, kalt und leer,", "tokens": ["Sein", "Au\u00b7ge", "gl\u00e4\u00b7sern", ",", "kalt", "und", "leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Barg keines Thaues linden Funken", "tokens": ["Barg", "kei\u00b7nes", "Thau\u00b7es", "lin\u00b7den", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr den gebr\u00e4unten Eppich mehr.", "tokens": ["F\u00fcr", "den", "ge\u00b7br\u00e4un\u00b7ten", "Ep\u00b7pich", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie\u2019s drau\u00dfen schauert! \u2014 l\u00e4ngs der Wand", "tokens": ["Wie's", "drau\u00b7\u00dfen", "schau\u00b7ert", "!", "l\u00e4ngs", "der", "Wand"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "$.", "$(", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Ruschelt das M\u00e4uslein unter\u2019m Halme", "tokens": ["Ru\u00b7schelt", "das", "M\u00e4us\u00b7lein", "un\u00b7ter'm", "Hal\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und langsam sprie\u00dft des Eises Palme", "tokens": ["Und", "lang\u00b7sam", "sprie\u00dft", "des", "Ei\u00b7ses", "Pal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am Scheibenrand.", "tokens": ["Am", "Schei\u00b7ben\u00b7rand", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "In tiefer Nacht wem soll noch frommen", "tokens": ["In", "tie\u00b7fer", "Nacht", "wem", "soll", "noch", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PWS", "VMFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Simse dort der Lampe Strahl?", "tokens": ["Am", "Sim\u00b7se", "dort", "der", "Lam\u00b7pe", "Strahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schon des Heerdes Scheit verglommen,", "tokens": ["Da", "schon", "des", "Heer\u00b7des", "Scheit", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welch sp\u00e4ten Gastes harrt das Mahl?", "tokens": ["Welch", "sp\u00e4\u00b7ten", "Gas\u00b7tes", "harrt", "das", "Mahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4ngst hat im Thurme zu Escout", "tokens": ["L\u00e4ngst", "hat", "im", "Thur\u00b7me", "zu", "Es\u00b7cout"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Glocke zw\u00f6lfmal angeschlagen", "tokens": ["Die", "Glo\u00b7cke", "zw\u00f6lf\u00b7mal", "an\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und glitzernd sinkt der Himmelswagen", "tokens": ["Und", "glit\u00b7zernd", "sinkt", "der", "Him\u00b7mels\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Pole zu.", "tokens": ["Dem", "Po\u00b7le", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Durch jener Kammer d\u00fcrre Barren", "tokens": ["Durch", "je\u00b7ner", "Kam\u00b7mer", "d\u00fcr\u00b7re", "Bar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zieh\u2019n Odemz\u00fcge, traumbeschwert,", "tokens": ["Zieh'n", "O\u00b7dem\u00b7z\u00fc\u00b7ge", ",", "traum\u00b7be\u00b7schwert", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Ruck mitunter, auch ein Knarren,", "tokens": ["Ein", "Ruck", "mi\u00b7tun\u00b7ter", ",", "auch", "ein", "Knar\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sich im Bett der Schl\u00e4fer kehrt;", "tokens": ["Wenn", "sich", "im", "Bett", "der", "Schl\u00e4\u00b7fer", "kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur ein leiser Husten wacht,", "tokens": ["Und", "nur", "ein", "lei\u00b7ser", "Hus\u00b7ten", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Traum die Mutter h\u00e4lt befangen,", "tokens": ["Kein", "Traum", "die", "Mut\u00b7ter", "h\u00e4lt", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie kann nicht schlafen in der langen", "tokens": ["Sie", "kann", "nicht", "schla\u00b7fen", "in", "der", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sylvesternacht.", "tokens": ["Syl\u00b7ves\u00b7ter\u00b7nacht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Jetzt ist die Zeit, wo los\u2019 und schleichend", "tokens": ["Jetzt", "ist", "die", "Zeit", ",", "wo", "los'", "und", "schlei\u00b7chend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "NE", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fey sich durch die Ritze schlingt,", "tokens": ["Die", "Fey", "sich", "durch", "die", "Rit\u00b7ze", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit langer Schlepp\u2019 den Estrich streichend,", "tokens": ["Mit", "lan\u00b7ger", "Schlepp'", "den", "Est\u00b7rich", "strei\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schicksal in die H\u00e4user bringt,", "tokens": ["Das", "Schick\u00b7sal", "in", "die", "H\u00e4u\u00b7ser", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An ihrer Hand das Gl\u00fcck, Gewind\u2019", "tokens": ["An", "ih\u00b7rer", "Hand", "das", "Gl\u00fcck", ",", "Ge\u00b7wind'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Ros\u2019 im Lockenhaar, ein schlankes,", "tokens": ["Und", "Ros'", "im", "Lo\u00b7cken\u00b7haar", ",", "ein", "schlan\u00b7kes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Mi\u00dfgeschick ein fieberkrankes,", "tokens": ["Das", "Mi\u00df\u00b7ge\u00b7schick", "ein", "fie\u00b7ber\u00b7kran\u00b7kes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein weinend Kind.", "tokens": ["Ein", "wei\u00b7nend", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und trifft sie Alles recht zu Danke", "tokens": ["Und", "trifft", "sie", "Al\u00b7les", "recht", "zu", "Dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geordnet von der Frauenhand,", "tokens": ["Ge\u00b7ord\u00b7net", "von", "der", "Frau\u00b7en\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann nippt vom Mahle wohl die schlanke", "tokens": ["Dann", "nippt", "vom", "Mah\u00b7le", "wohl", "die", "schlan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4\u00dft auch wohl ein heimlich Pfand;", "tokens": ["Und", "l\u00e4\u00dft", "auch", "wohl", "ein", "heim\u00b7lich", "Pfand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch sollt\u2019 ein Frevler lauschen, risch,", "tokens": ["Doch", "sollt'", "ein", "Frev\u00b7ler", "lau\u00b7schen", ",", "risch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Hui zerstoben ist die Scene,", "tokens": ["Im", "Hui", "zer\u00b7sto\u00b7ben", "ist", "die", "Sce\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und scheidend f\u00e4llt des Ungl\u00fccks Thr\u00e4ne", "tokens": ["Und", "schei\u00b7dend", "f\u00e4llt", "des", "Un\u00b7gl\u00fccks", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Heerd und Tisch.", "tokens": ["Auf", "Heerd", "und", "Tisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "O keine Bearnerin wird\u2019s wagen", "tokens": ["O", "kei\u00b7ne", "Be\u00b7ar\u00b7ne\u00b7rin", "wird's", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "NN", "VAFIN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu steh\u2019n am Astloch, lieber wird", "tokens": ["Zu", "steh'n", "am", "Ast\u00b7loch", ",", "lie\u00b7ber", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tuch sie um die Augen schlagen,", "tokens": ["Ein", "Tuch", "sie", "um", "die", "Au\u00b7gen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn durch den Spalt die Lampe flirrt;", "tokens": ["Wenn", "durch", "den", "Spalt", "die", "Lam\u00b7pe", "flirrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Manon auch dr\u00fcckt die Wimper zu,", "tokens": ["Ma\u00b7non", "auch", "dr\u00fcckt", "die", "Wim\u00b7per", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zupft an der Gardine Linnen;", "tokens": ["Und", "zupft", "an", "der", "Gar\u00b7di\u00b7ne", "Lin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Doch immer, immer l\u00e4\u00dft das Sinnen", "tokens": ["Doch", "im\u00b7mer", ",", "im\u00b7mer", "l\u00e4\u00dft", "das", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr keine Ruh.", "tokens": ["Ihr", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ward glatt das Lailach auch gebreitet?", "tokens": ["Ward", "glatt", "das", "Lai\u00b7lach", "auch", "ge\u00b7brei\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat hell der Becher auch geblinkt?", "tokens": ["Hat", "hell", "der", "Be\u00b7cher", "auch", "ge\u00b7blinkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob jetzt das Gl\u00fcck zum Tische gleitet,", "tokens": ["Ob", "jetzt", "das", "Gl\u00fcck", "zum", "Ti\u00b7sche", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Br\u00f6cklein nascht, ein Tr\u00f6pflein trinkt?", "tokens": ["Ein", "Br\u00f6ck\u00b7lein", "nascht", ",", "ein", "Tr\u00f6pf\u00b7lein", "trinkt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Oft glaubt sie zarter Stimmen Hauch,", "tokens": ["Oft", "glaubt", "sie", "zar\u00b7ter", "Stim\u00b7men", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Versch\u00e4mtes Trippeln oft zu h\u00f6ren,", "tokens": ["Ver\u00b7sch\u00e4m\u00b7tes", "Trip\u00b7peln", "oft", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dann am Brode leises St\u00f6ren", "tokens": ["Und", "dann", "am", "Bro\u00b7de", "lei\u00b7ses", "St\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Knuspern auch.", "tokens": ["Und", "Knus\u00b7pern", "auch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$."], "meter": "-+-+", "m