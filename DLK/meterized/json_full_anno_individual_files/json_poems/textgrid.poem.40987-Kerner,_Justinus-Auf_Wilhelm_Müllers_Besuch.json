{"textgrid.poem.40987": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf Wilhelm M\u00fcllers Besuch", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kamst zu mir, ein Stern in stiller Nacht,", "tokens": ["Du", "kamst", "zu", "mir", ",", "ein", "Stern", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warst mit der Sonne Wiederkehr verschwunden,", "tokens": ["Warst", "mit", "der", "Son\u00b7ne", "Wie\u00b7der\u00b7kehr", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Liedern nicht und nicht von Hellas Wunden", "tokens": ["Von", "Lie\u00b7dern", "nicht", "und", "nicht", "von", "Hel\u00b7las", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKNEG", "KON", "PTKNEG", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ward da gesprochen oder still gedacht.", "tokens": ["Ward", "da", "ge\u00b7spro\u00b7chen", "o\u00b7der", "still", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein! von des Erdentraumes kurzen Stunden,", "tokens": ["Nein", "!", "von", "des", "Er\u00b7den\u00b7trau\u00b7mes", "kur\u00b7zen", "Stun\u00b7den", ","], "token_info": ["w