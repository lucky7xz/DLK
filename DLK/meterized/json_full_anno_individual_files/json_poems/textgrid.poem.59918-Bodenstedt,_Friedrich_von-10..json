{"textgrid.poem.59918": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah ihn neulich sp\u00e4t nach Hause kommen,", "tokens": ["Ich", "sah", "ihn", "neu\u00b7lich", "sp\u00e4t", "nach", "Hau\u00b7se", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er hatte sich im Trinken \u00fcbernommen,", "tokens": ["Er", "hat\u00b7te", "sich", "im", "Trin\u00b7ken", "\u00fc\u00b7ber\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da fiel er in den Schmutz und seufzte trunken:", "tokens": ["Da", "fiel", "er", "in", "den", "Schmutz", "und", "seufz\u00b7te", "trun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbdie Welt ist in Verderben ganz versunken!\u00ab", "tokens": ["\u00bb", "die", "Welt", "ist", "in", "Ver\u00b7der\u00b7ben", "ganz", "ver\u00b7sun\u00b7ken", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPR", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sein Glaube ist so gro\u00df, da\u00df, wenn er f\u00e4llt,", "tokens": ["Sein", "Glau\u