{"dta.poem.13317": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eerkiestet aber ihr vielleicht den Ehren-Stein?", "tokens": ["\u201e", "er\u00b7kies\u00b7tet", "a\u00b7ber", "ihr", "viel\u00b7leicht", "den", "Eh\u00b7ren\u00b7Stein", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "KON", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201enein: dem gekr\u00f6nten Paar soll der gewidmet seyn.", "tokens": ["\u201e", "nein", ":", "dem", "ge\u00b7kr\u00f6n\u00b7ten", "Paar", "soll", "der", "ge\u00b7wid\u00b7met", "seyn", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "ART", "ADJA", "NN", "VMFIN", "ART", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewir k\u00f6nnen ihn dem Volck zu bauen \u00fcberlassen;", "tokens": ["\u201e", "wir", "k\u00f6n\u00b7nen", "ihn", "dem", "Volck", "zu", "bau\u00b7en", "\u00fc\u00b7ber\u00b7las\u00b7sen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "420\u201dEs wei\u00df sich ohne dem f\u00fcr Freude nicht zu fassen.", "tokens": ["\"", "Es", "wei\u00df", "sich", "oh\u00b7ne", "dem", "f\u00fcr", "Freu\u00b7de", "nicht", "zu", "fas\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "APPR", "ART", "APPR", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ees wird, wann dem Gemahl die Kayser-Krone gl\u00fcckt,", "tokens": ["\u201e", "es", "wird", ",", "wann", "dem", "Ge\u00b7mahl", "die", "Kay\u00b7ser\u00b7Kro\u00b7ne", "gl\u00fcckt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund sich der Feinde Zweck durch diese Wahl verr\u00fcckt,", "tokens": ["\u201e", "und", "sich", "der", "Fein\u00b7de", "Zweck", "durch", "die\u00b7se", "Wahl", "ver\u00b7r\u00fcckt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PRF", "ART", "NN", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edurch tausend Siegs-Geb\u00e4u und Freud- und Ehren-B\u00f6gen", "tokens": ["\u201e", "durch", "tau\u00b7send", "Siegs\u00b7Ge\u00b7b\u00e4u", "und", "Freu\u00b7d", "und", "Eh\u00b7ren\u00b7B\u00f6\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "CARD", "NN", "KON", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "\u201eder Treu und Liebe Pfand vor dessen Augen legen.", "tokens": ["\u201e", "der", "Treu", "und", "Lie\u00b7be", "Pfand", "vor", "des\u00b7sen", "Au\u00b7gen", "le\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "NN", "NN", "APPR", "PRELAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "425\u201dDa werdet ihr so wohl, als dieses Paar geehrt;", "tokens": ["\"", "Da", "wer\u00b7det", "ihr", "so", "wohl", ",", "als", "die\u00b7ses", "Paar", "ge\u00b7ehrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eda wird der Tugenden Lob, Ehr und Ruhm vermehrt;", "tokens": ["\u201e", "da", "wird", "der", "Tu\u00b7gen\u00b7den", "Lob", ",", "Ehr", "und", "Ruhm", "ver\u00b7mehrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "\u201ewie dort die Majest\u00e4t mit andern von der Sache", "tokens": ["\u201e", "wie", "dort", "die", "Ma\u00b7jes\u00b7t\u00e4t", "mit", "an\u00b7dern", "von", "der", "Sa\u00b7che"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "APPR", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eschon Vorbedeutungs-weis, und sehr wahrscheinlich sprache.", "tokens": ["\u201e", "schon", "Vor\u00b7be\u00b7deu\u00b7tungs\u00b7weis", ",", "und", "sehr", "wahr\u00b7schein\u00b7lich", "spra\u00b7che", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}