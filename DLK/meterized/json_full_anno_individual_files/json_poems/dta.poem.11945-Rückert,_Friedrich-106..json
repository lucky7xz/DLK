{"dta.poem.11945": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eh du ein Werk beginnst, sieh zu, ob auch die Krone,", "tokens": ["Eh", "du", "ein", "Werk", "be\u00b7ginnst", ",", "sieh", "zu", ",", "ob", "auch", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VVIMP", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die es verhei\u00dft, der M\u00fch, die es erfordert, lohne.", "tokens": ["Die", "es", "ver\u00b7hei\u00dft", ",", "der", "M\u00fch", ",", "die", "es", "er\u00b7for\u00b7dert", ",", "loh\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bist du erst mitten drinn, und nimmst es dann zu Sinn,", "tokens": ["Bist", "du", "erst", "mit\u00b7ten", "drinn", ",", "und", "nimmst", "es", "dann", "zu", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sp\u00e4t, was du auch thust, ist dann nur Ungewinn.", "tokens": ["Zu", "sp\u00e4t", ",", "was", "du", "auch", "thust", ",", "ist", "dann", "nur", "Un\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn wenn du abstehst, hast du dich umsonst geplagt;", "tokens": ["Denn", "wenn", "du", "ab\u00b7stehst", ",", "hast", "du", "dich", "um\u00b7sonst", "ge\u00b7plagt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und setzest du es fort, so ist noch mehr gewagt.", "tokens": ["Und", "set\u00b7zest", "du", "es", "fort", ",", "so", "ist", "noch", "mehr", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}