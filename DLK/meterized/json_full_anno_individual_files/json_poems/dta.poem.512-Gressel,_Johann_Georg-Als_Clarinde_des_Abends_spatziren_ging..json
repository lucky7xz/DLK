{"dta.poem.512": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als  Clarinde  des Abends spatziren ging.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wie! zeigt Diana sich", "tokens": ["Wie", "!", "zeigt", "Di\u00b7a\u00b7na", "sich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "NE", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf dieser nieder ", "tokens": ["Auf", "die\u00b7ser", "nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "L\u00e4\u00dft sie die Sternen-Bahn", "tokens": ["L\u00e4\u00dft", "sie", "die", "Ster\u00b7nen\u00b7Bahn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und ihr gew\u00f6lcktes Feld/", "tokens": ["Und", "ihr", "ge\u00b7w\u00f6lck\u00b7tes", "Feld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und strahlt die Erde an?", "tokens": ["Und", "strahlt", "die", "Er\u00b7de", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch Nein! ich irre mich/", "tokens": ["Doch", "Nein", "!", "ich", "ir\u00b7re", "mich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es ist ", "tokens": ["Es", "ist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Der ihr den Ehren-Krantz", "tokens": ["Der", "ihr", "den", "Eh\u00b7ren\u00b7Krantz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Vom Haupt und Stirne raubt", "tokens": ["Vom", "Haupt", "und", "Stir\u00b7ne", "raubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und sich damit belaubt.", "tokens": ["Und", "sich", "da\u00b7mit", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}