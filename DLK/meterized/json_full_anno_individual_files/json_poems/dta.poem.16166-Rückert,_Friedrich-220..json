{"dta.poem.16166": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "220.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich in seiner H\u00fclfsbed\u00fcrftigkeit ein Kind,", "tokens": ["Seh'", "ich", "in", "sei\u00b7ner", "H\u00fclfs\u00b7be\u00b7d\u00fcrf\u00b7tig\u00b7keit", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So f\u00fchl' ich, wie vor Gott wir alle Kinder sind.", "tokens": ["So", "f\u00fchl'", "ich", ",", "wie", "vor", "Gott", "wir", "al\u00b7le", "Kin\u00b7der", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "PPER", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie h\u00e4lfest du dir, Herz, wollt' er nicht dein des armen", "tokens": ["Wie", "h\u00e4l\u00b7fest", "du", "dir", ",", "Herz", ",", "wollt'", "er", "nicht", "dein", "des", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich ebenso, wie du dich deines Kinds, erbarmen!", "tokens": ["Sich", "e\u00b7ben\u00b7so", ",", "wie", "du", "dich", "dei\u00b7nes", "Kinds", ",", "er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "wor