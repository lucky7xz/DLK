{"dta.poem.8868": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Die gutgemuthet du bisher getragen,", "tokens": ["Die", "gut\u00b7ge\u00b7mu\u00b7thet", "du", "bis\u00b7her", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wolle nicht, mit kindischem Verzagen,", "tokens": ["Und", "wol\u00b7le", "nicht", ",", "mit", "kin\u00b7di\u00b7schem", "Ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der schn\u00f6den Mittelm\u00e4\u00dfigkeit gefallen!", "tokens": ["Der", "schn\u00f6\u00b7den", "Mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfig\u00b7keit", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mag die Bosheit auch die F\u00e4uste ballen,", "tokens": ["Und", "mag", "die", "Bos\u00b7heit", "auch", "die", "F\u00e4us\u00b7te", "bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch athmen Seelen, welche keck es wagen,", "tokens": ["Noch", "ath\u00b7men", "See\u00b7len", ",", "wel\u00b7che", "keck", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lebendig, wie die deinige, zu schlagen,", "tokens": ["Le\u00b7ben\u00b7dig", ",", "wie", "die", "dei\u00b7ni\u00b7ge", ",", "zu", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "PPOSS", "$,", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "D'rum la\u00df die frischen Lieder nur erschallen!", "tokens": ["D'\u00b7rum", "la\u00df", "die", "fri\u00b7schen", "Lie\u00b7der", "nur", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Geschw\u00e4tz'gen Krittlern g\u00f6nne du die Kleinheit,", "tokens": ["Ge\u00b7schw\u00e4tz'\u00b7gen", "Kritt\u00b7lern", "g\u00f6n\u00b7ne", "du", "die", "Klein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald dies und das zu tadeln und zu loben,", "tokens": ["Bald", "dies", "und", "das", "zu", "ta\u00b7deln", "und", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "KON", "PDS", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nie zu fassen eines Geistes Einheit.", "tokens": ["Und", "nie", "zu", "fas\u00b7sen", "ei\u00b7nes", "Geis\u00b7tes", "Ein\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr kurzer Groll wird allgemach vertoben,", "tokens": ["Ihr", "kur\u00b7zer", "Groll", "wird", "all\u00b7ge\u00b7mach", "ver\u00b7to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du aber sch\u00fcttelst ab des Tags Gemeinheit,", "tokens": ["Du", "a\u00b7ber", "sch\u00fct\u00b7telst", "ab", "des", "Tags", "Ge\u00b7mein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn dich der heil'ge ", "tokens": ["Wenn", "dich", "der", "heil'\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}}}}}