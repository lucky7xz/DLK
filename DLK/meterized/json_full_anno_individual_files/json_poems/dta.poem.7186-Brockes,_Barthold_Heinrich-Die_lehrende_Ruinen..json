{"dta.poem.7186": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrende Ruinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es zeigt uns Deine kluge Hand", "tokens": ["Es", "zeigt", "uns", "Dei\u00b7ne", "klu\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aegypten, das gelobte Land,", "tokens": ["A\u00b7e\u00b7gyp\u00b7ten", ",", "das", "ge\u00b7lob\u00b7te", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Colossen, Gr\u00e4ber, Mausoleen,", "tokens": ["Co\u00b7los\u00b7sen", ",", "Gr\u00e4\u00b7ber", ",", "Mau\u00b7so\u00b7leen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewesne Tempel, jetzt Moscheen,", "tokens": ["Ge\u00b7wes\u00b7ne", "Tem\u00b7pel", ",", "jetzt", "Mo\u00b7scheen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Palmyra, dessen Rest mich ungemein erg\u00f6tzt,", "tokens": ["Pal\u00b7my\u00b7ra", ",", "des\u00b7sen", "Rest", "mich", "un\u00b7ge\u00b7mein", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mich zugleich in Furcht und in Erstaunen setzt,", "tokens": ["Und", "mich", "zu\u00b7gleich", "in", "Furcht", "und", "in", "Er\u00b7stau\u00b7nen", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "De\u00df ungez\u00e4hlter S\u00e4ulen Menge", "tokens": ["De\u00df", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "S\u00e4u\u00b7len", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zerbrochner Uber-Rest, ein wunderbar Gepr\u00e4nge", "tokens": ["Zer\u00b7broch\u00b7ner", "U\u00b7ber\u00b7Rest", ",", "ein", "wun\u00b7der\u00b7bar", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch in dem Staube zeigt. Die unterbrochnen G\u00e4nge,", "tokens": ["Noch", "in", "dem", "Stau\u00b7be", "zeigt", ".", "Die", "un\u00b7ter\u00b7broch\u00b7nen", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die nicht zu z\u00e4hlen sind, die kommen mir", "tokens": ["Die", "nicht", "zu", "z\u00e4h\u00b7len", "sind", ",", "die", "kom\u00b7men", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,", "PRELS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Als lauter Grotten-Wercke f\u00fcr:", "tokens": ["Als", "lau\u00b7ter", "Grot\u00b7ten\u00b7\u00b7Wer\u00b7cke", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In welcher Nettigkeit und Moder, Lust und Grausen,", "tokens": ["In", "wel\u00b7cher", "Net\u00b7tig\u00b7keit", "und", "Mo\u00b7der", ",", "Lust", "und", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verachtung, Majest\u00e4t, erb\u00e4rmlich-sch\u00f6n,", "tokens": ["Ver\u00b7ach\u00b7tung", ",", "Ma\u00b7jes\u00b7t\u00e4t", ",", "er\u00b7b\u00e4rm\u00b7lich\u00b7sch\u00f6n", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vermischt, verwirrt, vereint zu sehn;", "tokens": ["Ver\u00b7mischt", ",", "ver\u00b7wirrt", ",", "ver\u00b7eint", "zu", "sehn", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wo Barbarey und Kunst verkn\u00fcpft zusammen hausen.", "tokens": ["Wo", "Bar\u00b7ba\u00b7rey", "und", "Kunst", "ver\u00b7kn\u00fcpft", "zu\u00b7sam\u00b7men", "hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}