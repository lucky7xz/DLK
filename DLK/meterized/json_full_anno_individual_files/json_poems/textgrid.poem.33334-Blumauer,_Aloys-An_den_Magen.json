{"textgrid.poem.33334": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An den Magen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfm\u00e4chtigster der irdischen Despoten,", "tokens": ["Gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7ter", "der", "ir\u00b7di\u00b7schen", "Des\u00b7po\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tyrann, vor welchem man", "tokens": ["Ty\u00b7rann", ",", "vor", "wel\u00b7chem", "man"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PRELS", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich pr\u00e4sentiren kann!", "tokens": ["Sich", "pr\u00e4\u00b7sen\u00b7ti\u00b7ren", "kann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bist \u2013 und dies macht unsern Grossen Ehre \u2013", "tokens": ["Du", "bist", "\u2013", "und", "dies", "macht", "un\u00b7sern", "Gros\u00b7sen", "Eh\u00b7re", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KON", "PDS", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr wahres Ebenbild,", "tokens": ["Ihr", "wah\u00b7res", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn du entv\u00f6lkerst L\u00e4nder, Fl\u00fcsse, Meere,", "tokens": ["Denn", "du", "ent\u00b7v\u00f6l\u00b7kerst", "L\u00e4n\u00b7der", ",", "Fl\u00fcs\u00b7se", ",", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Damit dein Schlund sich f\u00fcllt.", "tokens": ["Da\u00b7mit", "dein", "Schlund", "sich", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Allm\u00e4chtig, wie des weisen Sch\u00f6pfers Werde!", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", ",", "wie", "des", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "Wer\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist stets dein Machtgebot;", "tokens": ["Ist", "stets", "dein", "Macht\u00b7ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn was nur e\u00dfbar ist auf dieser Erde", "tokens": ["Denn", "was", "nur", "e\u00df\u00b7bar", "ist", "auf", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADJD", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwandelst du in Koth.", "tokens": ["Ver\u00b7wan\u00b7delst", "du", "in", "Koth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist kein Fleckchen unter allen Zonen,", "tokens": ["Es", "ist", "kein", "Fleck\u00b7chen", "un\u00b7ter", "al\u00b7len", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das dir nicht zinsbar ist:", "tokens": ["Das", "dir", "nicht", "zins\u00b7bar", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist es, der den Schwei\u00df von Millionen", "tokens": ["Du", "bist", "es", ",", "der", "den", "Schwei\u00df", "von", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gesch\u00e4ft'ger H\u00e4nde fri\u00dft.", "tokens": ["Ge\u00b7sch\u00e4ft'\u00b7ger", "H\u00e4n\u00b7de", "fri\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Heer ge\u00fcbter M\u00f6rder, Spie\u00dfer, W\u00fcrger,", "tokens": ["Ein", "Heer", "ge\u00b7\u00fcb\u00b7ter", "M\u00f6r\u00b7der", ",", "Spie\u00b7\u00dfer", ",", "W\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4ltst du dir f\u00fcr und f\u00fcr,", "tokens": ["H\u00e4ltst", "du", "dir", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Je m\u00fcrber die nun beitzen deine B\u00fcrger,", "tokens": ["Je", "m\u00fcr\u00b7ber", "die", "nun", "beit\u00b7zen", "dei\u00b7ne", "B\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRELS", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Je besser schmeckt es dir.", "tokens": ["Je", "bes\u00b7ser", "schmeckt", "es", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du bist der Gott, den alle V\u00f6lker lieben,", "tokens": ["Du", "bist", "der", "Gott", ",", "den", "al\u00b7le", "V\u00f6l\u00b7ker", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den alles venerirt,", "tokens": ["Den", "al\u00b7les", "ve\u00b7ne\u00b7rirt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur unter dir ist \u2013 wie es steht geschrieben \u2013", "tokens": ["Nur", "un\u00b7ter", "dir", "ist", "\u2013", "wie", "es", "steht", "ge\u00b7schrie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "$(", "PWAV", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Schafstall und ein Hirt.", "tokens": ["Ein", "Schaf\u00b7stall", "und", "ein", "Hirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dir dienet alles \u2013 Juden, Heiden, Christen \u2013", "tokens": ["Dir", "die\u00b7net", "al\u00b7les", "\u2013", "Ju\u00b7den", ",", "Hei\u00b7den", ",", "Chris\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$(", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich ehrt die ganze Welt:", "tokens": ["Dich", "ehrt", "die", "gan\u00b7ze", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist's allein, der weder Atheisten,", "tokens": ["Du", "bist's", "al\u00b7lein", ",", "der", "we\u00b7der", "A\u00b7theis\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PRELS", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch Glaubenszweifler z\u00e4hlt.", "tokens": ["Noch", "Glau\u00b7bens\u00b7zweif\u00b7ler", "z\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dir zollen Berg und Thal und Wies' und Triften", "tokens": ["Dir", "zol\u00b7len", "Berg", "und", "Thal", "und", "Wies'", "und", "Trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So manches Opferthier,", "tokens": ["So", "man\u00b7ches", "Op\u00b7fer\u00b7thier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hunderttausend Wohlger\u00fcche d\u00fcften", "tokens": ["Und", "hun\u00b7dert\u00b7tau\u00b7send", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus jedem Schornstein dir.", "tokens": ["Aus", "je\u00b7dem", "Schorn\u00b7stein", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In jedem Hause baut man ungefodert", "tokens": ["In", "je\u00b7dem", "Hau\u00b7se", "baut", "man", "un\u00b7ge\u00b7fo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PIS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir einen Altar auf,", "tokens": ["Dir", "ei\u00b7nen", "Al\u00b7tar", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und t\u00e4glich zweimal, wo nicht \u00f6fter, lodert", "tokens": ["Und", "t\u00e4g\u00b7lich", "zwei\u00b7mal", ",", "wo", "nicht", "\u00f6f\u00b7ter", ",", "lo\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "ADV", "$,", "PWAV", "PTKNEG", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Opferflamme d'rauf.", "tokens": ["Die", "Op\u00b7fer\u00b7flam\u00b7me", "d'\u00b7rauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Und Priesterinen mit schneeweissen Sch\u00fcrzen,", "tokens": ["Und", "Pries\u00b7te\u00b7ri\u00b7nen", "mit", "schnee\u00b7weis\u00b7sen", "Sch\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sonst K\u00f6chinen genannt,", "tokens": ["Sonst", "K\u00f6\u00b7chi\u00b7nen", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bem\u00fchen sich, das Opfer dir zu w\u00fcrzen,", "tokens": ["Be\u00b7m\u00fc\u00b7hen", "sich", ",", "das", "Op\u00b7fer", "dir", "zu", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ihre Kunst erfand.", "tokens": ["Das", "ih\u00b7re", "Kunst", "er\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Doch will man dich, der Allmacht Stellvertreter,", "tokens": ["Doch", "will", "man", "dich", ",", "der", "All\u00b7macht", "Stell\u00b7ver\u00b7tre\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deinem Glanze schau'n,", "tokens": ["In", "dei\u00b7nem", "Glan\u00b7ze", "schau'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00df man dich, wie uns're Erdeng\u00f6tter,", "tokens": ["So", "mu\u00df", "man", "dich", ",", "wie", "un\u00b7s'\u00b7re", "Er\u00b7den\u00b7g\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Betrachten im Verdau'n.", "tokens": ["Be\u00b7trach\u00b7ten", "im", "Ver\u00b7dau'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["NN", "APPRART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da flieht von dir, wie ein geschreckter Hase,", "tokens": ["Da", "flieht", "von", "dir", ",", "wie", "ein", "ge\u00b7schreck\u00b7ter", "Ha\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Selbst die Philosophie:", "tokens": ["Selbst", "die", "Phi\u00b7lo\u00b7so\u00b7phie", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kaum so viel Luft, als eine Seifenblase", "tokens": ["Kaum", "so", "viel", "Luft", ",", "als", "ei\u00b7ne", "Sei\u00b7fen\u00b7bla\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Enth\u00e4lt, verscheuet sie.", "tokens": ["Ent\u00b7h\u00e4lt", ",", "ver\u00b7scheu\u00b7et", "sie", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Dir huldigt selbst das edle Gottheitstheilchen,", "tokens": ["Dir", "hul\u00b7digt", "selbst", "das", "ed\u00b7le", "Got\u00b7theitst\u00b7heil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das uns im Kopf logirt,", "tokens": ["Das", "uns", "im", "Kopf", "lo\u00b7girt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sobald dich nur dein Amt ein Weilchen,", "tokens": ["So\u00b7bald", "dich", "nur", "dein", "Amt", "ein", "Weil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mindeste genirt.", "tokens": ["Das", "min\u00b7des\u00b7te", "ge\u00b7nirt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ja, du gebeutst dem m\u00e4chtigsten der Triebe;", "tokens": ["Ja", ",", "du", "ge\u00b7beutst", "dem", "m\u00e4ch\u00b7tigs\u00b7ten", "der", "Trie\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Denn auf dein Machtgebot", "tokens": ["Denn", "auf", "dein", "Macht\u00b7ge\u00b7bot"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergie\u00dft der Seladon auch seine Liebe,", "tokens": ["Ver\u00b7gie\u00dft", "der", "Se\u00b7la\u00b7don", "auch", "sei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "i